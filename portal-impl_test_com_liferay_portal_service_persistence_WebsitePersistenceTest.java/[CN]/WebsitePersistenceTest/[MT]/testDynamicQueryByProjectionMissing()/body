{
  Website newWebsite=addWebsite();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Website.class,Website.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("websiteId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("websiteId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
