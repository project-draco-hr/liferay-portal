{
  JournalFolder folder=JournalFolderLocalServiceUtil.fetchFolder(groupId,name);
  if (folder != null) {
    return folder;
  }
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext();
  return JournalFolderLocalServiceUtil.addFolder(TestPropsValues.getUserId(),groupId,parentFolderId,name,"This is a test folder.",serviceContext);
}
