{
  Long size1=null;
  Long size2=null;
  if (obj1 instanceof DLFileEntry) {
    size1=((DLFileEntry)obj1).getSize();
    size2=((DLFileEntry)obj2).getSize();
  }
 else {
    size1=((FileEntry)obj1).getSize();
    size2=((FileEntry)obj2).getSize();
  }
  int value=size1.compareTo(size2);
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
