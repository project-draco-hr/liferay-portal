{
  if (!entry.getFolderId().equals(folderId)) {
    BookmarksFolder oldFolder=BookmarksFolderUtil.findByPrimaryKey(entry.getFolderId());
    BookmarksFolder newFolder=BookmarksFolderUtil.fetchByPrimaryKey(folderId);
    if ((newFolder == null) || (oldFolder.getGroupId() != newFolder.getGroupId())) {
      folderId=entry.getFolderId();
    }
  }
  return BookmarksFolderUtil.findByPrimaryKey(folderId);
}
