{
  BookmarksEntry entry=BookmarksEntryUtil.findByPrimaryKey(entryId);
  BookmarksFolder folder=getFolder(entry,folderId);
  if (Validator.isNull(name)) {
    name=url;
  }
  validate(url);
  entry.setModifiedDate(new Date());
  entry.setFolderId(folder.getFolderId());
  entry.setName(name);
  entry.setUrl(url);
  entry.setComments(comments);
  BookmarksEntryUtil.update(entry);
  TagsAssetLocalServiceUtil.updateAsset(entry.getUserId(),BookmarksEntry.class.getName(),entry.getPrimaryKey().toString(),tagsEntries);
  return entry;
}
