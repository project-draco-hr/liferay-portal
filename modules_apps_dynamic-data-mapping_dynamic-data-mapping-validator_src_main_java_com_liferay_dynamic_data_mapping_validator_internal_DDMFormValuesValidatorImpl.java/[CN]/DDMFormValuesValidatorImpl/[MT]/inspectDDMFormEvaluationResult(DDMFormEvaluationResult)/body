{
  Map<String,DDMFormFieldEvaluationResult> ddmFormFieldEvaluationResultsMap=ddmFormEvaluationResult.getDDMFormFieldEvaluationResultsMap();
  StringBundler sb=new StringBundler(ddmFormFieldEvaluationResultsMap.size() * 3);
  for (  Map.Entry<String,DDMFormFieldEvaluationResult> entry : ddmFormFieldEvaluationResultsMap.entrySet()) {
    inspectDDMFormFieldEvaluationResult(entry,sb);
  }
  if (sb.index() > 0) {
    throw new DDMFormEvaluationException(sb.toString());
  }
}
