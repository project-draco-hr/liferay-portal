{
  Registry registry=RegistryUtil.getRegistry();
  Collection<Long> bundleIds=new ArrayList<>();
  Collection<ServiceReference<T>> serviceReferences=registry.getServiceReferences(clazz,filter);
  for (  ServiceReference<T> serviceReference : serviceReferences) {
    Long bundleId=(Long)serviceReference.getProperty("service.bundleid");
    bundleIds.add(bundleId);
  }
  return bundleIds;
}
