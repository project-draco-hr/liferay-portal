{
  String emailKBArticleAddedBody=preferences.getValue("emailKBArticleAddedBody",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleAddedBody)) {
    return emailKBArticleAddedBody;
  }
  return ContentUtil.get(AdminUtil.class.getClassLoader(),PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_ADDED_BODY);
}
