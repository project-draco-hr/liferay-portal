{
  String emailKBArticleSuggestionReceivedEnabled=preferences.getValue("emailKBArticleSuggestionReceivedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleSuggestionReceivedEnabled)) {
    return GetterUtil.getBoolean(emailKBArticleSuggestionReceivedEnabled);
  }
  return PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_SUGGESTION_RECEIVED_ENABLED;
}
