{
  String emailKBArticleSuggestionInProgressEnabled=preferences.getValue("emailKBArticleSuggestionInProgressEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleSuggestionInProgressEnabled)) {
    return GetterUtil.getBoolean(emailKBArticleSuggestionInProgressEnabled);
  }
  return PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_SUGGESTION_IN_PROGRESS_ENABLED;
}
