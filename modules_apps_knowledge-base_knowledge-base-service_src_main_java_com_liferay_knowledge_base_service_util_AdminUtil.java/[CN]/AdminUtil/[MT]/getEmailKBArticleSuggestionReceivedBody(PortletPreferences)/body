{
  String emailKBArticleSuggestionReceivedBody=preferences.getValue("emailKBArticleSuggestionReceivedBody",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleSuggestionReceivedBody)) {
    return emailKBArticleSuggestionReceivedBody;
  }
  return ContentUtil.get(AdminUtil.class.getClassLoader(),PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_SUGGESTION_RECEIVED_BODY);
}
