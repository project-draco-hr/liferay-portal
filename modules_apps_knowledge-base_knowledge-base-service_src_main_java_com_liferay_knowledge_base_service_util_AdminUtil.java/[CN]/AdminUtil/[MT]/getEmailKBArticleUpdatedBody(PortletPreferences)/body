{
  String emailKBArticleUpdatedBody=preferences.getValue("emailKBArticleUpdatedBody",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleUpdatedBody)) {
    return emailKBArticleUpdatedBody;
  }
  return ContentUtil.get(AdminUtil.class.getClassLoader(),PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_UPDATED_BODY);
}
