{
  String emailKBArticleSuggestionResolvedBody=preferences.getValue("emailKBArticleSuggestionResolvedBody",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleSuggestionResolvedBody)) {
    return emailKBArticleSuggestionResolvedBody;
  }
  return ContentUtil.get(AdminUtil.class.getClassLoader(),PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_SUGGESTION_RESOLVED_BODY);
}
