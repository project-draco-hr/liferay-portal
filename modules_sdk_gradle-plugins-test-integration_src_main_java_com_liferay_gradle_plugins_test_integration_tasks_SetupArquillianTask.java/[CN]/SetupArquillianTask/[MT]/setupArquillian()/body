{
  File outputFile=getOutputFile();
  String xml=FileUtil.read("com/liferay/gradle/plugins/test/integration/dependencies/" + "arquillian.xml");
  xml=xml.replace("${app.server.tomcat.manager.password}",getManagerPassword());
  xml=xml.replace("${app.server.tomcat.manager.user}",getManagerUserName());
  xml=xml.replace("${jmx.remote.port}",String.valueOf(getJmxRemotePort()));
  Files.write(outputFile.toPath(),xml.getBytes(StandardCharsets.UTF_8));
}
