{
  if (_bulletStyle != null) {
    return _bulletStyle;
  }
  _bulletStyle=ParamUtil.getString(_request,"bulletStyle",_siteNavigationMenuPortletInstanceConfiguration.bulletStyle());
  if (Validator.isNull(_bulletStyle)) {
    ThemeDisplay themeDisplay=(ThemeDisplay)_request.getAttribute(WebKeys.THEME_DISPLAY);
    _bulletStyle=GetterUtil.getString(themeDisplay.getThemeSetting("bullet-style"),_siteNavigationMenuWebConfiguration.defaultBulletStyle());
  }
  return _bulletStyle;
}
