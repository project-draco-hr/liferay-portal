{
  String message=getExceptionSnippet(consoleOutput);
  if (message != null) {
    return message;
  }
  message=getMergeTestResultsSnippet(consoleOutput);
  if (message != null) {
    return message;
  }
  message=getBuildFailedSnippet(consoleOutput);
  if (message != null) {
    return message;
  }
  return getConsoleOutputSnippet(consoleOutput,true,-1);
}
