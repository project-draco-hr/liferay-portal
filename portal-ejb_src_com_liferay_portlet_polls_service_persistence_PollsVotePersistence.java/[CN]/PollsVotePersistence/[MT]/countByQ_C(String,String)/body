{
  Session session=null;
  try {
    session=openSession();
    StringMaker query=new StringMaker();
    query.append("SELECT COUNT(*) ");
    query.append("FROM com.liferay.portlet.polls.model.PollsVote WHERE ");
    if (questionId == null) {
      query.append("questionId IS NULL");
    }
 else {
      query.append("questionId = ?");
    }
    query.append(" AND ");
    if (choiceId == null) {
      query.append("choiceId IS NULL");
    }
 else {
      query.append("choiceId = ?");
    }
    query.append(" ");
    Query q=session.createQuery(query.toString());
    q.setCacheable(true);
    int queryPos=0;
    if (questionId != null) {
      q.setString(queryPos++,questionId);
    }
    if (choiceId != null) {
      q.setString(queryPos++,choiceId);
    }
    Iterator itr=q.list().iterator();
    if (itr.hasNext()) {
      Long count=(Long)itr.next();
      if (count != null) {
        return count.intValue();
      }
    }
    return 0;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
