{
  Session session=null;
  try {
    session=openSession();
    StringBuffer query=new StringBuffer();
    query.append("FROM PollsVote IN CLASS com.liferay.portlet.polls.service.persistence.PollsVoteHBM WHERE ");
    query.append("questionId = ?");
    query.append(" AND ");
    query.append("choiceId = ?");
    query.append(" ");
    Query q=session.createQuery(query.toString());
    int queryPos=0;
    q.setString(queryPos++,questionId);
    q.setString(queryPos++,choiceId);
    Iterator itr=q.list().iterator();
    while (itr.hasNext()) {
      PollsVoteHBM pollsVoteHBM=(PollsVoteHBM)itr.next();
      session.delete(pollsVoteHBM);
    }
    session.flush();
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
