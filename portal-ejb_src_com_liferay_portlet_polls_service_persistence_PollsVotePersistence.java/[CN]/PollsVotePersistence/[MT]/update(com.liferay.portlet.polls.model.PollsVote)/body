{
  Session session=null;
  try {
    session=openSession();
    if (pollsVote.isNew()) {
      session.save(pollsVote);
    }
    session.flush();
    pollsVote.setNew(false);
    return pollsVote;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
