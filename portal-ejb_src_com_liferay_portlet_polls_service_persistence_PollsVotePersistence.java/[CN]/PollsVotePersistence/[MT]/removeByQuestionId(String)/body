{
  Session session=null;
  try {
    session=openSession();
    StringBuffer query=new StringBuffer();
    query.append("FROM com.liferay.portlet.polls.model.PollsVote WHERE ");
    if (questionId == null) {
      query.append("questionId IS NULL");
    }
 else {
      query.append("questionId = ?");
    }
    query.append(" ");
    Query q=session.createQuery(query.toString());
    int queryPos=0;
    if (questionId != null) {
      q.setString(queryPos++,questionId);
    }
    Iterator itr=q.list().iterator();
    while (itr.hasNext()) {
      PollsVote pollsVote=(PollsVote)itr.next();
      session.delete(pollsVote);
    }
    session.flush();
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
