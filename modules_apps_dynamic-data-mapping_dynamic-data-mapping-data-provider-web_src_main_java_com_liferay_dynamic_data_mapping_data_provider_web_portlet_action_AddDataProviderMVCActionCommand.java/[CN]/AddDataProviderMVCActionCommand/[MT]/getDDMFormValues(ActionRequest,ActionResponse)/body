{
  String dataProviderType=ParamUtil.getString(actionRequest,"dataProviderType");
  DDMDataProviderSettings ddmDataProviderSettings=_ddmDataProvidersMap.get(dataProviderType);
  Class<?> clazz=ddmDataProviderSettings.getSettings();
  DDMForm ddmForm=DDMFormFactory.create(clazz);
  return _ddmFormValuesFactory.create(actionRequest,ddmForm);
}
