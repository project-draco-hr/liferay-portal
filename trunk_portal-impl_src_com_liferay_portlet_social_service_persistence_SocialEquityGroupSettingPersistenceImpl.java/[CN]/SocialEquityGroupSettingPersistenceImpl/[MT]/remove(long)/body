{
  Session session=null;
  try {
    session=openSession();
    SocialEquityGroupSetting socialEquityGroupSetting=(SocialEquityGroupSetting)session.get(SocialEquityGroupSettingImpl.class,Long.valueOf(equityGroupSettingId));
    if (socialEquityGroupSetting == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + equityGroupSettingId);
      }
      throw new NoSuchEquityGroupSettingException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + equityGroupSettingId);
    }
    return socialEquityGroupSettingPersistence.remove(socialEquityGroupSetting);
  }
 catch (  NoSuchEquityGroupSettingException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
