{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps=connection.prepareStatement("select plid, typeSettings from Layout where type_ = ?")){
    ps.setString(1,"embedded");
    try (ResultSet rs=ps.executeQuery()){
      while (rs.next()) {
        long plid=rs.getLong("plid");
        String typeSettings=rs.getString("typeSettings");
        updateLayout(plid,typeSettings);
      }
    }
   }
 }
