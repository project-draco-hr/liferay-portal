{
  Company company=CompanyUtil.findByPrimaryKey(companyId);
  if (!company.isStrangers()) {
    checkPermission(userId,organizationId,locationId,ActionKeys.ADD_USER);
  }
  return UserLocalServiceUtil.addUser(getUserId(),companyId,autoUserId,userId,autoPassword,password1,password2,passwordReset,emailAddress,locale,firstName,middleName,lastName,nickName,prefixId,suffixId,male,birthdayMonth,birthdayDay,birthdayYear,jobTitle,organizationId,locationId);
}
