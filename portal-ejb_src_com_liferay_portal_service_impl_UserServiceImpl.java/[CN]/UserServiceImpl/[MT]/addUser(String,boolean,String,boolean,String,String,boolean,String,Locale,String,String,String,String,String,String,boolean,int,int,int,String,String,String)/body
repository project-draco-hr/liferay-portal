{
  Company company=CompanyUtil.findByPrimaryKey(companyId);
  if (!company.isStrangers()) {
    checkPermission(userId,organizationId,locationId,ActionKeys.ADD_USER);
  }
  String creatorUserId=null;
  try {
    creatorUserId=getUserId();
  }
 catch (  PrincipalException pe) {
  }
  return UserLocalServiceUtil.addUser(creatorUserId,companyId,autoUserId,userId,autoPassword,password1,password2,passwordReset,emailAddress,locale,firstName,middleName,lastName,nickName,prefixId,suffixId,male,birthdayMonth,birthdayDay,birthdayYear,jobTitle,organizationId,locationId);
}
