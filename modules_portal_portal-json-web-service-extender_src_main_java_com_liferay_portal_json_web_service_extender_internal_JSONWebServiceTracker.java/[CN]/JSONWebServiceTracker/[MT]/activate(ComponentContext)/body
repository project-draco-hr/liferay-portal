{
  _componentContext=componentContext;
  BundleContext bundleContext=componentContext.getBundleContext();
  try {
    _serviceTracker=ServiceTrackerFactory.open(bundleContext,"(&(json.web.service.context.name=*)(json.web.service." + "context.path=*))",this);
  }
 catch (  InvalidSyntaxException ise) {
    throw new RuntimeException("Unable to activate Liferay Portal JSON Web Service Tracker",ise);
  }
}
