{
  _validateDefinitionElement(element,filePath);
  List<Element> childElements=element.elements();
  if (childElements.isEmpty()) {
    throw new PoshiRunnerException("Missing child elements\n" + filePath + ":"+ element.attributeValue("line-number"));
  }
  for (  Element childElement : childElements) {
    String childElementName=childElement.getName();
    if (!childElementName.equals("command")) {
      throw new PoshiRunnerException("Invalid " + childElementName + " element\n"+ filePath+ ":"+ childElement.attributeValue("line-number"));
    }
    _validateCommandElement(childElement,filePath);
    _parseElements(childElement,filePath);
  }
}
