{
  String className=PoshiRunnerGetterUtil.getClassNameFromClassCommandName(testName);
  if (!PoshiRunnerContext.isRootElement("test-case#" + className)) {
    _exceptions.add(new Exception("Invalid test case class " + className + "\n"+ filePathLineNumber));
  }
 else   if (testName.contains("#")) {
    String commandElementKey="test-case#" + testName;
    if (!PoshiRunnerContext.isCommandElement(commandElementKey)) {
      String commandName=PoshiRunnerGetterUtil.getCommandNameFromClassCommandName(testName);
      _exceptions.add(new Exception("Invalid test case command " + commandName + "\n"+ filePathLineNumber));
    }
  }
}
