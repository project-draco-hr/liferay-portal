{
  String classCommandName=element.attributeValue("macro");
  String className=PoshiRunnerGetterUtil.getClassNameFromClassCommandName(classCommandName);
  if (!PoshiRunnerContext.isRootElement("macro#" + className)) {
    throw new PoshiRunnerException("Invalid macro class " + className + "\n"+ filePath+ ":"+ element.attributeValue("line-number"));
  }
  String commandElementKey="macro#" + classCommandName;
  if (!PoshiRunnerContext.isCommandElement(commandElementKey)) {
    throw new PoshiRunnerException("Invalid macro command " + classCommandName + "\n"+ filePath+ ":"+ element.attributeValue("line-number"));
  }
}
