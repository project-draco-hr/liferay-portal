{
  List<String> possibleAttributeNames=Arrays.asList("line-number","message");
  _validatePossibleAttributeNames(element,possibleAttributeNames,filePath);
  if ((element.attributeValue("message") == null) && Validator.isNull(element.getText())) {
    throw new Exception("Missing message attribute\n" + filePath + ":"+ element.attributeValue("line-number"));
  }
}
