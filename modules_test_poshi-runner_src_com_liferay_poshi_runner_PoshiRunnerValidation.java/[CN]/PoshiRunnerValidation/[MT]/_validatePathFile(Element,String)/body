{
  String rootElementName=element.getName();
  if (!StringUtils.equals(rootElementName,"html")) {
    throw new PoshiRunnerException("\nBUILD FAILED: Invalid " + rootElementName + " element\n"+ filePath+ ":"+ element.attributeValue("line-number"));
  }
  List<Element> childElements=element.elements();
  if (childElements.isEmpty()) {
    throw new PoshiRunnerException("\nBUILD FAILED: Missing child elements\n" + filePath + ":"+ element.attributeValue("line-number"));
  }
}
