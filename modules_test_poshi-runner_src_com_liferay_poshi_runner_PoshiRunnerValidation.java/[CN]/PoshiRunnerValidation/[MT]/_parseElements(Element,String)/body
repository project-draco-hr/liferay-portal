{
  List<Element> childElements=element.elements();
  List<String> possibleElementNames=Arrays.asList("description","echo","execute","fail","for","if","take-screenshot","task","var","while");
  for (  Element childElement : childElements) {
    String elementName=childElement.getName();
    if (!possibleElementNames.contains(elementName)) {
      throw new PoshiRunnerException("Invalid " + elementName + " element\n"+ filePath+ ":"+ childElement.attributeValue("line-number"));
    }
  }
}
