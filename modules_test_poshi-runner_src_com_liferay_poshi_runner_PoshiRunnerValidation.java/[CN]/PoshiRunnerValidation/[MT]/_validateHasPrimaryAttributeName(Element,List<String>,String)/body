{
  boolean found=false;
  for (  String primaryAttributeName : primaryAttributeNames) {
    if (Validator.isNotNull(element.attributeValue(primaryAttributeName))) {
      if (!found) {
        found=true;
      }
 else {
        throw new Exception("Too many attributes: " + "\n" + filePath + ":"+ element.attributeValue("line-number"));
      }
    }
  }
  if (!found) {
    throw new Exception("Invalid or missing attribute\n" + filePath + ":"+ element.attributeValue("line-number"));
  }
}
