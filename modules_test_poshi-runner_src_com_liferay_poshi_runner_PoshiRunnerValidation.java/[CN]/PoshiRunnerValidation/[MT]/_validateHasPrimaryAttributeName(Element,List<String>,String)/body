{
  boolean found=false;
  for (  String primaryAttributeName : primaryAttributeNames) {
    if (Validator.isNotNull(element.attributeValue(primaryAttributeName))) {
      if (!found) {
        found=true;
      }
 else {
        _logExceptions.add(new Exception("Too many attributes: " + "\n" + filePath + ":"+ element.attributeValue("line-number")));
      }
    }
  }
  if (!found) {
    _logExceptions.add(new Exception("Invalid or missing attribute\n" + filePath + ":"+ element.attributeValue("line-number")));
  }
}
