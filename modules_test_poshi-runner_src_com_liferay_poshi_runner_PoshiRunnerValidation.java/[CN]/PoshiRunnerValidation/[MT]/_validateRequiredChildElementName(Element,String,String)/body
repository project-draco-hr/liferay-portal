{
  boolean found=false;
  List<Element> childElements=element.elements();
  for (  Element childElement : childElements) {
    if (StringUtils.equals(childElement.getName(),requiredElementName)) {
      found=true;
      break;
    }
  }
  if (!found) {
    throw new Exception("Missing required " + requiredElementName + " child element\n"+ filePath+ ":"+ element.attributeValue("line-number"));
  }
}
