{
  List<String> possibleAttributeNames=Arrays.asList("line-number");
  _validatePossibleAttributeNames(element,possibleAttributeNames,filePath);
  List<Element> childElements=element.elements();
  if (!childElements.isEmpty()) {
    throw new PoshiRunnerException("Invalid child elements\n" + filePath + ":"+ element.attributeValue("line-number"));
  }
}
