{
  List<String> attributeNames=Arrays.asList("line-number","name","value");
  validatePossibleAttributeNames(element,attributeNames,filePath);
  validateRequiredAttributeNames(element,attributeNames,filePath);
  List<String> testCaseAvailablePropertyNames=PoshiRunnerContext.getTestCaseAvailablePropertyNames();
  String propertyName=element.attributeValue("name");
  if (!testCaseAvailablePropertyNames.contains(propertyName)) {
    _exceptions.add(new Exception("Invalid property name " + propertyName + "\n"+ filePath+ ":"+ element.attributeValue("line-number")));
  }
}
