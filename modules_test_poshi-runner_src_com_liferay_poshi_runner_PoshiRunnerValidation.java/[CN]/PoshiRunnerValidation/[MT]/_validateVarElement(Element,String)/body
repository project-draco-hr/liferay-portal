{
  if (Validator.isNull(element.attributeValue("name"))) {
    throw new PoshiRunnerException("Missing name attribute\n" + filePath + ":"+ element.attributeValue("line-number"));
  }
  List<Attribute> attributes=element.attributes();
  List<String> possibleAttributeNames=Arrays.asList("attribute","group","line-number","locator","method","name","pattern","value");
  for (  Attribute attribute : attributes) {
    String attributeName=attribute.getName();
    if (!possibleAttributeNames.contains(attributeName)) {
      throw new PoshiRunnerException("Invalid " + attributeName + " attribute\n"+ filePath+ ":"+ element.attributeValue("line-number"));
    }
    if (!attributeName.equals("value") && Validator.isNull(attribute.getValue())) {
      throw new PoshiRunnerException("Missing " + attributeName + "attribute value in\n"+ filePath+ ":"+ element.attributeValue("line-number"));
    }
  }
  if (attributes.size() == 2) {
    if (Validator.isNull(element.getText())) {
      throw new PoshiRunnerException("Missing value attribute\n" + filePath + ":"+ element.attributeValue("line-number"));
    }
  }
}
