{
  List<Element> thenElements=element.elements("then");
  if (thenElements.isEmpty()) {
    throw new Exception("Missing then elements\n" + filePath + ":"+ element.attributeValue("line-number"));
  }
 else   if (thenElements.size() > 1) {
    throw new Exception("Too many then elements\n" + filePath + ":"+ element.attributeValue("line-number"));
  }
}
