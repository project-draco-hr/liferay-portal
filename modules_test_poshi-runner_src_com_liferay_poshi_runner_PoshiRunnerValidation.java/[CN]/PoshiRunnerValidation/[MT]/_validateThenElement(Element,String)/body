{
  List<Element> thenElements=element.elements("then");
  if (thenElements.isEmpty()) {
    _logExceptions.add(new Exception("Missing then elements\n" + filePath + ":"+ element.attributeValue("line-number")));
  }
 else   if (thenElements.size() > 1) {
    _logExceptions.add(new Exception("Too many then elements\n" + filePath + ":"+ element.attributeValue("line-number")));
  }
}
