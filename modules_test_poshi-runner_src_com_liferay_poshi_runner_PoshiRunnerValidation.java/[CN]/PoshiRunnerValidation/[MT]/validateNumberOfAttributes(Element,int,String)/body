{
  List<Attribute> attributes=element.attributes();
  if (attributes.isEmpty()) {
    _exceptions.add(new Exception("Missing attributes\n" + filePath + ":"+ element.attributeValue("line-number")));
  }
 else   if (attributes.size() > number) {
    _exceptions.add(new Exception("Too many attributes\n" + filePath + ":"+ element.attributeValue("line-number")));
  }
 else   if (attributes.size() < number) {
    _exceptions.add(new Exception("Too few attributes\n" + filePath + ":"+ element.attributeValue("line-number")));
  }
}
