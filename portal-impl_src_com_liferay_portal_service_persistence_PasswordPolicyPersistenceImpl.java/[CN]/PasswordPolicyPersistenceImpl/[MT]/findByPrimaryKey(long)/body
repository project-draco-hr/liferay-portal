{
  PasswordPolicy passwordPolicy=fetchByPrimaryKey(passwordPolicyId);
  if (passwordPolicy == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No PasswordPolicy exists with the primary key " + passwordPolicyId);
    }
    throw new NoSuchPasswordPolicyException("No PasswordPolicy exists with the primary key " + passwordPolicyId);
  }
  return passwordPolicy;
}
