{
  PasswordPolicy result=(PasswordPolicy)EntityCacheUtil.getResult(PasswordPolicyModelImpl.ENTITY_CACHE_ENABLED,PasswordPolicy.class,passwordPolicyId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      PasswordPolicy passwordPolicy=(PasswordPolicy)session.get(PasswordPolicyImpl.class,new Long(passwordPolicyId));
      if (passwordPolicy != null) {
        cacheResult(passwordPolicy);
      }
      return passwordPolicy;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (PasswordPolicy)result;
  }
}
