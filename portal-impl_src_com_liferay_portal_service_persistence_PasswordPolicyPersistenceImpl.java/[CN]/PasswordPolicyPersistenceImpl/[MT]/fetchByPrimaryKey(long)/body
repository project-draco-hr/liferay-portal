{
  PasswordPolicy passwordPolicy=(PasswordPolicy)EntityCacheUtil.getResult(PasswordPolicyModelImpl.ENTITY_CACHE_ENABLED,PasswordPolicyImpl.class,passwordPolicyId,this);
  if (passwordPolicy == null) {
    Session session=null;
    try {
      session=openSession();
      passwordPolicy=(PasswordPolicy)session.get(PasswordPolicyImpl.class,new Long(passwordPolicyId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (passwordPolicy != null) {
        cacheResult(passwordPolicy);
      }
      closeSession(session);
    }
  }
  return passwordPolicy;
}
