{
  passwordPolicy=toUnwrappedModel(passwordPolicy);
  Session session=null;
  try {
    session=openSession();
    if (passwordPolicy.isCachedModel()) {
      passwordPolicy=(PasswordPolicy)session.get(PasswordPolicyImpl.class,passwordPolicy.getPrimaryKeyObj());
    }
    session.delete(passwordPolicy);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(passwordPolicy);
  return passwordPolicy;
}
