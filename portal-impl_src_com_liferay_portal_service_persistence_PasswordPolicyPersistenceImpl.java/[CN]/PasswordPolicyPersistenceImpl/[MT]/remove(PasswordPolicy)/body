{
  Session session=null;
  try {
    session=openSession();
    session.delete(passwordPolicy);
    session.flush();
    return passwordPolicy;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(PasswordPolicy.class.getName());
  }
}
