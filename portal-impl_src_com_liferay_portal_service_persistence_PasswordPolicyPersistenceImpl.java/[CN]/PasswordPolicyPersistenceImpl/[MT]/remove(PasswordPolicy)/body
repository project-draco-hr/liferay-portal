{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(passwordPolicy);
    }
  }
  passwordPolicy=removeImpl(passwordPolicy);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(passwordPolicy);
    }
  }
  return passwordPolicy;
}
