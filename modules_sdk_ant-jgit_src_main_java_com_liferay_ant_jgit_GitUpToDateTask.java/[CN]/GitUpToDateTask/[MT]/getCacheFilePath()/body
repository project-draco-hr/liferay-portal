{
  Project currentProject=getProject();
  File baseDir=currentProject.getBaseDir();
  Path path=baseDir.toPath();
  while (path != null) {
    Path cacheFilePath=path.resolve(_cacheFileName);
    if (Files.exists(cacheFilePath)) {
      return cacheFilePath;
    }
    path=path.getParent();
  }
  return null;
}
