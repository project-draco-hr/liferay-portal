{
  DDMStructure structure=ddmStructurePersistence.findByPrimaryKey(structureId);
  return addStructure(userId,structure.getGroupId(),structure.getParentStructureId(),structure.getClassNameId(),null,nameMap,descriptionMap,structure.getDDMForm(),structure.getDDMFormLayout(),structure.getStorageType(),structure.getType(),serviceContext);
}
