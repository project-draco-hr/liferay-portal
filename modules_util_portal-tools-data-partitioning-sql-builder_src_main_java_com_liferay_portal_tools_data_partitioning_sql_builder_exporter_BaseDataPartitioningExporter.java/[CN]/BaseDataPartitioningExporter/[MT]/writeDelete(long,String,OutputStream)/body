{
  DataSource dataSource=getDataSource();
  String sql="select * from " + tableName + " where companyId = ?";
  try (Connection connection=dataSource.getConnection();PreparedStatement preparedStatement=buildPreparedStatement(connection,sql,companyId);ResultSet resultSet=preparedStatement.executeQuery()){
    if (resultSet.next()) {
      String deleteSql="delete from " + tableName + " where companyId = "+ companyId+ ";\n";
      outputStream.write(deleteSql.getBytes());
    }
  }
 catch (  IOException|SQLException e) {
    _logger.error("Unable to export " + tableName,e);
  }
}
