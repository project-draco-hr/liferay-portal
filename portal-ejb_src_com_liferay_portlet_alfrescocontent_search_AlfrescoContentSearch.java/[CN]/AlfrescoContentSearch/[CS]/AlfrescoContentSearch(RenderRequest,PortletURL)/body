{
  super(req,new AlfrescoContentDisplayTerms(req),new AlfrescoContentSearchTerms(req),DEFAULT_CUR_PARAM,DEFAULT_DELTA,iteratorURL,headerNames,EMPTY_RESULTS_MESSAGE);
  AlfrescoContentDisplayTerms displayTerms=(AlfrescoContentDisplayTerms)getDisplayTerms();
  iteratorURL.setParameter(AlfrescoContentDisplayTerms.NAME,displayTerms.getName());
}
