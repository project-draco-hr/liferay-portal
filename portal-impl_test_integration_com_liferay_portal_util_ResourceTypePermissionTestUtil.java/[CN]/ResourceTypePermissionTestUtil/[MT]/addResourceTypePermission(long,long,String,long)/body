{
  long resourceTypePermissionId=CounterLocalServiceUtil.increment(ResourceTypePermission.class.getName());
  ResourceTypePermission resourceTypePermission=ResourceTypePermissionLocalServiceUtil.createResourceTypePermission(resourceTypePermissionId);
  resourceTypePermission.setActionIds(actionIds);
  resourceTypePermission.setCompanyId(TestPropsValues.getCompanyId());
  resourceTypePermission.setGroupId(groupId);
  resourceTypePermission.setName(name);
  resourceTypePermission.setRoleId(roleId);
  return ResourceTypePermissionLocalServiceUtil.addResourceTypePermission(resourceTypePermission);
}
