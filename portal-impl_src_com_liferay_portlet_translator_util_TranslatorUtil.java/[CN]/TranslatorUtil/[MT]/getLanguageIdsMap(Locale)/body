{
  Map<String,String> languageIdsMap=new HashMap<String,String>();
  String[] languageIds=PrefsPropsUtil.getStringArray(PropsKeys.TRANSLATOR_LANGUAGES,StringPool.COMMA);
  for (  String languageId : languageIds) {
    languageIdsMap.put(languageId,LanguageUtil.get(locale,"language." + languageId));
  }
  SortedMap languageIdsSortedMap=new TreeMap<String,String>(new ValueComparator(languageIdsMap));
  languageIdsSortedMap.putAll(languageIdsMap);
  return languageIdsSortedMap;
}
