{
  downloadSampleURL(sampleDir,url,"/api/json");
  downloadSampleURL(sampleDir,url,"/logText/progressiveText");
  Properties properties=new Properties();
  processProgressiveText(sampleDir,new File(sampleDir,"/logText/progressiveText"),properties);
  JSONObject jsonObject=JenkinsResultsParserUtil.toJSONObject(JenkinsResultsParserUtil.getLocalURL(toURLString(new File(sampleDir,"/api/json"))),false);
  properties.setProperty("env.BUILD_URL",toURLString(sampleDir));
  properties.setProperty("top.level.result",jsonObject.getString("result"));
  _saveProperties(new File(sampleDir,"sample.properties"),properties);
}
