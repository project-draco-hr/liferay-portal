{
  if (((shippingLastName == null) && (_shippingLastName != null)) || ((shippingLastName != null) && (_shippingLastName == null)) || ((shippingLastName != null) && (_shippingLastName != null) && !shippingLastName.equals(_shippingLastName))) {
    if (!XSS_ALLOW_SHIPPINGLASTNAME) {
      shippingLastName=XSSUtil.strip(shippingLastName);
    }
    _shippingLastName=shippingLastName;
    setModified(true);
  }
}
