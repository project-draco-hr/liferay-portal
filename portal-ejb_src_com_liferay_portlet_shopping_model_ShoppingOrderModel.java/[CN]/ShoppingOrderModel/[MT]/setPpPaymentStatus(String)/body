{
  if (((ppPaymentStatus == null) && (_ppPaymentStatus != null)) || ((ppPaymentStatus != null) && (_ppPaymentStatus == null)) || ((ppPaymentStatus != null) && (_ppPaymentStatus != null) && !ppPaymentStatus.equals(_ppPaymentStatus))) {
    if (!XSS_ALLOW_PPPAYMENTSTATUS) {
      ppPaymentStatus=XSSUtil.strip(ppPaymentStatus);
    }
    _ppPaymentStatus=ppPaymentStatus;
    setModified(true);
  }
}
