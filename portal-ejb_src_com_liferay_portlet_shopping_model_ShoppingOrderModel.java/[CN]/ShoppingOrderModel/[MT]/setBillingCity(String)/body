{
  if (((billingCity == null) && (_billingCity != null)) || ((billingCity != null) && (_billingCity == null)) || ((billingCity != null) && (_billingCity != null) && !billingCity.equals(_billingCity))) {
    if (!XSS_ALLOW_BILLINGCITY) {
      billingCity=XSSUtil.strip(billingCity);
    }
    _billingCity=billingCity;
    setModified(true);
  }
}
