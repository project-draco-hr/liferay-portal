{
  if (((billingLastName == null) && (_billingLastName != null)) || ((billingLastName != null) && (_billingLastName == null)) || ((billingLastName != null) && (_billingLastName != null) && !billingLastName.equals(_billingLastName))) {
    if (!XSS_ALLOW_BILLINGLASTNAME) {
      billingLastName=XSSUtil.strip(billingLastName);
    }
    _billingLastName=billingLastName;
    setModified(true);
  }
}
