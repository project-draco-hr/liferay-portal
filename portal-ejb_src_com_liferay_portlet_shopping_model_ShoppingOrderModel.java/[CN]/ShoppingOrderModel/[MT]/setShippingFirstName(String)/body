{
  if (((shippingFirstName == null) && (_shippingFirstName != null)) || ((shippingFirstName != null) && (_shippingFirstName == null)) || ((shippingFirstName != null) && (_shippingFirstName != null) && !shippingFirstName.equals(_shippingFirstName))) {
    if (!XSS_ALLOW_SHIPPINGFIRSTNAME) {
      shippingFirstName=XSSUtil.strip(shippingFirstName);
    }
    _shippingFirstName=shippingFirstName;
    setModified(true);
  }
