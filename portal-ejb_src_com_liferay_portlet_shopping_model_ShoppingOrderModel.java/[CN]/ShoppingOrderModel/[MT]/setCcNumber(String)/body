{
  if (((ccNumber == null) && (_ccNumber != null)) || ((ccNumber != null) && (_ccNumber == null)) || ((ccNumber != null) && (_ccNumber != null) && !ccNumber.equals(_ccNumber))) {
    if (!XSS_ALLOW_CCNUMBER) {
      ccNumber=XSSUtil.strip(ccNumber);
    }
    _ccNumber=ccNumber;
    setModified(true);
  }
}
