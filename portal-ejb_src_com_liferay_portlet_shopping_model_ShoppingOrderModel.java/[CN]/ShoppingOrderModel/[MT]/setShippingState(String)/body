{
  if (((shippingState == null) && (_shippingState != null)) || ((shippingState != null) && (_shippingState == null)) || ((shippingState != null) && (_shippingState != null) && !shippingState.equals(_shippingState))) {
    if (!XSS_ALLOW_SHIPPINGSTATE) {
      shippingState=XSSUtil.strip(shippingState);
    }
    _shippingState=shippingState;
    setModified(true);
  }
}
