{
  if (((billingCountry == null) && (_billingCountry != null)) || ((billingCountry != null) && (_billingCountry == null)) || ((billingCountry != null) && (_billingCountry != null) && !billingCountry.equals(_billingCountry))) {
    if (!XSS_ALLOW_BILLINGCOUNTRY) {
      billingCountry=XSSUtil.strip(billingCountry);
    }
    _billingCountry=billingCountry;
  }
}
