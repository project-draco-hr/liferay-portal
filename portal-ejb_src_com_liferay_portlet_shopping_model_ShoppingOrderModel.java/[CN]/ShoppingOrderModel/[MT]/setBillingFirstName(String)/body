{
  if (((billingFirstName == null) && (_billingFirstName != null)) || ((billingFirstName != null) && (_billingFirstName == null)) || ((billingFirstName != null) && (_billingFirstName != null) && !billingFirstName.equals(_billingFirstName))) {
    if (!XSS_ALLOW_BILLINGFIRSTNAME) {
      billingFirstName=XSSUtil.strip(billingFirstName);
    }
    _billingFirstName=billingFirstName;
    setModified(true);
  }
}
