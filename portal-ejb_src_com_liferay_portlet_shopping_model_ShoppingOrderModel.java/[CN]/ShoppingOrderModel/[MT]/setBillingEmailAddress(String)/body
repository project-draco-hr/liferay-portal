{
  if (((billingEmailAddress == null) && (_billingEmailAddress != null)) || ((billingEmailAddress != null) && (_billingEmailAddress == null)) || ((billingEmailAddress != null) && (_billingEmailAddress != null) && !billingEmailAddress.equals(_billingEmailAddress))) {
    if (!XSS_ALLOW_BILLINGEMAILADDRESS) {
      billingEmailAddress=XSSUtil.strip(billingEmailAddress);
    }
    _billingEmailAddress=billingEmailAddress;
    setModified(true);
  }
}
