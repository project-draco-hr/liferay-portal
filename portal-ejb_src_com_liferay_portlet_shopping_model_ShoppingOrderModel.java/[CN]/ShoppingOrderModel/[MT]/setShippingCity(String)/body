{
  if (((shippingCity == null) && (_shippingCity != null)) || ((shippingCity != null) && (_shippingCity == null)) || ((shippingCity != null) && (_shippingCity != null) && !shippingCity.equals(_shippingCity))) {
    if (!XSS_ALLOW_SHIPPINGCITY) {
      shippingCity=XSSUtil.strip(shippingCity);
    }
    _shippingCity=shippingCity;
    setModified(true);
  }
}
