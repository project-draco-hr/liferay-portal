{
  if (((altShipping == null) && (_altShipping != null)) || ((altShipping != null) && (_altShipping == null)) || ((altShipping != null) && (_altShipping != null) && !altShipping.equals(_altShipping))) {
    if (!XSS_ALLOW_ALTSHIPPING) {
      altShipping=XSSUtil.strip(altShipping);
    }
    _altShipping=altShipping;
  }
}
