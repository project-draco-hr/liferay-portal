{
  if (((shippingEmailAddress == null) && (_shippingEmailAddress != null)) || ((shippingEmailAddress != null) && (_shippingEmailAddress == null)) || ((shippingEmailAddress != null) && (_shippingEmailAddress != null) && !shippingEmailAddress.equals(_shippingEmailAddress))) {
    if (!XSS_ALLOW_SHIPPINGEMAILADDRESS) {
      shippingEmailAddress=XSSUtil.strip(shippingEmailAddress);
    }
    _shippingEmailAddress=shippingEmailAddress;
    setModified(true);
  }
}
