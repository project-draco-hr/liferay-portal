{
  if (((ccVerNumber == null) && (_ccVerNumber != null)) || ((ccVerNumber != null) && (_ccVerNumber == null)) || ((ccVerNumber != null) && (_ccVerNumber != null) && !ccVerNumber.equals(_ccVerNumber))) {
    if (!XSS_ALLOW_CCVERNUMBER) {
      ccVerNumber=XSSUtil.strip(ccVerNumber);
    }
    _ccVerNumber=ccVerNumber;
  }
}
