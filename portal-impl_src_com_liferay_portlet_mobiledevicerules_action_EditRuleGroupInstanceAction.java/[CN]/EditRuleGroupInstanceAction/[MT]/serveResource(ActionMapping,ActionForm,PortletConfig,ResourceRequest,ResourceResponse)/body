{
  long ruleGroupId=ParamUtil.getLong(resourceRequest,"ruleGroupId");
  MDRRuleGroup ruleGroup=MDRRuleGroupLocalServiceUtil.fetchRuleGroup(ruleGroupId);
  resourceRequest.setAttribute(WebKeys.MOBILE_DEVICE_RULES_RULE_GROUP,ruleGroup);
  String className=ParamUtil.getString(resourceRequest,"className");
  long classPK=ParamUtil.getLong(resourceRequest,"classPK");
  MDRRuleGroupInstance ruleGroupInstance=MDRRuleGroupInstanceLocalServiceUtil.fetchRuleGroupInstance(className,classPK,ruleGroupId);
  resourceRequest.setAttribute(WebKeys.MOBILE_DEVICE_RULES_RULE_GROUP_INSTANCE,ruleGroupInstance);
  PortletContext portletContext=portletConfig.getPortletContext();
  PortletRequestDispatcher portletRequestDispatcher=portletContext.getRequestDispatcher(_EDITOR_JSP);
  portletRequestDispatcher.include(resourceRequest,resourceResponse);
}
