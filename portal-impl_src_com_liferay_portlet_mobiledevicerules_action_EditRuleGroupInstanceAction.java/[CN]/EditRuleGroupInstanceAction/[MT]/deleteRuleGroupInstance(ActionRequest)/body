{
  long ruleGroupInstanceId=ParamUtil.getLong(actionRequest,MDRPortletConstants.MDR_RULE_GROUP_INSTANCE_ID);
  MDRRuleGroupInstance ruleGroupInstance=MDRRuleGroupInstanceLocalServiceUtil.fetchRuleGroupInstance(ruleGroupInstanceId);
  MDRRuleGroupInstanceServiceUtil.deleteRuleGroupInstance(ruleGroupInstance);
}
