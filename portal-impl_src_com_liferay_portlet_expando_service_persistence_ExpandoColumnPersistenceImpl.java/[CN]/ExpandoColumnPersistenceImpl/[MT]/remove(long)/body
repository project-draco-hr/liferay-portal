{
  Session session=null;
  try {
    session=openSession();
    ExpandoColumn expandoColumn=(ExpandoColumn)session.get(ExpandoColumnImpl.class,new Long(columnId));
    if (expandoColumn == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + columnId);
      }
      throw new NoSuchColumnException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + columnId);
    }
    return remove(expandoColumn);
  }
 catch (  NoSuchColumnException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
