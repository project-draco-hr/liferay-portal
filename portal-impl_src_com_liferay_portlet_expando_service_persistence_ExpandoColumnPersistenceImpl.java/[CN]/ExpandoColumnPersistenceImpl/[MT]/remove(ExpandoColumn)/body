{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(expandoColumn);
    }
  }
  expandoColumn=removeImpl(expandoColumn);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(expandoColumn);
    }
  }
  return expandoColumn;
}
