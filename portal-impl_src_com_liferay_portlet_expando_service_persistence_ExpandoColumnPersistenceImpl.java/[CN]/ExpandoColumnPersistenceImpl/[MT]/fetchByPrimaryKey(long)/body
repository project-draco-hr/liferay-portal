{
  ExpandoColumn result=(ExpandoColumn)EntityCacheUtil.getResult(ExpandoColumnModelImpl.ENTITY_CACHE_ENABLED,ExpandoColumn.class,columnId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      ExpandoColumn expandoColumn=(ExpandoColumn)session.get(ExpandoColumnImpl.class,new Long(columnId));
      if (expandoColumn != null) {
        cacheResult(expandoColumn);
      }
      return expandoColumn;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (ExpandoColumn)result;
  }
}
