{
  ExpandoColumn expandoColumn=(ExpandoColumn)EntityCacheUtil.getResult(ExpandoColumnModelImpl.ENTITY_CACHE_ENABLED,ExpandoColumnImpl.class,columnId,this);
  if (expandoColumn == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (expandoColumn == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      expandoColumn=(ExpandoColumn)session.get(ExpandoColumnImpl.class,Long.valueOf(columnId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (expandoColumn == null)) {
        EntityCacheUtil.putResult(ExpandoColumnModelImpl.ENTITY_CACHE_ENABLED,ExpandoColumnImpl.class,columnId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(expandoColumn);
      }
      closeSession(session);
    }
  }
  return expandoColumn;
}
