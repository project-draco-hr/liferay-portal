{
  ExpandoColumn expandoColumn=(ExpandoColumn)EntityCacheUtil.getResult(ExpandoColumnModelImpl.ENTITY_CACHE_ENABLED,ExpandoColumnImpl.class,columnId);
  if (expandoColumn == _nullExpandoColumn) {
    return null;
  }
  if (expandoColumn == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      expandoColumn=(ExpandoColumn)session.get(ExpandoColumnImpl.class,Long.valueOf(columnId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (expandoColumn != null) {
        cacheResult(expandoColumn);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(ExpandoColumnModelImpl.ENTITY_CACHE_ENABLED,ExpandoColumnImpl.class,columnId,_nullExpandoColumn);
      }
      closeSession(session);
    }
  }
  return expandoColumn;
}
