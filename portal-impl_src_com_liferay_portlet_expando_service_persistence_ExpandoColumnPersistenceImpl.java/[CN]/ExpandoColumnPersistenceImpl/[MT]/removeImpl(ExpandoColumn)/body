{
  Session session=null;
  try {
    session=openSession();
    session.delete(expandoColumn);
    session.flush();
    return expandoColumn;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ExpandoColumn.class.getName());
  }
}
