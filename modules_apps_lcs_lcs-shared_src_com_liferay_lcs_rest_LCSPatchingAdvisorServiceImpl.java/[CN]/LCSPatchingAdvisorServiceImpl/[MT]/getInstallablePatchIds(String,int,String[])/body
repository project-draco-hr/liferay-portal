{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < installedPatchIds.length - 1; i++) {
    sb.append(installedPatchIds[i]);
    sb.append(",");
  }
  sb.append(installedPatchIds[installedPatchIds.length - 1]);
  return doGetToList(String.class,_URL_PATCHING_ADVISOR,"key",key,"patchingToolVersion",String.valueOf(patchingToolVersion),"installedPatchIds",sb.toString());
}
