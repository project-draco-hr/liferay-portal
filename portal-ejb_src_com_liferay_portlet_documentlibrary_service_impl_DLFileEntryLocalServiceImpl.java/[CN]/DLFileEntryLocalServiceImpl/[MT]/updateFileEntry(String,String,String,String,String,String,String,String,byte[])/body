{
  InputStream is=null;
  long size=0;
  if ((byteArray != null) && (byteArray.length > 0)) {
    is=new BufferedInputStream(new ByteArrayInputStream(byteArray));
    size=byteArray.length;
  }
  return updateFileEntry(userId,folderId,newFolderId,name,sourceFileName,title,description,extraSettings,is,size);
}
