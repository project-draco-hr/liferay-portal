{
  InputStream is=null;
  long size=0;
  if ((file != null) && (file.length() > 0)) {
    try {
      is=new BufferedInputStream(new FileInputStream(file));
      size=file.length();
    }
 catch (    FileNotFoundException fnfe) {
      throw new NoSuchFileException();
    }
  }
  return updateFileEntry(userId,folderId,newFolderId,name,sourceFileName,title,description,extraSettings,is,size);
}
