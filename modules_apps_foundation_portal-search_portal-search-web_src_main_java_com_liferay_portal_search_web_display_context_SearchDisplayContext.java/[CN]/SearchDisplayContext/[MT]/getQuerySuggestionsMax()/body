{
  if (_querySuggestionsMax != null) {
    return _querySuggestionsMax;
  }
  _querySuggestionsMax=GetterUtil.getInteger(_portletPreferences.getValue("querySuggestionsMax",null),_indexSearchPropsValues.getQuerySuggestionMax());
  if (_querySuggestionsMax <= 0) {
    _querySuggestionsMax=_indexSearchPropsValues.getQuerySuggestionMax();
  }
  return _querySuggestionsMax;
}
