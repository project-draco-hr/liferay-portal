{
  PortletPermissionUtil.check(getPermissionChecker(),plid,PortletKeys.TAGS_ADMIN,ActionKeys.ADD_VOCABULARY);
  Boolean addGuestPermissions=null;
  Boolean addCommunityPermissions=null;
  return tagsVocabularyLocalService.addVocabulary(getUserId(),groupId,name,folksonomy,addGuestPermissions,addCommunityPermissions,communityPermissions,guestPermissions);
}
