{
  List<TagsVocabulary> tagsVocabularies=tagsVocabularyLocalService.getCompanyVocabularies(companyId,folksonomy);
  Iterator<TagsVocabulary> itr=tagsVocabularies.iterator();
  while (itr.hasNext()) {
    TagsVocabulary tagVocabulary=itr.next();
    if (!TagsVocabularyPermission.contains(getPermissionChecker(),tagVocabulary,ActionKeys.VIEW)) {
      itr.remove();
    }
  }
  return tagsVocabularies;
}
