{
  PluginRepositoriesConfiguration pluginRepositoriesConfiguration=ConfigurableUtil.createConfigurable(PluginRepositoriesConfiguration.class,properties);
  if (!pluginRepositoriesConfiguration.enabled()) {
    return;
  }
  schedulerEntryImpl.setTrigger(TriggerFactoryUtil.createTrigger(getEventListenerClass(),getEventListenerClass(),pluginRepositoriesConfiguration.interval(),pluginRepositoriesConfiguration.timeUnit()));
  _schedulerEngineHelper.register(this,schedulerEntryImpl,DestinationNames.SCHEDULER_DISPATCH);
}
