{
  String portletName=getPortletName();
  if ((portletName.equals(PortletKeys.HIGHEST_RATED_ASSETS) && PropsValues.HIGHEST_RATED_ASSETS_SHOW_PERMISSIONS_ENABLED) || (portletName.equals(PortletKeys.MOST_VIEWED_ASSETS) && PropsValues.MOST_VIEWED_ASSETS_SHOW_PERMISSIONS_ENABLED) || !PropsValues.ASSET_PUBLISHER_SEARCH_WITH_INDEX) {
    return true;
  }
  return false;
}
