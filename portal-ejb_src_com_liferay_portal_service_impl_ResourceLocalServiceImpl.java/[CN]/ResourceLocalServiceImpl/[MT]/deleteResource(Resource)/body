{
  Iterator itr=PermissionUtil.findByResourceId(resource.getResourceId()).iterator();
  while (itr.hasNext()) {
    Permission permission=(Permission)itr.next();
    OrgGroupPermissionUtil.removeByPermissionId(permission.getPermissionId());
  }
  PermissionUtil.removeByResourceId(resource.getResourceId());
  ResourceUtil.remove(resource.getResourceId());
}
