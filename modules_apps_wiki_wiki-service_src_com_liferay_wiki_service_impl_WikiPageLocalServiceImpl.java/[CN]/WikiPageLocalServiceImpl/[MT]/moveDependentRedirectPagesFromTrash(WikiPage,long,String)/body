{
  List<WikiPage> pages=getRedirectPages(oldNodeId,true,redirectPageTrashTitle,WorkflowConstants.STATUS_IN_TRASH);
  for (  WikiPage page : pages) {
    page.setRedirectTitle(newRedirectPage.getTitle());
    wikiPagePersistence.update(page);
    if (page.isInTrashImplicitly()) {
      moveDependentFromTrash(page,newRedirectPage.getNodeId(),page.getParentTitle());
    }
  }
}
