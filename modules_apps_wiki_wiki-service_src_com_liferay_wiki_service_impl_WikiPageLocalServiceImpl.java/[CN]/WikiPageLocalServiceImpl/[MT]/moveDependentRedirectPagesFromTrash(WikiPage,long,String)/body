{
  List<WikiPage> dependentPages=getRedirectPages(oldRedirectPageNodeId,true,oldRedirectPageTrashTitle,WorkflowConstants.STATUS_IN_TRASH);
  for (  WikiPage dependentPage : dependentPages) {
    dependentPage.setRedirectTitle(newRedirectPage.getTitle());
    wikiPagePersistence.update(dependentPage);
    if (dependentPage.isInTrashImplicitly()) {
      moveDependentFromTrash(dependentPage,newRedirectPage.getNodeId(),dependentPage.getParentTitle());
    }
  }
}
