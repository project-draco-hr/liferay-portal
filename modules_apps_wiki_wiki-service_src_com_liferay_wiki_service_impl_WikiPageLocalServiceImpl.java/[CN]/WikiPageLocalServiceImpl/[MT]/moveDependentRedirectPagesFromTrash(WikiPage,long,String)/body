{
  List<WikiPage> redirectPages=getRedirectPages(nodeId,true,trashTitle,WorkflowConstants.STATUS_IN_TRASH);
  for (  WikiPage redirectPage : redirectPages) {
    redirectPage.setRedirectTitle(page.getTitle());
    wikiPagePersistence.update(redirectPage);
    if (redirectPage.isInTrashImplicitly()) {
      moveDependentFromTrash(redirectPage,page.getNodeId(),redirectPage.getParentTitle());
    }
  }
}
