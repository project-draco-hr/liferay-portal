{
  List<WikiPage> redirectPages=getRedirectPages(nodeId,true,trashTitle,WorkflowConstants.STATUS_IN_TRASH);
  for (  WikiPage curPage : redirectPages) {
    curPage.setRedirectTitle(title);
    wikiPagePersistence.update(curPage);
    if (curPage.isInTrashImplicitly()) {
      moveDependentFromTrash(curPage,page.getNodeId(),curPage.getParentTitle());
    }
  }
}
