{
  List<WikiPage> dependentPages=wikiPagePersistence.findByN_H_P(parentNodeId,true,parentTitle);
  for (  WikiPage dependentPage : dependentPages) {
    dependentPage.setParentTitle(parentTrashTitle);
    wikiPagePersistence.update(dependentPage);
    if (!dependentPage.isInTrash()) {
      moveDependentToTrash(dependentPage,trashEntryId,createTrashVersion);
    }
  }
}
