{
  Indexer<WikiPage> indexer=IndexerRegistryUtil.nullSafeGetIndexer(WikiPage.class);
  List<WikiPage> childPages=wikiPagePersistence.findByN_P(nodeId,title);
  for (  WikiPage childPage : childPages) {
    childPage=doChangeNode(userId,nodeId,childPage.getTitle(),newNodeId,serviceContext);
    indexer.reindex(childPage);
  }
}
