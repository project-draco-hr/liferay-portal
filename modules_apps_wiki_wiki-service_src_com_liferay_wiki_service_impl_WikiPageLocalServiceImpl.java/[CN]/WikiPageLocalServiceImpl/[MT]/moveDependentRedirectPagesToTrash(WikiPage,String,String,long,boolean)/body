{
  List<WikiPage> redirectPages=wikiPagePersistence.findByN_H_R(page.getNodeId(),true,title);
  for (  WikiPage redirectPage : redirectPages) {
    redirectPage.setRedirectTitle(trashTitle);
    wikiPagePersistence.update(redirectPage);
    if (!redirectPage.isInTrash()) {
      moveDependentToTrash(redirectPage,trashEntryId,createTrashVersion);
    }
  }
}
