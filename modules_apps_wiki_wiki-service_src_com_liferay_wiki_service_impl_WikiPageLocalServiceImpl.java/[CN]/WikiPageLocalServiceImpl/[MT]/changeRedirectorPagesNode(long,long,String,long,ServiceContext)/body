{
  List<WikiPage> redirectorPages=getRedirectorPages(nodeId,title);
  Indexer<WikiPage> indexer=IndexerRegistryUtil.nullSafeGetIndexer(WikiPage.class);
  for (  WikiPage redirectorPage : redirectorPages) {
    redirectorPage=doChangeNode(userId,nodeId,redirectorPage.getTitle(),newNodeId,serviceContext);
    indexer.reindex(redirectorPage);
  }
}
