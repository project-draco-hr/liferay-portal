{
  Set layoutTemplateIds=new HashSet();
  if (xml == null) {
    return layoutTemplateIds;
  }
  SAXReader reader=SAXReaderFactory.getInstance();
  Document doc=reader.read(new StringReader(xml));
  Element root=doc.getRootElement();
  Element standardEl=root.element("standard");
  if (standardEl != null) {
    readLayoutTemplate(servletContextName,ctx,layoutTemplateIds,standardEl,true,null);
  }
  Element customEl=root.element("custom");
  if (customEl != null) {
    readLayoutTemplate(servletContextName,ctx,layoutTemplateIds,customEl,false,null);
  }
  return layoutTemplateIds;
}
