{
  List<SchedulerRequest> schedulerRequests=QuartzSchedulerEngineUtil.getScheduledJobs(schedulerRequest.getGroupName());
  JSONObject jsonObj=new JSONObject();
  jsonObj.put("lfrResponseId",responseId);
  jsonObj.put("schedulerRequests",JSONUtil.serialize(schedulerRequests));
  MessageBusUtil.sendMessage(responseDestination,jsonObj.toString());
}
