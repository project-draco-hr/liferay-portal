{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/document-library-shortcut-community/");
  selenium.clickAt("link=Document Library Page",RuntimeVariables.replace("Document Library Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Test2 Folder2"),selenium.getText("//a/strong[contains(.,'Test2 Folder2')]"));
  selenium.clickAt("//a/strong[contains(.,'Test2 Folder2')]",RuntimeVariables.replace("Test2 Folder2"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//td[1]/a","Test1 Document1.txt"));
  assertTrue(selenium.isPartialText("//td[1]/a","This is test1 document1."));
}
