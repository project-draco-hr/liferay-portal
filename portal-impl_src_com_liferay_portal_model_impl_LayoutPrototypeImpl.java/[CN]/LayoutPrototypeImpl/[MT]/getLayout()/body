{
  Layout layout=null;
  Group group=getGroup();
  try {
    if ((group != null) && (group.getPrivateLayoutsPageCount() > 0)) {
      List<Layout> layouts=LayoutLocalServiceUtil.getLayouts(group.getGroupId(),true);
      layout=layouts.get(0);
    }
  }
 catch (  Exception e) {
  }
  return layout;
}
