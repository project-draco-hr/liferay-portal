{
  PropertyImpl otherPropertyImpl=(PropertyImpl)other;
  String otherPropertyName=otherPropertyImpl.getWrappedProperty().getPropertyName();
  String[] arguments=new String[]{_property.getPropertyName() + "-GEPROPERTY-" + otherPropertyName};
  return new CriterionImpl(_property.geProperty(otherPropertyImpl.getWrappedProperty()),arguments);
}
