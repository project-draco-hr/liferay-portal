{
  PropertyImpl otherPropertyImpl=(PropertyImpl)other;
  String otherPropertyName=otherPropertyImpl.getWrappedProperty().getPropertyName();
  String[] arguments=new String[]{_property.getPropertyName() + "-LTPROPERTY-" + otherPropertyName};
  return new CriterionImpl(_property.ltProperty(otherPropertyImpl.getWrappedProperty()),arguments);
}
