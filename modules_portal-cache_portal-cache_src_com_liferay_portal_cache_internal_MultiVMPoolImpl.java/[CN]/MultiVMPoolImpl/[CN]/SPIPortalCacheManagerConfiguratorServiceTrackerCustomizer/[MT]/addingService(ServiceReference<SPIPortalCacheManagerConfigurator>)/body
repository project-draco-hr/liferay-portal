{
  _originalPortalCacheManager=_portalCacheManager;
  _spiPortalCacheManagerConfigurator=_bundleContext.getService(serviceReference);
  try {
    _portalCacheManager=_spiPortalCacheManagerConfigurator.createSPIPortalCacheManager(_portalCacheManager);
  }
 catch (  Exception e) {
    _log.error("Unable to create SPI portal cache manager",e);
  }
  _portalCacheManager.clearAll();
  return _spiPortalCacheManagerConfigurator;
}
