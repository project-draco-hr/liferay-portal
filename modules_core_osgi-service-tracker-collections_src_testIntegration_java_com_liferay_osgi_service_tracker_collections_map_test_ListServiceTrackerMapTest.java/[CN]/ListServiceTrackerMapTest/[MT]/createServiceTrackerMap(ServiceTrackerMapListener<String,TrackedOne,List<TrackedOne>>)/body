{
  ServiceTrackerMap<String,List<TrackedOne>> serviceTrackerMap=ServiceTrackerMapFactory.multiValueMap(_bundleContext,TrackedOne.class,null,new PropertyServiceReferenceMapper<String,TrackedOne>("target"),new DefaultServiceTrackerCustomizer<TrackedOne>(_bundleContext),new PropertyServiceReferenceComparator<TrackedOne>("service.ranking"),serviceTrackerMapListener);
  serviceTrackerMap.open();
