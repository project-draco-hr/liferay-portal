{
  try {
    com.liferay.portlet.shopping.model.ShoppingCoupon returnValue=ShoppingCouponServiceUtil.updateCoupon(couponId,name,description,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,endDateHour,endDateMinute,neverExpire,active,limitCategories,limitSkus,minOrder,discount,discountType,serviceContext);
