{
  try {
    SearchContainerRowTag<R> parentTag=(SearchContainerRowTag<R>)findAncestorWithClass(this,SearchContainerRowTag.class);
    ResultRow row=parentTag.getRow();
    if (index <= -1) {
      index=row.getEntries().size();
    }
    row.addButton(index,getAlign(),getValign(),getColspan(),getName(),(String)getHref());
    return EVAL_PAGE;
  }
  finally {
    index=-1;
    if (!ServerDetector.isResin()) {
      align=SearchEntry.DEFAULT_ALIGN;
      colspan=SearchEntry.DEFAULT_COLSPAN;
      _href=null;
      name=StringPool.BLANK;
      valign=SearchEntry.DEFAULT_VALIGN;
    }
  }
}
