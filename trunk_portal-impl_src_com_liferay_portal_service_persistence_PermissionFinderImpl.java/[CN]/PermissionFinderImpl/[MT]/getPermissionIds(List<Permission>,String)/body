{
  if (permissions.isEmpty()) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(permissions.size() * 3 - 1);
  for (int i=0; i < permissions.size(); i++) {
    sb.append(table);
    sb.append(".permissionId = ?");
    if ((i + 1) < permissions.size()) {
      sb.append(" OR ");
    }
  }
  return sb.toString();
}
