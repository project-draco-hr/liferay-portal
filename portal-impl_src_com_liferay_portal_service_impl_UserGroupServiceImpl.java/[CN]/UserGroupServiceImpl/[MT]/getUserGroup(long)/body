{
  if (!UserGroupPermission.contains(getPermissionChecker(),userGroupId,ActionKeys.VIEW) && !UserGroupPermission.contains(getPermissionChecker(),userGroupId,ActionKeys.UPDATE) && !UserGroupPermission.contains(getPermissionChecker(),userGroupId,ActionKeys.ASSIGN_USERS)) {
    throw new PrincipalException();
  }
  return UserGroupLocalServiceUtil.getUserGroup(userGroupId);
}
