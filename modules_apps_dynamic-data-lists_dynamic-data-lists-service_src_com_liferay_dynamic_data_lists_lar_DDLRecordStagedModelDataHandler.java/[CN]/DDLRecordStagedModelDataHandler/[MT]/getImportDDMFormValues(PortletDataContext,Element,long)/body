{
  DDLRecordSet recordSet=DDLRecordSetLocalServiceUtil.getRecordSet(recordSetId);
  DDMStructure ddmStructure=recordSet.getDDMStructure();
  String ddmFormValuesPath=recordElement.attributeValue("ddm-form-values-path");
  String serializedDDMFormValues=portletDataContext.getZipEntryAsString(ddmFormValuesPath);
  return DDMFormValuesJSONDeserializerUtil.deserialize(ddmStructure.getDDMForm(),serializedDDMFormValues);
}
