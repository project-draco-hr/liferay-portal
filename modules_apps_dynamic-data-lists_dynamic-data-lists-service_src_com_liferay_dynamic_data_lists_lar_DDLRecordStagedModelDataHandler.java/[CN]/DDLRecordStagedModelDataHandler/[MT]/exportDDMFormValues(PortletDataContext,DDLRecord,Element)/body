{
  String ddmFormValuesPath=ExportImportPathUtil.getModelPath(record,"ddm-form-values.json");
  recordElement.addAttribute("ddm-form-values-path",ddmFormValuesPath);
  DDMFormValues ddmFormValues=StorageEngineUtil.getDDMFormValues(record.getDDMStorageId());
  portletDataContext.addZipEntry(ddmFormValuesPath,DDMFormValuesJSONSerializerUtil.serialize(ddmFormValues));
}
