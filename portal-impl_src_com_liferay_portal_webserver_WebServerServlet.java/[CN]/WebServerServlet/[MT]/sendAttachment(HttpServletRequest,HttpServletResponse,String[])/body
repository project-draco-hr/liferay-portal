{
  DLFileEntry dlFileEntry=getAttachmentFileEntry(request,pathArray);
  if (dlFileEntry == null) {
    return;
  }
  DLFileVersion dlFileVersion=dlFileEntry.getFileVersion();
  String fileName=HttpUtil.decodeURL(HtmlUtil.escape(pathArray[2]),true);
  if (dlFileVersion.isInTrash()) {
    fileName=TrashUtil.getOriginalTitle(fileName);
  }
  ServletResponseUtil.sendFile(request,response,fileName,dlFileEntry.getContentStream(),dlFileEntry.getSize(),dlFileEntry.getMimeType());
}
