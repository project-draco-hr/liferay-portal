{
  PropertyResourceBundle propertyResourceBundle=new PropertyResourceBundle(url.openStream());
  Dictionary<String,Object> properties=new Hashtable<>();
  String languageId=StringPool.BLANK;
  String name=url.getFile();
  if (name.contains(StringPool.UNDERLINE)) {
    int x=name.indexOf(StringPool.UNDERLINE) + 1;
    int y=name.indexOf(".properties");
    languageId=name.substring(x,y);
  }
  properties.put("javax.portlet.name",PortalSettingsPortletKeys.PORTAL_SETTINGS);
  properties.put("language.id",languageId);
  ServiceRegistration<ResourceBundle> serviceRegistration=bundleContext.registerService(ResourceBundle.class,propertyResourceBundle,properties);
  _serviceRegistrations.add(serviceRegistration);
}
