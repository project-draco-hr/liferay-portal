{
  Map<String,Serializable> attributeValues=null;
  try {
    if (secure) {
      attributeValues=ExpandoValueServiceUtil.getData(_companyId,_className,ExpandoTableConstants.DEFAULT_TABLE_NAME,names,_classPK);
    }
 else {
      attributeValues=ExpandoValueLocalServiceUtil.getData(_companyId,_className,ExpandoTableConstants.DEFAULT_TABLE_NAME,names,_classPK);
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return attributeValues;
}
