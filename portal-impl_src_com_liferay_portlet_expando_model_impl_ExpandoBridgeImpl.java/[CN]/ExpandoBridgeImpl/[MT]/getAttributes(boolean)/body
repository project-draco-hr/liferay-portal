{
  Map<String,Serializable> attributes=new HashMap<String,Serializable>();
  List<ExpandoColumn> columns=new ArrayList<ExpandoColumn>();
  try {
    columns=ExpandoColumnLocalServiceUtil.getDefaultTableColumns(_companyId,_className);
  }
 catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      _log.debug(e,e);
    }
  }
  for (  ExpandoColumn column : columns) {
    attributes.put(column.getName(),getAttribute(column.getName(),secure));
  }
  return attributes;
}
