{
  Serializable data=null;
  try {
    if (secure) {
      data=ExpandoValueServiceUtil.getData(_companyId,_className,ExpandoTableConstants.DEFAULT_TABLE_NAME,name,_classPK);
    }
 else {
      data=ExpandoValueLocalServiceUtil.getData(_companyId,_className,ExpandoTableConstants.DEFAULT_TABLE_NAME,name,_classPK);
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return data;
}
