{
  if (entry.getFolderId() != BookmarksFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    StagedModelDataHandlerUtil.exportReferenceStagedModel(portletDataContext,entry,entry.getFolder(),PortletDataContext.REFERENCE_TYPE_PARENT);
  }
  Element entryElement=portletDataContext.getExportDataElement(entry);
  portletDataContext.addClassedModel(entryElement,ExportImportPathUtil.getModelPath(entry),entry);
}
