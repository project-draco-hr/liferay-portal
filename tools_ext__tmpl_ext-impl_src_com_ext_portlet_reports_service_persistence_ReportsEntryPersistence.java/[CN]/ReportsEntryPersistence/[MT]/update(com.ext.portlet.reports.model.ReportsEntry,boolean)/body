{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(reportsEntry);
    }
 else {
      if (reportsEntry.isNew()) {
        session.save(reportsEntry);
      }
    }
    session.flush();
    reportsEntry.setNew(false);
    return reportsEntry;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
