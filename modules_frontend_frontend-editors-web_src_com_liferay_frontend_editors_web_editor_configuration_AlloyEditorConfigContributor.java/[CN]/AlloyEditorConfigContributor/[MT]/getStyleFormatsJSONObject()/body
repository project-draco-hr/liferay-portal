  stylesJsonArray.put(getStyleFormatJSONObject("Preformatted Text","pre",null,_CKEDITOR_STYLE_BLOCK));
  stylesJsonArray.put(getStyleFormatJSONObject("Cited Work","cite",null,_CKEDITOR_STYLE_INLINE));
  stylesJsonArray.put(getStyleFormatJSONObject("Computer Code","code",null,_CKEDITOR_STYLE_INLINE));
