{
  DDMStructure ddmStructure=DDMStructureTestUtil.addStructure(_group.getGroupId(),DLFileEntryMetadata.class.getName());
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId(),_user.getUserId());
  return DLFileEntryTypeLocalServiceUtil.addFileEntryType(_user.getUserId(),_group.getGroupId(),_DL_FILE_ENTRY_TYPE_NAME,RandomTestUtil.randomString(),new long[]{ddmStructure.getStructureId()},serviceContext);
}
