{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Unit Converter Test Page");
  selenium.clickAt("link=Unit Converter Test Page",RuntimeVariables.replace("Unit Converter Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@name='_27_fromValue']",RuntimeVariables.replace("1.0"));
  selenium.select("//select[@name='_27_fromId']",RuntimeVariables.replace("Inch"));
  selenium.select("//select[@name='_27_toId']",RuntimeVariables.replace("Centimeter"));
  selenium.select("//select[@name='_27_type']",RuntimeVariables.replace("Length"));
  selenium.clickAt("//input[@value='Convert']",RuntimeVariables.replace("Convert"));
  selenium.waitForValue("//input[@name='_27_to_value']","2.5399999187200026");
  assertEquals("2.5399999187200026",selenium.getValue("//input[@name='_27_to_value']"));
}
