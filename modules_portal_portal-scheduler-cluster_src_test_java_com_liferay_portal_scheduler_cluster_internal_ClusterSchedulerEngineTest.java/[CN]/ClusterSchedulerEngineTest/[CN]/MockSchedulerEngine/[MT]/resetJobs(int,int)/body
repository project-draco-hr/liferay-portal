{
  _defaultJobs.clear();
  for (int i=0; i < memoryClusterJobs; i++) {
    _addJobs(_TEST_JOB_NAME_PREFIX.concat(String.valueOf(i)),_MEMORY_CLUSTER_TEST_GROUP_NAME,StorageType.MEMORY_CLUSTERED,null,null);
  }
  for (int i=0; i < persistentJobs; i++) {
    _addJobs(_TEST_JOB_NAME_PREFIX.concat(String.valueOf(i)),_PERSISTENT_TEST_GROUP_NAME,StorageType.PERSISTED,null,null);
  }
}
