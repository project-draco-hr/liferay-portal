{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(PortalUtil.getControlPanelPlid(liferayPortletRequest),PortletKeys.BOOKMARKS,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/bookmarks/edit_entry");
  portletURL.setParameter("folderId",String.valueOf(_entry.getFolderId()));
  portletURL.setParameter("entryId",String.valueOf(_entry.getEntryId()));
  return portletURL;
}
