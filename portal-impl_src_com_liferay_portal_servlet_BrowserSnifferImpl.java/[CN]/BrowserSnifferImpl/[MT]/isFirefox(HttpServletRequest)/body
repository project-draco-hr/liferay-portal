{
  if (!isMozilla(request)) {
    return false;
  }
  String userAgent=getUserAgent(request);
  Pattern pattern=Pattern.compile("(firefox|minefield|granparadiso|bonecho|firebird|phoenix|camino)");
  Matcher matcher=pattern.matcher(userAgent);
  if (matcher.find()) {
    return true;
  }
  return false;
}
