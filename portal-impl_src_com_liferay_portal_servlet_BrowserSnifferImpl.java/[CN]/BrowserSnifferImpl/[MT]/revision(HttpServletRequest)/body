{
  String _revision=null;
  String agent=_getUserAgent(request);
  Pattern pattern=Pattern.compile(".+(?:rv|it|ra|ie)[\\/: ]([\\d.]+)");
  Matcher matcher=pattern.matcher(agent);
  while (matcher.find()) {
    for (int i=1; i <= matcher.groupCount(); ++i) {
      _revision=matcher.group(i);
    }
  }
  return _revision;
}
