{
  float majorVersion=getMajorVersion(request);
  if (isIe(request) && (majorVersion >= 5.5)) {
    return true;
  }
  if (isMozilla(request) && (majorVersion >= 1.3)) {
    return true;
  }
  if (isSafari(request) && (majorVersion >= 3.0) && !isMobile(request)) {
    return true;
  }
  return false;
}
