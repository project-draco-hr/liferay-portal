{
  if (request == null) {
    return false;
  }
  String agent=request.getHeader(HttpHeaders.USER_AGENT);
  if (agent == null) {
    return false;
  }
  agent=agent.toLowerCase();
  if (is_safari(request) && (agent.indexOf("version/3.") != -1)) {
    return true;
  }
 else {
    return false;
  }
}
