{
  User organizationUser=addUser(RandomTestUtil.randomString(),organization.getGroupId());
  long[] userIds={organizationUser.getUserId()};
  UserLocalServiceUtil.addOrganizationUsers(organization.getOrganizationId(),userIds);
  Role role=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),roleName);
  UserGroupRoleLocalServiceUtil.addUserGroupRoles(userIds,organization.getGroupId(),role.getRoleId());
  return organizationUser;
}
