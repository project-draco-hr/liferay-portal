{
  User user=addUser();
  Company defaultCompany=CompanyLocalServiceUtil.getCompanyByMx(PropsUtil.get(PropsKeys.COMPANY_DEFAULT_WEB_ID));
  user.setCompanyId(defaultCompany.getCompanyId());
  UserLocalServiceUtil.updateUser(user);
  Role administratorRole=RoleLocalServiceUtil.getRole(defaultCompany.getCompanyId(),RoleConstants.ADMINISTRATOR);
  UserLocalServiceUtil.addRoleUser(administratorRole.getRoleId(),user);
  return user;
}
