{
  Dictionary<String,Object> properties=new Hashtable<>();
  properties.put(HttpWhiteboardConstants.HTTP_WHITEBOARD_CONTEXT_SELECT,createContextSelectFilterString());
  properties.put(HttpWhiteboardConstants.HTTP_WHITEBOARD_LISTENER,Boolean.TRUE.toString());
  return _bundleContext.registerService(ServletContextListener.class,_customServletContextHelper,properties);
}
