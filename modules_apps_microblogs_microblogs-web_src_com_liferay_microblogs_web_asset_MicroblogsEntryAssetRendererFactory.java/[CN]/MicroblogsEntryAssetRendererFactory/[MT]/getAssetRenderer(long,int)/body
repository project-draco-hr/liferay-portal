{
  MicroblogsEntry microblogsEntry=_microblogsEntryLocalService.getMicroblogsEntry(classPK);
  MicroblogsEntryAssetRenderer microblogsEntryAssetRenderer=new MicroblogsEntryAssetRenderer(microblogsEntry);
  microblogsEntryAssetRenderer.setServletContext(_servletContext);
  return microblogsEntryAssetRenderer;
}
