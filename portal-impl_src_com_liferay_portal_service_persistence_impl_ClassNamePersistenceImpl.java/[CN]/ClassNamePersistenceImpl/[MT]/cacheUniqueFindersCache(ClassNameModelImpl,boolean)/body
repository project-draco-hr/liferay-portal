{
  if (isNew) {
    Object[] args=new Object[]{classNameModelImpl.getValue()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_VALUE,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_VALUE,args,classNameModelImpl);
  }
 else {
    if ((classNameModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_VALUE.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{classNameModelImpl.getValue()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_VALUE,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_VALUE,args,classNameModelImpl);
    }
  }
}
