{
  if (_fileVersionIds.contains(destinationFileVersion.getFileVersionId())) {
    return;
  }
  boolean generateImages=false;
  String extension=destinationFileVersion.getExtension();
  if (extension.equals("pdf")) {
    generateImages=true;
  }
 else   if (DocumentConversionUtil.isEnabled()) {
    Set<String> conversions=DocumentConversionUtil.getConversions(extension);
    if (conversions.contains("pdf")) {
      generateImages=true;
    }
  }
  if (generateImages) {
    _fileVersionIds.add(destinationFileVersion.getFileVersionId());
    sendGenerationMessage(DestinationNames.DOCUMENT_LIBRARY_PDF_PROCESSOR,PropsValues.DL_FILE_ENTRY_PROCESSORS_TRIGGER_SYNCHRONOUSLY,sourceFileVersion,destinationFileVersion);
  }
}
