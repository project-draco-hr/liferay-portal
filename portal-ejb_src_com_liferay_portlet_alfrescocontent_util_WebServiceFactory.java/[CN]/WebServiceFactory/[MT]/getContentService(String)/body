{
  if (_CONTENT_SERVICE == null) {
    try {
      ContentServiceLocator locator=new ContentServiceLocator(AuthenticationUtils.getEngineConfiguration());
      locator.setContentServiceEndpointAddress(alfrescoWebClientURL + _CONTENT_SERVICE_ADDRESS);
      _CONTENT_SERVICE=(ContentServiceSoapBindingStub)locator.getContentService();
    }
 catch (    ServiceException jre) {
      if (logger.isDebugEnabled() == true) {
        if (jre.getLinkedCause() != null) {
          jre.getLinkedCause().printStackTrace();
        }
      }
      throw new WebServiceException("Error creating content service: " + jre.getMessage(),jre);
    }
    _CONTENT_SERVICE.setTimeout(60000);
  }
  return _CONTENT_SERVICE;
}
