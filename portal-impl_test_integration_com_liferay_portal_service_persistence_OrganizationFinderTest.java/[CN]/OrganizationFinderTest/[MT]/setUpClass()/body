{
  _group=GroupTestUtil.addGroup();
  _organization=OrganizationTestUtil.addOrganization();
  _user=UserTestUtil.addUser();
  GroupLocalServiceUtil.addUserGroup(_user.getUserId(),_group);
  OrganizationLocalServiceUtil.addGroupOrganization(_group.getGroupId(),_organization);
  OrganizationLocalServiceUtil.addUserOrganization(_user.getUserId(),_organization);
}
