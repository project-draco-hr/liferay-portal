{
  for (  BlogsEntryFileEntryReference blogsEntryFileEntryReference : blogsEntryFileEntryReferences) {
    StringBundler sb=new StringBundler(5);
    sb.append("<img.*");
    sb.append(EditorConstants.ATTRIBUTE_DATA_IMAGE_ID);
    sb.append("=\\s?\"");
    sb.append(blogsEntryFileEntryReference.getTempFileEntryId());
    sb.append("\".*src=\\s?\"(.*)\".*/>");
    content=content.replaceAll(sb.toString(),getFileEntryLink(blogsEntryFileEntryReference.getFileEntry()));
  }
  return content;
}
