{
  MailingListRequest mailingListRequest=(MailingListRequest)JSONFactoryUtil.deserialize(message);
  Folder folder=null;
  Message[] messages=null;
  try {
    folder=getFolder(mailingListRequest);
    messages=folder.getMessages();
    processMessages(messages);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
 finally {
    if ((folder != null) && folder.isOpen()) {
      try {
        folder.setFlags(messages,new Flags(Flags.Flag.DELETED),true);
      }
 catch (      Exception e) {
      }
      try {
        folder.close(true);
      }
 catch (      Exception e) {
      }
    }
  }
}
