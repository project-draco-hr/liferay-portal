{
  MailingListRequest mailingListRequest=(MailingListRequest)JSONFactoryUtil.deserialize(message);
  Folder folder=null;
  Message[] mailMessages=null;
  try {
    folder=openInbox(mailingListRequest);
    mailMessages=folder.getMessages();
    for (    Message mailMessage : mailMessages) {
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
 finally {
    if ((folder != null) && folder.isOpen()) {
      try {
        folder.setFlags(mailMessages,new Flags(Flags.Flag.DELETED),true);
      }
 catch (      Exception e) {
      }
      try {
        folder.close(true);
      }
 catch (      Exception e) {
      }
    }
  }
}
