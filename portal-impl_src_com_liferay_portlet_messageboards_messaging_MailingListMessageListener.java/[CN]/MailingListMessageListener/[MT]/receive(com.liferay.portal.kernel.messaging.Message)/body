{
  MailingListRequest mailingListRequest=(MailingListRequest)message.getPayload();
  Store store=null;
  Folder folder=null;
  Message[] messages=null;
  try {
    store=getStore(mailingListRequest);
    store.connect();
    folder=getFolder(store);
    messages=folder.getMessages();
    processMessages(mailingListRequest,messages);
  }
 catch (  Exception e) {
    _log.error(e,e);
    message.put(SchedulerEngine.EXCEPTION,e);
  }
 finally {
    if ((folder != null) && folder.isOpen()) {
      try {
        folder.setFlags(messages,new Flags(Flags.Flag.DELETED),true);
      }
 catch (      Exception e) {
      }
      try {
        folder.close(true);
      }
 catch (      Exception e) {
      }
    }
    if ((store != null) && store.isConnected()) {
      try {
        store.close();
      }
 catch (      MessagingException e) {
      }
    }
  }
}
