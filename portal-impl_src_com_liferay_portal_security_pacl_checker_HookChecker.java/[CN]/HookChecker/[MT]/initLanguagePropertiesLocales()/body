{
  _languagePropertiesLanguageIds=getPropertySet("security-manager-hook-language-properties-locales");
  if (_log.isDebugEnabled()) {
    Set<String> languageIds=new TreeSet<String>(_languagePropertiesLanguageIds);
    for (    String languageId : languageIds) {
      _log.debug("Allowing language " + languageId);
    }
  }
}
