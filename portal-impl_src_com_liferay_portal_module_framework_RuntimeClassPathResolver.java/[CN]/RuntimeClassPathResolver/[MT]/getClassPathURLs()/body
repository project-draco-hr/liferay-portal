{
  File coreDir=new File(PropsValues.MODULE_FRAMEWORK_BASE_DIR,"core");
  File[] files=coreDir.listFiles();
  if (files == null) {
    throw new IllegalStateException("Missing " + coreDir.getCanonicalPath());
  }
  URL[] urls=new URL[files.length];
  for (int i=0; i < urls.length; i++) {
    String path=StringUtil.replace(files[i].getAbsolutePath(),CharPool.BACK_SLASH,CharPool.SLASH);
    if (!path.startsWith(StringPool.SLASH)) {
      path=StringPool.SLASH + path;
    }
    urls[i]=new URL("file",null,path);
  }
  return urls;
}
