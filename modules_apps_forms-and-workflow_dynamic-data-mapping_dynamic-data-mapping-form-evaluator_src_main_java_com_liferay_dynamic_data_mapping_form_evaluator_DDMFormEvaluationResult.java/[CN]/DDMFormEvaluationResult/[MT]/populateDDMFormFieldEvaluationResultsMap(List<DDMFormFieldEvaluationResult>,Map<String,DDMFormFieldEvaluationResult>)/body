{
  for (  DDMFormFieldEvaluationResult ddmFormFieldEvaluationResult : ddmFormFieldEvaluationResults) {
    String key=_getKey(ddmFormFieldEvaluationResult.getName(),ddmFormFieldEvaluationResult.getInstanceId());
    ddmFormFieldEvaluationResultsMap.put(key,ddmFormFieldEvaluationResult);
    populateDDMFormFieldEvaluationResultsMap(ddmFormFieldEvaluationResult.getNestedDDMFormFieldEvaluationResults(),ddmFormFieldEvaluationResultsMap);
  }
}
