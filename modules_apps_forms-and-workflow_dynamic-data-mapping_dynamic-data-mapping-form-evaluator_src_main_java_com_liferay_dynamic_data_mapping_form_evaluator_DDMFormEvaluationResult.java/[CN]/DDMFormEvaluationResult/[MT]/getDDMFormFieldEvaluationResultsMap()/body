{
  if (_ddmFormFieldEvaluationResultsMap == null) {
    Map<String,DDMFormFieldEvaluationResult> ddmFormFieldEvaluationResultsMap=new HashMap<>();
    populateDDMFormFieldEvaluationResultsMap(_ddmFormFieldEvaluationResults,ddmFormFieldEvaluationResultsMap);
    _ddmFormFieldEvaluationResultsMap=ddmFormFieldEvaluationResultsMap;
  }
  return _ddmFormFieldEvaluationResultsMap;
}
