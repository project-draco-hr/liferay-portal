{
  LiferayPortletURL liferayPortletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),JournalPortletKeys.JOURNAL,PortletRequest.RESOURCE_PHASE);
  liferayPortletURL.setParameter("groupId",String.valueOf(_article.getGroupId()));
  liferayPortletURL.setParameter("articleId",_article.getArticleId());
  liferayPortletURL.setResourceID("exportArticle");
  return liferayPortletURL;
}
