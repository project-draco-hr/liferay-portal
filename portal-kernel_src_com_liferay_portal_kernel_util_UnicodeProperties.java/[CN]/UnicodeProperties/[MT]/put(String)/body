{
  line=line.trim();
  if (_isComment(line)) {
    return;
  }
  int pos=line.indexOf(CharPool.EQUAL);
  if (pos == -1) {
    _log.error("Invalid property on line " + line);
  }
 else {
    String value=line.substring(pos + 1).trim();
    if (_safe) {
      value=_decode(value);
    }
    setProperty(line.substring(0,pos).trim(),value);
  }
}
