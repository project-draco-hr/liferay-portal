{
  if (Validator.isNull(props)) {
    return;
  }
  int lastIndex=0;
  int index=props.indexOf(CharPool.NEW_LINE);
  while (index != -1) {
    put(props.substring(lastIndex,index));
    lastIndex=index;
    index=props.indexOf(CharPool.NEW_LINE,lastIndex + 1);
  }
  put(props.substring(lastIndex));
}
