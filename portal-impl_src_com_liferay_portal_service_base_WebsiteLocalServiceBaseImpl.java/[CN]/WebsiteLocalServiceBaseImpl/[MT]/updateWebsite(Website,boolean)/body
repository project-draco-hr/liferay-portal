{
  website.setNew(false);
  website=websitePersistence.update(website,merge);
  Indexer indexer=IndexerRegistryUtil.getIndexer(getModelClassName());
  if (indexer != null) {
    try {
      indexer.reindex(website);
    }
 catch (    SearchException se) {
      if (_log.isWarnEnabled()) {
        _log.warn(se,se);
      }
    }
  }
  return website;
}
