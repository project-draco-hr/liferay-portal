{
  Artifact pomArtifact=artifact;
  String type=artifact.getType();
  if (!type.equals("pom")) {
    pomArtifact=artifactFactory.createArtifact(artifact.getGroupId(),artifact.getArtifactId(),artifact.getVersion(),"","pom");
  }
  return projectBuilder.buildFromRepository(pomArtifact,remoteArtifactRepositories,localArtifactRepository);
}
