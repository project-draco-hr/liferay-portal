{
  Folder folder=DLAppTestUtil.addFolder(stagingGroup.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,ServiceTestUtil.randomString());
  FileEntry fileEntry=DLAppTestUtil.addFileEntry(stagingGroup.getGroupId(),folder.getFolderId(),ServiceTestUtil.randomString());
  DLAppTestUtil.addDLFileShortcut(fileEntry,stagingGroup.getGroupId(),folder.getFolderId());
}
