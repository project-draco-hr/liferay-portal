def uninstallApp(appName):
    app = ''
    appList = AdminApp.list()
    if (len(appList) > 0):
        for item in appList.split('\n'):
            item = item.rstrip()
            if (item.find(appName) >= 0):
                app = item
                break
    if (app != ''):
        print AdminApp.uninstall(appName)
        print AdminConfig.save()
