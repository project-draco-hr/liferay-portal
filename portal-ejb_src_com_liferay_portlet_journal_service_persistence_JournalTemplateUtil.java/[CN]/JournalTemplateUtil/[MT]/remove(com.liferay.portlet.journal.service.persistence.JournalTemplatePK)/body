{
  ModelListener listener=null;
  if (Validator.isNotNull(_LISTENER)) {
    try {
      listener=(ModelListener)Class.forName(_LISTENER).newInstance();
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(journalTemplatePK));
  }
  com.liferay.portlet.journal.model.JournalTemplate journalTemplate=getPersistence().remove(journalTemplatePK);
  if (listener != null) {
    listener.onAfterRemove(journalTemplate);
  }
  return journalTemplate;
}
