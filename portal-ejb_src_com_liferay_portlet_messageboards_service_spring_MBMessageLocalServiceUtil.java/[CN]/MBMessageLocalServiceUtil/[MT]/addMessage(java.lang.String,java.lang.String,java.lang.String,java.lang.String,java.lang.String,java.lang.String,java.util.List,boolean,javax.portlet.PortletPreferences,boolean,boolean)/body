{
  try {
    MBMessageLocalService mbMessageLocalService=MBMessageLocalServiceFactory.getService();
    return mbMessageLocalService.addMessage(userId,categoryId,threadId,parentMessageId,subject,body,files,anonymous,prefs,addCommunityPermissions,addGuestPermissions);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
