{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByGroupId_PrevAndNext(entryId,groupId,orderByComparator);
  }
  BlogsEntry blogsEntry=findByPrimaryKey(entryId);
  Session session=null;
  try {
    session=openSession();
    BlogsEntry[] array=new BlogsEntryImpl[3];
    array[0]=filterGetByGroupId_PrevAndNext(session,blogsEntry,groupId,orderByComparator,true);
    array[1]=blogsEntry;
    array[2]=filterGetByGroupId_PrevAndNext(session,blogsEntry,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
