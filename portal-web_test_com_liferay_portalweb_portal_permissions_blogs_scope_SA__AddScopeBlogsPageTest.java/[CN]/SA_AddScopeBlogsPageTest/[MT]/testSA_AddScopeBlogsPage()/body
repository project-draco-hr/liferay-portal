{
  selenium.open("/web/scope-community/scope-test-page/");
  selenium.clickAt("main-content",RuntimeVariables.replace(""));
  selenium.clickAt("dockbar",RuntimeVariables.replace(""));
  selenium.clickAt("navigation",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("addPage")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("addPage",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//input")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.type("//input",RuntimeVariables.replace("Blogs Scope Permissions Page"));
  selenium.clickAt("save",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Blogs Scope Permissions Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Blogs Scope Permissions Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
}
