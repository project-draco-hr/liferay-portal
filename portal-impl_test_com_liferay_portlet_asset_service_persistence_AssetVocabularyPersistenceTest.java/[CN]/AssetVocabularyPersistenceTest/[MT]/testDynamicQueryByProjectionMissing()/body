{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AssetVocabulary.class,AssetVocabulary.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("vocabularyId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("vocabularyId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
