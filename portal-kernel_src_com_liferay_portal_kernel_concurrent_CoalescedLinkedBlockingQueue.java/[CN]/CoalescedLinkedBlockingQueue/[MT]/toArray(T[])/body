{
  fullyLock();
  try {
    int size=count.get();
    if (a.length < size) {
      a=(T[])java.lang.reflect.Array.newInstance(a.getClass().getComponentType(),size);
    }
    int k=0;
    for (Node<E> p=head.next; p != null; p=p.next) {
      a[k++]=(T)p.item;
    }
    if (a.length > k) {
      a[k]=null;
    }
    return a;
  }
  finally {
    fullyUnlock();
  }
}
