{
  PreparedStatement ps=null;
  try {
    ps=connection.prepareStatement("update UserNotificationEvent set payload = ? " + "where userNotificationEventId = ?");
    ps.setString(1,jsonObject.toString());
    ps.setLong(2,userNotificationEventId);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(ps);
  }
}
