{
  StringBuilder sb=new StringBuilder(name.toUpperCase());
  for (int i=0; i < name.length(); i++) {
    char c=name.charAt(i);
    if (Character.isUpperCase(c) && (i > 0)) {
      int delta=sb.length() - name.length();
      sb.insert(i + delta,CharPool.UNDERLINE);
    }
  }
  return sb.toString();
}
