{
  actionId=_formatActionId(actionId) + StringPool.POUND + StringUtil.toUpperCase(controller);
  try {
    ResourceActionsUtil.checkAction(portletId,actionId);
  }
 catch (  NoSuchResourceActionException e) {
    return true;
  }
  return permissionChecker.hasPermission(groupId,portletId,groupId,actionId);
}
