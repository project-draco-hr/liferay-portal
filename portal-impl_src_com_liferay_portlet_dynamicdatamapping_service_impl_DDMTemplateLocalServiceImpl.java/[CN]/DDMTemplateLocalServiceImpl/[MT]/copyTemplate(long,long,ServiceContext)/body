{
  DDMTemplate template=ddmTemplatePersistence.findByPrimaryKey(templateId);
  File smallImageFile=copySmallImage(template);
  return addTemplate(userId,template.getGroupId(),template.getClassNameId(),template.getClassPK(),null,template.getNameMap(),template.getDescriptionMap(),template.getType(),template.getMode(),template.getLanguage(),template.getScript(),template.isCacheable(),template.isSmallImage(),template.getSmallImageURL(),smallImageFile,serviceContext);
}
