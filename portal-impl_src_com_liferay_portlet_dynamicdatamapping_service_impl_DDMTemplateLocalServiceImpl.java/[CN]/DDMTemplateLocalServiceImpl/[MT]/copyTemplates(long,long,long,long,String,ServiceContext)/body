{
  List<DDMTemplate> newTemplates=new ArrayList<>();
  List<DDMTemplate> oldTemplates=ddmTemplatePersistence.findByC_C_T(classNameId,oldClassPK,type);
  for (  DDMTemplate oldTemplate : oldTemplates) {
    DDMTemplate newTemplate=copyTemplate(userId,oldTemplate,newClassPK,oldTemplate.getNameMap(),oldTemplate.getDescriptionMap(),serviceContext);
    newTemplates.add(newTemplate);
  }
  return newTemplates;
}
