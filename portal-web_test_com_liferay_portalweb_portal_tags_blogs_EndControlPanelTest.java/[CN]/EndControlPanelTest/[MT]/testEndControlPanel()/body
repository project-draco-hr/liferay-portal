{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      Thread.sleep(500);
    boolean InControlPanel=selenium.isElementPresent("link=Back to My Community");
  if (!InControlPanel) {
    label=2;
    continue;
  }
selenium.click(RuntimeVariables.replace("link=Back to My Community"));
selenium.waitForPageToLoad("30000");
case 2:
selenium.click(RuntimeVariables.replace("//div[@id='_145_myPlacesContainer']/ul/li[2]/a/span[1]"));
selenium.waitForPageToLoad("30000");
case 100:
label=-1;
}
}
}
