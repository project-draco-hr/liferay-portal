{
  List<JournalArticle> journalArticles=JournalArticleLocalServiceUtil.getNoPermissionArticles();
  for (  JournalArticle journalArticle : journalArticles) {
    ResourceLocalServiceUtil.addResources(journalArticle.getCompanyId(),0,0,JournalArticle.class.getName(),journalArticle.getResourcePrimKey(),false,false,false);
  }
}
