{
  Element dynamicContentElement=element.element("dynamic-content");
  long imageId=GetterUtil.getLong(dynamicContentElement.attributeValue("id"));
  JournalArticleImage image=JournalArticleImageLocalServiceUtil.getArticleImage(imageId);
  image.setElName(name + StringPool.UNDERLINE + index);
  JournalArticleImageLocalServiceUtil.updateJournalArticleImage(image);
}
