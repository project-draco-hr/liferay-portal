{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select distinct urlTitle from JournalArticle where urlTitle " + "like '%\u2018%' or urlTitle like '%\u2019%' or urlTitle " + "like '%\u201c%' or urlTitle like '%\u201d%'");
    rs=ps.executeQuery();
    while (rs.next()) {
      String urlTitle=rs.getString("urlTitle");
      updateURLTitle(urlTitle);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
