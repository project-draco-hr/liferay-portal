{
  mbBan=toUnwrappedModel(mbBan);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(mbBan)) {
      mbBan=(MBBan)session.get(MBBanImpl.class,mbBan.getPrimaryKeyObj());
    }
    if (mbBan != null) {
      session.delete(mbBan);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (mbBan != null) {
    clearCache(mbBan);
  }
  return mbBan;
}
