{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(mbBan);
    }
  }
  mbBan=removeImpl(mbBan);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(mbBan);
    }
  }
  return mbBan;
}
