{
  Session session=null;
  try {
    session=openSession();
    session.delete(mbBan);
    session.flush();
    return mbBan;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(MBBan.class.getName());
  }
}
