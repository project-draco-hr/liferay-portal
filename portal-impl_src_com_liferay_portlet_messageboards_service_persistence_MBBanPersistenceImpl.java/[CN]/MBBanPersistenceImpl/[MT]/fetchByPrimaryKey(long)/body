{
  MBBan mbBan=(MBBan)EntityCacheUtil.getResult(MBBanModelImpl.ENTITY_CACHE_ENABLED,MBBanImpl.class,banId,this);
  if (mbBan == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (mbBan == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      mbBan=(MBBan)session.get(MBBanImpl.class,Long.valueOf(banId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (mbBan == null)) {
        EntityCacheUtil.putResult(MBBanModelImpl.ENTITY_CACHE_ENABLED,MBBanImpl.class,banId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(mbBan);
      }
      closeSession(session);
    }
  }
  return mbBan;
}
