{
  MBBan mbBan=(MBBan)EntityCacheUtil.getResult(MBBanModelImpl.ENTITY_CACHE_ENABLED,MBBanImpl.class,banId,this);
  if (mbBan == null) {
    Session session=null;
    try {
      session=openSession();
      mbBan=(MBBan)session.get(MBBanImpl.class,new Long(banId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (mbBan != null) {
        cacheResult(mbBan);
      }
      closeSession(session);
    }
  }
  return mbBan;
}
