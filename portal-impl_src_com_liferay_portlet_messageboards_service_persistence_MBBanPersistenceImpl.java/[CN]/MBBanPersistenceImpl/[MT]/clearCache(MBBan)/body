{
  EntityCacheUtil.removeResult(MBBanModelImpl.ENTITY_CACHE_ENABLED,MBBanImpl.class,mbBan.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_B,new Object[]{Long.valueOf(mbBan.getGroupId()),Long.valueOf(mbBan.getBanUserId())});
}
