{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,mbBan,merge);
    mbBan.setNew(false);
    return mbBan;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(MBBan.class.getName());
  }
}
