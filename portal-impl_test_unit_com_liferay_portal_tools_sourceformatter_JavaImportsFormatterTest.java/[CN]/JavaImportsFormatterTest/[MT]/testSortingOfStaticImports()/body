{
  String original="import static org.mockito.Mockito.when;" + "\n" + "\n"+ "import static org.junit.Assert.assertEquals;";
  String expected="import static org.junit.Assert.assertEquals;" + "\n" + "\n"+ "import static org.mockito.Mockito.when;"+ "\n";
  _assertFormat("multiple static imports must be sorted, " + "however separate package groups must remain separate",original,expected);
}
