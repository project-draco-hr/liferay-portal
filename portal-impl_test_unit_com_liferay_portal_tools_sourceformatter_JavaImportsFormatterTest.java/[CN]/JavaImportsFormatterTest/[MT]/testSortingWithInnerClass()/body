{
  String original="import javax.servlet.FilterRegistration.Dynamic;" + "\n" + "import javax.servlet.FilterRegistration;";
  String expected="import javax.servlet.FilterRegistration;" + "\n" + "import javax.servlet.FilterRegistration.Dynamic;"+ '\n';
  _assertFormat("when a class and an inner class of itself are both imported, " + "the inner class must be second in order",original,expected);
}
