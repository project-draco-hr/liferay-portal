{
  DDMTemplate ddmTemplate=fetchByC_C_T_M_First(classNameId,classPK,type,mode,orderByComparator);
  if (ddmTemplate != null) {
    return ddmTemplate;
  }
  StringBundler msg=new StringBundler(10);
  msg.append(_NO_SUCH_ENTITY_WITH_KEY);
  msg.append("classNameId=");
  msg.append(classNameId);
  msg.append(", classPK=");
  msg.append(classPK);
  msg.append(", type=");
  msg.append(type);
  msg.append(", mode=");
  msg.append(mode);
  msg.append(StringPool.CLOSE_CURLY_BRACE);
  throw new NoSuchTemplateException(msg.toString());
}
