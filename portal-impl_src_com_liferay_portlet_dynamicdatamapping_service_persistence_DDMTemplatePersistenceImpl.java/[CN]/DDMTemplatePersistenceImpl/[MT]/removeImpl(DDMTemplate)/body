{
  ddmTemplate=toUnwrappedModel(ddmTemplate);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,ddmTemplate);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  DDMTemplateModelImpl ddmTemplateModelImpl=(DDMTemplateModelImpl)ddmTemplate;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddmTemplateModelImpl.getUuid(),Long.valueOf(ddmTemplateModelImpl.getGroupId())});
  EntityCacheUtil.removeResult(DDMTemplateModelImpl.ENTITY_CACHE_ENABLED,DDMTemplateImpl.class,ddmTemplate.getPrimaryKey());
  return ddmTemplate;
}
