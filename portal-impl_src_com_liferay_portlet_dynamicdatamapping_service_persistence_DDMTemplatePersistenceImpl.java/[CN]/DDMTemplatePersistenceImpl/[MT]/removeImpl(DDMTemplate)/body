{
  ddmTemplate=toUnwrappedModel(ddmTemplate);
  Session session=null;
  try {
    session=openSession();
    if (ddmTemplate.isCachedModel()) {
      ddmTemplate=(DDMTemplate)session.get(DDMTemplateImpl.class,ddmTemplate.getPrimaryKeyObj());
    }
    if (ddmTemplate != null) {
      session.delete(ddmTemplate);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (ddmTemplate != null) {
    clearCache(ddmTemplate);
  }
  return ddmTemplate;
}
