{
  ddmTemplate=toUnwrappedModel(ddmTemplate);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,ddmTemplate);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(ddmTemplate);
  return ddmTemplate;
}
