{
  DDMTemplate ddmTemplate=(DDMTemplate)EntityCacheUtil.getResult(DDMTemplateModelImpl.ENTITY_CACHE_ENABLED,DDMTemplateImpl.class,templateId,this);
  if (ddmTemplate == null) {
    Session session=null;
    try {
      session=openSession();
      ddmTemplate=(DDMTemplate)session.get(DDMTemplateImpl.class,Long.valueOf(templateId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (ddmTemplate != null) {
        cacheResult(ddmTemplate);
      }
      closeSession(session);
    }
  }
  return ddmTemplate;
}
