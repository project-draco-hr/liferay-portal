{
  DDMTemplate ddmTemplate=(DDMTemplate)EntityCacheUtil.getResult(DDMTemplateModelImpl.ENTITY_CACHE_ENABLED,DDMTemplateImpl.class,templateId,this);
  if (ddmTemplate == _nullDDMTemplate) {
    return null;
  }
  if (ddmTemplate == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      ddmTemplate=(DDMTemplate)session.get(DDMTemplateImpl.class,Long.valueOf(templateId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (ddmTemplate != null) {
        cacheResult(ddmTemplate);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(DDMTemplateModelImpl.ENTITY_CACHE_ENABLED,DDMTemplateImpl.class,templateId,_nullDDMTemplate);
      }
      closeSession(session);
    }
  }
  return ddmTemplate;
}
