{
  JournalArticle journalArticle=(JournalArticle)EntityCacheUtil.getResult(JournalArticleModelImpl.ENTITY_CACHE_ENABLED,JournalArticleImpl.class,id,this);
  if (journalArticle == null) {
    Session session=null;
    try {
      session=openSession();
      journalArticle=(JournalArticle)session.get(JournalArticleImpl.class,Long.valueOf(id));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (journalArticle != null) {
        cacheResult(journalArticle);
      }
      closeSession(session);
    }
  }
  return journalArticle;
}
