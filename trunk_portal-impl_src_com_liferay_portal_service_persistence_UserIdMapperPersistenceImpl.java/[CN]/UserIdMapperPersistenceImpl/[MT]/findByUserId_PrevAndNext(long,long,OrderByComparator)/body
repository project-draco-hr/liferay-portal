{
  UserIdMapper userIdMapper=findByPrimaryKey(userIdMapperId);
  Session session=null;
  try {
    session=openSession();
    UserIdMapper[] array=new UserIdMapperImpl[3];
    array[0]=getByUserId_PrevAndNext(session,userIdMapper,userId,orderByComparator,true);
    array[1]=userIdMapper;
    array[2]=getByUserId_PrevAndNext(session,userIdMapper,userId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
