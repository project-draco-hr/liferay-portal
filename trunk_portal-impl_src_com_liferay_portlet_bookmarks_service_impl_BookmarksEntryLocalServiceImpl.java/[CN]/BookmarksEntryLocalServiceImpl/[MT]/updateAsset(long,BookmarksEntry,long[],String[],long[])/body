{
  AssetEntry assetEntry=assetEntryLocalService.updateEntry(userId,entry.getGroupId(),BookmarksEntry.class.getName(),entry.getEntryId(),entry.getUuid(),assetCategoryIds,assetTagNames,true,null,null,null,null,ContentTypes.TEXT_PLAIN,entry.getName(),entry.getDescription(),null,null,entry.getUrl(),0,0,null,false);
  assetLinkLocalService.updateLinks(userId,assetEntry.getEntryId(),assetLinkEntryIds,AssetLinkConstants.TYPE_RELATED);
}
