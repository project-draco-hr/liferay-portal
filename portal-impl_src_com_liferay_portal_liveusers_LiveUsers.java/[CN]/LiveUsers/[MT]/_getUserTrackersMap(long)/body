{
  Map<Long,List<UserTracker>> userTrackersMap=(Map<Long,List<UserTracker>>)WebAppPool.get(companyId,WebKeys.LIVE_USER_TRACKERS);
  if (userTrackersMap == null) {
    userTrackersMap=new ConcurrentHashMap<Long,List<UserTracker>>();
    WebAppPool.put(companyId,WebKeys.LIVE_USER_TRACKERS,userTrackersMap);
  }
  return userTrackersMap;
}
