{
  String companyIdString=String.valueOf(companyId);
  Map<Long,List<UserTracker>> userTrackersMap=(Map<Long,List<UserTracker>>)WebAppPool.get(companyIdString,WebKeys.LIVE_USER_TRACKERS);
  if (userTrackersMap == null) {
    userTrackersMap=new ConcurrentHashMap<Long,List<UserTracker>>();
    WebAppPool.put(companyIdString,WebKeys.LIVE_USER_TRACKERS,userTrackersMap);
  }
  return userTrackersMap;
}
