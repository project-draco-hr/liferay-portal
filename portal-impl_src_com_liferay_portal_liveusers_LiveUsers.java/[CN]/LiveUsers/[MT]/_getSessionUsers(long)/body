{
  String companyIdString=String.valueOf(companyId);
  Map<String,UserTracker> sessionUsers=(Map<String,UserTracker>)WebAppPool.get(companyIdString,WebKeys.LIVE_SESSION_USERS);
  if (sessionUsers == null) {
    sessionUsers=new ConcurrentHashMap<String,UserTracker>();
    WebAppPool.put(companyIdString,WebKeys.LIVE_SESSION_USERS,sessionUsers);
  }
  return sessionUsers;
}
