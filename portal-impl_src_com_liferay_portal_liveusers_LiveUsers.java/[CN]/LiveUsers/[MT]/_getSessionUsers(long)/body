{
  Map<String,UserTracker> sessionUsers=(Map<String,UserTracker>)WebAppPool.get(companyId,WebKeys.LIVE_SESSION_USERS);
  if (sessionUsers == null) {
    sessionUsers=new ConcurrentHashMap<String,UserTracker>();
    WebAppPool.put(companyId,WebKeys.LIVE_SESSION_USERS,sessionUsers);
  }
  return sessionUsers;
}
