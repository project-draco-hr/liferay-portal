{
  String portletId=PortletProviderUtil.getPortletId(DDMDataProviderInstance.class.getName(),PortletProvider.Action.EDIT);
  PortletURL portletURL=PortletURLFactoryUtil.create(portletRequest,portletId,themeDisplay.getPlid(),PortletRequest.RENDER_PHASE);
  try {
    portletURL.setWindowState(LiferayWindowState.POP_UP);
  }
 catch (  WindowStateException wse) {
  }
  return portletURL.toString();
}
