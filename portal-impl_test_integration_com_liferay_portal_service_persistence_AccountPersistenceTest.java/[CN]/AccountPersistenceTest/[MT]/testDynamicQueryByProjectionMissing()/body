{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Account.class,Account.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("accountId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("accountId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
