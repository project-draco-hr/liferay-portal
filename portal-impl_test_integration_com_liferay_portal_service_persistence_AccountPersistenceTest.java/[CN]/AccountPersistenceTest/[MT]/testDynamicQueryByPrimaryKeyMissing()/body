{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Account.class,Account.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("accountId",ServiceTestUtil.nextLong()));
  List<Account> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
