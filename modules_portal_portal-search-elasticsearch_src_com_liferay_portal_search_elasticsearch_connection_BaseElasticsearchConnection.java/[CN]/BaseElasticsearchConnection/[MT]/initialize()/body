{
  if (_client != null) {
    return;
  }
  ImmutableSettings.Builder builder=ImmutableSettings.settingsBuilder();
  loadOptionalDefaultConfigurations(builder);
  if (Validator.isNotNull(elasticsearchConfiguration.additionalConfigurations())) {
    builder.loadFromSource(elasticsearchConfiguration.additionalConfigurations());
  }
  loadRequiredDefaultConfigurations(builder);
  _client=createClient(builder);
}
