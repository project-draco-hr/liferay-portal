{
  _mimeType=mimeType;
  if (_syncDLObjectRemoteModel != null) {
    try {
      Class<?> clazz=_syncDLObjectRemoteModel.getClass();
      Method method=clazz.getMethod("setMimeType",String.class);
      method.invoke(_syncDLObjectRemoteModel,mimeType);
