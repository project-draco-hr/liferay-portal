{
  if (_denyAll || _forbiddenClassNames.contains(className)) {
    return false;
  }
  if (_allowAll) {
    return true;
  }
  for (  Pattern allowedPattern : _allowedPatterns) {
    Matcher matcher=allowedPattern.matcher(className);
    if (matcher.find()) {
      return true;
    }
  }
  return false;
}
