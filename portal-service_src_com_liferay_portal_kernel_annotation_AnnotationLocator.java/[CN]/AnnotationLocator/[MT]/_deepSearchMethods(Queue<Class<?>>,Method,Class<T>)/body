{
  if (queue.isEmpty()) {
    return null;
  }
  T annotation=null;
  Class<?> clazz=queue.poll();
  try {
    Method specificMethod=clazz.getDeclaredMethod(method.getName(),method.getParameterTypes());
    annotation=specificMethod.getAnnotation(annotationClass);
    if (annotation != null) {
      return annotation;
    }
  }
 catch (  Exception e) {
  }
  annotation=clazz.getAnnotation(annotationClass);
  if (annotation == null) {
    _queueSuperTypes(queue,clazz);
    return _deepSearchMethods(queue,method,annotationClass);
  }
 else {
    return annotation;
  }
}
