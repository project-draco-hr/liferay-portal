{
  Queue<Class<?>> queue=new LinkedList<>();
  queue.offer(targetClass);
  Class<?> clazz=null;
  while ((clazz=queue.poll()) != null) {
    T annotation=clazz.getAnnotation(annotationClass);
    if (annotation == null) {
      _queueSuperTypes(queue,clazz);
    }
 else {
      return annotation;
    }
  }
  return null;
}
