{
  List<WikiNode> nodes=WikiNodeLocalServiceUtil.getNodes(group.getGroupId());
  for (  WikiNode node : nodes) {
    List<WikiPage> pages=WikiPageLocalServiceUtil.getPages(node.getNodeId(),QueryUtil.ALL_POS,QueryUtil.ALL_POS);
    for (    WikiPage page : pages) {
      WikiPage importedPage=(WikiPage)getStagedModel(page.getUuid(),importedGroup.getGroupId());
      Assert.assertNotNull(importedPage);
      validateImportedStagedModel(page,importedPage);
    }
  }
}
