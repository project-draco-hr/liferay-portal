{
  _groupUser=UserTestUtil.addUser();
  _group=GroupTestUtil.addGroup();
  GroupLocalServiceUtil.addUserGroup(_groupUser.getUserId(),_group);
  _organizationUser=UserTestUtil.addUser();
  _organization=OrganizationTestUtil.addOrganization();
  OrganizationLocalServiceUtil.addUserOrganization(_organizationUser.getUserId(),_organization);
  _userGroupUser=UserTestUtil.addUser();
  _userGroup=UserGroupTestUtil.addUserGroup();
  UserGroupLocalServiceUtil.addUserUserGroup(_userGroupUser.getUserId(),_userGroup);
}
