{
  LinkedHashMap<String,Object> params=new LinkedHashMap<>();
  params.put("usersUserGroups",_userGroup.getUserGroupId());
  List<User> users=UserFinderUtil.findByKeywords(TestPropsValues.getCompanyId(),null,WorkflowConstants.STATUS_APPROVED,params,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  Assert.assertTrue(users.contains(_userGroupUser));
}
