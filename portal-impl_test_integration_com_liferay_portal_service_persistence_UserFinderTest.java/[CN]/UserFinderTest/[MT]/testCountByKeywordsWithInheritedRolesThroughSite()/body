{
  GroupLocalServiceUtil.addOrganizationGroup(_organization.getOrganizationId(),_group);
  GroupLocalServiceUtil.addUserGroupGroup(_userGroup.getUserGroupId(),_group);
  long roleId=RoleTestUtil.addRegularRole(_group.getGroupId());
  LinkedHashMap<String,Object> params=new LinkedHashMap<String,Object>();
  params.put("inherit",Boolean.TRUE);
  params.put("usersRoles",roleId);
  int count=UserFinderUtil.countByKeywords(TestPropsValues.getCompanyId(),null,WorkflowConstants.STATUS_APPROVED,params);
  Assert.assertEquals(4,count);
}
