{
  try {
    clearAssetTags.clear(pk);
    for (    com.liferay.portlet.asset.model.AssetTag assetTag : assetTags) {
      addAssetTag.add(pk,assetTag.getPrimaryKey());
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    FinderCacheUtil.clearCache("Assets_AssetTags");
  }
}
