{
  Asset asset=(Asset)EntityCacheUtil.getResult(AssetModelImpl.ENTITY_CACHE_ENABLED,AssetImpl.class,assetId,this);
  if (asset == null) {
    Session session=null;
    try {
      session=openSession();
      asset=(Asset)session.get(AssetImpl.class,new Long(assetId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (asset != null) {
        cacheResult(asset);
      }
      closeSession(session);
    }
  }
  return asset;
}
