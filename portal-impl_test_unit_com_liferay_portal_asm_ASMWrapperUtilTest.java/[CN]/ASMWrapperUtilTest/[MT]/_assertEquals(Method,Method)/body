{
  Assert.assertEquals("Expected:" + expectedMethod + ", actual: "+ actualMethod,expectedMethod.getModifiers() - Modifier.ABSTRACT,actualMethod.getModifiers());
  Assert.assertSame("Expected:" + expectedMethod + ", actual: "+ actualMethod,expectedMethod.getReturnType(),actualMethod.getReturnType());
  Assert.assertEquals("Expected:" + expectedMethod + ", actual: "+ actualMethod,expectedMethod.getName(),actualMethod.getName());
  Assert.assertArrayEquals("Expected:" + expectedMethod + ", actual: "+ actualMethod,expectedMethod.getParameterTypes(),actualMethod.getParameterTypes());
  Assert.assertArrayEquals("Expected:" + expectedMethod + ", actual: "+ actualMethod,expectedMethod.getExceptionTypes(),actualMethod.getExceptionTypes());
}
