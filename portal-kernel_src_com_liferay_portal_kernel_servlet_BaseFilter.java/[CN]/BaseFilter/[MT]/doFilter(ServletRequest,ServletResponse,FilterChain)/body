{
  Log log=getLog();
  if (log.isDebugEnabled()) {
    if (isFilterEnabled()) {
      log.debug(_filterClass + " is enabled");
    }
 else {
      log.debug(_filterClass + " is disabled");
    }
  }
  if (isFilterEnabled()) {
    processFilter(req,res,chain);
  }
 else {
    processFilter(_filterClass,req,res,chain);
  }
}
