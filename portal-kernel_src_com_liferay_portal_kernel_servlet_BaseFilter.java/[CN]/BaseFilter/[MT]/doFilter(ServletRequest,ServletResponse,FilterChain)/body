{
  if (_log.isDebugEnabled()) {
    if (isFilterEnabled()) {
      _log.debug(_filterClass + " is enabled");
    }
 else {
      _log.debug(_filterClass + " is disabled");
    }
  }
  if (isFilterEnabled()) {
    processFilter(req,res,chain);
  }
 else {
    processFilter(_filterClass,req,res,chain);
  }
}
