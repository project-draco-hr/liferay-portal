{
  DDMFormFieldValue ddmFormFieldValue=DDMFormValuesTestUtil.createDDMFormFieldValue("Radio",new UnlocalizedValue("[\"value 1\"]"));
  RadioDDMFormFieldValueAccessor radioDDMFormFieldValueAccessor=new RadioDDMFormFieldValueAccessor(LocaleUtil.US);
  Assert.assertEquals("value 1",radioDDMFormFieldValueAccessor.get(ddmFormFieldValue));
}
