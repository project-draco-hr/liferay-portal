{
  if (((properties == null) && (_properties != null)) || ((properties != null) && (_properties == null)) || ((properties != null) && (_properties != null) && !properties.equals(_properties))) {
    if (!XSS_ALLOW_PROPERTIES) {
      properties=XSSUtil.strip(properties);
    }
    _properties=properties;
    setModified(true);
  }
}
