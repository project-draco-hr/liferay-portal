{
  if (FileAvailabilityUtil.isAvailable(getServletContext(),_PAGE)) {
    return _PAGE;
  }
  PortletDisplay portletDisplay=(PortletDisplay)pageContext.getAttribute("portletDisplay");
  if (!portletDisplay.isShowRefreshIcon()) {
    return null;
  }
  setCssClass("portlet-refresh portlet-refresh-icon");
  setImage("../portlet/refresh");
  setMessage("refresh");
  String onClick="Liferay.Portlet.refresh('#p_p_id_".concat(portletDisplay.getId()).concat("_'); return false;");
  setOnClick(onClick);
  setToolTip(false);
  setUrl(portletDisplay.getURLRefresh());
  return super.getPage();
}
