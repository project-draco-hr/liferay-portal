{
  try {
    if (sender != null) {
      sender.close();
    }
  }
 catch (  JMSException jmse) {
    jmse.printStackTrace();
  }
  try {
    if (session != null) {
      session.close();
    }
  }
 catch (  JMSException jmse) {
    jmse.printStackTrace();
  }
  try {
    if (con != null) {
      con.close();
    }
  }
 catch (  JMSException jmse) {
    jmse.printStackTrace();
  }
}
