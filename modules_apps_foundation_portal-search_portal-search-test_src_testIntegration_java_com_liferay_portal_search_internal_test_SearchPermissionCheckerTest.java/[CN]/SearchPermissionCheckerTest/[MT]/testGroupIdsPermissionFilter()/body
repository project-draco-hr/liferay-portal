{
  _user=UserTestUtil.addOrganizationAdminUser(_organization);
  PermissionThreadLocal.setPermissionChecker(PermissionCheckerFactoryUtil.create(_user));
  Role role=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),RoleConstants.ORGANIZATION_ADMINISTRATOR);
  assertFieldValue(new long[]{_group.getGroupId()},Field.GROUP_ROLE_ID,_group.getGroupId() + StringPool.DASH + role.getRoleId(),false);
}
