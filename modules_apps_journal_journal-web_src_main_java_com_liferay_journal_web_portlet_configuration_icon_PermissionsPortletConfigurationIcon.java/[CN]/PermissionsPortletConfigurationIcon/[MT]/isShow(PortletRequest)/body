{
  try {
    JournalArticle article=ActionUtil.getArticle(portletRequest);
    if ((article == null) || article.isNew()) {
      return false;
    }
    if (JournalArticlePermission.contains(themeDisplay.getPermissionChecker(),article,ActionKeys.PERMISSIONS)) {
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
