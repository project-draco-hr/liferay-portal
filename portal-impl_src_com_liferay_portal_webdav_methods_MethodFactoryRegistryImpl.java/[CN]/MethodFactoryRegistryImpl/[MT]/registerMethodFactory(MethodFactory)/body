{
  Class<?> clazz=methodFactory.getClass();
  MethodFactory previousMethodFactory=_methodFactories.put(clazz.getName(),methodFactory);
  if (previousMethodFactory == _defaultMethodFactory) {
    _defaultMethodFactory=methodFactory;
  }
  if (_log.isWarnEnabled() && (previousMethodFactory != null)) {
    _log.warn("Replacing " + previousMethodFactory + " for class name "+ clazz.getName()+ " with "+ methodFactory);
  }
}
