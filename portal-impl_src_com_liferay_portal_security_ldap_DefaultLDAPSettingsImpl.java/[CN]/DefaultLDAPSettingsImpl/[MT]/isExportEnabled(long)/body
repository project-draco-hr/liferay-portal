{
  boolean defaultImportUserPasswordAutogenerated=GetterUtil.get(PropsUtil.get(PropsKeys.LDAP_IMPORT_USER_PASSWORD_AUTOGENERATED),false);
  if (isImportEnabled(companyId) && defaultImportUserPasswordAutogenerated) {
    return false;
  }
  boolean defaultLDAPExportEnabled=GetterUtil.get(PropsUtil.get(PropsKeys.LDAP_EXPORT_ENABLED),false);
  if (PrefsPropsUtil.getBoolean(companyId,PropsKeys.LDAP_EXPORT_ENABLED,defaultLDAPExportEnabled)) {
    return true;
  }
 else {
    return false;
  }
}
