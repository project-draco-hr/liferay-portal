{
  String rules=ParamUtil.getString(actionRequest,"rules");
  if (Validator.isNull(rules) || Objects.equals("[]",rules)) {
    return Collections.emptyList();
  }
  JSONDeserializer<DDLFormRule[]> jsonDeserializer=jsonFactory.createJSONDeserializer();
  DDLFormRule[] ddlFormRules=jsonDeserializer.deserialize(rules,DDLFormRule[].class);
  return ddlFormRulesToDDMFormRulesConverter.convert(ListUtil.toList(ddlFormRules));
}
