{
  MBCategory category=MBTestUtil.addCategory(stagingGroup.getGroupId());
  MBMessage message=MBTestUtil.addMessageWithWorkflow(stagingGroup.getGroupId(),category.getCategoryId(),true);
  MBTestUtil.addThreadFlag(stagingGroup.getGroupId(),message.getThread());
  MBTestUtil.addBan(stagingGroup.getGroupId());
}
