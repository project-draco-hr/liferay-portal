{
  System.out.print("Asserting sample " + caseDir.getName() + ": ");
  File expectedUnstableMessageFile=new File(caseDir,"expected_unstable_message.html");
  String expectedUnstableMessage=_read(expectedUnstableMessageFile);
  String actualUnstableMessage=UnstableMessageUtil.getUnstableMessage(_toURLString(caseDir));
  boolean value=expectedUnstableMessage.equals(actualUnstableMessage);
  if (value) {
    System.out.println(" PASSED");
  }
 else {
    System.out.println(" FAILED");
    System.out.println("\nActual results: \n" + actualUnstableMessage);
    System.out.println("\nExpected results: \n" + expectedUnstableMessage);
  }
  Assert.assertTrue(value);
}
