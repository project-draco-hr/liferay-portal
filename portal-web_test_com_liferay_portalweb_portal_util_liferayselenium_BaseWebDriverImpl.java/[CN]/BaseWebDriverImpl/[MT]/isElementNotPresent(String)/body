{
  WebDriver.Options options=manage();
  Timeouts timeouts=options.timeouts();
  timeouts.implicitlyWait(TestPropsValues.TIMEOUT_IMPLICIT_WAIT,TimeUnit.MILLISECONDS);
  List<WebElement> webElements=getWebElements(locator);
  timeouts.implicitlyWait(TestPropsValues.TIMEOUT_EXPLICIT_WAIT,TimeUnit.MILLISECONDS);
  return webElements.isEmpty();
}
