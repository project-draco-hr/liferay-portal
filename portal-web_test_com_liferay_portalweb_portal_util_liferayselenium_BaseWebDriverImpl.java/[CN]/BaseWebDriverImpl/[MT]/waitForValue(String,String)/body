{
  for (int second=0; ; second++) {
    if (second >= TestPropsValues.TIMEOUT_EXPLICIT_WAIT) {
      BaseTestCase.fail("timeout");
    }
    try {
      if (RuntimeVariables.replace(value).equals(getValue(locator))) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
