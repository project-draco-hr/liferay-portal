{
  int timeout=TestPropsValues.TIMEOUT_EXPLICIT_WAIT / TestPropsValues.TIMEOUT_IMPLICIT_WAIT;
  for (int second=0; ; second++) {
    if (second >= timeout) {
      BaseTestCase.fail("Timeout: unable to find the locator \"" + locator + "\"");
    }
    try {
      if (isElementPresent(locator)) {
        break;
      }
    }
 catch (    Exception e) {
    }
  }
}
