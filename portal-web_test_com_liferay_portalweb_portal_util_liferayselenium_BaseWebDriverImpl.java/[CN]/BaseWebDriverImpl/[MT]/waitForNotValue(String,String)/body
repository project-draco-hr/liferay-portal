{
  String valueReplace=RuntimeVariables.replace(value);
  for (int second=0; ; second++) {
    if (second >= 30) {
      BaseTestCase.fail("timeout");
    }
    try {
      if (!valueReplace.equals(getValue(locator))) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
