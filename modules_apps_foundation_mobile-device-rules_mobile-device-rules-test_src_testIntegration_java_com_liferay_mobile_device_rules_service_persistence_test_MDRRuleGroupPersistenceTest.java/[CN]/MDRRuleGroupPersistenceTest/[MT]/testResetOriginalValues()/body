{
  MDRRuleGroup newMDRRuleGroup=addMDRRuleGroup();
  _persistence.clearCache();
  MDRRuleGroup existingMDRRuleGroup=_persistence.findByPrimaryKey(newMDRRuleGroup.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingMDRRuleGroup.getUuid(),ReflectionTestUtil.invoke(existingMDRRuleGroup,"getOriginalUuid",new Class<?>[0])));
  Assert.assertEquals(Long.valueOf(existingMDRRuleGroup.getGroupId()),ReflectionTestUtil.<Long>invoke(existingMDRRuleGroup,"getOriginalGroupId",new Class<?>[0]));
}
