{
  return new DDMFormFieldTemplateContextContributor(){
    public Map<String,Object> getParameters(    DDMFormField ddmFormField,    DDMFormFieldRenderingContext ddmFormFieldRenderingContext){
      Map<String,Object> parameters=new HashMap<>();
      parameters.put("displayStyle",ddmFormField.getProperty("displayStyle"));
      return parameters;
    }
  }
;
}
