{
  long size=getSize();
  String contentType=getContentType();
  if (contentType != null) {
    byte[] bytes=contentType.getBytes();
    size+=bytes.length;
  }
  String fieldName=getFieldName();
  if (fieldName != null) {
    byte[] bytes=fieldName.getBytes();
    size+=bytes.length;
  }
  String fileName=getFileName();
  if (fileName != null) {
    byte[] bytes=fileName.getBytes();
    size+=bytes.length;
  }
  return size;
}
