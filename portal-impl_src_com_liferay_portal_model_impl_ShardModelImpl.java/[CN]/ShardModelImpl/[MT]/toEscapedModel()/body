{
  if (isEscapedModel()) {
    return (Shard)this;
  }
 else {
    Shard model=new ShardImpl();
    model.setNew(isNew());
    model.setEscapedModel(true);
    model.setShardId(getShardId());
    model.setName(HtmlUtil.escape(getName()));
    model=(Shard)Proxy.newProxyInstance(Shard.class.getClassLoader(),new Class[]{Shard.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
