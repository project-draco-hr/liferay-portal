{
  super.prepareLocalizedFieldsForImport(defaultImportLocale);
  Locale ddmStructureDefaultLocale=LocaleUtil.fromLanguageId(getDefaultLanguageId());
  try {
    setDefinition(DDMXMLUtil.updateXMLDefaultLocale(getDefinition(),ddmStructureDefaultLocale,defaultImportLocale));
  }
 catch (  Exception e) {
    throw new LocaleException(LocaleException.TYPE_EXPORT_IMPORT,e);
  }
}
