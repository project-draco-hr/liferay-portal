{
  Session session=null;
  try {
    session=openSession();
    PermissionHBM permissionHBM=(PermissionHBM)session.get(PermissionHBM.class,permissionId);
    if (permissionHBM == null) {
      _log.warn("No Permission exists with the primary key " + permissionId.toString());
      throw new NoSuchPermissionException("No Permission exists with the primary key " + permissionId.toString());
    }
    return PermissionHBMUtil.model(permissionHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
