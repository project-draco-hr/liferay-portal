{
  ShoppingOrderItem shoppingOrderItem=fetchByPrimaryKey(shoppingOrderItemPK);
  if (shoppingOrderItem == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ShoppingOrderItem exists with the primary key " + shoppingOrderItemPK);
    }
    throw new NoSuchOrderItemException("No ShoppingOrderItem exists with the primary key " + shoppingOrderItemPK);
  }
  return shoppingOrderItem;
}
