{
  if (_groupGoogleMapsAPIKey != null) {
    return _groupGoogleMapsAPIKey;
  }
  Group liveGroup=(Group)_request.getAttribute("site.liveGroup");
  UnicodeProperties groupTypeSettings=null;
  if (liveGroup != null) {
    groupTypeSettings=liveGroup.getTypeSettingsProperties();
  }
 else {
    groupTypeSettings=new UnicodeProperties();
  }
  _groupGoogleMapsAPIKey=PropertiesParamUtil.getString(groupTypeSettings,_request,"googleMapsAPIKey",getCompanyGoogleMapsAPIKey());
  return _groupGoogleMapsAPIKey;
}
