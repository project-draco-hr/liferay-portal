{
  if (StringUtil.equalsIgnoreCase(key,"autoCommit") || StringUtil.equalsIgnoreCase(key,"connectionTimeout") || StringUtil.equalsIgnoreCase(key,"idleTimeout")|| StringUtil.equalsIgnoreCase(key,"maximumPoolSize")|| StringUtil.equalsIgnoreCase(key,"maxLifetime")|| StringUtil.equalsIgnoreCase(key,"minimumIdle")|| StringUtil.equalsIgnoreCase(key,"registerMbeans")) {
    return true;
  }
  return false;
}
