{
  ExpandoRow newExpandoRow=addExpandoRow();
  _persistence.clearCache();
  ExpandoRow existingExpandoRow=_persistence.findByPrimaryKey(newExpandoRow.getPrimaryKey());
  Assert.assertEquals(existingExpandoRow.getTableId(),ReflectionTestUtil.invoke(existingExpandoRow,"getOriginalTableId",new Class<?>[0]));
  Assert.assertEquals(existingExpandoRow.getClassPK(),ReflectionTestUtil.invoke(existingExpandoRow,"getOriginalClassPK",new Class<?>[0]));
}
