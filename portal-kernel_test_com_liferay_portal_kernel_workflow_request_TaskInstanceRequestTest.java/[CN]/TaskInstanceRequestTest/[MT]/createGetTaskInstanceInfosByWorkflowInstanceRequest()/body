{
  final long workflowInstanceId=1;
  _context.checking(new Expectations(){
{
      oneOf(_factory).createCredential(0);
      will(returnValue(null));
      oneOf(_manager).getTaskInstanceInfosByWorkflowInstance(workflowInstanceId);
    }
  }
);
  TaskInstanceRequest request=TaskInstanceRequest.createGetTaskInstanceInfosByWorkflowInstanceRequest(workflowInstanceId);
  assertTrue(request.hasReturnValue());
  request.execute(_manager);
}
