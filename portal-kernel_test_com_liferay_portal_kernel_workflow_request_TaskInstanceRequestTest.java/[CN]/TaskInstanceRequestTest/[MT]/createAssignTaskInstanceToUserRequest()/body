{
  final long taskInstanceId=1;
  final UserCredential userCredential=new UserCredential();
  final String comment="";
  final long callingUserId=1;
  final Map<String,Object> attributes=new HashMap<String,Object>();
  _context.checking(new Expectations(){
{
      oneOf(_factory).createCredential(callingUserId);
      will(returnValue(null));
      oneOf(_manager).assignTaskInstanceToUser(taskInstanceId,userCredential,comment,attributes,callingUserId);
    }
  }
);
  TaskInstanceRequest request=TaskInstanceRequest.createAssignTaskInstanceToUserRequest(taskInstanceId,userCredential,comment,attributes,callingUserId);
  assertTrue(request.hasReturnValue());
  request.execute(_manager);
}
