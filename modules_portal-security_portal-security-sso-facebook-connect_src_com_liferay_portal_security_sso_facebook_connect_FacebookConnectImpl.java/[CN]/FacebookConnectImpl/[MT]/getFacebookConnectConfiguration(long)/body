{
  try {
    FacebookConnectConfiguration facebookConnectCompanyServiceSettings=_moduleConfigurationFactory.getModuleConfiguration(FacebookConnectConfiguration.class,new CompanyServiceSettingsLocator(companyId,FacebookConnectConstants.SERVICE_NAME));
    return facebookConnectCompanyServiceSettings;
  }
 catch (  ModuleConfigurationException mce) {
    _log.error("Unable to get Facebook Connect configuration",mce);
  }
  return null;
}
