{
  Map<String,List<ImageEditorCapabilityInformation>> groupedCapabilities=new HashMap<>();
  for (  ImageEditorCapabilityInformation imageEditorCapability : imageEditorCapabilities) {
    Map<String,Object> capabilityProperties=imageEditorCapability.getProperties();
    String imageEditorCapabilityCategory=GetterUtil.getString(capabilityProperties.get("com.liferay.image.editor.capability.category"));
    groupedCapabilities.putIfAbsent(imageEditorCapabilityCategory,new ArrayList<ImageEditorCapabilityInformation>());
    List<ImageEditorCapabilityInformation> imageEditorCapabilityList=groupedCapabilities.get(imageEditorCapabilityCategory);
    imageEditorCapabilityList.add(imageEditorCapability);
  }
  return new ArrayList<>(groupedCapabilities.values());
}
