{
  String emailMessageAddedEnabled=prefs.getValue("email-message-added-enabled",StringPool.BLANK);
  if (Validator.isNotNull(emailMessageAddedEnabled)) {
    return GetterUtil.getBoolean(emailMessageAddedEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsUtil.MESSAGE_BOARDS_EMAIL_MESSAGE_ADDED_ENABLED));
  }
}
