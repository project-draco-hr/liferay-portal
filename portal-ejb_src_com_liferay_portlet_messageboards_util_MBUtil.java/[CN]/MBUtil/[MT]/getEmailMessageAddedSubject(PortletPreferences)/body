{
  String emailMessageAddedSubject=prefs.getValue("email-message-added-subject",StringPool.BLANK);
  if (Validator.isNotNull(emailMessageAddedSubject)) {
    return emailMessageAddedSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsUtil.MESSAGE_BOARDS_EMAIL_MESSAGE_ADDED_SUBJECT));
  }
}
