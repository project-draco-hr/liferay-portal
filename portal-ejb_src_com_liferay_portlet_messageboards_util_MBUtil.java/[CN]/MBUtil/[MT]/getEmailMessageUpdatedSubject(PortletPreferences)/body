{
  String emailMessageUpdatedSubject=prefs.getValue("email-message-updated-subject",StringPool.BLANK);
  if (Validator.isNotNull(emailMessageUpdatedSubject)) {
    return emailMessageUpdatedSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsUtil.MESSAGE_BOARDS_EMAIL_MESSAGE_UPDATED_SUBJECT));
  }
}
