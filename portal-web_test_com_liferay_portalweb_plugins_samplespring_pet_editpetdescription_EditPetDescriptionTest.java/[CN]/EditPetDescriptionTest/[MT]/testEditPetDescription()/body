{
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Description Upload Test Page",RuntimeVariables.replace("Description Upload Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Upload a Pet Description Here"),selenium.getText("//div[@class='portlet-body']/h1"));
  assertTrue(selenium.isVisible("//select[@name='selectedPet']"));
  selenium.select("//select[@name='selectedPet']",RuntimeVariables.replace("Bubbles (Fish/Goldfish)"));
  selenium.uploadCommonFile("//input[@name='file']",RuntimeVariables.replace("Document_1.txt"));
  selenium.clickAt("//button[@type='submit']",RuntimeVariables.replace("Upload"));
  Thread.sleep(5000);
}
