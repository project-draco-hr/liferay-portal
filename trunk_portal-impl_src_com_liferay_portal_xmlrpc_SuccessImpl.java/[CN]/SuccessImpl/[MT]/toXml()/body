{
  StringBundler sb=new StringBundler(8);
  sb.append("<?xml version=\"1.0\" encoding=\"UTF-8\"?>");
  sb.append("<methodResponse>");
  sb.append("<params>");
  sb.append("<param>");
  sb.append(XmlRpcParser.wrapValue(_description));
  sb.append("</param>");
  sb.append("</params>");
  sb.append("</methodResponse>");
  return sb.toString();
}
