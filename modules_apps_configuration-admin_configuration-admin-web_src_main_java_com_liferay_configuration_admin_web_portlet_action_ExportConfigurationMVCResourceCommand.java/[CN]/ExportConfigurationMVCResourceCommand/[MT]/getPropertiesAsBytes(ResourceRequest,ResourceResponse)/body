{
  Properties properties=getProperties(resourceRequest,resourceResponse);
  String propertiesString=PropertiesUtil.toString(properties);
  propertiesString="##\n## Deploy this file to a Liferay installation to apply the " + "configuration.\n## Make sure the file name is " + getFileName(resourceRequest) + "\n##\n"+ propertiesString;
  return propertiesString.getBytes();
}
