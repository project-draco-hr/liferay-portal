{
  if (!(resourceResponse instanceof MimeResponse)) {
    return false;
  }
  String pid=ParamUtil.getString(resourceRequest,"pid");
  try {
    PortletResponseUtil.sendFile(resourceRequest,resourceResponse,pid + ".cfg",getPropertiesAsBytes(resourceRequest,resourceResponse),ContentTypes.TEXT_XML_UTF8);
  }
 catch (  Exception e) {
    throw new PortletException(e);
  }
  return true;
}
