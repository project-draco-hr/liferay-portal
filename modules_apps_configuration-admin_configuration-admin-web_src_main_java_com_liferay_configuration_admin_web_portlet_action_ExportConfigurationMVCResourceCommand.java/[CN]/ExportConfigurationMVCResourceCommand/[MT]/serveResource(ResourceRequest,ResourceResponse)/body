{
  if (!(resourceResponse instanceof MimeResponse)) {
    return false;
  }
  String fileName=getFileName(resourceRequest);
  try {
    PortletResponseUtil.sendFile(resourceRequest,resourceResponse,fileName,getPropertiesAsBytes(resourceRequest,resourceResponse),ContentTypes.TEXT_XML_UTF8);
  }
 catch (  Exception e) {
    throw new PortletException(e);
  }
  return true;
}
