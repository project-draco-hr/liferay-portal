{
  RenderedImage croppedRenderedImage=ImageToolUtil.crop(renderedImage,height,width,x,y);
  int maxHeight=renderedImage.getHeight() - Math.abs(y);
  int maxWidth=renderedImage.getWidth() - Math.abs(x);
  Assert.assertEquals(croppedRenderedImage.getHeight(),Math.min(maxHeight,height));
  Assert.assertEquals(croppedRenderedImage.getWidth(),Math.min(maxWidth,width));
}
