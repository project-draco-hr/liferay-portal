{
  name=name.trim();
  AssetCategoryVocabulary vocabulary=assetCategoryVocabularyPersistence.findByPrimaryKey(vocabularyId);
  if (!vocabulary.getName().equals(name) && hasCategoryVocabulary(vocabulary.getGroupId(),name)) {
    throw new DuplicateVocabularyException(name);
  }
  vocabulary.setModifiedDate(new Date());
  vocabulary.setName(name);
  assetCategoryVocabularyPersistence.update(vocabulary,false);
  return vocabulary;
}
