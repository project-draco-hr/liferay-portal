{
  assetCategoryLocalService.deleteVocabularyCategories(categoryVocabulary.getCategoryVocabularyId());
  resourceLocalService.deleteResource(categoryVocabulary.getCompanyId(),AssetCategoryVocabulary.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,categoryVocabulary.getCategoryVocabularyId());
  assetCategoryVocabularyPersistence.remove(categoryVocabulary);
}
