{
  List<AssetCategoryVocabulary> categoryVocabularies=assetCategoryVocabularyPersistence.findByGroupId(groupId);
  if (categoryVocabularies.isEmpty()) {
    Group group=groupLocalService.getGroup(groupId);
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setScopeGroupId(groupId);
    addCategoryVocabulary(userLocalService.getDefaultUserId(group.getCompanyId()),PropsValues.ASSET_CATEGORIES_VOCABULARY_DEFAULT,serviceContext);
  }
  return categoryVocabularies;
}
