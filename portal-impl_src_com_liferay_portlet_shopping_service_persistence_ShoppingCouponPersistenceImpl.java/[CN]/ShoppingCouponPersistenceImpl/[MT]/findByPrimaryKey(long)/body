{
  ShoppingCoupon shoppingCoupon=fetchByPrimaryKey(couponId);
  if (shoppingCoupon == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ShoppingCoupon exists with the primary key " + couponId);
    }
    throw new NoSuchCouponException("No ShoppingCoupon exists with the primary key " + couponId);
  }
  return shoppingCoupon;
}
