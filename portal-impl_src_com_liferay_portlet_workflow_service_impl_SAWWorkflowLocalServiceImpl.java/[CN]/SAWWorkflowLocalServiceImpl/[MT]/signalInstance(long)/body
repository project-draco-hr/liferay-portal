{
  OutputVO outputVO=null;
  BusinessProcessInstanceVO businessProcessInstance=new BusinessProcessInstanceVO();
  businessProcessInstance.setId(String.valueOf(instanceId));
  try {
    Workflow workflow=_getWorkflowImpl();
    outputVO=workflow.signalBusinessProcessInstance(businessProcessInstance);
  }
 catch (  WorkflowException e) {
    throw new WorkflowComponentException(e);
  }
}
