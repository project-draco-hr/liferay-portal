{
  User user=null;
  try {
    user=_getUser();
  }
 catch (  PortalException e) {
    throw new WorkflowComponentException(e);
  }
catch (  SystemException e) {
    throw new WorkflowComponentException(e);
  }
  String userId=String.valueOf(user.getUserId());
  if (_log.isDebugEnabled()) {
    _log.debug("User is: " + userId);
  }
  return userId;
}
