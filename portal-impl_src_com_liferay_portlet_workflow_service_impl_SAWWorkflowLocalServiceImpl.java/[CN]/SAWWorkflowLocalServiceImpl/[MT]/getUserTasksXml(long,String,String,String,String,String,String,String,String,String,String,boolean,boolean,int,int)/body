{
  List<TaskVO> taskVOList=_getUserTasks(instanceId,taskName,definitionName,assignedTo,createDateGT,createDateLT,startDateGT,startDateLT,endDateGT,endDateLT,userId,hideEndedTasks,andOperator,begin,end);
  Document doc=DocumentHelper.createDocument();
  Element root=doc.addElement("result");
  for (int i=0; i < taskVOList.size(); i++) {
    TaskVO task=taskVOList.get(i);
    _createElement(task,root);
  }
  return doc.asXML();
}
