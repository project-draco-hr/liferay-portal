{
  TrashRenderer trashRenderer=null;
  if (Validator.isNotNull(className) && (classPK > 0)) {
    TrashHandler trashHandler=TrashHandlerRegistryUtil.getTrashHandler(className);
    trashRenderer=trashHandler.getTrashRenderer(classPK);
  }
  Format dateFormatDateTime=FastDateFormatFactoryUtil.getDateTime(themeDisplay.getLocale(),themeDisplay.getTimeZone());
  StringBundler sb=new StringBundler(3);
  sb.append(StringPool.OPEN_PARENTHESIS);
  sb.append(StringUtil.replace(dateFormatDateTime.format(new Date()),new String[]{StringPool.SLASH,StringPool.COLON},new String[]{StringPool.PERIOD,StringPool.PERIOD}));
  sb.append(StringPool.CLOSE_PARENTHESIS);
  if (trashRenderer != null) {
    return trashRenderer.getNewName(oldName,sb.toString());
  }
 else {
    return getNewName(oldName,sb.toString());
  }
}
