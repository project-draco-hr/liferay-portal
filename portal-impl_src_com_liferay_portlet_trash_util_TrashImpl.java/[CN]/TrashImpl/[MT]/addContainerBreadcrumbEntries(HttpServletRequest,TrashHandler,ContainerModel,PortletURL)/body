{
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  String rootContainerModelName=LanguageUtil.get(themeDisplay.getLocale(),trashHandler.getRootContainerModelName());
  List<ContainerModel> containerModels=trashHandler.getAncestors(containerModel.getContainerModelId());
  if ((containerModels == null) || containerModels.isEmpty()) {
    PortalUtil.addPortletBreadcrumbEntry(request,rootContainerModelName,null);
    return;
  }
  containerURL.setParameter("containerModelId","0");
  PortalUtil.addPortletBreadcrumbEntry(request,rootContainerModelName,containerURL.toString());
  Collections.reverse(containerModels);
  for (  ContainerModel curContainerModel : containerModels) {
    containerURL.setParameter("containerModelId",String.valueOf(curContainerModel.getContainerModelId()));
    PortalUtil.addPortletBreadcrumbEntry(request,curContainerModel.getContainerModelName(),containerURL.toString());
  }
  PortalUtil.addPortletBreadcrumbEntry(request,containerModel.getContainerModelName(),null);
}
