{
  if (!title.startsWith(prefix)) {
    return title;
  }
  title=title.substring(prefix.length());
  long trashEntryId=GetterUtil.getLong(title);
  if (trashEntryId <= 0) {
    return title;
  }
  try {
    TrashEntry trashEntry=TrashEntryLocalServiceUtil.getEntry(trashEntryId);
    title=trashEntry.getTypeSettingsProperty("title");
  }
 catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      _log.debug("No TrashEntry exists with ID " + String.valueOf(trashEntryId));
    }
  }
  return title;
}
