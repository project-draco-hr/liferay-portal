{
  ClassNameMapper classNameMapper=fetchByPrimaryKey(classNameMapperId);
  if (classNameMapper == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ClassNameMapper exists with the primary key " + classNameMapperId);
    }
    throw new NoSuchClassNameMapperException("No ClassNameMapper exists with the primary key " + classNameMapperId);
  }
  return classNameMapper;
}
