{
  ClassLoader classLoader=PortalClassLoaderUtil.getClassLoader();
  try {
    Class<?> keyConstants=classLoader.loadClass(socialActivityKeyClass);
    Integer socialActivity=(Integer)keyConstants.getField(socialActivityKey).get(null);
    addMapping(socialActivity,equityAction);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
