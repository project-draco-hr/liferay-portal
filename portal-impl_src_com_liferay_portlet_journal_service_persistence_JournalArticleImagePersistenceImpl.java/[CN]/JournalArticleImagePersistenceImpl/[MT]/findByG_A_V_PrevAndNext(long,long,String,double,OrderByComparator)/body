{
  JournalArticleImage journalArticleImage=findByPrimaryKey(articleImageId);
  Session session=null;
  try {
    session=openSession();
    JournalArticleImage[] array=new JournalArticleImageImpl[3];
    array[0]=getByG_A_V_PrevAndNext(session,journalArticleImage,groupId,articleId,version,orderByComparator,true);
    array[1]=journalArticleImage;
    array[2]=getByG_A_V_PrevAndNext(session,journalArticleImage,groupId,articleId,version,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
