{
  JournalArticleImage result=(JournalArticleImage)EntityCacheUtil.getResult(JournalArticleImageModelImpl.ENTITY_CACHE_ENABLED,JournalArticleImageImpl.class,articleImageId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      JournalArticleImage journalArticleImage=(JournalArticleImage)session.get(JournalArticleImageImpl.class,new Long(articleImageId));
      if (journalArticleImage != null) {
        cacheResult(journalArticleImage);
      }
      return journalArticleImage;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (JournalArticleImage)result;
  }
}
