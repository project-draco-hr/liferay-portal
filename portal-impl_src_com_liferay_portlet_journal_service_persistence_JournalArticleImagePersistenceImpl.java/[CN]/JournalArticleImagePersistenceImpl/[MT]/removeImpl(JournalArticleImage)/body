{
  Session session=null;
  try {
    session=openSession();
    session.delete(journalArticleImage);
    session.flush();
    return journalArticleImage;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(JournalArticleImage.class.getName());
  }
}
