{
  Session session=null;
  try {
    session=openSession();
    JournalArticleImage journalArticleImage=(JournalArticleImage)session.get(JournalArticleImageImpl.class,new Long(articleImageId));
    if (journalArticleImage == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No JournalArticleImage exists with the primary key " + articleImageId);
      }
      throw new NoSuchArticleImageException("No JournalArticleImage exists with the primary key " + articleImageId);
    }
    return remove(journalArticleImage);
  }
 catch (  NoSuchArticleImageException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
