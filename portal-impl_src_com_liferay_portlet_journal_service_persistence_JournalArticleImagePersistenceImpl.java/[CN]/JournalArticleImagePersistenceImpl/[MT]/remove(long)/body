{
  Session session=null;
  try {
    session=openSession();
    JournalArticleImage journalArticleImage=(JournalArticleImage)session.get(JournalArticleImageImpl.class,Long.valueOf(articleImageId));
    if (journalArticleImage == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + articleImageId);
      }
      throw new NoSuchArticleImageException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + articleImageId);
    }
    return journalArticleImagePersistence.remove(journalArticleImage);
  }
 catch (  NoSuchArticleImageException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
