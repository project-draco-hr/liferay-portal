{
  org.osgi.framework.ServiceReference<S>[] serviceReferences=_serviceTracker.getServiceReferences();
  if (serviceReferences == null) {
    return null;
  }
  ServiceReference<S>[] array=new ServiceReference[serviceReferences.length];
  for (int i=0; i < serviceReferences.length; i++) {
    org.osgi.framework.ServiceReference<S> serviceReference=serviceReferences[i];
    array[i]=new ServiceReferenceWrapper<S>(serviceReference);
  }
  return array;
}
