{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select serviceComponentId from ServiceComponent " + "where buildNamespace = ?");
    ps.setString(1,buildNamespace);
    rs=ps.executeQuery();
    if (rs.next()) {
      return true;
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
  return false;
}
