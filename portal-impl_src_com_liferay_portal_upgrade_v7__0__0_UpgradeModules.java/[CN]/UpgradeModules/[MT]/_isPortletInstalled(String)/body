{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select portletId from Portlet where portletId like ?");
    ps.setString(1,portletId);
    rs=ps.executeQuery();
    if (rs.next()) {
      return true;
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
  return false;
}
