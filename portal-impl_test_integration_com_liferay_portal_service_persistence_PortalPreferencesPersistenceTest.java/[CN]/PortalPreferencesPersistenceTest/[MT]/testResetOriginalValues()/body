{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  PortalPreferences newPortalPreferences=addPortalPreferences();
  _persistence.clearCache();
  PortalPreferencesModelImpl existingPortalPreferencesModelImpl=(PortalPreferencesModelImpl)_persistence.findByPrimaryKey(newPortalPreferences.getPrimaryKey());
  Assert.assertEquals(existingPortalPreferencesModelImpl.getOwnerId(),existingPortalPreferencesModelImpl.getOriginalOwnerId());
  Assert.assertEquals(existingPortalPreferencesModelImpl.getOwnerType(),existingPortalPreferencesModelImpl.getOriginalOwnerType());
}
