{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PortalPreferences.class,PortalPreferences.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("portalPreferencesId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("portalPreferencesId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
