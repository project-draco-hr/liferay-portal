{
  Object[] advices=DO_NOT_PROXY;
  if ((!SecurityManagerUtil.isPACLDisabled() && (beanName.endsWith(_FINDER_SUFFIX) || beanName.endsWith(_PERSISTENCE_SUFFIX))) || beanName.endsWith(_SERVICE_SUFFIX)) {
    advices=super.getAdvicesAndAdvisorsForBean(beanClass,beanName,targetSource);
    if (advices == DO_NOT_PROXY) {
      advices=PROXY_WITHOUT_ADDITIONAL_INTERCEPTORS;
    }
  }
  return advices;
}
