{
  try {
    Class<?> clazz=_bundle.loadClass(listenerClassName);
    String[] classNames=getListenerClassNames(clazz);
    Dictionary<String,Object> properties=new Hashtable<>();
    properties.put("osgi.http.whiteboard.context.select",servletContext.getServletContextName());
    properties.put("osgi.http.whiteboard.listener",Boolean.TRUE.toString());
    ServiceRegistration<?> serviceRegistration=bundleContext.registerService(classNames,clazz.newInstance(),properties);
    _serviceRegistrations.add(serviceRegistration);
  }
 catch (  Exception e) {
    log("Unable to create listener " + listenerClassName,e);
  }
}
