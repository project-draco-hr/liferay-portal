{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Message.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("messageId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("messageId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
