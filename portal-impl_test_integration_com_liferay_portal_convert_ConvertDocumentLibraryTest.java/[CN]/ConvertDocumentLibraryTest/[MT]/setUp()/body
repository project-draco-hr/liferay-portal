{
  PropsValues.DL_STORE_IMPL="com.liferay.portal.store.filesystem.FileSystemStore";
  _sourceStore=_storeFactory.getStoreInstance(PropsValues.DL_STORE_IMPL);
  _storeFactory.setStoreInstance(_sourceStore);
