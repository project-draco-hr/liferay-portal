{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId(),TestPropsValues.getUserId());
  DLAppTestUtil.populateServiceContext(serviceContext,Constants.ADD,DLFileEntryTypeConstants.FILE_ENTRY_TYPE_ID_ALL,true);
  return DLAppLocalServiceUtil.addFileEntry(TestPropsValues.getUserId(),_group.getGroupId(),folderId,fileName,mimeType,bytes,serviceContext);
}
