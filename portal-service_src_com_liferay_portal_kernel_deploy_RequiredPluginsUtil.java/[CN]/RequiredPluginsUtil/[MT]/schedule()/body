{
  _scheduledExecutorService=Executors.newSingleThreadScheduledExecutor(new NamedThreadFactory(RequiredPluginsUtil.class.getName(),Thread.NORM_PRIORITY,null));
  _scheduledExecutorService.scheduleWithFixedDelay(new Runnable(){
    @Override public void run(){
      doCheckRequiredPlugins();
    }
  }
,_DELAYED_TIME,_DELAYED_TIME,TimeUnit.MILLISECONDS);
}
