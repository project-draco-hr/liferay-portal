{
  try {
    byte[] bytes=null;
    if ((file != null) && file.exists()) {
      bytes=FileUtil.getBytes(file);
    }
    return updateImage(userId,imageId,groupId,folderId,name,description,bytes,contentType,serviceContext);
  }
 catch (  IOException ioe) {
    throw new SystemException(ioe);
  }
}
