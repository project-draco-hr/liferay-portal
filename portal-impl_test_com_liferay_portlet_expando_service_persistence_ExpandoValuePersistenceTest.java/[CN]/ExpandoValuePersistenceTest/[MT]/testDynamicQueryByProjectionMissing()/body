{
  ExpandoValue newExpandoValue=addExpandoValue();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ExpandoValue.class,ExpandoValue.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("valueId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("valueId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
