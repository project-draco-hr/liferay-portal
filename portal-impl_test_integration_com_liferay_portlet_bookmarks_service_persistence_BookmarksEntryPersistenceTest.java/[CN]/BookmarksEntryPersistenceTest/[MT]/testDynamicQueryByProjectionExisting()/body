{
  BookmarksEntry newBookmarksEntry=addBookmarksEntry();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(BookmarksEntry.class,BookmarksEntry.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("entryId"));
  Object newEntryId=newBookmarksEntry.getEntryId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("entryId",new Object[]{newEntryId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Object existingEntryId=result.get(0);
  assertEquals(existingEntryId,newEntryId);
}
