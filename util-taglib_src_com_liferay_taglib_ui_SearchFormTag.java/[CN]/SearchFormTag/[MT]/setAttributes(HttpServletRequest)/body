{
  SearchContainerTag parentTag=(SearchContainerTag)findAncestorWithClass(this,SearchContainerTag.class);
  if (parentTag != null) {
    _searchContainer=parentTag.getSearchContainer();
  }
  request.setAttribute("liferay-ui:search:searchContainer",_searchContainer);
  request.setAttribute("liferay-ui:search:showAddButton",String.valueOf(_showAddButton));
}
