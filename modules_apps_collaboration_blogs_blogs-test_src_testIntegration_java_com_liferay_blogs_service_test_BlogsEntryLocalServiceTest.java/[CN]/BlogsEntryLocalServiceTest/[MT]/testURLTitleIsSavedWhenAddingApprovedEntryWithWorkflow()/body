{
  String title=RandomTestUtil.randomString();
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group,_user.getUserId());
  BlogsEntry entry=BlogsTestUtil.addEntryWithWorkflow(_user.getUserId(),title,true,serviceContext);
  Assert.assertEquals(BlogsUtil.getUrlTitle(entry.getEntryId(),title),entry.getUrlTitle());
}
