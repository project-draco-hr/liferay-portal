{
  if (permissionChecker.hasOwnerPermission(fileEntry.getCompanyId(),DLFileEntry.class.getName(),fileEntry.getFileEntryId(),fileEntry.getUserId(),actionId)) {
    return true;
  }
  DLFolder folder=fileEntry.getFolder();
  return permissionChecker.hasPermission(folder.getGroupId(),DLFileEntry.class.getName(),fileEntry.getFileEntryId(),actionId);
}
