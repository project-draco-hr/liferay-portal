{
  Matcher matcher=_classPattern.matcher(_classContent);
  if (matcher.find()) {
    String accessModifier=matcher.group(1);
    if (accessModifier.equals(_ACCESS_MODIFIER_PRIVATE) || accessModifier.equals(_ACCESS_MODIFIER_PROTECTED) || accessModifier.equals(_ACCESS_MODIFIER_PUBLIC)) {
      return accessModifier;
    }
  }
  return _ACCESS_MODIFIER_UNKNOWN;
}
