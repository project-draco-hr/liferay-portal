{
  int methodType=javaTerm.getType();
  if ((methodType != JavaTerm.TYPE_METHOD_PUBLIC) && (methodType != JavaTerm.TYPE_METHOD_PUBLIC_STATIC)) {
    return;
  }
  checkAnnotationForMethod(javaTerm,"After","\\btearDown(?!Class)",JavaTerm.TYPE_METHOD_PUBLIC,_fileName);
  checkAnnotationForMethod(javaTerm,"AfterClass","\\btearDownClass",JavaTerm.TYPE_METHOD_PUBLIC_STATIC,_fileName);
  checkAnnotationForMethod(javaTerm,"Before","\\bsetUp(?!Class)",JavaTerm.TYPE_METHOD_PUBLIC,_fileName);
  checkAnnotationForMethod(javaTerm,"BeforeClass","\\bsetUpClass",JavaTerm.TYPE_METHOD_PUBLIC_STATIC,_fileName);
  checkAnnotationForMethod(javaTerm,"Test","^.*test",JavaTerm.TYPE_METHOD_PUBLIC,_fileName);
}
