{
  for (  String parameterName : javaTerm.getParameterNames()) {
    if (Validator.isVariableName(parameterName) && parameterName.matches("_?[A-Z].+")) {
      _javaSourceProcessor.processErrorMessage(_fileName,"Parameter " + parameterName + " should not start with uppercase: "+ _fileName+ " "+ javaTerm.getLineCount());
    }
  }
}
