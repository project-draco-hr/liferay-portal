{
  Matcher matcher=_chainingPattern.matcher(javaTerm.getContent());
  while (matcher.find()) {
    int lineCount=javaTerm.getLineCount() + _javaSourceProcessor.getLineCount(javaTerm.getContent(),matcher.end()) - 1;
    _javaSourceProcessor.processErrorMessage(_fileName,"chaining: " + _fileName + " "+ lineCount);
  }
}
