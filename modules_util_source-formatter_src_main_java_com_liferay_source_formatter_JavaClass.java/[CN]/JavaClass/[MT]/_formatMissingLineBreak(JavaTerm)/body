{
  String javaTermContent=javaTerm.getContent();
  Matcher matcher=_missingEmptyLinePattern.matcher(javaTermContent);
  if (matcher.find()) {
    String newJavaTermContent=StringUtil.insert(javaTermContent,"\n",matcher.start(1));
    _classContent=StringUtil.replace(_classContent,javaTermContent,newJavaTermContent);
  }
}
