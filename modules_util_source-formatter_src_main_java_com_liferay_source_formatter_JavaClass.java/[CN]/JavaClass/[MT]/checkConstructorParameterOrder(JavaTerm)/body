{
  int previousPos=-1;
  for (  String parameterName : javaTerm.getParameterNames()) {
    Pattern pattern=Pattern.compile("\\{\n([\\s\\S]*?)(_" + parameterName + " =[ \t\n]+"+ parameterName+ ";)");
    Matcher matcher=pattern.matcher(javaTerm.getContent());
    if (!matcher.find()) {
      continue;
    }
    String beforeParameter=matcher.group(1);
    if (beforeParameter.contains(parameterName + " =")) {
      continue;
    }
    int pos=matcher.start(2);
    if (previousPos > pos) {
      _javaSourceProcessor.processErrorMessage(_fileName,"Constructor parameter order " + parameterName + ": "+ _fileName);
      return;
    }
    previousPos=pos;
  }
}
