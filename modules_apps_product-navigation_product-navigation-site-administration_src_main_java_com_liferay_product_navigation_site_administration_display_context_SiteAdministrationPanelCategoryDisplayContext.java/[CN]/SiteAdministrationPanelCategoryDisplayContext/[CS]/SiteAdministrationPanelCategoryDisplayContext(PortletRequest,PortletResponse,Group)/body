{
  _portletRequest=portletRequest;
  _portletResponse=portletResponse;
  if (group != null) {
    _group=group;
  }
  _panelCategoryHelper=(PanelCategoryHelper)_portletRequest.getAttribute(ApplicationListWebKeys.PANEL_CATEGORY_HELPER);
