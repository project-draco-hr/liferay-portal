{
  if (_stagingGroupURL != null) {
    return _stagingGroupURL;
  }
  _stagingGroupURL=StringPool.BLANK;
  Group group=getGroup();
  if (!group.isStagedRemotely() && group.hasStagingGroup()) {
    Group stagingGroup=StagingUtil.getStagingGroup(group.getGroupId());
    if (stagingGroup != null) {
      _stagingGroupURL=_groupURLProvider.getGroupURL(stagingGroup,_portletRequest);
    }
  }
  return _stagingGroupURL;
}
