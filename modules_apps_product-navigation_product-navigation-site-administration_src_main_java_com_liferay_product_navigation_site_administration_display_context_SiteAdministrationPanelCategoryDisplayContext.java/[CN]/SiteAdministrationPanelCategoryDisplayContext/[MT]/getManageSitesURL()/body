{
  if (_manageSitesURL != null) {
    return _manageSitesURL;
  }
  _manageSitesURL=StringPool.BLANK;
  String portletId=PortletProviderUtil.getPortletId(Group.class.getName(),PortletProvider.Action.MANAGE);
  if (Validator.isNotNull(portletId) && PortletPermissionUtil.hasControlPanelAccessPermission(_themeDisplay.getPermissionChecker(),_themeDisplay.getScopeGroupId(),portletId)) {
    PortletURL portletURL=PortletProviderUtil.getPortletURL(_portletRequest,Group.class.getName(),PortletProvider.Action.MANAGE);
    _manageSitesURL=portletURL.toString();
  }
  return _manageSitesURL;
}
