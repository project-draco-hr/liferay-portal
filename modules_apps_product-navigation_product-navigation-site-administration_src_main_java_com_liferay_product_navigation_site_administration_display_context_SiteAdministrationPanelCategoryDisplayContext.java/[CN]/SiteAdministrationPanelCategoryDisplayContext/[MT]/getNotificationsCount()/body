{
  if (_notificationsCount != null) {
    return _notificationsCount.intValue();
  }
  SiteAdministrationPanelCategory siteAdministrationPanelCategory=(SiteAdministrationPanelCategory)_portletRequest.getAttribute(ApplicationListWebKeys.PANEL_CATEGORY);
  _notificationsCount=_panelCategoryHelper.getNotificationsCount(siteAdministrationPanelCategory.getKey(),_themeDisplay.getPermissionChecker(),getGroup(),_themeDisplay.getUser());
  return _notificationsCount;
}
