{
  if (_group != null) {
    return _group;
  }
  _group=_themeDisplay.getScopeGroup();
  if (!_group.isControlPanel()) {
    updateLatentGroup(_group.getGroupId());
    return _group;
  }
  HttpSession session=getSession();
  _group=LatentGroupManagerUtil.getLatentGroup(session);
  return _group;
}
