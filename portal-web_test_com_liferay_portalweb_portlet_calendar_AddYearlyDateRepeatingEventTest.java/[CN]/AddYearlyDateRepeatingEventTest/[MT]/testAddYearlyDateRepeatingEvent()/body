  selenium.click(RuntimeVariables.replace("link=Year"));
  selenium.waitForPageToLoad("30000");
  selenium.select("//select","label=2010");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
