{
  BundleWiring bundleWiring=_bundle.adapt(BundleWiring.class);
  for (  BundleWire bundleWire : bundleWiring.getRequiredWires("soy")) {
    Bundle providerBundle=getProviderBundle(bundleWire);
    List<URL> urls=getSoyResourceURLs(providerBundle,StringPool.SLASH);
    for (    URL url : urls) {
      String templateId=getTemplateId(providerBundle.getBundleId(),url);
      TemplateResource templateResource=TemplateResourceLoaderUtil.getTemplateResource(TemplateConstants.LANG_TYPE_SOY,templateId);
      templateResources.add(templateResource);
    }
  }
}
