{
  Session session=null;
  try {
    session=openSession();
    NoteHBM noteHBM=(NoteHBM)session.get(NoteHBM.class,noteId);
    if (noteHBM == null) {
      _log.warn("No Note exists with the primary key " + noteId.toString());
      throw new NoSuchNoteException("No Note exists with the primary key " + noteId.toString());
    }
    return NoteHBMUtil.model(noteHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
