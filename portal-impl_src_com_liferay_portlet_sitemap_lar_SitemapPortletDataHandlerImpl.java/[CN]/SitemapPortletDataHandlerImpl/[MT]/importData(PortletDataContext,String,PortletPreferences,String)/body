{
  try {
    long rootPlid=GetterUtil.getLong(prefs.getValue("root-plid",StringPool.BLANK));
    if (Validator.isNotNull(rootPlid)) {
      long newRootPlid=MapUtil.getLong(context.getNewPrimaryKeysMap(Layout.class),rootPlid);
      prefs.setValue("root-plid",String.valueOf(newRootPlid));
    }
    return prefs;
  }
 catch (  Exception e) {
    throw new PortletDataException(e);
  }
}
