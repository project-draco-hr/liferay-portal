{
  PortletURL itemSelectorURL=getItemSelectorPortletURL(inputEditorTaglibAttributes,portletURLBuilder,new ImageItemSelectorCriterion());
  if (itemSelectorURL != null) {
    jsonObject.put("filebrowserImageBrowseLinkUrl",itemSelectorURL.toString());
    jsonObject.put("filebrowserImageBrowseUrl",itemSelectorURL.toString());
  }
}
