{
  try {
    long classNameId=PortalUtil.getClassNameId(Group.class);
    CalendarResource calendarResource=_calendarResourceLocalService.fetchCalendarResource(classNameId,group.getGroupId());
    if (calendarResource == null) {
      return;
    }
    calendarResource.setNameMap(group.getNameMap());
    _calendarResourceLocalService.updateCalendarResource(calendarResource);
  }
 catch (  Exception e) {
    throw new ModelListenerException(e);
  }
}
