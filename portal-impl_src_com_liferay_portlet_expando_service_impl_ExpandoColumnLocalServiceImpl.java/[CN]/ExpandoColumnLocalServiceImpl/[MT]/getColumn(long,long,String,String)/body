{
  ExpandoTable table=expandoTablePersistence.fetchByC_C_N(companyId,classNameId,tableName);
  if (table == null) {
    return null;
  }
  List<ExpandoColumn> columns=expandoColumnPersistence.findByT_N(table.getTableId(),name);
  if (!columns.isEmpty()) {
    return columns.get(0);
  }
  return null;
}
