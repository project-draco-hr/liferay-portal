{
  AssetEntry assetEntry=(AssetEntry)EntityCacheUtil.getResult(AssetEntryModelImpl.ENTITY_CACHE_ENABLED,AssetEntryImpl.class,entryId);
  if (assetEntry == _nullAssetEntry) {
    return null;
  }
  if (assetEntry == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      assetEntry=(AssetEntry)session.get(AssetEntryImpl.class,Long.valueOf(entryId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (assetEntry != null) {
        cacheResult(assetEntry);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(AssetEntryModelImpl.ENTITY_CACHE_ENABLED,AssetEntryImpl.class,entryId,_nullAssetEntry);
      }
      closeSession(session);
    }
  }
  return assetEntry;
}
