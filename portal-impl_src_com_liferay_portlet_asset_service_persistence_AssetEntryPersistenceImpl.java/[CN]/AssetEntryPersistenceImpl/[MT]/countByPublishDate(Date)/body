{
  Object[] finderArgs=new Object[]{publishDate};
  Long count=(Long)FinderCacheUtil.getResult(FINDER_PATH_COUNT_BY_PUBLISHDATE,finderArgs,this);
  if (count == null) {
    StringBundler query=new StringBundler(2);
    query.append(_SQL_COUNT_ASSETENTRY_WHERE);
    if (publishDate == null) {
      query.append(_FINDER_COLUMN_PUBLISHDATE_PUBLISHDATE_1);
    }
 else {
      query.append(_FINDER_COLUMN_PUBLISHDATE_PUBLISHDATE_2);
    }
    String sql=query.toString();
    Session session=null;
    try {
      session=openSession();
      Query q=session.createQuery(sql);
      QueryPos qPos=QueryPos.getInstance(q);
      if (publishDate != null) {
        qPos.add(CalendarUtil.getTimestamp(publishDate));
      }
      count=(Long)q.uniqueResult();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (count == null) {
        count=Long.valueOf(0);
      }
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_PUBLISHDATE,finderArgs,count);
      closeSession(session);
    }
  }
  return count.intValue();
}
