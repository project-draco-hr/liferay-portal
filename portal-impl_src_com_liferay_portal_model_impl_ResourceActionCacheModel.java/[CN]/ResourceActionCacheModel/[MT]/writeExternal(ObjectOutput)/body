{
  objectOutput.writeLong(resourceActionId);
  if (name == null) {
    objectOutput.writeUTF(StringPool.BLANK);
  }
 else {
    objectOutput.writeUTF(name);
  }
  if (actionId == null) {
    objectOutput.writeUTF(StringPool.BLANK);
  }
 else {
    objectOutput.writeUTF(actionId);
  }
  objectOutput.writeLong(bitwiseValue);
}
