{
  String useDownloadURL=getRepository().getSettings().getProperty(RemotePluginPackageRepository.SETTING_USE_DOWNLOAD_URL);
  if (!GetterUtil.getBoolean(useDownloadURL,true)) {
    return getArtifactURL();
  }
  if (Validator.isNotNull(_downloadURL)) {
    return _downloadURL;
  }
  return getArtifactURL();
}
