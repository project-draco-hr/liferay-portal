{
  List<DDMStructure> ddmStructures=DDMStructureManagerUtil.getClassStructures(_fileEntry.getCompanyId(),PortalUtil.getClassNameId(DLFileEntryMetadata.class));
  DLFileEntryMetadata dlFileEntryMetadata=null;
  for (  DDMStructure ddmStructure : ddmStructures) {
    dlFileEntryMetadata=DLFileEntryMetadataLocalServiceUtil.fetchFileEntryMetadata(ddmStructure.getStructureId(),_fileVersion.getFileVersionId());
    if (dlFileEntryMetadata != null) {
      break;
    }
  }
  return dlFileEntryMetadata;
}
