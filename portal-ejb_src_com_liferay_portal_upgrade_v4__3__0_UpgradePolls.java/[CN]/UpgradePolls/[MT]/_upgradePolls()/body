{
  PKUpgradeColumnImpl pkUpgradeColumn=new PKUpgradeColumnImpl(0,true);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(PollsQuestionImpl.TABLE_NAME,PollsQuestionImpl.TABLE_COLUMNS,pkUpgradeColumn);
  upgradeTable.updateTable();
  _questionIdMapper=pkUpgradeColumn.getValueMapper();
  upgradeTable=new DefaultUpgradeTableImpl(PollsChoiceImpl.TABLE_NAME,PollsChoiceImpl.TABLE_COLUMNS,new SwapUpgradeColumnImpl("questionId",_questionIdMapper));
  upgradeTable=new DefaultUpgradeTableImpl(PollsVoteImpl.TABLE_NAME,PollsVoteImpl.TABLE_COLUMNS,new SwapUpgradeColumnImpl("questionId",_questionIdMapper));
  upgradeTable.updateTable();
}
