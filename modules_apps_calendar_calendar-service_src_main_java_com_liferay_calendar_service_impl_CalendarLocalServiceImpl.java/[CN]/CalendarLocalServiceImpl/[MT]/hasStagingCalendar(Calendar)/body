{
  long liveGroupId=calendar.getGroupId();
  try {
    Group stagingGroup=GroupLocalServiceUtil.getStagingGroup(liveGroupId);
    Calendar stagedCalendar=CalendarLocalServiceUtil.fetchCalendarByUuidAndGroupId(calendar.getUuid(),stagingGroup.getGroupId());
    if (stagedCalendar == null) {
      return false;
    }
    return true;
  }
 catch (  NoSuchGroupException nsge) {
    return false;
  }
}
