{
  try {
    if (_category.getCategoryId() == MBCategoryConstants.DEFAULT_PARENT_CATEGORY_ID) {
      return false;
    }
    ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
    if (MBCategoryPermission.contains(themeDisplay.getPermissionChecker(),_category,ActionKeys.UPDATE)) {
      return true;
    }
  }
 catch (  PortalException pe) {
  }
  return false;
}
