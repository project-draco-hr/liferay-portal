{
  POPServerUtil.start();
  Assert.assertTrue(_atomicState.isSet());
  List<MessageListener> messageListeners=POPServerUtil.getListeners();
  if (messageListeners != null) {
    for (    MessageListener messageListenerWrapper : messageListeners) {
      MessageListener messageListener=((MessageListenerWrapper)messageListenerWrapper).getMessageListener();
      Class<?> clazz=messageListener.getClass();
      if (TestMessageListener.class.getName().equals(clazz.getName())) {
        return;
      }
    }
  }
  Assert.fail();
}
