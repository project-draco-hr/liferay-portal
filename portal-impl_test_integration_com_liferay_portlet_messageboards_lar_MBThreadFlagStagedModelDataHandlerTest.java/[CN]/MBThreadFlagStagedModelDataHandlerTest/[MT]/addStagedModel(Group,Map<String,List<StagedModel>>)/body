{
  List<StagedModel> dependentStagedModels=dependentStagedModelsMap.get(MBMessage.class.getSimpleName());
  MBMessage message=(MBMessage)dependentStagedModels.get(0);
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId(),TestPropsValues.getUserId());
  return MBThreadFlagLocalServiceUtil.addThreadFlag(TestPropsValues.getUserId(),message.getThread(),serviceContext);
}
