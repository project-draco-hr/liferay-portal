{
  selenium.open("/web/guest/home/");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Wiki Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace("Wiki Test Page"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  selenium.clickAt("link=Recent Changes",RuntimeVariables.replace("Recent Changes"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertEquals(RuntimeVariables.replace("Wiki Page Title"),selenium.getText("//tr[3]/td[1]/a"));
  assertEquals(RuntimeVariables.replace("FrontPage"),selenium.getText("//tr[4]/td[1]/a"));
}
