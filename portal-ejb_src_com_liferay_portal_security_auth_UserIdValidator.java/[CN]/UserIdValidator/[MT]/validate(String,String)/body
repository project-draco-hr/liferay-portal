{
  if (Validator.isNull(userId) || Validator.isNumber(userId) || Validator.isEmailAddress(userId)|| (userId.equalsIgnoreCase("cyrus"))|| (userId.equalsIgnoreCase("postfix"))|| (userId.indexOf(User.DEFAULT) != -1)|| (userId.indexOf(companyId) != -1)|| (userId.indexOf("_") != -1)) {
    return false;
  }
 else {
    return true;
  }
}
