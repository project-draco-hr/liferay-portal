{
  addResponse(webDavRequest,resource,props,multistatus);
  if (resource.isCollection() && (depth != 0)) {
    Iterator<Resource> itr=storage.getResources(webDavRequest).iterator();
    while (itr.hasNext()) {
      resource=itr.next();
      addResponse(webDavRequest,resource,props,multistatus);
    }
  }
}
