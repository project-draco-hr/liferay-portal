{
  RepositoryHandler repositoryHandler=project.getRepositories();
  ArtifactRepository artifactRepository=repositoryHandler.findByName(ArtifactRepositoryContainer.DEFAULT_MAVEN_LOCAL_REPO_NAME);
  if (!(artifactRepository instanceof MavenArtifactRepository)) {
    return false;
  }
  MavenArtifactRepository mavenArtifactRepository=(MavenArtifactRepository)artifactRepository;
  Path repositoryPath=Paths.get(mavenArtifactRepository.getUrl());
  if (FileUtil.isChild(file,repositoryPath.toFile())) {
    return true;
  }
  return false;
}
