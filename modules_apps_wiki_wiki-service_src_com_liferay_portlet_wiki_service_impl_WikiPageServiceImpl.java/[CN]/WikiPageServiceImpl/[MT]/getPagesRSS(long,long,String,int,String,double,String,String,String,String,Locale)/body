{
  WikiPagePermission.check(getPermissionChecker(),nodeId,title,ActionKeys.VIEW);
  List<WikiPage> pages=wikiPageLocalService.getPages(nodeId,title,0,max,new PageCreateDateComparator(true));
  return exportToRSS(companyId,title,title,type,version,displayStyle,feedURL,entryURL,attachmentURLPrefix,pages,true,locale);
}
