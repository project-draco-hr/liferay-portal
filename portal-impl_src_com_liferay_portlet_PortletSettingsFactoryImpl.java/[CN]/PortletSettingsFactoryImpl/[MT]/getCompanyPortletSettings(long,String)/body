{
  PortletPreferences companyPortletPreferences=PortletPreferencesLocalServiceUtil.getPreferences(companyId,companyId,PortletKeys.PREFS_OWNER_TYPE_COMPANY,0,portletId);
  CompanyPortletSettings companyPortletSettings=new CompanyPortletSettings(companyPortletPreferences);
  Properties portalProperties=PropsUtil.getProperties(portletId,false);
  companyPortletSettings.setPortalProperties(portalProperties);
  return companyPortletSettings;
}
