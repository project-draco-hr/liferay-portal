{
  if (isNew) {
    Object[] args=new Object[]{portletModelImpl.getCompanyId(),portletModelImpl.getPortletId()};
    finderCache.putResult(FINDER_PATH_COUNT_BY_C_P,args,Long.valueOf(1));
    finderCache.putResult(FINDER_PATH_FETCH_BY_C_P,args,portletModelImpl);
  }
 else {
    if ((portletModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_C_P.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{portletModelImpl.getCompanyId(),portletModelImpl.getPortletId()};
      finderCache.putResult(FINDER_PATH_COUNT_BY_C_P,args,Long.valueOf(1));
      finderCache.putResult(FINDER_PATH_FETCH_BY_C_P,args,portletModelImpl);
    }
  }
}
