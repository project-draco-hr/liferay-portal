{
  if (isNew) {
    Object[] args=new Object[]{portlet.getCompanyId(),portlet.getPortletId()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_C_P,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_P,args,portlet);
  }
 else {
    PortletModelImpl portletModelImpl=(PortletModelImpl)portlet;
    if ((portletModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_C_P.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{portlet.getCompanyId(),portlet.getPortletId()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_C_P,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_P,args,portlet);
    }
  }
}
