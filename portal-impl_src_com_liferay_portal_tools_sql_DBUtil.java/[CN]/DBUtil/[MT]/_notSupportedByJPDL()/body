{
  if (this instanceof DB2Util || this instanceof InformixUtil || this instanceof InterBaseUtil|| this instanceof JDataStoreUtil|| this instanceof SAPUtil|| this instanceof SQLServerUtil) {
    if (_log.isDebugEnabled()) {
      _log.debug("JPDL is not supported on DB2,Informix," + "InterBase,JDataStore,SAP,SQLServer Databases");
    }
    return true;
  }
 else {
    return false;
  }
}
