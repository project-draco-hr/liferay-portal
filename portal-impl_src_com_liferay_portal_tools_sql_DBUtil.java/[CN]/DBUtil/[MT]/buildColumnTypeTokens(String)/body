{
  String[] words=StringUtil.split(line," ");
  String nullable="";
  if (words.length == 6) {
    nullable="not null;";
  }
 else   if (words.length == 5) {
    nullable=words[4];
  }
 else   if (words.length == 4) {
    nullable="not null;";
    if (words[3].endsWith(";")) {
      words[3]=words[3].substring(0,words[3].length() - 1);
    }
  }
  String[] template={words[1],words[2],"",words[3],nullable};
  return template;
}
