{
  try {
    boolean result=ProcessContext.attach("Child Process",Long.MAX_VALUE,new TestShutdownHook());
    if (!result || !ProcessContext.isAttached()) {
      throw new ProcessException("Attach failed!");
    }
    super.call();
  }
 catch (  Exception e) {
    throw new ProcessException(e);
  }
  return null;
}
