{
  try {
    InputStream inputStream=socket.getInputStream();
    OutputStream outputStream=socket.getOutputStream();
    outputStream.write(_echo);
    outputStream.flush();
    if (inputStream.read() == _echo) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  Exception e) {
    return false;
  }
}
