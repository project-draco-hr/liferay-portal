{
  try {
    ServerThread serverThread=new ServerThread(Thread.currentThread(),"Child Server Thread",_serverPort);
    serverThread.start();
  }
 catch (  Exception e) {
    throw new ProcessException(e);
  }
  try {
    Thread.sleep(Long.MAX_VALUE);
  }
 catch (  InterruptedException ie) {
  }
  return null;
}
