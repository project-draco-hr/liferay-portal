{
  DummyReturnProcessCallable dummyReturnProcessCallable=new DummyReturnProcessCallable();
  Future<String> future=ProcessExecutor.execute(_classPath,dummyReturnProcessCallable);
  String result=future.get(100,TimeUnit.SECONDS);
  assertEquals(DummyReturnProcessCallable.class.getName(),result);
  assertFalse(future.isCancelled());
  assertTrue(future.isDone());
  ReturnWithoutExitProcessCallable returnWithoutExitProcessCallable=new ReturnWithoutExitProcessCallable("");
  future=ProcessExecutor.execute(_classPath,returnWithoutExitProcessCallable);
  try {
    future.get(1,TimeUnit.SECONDS);
    fail();
  }
 catch (  TimeoutException te) {
  }
  assertFalse(future.isCancelled());
  assertFalse(future.isDone());
  ExecutorService executorService=_getExecutorService();
  executorService.shutdownNow();
  executorService.awaitTermination(10,TimeUnit.SECONDS);
  assertFalse(future.isCancelled());
  assertTrue(future.isDone());
  _nullOutExecutorService();
}
