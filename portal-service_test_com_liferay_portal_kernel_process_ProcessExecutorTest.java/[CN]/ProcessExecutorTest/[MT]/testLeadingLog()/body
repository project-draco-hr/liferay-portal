{
  String leadingLog="Test leading log.\n";
  String bodyLog="Test body log.\n";
  Logger logger=_getLogger();
  Level oldLevel=logger.getLevel();
  logger.setLevel(Level.WARNING);
  CaptureHandler captureHandler=new CaptureHandler();
  logger.addHandler(captureHandler);
  try {
    LeadingLogProcessCallable leadingLogProcessCallable=new LeadingLogProcessCallable(leadingLog,bodyLog);
    List<String> arguments=_createArguments();
    Future<String> future=ProcessExecutor.execute(_classPath,arguments,leadingLogProcessCallable);
    future.get();
    assertFalse(future.isCancelled());
    assertTrue(future.isDone());
    List<LogRecord> capturedLogRecords=captureHandler.getCapturedLogRecords();
    assertEquals(1,capturedLogRecords.size());
    LogRecord logRecord=capturedLogRecords.get(0);
    assertEquals("Found corrupted leading log: " + leadingLog,logRecord.getMessage());
  }
  finally {
    logger.removeHandler(captureHandler);
    logger.setLevel(oldLevel);
  }
  logger.setLevel(Level.FINE);
  captureHandler=new CaptureHandler();
  logger.addHandler(captureHandler);
  try {
    LeadingLogProcessCallable leadingLogProcessCallable=new LeadingLogProcessCallable(leadingLog,bodyLog);
    List<String> arguments=_createArguments();
    Future<String> future=ProcessExecutor.execute(_classPath,arguments,leadingLogProcessCallable);
    future.get();
    assertFalse(future.isCancelled());
    assertTrue(future.isDone());
    List<LogRecord> capturedLogRecords=captureHandler.getCapturedLogRecords();
    assertEquals(2,capturedLogRecords.size());
    LogRecord logRecord1=capturedLogRecords.get(0);
    assertEquals("Found corrupted leading log: " + leadingLog,logRecord1.getMessage());
    LogRecord logRecord2=capturedLogRecords.get(1);
    assertTrue(logRecord2.getMessage().contains("Invoked generic process callable "));
  }
  finally {
    logger.removeHandler(captureHandler);
    logger.setLevel(oldLevel);
  }
  logger.setLevel(Level.SEVERE);
  captureHandler=new CaptureHandler();
  logger.addHandler(captureHandler);
  try {
    LeadingLogProcessCallable leadingLogProcessCallable=new LeadingLogProcessCallable(leadingLog,bodyLog);
    List<String> arguments=_createArguments();
    Future<String> future=ProcessExecutor.execute(_classPath,arguments,leadingLogProcessCallable);
    future.get();
    assertFalse(future.isCancelled());
    assertTrue(future.isDone());
    List<LogRecord> capturedLogRecords=captureHandler.getCapturedLogRecords();
    assertEquals(0,capturedLogRecords.size());
  }
  finally {
    logger.removeHandler(captureHandler);
    logger.setLevel(oldLevel);
  }
}
