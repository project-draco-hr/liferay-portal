{
  Field objectOutputStreamField=ReflectionUtil.getDeclaredField(ProcessOutputStream.class,"_objectOutputStream");
  int modifiers=objectOutputStreamField.getModifiers();
  Field modifiersField=ReflectionUtil.getDeclaredField(Field.class,"modifiers");
  modifiersField.setInt(objectOutputStreamField,modifiers & ~Modifier.FINAL);
  return objectOutputStreamField;
}
