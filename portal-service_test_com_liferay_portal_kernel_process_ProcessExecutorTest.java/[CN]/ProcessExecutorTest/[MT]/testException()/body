{
  DummyExceptionProcessCallable dummyExceptionProcessCallable=new DummyExceptionProcessCallable();
  try {
    ProcessExecutor.execute(dummyExceptionProcessCallable,_classPath);
    fail();
  }
 catch (  ProcessException pe) {
    assertEquals(DummyExceptionProcessCallable.class.getName(),pe.getMessage());
  }
}
