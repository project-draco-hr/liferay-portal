{
  for (  String name : new String[]{_NAME_1,_NAME_2}) {
    for (    String actionId : new String[]{_ACTION_ID_1,_ACTION_ID_2}) {
      ResourceAction resourceAction=ResourceActionLocalServiceUtil.fetchResourceAction(name,actionId);
      if (resourceAction != null) {
        ResourceActionLocalServiceUtil.deleteResourceAction(resourceAction);
      }
    }
  }
}
