{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AnnouncementsFlag.class,AnnouncementsFlag.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("flagId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("flagId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
