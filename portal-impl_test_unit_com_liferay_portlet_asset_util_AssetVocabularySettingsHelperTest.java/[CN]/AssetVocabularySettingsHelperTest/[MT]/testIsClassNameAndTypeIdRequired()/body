{
  AssetVocabularySettingsHelper vocabularySettingsHelper=getVocabularySettingsHelper(AssetCategoryConstants.ALL_CLASS_NAME_IDS,false);
  Assert.assertFalse(vocabularySettingsHelper.isClassNameAndTypeRequired(1,1));
  Assert.assertFalse(vocabularySettingsHelper.isClassNameAndTypeRequired(2,2));
  vocabularySettingsHelper=getVocabularySettingsHelper(AssetCategoryConstants.ALL_CLASS_NAME_IDS,true);
  Assert.assertTrue(vocabularySettingsHelper.isClassNameAndTypeRequired(1,1));
  Assert.assertTrue(vocabularySettingsHelper.isClassNameAndTypeRequired(2,2));
  vocabularySettingsHelper=getVocabularySettingsHelper(1,1,false);
  Assert.assertFalse(vocabularySettingsHelper.isClassNameAndTypeRequired(1,1));
  Assert.assertFalse(vocabularySettingsHelper.isClassNameAndTypeRequired(2,2));
  vocabularySettingsHelper=getVocabularySettingsHelper(1,1,true);
  Assert.assertTrue(vocabularySettingsHelper.isClassNameAndTypeRequired(1,1));
  Assert.assertFalse(vocabularySettingsHelper.isClassNameAndTypeRequired(2,2));
}
