{
  AssetVocabularySettingsHelper vocabularySettingsHelper=getVocabularySettingsHelper(AssetCategoryConstants.ALL_CLASS_NAMES_ID,false);
  Assert.assertFalse(vocabularySettingsHelper.isClassNameAndTypeIdRequired(1,1));
  Assert.assertFalse(vocabularySettingsHelper.isClassNameAndTypeIdRequired(2,2));
  vocabularySettingsHelper=getVocabularySettingsHelper(AssetCategoryConstants.ALL_CLASS_NAMES_ID,true);
  Assert.assertTrue(vocabularySettingsHelper.isClassNameAndTypeIdRequired(1,1));
  Assert.assertTrue(vocabularySettingsHelper.isClassNameAndTypeIdRequired(2,2));
  vocabularySettingsHelper=getVocabularySettingsHelper(1,1,false);
  Assert.assertFalse(vocabularySettingsHelper.isClassNameAndTypeIdRequired(1,1));
  Assert.assertFalse(vocabularySettingsHelper.isClassNameAndTypeIdRequired(2,2));
  vocabularySettingsHelper=getVocabularySettingsHelper(1,1,true);
  Assert.assertTrue(vocabularySettingsHelper.isClassNameAndTypeIdRequired(1,1));
  Assert.assertFalse(vocabularySettingsHelper.isClassNameAndTypeIdRequired(2,2));
}
