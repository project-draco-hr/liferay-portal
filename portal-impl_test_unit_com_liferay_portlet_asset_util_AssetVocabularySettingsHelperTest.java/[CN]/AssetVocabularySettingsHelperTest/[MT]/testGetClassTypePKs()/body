{
  AssetVocabularySettingsHelper vocabularySettingsHelper=getVocabularySettingsHelper(1,2,true);
  long[] classTypePKs=vocabularySettingsHelper.getClassTypePKs();
  Assert.assertNotNull(classTypePKs);
  Assert.assertEquals(1,classTypePKs.length);
  Assert.assertEquals(2,classTypePKs[0]);
  vocabularySettingsHelper=new AssetVocabularySettingsHelper("multiValued=false\nselectedClassNameIds=1\n");
  classTypePKs=vocabularySettingsHelper.getClassTypePKs();
  Assert.assertNotNull(classTypePKs);
  Assert.assertEquals(1,classTypePKs.length);
  Assert.assertEquals(AssetCategoryConstants.ALL_CLASS_TYPE_PKS,classTypePKs[0]);
}
