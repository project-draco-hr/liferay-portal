{
  AssetVocabularySettingsHelper vocabularySettingsHelper=getVocabularySettingsHelper(AssetCategoryConstants.ALL_CLASS_NAME_ID,false);
  Assert.assertFalse(vocabularySettingsHelper.isClassNameIdAndClassTypePKRequired(1,1));
  Assert.assertFalse(vocabularySettingsHelper.isClassNameIdAndClassTypePKRequired(2,2));
  vocabularySettingsHelper=getVocabularySettingsHelper(AssetCategoryConstants.ALL_CLASS_NAME_ID,true);
  Assert.assertTrue(vocabularySettingsHelper.isClassNameIdAndClassTypePKRequired(1,1));
  Assert.assertTrue(vocabularySettingsHelper.isClassNameIdAndClassTypePKRequired(2,2));
  vocabularySettingsHelper=getVocabularySettingsHelper(1,1,false);
  Assert.assertFalse(vocabularySettingsHelper.isClassNameIdAndClassTypePKRequired(1,1));
  Assert.assertFalse(vocabularySettingsHelper.isClassNameIdAndClassTypePKRequired(2,2));
  vocabularySettingsHelper=getVocabularySettingsHelper(1,1,true);
  Assert.assertTrue(vocabularySettingsHelper.isClassNameIdAndClassTypePKRequired(1,1));
  Assert.assertFalse(vocabularySettingsHelper.isClassNameIdAndClassTypePKRequired(2,2));
}
