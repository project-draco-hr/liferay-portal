{
  if (_persistence == null) {
    _persistence=(KaleoInstanceTokenPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoInstanceTokenPersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoInstanceTokenUtil.class,"_persistence");
  }
  return _persistence;
}
