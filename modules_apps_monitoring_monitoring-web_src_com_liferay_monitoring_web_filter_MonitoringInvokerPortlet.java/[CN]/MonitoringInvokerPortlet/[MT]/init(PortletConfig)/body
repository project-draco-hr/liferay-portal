{
  LiferayPortletConfig liferayPortletConfig=(LiferayPortletConfig)portletConfig;
  _invokerPortlet.init(liferayPortletConfig);
  com.liferay.portal.model.Portlet portletModel=liferayPortletConfig.getPortlet();
