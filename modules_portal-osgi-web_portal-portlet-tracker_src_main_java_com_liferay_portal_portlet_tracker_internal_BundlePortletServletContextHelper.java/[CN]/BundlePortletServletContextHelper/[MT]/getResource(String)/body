{
  URL url=BundleUtil.getResourceInBundleOrFragments(_bundle,name);
  if (url == null) {
    if (name.startsWith("/")) {
      name=name.substring(1);
    }
    if (!name.startsWith("META-INF/resources/")) {
      url=BundleUtil.getResourceInBundleOrFragments(_bundle,"META-INF/resources/" + name);
    }
  }
  return url;
}
