{
  if (pollsQuestion.isNew()) {
    Object[] args=new Object[]{pollsQuestion.getUuid(),pollsQuestion.getGroupId()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_UUID_G,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,args,pollsQuestion);
  }
 else {
    PollsQuestionModelImpl pollsQuestionModelImpl=(PollsQuestionModelImpl)pollsQuestion;
    if ((pollsQuestionModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_UUID_G.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{pollsQuestion.getUuid(),pollsQuestion.getGroupId()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_UUID_G,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,args,pollsQuestion);
    }
  }
}
