{
  try {
    ActionUtil.getProductEntry(renderRequest);
    SCProductEntry productEntry=(SCProductEntry)renderRequest.getAttribute(WebKeys.SOFTWARE_CATALOG_PRODUCT_ENTRY);
    if (productEntry == null) {
      throw new NoSuchProductEntryException();
    }
  }
 catch (  Exception e) {
    if (e instanceof NoSuchProductEntryException || e instanceof PrincipalException) {
      SessionErrors.add(renderRequest,e.getClass());
      return actionMapping.findForward("portlet.software_catalog.error");
    }
 else {
      throw e;
    }
  }
  return actionMapping.findForward("portlet.software_catalog.view_product_entry");
}
