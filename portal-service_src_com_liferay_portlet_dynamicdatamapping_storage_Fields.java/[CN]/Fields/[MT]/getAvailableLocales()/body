{
  Set<Locale> availableLocales=new HashSet<Locale>();
  for (  Field field : _fieldsMap.values()) {
    if (field.isPrivate()) {
      continue;
    }
    for (    Locale availableLocale : field.getAvailableLocales()) {
      availableLocales.add(availableLocale);
    }
  }
  return availableLocales;
}
