{
  long ruleId=ParamUtil.getLong(resourceRequest,"ruleId");
  if (ruleId > 0) {
    MDRRule rule=_mdrRuleService.fetchRule(ruleId);
    resourceRequest.setAttribute(MDRWebKeys.MOBILE_DEVICE_RULES_RULE,rule);
  }
  String type=ParamUtil.getString(resourceRequest,"type");
  ActionUtil.includeEditorJSP(getPortletConfig(resourceRequest),resourceRequest,resourceResponse,ActionUtil.getRuleEditorJSP(type));
}
