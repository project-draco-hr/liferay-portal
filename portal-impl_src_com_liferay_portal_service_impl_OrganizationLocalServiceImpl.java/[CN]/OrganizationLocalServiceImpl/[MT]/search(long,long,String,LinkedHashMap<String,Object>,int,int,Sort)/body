{
  String name=null;
  String type=null;
  String street=null;
  String city=null;
  String zip=null;
  String region=null;
  String country=null;
  boolean andOperator=false;
  params.put("keywords",keywords);
  if (Validator.isNotNull(keywords)) {
    name=keywords;
    type=keywords;
    street=keywords;
    city=keywords;
    zip=keywords;
    region=keywords;
    country=keywords;
  }
 else {
    andOperator=true;
  }
  return search(companyId,parentOrganizationId,name,type,street,city,zip,region,country,params,andOperator,start,end,sort);
}
