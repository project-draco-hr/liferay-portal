{
  String parentOrganizationComparator=StringPool.EQUAL;
  if (parentOrganizationId == OrganizationConstants.ANY_PARENT_ORGANIZATION_ID) {
    parentOrganizationComparator=StringPool.NOT_EQUAL;
  }
  return organizationFinder.countByKeywords(companyId,parentOrganizationId,parentOrganizationComparator,keywords,type,regionId,countryId,params);
}
