{
  String regionCode=null;
  if (regionId != null) {
    Region region=regionPersistence.fetchByPrimaryKey(regionId);
    regionCode=region.getRegionCode();
  }
  String countryName=null;
  if (countryId != null) {
    Country country=countryPersistence.fetchByPrimaryKey(countryId);
    countryName=country.getName();
  }
  return buildSearchContext(companyId,parentOrganizationId,name,type,street,city,zip,regionCode,countryName,params,andSearch,start,end,sort);
}
