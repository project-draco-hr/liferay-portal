{
  String parentOrganizationComparator=StringPool.EQUAL;
  if (parentOrganizationId == OrganizationConstants.ANY_PARENT_ORGANIZATION_ID) {
    parentOrganizationComparator=StringPool.NOT_EQUAL;
  }
  return organizationFinder.countByC_PO_N_T_S_C_Z_R_C(companyId,parentOrganizationId,parentOrganizationComparator,name,type,street,city,zip,regionId,countryId,params,andOperator);
}
