{
  List organizations=null;
  if (organizationId == OrganizationImpl.DEFAULT_PARENT_ORGANIZATION_ID) {
    organizations=new ArrayList();
  }
 else {
    Organization organization=organizationPersistence.findByPrimaryKey(organizationId);
    organizations=getAncestorOrganizations(organization.getParentOrganizationId());
    organizations.add(organization);
  }
  return organizations;
}
