{
  if (organizationId == OrganizationConstants.DEFAULT_PARENT_ORGANIZATION_ID) {
    return false;
  }
  Organization organization=organizationPersistence.findByPrimaryKey(organizationId);
  String treePath=organization.getTreePath();
  if (treePath.contains(StringPool.SLASH + parentOrganizationId + StringPool.SLASH)) {
    return true;
  }
 else {
    return false;
  }
}
