{
  if ((organizationId == OrganizationConstants.DEFAULT_PARENT_ORGANIZATION_ID) && (groupId == GroupConstants.DEFAULT_PARENT_GROUP_ID)) {
    return true;
  }
  if (organizationId != OrganizationConstants.DEFAULT_PARENT_ORGANIZATION_ID) {
    Organization organization=organizationPersistence.fetchByPrimaryKey(organizationId);
    if (organization.getGroupId() == groupId) {
      return true;
    }
  }
  return false;
}
