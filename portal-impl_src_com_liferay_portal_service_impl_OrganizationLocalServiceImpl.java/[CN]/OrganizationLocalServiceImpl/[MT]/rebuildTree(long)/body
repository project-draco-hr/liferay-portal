{
  TreePathUtil.rebuildTree(companyId,OrganizationConstants.DEFAULT_PARENT_ORGANIZATION_ID,new TreeModelFinder<Organization>(){
    @Override public List<Organization> findTreeModels(    long previousId,    long companyId,    long parentPrimaryKey,    int size){
      return organizationPersistence.findByO_C_P(previousId,companyId,parentPrimaryKey,QueryUtil.ALL_POS,size,new OrganizationIdComparator(true));
    }
  }
);
}
