{
  User user=userPersistence.findByPrimaryKey(userId);
  Company company=companyPersistence.findByPrimaryKey(user.getCompanyId());
  Group companyGroup=company.getGroup();
  assetEntryLocalService.updateEntry(userId,companyGroup.getGroupId(),null,null,Organization.class.getName(),organization.getOrganizationId(),organization.getUuid(),0,assetCategoryIds,assetTagNames,true,false,null,null,null,null,organization.getName(),StringPool.BLANK,null,null,null,0,0,null);
}
