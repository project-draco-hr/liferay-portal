{
  try {
    SearchContext searchContext=getSearchContext(companyId,parentOrganizationId,name,type,street,city,zip,region,country,params,andSearch,start,end,sort);
    Indexer indexer=IndexerRegistryUtil.nullSafeGetIndexer(Organization.class);
    return indexer.search(searchContext);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
