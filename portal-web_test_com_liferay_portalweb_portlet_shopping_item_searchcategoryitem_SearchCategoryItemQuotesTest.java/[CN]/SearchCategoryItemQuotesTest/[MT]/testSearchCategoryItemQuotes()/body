{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Shopping Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace("Shopping Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Categories",RuntimeVariables.replace("Categories"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_34_keywords1']",RuntimeVariables.replace("\"Item Test\""));
  selenium.click(RuntimeVariables.replace("//input[@value='Search Categories']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Item Test\nThis is an item test.\nLimited: Time Only"));
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Shopping Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace("Shopping Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Categories",RuntimeVariables.replace("Categories"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_34_keywords1']",RuntimeVariables.replace("\"Item1 Test1\""));
  selenium.click(RuntimeVariables.replace("//input[@value='Search Categories']"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isTextPresent("Item Test\nThis is an item test.\nLimited: Time Only"));
}
