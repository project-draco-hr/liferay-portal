{
  StringMaker sm=new StringMaker();
  for (int i=0; i < list.size(); i++) {
    Object bean=list.get(i);
    Object value=BeanUtil.getObject(bean,param);
    if (value == null) {
      value=StringPool.BLANK;
    }
    sm.append(value.toString());
    if ((i + 1) != list.size()) {
      sm.append(delimiter);
    }
  }
  return sm.toString();
}
