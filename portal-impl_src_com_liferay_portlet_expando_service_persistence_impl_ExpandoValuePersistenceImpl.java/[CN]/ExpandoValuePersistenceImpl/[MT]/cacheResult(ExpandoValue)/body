{
  EntityCacheUtil.putResult(ExpandoValueModelImpl.ENTITY_CACHE_ENABLED,ExpandoValueImpl.class,expandoValue.getPrimaryKey(),expandoValue);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_R,new Object[]{expandoValue.getColumnId(),expandoValue.getRowId()},expandoValue);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_T_C_C,new Object[]{expandoValue.getTableId(),expandoValue.getColumnId(),expandoValue.getClassPK()},expandoValue);
  expandoValue.resetOriginalValues();
}
