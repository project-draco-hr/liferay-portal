{
  Serializable serializable=entityCache.getResult(ExpandoValueModelImpl.ENTITY_CACHE_ENABLED,ExpandoValueImpl.class,primaryKey);
  if (serializable == nullModel) {
    return null;
  }
  ExpandoValue expandoValue=(ExpandoValue)serializable;
  if (expandoValue == null) {
    Session session=null;
    try {
      session=openSession();
      expandoValue=(ExpandoValue)session.get(ExpandoValueImpl.class,primaryKey);
      if (expandoValue != null) {
        cacheResult(expandoValue);
      }
 else {
        entityCache.putResult(ExpandoValueModelImpl.ENTITY_CACHE_ENABLED,ExpandoValueImpl.class,primaryKey,nullModel);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(ExpandoValueModelImpl.ENTITY_CACHE_ENABLED,ExpandoValueImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return expandoValue;
}
