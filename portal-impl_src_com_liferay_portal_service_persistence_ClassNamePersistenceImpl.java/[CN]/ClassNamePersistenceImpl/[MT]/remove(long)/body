{
  Session session=null;
  try {
    session=openSession();
    ClassName className=(ClassName)session.get(ClassNameImpl.class,new Long(classNameId));
    if (className == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + classNameId);
      }
      throw new NoSuchClassNameException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + classNameId);
    }
    return remove(className);
  }
 catch (  NoSuchClassNameException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
