{
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(className);
    }
 else {
      if (className.isNew()) {
        session.save(className);
      }
    }
    session.flush();
    className.setNew(false);
    return className;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ClassName.class.getName());
  }
}
