{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,className,merge);
    className.setNew(false);
    return className;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ClassName.class.getName());
  }
}
