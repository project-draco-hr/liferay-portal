{
  Session session=null;
  try {
    session=openSession();
    session.delete(className);
    session.flush();
    return className;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(ClassName.class.getName());
  }
}
