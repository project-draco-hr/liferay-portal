{
  Session session=null;
  try {
    session=openSession();
    if (BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(ClassNameImpl.class,className.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(className);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  ClassNameModelImpl classNameModelImpl=(ClassNameModelImpl)className;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_VALUE,new Object[]{classNameModelImpl.getOriginalValue()});
  EntityCacheUtil.removeResult(ClassNameModelImpl.ENTITY_CACHE_ENABLED,ClassName.class,className.getPrimaryKey());
  return className;
}
