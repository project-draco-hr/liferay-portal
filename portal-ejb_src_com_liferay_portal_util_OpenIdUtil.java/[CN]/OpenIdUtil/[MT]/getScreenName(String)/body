{
  String result=openid;
  if (result.startsWith("http://")) {
    result=result.substring("http://".length());
  }
  if (result.endsWith("/")) {
    result=result.substring(0,result.length() - 1);
  }
  StringUtils.replace(result,"/",".");
  StringUtils.replace(result,"_",".");
  return result;
}
