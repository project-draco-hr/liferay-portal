{
  String key=String.valueOf(companyId);
  Boolean value=(Boolean)_isCompanyAdmin.get(key);
  if (value == null) {
    boolean hasAdminRole=RoleServiceUtil.hasUserRole(_userId,companyId,RoleImpl.ADMINISTRATOR,true);
    value=new Boolean(hasAdminRole);
    _isCompanyAdmin.put(key,value);
  }
  return value.booleanValue();
}
