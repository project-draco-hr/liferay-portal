{
  Boolean value=PermissionCacheUtil.getUserPrimaryKeyRole(permissionChecker.getUserId(),group.getGroupId(),RoleConstants.SITE_OWNER);
  try {
    if (value == null) {
      value=isGroupOwnerImpl(permissionChecker,group);
      PermissionCacheUtil.putUserPrimaryKeyRole(getUserId(),group.getGroupId(),RoleConstants.SITE_OWNER,value);
    }
  }
 catch (  Exception e) {
    PermissionCacheUtil.removeUserPrimaryKeyRole(getUserId(),group.getGroupId(),RoleConstants.SITE_OWNER);
    throw e;
  }
  return value;
}
