{
  if (_roleIds == null) {
    List<Role> roles=ListUtil.fromCollection(getRoles());
    long[] roleIds=new long[roles.size()];
    for (int i=0; i < roles.size(); i++) {
      Role role=roles.get(i);
      roleIds[i]=role.getRoleId();
    }
    Arrays.sort(roleIds);
    _roleIds=roleIds;
  }
  return _roleIds;
}
