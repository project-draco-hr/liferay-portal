{
  Boolean value=PermissionCacheUtil.getUserPrimaryKeyRole(getUserId(),organization.getOrganizationId(),RoleConstants.ORGANIZATION_OWNER);
  try {
    if (value == null) {
      value=isOrganizationOwnerImpl(permissionChecker,organization);
      PermissionCacheUtil.putUserPrimaryKeyRole(getUserId(),organization.getOrganizationId(),RoleConstants.ORGANIZATION_OWNER,value);
    }
  }
 catch (  Exception e) {
    PermissionCacheUtil.removeUserPrimaryKeyRole(getUserId(),organization.getOrganizationId(),RoleConstants.ORGANIZATION_OWNER);
    throw e;
  }
  return value;
}
