{
  _cacheManager.addCache(_PORTAL_CACHE_NAME);
  Cache cache=_cacheManager.getCache(_PORTAL_CACHE_NAME);
  PortalCacheManager<String,String> portalCacheManager=TestPortalCacheManager.createTestPortalCacheManager(_PORTAL_CACHE_NAME);
  _ehcachePortalCache=new EhcachePortalCache<>(portalCacheManager,cache);
  _ehcachePortalCache.put(_KEY_1,_VALUE_1);
  _defaultCacheListener=new TestCacheListener<>();
  _ehcachePortalCache.registerCacheListener(_defaultCacheListener);
  _defaultCacheReplicator=new TestCacheReplicator<>();
  _ehcachePortalCache.registerCacheListener(_defaultCacheReplicator);
}
