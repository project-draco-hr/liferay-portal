{
  ActionAdapter actionAdapter=(ActionAdapter)StrutsActionRegistryUtil.getAction("TestStrutsAction");
  Assert.assertNotNull(actionAdapter);
  _atomicState.reset();
  actionAdapter.execute(null,null,null,null);
  Assert.assertTrue(_atomicState.isSet());
  PortletActionAdapter portletActionAdapter=(PortletActionAdapter)StrutsActionRegistryUtil.getAction("TestStrutsPortletAction");
  Assert.assertNotNull(portletActionAdapter);
  _atomicState.reset();
  portletActionAdapter.isCheckMethodOnProcessAction();
  Assert.assertTrue(_atomicState.isSet());
}
