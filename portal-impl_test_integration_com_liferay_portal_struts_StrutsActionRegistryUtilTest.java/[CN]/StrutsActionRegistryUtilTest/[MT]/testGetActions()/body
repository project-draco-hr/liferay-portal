{
  Map<String,org.apache.struts.action.Action> actions=StrutsActionRegistryUtil.getActions();
  ActionAdapter actionAdapter=(ActionAdapter)actions.get("TestStrutsAction");
  Assert.assertNotNull(actionAdapter);
  _atomicState.reset();
  actionAdapter.execute(null,null,null,null);
  Assert.assertTrue(_atomicState.isSet());
  PortletActionAdapter portletActionAdapter=(PortletActionAdapter)actions.get("TestStrutsPortletAction");
  Assert.assertNotNull(portletActionAdapter);
  _atomicState.reset();
  portletActionAdapter.isCheckMethodOnProcessAction();
  Assert.assertTrue(_atomicState.isSet());
}
