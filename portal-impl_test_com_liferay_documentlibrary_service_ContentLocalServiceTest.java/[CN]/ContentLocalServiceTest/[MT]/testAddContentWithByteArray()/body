{
  Content content=createContent();
  _service.addContent(content.getCompanyId(),content.getPortletId(),content.getGroupId(),content.getRepositoryId(),content.getPath(),content.getVersion(),new byte[1024]);
  boolean exists=_service.hasContent(content.getCompanyId(),content.getRepositoryId(),content.getPath(),content.getVersion());
  assertTrue(exists);
  Content content2=_service.getContent(content.getCompanyId(),content.getRepositoryId(),content.getPath());
  Content content3=_service.getContent(content.getCompanyId(),content.getRepositoryId(),content.getPath());
  assertEquals(content2,content3);
}
