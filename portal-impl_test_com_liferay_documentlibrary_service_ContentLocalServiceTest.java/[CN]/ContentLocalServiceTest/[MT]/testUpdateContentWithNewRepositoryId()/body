{
  Content content=addContent();
  long newRepositoryId=nextLong();
  _service.updateContent(content.getCompanyId(),content.getPortletId(),content.getGroupId(),content.getRepositoryId(),newRepositoryId,content.getPath());
  Content newContent=_service.getContent(content.getCompanyId(),newRepositoryId,content.getPath());
  assertFalse(content.getRepositoryId() == newContent.getRepositoryId());
  assertFalse(content.equals(newContent));
}
