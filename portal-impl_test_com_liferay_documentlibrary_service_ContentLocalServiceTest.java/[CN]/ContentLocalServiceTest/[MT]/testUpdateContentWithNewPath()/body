{
  Content content=addContent();
  String newPath=randomString();
  _service.updateContent(content.getCompanyId(),content.getPortletId(),content.getGroupId(),content.getRepositoryId(),content.getPath(),newPath);
  Content newContent=_service.getContent(content.getCompanyId(),content.getRepositoryId(),newPath);
  assertFalse(content.getPath().equals(newContent.getPath()));
  assertFalse(content.equals(newContent));
}
