{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    Folder folder=ActionUtil.getFolder(portletRequest);
    return DLFolderPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),folder.getFolderId(),ActionKeys.DELETE);
  }
 catch (  Exception pe) {
  }
  return false;
}
