{
  Fields expectedFields=new Fields();
  Field linkToPageField=getLinkToPageField(_ddmStructure.getStructureId());
  expectedFields.put(linkToPageField);
  Field fieldsDisplayField=getFieldsDisplayField(_ddmStructure.getStructureId(),"link_INSTANCE_MiO7vIJu");
  expectedFields.put(fieldsDisplayField);
  String xml=readText("sample-journal-content-link-to-page-field.xml");
  Fields actualFields=JournalConverterUtil.getDDMFields(_ddmStructure,xml);
  assertEquals(expectedFields,actualFields);
}
