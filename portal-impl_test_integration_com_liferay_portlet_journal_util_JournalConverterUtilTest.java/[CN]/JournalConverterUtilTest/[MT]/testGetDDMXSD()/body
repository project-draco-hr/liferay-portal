{
  DDMStructure expectedDDMStructure=new DDMStructureImpl();
  expectedDDMStructure.setXsd(readText("test-ddm-structure-all-fields.xml"));
  DDMStructure actualDDMStructure=new DDMStructureImpl();
  actualDDMStructure.setXsd(JournalConverterUtil.getDDMXSD(readText("test-journal-structure-all-fields.xml")));
  Assert.assertEquals(expectedDDMStructure.getFieldsMap(),actualDDMStructure.getFieldsMap());
}
