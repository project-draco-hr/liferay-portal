{
  Fields fields=new Fields();
  Field booleanField=getBooleanField(_ddmStructure.getStructureId());
  fields.put(booleanField);
  Field fieldsDisplayField=new Field(DDMImpl.FIELDS_DISPLAY_NAME,"boolean_INSTANCE_Okhyj6Ni,boolean_INSTANCE_1SYNQuhg");
  fields.put(fieldsDisplayField);
  String expectedXML=readText("sample-journal-content-boolean-repeatable-field.xml");
  String actualXML=JournalConverterUtil.getXML(_ddmStructure,fields);
  assertEquals(expectedXML,actualXML);
}
