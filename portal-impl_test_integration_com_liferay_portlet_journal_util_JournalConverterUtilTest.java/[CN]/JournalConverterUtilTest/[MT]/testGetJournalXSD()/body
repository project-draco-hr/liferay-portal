{
  String expectedXSD=readText("test-journal-structure-all-fields.xml");
  Map<String,Map<String,String>> expectedMap=JournalTestUtil.getXsdMap(expectedXSD);
  String actualXSD=JournalConverterUtil.getJournalXSD(readText("test-ddm-structure-all-fields.xml"));
  Map<String,Map<String,String>> actualMap=JournalTestUtil.getXsdMap(actualXSD);
  Assert.assertEquals(expectedMap,actualMap);
}
