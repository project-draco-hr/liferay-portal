{
  Fields fields=new Fields();
  Field textBoxField=getTextBoxField(_ddmStructure.getStructureId());
  fields.put(textBoxField);
  Field fieldsDisplayField=getFieldsDisplayField(_ddmStructure.getStructureId(),"text_box_INSTANCE_ND057krU,text_box_INSTANCE_HvemvQgl," + "text_box_INSTANCE_enAnbvq6");
  fields.put(fieldsDisplayField);
  String expectedXML=readText("test-journal-content-text-box-repeatable-field.xml");
  String actualXML=JournalConverterUtil.getXML(_ddmStructure,fields);
  assertEquals(expectedXML,actualXML);
}
