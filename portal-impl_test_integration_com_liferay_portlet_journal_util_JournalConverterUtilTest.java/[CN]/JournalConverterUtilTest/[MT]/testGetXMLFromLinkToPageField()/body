{
  Fields fields=new Fields();
  Field linkToPageField=getLinkToPageField(_ddmStructure.getStructureId());
  fields.put(linkToPageField);
  Field fieldsDisplay=new Field(DDMImpl.FIELDS_DISPLAY_NAME,"link_INSTANCE_MiO7vIJu");
  fields.put(fieldsDisplay);
  String expectedXML=readText("sample-journal-content-link-to-page-field.xml");
  String actualXML=JournalConverterUtil.getXML(_ddmStructure,fields);
  assertEquals(expectedXML,actualXML);
}
