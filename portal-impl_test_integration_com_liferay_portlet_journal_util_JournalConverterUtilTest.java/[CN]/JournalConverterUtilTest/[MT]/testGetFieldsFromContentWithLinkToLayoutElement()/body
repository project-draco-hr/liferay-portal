{
  Fields expectedFields=new Fields();
  Field linkToLayoutField=getLinkToLayoutField(_ddmStructure.getStructureId());
  expectedFields.put(linkToLayoutField);
  Field fieldsDisplayField=getFieldsDisplayField(_ddmStructure.getStructureId(),"link_to_layout_INSTANCE_MiO7vIJu");
  expectedFields.put(fieldsDisplayField);
  String content=readText("test-journal-content-link-to-page-field.xml");
  Fields actualFields=JournalConverterUtil.getDDMFields(_ddmStructure,content);
  assertEquals(expectedFields,actualFields);
}
