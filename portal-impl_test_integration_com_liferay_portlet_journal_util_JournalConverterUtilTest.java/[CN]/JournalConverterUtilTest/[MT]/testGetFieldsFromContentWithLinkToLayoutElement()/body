{
  Fields expectedFields=new Fields();
  Map<String,Layout> layoutsMap=getLayoutsMap();
  Field linkToLayoutField=getLinkToLayoutField(_ddmStructure.getStructureId(),layoutsMap);
  expectedFields.put(linkToLayoutField);
  Field fieldsDisplayField=getFieldsDisplayField(_ddmStructure.getStructureId(),"link_to_layout_INSTANCE_MiO7vIJu," + "link_to_layout_INSTANCE_9FLzJNUX," + "link_to_layout_INSTANCE_WqABvmxw,"+ "link_to_layout_INSTANCE_31abnWkB,"+ "link_to_layout_INSTANCE_pWIUF15B,"+ "link_to_layout_INSTANCE_OGQypdcj,"+ "link_to_layout_INSTANCE_TB2XZ3wn,"+ "link_to_layout_INSTANCE_3IRNS4jM");
  expectedFields.put(fieldsDisplayField);
  String content=replaceLinksToLayoutsParameters(readText("test-journal-content-link-to-page-field.xml"),layoutsMap);
  Fields actualFields=JournalConverterUtil.getDDMFields(_ddmStructure,content);
  assertEquals(expectedFields,actualFields);
}
