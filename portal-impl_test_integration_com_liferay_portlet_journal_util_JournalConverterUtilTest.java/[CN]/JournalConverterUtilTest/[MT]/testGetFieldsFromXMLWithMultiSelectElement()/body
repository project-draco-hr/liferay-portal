{
  Fields expectedFields=new Fields();
  Field multiSelectField=getMultiSelectField(_ddmStructure.getStructureId());
  expectedFields.put(multiSelectField);
  Field fieldsDisplay=new Field(DDMImpl.FIELDS_DISPLAY_NAME,"multi_select_INSTANCE_9X5wVsSv");
  expectedFields.put(fieldsDisplay);
  String xml=readText("sample-journal-content-multi-list-field.xml");
  Fields actualFields=JournalConverterUtil.getDDMFields(_ddmStructure,xml);
  assertEquals(expectedFields,actualFields);
}
