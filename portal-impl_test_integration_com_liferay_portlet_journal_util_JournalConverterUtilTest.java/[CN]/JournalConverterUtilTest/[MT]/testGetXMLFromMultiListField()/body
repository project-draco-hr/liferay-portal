{
  Fields fields=new Fields();
  Field multiListField=getMultiListField(_ddmStructure.getStructureId());
  fields.put(multiListField);
  Field fieldsDisplayField=getFieldsDisplayField(_ddmStructure.getStructureId(),"multi-list_INSTANCE_9X5wVsSv");
  fields.put(fieldsDisplayField);
  String expectedXML=readText("test-journal-content-multi-list-field.xml");
  String actualXML=JournalConverterUtil.getXML(_ddmStructure,fields);
  assertEquals(expectedXML,actualXML);
}
