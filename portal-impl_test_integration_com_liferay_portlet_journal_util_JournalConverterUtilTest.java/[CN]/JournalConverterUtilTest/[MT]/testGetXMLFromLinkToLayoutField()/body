{
  Fields fields=new Fields();
  Field linkToLayoutField=getLinkToLayoutField(_ddmStructure.getStructureId());
  fields.put(linkToLayoutField);
  Field fieldsDisplayField=getFieldsDisplayField(_ddmStructure.getStructureId(),"link_to_layout_INSTANCE_MiO7vIJu");
  fields.put(fieldsDisplayField);
  String expectedXML=readText("test-journal-content-link-to-page-field.xml");
  String actualXML=JournalConverterUtil.getXML(_ddmStructure,fields);
  assertEquals(expectedXML,actualXML);
}
