{
  Fields fields=new Fields();
  Map<String,Layout> layouts=getLayoutsMap();
  Field linkToLayoutField=getLinkToLayoutField(_ddmStructure.getStructureId(),layouts);
  fields.put(linkToLayoutField);
  Field fieldsDisplayField=getFieldsDisplayField(_ddmStructure.getStructureId(),"link_to_layout_INSTANCE_MiO7vIJu," + "link_to_layout_INSTANCE_9FLzJNUX," + "link_to_layout_INSTANCE_WqABvmxw,"+ "link_to_layout_INSTANCE_31abnWkB,"+ "link_to_layout_INSTANCE_pWIUF15B,"+ "link_to_layout_INSTANCE_OGQypdcj,"+ "link_to_layout_INSTANCE_TB2XZ3wn,"+ "link_to_layout_INSTANCE_3IRNS4jM");
  fields.put(fieldsDisplayField);
  String expectedContent=replaceLinksToLayoutsParameters(readText("test-journal-content-link-to-page-field.xml"),layouts);
  String actualContent=JournalConverterUtil.getContent(_ddmStructure,fields);
  assertEquals(expectedContent,actualContent);
}
