{
  LazyInitializeSupport lazyInitializeSupport=new LazyInitializeSupport();
  Set<LazyInitializeSupport> group=_registerMap.get(groupName);
  if (group == null) {
    group=new CopyOnWriteArraySet<LazyInitializeSupport>();
    Set<LazyInitializeSupport> oldGroup=_registerMap.putIfAbsent(groupName,group);
    if (oldGroup != null) {
      group=oldGroup;
    }
  }
  group.add(lazyInitializeSupport);
  return lazyInitializeSupport;
}
