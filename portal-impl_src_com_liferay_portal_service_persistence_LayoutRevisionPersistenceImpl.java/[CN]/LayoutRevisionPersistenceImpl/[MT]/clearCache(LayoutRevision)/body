{
  EntityCacheUtil.removeResult(LayoutRevisionModelImpl.ENTITY_CACHE_ENABLED,LayoutRevisionImpl.class,layoutRevision.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_L_H_P,new Object[]{Long.valueOf(layoutRevision.getLayoutSetBranchId()),Boolean.valueOf(layoutRevision.getHead()),Long.valueOf(layoutRevision.getPlid())});
}
