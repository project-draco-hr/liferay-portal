{
  EntityCacheUtil.putResult(LayoutRevisionModelImpl.ENTITY_CACHE_ENABLED,LayoutRevisionImpl.class,layoutRevision.getPrimaryKey(),layoutRevision);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_L_H_P,new Object[]{layoutRevision.getLayoutSetBranchId(),layoutRevision.getHead(),layoutRevision.getPlid()},layoutRevision);
  layoutRevision.resetOriginalValues();
}
