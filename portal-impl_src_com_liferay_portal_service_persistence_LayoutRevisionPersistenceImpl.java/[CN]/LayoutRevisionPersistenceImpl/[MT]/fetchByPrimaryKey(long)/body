{
  LayoutRevision layoutRevision=(LayoutRevision)EntityCacheUtil.getResult(LayoutRevisionModelImpl.ENTITY_CACHE_ENABLED,LayoutRevisionImpl.class,layoutRevisionId);
  if (layoutRevision == _nullLayoutRevision) {
    return null;
  }
  if (layoutRevision == null) {
    Session session=null;
    try {
      session=openSession();
      layoutRevision=(LayoutRevision)session.get(LayoutRevisionImpl.class,Long.valueOf(layoutRevisionId));
      if (layoutRevision != null) {
        cacheResult(layoutRevision);
      }
 else {
        EntityCacheUtil.putResult(LayoutRevisionModelImpl.ENTITY_CACHE_ENABLED,LayoutRevisionImpl.class,layoutRevisionId,_nullLayoutRevision);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(LayoutRevisionModelImpl.ENTITY_CACHE_ENABLED,LayoutRevisionImpl.class,layoutRevisionId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return layoutRevision;
}
