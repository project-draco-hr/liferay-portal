{
  LayoutRevision layoutRevision=(LayoutRevision)EntityCacheUtil.getResult(LayoutRevisionModelImpl.ENTITY_CACHE_ENABLED,LayoutRevisionImpl.class,layoutRevisionId,this);
  if (layoutRevision == null) {
    Session session=null;
    try {
      session=openSession();
      layoutRevision=(LayoutRevision)session.get(LayoutRevisionImpl.class,new Long(layoutRevisionId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (layoutRevision != null) {
        cacheResult(layoutRevision);
      }
      closeSession(session);
    }
  }
  return layoutRevision;
}
