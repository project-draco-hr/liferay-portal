{
  layoutRevision=toUnwrappedModel(layoutRevision);
  boolean isNew=layoutRevision.isNew();
  LayoutRevisionModelImpl layoutRevisionModelImpl=(LayoutRevisionModelImpl)layoutRevision;
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,layoutRevision,merge);
    layoutRevision.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.putResult(LayoutRevisionModelImpl.ENTITY_CACHE_ENABLED,LayoutRevisionImpl.class,layoutRevision.getPrimaryKey(),layoutRevision);
  if (!isNew && ((layoutRevision.getBranchId() != layoutRevisionModelImpl.getOriginalBranchId()) || (layoutRevision.getPlid() != layoutRevisionModelImpl.getOriginalPlid()) || (layoutRevision.getHead() != layoutRevisionModelImpl.getOriginalHead()))) {
    FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_B_P_H,new Object[]{new Long(layoutRevisionModelImpl.getOriginalBranchId()),new Long(layoutRevisionModelImpl.getOriginalPlid()),Boolean.valueOf(layoutRevisionModelImpl.getOriginalHead())});
  }
  if (isNew || ((layoutRevision.getBranchId() != layoutRevisionModelImpl.getOriginalBranchId()) || (layoutRevision.getPlid() != layoutRevisionModelImpl.getOriginalPlid()) || (layoutRevision.getHead() != layoutRevisionModelImpl.getOriginalHead()))) {
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_B_P_H,new Object[]{new Long(layoutRevision.getBranchId()),new Long(layoutRevision.getPlid()),Boolean.valueOf(layoutRevision.getHead())},layoutRevision);
  }
  return layoutRevision;
}
