{
  Session session=null;
  try {
    session=openSession();
    LayoutRevision layoutRevision=(LayoutRevision)session.get(LayoutRevisionImpl.class,new Long(revisionId));
    if (layoutRevision == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + revisionId);
      }
      throw new NoSuchLayoutRevisionException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + revisionId);
    }
    return remove(layoutRevision);
  }
 catch (  NoSuchLayoutRevisionException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
