{
  if (_enableViewCountIncrement != null) {
    return _enableViewCountIncrement;
  }
  if (Validator.isNotNull(_journalContentPortletInstanceConfiguration.enableViewCountIncrement())) {
    _enableViewCountIncrement=GetterUtil.getBoolean(_journalContentPortletInstanceConfiguration.enableViewCountIncrement());
  }
 else {
    _enableViewCountIncrement=PropsValues.ASSET_ENTRY_BUFFERED_INCREMENT_ENABLED;
  }
  return _enableViewCountIncrement;
}
