{
  Map<Locale,String> titleMap=new HashMap<Locale,String>();
  Map<Locale,String> descriptionMap=new HashMap<Locale,String>();
  for (  Locale locale : _locales) {
    titleMap.put(locale,title.concat(LocaleUtil.toLanguageId(locale)));
    descriptionMap.put(locale,description.concat(LocaleUtil.toLanguageId(locale)));
  }
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext();
  serviceContext.setScopeGroupId(groupId);
  AssetVocabulary assetVocabulary=AssetVocabularyLocalServiceUtil.addVocabulary(TestPropsValues.getUserId(),"TestVocabulary",titleMap,descriptionMap,null,serviceContext);
  return AssetCategoryLocalServiceUtil.addCategory(TestPropsValues.getUserId(),0,titleMap,descriptionMap,assetVocabulary.getVocabularyId(),new String[0],serviceContext);
}
