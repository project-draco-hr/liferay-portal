{
  Map<Locale,String> titleMap=new HashMap<Locale,String>();
  for (  Locale locale : getLocales()) {
    titleMap.put(locale,name.concat(LocaleUtil.toLanguageId(locale)));
  }
  return JournalArticleLocalServiceUtil.updateArticle(article.getUserId(),article.getGroupId(),article.getFolderId(),article.getArticleId(),article.getVersion(),titleMap,article.getDescriptionMap(),content,article.getLayoutUuid(),ServiceTestUtil.getServiceContext());
}
