{
  this.file=file;
  this.header=header;
  if (file.exists()) {
    FileInputStream fis=null;
    try {
      fis=new FileInputStream(file);
      load(fis);
    }
 catch (    Exception ex) {
      Message.warn("exception occurred while reading properties file " + file + ": "+ ex.getMessage());
    }
    try {
      if (fis != null) {
        fis.close();
      }
    }
 catch (    IOException e) {
    }
  }
}
