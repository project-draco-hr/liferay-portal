{
  SCProductVersion newSCProductVersion=addSCProductVersion();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SCProductVersion.class,SCProductVersion.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("productVersionId",newSCProductVersion.getProductVersionId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  SCProductVersion existingSCProductVersion=(SCProductVersion)result.get(0);
  assertEquals(existingSCProductVersion,newSCProductVersion);
}
