{
  Group group=_groupLocalService.fetchFriendlyURLGroup(companyId,"/forms");
  if (group == null) {
    group=addFormsGroup(companyId);
  }
  Layout layout=_layoutLocalService.fetchLayoutByFriendlyURL(group.getGroupId(),false,"/shared");
  if (layout == null) {
    addSharedLayout(companyId,group.getGroupId());
  }
}
