{
  if (_socialContributionEquity == null) {
    try {
      SocialEquityValue contributionEquity=SocialEquityUserLocalServiceUtil.getContributionEquity(getUserId());
      _socialContributionEquity=new AtomicReference<Double>(contributionEquity.getValue());
    }
 catch (    SystemException se) {
      return 0;
    }
  }
  return _socialContributionEquity.get();
}
