{
  if (_socialContributionEquity == null) {
    try {
      SocialEquityValue socialEquityValue=SocialEquityUserLocalServiceUtil.getContributionEquity(getUserId());
      _socialContributionEquity=new AtomicReference<Double>(socialEquityValue.getValue());
    }
 catch (    SystemException se) {
      return 0;
    }
  }
  return _socialContributionEquity.get();
}
