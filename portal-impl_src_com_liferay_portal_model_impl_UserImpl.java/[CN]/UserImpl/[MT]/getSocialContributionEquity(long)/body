{
  AtomicReference<Double> socialContributionEquity=_socialContributionEquities.get(groupId);
  if (socialContributionEquity == null) {
    try {
      SocialEquityValue socialEquityValue=SocialEquityUserLocalServiceUtil.getContributionEquity(getUserId(),groupId);
      socialContributionEquity=new AtomicReference<Double>(socialEquityValue.getValue());
      _socialContributionEquities.put(groupId,socialContributionEquity);
    }
 catch (    SystemException se) {
      return 0;
    }
  }
  return socialContributionEquity.get();
}
