{
  ThreadLocalCache<List<Group>> threadLocalCache=ThreadLocalCacheManager.getThreadLocalCache(Lifecycle.REQUEST,_GET_MY_SITES_CACHE_NAME);
  String key=StringUtil.toHexString(max);
  if ((classNames != null) && (classNames.length > 0)) {
    key=StringUtil.merge(classNames).concat(StringPool.POUND).concat(key);
  }
  List<Group> myPlaces=threadLocalCache.get(key);
  if (myPlaces != null) {
    return myPlaces;
  }
  myPlaces=GroupServiceUtil.getUserPlaces(getUserId(),classNames,max);
  threadLocalCache.put(key,myPlaces);
  return myPlaces;
}
