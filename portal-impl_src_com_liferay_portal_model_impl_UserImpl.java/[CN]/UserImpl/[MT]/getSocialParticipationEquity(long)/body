{
  AtomicReference<Double> participationEquityByGroup=_participationEquityByGroup.get(groupId);
  if (participationEquityByGroup == null) {
    try {
      SocialEquityValue socialEquityValue=SocialEquityUserLocalServiceUtil.getParticipationEquity(getUserId(),groupId);
      participationEquityByGroup=new AtomicReference<Double>(socialEquityValue.getValue());
      _participationEquityByGroup.put(groupId,participationEquityByGroup);
    }
 catch (    SystemException se) {
      return 0;
    }
  }
  return participationEquityByGroup.get();
}
