{
  if (Validator.isNull(PropsValues.USERS_PROFILE_FRIENDLY_URL)) {
    return null;
  }
  return StringUtil.replace(PropsValues.USERS_PROFILE_FRIENDLY_URL,new String[]{"${liferay:userId}","${liferay:userScreenName}"},new String[]{String.valueOf(getUserId()),HtmlUtil.escapeURL(getScreenName())});
}
