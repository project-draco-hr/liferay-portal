{
  FullNameGenerator fullNameGenerator=FullNameGeneratorFactory.getInstance();
  int prefixId=0;
  int suffixId=0;
  try {
    prefixId=getContact().getPrefixId();
    suffixId=getContact().getSuffixId();
  }
  finally {
    return fullNameGenerator.getLocalizedFullName(getFirstName(),getMiddleName(),getLastName(),getLocale(),prefixId,suffixId);
  }
}
