{
  double currentValue=0;
  double newValue=0;
  AtomicReference<Double> socialParticipationEquity=_socialParticipationEquities.get(groupId);
  if (socialParticipationEquity == null) {
    return;
  }
  do {
    currentValue=socialParticipationEquity.get();
    newValue=currentValue + value;
  }
 while (!socialParticipationEquity.compareAndSet(currentValue,newValue));
}
