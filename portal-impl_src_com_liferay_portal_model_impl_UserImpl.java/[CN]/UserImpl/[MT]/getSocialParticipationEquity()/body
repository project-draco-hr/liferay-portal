{
  if (_socialParticipationEquity == null) {
    try {
      SocialEquityValue participationEquity=SocialEquityUserLocalServiceUtil.getParticipationEquity(getUserId());
      _socialParticipationEquity=new AtomicReference<Double>(participationEquity.getValue());
    }
 catch (    SystemException se) {
      return 0;
    }
  }
  return _socialParticipationEquity.get();
}
