{
  try {
    Group group=getGroup();
    if (group != null) {
      int publicLayoutsPageCount=group.getPublicLayoutsPageCount();
      if (publicLayoutsPageCount > 0) {
        return portalURL + PortalUtil.getPathMain() + "/my_places/view?groupId="+ group.getGroupId()+ "&privateLayout=0";
      }
    }
  }
 catch (  Exception e) {
    _log.error(e);
  }
  return StringPool.BLANK;
}
