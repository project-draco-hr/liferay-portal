{
  try {
    MethodKey methodKey=new MethodKey(DLFolderServiceUtil.class.getName(),"getFolders",_getFoldersParameterTypes10);
    MethodHandler methodHandler=new MethodHandler(methodKey,groupId,parentFolderId,status,includeMountfolders,start,end,obc);
    Object returnObj=null;
    try {
      returnObj=TunnelUtil.invoke(httpPrincipal,methodHandler);
    }
 catch (    Exception e) {
      if (e instanceof com.liferay.portal.kernel.exception.SystemException) {
        throw (com.liferay.portal.kernel.exception.SystemException)e;
      }
      throw new com.liferay.portal.kernel.exception.SystemException(e);
    }
    return (java.util.List<com.liferay.portlet.documentlibrary.model.DLFolder>)returnObj;
  }
 catch (  com.liferay.portal.kernel.exception.SystemException se) {
    _log.error(se,se);
    throw se;
  }
}
