{
  KaleoTaskFormInstance kaleoTaskFormInstance=findByPrimaryKey(kaleoTaskFormInstanceId);
  Session session=null;
  try {
    session=openSession();
    KaleoTaskFormInstance[] array=new KaleoTaskFormInstanceImpl[3];
    array[0]=getByKaleoTaskId_PrevAndNext(session,kaleoTaskFormInstance,kaleoTaskId,orderByComparator,true);
    array[1]=kaleoTaskFormInstance;
    array[2]=getByKaleoTaskId_PrevAndNext(session,kaleoTaskFormInstance,kaleoTaskId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
