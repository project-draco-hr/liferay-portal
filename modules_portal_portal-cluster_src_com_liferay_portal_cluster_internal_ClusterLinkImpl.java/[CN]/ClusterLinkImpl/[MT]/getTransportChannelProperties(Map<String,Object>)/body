{
  Properties transportProperties=new Properties();
  int prefixLength=ClusterPropsKeys.CHANNEL_PROPERTIES_TRANSPORT_PREFIX.length();
  for (  String key : properties.keySet()) {
    if (key.startsWith(ClusterPropsKeys.CHANNEL_PROPERTIES_TRANSPORT_PREFIX)) {
      transportProperties.put(key.substring(prefixLength + 1),properties.get(key));
    }
  }
  if (transportProperties.isEmpty()) {
    transportProperties=_props.getProperties(PropsKeys.CLUSTER_LINK_CHANNEL_PROPERTIES_TRANSPORT,true);
  }
  return transportProperties;
}
