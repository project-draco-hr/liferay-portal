{
  _executorService=_portalExecutorManager.getPortalExecutor(ClusterLinkImpl.class.getName());
  try {
    initChannels(transportChannelProperties,transportChannelNames);
  }
 catch (  Exception e) {
    if (_log.isErrorEnabled()) {
      _log.error("Unable to initialize channels",e);
    }
    throw new IllegalStateException(e);
  }
  for (  ClusterReceiver clusterReceiver : _clusterReceivers) {
    clusterReceiver.openLatch();
  }
}
