{
  clusterLinkConfiguration=Configurable.createConfigurable(ClusterLinkConfiguration.class,properties);
  if (!clusterLinkConfiguration.enabled() && (_transportChannels != null)) {
    deactivate();
  }
 else   if (clusterLinkConfiguration.enabled() && (_transportChannels == null)) {
    Properties transportProperties=getTransportProperties(properties);
    initialize(transportProperties);
  }
}
