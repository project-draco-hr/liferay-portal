{
  if (_counterThreshold == 0) {
    return;
  }
  try {
    int count=SocialActivityAchievementLocalServiceUtil.getUserAchievementCount(activity.getUserId(),activity.getGroupId(),_name);
    if (count > 0) {
      return;
    }
    doProcessActivity(activity);
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to process activity.",e);
    }
  }
}
