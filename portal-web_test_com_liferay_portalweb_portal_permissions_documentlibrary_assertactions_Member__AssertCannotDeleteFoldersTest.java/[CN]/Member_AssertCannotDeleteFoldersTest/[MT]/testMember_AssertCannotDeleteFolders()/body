{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Document Library Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Document Library Permissions Test Page"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isElementPresent("//tr[4]/td[4]/ul/li/strong/span"));
  assertFalse(selenium.isElementPresent("//td[4]/ul/li/strong/span"));
  assertFalse(selenium.isElementPresent("link=Delete"));
  selenium.click(RuntimeVariables.replace("link=Permissions2 Test2 Folder2"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isElementPresent("//strong/span"));
  assertFalse(selenium.isElementPresent("link=Delete"));
  selenium.click(RuntimeVariables.replace("link=Document Library Permissions Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("link=Permissions Edited Test Folder"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isElementPresent("//strong/span"));
  assertFalse(selenium.isElementPresent("link=Delete"));
}
