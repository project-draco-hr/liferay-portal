{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=All Communities")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("link=All Communities");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//div[@id=\"portlet-wrapper-29\"]/div[2]/div/div/form/table/tbody/tr[3]/td[6]/ul/li/ul/li[3]/nobr/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("//div[@id=\"portlet-wrapper-29\"]/div[2]/div/div/form/table/tbody/tr[3]/td[6]/ul/li/ul/li[3]/nobr/a");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Settings");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Merge Pages");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("_29_mergeGuestPublicPagesCheckbox")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("_29_mergeGuestPublicPagesCheckbox");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//input[@value='Save']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("//input[@value='Save']");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Return to Full Page");
  selenium.waitForPageToLoad("30000");
}
