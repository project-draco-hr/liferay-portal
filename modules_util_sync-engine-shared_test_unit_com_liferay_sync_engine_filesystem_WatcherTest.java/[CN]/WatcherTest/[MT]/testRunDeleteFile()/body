{
  setResponse("dependencies/watcher_test_delete_file.json");
  Path filePath=Paths.get(FileUtil.getFilePathName(_syncSite.getFilePathName(),"test.txt"));
  Files.createFile(filePath);
  sleep();
  Files.delete(filePath);
  sleep();
  _syncFiles=SyncFileService.findSyncFiles(syncAccount.getSyncAccountId());
  Assert.assertEquals(2,_syncFiles.size());
  Assert.assertNull(SyncFileService.fetchSyncFile(filePath.toString()));
}
