{
  setResponse("dependencies/watcher_test_add_file.json");
  if (OSDetector.isWindows()) {
    Path hiddenFilePath=Paths.get(FileUtil.getFilePathName(_syncSite.getFilePathName(),"hidden_file.txt"));
    Files.createFile(hiddenFilePath);
    Files.setAttribute(hiddenFilePath,"dos:hidden",true);
    Path shortcutFilePath=Paths.get(FileUtil.getFilePathName(_syncSite.getFilePathName(),"test.txt - Shortcut.lnk"));
    Files.createFile(shortcutFilePath);
  }
 else {
    Path ignoredFilePath=Paths.get(FileUtil.getFilePathName(_syncSite.getFilePathName(),".DS_Store"));
    Files.createFile(ignoredFilePath);
    Path symbolicLinkFilePath=Paths.get(FileUtil.getFilePathName(_syncSite.getFilePathName(),"symbolic_link"));
    Files.createSymbolicLink(symbolicLinkFilePath,ignoredFilePath);
  }
  sleep();
  _syncFiles=SyncFileService.findSyncFiles(syncAccount.getSyncAccountId());
  Assert.assertEquals(2,_syncFiles.size());
}
