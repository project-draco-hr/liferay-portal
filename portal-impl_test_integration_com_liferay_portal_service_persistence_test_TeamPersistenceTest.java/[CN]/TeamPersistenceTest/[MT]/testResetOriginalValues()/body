{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  Team newTeam=addTeam();
  _persistence.clearCache();
  Team existingTeam=_persistence.findByPrimaryKey(newTeam.getPrimaryKey());
  Assert.assertEquals(existingTeam.getGroupId(),ReflectionTestUtil.invoke(existingTeam,"getOriginalGroupId",new Class<?>[0]));
  Assert.assertTrue(Validator.equals(existingTeam.getName(),ReflectionTestUtil.invoke(existingTeam,"getOriginalName",new Class<?>[0])));
}
