{
  String credentialsPassword=_jcrStoreConfiguration.jackrabbitCredentialsPassword();
  Credentials credentials=new SimpleCredentials(_jcrStoreConfiguration.jackrabbitCredentialsUsername(),credentialsPassword.toCharArray());
  Session session=null;
  try {
    session=_transientRepository.login(credentials,workspaceName);
  }
 catch (  RepositoryException re) {
    _log.error("Unable to login to the workspace " + workspaceName);
    throw re;
  }
  return session;
}
