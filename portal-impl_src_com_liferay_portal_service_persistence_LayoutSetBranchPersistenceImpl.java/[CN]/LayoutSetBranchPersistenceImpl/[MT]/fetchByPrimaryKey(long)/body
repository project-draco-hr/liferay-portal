{
  LayoutSetBranch layoutSetBranch=(LayoutSetBranch)EntityCacheUtil.getResult(LayoutSetBranchModelImpl.ENTITY_CACHE_ENABLED,LayoutSetBranchImpl.class,layoutSetBranchId,this);
  if (layoutSetBranch == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (layoutSetBranch == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      layoutSetBranch=(LayoutSetBranch)session.get(LayoutSetBranchImpl.class,Long.valueOf(layoutSetBranchId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (layoutSetBranch == null)) {
        EntityCacheUtil.putResult(LayoutSetBranchModelImpl.ENTITY_CACHE_ENABLED,LayoutSetBranchImpl.class,layoutSetBranchId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(layoutSetBranch);
      }
      closeSession(session);
    }
  }
  return layoutSetBranch;
}
