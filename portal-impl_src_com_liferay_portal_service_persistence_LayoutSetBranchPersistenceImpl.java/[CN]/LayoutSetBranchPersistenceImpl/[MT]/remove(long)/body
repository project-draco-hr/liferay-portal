{
  Session session=null;
  try {
    session=openSession();
    LayoutSetBranch layoutSetBranch=(LayoutSetBranch)session.get(LayoutSetBranchImpl.class,new Long(layoutSetBranchId));
    if (layoutSetBranch == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + layoutSetBranchId);
      }
      throw new NoSuchLayoutSetBranchException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + layoutSetBranchId);
    }
    return layoutSetBranchPersistence.remove(layoutSetBranch);
  }
 catch (  NoSuchLayoutSetBranchException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
