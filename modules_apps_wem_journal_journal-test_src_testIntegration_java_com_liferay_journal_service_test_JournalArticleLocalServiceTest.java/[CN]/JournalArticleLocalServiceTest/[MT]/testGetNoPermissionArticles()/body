{
  JournalArticle article=JournalTestUtil.addArticle(_group.getGroupId(),JournalFolderConstants.DEFAULT_PARENT_FOLDER_ID);
  List<ResourcePermission> resourcePermissions=ResourcePermissionLocalServiceUtil.getResourcePermissions(article.getCompanyId(),JournalArticle.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,String.valueOf(article.getResourcePrimKey()));
  for (  ResourcePermission resourcePermission : resourcePermissions) {
    ResourcePermissionLocalServiceUtil.deleteResourcePermission(resourcePermission.getResourcePermissionId());
  }
  List<JournalArticle> articles=JournalArticleLocalServiceUtil.getNoPermissionArticles();
  Assert.assertEquals(1,articles.size());
  Assert.assertEquals(article,articles.get(0));
}
