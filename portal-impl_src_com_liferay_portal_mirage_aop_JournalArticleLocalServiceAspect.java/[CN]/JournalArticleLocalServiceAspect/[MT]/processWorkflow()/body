{
  if ((_outputVO != null) || _outputVOCheckFailed) {
    return _outputVO;
  }
  try {
    Workflow workflow=getWorkflow();
    SaveTaskVO saveTaskVO=new SaveTaskVO();
    _outputVO=workflow.saveTasks(saveTaskVO);
  }
 catch (  WorkflowException we) {
    _outputVOCheckFailed=true;
  }
  return _outputVO;
}
