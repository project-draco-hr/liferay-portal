{
  FormsStructureEntry formsStructureEntry=findByPrimaryKey(structureEntryId);
  Session session=null;
  try {
    session=openSession();
    FormsStructureEntry[] array=new FormsStructureEntryImpl[3];
    array[0]=getByGroupId_PrevAndNext(session,formsStructureEntry,groupId,orderByComparator,true);
    array[1]=formsStructureEntry;
    array[2]=getByGroupId_PrevAndNext(session,formsStructureEntry,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
