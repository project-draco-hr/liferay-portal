{
  FormsStructureEntry formsStructureEntry=fetchByPrimaryKey(id);
  if (formsStructureEntry == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + id);
    }
    throw new NoSuchStructureEntryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + id);
  }
  return formsStructureEntry;
}
