{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByGroupId_PrevAndNext(id,groupId,orderByComparator);
  }
  FormsStructureEntry formsStructureEntry=findByPrimaryKey(id);
  Session session=null;
  try {
    session=openSession();
    FormsStructureEntry[] array=new FormsStructureEntryImpl[3];
    array[0]=filterGetByGroupId_PrevAndNext(session,formsStructureEntry,groupId,orderByComparator,true);
    array[1]=formsStructureEntry;
    array[2]=filterGetByGroupId_PrevAndNext(session,formsStructureEntry,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
