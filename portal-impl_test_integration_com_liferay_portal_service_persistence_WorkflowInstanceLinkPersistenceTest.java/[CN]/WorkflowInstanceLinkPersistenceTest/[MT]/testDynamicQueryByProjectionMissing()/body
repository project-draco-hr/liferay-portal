{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(WorkflowInstanceLink.class,WorkflowInstanceLink.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("workflowInstanceLinkId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("workflowInstanceLinkId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
