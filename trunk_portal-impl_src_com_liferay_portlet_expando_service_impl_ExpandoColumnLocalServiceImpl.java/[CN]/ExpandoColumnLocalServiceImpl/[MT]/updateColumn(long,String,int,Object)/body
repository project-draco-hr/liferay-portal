{
  ExpandoColumn column=expandoColumnPersistence.findByPrimaryKey(columnId);
  ExpandoValue value=validate(columnId,column.getTableId(),name,type,defaultData);
  column.setName(name);
  column.setType(type);
  column.setDefaultData(value.getData());
  expandoColumnPersistence.update(column,false);
  return column;
}
