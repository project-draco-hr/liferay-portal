{
  if (_coordinator == null) {
    _coordinator=coordinator;
    return;
  }
  Address oldCoordinator=null;
  try {
    _countDownLatch.await();
    oldCoordinator=_coordinator;
    _coordinator=coordinator;
    if (_hasDoCoordinatorUpdated) {
      _executorService.execute(new CoordinatorUpdatedRunnable(oldCoordinator,coordinator));
    }
  }
 catch (  InterruptedException ie) {
    _log.error("Latch opened prematurely by interruption. Dependence may " + "not be ready.");
  }
catch (  RejectedExecutionException ree) {
    _log.error("Unable to handle coordinator update from " + oldCoordinator + " to "+ coordinator,ree);
  }
}
