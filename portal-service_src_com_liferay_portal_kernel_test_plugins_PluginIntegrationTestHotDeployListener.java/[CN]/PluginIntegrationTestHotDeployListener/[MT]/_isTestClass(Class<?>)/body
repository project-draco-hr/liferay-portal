{
  Class<?> declaringClass=ReflectionUtil.getAnnotationDeclaringClass(RunWith.class,clazz);
  if (declaringClass == null) {
    return false;
  }
  RunWith runWith=declaringClass.getAnnotation(RunWith.class);
  Class<? extends Runner> value=runWith.value();
  String className=clazz.getName();
  if (!value.equals(LiferayPluginsIntegrationJUnitRunner.class) || !className.endsWith("Test")) {
    return false;
  }
  return true;
}
