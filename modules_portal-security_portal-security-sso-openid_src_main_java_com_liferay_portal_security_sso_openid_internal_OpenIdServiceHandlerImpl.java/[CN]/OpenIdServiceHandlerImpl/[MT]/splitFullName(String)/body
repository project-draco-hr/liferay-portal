{
  if (Validator.isNull(fullName)) {
    return null;
  }
  int pos=fullName.indexOf(CharPool.SPACE);
  if ((pos != -1) && ((pos + 1) < fullName.length())) {
    String[] names=new String[2];
    names[0]=fullName.substring(0,pos);
    names[1]=fullName.substring(pos + 1);
    return names;
  }
  return null;
}
