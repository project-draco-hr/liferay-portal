{
  long recordSetId=PrefsParamUtil.getLong(renderRequest.getPreferences(),renderRequest,"recordSetId");
  if (recordSetId == 0) {
    return;
  }
  DDLRecordSet recordSet=_ddlRecordSetService.getRecordSet(recordSetId);
  renderRequest.setAttribute(WebKeys.DYNAMIC_DATA_LISTS_RECORD_SET,recordSet);
  String ddmFormHTML=getDDMFormHTML(renderRequest,renderResponse,recordSet.getDDMStructure());
  renderRequest.setAttribute(WebKeys.DYNAMIC_DATA_MAPPING_FORM_HTML,ddmFormHTML);
}
