{
  List<KaleoTaskAssignment> reassignedKaleoTaskAssignments=new ArrayList<>();
  for (  KaleoTaskAssignment kaleoTaskAssignment : kaleoTaskAssignments) {
    String assigneeClassName=kaleoTaskAssignment.getAssigneeClassName();
    TaskAssignmentSelector taskAssignmentSelector=_taskAssignmentSelectorTracker.getTaskAssignmentSelector(assigneeClassName);
    Collection<KaleoTaskAssignment> calculatedKaleoTaskAssignments=taskAssignmentSelector.calculateTaskAssignments(kaleoTaskAssignment,executionContext);
    reassignedKaleoTaskAssignments.addAll(calculatedKaleoTaskAssignments);
  }
  KaleoTaskInstanceToken kaleoTaskInstanceToken=executionContext.getKaleoTaskInstanceToken();
  _kaleoTaskAssignmentInstanceLocalService.deleteKaleoTaskAssignmentInstances(kaleoTaskInstanceToken);
  _kaleoTaskAssignmentInstanceLocalService.addTaskAssignmentInstances(kaleoTaskInstanceToken,reassignedKaleoTaskAssignments,executionContext.getWorkflowContext(),executionContext.getServiceContext());
}
