{
  _gadgetId=gadgetId;
  if (_gadgetRemoteModel != null) {
    try {
      Class<?> clazz=_gadgetRemoteModel.getClass();
      Method method=clazz.getMethod("setGadgetId",long.class);
      method.invoke(_gadgetRemoteModel,gadgetId);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
