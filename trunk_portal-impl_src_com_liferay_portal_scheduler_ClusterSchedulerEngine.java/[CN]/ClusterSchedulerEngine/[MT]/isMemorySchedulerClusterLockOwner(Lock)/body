{
  boolean master=_localClusterNodeAddress.equals(lock.getOwner());
  if (master == _master) {
    return master;
  }
  if (!_master) {
    _master=master;
    return _master;
  }
  _localClusterNodeAddress=getSerializedString(ClusterExecutorUtil.getLocalClusterNodeAddress());
  for (  ObjectValuePair<SchedulerResponse,TriggerState> memoryClusteredJob : _memoryClusteredJobs.values()) {
    SchedulerResponse schedulerResponse=memoryClusteredJob.getKey();
    _schedulerEngine.delete(schedulerResponse.getJobName(),schedulerResponse.getGroupName());
  }
  initMemoryClusteredJobs();
  if (_log.isInfoEnabled()) {
    _log.info("Another node is now the memory scheduler master");
  }
  _master=master;
  return master;
}
