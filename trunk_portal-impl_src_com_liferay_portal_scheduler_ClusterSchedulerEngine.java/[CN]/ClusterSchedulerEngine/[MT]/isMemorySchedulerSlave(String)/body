{
  if (groupName != null) {
    StorageType storageType=getStorageType(groupName);
    if (!storageType.equals(StorageType.MEMORY_CLUSTERED)) {
      return false;
    }
  }
  Lock lock=lockMemorySchedulerCluster(null);
  if (isMemorySchedulerClusterLockOwner(lock)) {
    return false;
  }
  return true;
}
