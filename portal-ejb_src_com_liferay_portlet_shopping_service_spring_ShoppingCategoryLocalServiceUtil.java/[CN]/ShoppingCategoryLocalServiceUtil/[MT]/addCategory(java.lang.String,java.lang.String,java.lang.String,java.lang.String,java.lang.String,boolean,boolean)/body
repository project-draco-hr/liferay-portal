{
  try {
    ShoppingCategoryLocalService shoppingCategoryLocalService=ShoppingCategoryLocalServiceFactory.getService();
    return shoppingCategoryLocalService.addCategory(userId,plid,parentCategoryId,name,description,addCommunityPermissions,addGuestPermissions);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
