{
  setCacheDir(new Callable<File>(){
    @Override public File call() throws Exception {
      return new File(getNodeDir(),".cache");
    }
  }
);
  setScriptFile(new Callable<File>(){
    @Override public File call() throws Exception {
      return new File(getNodeDir(),"lib/node_modules/npm/bin/npm-cli.js");
    }
  }
);
}
