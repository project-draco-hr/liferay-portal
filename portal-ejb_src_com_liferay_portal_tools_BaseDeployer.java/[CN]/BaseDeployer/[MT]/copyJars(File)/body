{
  for (int i=0; i < jars.size(); i++) {
    String jarFullName=(String)jars.get(i);
    String jarName=jarFullName.substring(jarFullName.lastIndexOf("/") + 1,jarFullName.length());
    if ((!appServerType.equals("tomcat")) || (appServerType.equals("tomcat") && !jarFullName.equals("util-java.jar"))) {
      FileUtil.copyFile(jarFullName,srcFile + "/WEB-INF/lib/" + jarName,true);
    }
  }
  FileUtil.delete(srcFile + "/WEB-INF/lib/util-jsf.jar");
}
