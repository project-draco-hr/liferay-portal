{
  long pageResourcePrimKey=ParamUtil.getLong(request,getPrimaryKeyParameterName());
  WikiPageResource pageResource=_wikiPageResourceLocalService.getPageResource(pageResourcePrimKey);
  WikiNode node=_wikiNodeLocalService.getNode(pageResource.getNodeId());
  portletURL.setParameter("nodeName",node.getName());
  portletURL.setParameter("title",pageResource.getTitle());
  return portletURL;
}
