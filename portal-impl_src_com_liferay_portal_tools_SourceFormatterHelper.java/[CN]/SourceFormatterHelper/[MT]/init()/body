{
  start=System.currentTimeMillis();
  if (useCache == false) {
    return;
  }
  String baseName=new File("./portal-iml").getAbsolutePath();
  baseName=StringUtil.replace(baseName,new String[]{".",":","/","\\"},new String[]{"_","_","_","_"});
  timestampsFile=new File(getLiferayTempFolder(),baseName + ".properties");
  timestampsFile.createNewFile();
  timestamps=PropertiesUtil.createFromFile(timestampsFile);
}
