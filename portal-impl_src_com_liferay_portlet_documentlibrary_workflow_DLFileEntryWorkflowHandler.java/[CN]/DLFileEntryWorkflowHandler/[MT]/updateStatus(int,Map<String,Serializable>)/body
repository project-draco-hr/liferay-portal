{
  long userId=GetterUtil.getLong((String)workflowContext.get(WorkflowConstants.CONTEXT_USER_ID));
  long dlFileVersionId=GetterUtil.getLong((String)workflowContext.get(WorkflowConstants.CONTEXT_ENTRY_CLASS_PK));
  ServiceContext serviceContext=(ServiceContext)workflowContext.get("serviceContext");
  return DLFileEntryLocalServiceUtil.updateStatus(userId,dlFileVersionId,status,serviceContext);
}
