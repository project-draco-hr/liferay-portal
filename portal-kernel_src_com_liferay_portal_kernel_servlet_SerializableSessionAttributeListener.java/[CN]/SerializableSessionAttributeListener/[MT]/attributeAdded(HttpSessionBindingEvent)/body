{
  if (!(event.getValue() instanceof Serializable)) {
    System.out.println("WARN [com.liferay.portal.kernel.servlet.SerializableSessionAttributeListener] " + "An object was added to session " + "but it doesn't implement java.io.Serializable.\n"+ "If your application must be distributable, "+ "every object stored in session must implement Serializable.\n"+ "Object's attribute name: " + event.getName() + "\n"+ "Object's class name: "+ event.getValue().getClass());
  }
}
