{
  String key=groupId + StringPool.UNDERLINE + entryId;
  Long newEntryId=_entryIdsMap.get(key);
  if (newEntryId != null) {
    return newEntryId.longValue();
  }
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select * from TagsEntry where entryId = ?");
    ps.setLong(1,entryId);
    rs=ps.executeQuery();
    while (rs.next()) {
      long companyId=rs.getLong("companyId");
      long userId=rs.getLong("userId");
      String userName=rs.getString("userName");
      Timestamp createDate=rs.getTimestamp("createDate");
      Timestamp modifiedDate=rs.getTimestamp("modifiedDate");
      long parentEntryId=rs.getLong("parentEntryId");
      String name=rs.getString("name");
      long vocabularyId=rs.getLong("vocabularyId");
      newEntryId=increment();
      addEntry(newEntryId,groupId,companyId,userId,userName,createDate,modifiedDate,parentEntryId,name,vocabularyId);
      copyProperties(entryId,newEntryId);
      _entryIdsMap.put(key,newEntryId);
      return newEntryId;
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  throw new NoSuchTagException("No AssetTag exists with the primary key " + entryId);
}
