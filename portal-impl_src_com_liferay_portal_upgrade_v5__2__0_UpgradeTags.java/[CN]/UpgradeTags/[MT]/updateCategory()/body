{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select TE.entryId, TE.groupId, TE.userId, TP.propertyId, " + "TP.value from TagsEntry TE, TagsProperty TP where " + "TE.entryId = TP.entryId and TE.vocabularyId <= 0 "+ "and TP.key_ = 'category'");
    rs=ps.executeQuery();
    rs=new ResultSetWrapper(rs);
    while (rs.next()) {
      long entryId=rs.getLong("TE.entryId");
      long groupId=rs.getLong("TE.groupId");
      long userId=rs.getLong("TE.userId");
      long propertyId=rs.getLong("TP.propertyId");
      String value=rs.getString("TP.value");
      long vocabularyId=getVocabularyId(userId,groupId,value);
      ps=con.prepareStatement("update TagsEntry set vocabularyId = ? where entryId = ?");
      ps.setLong(1,vocabularyId);
      ps.setLong(2,entryId);
      ps.executeUpdate();
      ps.close();
      ps=con.prepareStatement("delete from TagsProperty where propertyId = ?");
      ps.setLong(1,propertyId);
      ps.executeUpdate();
      ps.close();
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
