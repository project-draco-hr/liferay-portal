{
  List<String> changedLines=new ArrayList<String>();
  if (margin == 0 || beginPos == 0) {
    return changedLines;
  }
  int i=beginPos - margin;
  for (; i < 0; i++) {
    changedLines.add(CONTEXT_LINE);
  }
  for (; i < beginPos; i++) {
    if (i < stringList.size()) {
      changedLines.add(stringList.get(i));
    }
  }
  return changedLines;
}
