{
  Session session=null;
  try {
    session=openSession();
    if (shoppingCart.isNew()) {
      session.save(shoppingCart);
    }
    session.flush();
    shoppingCart.setNew(false);
    return shoppingCart;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
