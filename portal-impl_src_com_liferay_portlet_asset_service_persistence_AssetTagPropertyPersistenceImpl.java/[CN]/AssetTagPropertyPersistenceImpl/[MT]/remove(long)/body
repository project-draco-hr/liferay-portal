{
  Session session=null;
  try {
    session=openSession();
    AssetTagProperty assetTagProperty=(AssetTagProperty)session.get(AssetTagPropertyImpl.class,Long.valueOf(tagPropertyId));
    if (assetTagProperty == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + tagPropertyId);
      }
      throw new NoSuchTagPropertyException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + tagPropertyId);
    }
    return assetTagPropertyPersistence.remove(assetTagProperty);
  }
 catch (  NoSuchTagPropertyException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
