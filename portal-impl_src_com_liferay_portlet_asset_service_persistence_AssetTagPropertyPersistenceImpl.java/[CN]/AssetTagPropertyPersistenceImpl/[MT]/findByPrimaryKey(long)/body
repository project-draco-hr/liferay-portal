{
  AssetTagProperty assetTagProperty=fetchByPrimaryKey(tagPropertyId);
  if (assetTagProperty == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No AssetTagProperty exists with the primary key " + tagPropertyId);
    }
    throw new NoSuchTagPropertyException("No AssetTagProperty exists with the primary key " + tagPropertyId);
  }
  return assetTagProperty;
}
