{
  AssetTagProperty assetTagProperty=(AssetTagProperty)EntityCacheUtil.getResult(AssetTagPropertyModelImpl.ENTITY_CACHE_ENABLED,AssetTagPropertyImpl.class,tagPropertyId,this);
  if (assetTagProperty == _nullAssetTagProperty) {
    return null;
  }
  if (assetTagProperty == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      assetTagProperty=(AssetTagProperty)session.get(AssetTagPropertyImpl.class,Long.valueOf(tagPropertyId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (!hasException && (assetTagProperty == null)) {
        EntityCacheUtil.putResult(AssetTagPropertyModelImpl.ENTITY_CACHE_ENABLED,AssetTagPropertyImpl.class,tagPropertyId,_nullAssetTagProperty);
      }
 else {
        cacheResult(assetTagProperty);
      }
      closeSession(session);
    }
  }
  return assetTagProperty;
}
