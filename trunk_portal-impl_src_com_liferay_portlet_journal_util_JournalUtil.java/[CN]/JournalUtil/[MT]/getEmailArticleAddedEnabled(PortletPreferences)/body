{
  String emailArticleAddedEnabled=preferences.getValue("emailArticleAddedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleAddedEnabled)) {
    return GetterUtil.getBoolean(emailArticleAddedEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_ADDED_ENABLED));
  }
}
