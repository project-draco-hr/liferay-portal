{
  String emailArticleReviewEnabled=preferences.getValue("emailArticleReviewEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleReviewEnabled)) {
    return GetterUtil.getBoolean(emailArticleReviewEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_REVIEW_ENABLED));
  }
}
