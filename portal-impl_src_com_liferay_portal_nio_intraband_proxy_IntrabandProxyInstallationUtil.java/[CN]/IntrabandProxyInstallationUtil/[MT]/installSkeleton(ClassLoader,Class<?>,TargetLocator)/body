{
  try {
    Class<?> proxySkeletonClass=IntrabandProxyUtil.getSkeletonClass(classLoader,clazz);
    Constructor<? extends IntrabandProxySkeleton> constructor=(Constructor<? extends IntrabandProxySkeleton>)proxySkeletonClass.getConstructor(TargetLocator.class);
    IntrabandProxySkeletonRegistryUtil.register(clazz.getName(),constructor.newInstance(targetLocator));
    return IntrabandProxyUtil.getProxyMethodSignatures(proxySkeletonClass);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
