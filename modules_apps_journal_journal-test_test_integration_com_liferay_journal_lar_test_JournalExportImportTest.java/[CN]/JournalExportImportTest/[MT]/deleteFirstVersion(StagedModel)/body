{
  JournalArticle article=(JournalArticle)stagedModel;
  List<JournalArticle> articles=JournalArticleLocalServiceUtil.getArticles(article.getGroupId(),article.getArticleId());
  JournalArticle firstArticle=null;
  for (  JournalArticle journalArticle : articles) {
    if ((firstArticle == null) || (journalArticle.getVersion() < firstArticle.getVersion())) {
      firstArticle=journalArticle;
    }
  }
  deleteStagedModel(firstArticle);
}
