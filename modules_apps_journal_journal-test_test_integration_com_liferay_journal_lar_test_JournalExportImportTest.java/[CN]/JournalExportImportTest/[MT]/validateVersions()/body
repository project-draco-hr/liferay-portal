{
  List<JournalArticle> articles=JournalArticleLocalServiceUtil.getArticles(group.getGroupId());
  for (  JournalArticle article : articles) {
    JournalArticle importedArticle=(JournalArticle)getStagedModel(article.getUuid(),importedGroup.getGroupId());
    Assert.assertNotNull(importedArticle);
    validateImportedStagedModel(article,importedArticle);
  }
}
