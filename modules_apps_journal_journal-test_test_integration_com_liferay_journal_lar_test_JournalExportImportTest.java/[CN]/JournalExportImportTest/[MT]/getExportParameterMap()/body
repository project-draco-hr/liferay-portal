{
  Map<String,String[]> parameterMap=super.getExportParameterMap();
  MapUtil.merge(parameterMap,getBaseParameterMap(group.getGroupId(),layout.getPlid()));
  parameterMap.put(PortletDataHandlerKeys.PORTLET_DATA + StringPool.UNDERLINE + JournalPortletKeys.JOURNAL,new String[]{Boolean.TRUE.toString()});
  return parameterMap;
}
