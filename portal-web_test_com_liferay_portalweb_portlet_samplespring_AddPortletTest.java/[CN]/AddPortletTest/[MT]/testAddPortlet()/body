{
  selenium.click("link=Add Application");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//div[@id=\"Sample-Spring-DescriptionUpload\"]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("//div[@id=\"Sample-Spring-DescriptionUpload\"]/p/a");
  for (int second=0; ; second++) {
