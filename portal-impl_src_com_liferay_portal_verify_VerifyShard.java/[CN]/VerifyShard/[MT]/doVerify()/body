{
  List<Company> companies=CompanyLocalServiceUtil.getCompanies();
  for (  Company company : companies) {
    try {
      ShardLocalServiceUtil.getShard(Company.class.getName(),company.getCompanyId());
    }
 catch (    NoSuchShardException nsse) {
      ShardLocalServiceUtil.addShard(Company.class.getName(),company.getCompanyId(),PropsValues.SHARD_DEFAULT_NAME);
    }
  }
}
