{
  if (!_started) {
    _started=true;
  }
  for (  Ehcache ehcache : _deferredEhcaches) {
    if (_log.isDebugEnabled()) {
      _log.debug("Loading deferred cache " + ehcache.getName());
    }
    try {
      EhcacheStreamBootstrapHelpUtil.acquireCachePeers(ehcache);
    }
 catch (    Exception e) {
      throw new CacheException(e);
    }
  }
}
