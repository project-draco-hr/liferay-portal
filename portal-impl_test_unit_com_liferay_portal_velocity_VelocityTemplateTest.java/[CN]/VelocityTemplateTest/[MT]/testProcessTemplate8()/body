{
  VelocityContext velocityContext=new VelocityContext();
  velocityContext.put(_TEST_KEY,_TEST_VALUE);
  Template template=new VelocityTemplate(_TEMPLATE_FILE_NAME,null,null,null,velocityContext,_velocityEngine,_templateContextHelper);
  UnsyncStringWriter unsyncStringWriter=new UnsyncStringWriter();
  template.processTemplate(unsyncStringWriter);
  String result=unsyncStringWriter.toString();
  assertEquals(_TEST_VALUE,result);
}
