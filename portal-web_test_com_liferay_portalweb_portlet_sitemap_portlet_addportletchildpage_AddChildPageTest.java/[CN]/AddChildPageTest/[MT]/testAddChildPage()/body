selenium.waitForPageToLoad("30000");
selenium.saveScreenShotAndSource();
assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
selenium.open("/web/guest/home/");
for (int second=0; ; second++) {
if (second >= 60) {
fail("timeout");
}
try {
if (selenium.isVisible("link=Site Map Test Page")) {
break;
}
}
 catch (Exception e) {
}
Thread.sleep(1000);
}
selenium.saveScreenShotAndSource();
selenium.clickAt("link=Site Map Test Page",RuntimeVariables.replace(""));
selenium.waitForPageToLoad("30000");
selenium.saveScreenShotAndSource();
