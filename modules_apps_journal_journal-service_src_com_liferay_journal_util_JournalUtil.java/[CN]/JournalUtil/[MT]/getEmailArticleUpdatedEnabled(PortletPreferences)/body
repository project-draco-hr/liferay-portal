{
  String emailArticleUpdatedEnabled=preferences.getValue("emailArticleUpdatedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleUpdatedEnabled)) {
    return GetterUtil.getBoolean(emailArticleUpdatedEnabled);
  }
 else {
    return JournalServiceConfigurationValues.EMAIL_ARTICLE_UPDATED_ENABLED;
  }
}
