{
  String emailArticleApprovalDeniedEnabled=preferences.getValue("emailArticleApprovalDeniedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleApprovalDeniedEnabled)) {
    return GetterUtil.getBoolean(emailArticleApprovalDeniedEnabled);
  }
 else {
    return JournalServiceConfigurationValues.EMAIL_ARTICLE_APPROVAL_DENIED_ENABLED;
  }
}
