{
  String emailArticleApprovalRequestedEnabled=preferences.getValue("emailArticleApprovalRequestedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleApprovalRequestedEnabled)) {
    return GetterUtil.getBoolean(emailArticleApprovalRequestedEnabled);
  }
 else {
    return JournalServiceConfigurationValues.EMAIL_ARTICLE_APPROVAL_REQUESTED_ENABLED;
  }
}
