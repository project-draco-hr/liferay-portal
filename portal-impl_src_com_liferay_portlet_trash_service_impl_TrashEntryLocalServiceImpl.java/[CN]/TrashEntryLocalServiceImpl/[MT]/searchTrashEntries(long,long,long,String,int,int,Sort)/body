{
  try {
    Indexer indexer=IndexerRegistryUtil.nullSafeGetIndexer(TrashEntry.class);
    SearchContext searchContext=buildSearchContext(companyId,groupId,userId,keywords,start,end,sort);
    Hits hits=indexer.search(searchContext,Trash.SELECTED_FIELD_NAMES);
    List<TrashEntry> trashEntries=TrashUtil.getEntries(hits);
    return new BaseModelSearchResult<TrashEntry>(trashEntries,hits.getLength());
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
