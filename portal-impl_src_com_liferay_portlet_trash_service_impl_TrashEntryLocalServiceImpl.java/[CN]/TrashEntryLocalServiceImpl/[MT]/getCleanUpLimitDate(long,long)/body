{
  Group group=GroupLocalServiceUtil.getGroup(groupId);
  int trashEntriesMaxAge=PrefsPropsUtil.getInteger(companyId,PropsKeys.TRASH_ENTRIES_MAX_AGE,PropsValues.TRASH_ENTRIES_MAX_AGE);
  UnicodeProperties groupTypeSettings=group.getTypeSettingsProperties();
  trashEntriesMaxAge=GetterUtil.getInteger(groupTypeSettings.getProperty("trashEntriesMaxAge"),trashEntriesMaxAge);
  Calendar cal=Calendar.getInstance();
  cal.setTime(new Date());
  cal.add(Calendar.DATE,-trashEntriesMaxAge);
  return cal.getTime();
}
