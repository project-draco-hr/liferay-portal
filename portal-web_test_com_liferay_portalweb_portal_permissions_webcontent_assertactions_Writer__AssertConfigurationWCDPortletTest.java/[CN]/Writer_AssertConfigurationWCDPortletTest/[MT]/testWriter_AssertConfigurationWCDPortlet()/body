{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Web Content Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Print CA Permissions Web Content Test"),selenium.getText("//span/a/span"));
  assertTrue(selenium.isElementPresent("link=Be the first."));
  assertTrue(selenium.isTextPresent("Your Rating"));
}
