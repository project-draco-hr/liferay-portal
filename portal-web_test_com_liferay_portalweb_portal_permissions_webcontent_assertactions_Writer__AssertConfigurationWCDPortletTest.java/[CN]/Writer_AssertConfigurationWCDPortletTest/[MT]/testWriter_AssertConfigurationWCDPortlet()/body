{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Display Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Web Content Display Permissions Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("//span/a/img"));
  assertTrue(selenium.isElementPresent("//img[@alt='Print']"));
  assertTrue(selenium.isElementPresent("link=Add Comment"));
  assertTrue(selenium.isTextPresent("Your Rating"));
}
