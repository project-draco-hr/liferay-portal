{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,reportsEntry,merge);
    reportsEntry.setNew(false);
    return reportsEntry;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ReportsEntry.class.getName());
  }
}
