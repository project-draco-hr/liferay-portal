{
  Analyzer analyzer=_analyzerMap.get(fieldName);
  if (analyzer == null) {
    for (    String key : _analyzerMap.keySet()) {
      if (Pattern.matches(key,fieldName)) {
        analyzer=_analyzerMap.get(key);
        break;
      }
    }
  }
  if (analyzer != null) {
    return analyzer;
  }
  return _defaultAnalyzer;
}
