{
  try {
    DBUpgrader.upgrade();
  }
 catch (  Throwable t) {
    _log.error(t,t);
  }
  MessageBusUtil messageBusUtil=new MessageBusUtil();
  messageBusUtil.setSynchronousMessageSenderMode(SynchronousMessageSender.Mode.DEFAULT);
  try {
    SchedulerEngineHelperUtil.start();
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  try {
    DBUpgrader.verify();
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
