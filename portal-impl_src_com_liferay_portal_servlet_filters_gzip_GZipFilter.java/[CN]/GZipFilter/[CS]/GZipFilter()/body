{
  if (super.isFilterEnabled()) {
    if (ServerDetector.isJBoss() || ServerDetector.isJetty() || ServerDetector.isJOnAS()|| ServerDetector.isOC4J()|| ServerDetector.isTomcat()|| ServerDetector.isWebLogic()|| ServerDetector.isWebSphere()) {
      _filterEnabled=true;
    }
 else {
      _filterEnabled=false;
    }
  }
}
