{
  if (PropsValues.SOCIAL_ACTIVITY_SETS_ENABLED) {
    socialActivitySetLocalService.decrementActivityCount(assetEntry.getClassNameId(),assetEntry.getClassPK());
  }
  socialActivityPersistence.removeByC_C(assetEntry.getClassNameId(),assetEntry.getClassPK());
  socialActivityCounterLocalService.deleteActivityCounters(assetEntry);
}
