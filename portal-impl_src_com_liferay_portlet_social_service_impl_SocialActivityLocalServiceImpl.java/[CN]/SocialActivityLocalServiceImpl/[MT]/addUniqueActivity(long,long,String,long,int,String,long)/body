{
  long classNameId=PortalUtil.getClassNameId(className);
  List<SocialActivity> socialActivities=socialActivityPersistence.findByG_U_C_C_T_R(groupId,userId,classNameId,classPK,type,receiverUserId);
  if (!socialActivities.isEmpty()) {
    return;
  }
  addActivity(userId,groupId,new Date(),className,classPK,type,extraData,receiverUserId);
}
