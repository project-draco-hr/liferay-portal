{
  String pollerRequestString=ParamUtil.getString(request,"pollerRequest");
  if (Validator.isNull(pollerRequestString)) {
    return null;
  }
  return StringUtil.replace(pollerRequestString,new String[]{StringPool.OPEN_CURLY_BRACE,StringPool.CLOSE_CURLY_BRACE,_ESCAPED_OPEN_CURLY_BRACE,_ESCAPED_CLOSE_CURLY_BRACE},new String[]{_OPEN_HASH_MAP_WRAPPER,StringPool.DOUBLE_CLOSE_CURLY_BRACE,StringPool.OPEN_CURLY_BRACE,StringPool.CLOSE_CURLY_BRACE});
}
