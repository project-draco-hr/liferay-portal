{
  if (!BrowserSnifferUtil.isIe(request)) {
    processFilter(JSONContentTypeFilter.class,request,response,filterChain);
    return;
  }
  processFilter(JSONContentTypeFilter.class,request,new HttpServletResponseWrapper(response){
    @Override public void setContentType(    String contentType){
      if (contentType.toLowerCase().equals(ContentTypes.APPLICATION_JSON)) {
        contentType=ContentTypes.TEXT_JAVASCRIPT;
      }
      super.setContentType(contentType);
    }
  }
,filterChain);
}
