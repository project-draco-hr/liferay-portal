{
  Organization organization=OrganizationTestUtil.addOrganization();
  User orgAdminOperator=UserTestUtil.addOrganizationAdminUser(organization);
  User orgAdminUpdated=UserTestUtil.addOrganizationAdminUser(organization);
  unsetOrganizationUsers(organization.getOrganizationId(),orgAdminOperator,orgAdminUpdated);
  Assert.assertTrue(UserLocalServiceUtil.hasOrganizationUser(organization.getOrganizationId(),orgAdminUpdated.getUserId()));
}
