{
  Organization organization=OrganizationTestUtil.addOrganization();
  User orgAdminOperator=UserTestUtil.addOrganizationAdminUser(organization);
  User orgOwnerUpdated=UserTestUtil.addOrganizationOwnerUser(organization);
  unsetOrganizationUsers(organization.getOrganizationId(),orgAdminOperator,orgOwnerUpdated);
  Assert.assertTrue(UserLocalServiceUtil.hasOrganizationUser(organization.getOrganizationId(),orgOwnerUpdated.getUserId()));
}
