{
  Organization organization=OrganizationTestUtil.addOrganization();
  User subjectUser=UserTestUtil.addOrganizationOwnerUser(organization);
  User objectUser=UserTestUtil.addOrganizationOwnerUser(organization);
  unsetOrganizationUsers(organization.getOrganizationId(),subjectUser,objectUser);
  Assert.assertFalse(UserLocalServiceUtil.hasOrganizationUser(organization.getOrganizationId(),objectUser.getUserId()));
}
