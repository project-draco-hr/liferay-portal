{
  User groupAdminUser=UserTestUtil.addGroupAdminUser(_group);
  try {
    _unsetGroupUsers(_group.getGroupId(),_organizationAdminUser,groupAdminUser);
    Assert.assertTrue(UserLocalServiceUtil.hasGroupUser(_group.getGroupId(),groupAdminUser.getUserId()));
  }
  finally {
    UserLocalServiceUtil.deleteUser(groupAdminUser);
  }
}
