{
  User otherOrganizationAdminUser=UserTestUtil.addOrganizationAdminUser(_organization);
  try {
    _unsetOrganizationUsers(_organization.getOrganizationId(),_organizationAdminUser,otherOrganizationAdminUser);
    Assert.assertTrue(UserLocalServiceUtil.hasOrganizationUser(_organization.getOrganizationId(),otherOrganizationAdminUser.getUserId()));
  }
  finally {
    UserLocalServiceUtil.deleteUser(otherOrganizationAdminUser);
  }
}
