{
  User groupOwnerUser=UserTestUtil.addGroupOwnerUser(_group);
  try {
    _unsetGroupUsers(_group.getGroupId(),_groupAdminUser,groupOwnerUser);
    Assert.assertTrue(UserLocalServiceUtil.hasGroupUser(_group.getGroupId(),groupOwnerUser.getUserId()));
  }
  finally {
    UserLocalServiceUtil.deleteUser(groupOwnerUser);
  }
}
