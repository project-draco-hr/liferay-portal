{
  User groupOwnerUser=UserTestUtil.addGroupOwnerUser(_group);
  try {
    _unsetGroupUsers(_group.getGroupId(),_groupOwnerUser,groupOwnerUser);
    Assert.assertFalse(UserLocalServiceUtil.hasGroupUser(_group.getGroupId(),groupOwnerUser.getUserId()));
  }
  finally {
    UserLocalServiceUtil.deleteUser(groupOwnerUser);
  }
}
