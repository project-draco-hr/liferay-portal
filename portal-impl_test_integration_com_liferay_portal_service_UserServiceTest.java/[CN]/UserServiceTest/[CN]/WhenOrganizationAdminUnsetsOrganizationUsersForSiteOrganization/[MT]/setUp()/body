{
  _organization=OrganizationTestUtil.addOrganization(true);
  _group=_organization.getGroup();
  _groupAdminUser=UserTestUtil.addGroupAdminUser(_group);
  _groupOwnerUser=UserTestUtil.addGroupOwnerUser(_group);
  _organizationAdminUser=UserTestUtil.addOrganizationAdminUser(_organization);
  _organizationOwnerUser=UserTestUtil.addOrganizationOwnerUser(_organization);
}
