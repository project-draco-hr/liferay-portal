{
  Object companySecurityStrangersWithMx=PropsValues.COMPANY_SECURITY_STRANGERS_WITH_MX;
  String name=PrincipalThreadLocal.getName();
  try {
    PropsUtil.set(PropsKeys.COMPANY_SECURITY_STRANGERS_WITH_MX,Boolean.TRUE.toString());
    PrincipalThreadLocal.setName(0);
    UserTestUtil.addUser(true);
  }
  finally {
    PropsUtil.set(PropsKeys.COMPANY_SECURITY_STRANGERS_WITH_MX,companySecurityStrangersWithMx.toString());
    PrincipalThreadLocal.setName(name);
  }
}
