{
  portletRequest.setAttribute(ApplicationListWebKeys.PANEL_APP_REGISTRY,_panelAppRegistry);
  PanelCategoryHelper panelCategoryHelper=new PanelCategoryHelper(_panelAppRegistry,_panelCategoryRegistry);
  portletRequest.setAttribute(ApplicationListWebKeys.PANEL_CATEGORY_HELPER,panelCategoryHelper);
  portletRequest.setAttribute(ApplicationListWebKeys.PANEL_CATEGORY_REGISTRY,_panelCategoryRegistry);
}
