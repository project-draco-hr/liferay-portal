{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),CalendarService.class.getName());
    if (invokableService instanceof CalendarService) {
      _service=(CalendarService)invokableService;
    }
 else {
      _service=new CalendarServiceClp(invokableService);
    }
    ReferenceRegistry.registerReference(CalendarServiceUtil.class,"_service");
  }
  return _service;
}
