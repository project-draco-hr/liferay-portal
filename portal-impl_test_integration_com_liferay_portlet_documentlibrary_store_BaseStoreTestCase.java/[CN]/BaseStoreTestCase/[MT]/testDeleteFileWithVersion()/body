{
  String fileName=addFile(1);
  store.deleteFile(companyId,repositoryId,fileName,Store.VERSION_DEFAULT);
  Assert.assertFalse(store.hasFile(companyId,repositoryId,fileName,Store.VERSION_DEFAULT));
  store.deleteFile(companyId,repositoryId,fileName,"1.1");
  Assert.assertFalse(store.hasFile(companyId,repositoryId,fileName,"1.1"));
}
