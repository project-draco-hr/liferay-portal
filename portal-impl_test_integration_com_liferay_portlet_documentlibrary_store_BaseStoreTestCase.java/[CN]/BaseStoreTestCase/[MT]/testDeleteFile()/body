{
  String fileName=addFile(1);
  Assert.assertTrue(store.hasFile(companyId,repositoryId,fileName,Store.VERSION_DEFAULT));
  Assert.assertTrue(store.hasFile(companyId,repositoryId,fileName,"1.1"));
  store.deleteFile(companyId,repositoryId,fileName);
  Assert.assertFalse(store.hasFile(companyId,repositoryId,fileName,Store.VERSION_DEFAULT));
  Assert.assertFalse(store.hasFile(companyId,repositoryId,fileName,"1.1"));
}
