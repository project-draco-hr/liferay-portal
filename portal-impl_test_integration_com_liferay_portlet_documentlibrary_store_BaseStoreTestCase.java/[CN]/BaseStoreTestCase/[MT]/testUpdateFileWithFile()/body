{
  String fileName=RandomTestUtil.randomString();
  store.addFile(companyId,repositoryId,fileName,_DATA_VERSION_1);
  File file=createFile(_DATA_VERSION_2);
  store.updateFile(companyId,repositoryId,fileName,"1.1",file);
  byte[] bytesFirstVersion=store.getFileAsBytes(companyId,repositoryId,fileName,"1.0");
  Assert.assertTrue(Arrays.equals(_DATA_VERSION_1,bytesFirstVersion));
  byte[] bytesSecondVersion=store.getFileAsBytes(companyId,repositoryId,fileName,"1.1");
  Assert.assertTrue(Arrays.equals(_DATA_VERSION_2,bytesSecondVersion));
  byte[] bytesCurrentVersion=store.getFileAsBytes(companyId,repositoryId,fileName);
  Assert.assertTrue(Arrays.equals(_DATA_VERSION_2,bytesCurrentVersion));
}
