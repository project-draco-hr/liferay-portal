{
  String fileName=RandomTestUtil.randomString();
  store.addFile(companyId,repositoryId,fileName,_DATA_VERSION_1);
  addFile(fileName,1);
  try (InputStream inputStream=store.getFileAsStream(companyId,repositoryId,fileName)){
    for (int i=0; i < _DATA_SIZE; i++) {
      Assert.assertEquals(_DATA_VERSION_1[i],(byte)inputStream.read());
    }
    Assert.assertEquals(-1,inputStream.read());
  }
 }
