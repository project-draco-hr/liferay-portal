{
  ServiceTestUtil.setUser(TestPropsValues.getUser());
  Registry registry=RegistryUtil.getRegistry();
  Collection<ServiceReference<StoreWrapper>> serviceReferences=registry.getServiceReferences(StoreWrapper.class,"(store.type=" + _STORE_TYPE + ")");
  for (  ServiceReference<StoreWrapper> serviceReference : serviceReferences) {
    Long bundleId=(Long)serviceReference.getProperty("service.bundleid");
    _bundleIds.add(bundleId);
    ModuleFrameworkUtilAdapter.stopBundle(bundleId);
  }
}
