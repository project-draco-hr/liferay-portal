{
  for (  BundleContext bundleContext : _maps.keySet()) {
    if (bundleContext == null) {
      continue;
    }
    Map<String,String> map=_maps.get(bundleContext);
    if (map == null) {
      continue;
    }
    String content=map.get(id);
    if (content != null) {
      return content;
    }
  }
  return null;
}
