{
  String resourceName=Layout.class.getName();
  importGroupRoles(layoutCache,companyId,groupId,resourceName,"community",rolesElement);
  importUserRoles(layoutCache,companyId,groupId,resourceName,rolesElement);
  importInheritedRoles(layoutCache,companyId,groupId,resourceName,"organization",rolesElement);
  importInheritedRoles(layoutCache,companyId,groupId,resourceName,"user-group",rolesElement);
}
