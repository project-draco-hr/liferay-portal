{
  sb.append(" AND (");
  for (int i=0; i < categoryIds.length; i++) {
    sb.append("AssetEntry.entryId NOT IN (");
    sb.append(CustomSQLUtil.get(sqlId));
    sb.append(StringPool.CLOSE_PARENTHESIS);
    if ((i + 1) < categoryIds.length) {
      sb.append(" OR ");
    }
  }
  sb.append(StringPool.CLOSE_PARENTHESIS);
}
