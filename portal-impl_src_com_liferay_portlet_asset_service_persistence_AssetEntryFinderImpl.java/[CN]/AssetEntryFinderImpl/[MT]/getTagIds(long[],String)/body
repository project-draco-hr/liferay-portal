{
  if (tagIds.length == 0) {
    return "(1 = 1)";
  }
  StringBundler sb=new StringBundler(tagIds.length * 4 - 1);
  for (int i=0; i < tagIds.length; i++) {
    sb.append("AssetTag.tagId ");
    sb.append(operator);
    sb.append(" ? ");
    if ((i + 1) != tagIds.length) {
      sb.append("OR ");
    }
  }
  return sb.toString();
}
