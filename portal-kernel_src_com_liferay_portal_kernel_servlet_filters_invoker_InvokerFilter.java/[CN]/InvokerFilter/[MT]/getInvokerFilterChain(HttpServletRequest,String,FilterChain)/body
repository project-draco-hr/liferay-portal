{
  if (_filterChains == null) {
    return _invokerFilterHelper.createInvokerFilterChain(request,_dispatcher,uri,filterChain);
  }
  CacheKeyGenerator cacheKeyGenerator=CacheKeyGeneratorUtil.getCacheKeyGenerator(InvokerFilter.class.getName());
  String key=String.valueOf(cacheKeyGenerator.getCacheKey(uri));
  InvokerFilterChain invokerFilterChain=_filterChains.get(key);
  if (invokerFilterChain == null) {
    invokerFilterChain=_invokerFilterHelper.createInvokerFilterChain(request,_dispatcher,uri,filterChain);
    _filterChains.put(key,invokerFilterChain);
  }
  return invokerFilterChain.clone(filterChain);
}
