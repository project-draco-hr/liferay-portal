{
  Mockito.doThrow(IOException.class).when(_http).URLtoString("__PROBLEM_URL__");
  long messageId=RandomTestUtil.randomLong();
  LinkbackConsumerUtil.addNewTrackback(messageId,"__PROBLEM_URL__","__entryUrl__");
  LinkbackConsumerUtil.verifyNewTrackbacks(_comments);
  Mockito.verify(_comments).deleteComment(messageId);
  Mockito.verify(_http).URLtoString("__PROBLEM_URL__");
}
