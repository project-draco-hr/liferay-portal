{
  Mockito.when(_http.URLtoString("__url__")).thenReturn("__URLtoString_containing_**entryUrl**__");
  LinkbackConsumerUtil.addNewTrackback(RandomTestUtil.randomLong(),"__url__","**entryUrl**");
  LinkbackConsumerUtil.verifyNewTrackbacks(_comments);
  Mockito.verifyZeroInteractions(_comments);
  Mockito.verify(_http).URLtoString("__url__");
}
