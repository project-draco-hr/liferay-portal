{
  Mockito.when(_http.URLtoString("__url__")).thenReturn("__URLtoString_containing_**entryUrl**__");
  long messageId=RandomUtil.randomLong();
  LinkbackConsumerUtil.addNewTrackback(messageId,"__url__","**entryUrl**");
  LinkbackConsumerUtil.verifyNewTrackbacks();
  Mockito.verifyZeroInteractions(_mbMessageLocalService);
  Mockito.verify(_http).URLtoString("__url__");
}
