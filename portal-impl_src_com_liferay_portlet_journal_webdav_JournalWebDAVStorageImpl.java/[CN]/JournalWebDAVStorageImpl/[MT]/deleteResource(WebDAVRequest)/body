{
  try {
    Resource resource=getResource(webDavRequest);
    if (resource == null) {
      return HttpServletResponse.SC_NOT_FOUND;
    }
    return HttpServletResponse.SC_FORBIDDEN;
  }
 catch (  PortalException pe) {
    return HttpServletResponse.SC_FORBIDDEN;
  }
catch (  Exception e) {
    throw new WebDAVException(e);
  }
}
