{
  boolean sessionRedirectOnExpire=PropsValues.SESSION_TIMEOUT_REDIRECT_ON_EXPIRE;
  if (_instance._ssoMap.isEmpty()) {
    return sessionRedirectOnExpire;
  }
  if (PrefsPropsUtil.getBoolean(companyId,PropsKeys.OPEN_SSO_AUTH_ENABLED,PropsValues.OPEN_SSO_AUTH_ENABLED) && PropsValues.OPEN_SSO_LOGOUT_ON_SESSION_EXPIRATION) {
    return true;
  }
  return _instance._isSessionRedirectOnExpire(companyId);
}
