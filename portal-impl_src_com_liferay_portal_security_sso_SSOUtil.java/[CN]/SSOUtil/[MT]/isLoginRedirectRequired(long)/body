{
  if (PrefsPropsUtil.getBoolean(companyId,PropsKeys.LOGIN_DIALOG_DISABLED,PropsValues.LOGIN_DIALOG_DISABLED) || PrefsPropsUtil.getBoolean(companyId,PropsKeys.OPEN_SSO_AUTH_ENABLED,PropsValues.OPEN_SSO_AUTH_ENABLED)) {
    return true;
  }
  if (_instance._ssoMap.isEmpty()) {
    return false;
  }
  return _instance._isLoginRedirectRequired(companyId);
}
