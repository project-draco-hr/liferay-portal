{
  if (PrefsPropsUtil.getBoolean(companyId,PropsKeys.OPEN_SSO_AUTH_ENABLED,PropsValues.OPEN_SSO_AUTH_ENABLED) && PropsValues.OPEN_SSO_LOGOUT_ON_SESSION_EXPIRATION) {
    return PrefsPropsUtil.getString(companyId,PropsKeys.OPEN_SSO_LOGOUT_URL,PropsValues.OPEN_SSO_LOGOUT_URL);
  }
  if (_instance._ssoMap.isEmpty()) {
    return sessionExpirationRedirectURL;
  }
  return _instance._getSessionExpirationRedirectUrl(companyId);
}
