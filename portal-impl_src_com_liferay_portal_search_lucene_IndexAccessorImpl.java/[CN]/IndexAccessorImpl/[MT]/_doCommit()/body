{
  if (_indexWriter == null) {
    if (_log.isDebugEnabled()) {
      _log.debug("Index in read only mode");
    }
    return;
  }
  _commitLock.lock();
  try {
    _indexWriter.commit();
  }
  finally {
    _commitLock.unlock();
    _indexSearcherManager.invalidate();
    _invalidate(_companyId);
  }
  _batchCount=0;
}
