{
  if (!PropsValues.LUCENE_STORE_TYPE.equals(_LUCENE_STORE_TYPE_JDBC) || !PropsValues.LUCENE_STORE_JDBC_AUTO_CLEAN_UP_ENABLED) {
    return;
  }
  ScheduledExecutorService scheduledExecutorService=Executors.newSingleThreadScheduledExecutor();
  Runnable runnable=new Runnable(){
    public void run(){
      _doCleanUpJdbc();
    }
  }
;
  scheduledExecutorService.scheduleWithFixedDelay(runnable,0,PropsValues.LUCENE_STORE_JDBC_AUTO_CLEAN_UP_INTERVAL,TimeUnit.MINUTES);
}
