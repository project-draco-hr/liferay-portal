{
  int initialVersionPagesCount=TrashVersionLocalServiceUtil.getTrashVersionsCount();
  WikiPage[] pages=WikiTestUtil.addTrashedParentPageWithRedirectPage(group.getGroupId(),_node.getNodeId(),false,false);
  WikiPage parentPage=pages[0];
  restoreTrashEntry(parentPage);
  Assert.assertEquals(initialVersionPagesCount,TrashVersionLocalServiceUtil.getTrashVersionsCount());
}
