{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      selenium.selectWindow("null");
    selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace("Shopping Test Page"));
selenium.waitForPageToLoad("30000");
selenium.clickAt("link=Orders",RuntimeVariables.replace("Orders"));
selenium.waitForPageToLoad("30000");
boolean ordersNotPresent=selenium.isElementPresent("//span[@title='Actions']/ul/li/strong/a/span");
if (!ordersNotPresent) {
label=2;
continue;
}
assertFalse(selenium.isChecked("//input[@name='_34_allRowIds']"));
selenium.clickAt("//input[@name='_34_allRowIds']",RuntimeVariables.replace("All Rows Checkbox"));
assertTrue(selenium.isChecked("//input[@name='_34_allRowIds']"));
selenium.waitForElementPresent("//tr[contains(@class,'last selected')]");
selenium.clickAt("//input[@value='Delete']",RuntimeVariables.replace("Delete"));
selenium.waitForPageToLoad("30000");
assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete the selected orders[\\s\\S]$"));
assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
case 2:
assertEquals(RuntimeVariables.replace("No orders were found."),selenium.getText("//div[@class='portlet-msg-info']"));
case 100:
label=-1;
}
}
}
