{
  if (getLastGeneration(companyId) == IndexAccessor.DEFAULT_LOCAL_LAST_GENERATION) {
    if (_log.isDebugEnabled()) {
      _log.debug("Load index from cluster for company: " + companyId + " is not enabled");
    }
    return;
  }
  try {
    LuceneClusterUtil.loadIndexesFromCluster(companyId);
  }
 catch (  Exception e) {
    _log.error("Unable to load index for company: " + companyId,e);
  }
}
