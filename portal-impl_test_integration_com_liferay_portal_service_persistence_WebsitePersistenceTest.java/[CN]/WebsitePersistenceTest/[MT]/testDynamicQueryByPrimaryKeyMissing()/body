{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Website.class,Website.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("websiteId",ServiceTestUtil.nextLong()));
  List<Website> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
