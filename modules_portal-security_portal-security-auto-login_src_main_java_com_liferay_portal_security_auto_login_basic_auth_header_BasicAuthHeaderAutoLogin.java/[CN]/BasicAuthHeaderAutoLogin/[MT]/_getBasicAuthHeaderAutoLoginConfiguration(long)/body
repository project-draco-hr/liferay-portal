{
  try {
    BasicAuthHeaderAutoLoginConfiguration basicAuthHeaderAutoLoginConfiguration=_configurationProvider.getConfiguration(BasicAuthHeaderAutoLoginConfiguration.class,new CompanyServiceSettingsLocator(companyId,BasicAuthHeaderAutoLoginConstants.SERVICE_NAME));
    return basicAuthHeaderAutoLoginConfiguration;
  }
 catch (  ConfigurationException ce) {
    _log.error("Unable to get basic auth header configuration",ce);
  }
  return null;
}
