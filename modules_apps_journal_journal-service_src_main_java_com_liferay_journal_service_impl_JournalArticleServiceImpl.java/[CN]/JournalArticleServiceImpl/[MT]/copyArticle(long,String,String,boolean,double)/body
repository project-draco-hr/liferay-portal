{
  JournalArticle article=journalArticleLocalService.getArticle(groupId,oldArticleId);
  JournalFolderPermission.check(getPermissionChecker(),groupId,article.getFolderId(),ActionKeys.ADD_ARTICLE);
  return journalArticleLocalService.copyArticle(getUserId(),groupId,oldArticleId,newArticleId,autoArticleId,version);
}
