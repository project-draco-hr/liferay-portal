{
  DLFileEntryTypeData dlFileEntryTypeData=getDLFileEntryTypeData(groupId,dlFileEntryTypeKey);
  if (dlFileEntryTypeData == null) {
    return;
  }
  Map<Locale,String> descriptionMap=dlFileEntryTypeData.getDescriptionMap();
  Map<Locale,String> nameMap=dlFileEntryTypeData.getNameMap();
  boolean needsUpdate=false;
  Locale defaultLocale=LocaleUtil.fromLanguageId(UpgradeProcessUtil.getDefaultLanguageId(companyId));
  String defaultValue=LanguageUtil.get(defaultLocale,nameLanguageKey);
  for (  Locale locale : LanguageUtil.getSupportedLocales()) {
    String description=descriptionMap.get(locale);
    String name=nameMap.get(locale);
    String localizedValue=LanguageUtil.get(locale,nameLanguageKey);
    if (!locale.equals(defaultLocale) && localizedValue.equals(defaultValue)) {
      continue;
    }
    if (description == null) {
      descriptionMap.put(locale,localizedValue);
      needsUpdate=true;
    }
    if (name == null) {
      nameMap.put(locale,localizedValue);
      needsUpdate=true;
    }
  }
  if (needsUpdate) {
    upgradeDLFileEntryType(dlFileEntryTypeData,nameMap,descriptionMap,defaultLocale);
  }
}
