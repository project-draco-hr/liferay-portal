{
  _initialize();
  if (captcha == null) {
    if (_log.isInfoEnabled()) {
      _log.info("Restoring " + ClassUtil.getClassName(_originalCaptcha));
    }
    _captcha=_originalCaptcha;
  }
 else {
    if (_log.isInfoEnabled()) {
      _log.info("Setting " + ClassUtil.getClassName(captcha));
    }
    _captcha=captcha;
  }
}
