{
  _initialize();
  if (captcha == null) {
    if (_log.isInfoEnabled()) {
      _log.info("Restoring " + _originalCaptcha.getClass().getName());
    }
    _captcha=_originalCaptcha;
  }
 else {
    if (_log.isInfoEnabled()) {
      _log.info("Setting " + captcha.getClass().getName());
    }
    _captcha=captcha;
  }
}
