{
  DDMFormFieldEvaluationResult ddmFormFieldEvaluationResult=new DDMFormFieldEvaluationResult("field",StringUtil.randomString());
  ddmFormFieldEvaluationResult.setReadOnly(false);
  boolean propertyValue=evaluatePropertyGetFunction(ddmFormFieldEvaluationResult,"readOnly");
  Assert.assertEquals(false,propertyValue);
  ddmFormFieldEvaluationResult.setReadOnly(true);
  propertyValue=evaluatePropertyGetFunction(ddmFormFieldEvaluationResult,"readOnly");
  Assert.assertTrue(propertyValue);
}
