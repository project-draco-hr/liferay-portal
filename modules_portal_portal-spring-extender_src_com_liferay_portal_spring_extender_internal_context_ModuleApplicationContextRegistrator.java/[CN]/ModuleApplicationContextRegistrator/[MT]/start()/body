{
  ConfigurableApplicationContext configurableApplicationContext=_createApplicationContext(_extenderBundle,_extendeeBundle);
  _registerBeanLocator(_extendeeBundle,configurableApplicationContext);
  _refreshBeanFactory(configurableApplicationContext);
  _serviceConfigurator.initServices(new ModuleResourceLoader(_extendeeBundle),_extendeeClassLoader);
  _configurableApplicationContext=configurableApplicationContext;
  _applicationContextServicePublisher=new ApplicationContextServicePublisher(_configurableApplicationContext,_extendeeBundle.getBundleContext());
  _applicationContextServicePublisher.register();
}
