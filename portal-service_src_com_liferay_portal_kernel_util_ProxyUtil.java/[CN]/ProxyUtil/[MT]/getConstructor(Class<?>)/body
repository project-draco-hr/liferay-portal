{
  Constructor<?> constructor=null;
  WeakReference<Constructor<?>> constructorWeakReference=_constructors.get(clazz);
  if (constructorWeakReference == null || ((constructor=constructorWeakReference.get()) == null)) {
    try {
      constructor=clazz.getConstructor(_argumentsClazz);
      constructor.setAccessible(true);
      WeakReference<Constructor<?>> weakReference=new WeakReference<>(constructor);
      _constructors.putIfAbsent(clazz,weakReference);
      return constructor;
    }
 catch (    Exception e) {
      throw new InternalError(e.toString());
    }
  }
 else {
    return constructor;
  }
}
