{
  Session session=null;
  try {
    session=openSession();
    StringBundler query=null;
    if (orderByComparator != null) {
      query=new StringBundler(4 + (orderByComparator.getOrderByFields().length * 3));
    }
 else {
      query=new StringBundler(4);
    }
    query.append(_FILTER_SELECT_CALEVENT_WHERE);
    query.append(_FINDER_COLUMN_G_T_GROUPID_2);
    if (type == null) {
      query.append(_FINDER_COLUMN_G_T_TYPE_1);
    }
 else {
      if (type.equals(StringPool.BLANK)) {
        query.append(_FINDER_COLUMN_G_T_TYPE_3);
      }
 else {
        query.append(_FINDER_COLUMN_G_T_TYPE_2);
      }
    }
    if (orderByComparator != null) {
      appendOrderByComparator(query,_ORDER_BY_ENTITY_ALIAS,orderByComparator);
    }
 else {
      query.append(CalEventModelImpl.ORDER_BY_JPQL);
    }
    String sql=InlineSQLHelperUtil.replacePermissionCheck(query.toString(),CalEvent.class.getName(),_FILTER_COLUMN_EVENTID,_FILTER_COLUMN_USERID,groupId);
    SQLQuery q=session.createSQLQuery(sql);
    q.addEntity(_ENTITY_ALIAS,CalEventImpl.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(groupId);
    if (type != null) {
      qPos.add(type);
    }
    return (List<CalEvent>)QueryUtil.list(q,getDialect(),start,end);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
