{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(calEvent);
    }
 else {
      if (calEvent.isNew()) {
        session.save(calEvent);
      }
    }
    session.flush();
    calEvent.setNew(false);
    return calEvent;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
