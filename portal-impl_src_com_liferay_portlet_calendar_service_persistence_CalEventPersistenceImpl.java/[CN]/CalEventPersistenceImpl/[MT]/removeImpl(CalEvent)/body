{
  Session session=null;
  try {
    session=openSession();
    session.delete(calEvent);
    session.flush();
    return calEvent;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(CalEvent.class.getName());
  }
}
