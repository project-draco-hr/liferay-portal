{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(calEvent);
    }
  }
  calEvent=removeImpl(calEvent);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(calEvent);
    }
  }
  return calEvent;
}
