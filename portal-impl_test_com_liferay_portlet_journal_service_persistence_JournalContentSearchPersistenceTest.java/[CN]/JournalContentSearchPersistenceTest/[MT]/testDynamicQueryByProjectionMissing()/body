{
  JournalContentSearch newJournalContentSearch=addJournalContentSearch();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalContentSearch.class,JournalContentSearch.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("contentSearchId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("contentSearchId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
