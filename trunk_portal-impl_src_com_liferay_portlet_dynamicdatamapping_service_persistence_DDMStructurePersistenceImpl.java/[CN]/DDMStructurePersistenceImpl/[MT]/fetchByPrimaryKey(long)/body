{
  DDMStructure ddmStructure=(DDMStructure)EntityCacheUtil.getResult(DDMStructureModelImpl.ENTITY_CACHE_ENABLED,DDMStructureImpl.class,structureId,this);
  if (ddmStructure == null) {
    Session session=null;
    try {
      session=openSession();
      ddmStructure=(DDMStructure)session.get(DDMStructureImpl.class,Long.valueOf(structureId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (ddmStructure != null) {
        cacheResult(ddmStructure);
      }
      closeSession(session);
    }
  }
  return ddmStructure;
}
