{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  int entriesCount=BookmarksEntryServiceUtil.getEntriesCount(_folder.getGroupId(),_folder.getFolderId());
  int foldersCount=BookmarksFolderServiceUtil.getFoldersCount(_folder.getGroupId(),_folder.getFolderId());
  if ((entriesCount > 0) || (foldersCount > 0)) {
    return themeDisplay.getPathThemeImages() + "/file_system/large/folder_full_bookmark.png";
  }
  return themeDisplay.getPathThemeImages() + "/file_system/large/folder_empty_bookmark.png";
}
