{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select fileEntryId, folderId, groupId, name from DLFileEntry");
    rs=ps.executeQuery();
    while (rs.next()) {
      long fileEntryId=rs.getLong("fileEntryId");
      long folderId=rs.getLong("folderId");
      long groupId=rs.getLong("groupId");
      String name=rs.getString("name");
      long fileVersionId=getLatestDLFileVersionId(folderId,groupId,name);
      runSQL("update ExpandoRow set classPK = " + fileVersionId + " where classPK = "+ fileEntryId);
      runSQL("update ExpandoValue set classPK = " + fileVersionId + " where classPK = "+ fileEntryId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
