{
  String className=_getClassName(fileName);
  JavaDocBuilder builder=new JavaDocBuilder();
  if (reader == null) {
    File file=new File(fileName);
    if (!file.exists()) {
      return null;
    }
    builder.addSource(file);
  }
 else {
    builder.addSource(reader);
  }
  return builder.getClassByName(className);
}
