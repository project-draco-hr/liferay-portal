{
  if (_framework != null) {
    return _framework;
  }
  try {
    Adaptor adaptor=AdaptorUtil.getAdaptor();
    if ((adaptor != null) && (adaptor instanceof OSGiAdaptor)) {
      OSGiAdaptor osgiAdaptor=(OSGiAdaptor)adaptor;
      _framework=osgiAdaptor.getFramework();
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return _framework;
}
