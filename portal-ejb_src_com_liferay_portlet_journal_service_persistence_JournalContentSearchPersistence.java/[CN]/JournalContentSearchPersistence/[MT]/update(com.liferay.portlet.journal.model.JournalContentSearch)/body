{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(journalContentSearch);
    session.flush();
    journalContentSearch.setNew(false);
    return journalContentSearch;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
