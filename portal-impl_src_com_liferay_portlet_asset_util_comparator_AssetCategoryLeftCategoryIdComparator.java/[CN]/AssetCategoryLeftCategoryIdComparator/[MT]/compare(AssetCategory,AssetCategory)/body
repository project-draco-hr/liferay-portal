{
  long leftCategoryId1=assetCategory1.getLeftCategoryId();
  long leftCategoryId2=assetCategory2.getLeftCategoryId();
  int value=0;
  if (leftCategoryId1 < leftCategoryId2) {
    value=-1;
  }
 else   if (leftCategoryId1 > leftCategoryId2) {
    value=1;
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
