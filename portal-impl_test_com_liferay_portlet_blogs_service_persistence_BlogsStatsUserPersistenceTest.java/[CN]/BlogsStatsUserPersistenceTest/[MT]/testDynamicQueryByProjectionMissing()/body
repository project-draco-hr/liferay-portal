{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(BlogsStatsUser.class,BlogsStatsUser.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("statsUserId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("statsUserId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
