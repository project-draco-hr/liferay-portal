{
  if (_videoBitRateMap != null) {
    return;
  }
  _videoBitRateMap=new HashMap<String,Integer>();
  for (  String previewVideoContainer : PropsValues.DL_FILE_ENTRY_PREVIEW_VIDEO_CONTAINERS) {
    Filter filter=new Filter(previewVideoContainer);
    int videoBitRate=GetterUtil.getInteger(PropsUtil.get(PropsKeys.DL_FILE_ENTRY_PREVIEW_VIDEO_BIT_RATE,filter));
    if (videoBitRate > _VIDEO_BIT_RATE_MAX) {
      videoBitRate=_VIDEO_BIT_RATE_MAX;
    }
    if (videoBitRate > 0) {
      _videoBitRateMap.put(previewVideoContainer,videoBitRate);
      if (_log.isInfoEnabled()) {
        _log.info("Bit rate for " + previewVideoContainer + " set to "+ videoBitRate);
      }
    }
  }
}
