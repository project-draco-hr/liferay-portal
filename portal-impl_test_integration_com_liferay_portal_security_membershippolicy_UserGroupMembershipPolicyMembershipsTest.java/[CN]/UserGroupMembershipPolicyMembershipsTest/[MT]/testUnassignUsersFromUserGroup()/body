{
  long[] standardUserGroupIds=addStandardUserGroups();
  User user=MembershipPolicyTestUtil.addUser(null,null,null,standardUserGroupIds);
  int initialUserUserGroupCount=UserLocalServiceUtil.getUserGroupUsersCount(standardUserGroupIds[0]);
  UserServiceUtil.unsetUserGroupUsers(standardUserGroupIds[0],new long[]{user.getUserId()});
  Assert.assertEquals(initialUserUserGroupCount - 1,UserLocalServiceUtil.getUserGroupUsersCount(standardUserGroupIds[0]));
  Assert.assertTrue(isPropagateMembership());
}
