{
  Session session=null;
  try {
    session=openSession();
    StringBuffer query=new StringBuffer();
    query.append("FROM Organization_ IN CLASS com.liferay.portal.service.persistence.OrganizationHBM WHERE ");
    if (companyId == null) {
      query.append("companyId is null");
    }
 else {
      query.append("companyId = ?");
    }
    query.append(" AND ");
    if (name == null) {
      query.append("name is null");
    }
 else {
      query.append("name = ?");
    }
    query.append(" ");
    query.append("ORDER BY ");
    query.append("name ASC");
    Query q=session.createQuery(query.toString());
    int queryPos=0;
    if (companyId != null) {
      q.setString(queryPos++,companyId);
    }
    if (name != null) {
      q.setString(queryPos++,name);
    }
    Iterator itr=q.list().iterator();
    if (!itr.hasNext()) {
      String msg="No Organization exists with the key ";
      msg+=StringPool.OPEN_CURLY_BRACE;
      msg+="companyId=";
      msg+=companyId;
      msg+=", ";
      msg+="name=";
      msg+=name;
      msg+=StringPool.CLOSE_CURLY_BRACE;
      throw new NoSuchOrganizationException(msg);
    }
    OrganizationHBM organizationHBM=(OrganizationHBM)itr.next();
    return OrganizationHBMUtil.model(organizationHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
