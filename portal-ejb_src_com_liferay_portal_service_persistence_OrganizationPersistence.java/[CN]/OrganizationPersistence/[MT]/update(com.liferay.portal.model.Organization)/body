{
  Session session=null;
  try {
    session=openSession();
    if (organization.isNew()) {
      session.save(organization);
    }
    session.flush();
    organization.setNew(false);
    return organization;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
