{
  AnnouncementsEntryPermission.check(getPermissionChecker(),entryId,ActionKeys.UPDATE);
  return announcementsEntryLocalService.updateEntry(entryId,title,content,url,type,displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute,expirationDateMonth,expirationDateDay,expirationDateYear,expirationDateHour,expirationDateMinute,priority);
}
