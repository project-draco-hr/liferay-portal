{
  CalendarResource calendarResource=CalendarResourceUtil.getGroupCalendarResource(_group.getGroupId(),new ServiceContext());
  UpgradeCalendar upgradeCalendar=new UpgradeCalendar();
  upgradeCalendar.doUpgrade();
  List<Calendar> calendars=CalendarLocalServiceUtil.getCalendarResourceCalendars(_group.getGroupId(),calendarResource.getCalendarResourceId());
  Calendar calendar=calendars.get(0);
  Assert.assertEquals(PropsUtil.get(PropsKeys.COMPANY_DEFAULT_TIME_ZONE),calendar.getTimeZoneId());
}
