{
  return CalendarFactoryUtil.getCalendar(dateJCalendar.get(Calendar.YEAR),dateJCalendar.get(Calendar.MONTH),dateJCalendar.get(Calendar.DAY_OF_MONTH),timeJCalendar.get(Calendar.HOUR_OF_DAY),timeJCalendar.get(Calendar.MINUTE),timeJCalendar.get(Calendar.SECOND),timeJCalendar.get(Calendar.MILLISECOND),timeZone);
}
