{
  return CalendarFactoryUtil.getCalendar(dateCalendar.get(Calendar.YEAR),dateCalendar.get(Calendar.MONTH),dateCalendar.get(Calendar.DAY_OF_MONTH),timeCalendar.get(Calendar.HOUR_OF_DAY),timeCalendar.get(Calendar.MINUTE),timeCalendar.get(Calendar.SECOND),timeCalendar.get(Calendar.MILLISECOND),timeZone);
}
