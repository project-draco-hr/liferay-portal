{
  ChannelHub channelHub=_channelHubs.get(companyId);
  if (channelHub == null) {
synchronized (_channelHubs) {
      channelHub=_channelHubs.get(companyId);
      if (channelHub == null) {
        if (createIfAbsent) {
          channelHub=createChannelHub(companyId);
        }
      }
    }
  }
  return channelHub;
}
