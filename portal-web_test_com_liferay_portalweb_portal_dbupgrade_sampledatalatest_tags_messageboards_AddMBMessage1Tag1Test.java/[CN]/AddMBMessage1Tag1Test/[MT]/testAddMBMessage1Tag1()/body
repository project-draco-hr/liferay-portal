    Thread.sleep(1000);
  }
  selenium.type("//td[@id='cke_contents__19_editor']/textarea",RuntimeVariables.replace("This is a message1 tag1 test1."));
  assertEquals(RuntimeVariables.replace("Source"),selenium.getText("//span[.='Source']"));
