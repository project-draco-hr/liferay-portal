{
  List<Long> layoutIds=new ArrayList<Long>();
  List<JournalContentSearch> contentSearches=journalContentSearchPersistence.findByG_P_A(groupId,privateLayout,articleId);
  for (  JournalContentSearch contentSearch : contentSearches) {
    layoutIds.add(new Long(contentSearch.getLayoutId()));
  }
  return layoutIds;
}
