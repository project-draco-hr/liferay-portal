{
  journalContentSearchPersistence.removeByG_P_L_P(groupId,privateLayout,layoutId,portletId);
  List<JournalContentSearch> contentSearches=new ArrayList<>();
  for (  String articleId : articleIds) {
    JournalContentSearch contentSearch=updateContentSearch(groupId,privateLayout,layoutId,portletId,articleId,false);
    contentSearches.add(contentSearch);
  }
  return contentSearches;
}
