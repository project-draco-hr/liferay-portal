{
  Assert.assertTrue(isValidFilter("(object=value)"));
  Assert.assertTrue(isValidFilter("  (object=value)"));
  Assert.assertTrue(isValidFilter("(object=value)  "));
  Assert.assertFalse(isValidFilter("object=value)"));
  Assert.assertFalse(isValidFilter("(object=value"));
  Assert.assertFalse(isValidFilter("object=value"));
  Assert.assertFalse(isValidFilter("("));
  Assert.assertFalse(isValidFilter(")"));
  Assert.assertFalse(isValidFilter(")("));
}
