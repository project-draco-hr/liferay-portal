{
  assertEquals(true,LDAPUtil.validateFilter("(object=value)"));
  assertEquals(false,LDAPUtil.validateFilter("(=value)"));
  assertEquals(false,LDAPUtil.validateFilter("(<=value)"));
  assertEquals(false,LDAPUtil.validateFilter("(>=value)"));
  assertEquals(false,LDAPUtil.validateFilter("(~=value)"));
  assertEquals(false,LDAPUtil.validateFilter("(~=value)(object=value)"));
}
