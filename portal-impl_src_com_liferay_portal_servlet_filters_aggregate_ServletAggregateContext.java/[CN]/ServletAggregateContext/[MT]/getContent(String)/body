{
  try {
    URL resourceURL=_servletContext.getResource(generatePathFromStack().concat(path));
    if (resourceURL == null) {
      return null;
    }
    URLConnection urlConnection=resourceURL.openConnection();
    return StringUtil.read(urlConnection.getInputStream());
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
  return null;
}
