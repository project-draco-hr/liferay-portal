{
  boolean bypass=false;
  while (true) {
    if (!file.exists()) {
      JenkinsResultsParserUtil.write(file,"");
      bypass=true;
    }
    long age=System.currentTimeMillis() - file.lastModified();
    String content=JenkinsResultsParserUtil.read(file);
    if (!bypass && (age < MIN_RUN_INTERVAL || (content.length() > 0 && age < MAX_AGE))) {
      long sleepPeriod=MIN_RUN_INTERVAL - age + (getRandomValue(0,maxHostNames) * 10);
      System.out.println("Waiting " + sleepPeriod + " milliseconds.");
      Thread.sleep(sleepPeriod);
      continue;
    }
    return;
  }
}
