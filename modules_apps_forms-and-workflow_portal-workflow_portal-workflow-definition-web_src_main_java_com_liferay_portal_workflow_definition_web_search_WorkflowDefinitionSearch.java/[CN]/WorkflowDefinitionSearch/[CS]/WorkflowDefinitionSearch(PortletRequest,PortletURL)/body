{
  super(portletRequest,new WorkflowDefinitionDisplayTerms(portletRequest),new WorkflowDefinitionSearchTerms(portletRequest),DEFAULT_CUR_PARAM,DEFAULT_DELTA,iteratorURL,headerNames,EMPTY_RESULTS_MESSAGE);
  WorkflowDefinitionDisplayTerms displayTerms=(WorkflowDefinitionDisplayTerms)getDisplayTerms();
  iteratorURL.setParameter(WorkflowDefinitionDisplayTerms.NAME,displayTerms.getName());
  iteratorURL.setParameter(WorkflowDefinitionDisplayTerms.TITLE,displayTerms.getTitle());
}
