{
  PortletSession portletSession=portletRequest.getPortletSession();
  Stack<DDMStructure> recentDDMStructures=(Stack<DDMStructure>)portletSession.getAttribute(WebKeys.JOURNAL_RECENT_DYNAMIC_DATA_MAPPING_STRUCTURES);
  if (recentDDMStructures == null) {
    recentDDMStructures=new FiniteUniqueStack<>(MAX_STACK_SIZE);
    portletSession.setAttribute(WebKeys.JOURNAL_RECENT_DYNAMIC_DATA_MAPPING_STRUCTURES,recentDDMStructures);
  }
  return recentDDMStructures;
}
