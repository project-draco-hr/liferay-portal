{
  if (!PropsValues.ORGANIZATIONS_ASSIGNMENT_STRICT) {
    return false;
  }
  User user=(User)obj;
  try {
    PermissionChecker permissionChecker=PermissionThreadLocal.getPermissionChecker();
    if (isChecked(user)) {
      if (OrganizationMembershipPolicyUtil.isMembershipProtected(permissionChecker,user.getUserId(),_organization.getOrganizationId()) || OrganizationMembershipPolicyUtil.isMembershipRequired(user.getUserId(),_organization.getOrganizationId())) {
        return true;
      }
    }
 else {
      if (!OrganizationMembershipPolicyUtil.isMembershipAllowed(user.getUserId(),_organization.getOrganizationId())) {
        return true;
      }
    }
    return !UserPermissionUtil.contains(permissionChecker,user.getUserId(),ActionKeys.UPDATE);
  }
 catch (  Exception e) {
    _log.error(e,e);
    return super.isDisabled(obj);
  }
}
