{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(IGImage.class,IGImage.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("imageId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("imageId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
