{
  if (Validator.isNotNull(_cssClass) && Validator.isNotNull(_portletDecoratorThumbnailPath)) {
    int pos=_cssClass.indexOf(CharPool.SPACE);
    if (pos > 0) {
      if (_portletDecoratorThumbnailPath.endsWith(_cssClass.substring(0,pos))) {
        String subclassPath=StringUtil.replace(_cssClass,CharPool.SPACE,CharPool.SLASH);
        return _portletDecoratorThumbnailPath + subclassPath.substring(pos);
      }
    }
  }
  return _portletDecoratorThumbnailPath;
}
