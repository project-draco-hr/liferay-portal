{
  WikiPage wikiPage=WikiPageServiceUtil.getPage(nodeId,title);
  DLFileEntry dlFileEntry=PortletFileRepositoryUtil.getPortletFileEntry(wikiPage.getGroupId(),wikiPage.getAttachmentsFolderId(),fileName);
  ServletResponseUtil.sendFile(request,response,fileName,dlFileEntry.getContentStream(),dlFileEntry.getSize(),dlFileEntry.getMimeType());
}
