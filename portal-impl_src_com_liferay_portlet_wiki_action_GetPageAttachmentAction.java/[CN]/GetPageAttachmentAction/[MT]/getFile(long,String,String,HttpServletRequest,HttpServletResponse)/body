{
  WikiPage wikiPage=WikiPageServiceUtil.getPage(nodeId,title);
  FileEntry fileEntry=PortletFileRepositoryUtil.getPortletFileEntry(wikiPage.getGroupId(),wikiPage.getAttachmentsFolderId(),fileName);
  ServletResponseUtil.sendFile(request,response,fileName,fileEntry.getContentStream(),fileEntry.getSize(),fileEntry.getMimeType());
}
