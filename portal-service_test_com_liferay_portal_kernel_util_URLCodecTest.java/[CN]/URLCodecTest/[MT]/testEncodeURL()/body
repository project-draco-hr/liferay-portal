{
  for (int i=0; i < _rawURLStrings.length; i++) {
    String result=URLCodec.encodeURL(_rawURLStrings[i],StringPool.UTF8,false);
    assertTrue(_encodedURLStrings[i].equalsIgnoreCase(result));
    result=URLCodec.encodeURL(_rawURLStrings[i],StringPool.UTF8,true);
    assertTrue(_escapeSpacesEncodeURLStrings[i].equalsIgnoreCase(result));
  }
}
