{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MDRRuleGroup.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("ruleGroupId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("ruleGroupId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
