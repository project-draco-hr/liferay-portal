{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace("Asset Publisher Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("DM Folder Document Title"),selenium.getText("//h3[@class='asset-title']/a"));
  assertEquals(RuntimeVariables.replace("Download DM Folder Document Title (0k)"),selenium.getText("//div[@class='asset-resource-info']/span/a/span"));
  assertEquals(RuntimeVariables.replace("Read More About DM Folder Document Title \u00bb"),selenium.getText("//div[@class='asset-more']/a"));
  assertFalse(selenium.isTextPresent("//div[@class='portlet-msg-info']"));
}
