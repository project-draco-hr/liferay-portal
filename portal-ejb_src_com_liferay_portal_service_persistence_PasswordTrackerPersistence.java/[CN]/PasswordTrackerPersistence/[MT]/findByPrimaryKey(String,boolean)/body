{
  Session session=null;
  try {
    session=openSession();
    PasswordTracker passwordTracker=(PasswordTracker)session.get(PasswordTracker.class,passwordTrackerId);
    if (passwordTracker == null) {
      _log.warn("No PasswordTracker exists with the primary key " + passwordTrackerId.toString());
      if (throwNoSuchObjectException) {
        throw new NoSuchPasswordTrackerException("No PasswordTracker exists with the primary key " + passwordTrackerId.toString());
      }
    }
    return passwordTracker;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
