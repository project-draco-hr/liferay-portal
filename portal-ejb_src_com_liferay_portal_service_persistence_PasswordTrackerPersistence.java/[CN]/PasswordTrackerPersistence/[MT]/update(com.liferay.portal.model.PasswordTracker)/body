{
  Session session=null;
  try {
    session=openSession();
    if (passwordTracker.isNew()) {
      session.save(passwordTracker);
    }
    session.flush();
    passwordTracker.setNew(false);
    return passwordTracker;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
