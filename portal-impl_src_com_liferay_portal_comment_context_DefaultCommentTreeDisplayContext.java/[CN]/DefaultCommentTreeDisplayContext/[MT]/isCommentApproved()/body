{
  boolean approved=true;
  if (_comment instanceof WorkflowableComment) {
    WorkflowableComment workflowableComment=(WorkflowableComment)_comment;
    if (workflowableComment.getStatus() == WorkflowConstants.STATUS_APPROVED) {
      approved=true;
    }
 else {
      approved=false;
    }
  }
  return approved;
}
