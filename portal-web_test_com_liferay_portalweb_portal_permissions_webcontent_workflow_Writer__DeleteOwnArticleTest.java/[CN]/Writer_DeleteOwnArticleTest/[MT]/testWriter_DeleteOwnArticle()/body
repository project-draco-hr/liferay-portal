{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("link=Web Content"));
  selenium.waitForPageToLoad("30000");
  selenium.click("_15_allRowIds");
  selenium.click(RuntimeVariables.replace("//input[@value='Delete']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete the selected web content[\\s\\S]$"));
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertFalse(selenium.isElementPresent("link=Writer Permissions Web Content Test Edit"));
}
