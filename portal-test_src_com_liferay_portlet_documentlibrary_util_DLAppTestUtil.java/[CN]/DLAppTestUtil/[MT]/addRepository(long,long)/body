{
  long userId=TestPropsValues.getUserId();
  Folder folder=addFolder(groupId,DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,ServiceTestUtil.randomString());
  long parentFolderId=folder.getFolderId();
  String name=ServiceTestUtil.randomString();
  String description=ServiceTestUtil.randomString();
  String portletId=PortletKeys.DOCUMENT_LIBRARY;
  UnicodeProperties typeSettingsProperties=new UnicodeProperties();
  boolean hidden=false;
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext(groupId);
  return addRepository(userId,groupId,classNameId,parentFolderId,name,description,portletId,typeSettingsProperties,hidden,serviceContext);
}
