{
  if (actionId.equals(ActionKeys.ADD_ENTRY) && layout instanceof VirtualLayout) {
    return false;
  }
  if (permissionChecker.isGroupAdmin(layout.getGroupId()) || permissionChecker.isGroupOwner(layout.getGroupId())) {
    return true;
  }
  return PortletPermissionUtil.contains(permissionChecker,layout,name,actionId);
}
