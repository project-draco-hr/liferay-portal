{
  if (permissionChecker.hasOwnerPermission(entry.getCompanyId(),AnnouncementsEntry.class.getName(),entry.getEntryId(),entry.getUserId(),actionId)) {
    return true;
  }
  long groupId=0;
  long classPK=entry.getClassPK();
  if (classPK > 0) {
    try {
      groupId=AnnouncementsEntryLocalServiceUtil.getGroupId(entry);
    }
 catch (    Exception e) {
    }
  }
  return permissionChecker.hasPermission(groupId,AnnouncementsEntry.class.getName(),entry.getEntryId(),actionId);
}
