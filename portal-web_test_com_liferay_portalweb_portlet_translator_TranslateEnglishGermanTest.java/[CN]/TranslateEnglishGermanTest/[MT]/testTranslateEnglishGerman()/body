{
  selenium.select("_26_id","label=English to German");
  selenium.type("_26_text","My name is Liferay Translator, fluent in over 6 million forms of communication.");
  selenium.click("//input[@value='Translate']");
  selenium.waitForPageToLoad("30000");
  verifyTrue(selenium.isTextPresent("Mein Name ist der Liferay \u00dcbersetzer, der innen \u00fcber 6 Million Formen Kommunikation flie\u00dfend ist."));
}
