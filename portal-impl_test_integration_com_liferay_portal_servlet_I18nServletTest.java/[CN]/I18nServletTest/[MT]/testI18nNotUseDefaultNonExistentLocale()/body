{
  boolean originalLocaleUseDefault=PropsValues.LOCALE_USE_DEFAULT_IF_NOT_AVAILABLE;
  try {
    PropsValues.LOCALE_USE_DEFAULT_IF_NOT_AVAILABLE=false;
    Locale expectedLocale=_nonExistentLocale;
    testGetI18nData(expectedLocale,null);
  }
  finally {
    PropsValues.LOCALE_USE_DEFAULT_IF_NOT_AVAILABLE=originalLocaleUseDefault;
  }
}
