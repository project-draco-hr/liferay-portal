{
  boolean originalLocaleUseDefaultIfNotAvailable=PropsValues.LOCALE_USE_DEFAULT_IF_NOT_AVAILABLE;
  try {
    PropsValues.LOCALE_USE_DEFAULT_IF_NOT_AVAILABLE=false;
    Locale expectedLocale=LocaleUtil.CHINA;
    testGetI18nData(expectedLocale,null);
  }
  finally {
    PropsValues.LOCALE_USE_DEFAULT_IF_NOT_AVAILABLE=originalLocaleUseDefaultIfNotAvailable;
  }
}
