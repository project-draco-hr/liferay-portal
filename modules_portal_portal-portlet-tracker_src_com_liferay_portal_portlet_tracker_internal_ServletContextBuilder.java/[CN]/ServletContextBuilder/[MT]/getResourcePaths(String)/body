{
  if (path == null) {
    return null;
  }
  Enumeration<URL> enumeration=_bundle.findEntries(path,null,false);
  if (enumeration == null) {
    return null;
  }
  Set<String> paths=new HashSet<String>();
  while (enumeration.hasMoreElements()) {
    URL url=enumeration.nextElement();
    paths.add(url.toExternalForm());
  }
  return paths;
}
