{
  ScheduledFuture scheduledFuture=null;
  if (PropsValues.SLOWNESS_DETECTER_ENABLED) {
    scheduledFuture=_scheduledExecutorService.schedule(_dumper,_slownessDetecterThreshold,TimeUnit.SECONDS);
  }
  try {
    processFilter(ThreadLocalFilter.class,request,response,filterChain);
  }
  finally {
    if (scheduledFuture != null) {
      scheduledFuture.cancel(true);
    }
    ThreadLocalRegistry.resetThreadLocals();
  }
}
