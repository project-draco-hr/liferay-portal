{
  DDMListItemImpl ddmListItemImpl=new DDMListItemImpl();
  ddmListItemImpl.setUuid(getUuid());
  ddmListItemImpl.setListItemId(getListItemId());
  ddmListItemImpl.setClassNameId(getClassNameId());
  ddmListItemImpl.setClassPK(getClassPK());
  ddmListItemImpl.setListId(getListId());
  ddmListItemImpl.resetOriginalValues();
  return ddmListItemImpl;
}
