{
  PortalUtil.setPortalPort(request);
  ServletContext servletContext=getServletContext();
  ServletContext portalServletContext=servletContext.getContext("/");
  RequestDispatcher requestDispatcher=portalServletContext.getRequestDispatcher("/c/portal/resiliency");
  SPI spi=SPIUtil.getSPI();
  SPIAgent spiAgent=spi.getSPIAgent();
  HttpServletRequest agentServletRequest=spiAgent.prepareRequest(request);
  HttpServletResponse agentServletResponse=spiAgent.prepareResponse(request,response);
  Exception exception=null;
  try {
    requestDispatcher.forward(agentServletRequest,agentServletResponse);
  }
 catch (  Exception e) {
    exception=e;
  }
  spiAgent.transferResponse(agentServletRequest,agentServletResponse,exception);
  HttpSession session=agentServletRequest.getSession();
  session.invalidate();
}
