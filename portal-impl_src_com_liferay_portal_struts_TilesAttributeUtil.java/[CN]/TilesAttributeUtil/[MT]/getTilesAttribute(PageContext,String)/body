{
  ServletRequest servletRequest=pageContext.getRequest();
  String parameter=servletRequest.getParameter(tilesAttributeName);
  if (parameter != null) {
    return parameter;
  }
  ComponentContext componentContext=(ComponentContext)pageContext.getAttribute(ComponentConstants.COMPONENT_CONTEXT,PageContext.REQUEST_SCOPE);
  if (componentContext == null) {
    return null;
  }
  return componentContext.getAttribute(tilesAttributeName);
}
