{
  int bufferSize=10;
  UnsyncBufferedInputStream unsyncBis=new UnsyncBufferedInputStream(new ByteArrayInputStream(new byte[bufferSize]));
  assertEquals(bufferSize,unsyncBis.available());
  unsyncBis=new UnsyncBufferedInputStream(new ByteArrayInputStream(new byte[bufferSize]),_BUFFER_SIZE);
  assertEquals(bufferSize,unsyncBis.available());
}
