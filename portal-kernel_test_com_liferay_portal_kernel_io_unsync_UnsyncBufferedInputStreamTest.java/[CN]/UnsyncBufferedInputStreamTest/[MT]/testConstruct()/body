{
  int testSize=10;
  UnsyncBufferedInputStream unsyncBis=new UnsyncBufferedInputStream(new ByteArrayInputStream(new byte[testSize]));
  assertEquals(testSize,unsyncBis.available());
  unsyncBis=new UnsyncBufferedInputStream(new ByteArrayInputStream(new byte[testSize]),testDataSize);
  assertEquals(testSize,unsyncBis.available());
}
