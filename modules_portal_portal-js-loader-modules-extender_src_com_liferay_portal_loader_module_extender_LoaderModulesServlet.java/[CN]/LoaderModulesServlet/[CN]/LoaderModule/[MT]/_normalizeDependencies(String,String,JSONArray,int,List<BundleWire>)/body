{
  for (  BundleWire bundleWire : requiredWires) {
    BundleCapability capability=bundleWire.getCapability();
    Map<String,Object> attributes=capability.getAttributes();
    String requiredWireName=(String)attributes.get(Details.OSGI_WEBRESOURCE);
    if (!requiredWireName.equals(dependencyName)) {
      continue;
    }
    Version requiredWireVersion=(Version)attributes.get(Constants.VERSION_ATTRIBUTE);
    dependencyName=dependencyName + "@" + requiredWireVersion.toString()+ dependencyPath;
    jsonArray.put(index,dependencyName);
    return;
  }
}
