{
  _databaseName=databaseName;
  _databaseType=databaseType;
  _sqlDir=sqlDir;
  _fileName=fileName;
  try {
    if (_databaseType.equals("derby")) {
      _loadDerby();
    }
 else     if (_databaseType.equals("hypersonic")) {
      _loadHypersonic();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
