{
  PortalClassLoaderUtil.setClassLoader(Thread.currentThread().getContextClassLoader());
  try {
    _databaseType=databaseType;
    _databaseName=databaseName;
    _fileName=fileName;
    if (_databaseType.equals("derby")) {
      _loadDerby();
    }
 else     if (_databaseType.equals("hypersonic")) {
      _loadHypersonic();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
