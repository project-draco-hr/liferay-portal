{
  long realUserId=0;
  if (_threadLocal.get() != null) {
    ThemeDisplay themeDisplay=(ThemeDisplay)_threadLocal.get().getAttribute(WebKeys.THEME_DISPLAY);
    realUserId=themeDisplay.getRealUserId();
  }
  if (_log.isDebugEnabled()) {
    _log.debug("getRealUserId " + realUserId);
  }
  return realUserId;
}
