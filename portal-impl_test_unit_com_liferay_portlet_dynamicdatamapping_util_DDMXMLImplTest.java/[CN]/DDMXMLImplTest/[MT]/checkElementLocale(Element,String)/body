{
  List<Node> nodes=element.selectNodes("dynamic-element");
  for (  Node node : nodes) {
    Element metadataElement=(Element)node.selectSingleNode("meta-data[@locale='" + newLocaleId + "']");
    if (metadataElement == null) {
      return false;
    }
    if (!checkElementLocale((Element)node,newLocaleId)) {
      return false;
    }
  }
  return true;
}
