{
  Assert.assertTrue(JournalArticlePermission.contains(permissionChecker,_article,ActionKeys.VIEW));
  Assert.assertTrue(JournalArticlePermission.contains(permissionChecker,_subarticle,ActionKeys.VIEW));
  removePortletModelViewPermission();
  Assert.assertNotEquals(PropsValues.JOURNAL_ARTICLE_VIEW_PERMISSION_CHECK_ENABLED,JournalArticlePermission.contains(permissionChecker,_article,ActionKeys.VIEW));
  Assert.assertNotEquals(PropsValues.JOURNAL_ARTICLE_VIEW_PERMISSION_CHECK_ENABLED,JournalArticlePermission.contains(permissionChecker,_subarticle,ActionKeys.VIEW));
}
