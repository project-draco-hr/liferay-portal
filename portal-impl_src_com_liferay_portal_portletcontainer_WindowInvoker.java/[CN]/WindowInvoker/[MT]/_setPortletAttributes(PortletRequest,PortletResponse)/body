{
  HttpServletRequest request=PortalUtil.getHttpServletRequest(portletRequest);
  if (_portletConfig != null) {
    request.setAttribute(JavaConstants.JAVAX_PORTLET_CONFIG,_portletConfig);
  }
 else {
    request.setAttribute(JavaConstants.JAVAX_PORTLET_CONFIG,getPortletConfig());
  }
  request.setAttribute(JavaConstants.JAVAX_PORTLET_REQUEST,portletRequest);
  request.setAttribute(JavaConstants.JAVAX_PORTLET_RESPONSE,portletResponse);
}
