{
  if (_remoteUserId <= 0) {
    return Collections.EMPTY_LIST;
  }
  try {
    List<Role> roles=RoleLocalServiceUtil.getUserRoles(_remoteUserId);
    List<String> roleNames=new ArrayList<String>(roles.size());
    for (    Role role : roles) {
      roleNames.add(role.getName());
    }
    return roleNames;
  }
 catch (  Exception e) {
    _log.error(e,e);
    return Collections.EMPTY_LIST;
  }
}
