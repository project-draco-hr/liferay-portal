{
  if (!_include) {
    if (_lifecycle.equals(PortletRequest.RENDER_PHASE) || _lifecycle.equals(PortletRequest.RESOURCE_PHASE)) {
      return _response.isCommitted();
    }
 else {
      return false;
    }
  }
 else {
    if (_lifecycle.equals(PortletRequest.RENDER_PHASE) || _lifecycle.equals(PortletRequest.RESOURCE_PHASE)) {
      return _response.isCommitted();
    }
 else {
      return true;
    }
  }
}
