{
  EntityCacheUtil.removeResult(PollsChoiceModelImpl.ENTITY_CACHE_ENABLED,PollsChoiceImpl.class,pollsChoice.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_Q_N,new Object[]{Long.valueOf(pollsChoice.getQuestionId()),pollsChoice.getName()});
}
