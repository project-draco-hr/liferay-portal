{
  Session session=null;
  try {
    session=openSession();
    PollsChoice pollsChoice=(PollsChoice)session.get(PollsChoiceImpl.class,Long.valueOf(choiceId));
    if (pollsChoice == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + choiceId);
      }
      throw new NoSuchChoiceException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + choiceId);
    }
    return pollsChoicePersistence.remove(pollsChoice);
  }
 catch (  NoSuchChoiceException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
