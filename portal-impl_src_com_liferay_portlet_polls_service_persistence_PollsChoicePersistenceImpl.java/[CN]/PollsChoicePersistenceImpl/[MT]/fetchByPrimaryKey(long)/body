{
  PollsChoice result=(PollsChoice)EntityCacheUtil.getResult(PollsChoiceModelImpl.ENTITY_CACHE_ENABLED,PollsChoice.class,choiceId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      PollsChoice pollsChoice=(PollsChoice)session.get(PollsChoiceImpl.class,new Long(choiceId));
      cacheResult(pollsChoice);
      return pollsChoice;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (PollsChoice)result;
  }
}
