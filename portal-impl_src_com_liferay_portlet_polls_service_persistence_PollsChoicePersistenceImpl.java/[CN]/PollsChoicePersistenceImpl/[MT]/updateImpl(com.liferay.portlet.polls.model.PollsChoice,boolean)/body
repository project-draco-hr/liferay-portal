{
  pollsChoice=toUnwrappedModel(pollsChoice);
  boolean isNew=pollsChoice.isNew();
  PollsChoiceModelImpl pollsChoiceModelImpl=(PollsChoiceModelImpl)pollsChoice;
  if (Validator.isNull(pollsChoice.getUuid())) {
    String uuid=PortalUUIDUtil.generate();
    pollsChoice.setUuid(uuid);
  }
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,pollsChoice,merge);
    pollsChoice.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  if (isNew) {
    FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  }
 else {
    if (!Validator.equals(pollsChoice.getUuid(),pollsChoiceModelImpl.getOriginalUuid())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_UUID,new Object[]{pollsChoiceModelImpl.getOriginalUuid()});
    }
    if (pollsChoice.getQuestionId() != pollsChoiceModelImpl.getOriginalQuestionId()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_QUESTIONID,new Object[]{Long.valueOf(pollsChoiceModelImpl.getOriginalQuestionId())});
    }
  }
  EntityCacheUtil.putResult(PollsChoiceModelImpl.ENTITY_CACHE_ENABLED,PollsChoiceImpl.class,pollsChoice.getPrimaryKey(),pollsChoice);
  if (isNew) {
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_Q_N,new Object[]{Long.valueOf(pollsChoice.getQuestionId()),pollsChoice.getName()},pollsChoice);
  }
 else {
    if ((pollsChoice.getQuestionId() != pollsChoiceModelImpl.getOriginalQuestionId()) || !Validator.equals(pollsChoice.getName(),pollsChoiceModelImpl.getOriginalName())) {
      FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_Q_N,new Object[]{Long.valueOf(pollsChoiceModelImpl.getOriginalQuestionId()),pollsChoiceModelImpl.getOriginalName()});
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_Q_N,new Object[]{Long.valueOf(pollsChoice.getQuestionId()),pollsChoice.getName()},pollsChoice);
    }
  }
  return pollsChoice;
}
