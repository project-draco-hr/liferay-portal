{
  ResourceBundle resourceBundle=ResourceBundle.getBundle("content.Language",LocaleUtil.getDefault());
  String name=ResourceBundleUtil.getString(resourceBundle,nameKey);
  String description=ResourceBundleUtil.getString(resourceBundle,descriptionKey);
  for (  LayoutPrototype layoutPrototype : layoutPrototypes) {
    String curName=layoutPrototype.getName(LocaleUtil.getDefault());
    String curDescription=layoutPrototype.getDescription(LocaleUtil.getDefault());
    if (name.equals(curName) && description.equals(curDescription)) {
      return null;
    }
  }
  Map<Locale,String> nameMap=new HashMap<>();
  Map<Locale,String> descriptionMap=new HashMap<>();
  Set<Locale> locales=LanguageUtil.getAvailableLocales();
  for (  Locale locale : locales) {
    resourceBundle=ResourceBundle.getBundle("content.Language",locale);
    if (Validator.isNull(resourceBundle)) {
      continue;
    }
    nameMap.put(locale,ResourceBundleUtil.getString(resourceBundle,nameKey));
    descriptionMap.put(locale,ResourceBundleUtil.getString(resourceBundle,descriptionKey));
  }
  LayoutPrototype layoutPrototype=_layoutPrototypeLocalService.addLayoutPrototype(defaultUserId,companyId,nameMap,descriptionMap,true,new ServiceContext());
  Layout layout=layoutPrototype.getLayout();
  LayoutTypePortlet layoutTypePortlet=(LayoutTypePortlet)layout.getLayoutType();
  layoutTypePortlet.setLayoutTemplateId(0,layoutTemplateId,false);
  return layout;
}
