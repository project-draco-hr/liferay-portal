{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(PortalUtil.getControlPanelPlid(liferayPortletRequest),PortletKeys.MESSAGE_BOARDS,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/message_boards/edit_message");
  portletURL.setParameter("messageId",String.valueOf(_message.getMessageId()));
  return portletURL;
}
