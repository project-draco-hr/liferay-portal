{
  GeoDistanceRangeQueryBuilder geoDistanceRangeQueryBuilder=QueryBuilders.geoDistanceRangeQuery(geoDistanceRangeFilter.getField());
  geoDistanceRangeQueryBuilder.from(String.valueOf(geoDistanceRangeFilter.getLowerBoundGeoDistance()));
  geoDistanceRangeQueryBuilder.includeLower(geoDistanceRangeFilter.isIncludesLower());
  geoDistanceRangeQueryBuilder.includeUpper(geoDistanceRangeFilter.isIncludesUpper());
  GeoLocationPoint pinGeoLocationPoint=geoDistanceRangeFilter.getPinGeoLocationPoint();
  geoDistanceRangeQueryBuilder.point(pinGeoLocationPoint.getLatitude(),pinGeoLocationPoint.getLongitude());
  geoDistanceRangeQueryBuilder.to(String.valueOf(geoDistanceRangeFilter.getUpperBoundGeoDistance()));
  return geoDistanceRangeQueryBuilder;
}
