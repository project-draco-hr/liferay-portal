{
  EventExecutorGroup eventExecutorGroup=new DefaultEventExecutorGroup(threadCount,new NamedThreadFactory(threadPoolName,Thread.NORM_PRIORITY,null));
  EventLoop eventLoop=_serverChannel.eventLoop();
  NettyUtil.bindShutdown(eventLoop.parent(),eventExecutorGroup,_nettyFabricServerConfig.getShutdownQuietPeriod(),_nettyFabricServerConfig.getShutdownTimeout());
  return eventExecutorGroup;
}
