{
  Channel channel=channelFuture.channel();
  if (channelFuture.isSuccess()) {
    if (_log.isInfoEnabled()) {
      _log.info("Started netty fabric server on " + channel.localAddress());
    }
    return;
  }
  String serverAddress=_nettyFabricServerConfig.getNettyFabricServerHost() + ":" + _nettyFabricServerConfig.getNettyFabricServerPort();
  if (channelFuture.isCancelled()) {
    _log.error("Cancelled starting netty fabric server on " + serverAddress);
  }
 else {
    _log.error("Unable to start netty fabric server on " + serverAddress,channelFuture.cause());
  }
  stop();
}
