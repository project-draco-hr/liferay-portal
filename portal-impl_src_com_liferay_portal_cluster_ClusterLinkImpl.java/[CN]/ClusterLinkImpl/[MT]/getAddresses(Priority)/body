{
  if (!PropsValues.CLUSTER_LINK_ENABLED) {
    return Collections.EMPTY_LIST;
  }
  Vector<org.jgroups.Address> jGroupsAddresses=getChannel(priority).getView().getMembers();
  if (jGroupsAddresses == null) {
    return new ArrayList<Address>();
  }
  List<Address> addresses=new ArrayList<Address>(jGroupsAddresses.size());
  for (  org.jgroups.Address address : jGroupsAddresses) {
    addresses.add(new AddressImpl(address));
  }
  return addresses;
}
