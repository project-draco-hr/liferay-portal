{
  if (!PropsValues.CLUSTER_LINK_ENABLED) {
    return;
  }
  if (OSDetector.isUnix() && IPProtocolDetector.supportIPv6() && !IPProtocolDetector.isPreferIPv4()) {
    if (_log.isWarnEnabled()) {
      _log.warn("You are on an Unix/Linux machine with IPv6 " + "enabled, JGroups may not work well, if you see any " + "IP multicast error, try to add "+ "java.net.preferIPv4Stack=true to your JVM startup "+ "parameters.");
    }
  }
  initSystemProperties();
  try {
    initBindAddress();
  }
 catch (  IOException ioe) {
    if (_log.isWarnEnabled()) {
      _log.warn("Failed to initialize outgoing IP address",ioe);
    }
  }
  try {
    initChannels();
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
