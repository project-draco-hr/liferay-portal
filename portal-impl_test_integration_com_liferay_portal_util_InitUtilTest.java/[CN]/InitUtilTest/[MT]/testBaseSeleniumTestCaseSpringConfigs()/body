{
  String springConfigs=PropsUtil.get(PropsKeys.SPRING_CONFIGS);
  try {
    String baseSeleniumTestCaseSpringConfigs="META-INF/management-spring.xml,META-INF/util-spring.xml";
    PropsUtil.set(PropsKeys.SPRING_CONFIGS,baseSeleniumTestCaseSpringConfigs);
    InitUtil.initWithSpringAndModuleFramework();
  }
  finally {
    PropsUtil.set(PropsKeys.SPRING_CONFIGS,springConfigs);
  }
}
