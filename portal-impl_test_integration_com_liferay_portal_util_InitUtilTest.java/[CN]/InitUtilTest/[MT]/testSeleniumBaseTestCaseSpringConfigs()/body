{
  String springConfigs=PropsUtil.get(PropsKeys.SPRING_CONFIGS);
  try {
    String seleniumBaseTestCaseSpringConfigs="META-INF/management-spring.xml,META-INF/util-spring.xml";
    PropsUtil.set(PropsKeys.SPRING_CONFIGS,seleniumBaseTestCaseSpringConfigs);
    InitUtil.initWithSpringAndModuleFramework();
  }
  finally {
    PropsUtil.set(PropsKeys.SPRING_CONFIGS,springConfigs);
  }
}
