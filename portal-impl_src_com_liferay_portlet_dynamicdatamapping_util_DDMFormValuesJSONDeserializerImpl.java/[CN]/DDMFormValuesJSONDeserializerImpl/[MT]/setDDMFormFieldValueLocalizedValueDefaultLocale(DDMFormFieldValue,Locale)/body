{
  List<Value> values=ddmFormFieldValue.getValues();
  for (  Value value : values) {
    if (!value.isLocalized()) {
      break;
    }
    value.setDefaultLocale(defaultLocale);
  }
  for (  DDMFormFieldValue nestedDDMFormFieldValue : ddmFormFieldValue.getNestedDDMFormFieldValues()) {
    setDDMFormFieldValueLocalizedValueDefaultLocale(nestedDDMFormFieldValue,defaultLocale);
  }
}
