{
  DDMFormFieldValue ddmFormFieldValue=new DDMFormFieldValue();
  ddmFormFieldValue.setName(jsonObject.getString("name"));
  setDDMFormFieldValueValues(jsonObject.getJSONArray("values"),ddmFormFieldValue);
  setNestedDDMFormFieldValues(jsonObject.getJSONArray("nestedFieldValues"),ddmFormFieldValue);
  return ddmFormFieldValue;
}
