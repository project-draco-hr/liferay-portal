{
  if (_ddmStructures == null) {
    getDDMStructures();
  }
  if (!_ddmStructures.contains(ddmStructure)) {
    throw new IllegalArgumentException("DDM structure does not belong to this file version");
  }
  DLFileEntryMetadata dlFileEntryMetadata=_dlFileEntryMetadataLocalService.getFileEntryMetadata(ddmStructure.getStructureId(),_fileVersion.getFileVersionId());
  return _storageEngine.getFields(dlFileEntryMetadata.getDDMStorageId());
}
