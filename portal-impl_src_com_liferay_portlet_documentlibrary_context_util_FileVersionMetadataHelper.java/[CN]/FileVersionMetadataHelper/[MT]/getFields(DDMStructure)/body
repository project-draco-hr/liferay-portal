{
  if (_ddmStructures == null) {
    getDDMStructures();
  }
  if (!_ddmStructures.contains(ddmStructure)) {
    throw new IllegalArgumentException("DDMStructure does not belong to this file version");
  }
  DLFileEntryMetadata fileEntryMetadata=_dlFileEntryMetadataLocalService.getFileEntryMetadata(ddmStructure.getStructureId(),_fileVersion.getFileVersionId());
  return _storageEngine.getFields(fileEntryMetadata.getDDMStorageId());
}
