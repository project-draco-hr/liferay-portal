{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(shoppingItemField);
    session.flush();
    shoppingItemField.setNew(false);
    return shoppingItemField;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
