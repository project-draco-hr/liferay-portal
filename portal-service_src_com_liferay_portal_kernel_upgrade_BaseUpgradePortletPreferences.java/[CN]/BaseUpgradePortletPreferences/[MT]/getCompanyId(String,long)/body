{
  long companyId=0;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement(sql);
    ps.setLong(1,primaryKey);
    rs=ps.executeQuery();
    while (rs.next()) {
      companyId=rs.getLong("companyId");
    }
  }
  finally {
    DataAccess.cleanUp(null,ps,rs);
  }
  return companyId;
}
