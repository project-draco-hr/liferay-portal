{
  super(webDAVRequest.getRootPath() + webDAVRequest.getPath(),_getName(fileEntry,appendPath),_getName(fileEntry,true),fileEntry.getCreateDate(),fileEntry.getModifiedDate(),fileEntry.getSize());
  setModel(fileEntry);
  setClassName(DLFileEntry.class.getName());
  setPrimaryKey(fileEntry.getPrimaryKey());
}
