{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Events Display Test Page");
  selenium.clickAt("link=Events Display Test Page",RuntimeVariables.replace("Events Display Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//a[@id='_145_addApplication']","More"));
  selenium.clickAt("//a[@id='_145_addApplication']",RuntimeVariables.replace("More"));
  selenium.waitForElementPresent("//div[@title=\"Events Display\"]");
  assertFalse(selenium.isVisible("//div[@title=\"Events Display\"]/p/a"));
}
