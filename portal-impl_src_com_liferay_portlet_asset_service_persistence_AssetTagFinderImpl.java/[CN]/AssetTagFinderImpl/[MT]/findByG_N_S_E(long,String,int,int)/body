{
  List<AssetTag> list=new ArrayList<AssetTag>();
  Session session=null;
  try {
    session=openSession();
    String sql=CustomSQLUtil.get(FIND_BY_G_N_S_E);
    SQLQuery q=session.createSQLQuery(sql);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(groupId);
    qPos.add(name);
    qPos.add(startPeriod);
    qPos.add(endPeriod);
    Iterator<Object[]> itr=q.iterate();
    while (itr.hasNext()) {
      Object[] array=itr.next();
      AssetTag assetTag=new AssetTagImpl();
      assetTag.setTagId(GetterUtil.getLong((Serializable)array[0]));
      assetTag.setName(GetterUtil.getString((Serializable)array[1]));
      assetTag.setAssetCount(GetterUtil.getInteger((Serializable)array[2]));
      list.add(assetTag);
    }
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
  return list;
}
