{
  Group group=GroupTestUtil.addGroup();
  BookmarksFolder parentFolder=BookmarksTestUtil.addFolder(group.getGroupId(),"parentFolder");
  BookmarksFolder folder=BookmarksTestUtil.addFolder(group.getGroupId(),parentFolder.getFolderId(),"folder");
  BookmarksFolderLocalServiceUtil.moveFolderToTrash(TestPropsValues.getUserId(),folder.getFolderId());
  BookmarksFolderLocalServiceUtil.deleteFolder(parentFolder.getFolderId(),false);
  doVerify();
}
