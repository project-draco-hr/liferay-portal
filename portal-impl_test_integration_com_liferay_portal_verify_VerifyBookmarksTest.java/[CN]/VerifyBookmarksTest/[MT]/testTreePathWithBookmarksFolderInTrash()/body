{
  Group group=GroupTestUtil.addGroup();
  BookmarksFolder parentFolder=BookmarksTestUtil.addFolder(group.getGroupId(),"parentFolder");
  BookmarksFolder childFolder=BookmarksTestUtil.addFolder(group.getGroupId(),parentFolder.getFolderId(),"childFolder");
  BookmarksFolderLocalServiceUtil.moveFolderToTrash(TestPropsValues.getUserId(),childFolder.getFolderId());
  BookmarksFolderLocalServiceUtil.deleteFolder(parentFolder.getFolderId(),false);
  doVerify();
}
