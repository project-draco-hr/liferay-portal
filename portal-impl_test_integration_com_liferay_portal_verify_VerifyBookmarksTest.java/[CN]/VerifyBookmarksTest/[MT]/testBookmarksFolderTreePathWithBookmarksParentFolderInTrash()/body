{
  Group group=GroupTestUtil.addGroup();
  BookmarksFolder grandParentFolder=BookmarksTestUtil.addFolder(group.getGroupId(),"grandParentFolder");
  BookmarksFolder parentFolder=BookmarksTestUtil.addFolder(group.getGroupId(),grandParentFolder.getFolderId(),"parentFolder");
  BookmarksTestUtil.addFolder(group.getGroupId(),parentFolder.getFolderId(),"folder");
  BookmarksFolderLocalServiceUtil.moveFolderToTrash(TestPropsValues.getUserId(),parentFolder.getFolderId());
  BookmarksFolderLocalServiceUtil.deleteFolder(grandParentFolder.getFolderId(),false);
  doVerify();
}
