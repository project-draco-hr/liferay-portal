{
  try {
    com.liferay.portlet.documentlibrary.model.DLFolder returnValue=DLRepositoryServiceUtil.getFolder(groupId,parentFolderId,name);
    return com.liferay.portlet.documentlibrary.model.DLFolderSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
