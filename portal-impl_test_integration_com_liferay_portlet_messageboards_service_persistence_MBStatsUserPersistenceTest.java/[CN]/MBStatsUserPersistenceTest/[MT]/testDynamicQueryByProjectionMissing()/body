{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBStatsUser.class,MBStatsUser.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("statsUserId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("statsUserId",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
