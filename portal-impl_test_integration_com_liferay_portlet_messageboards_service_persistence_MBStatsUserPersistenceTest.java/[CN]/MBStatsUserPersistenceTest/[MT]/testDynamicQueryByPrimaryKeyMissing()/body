{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBStatsUser.class,MBStatsUser.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("statsUserId",ServiceTestUtil.nextLong()));
  List<MBStatsUser> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
