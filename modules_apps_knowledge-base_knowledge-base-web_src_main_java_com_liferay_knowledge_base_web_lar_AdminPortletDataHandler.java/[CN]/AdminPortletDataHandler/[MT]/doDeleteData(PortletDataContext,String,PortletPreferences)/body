{
  if (portletDataContext.addPrimaryKey(AdminPortletDataHandler.class,"deleteData")) {
    return portletPreferences;
  }
  KBArticleLocalServiceUtil.deleteGroupKBArticles(portletDataContext.getScopeGroupId());
  KBTemplateLocalServiceUtil.deleteGroupKBTemplates(portletDataContext.getScopeGroupId());
  return portletPreferences;
}
