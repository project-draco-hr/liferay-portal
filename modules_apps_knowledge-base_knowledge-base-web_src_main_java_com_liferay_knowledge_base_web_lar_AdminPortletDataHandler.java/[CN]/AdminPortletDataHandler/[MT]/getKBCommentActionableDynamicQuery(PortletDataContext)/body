{
  ExportActionableDynamicQuery exportActionableDynamicQuery=KBCommentLocalServiceUtil.getExportActionableDynamicQuery(portletDataContext);
  exportActionableDynamicQuery.setStagedModelType(new StagedModelType(PortalUtil.getClassNameId(KBComment.class),StagedModelType.REFERRER_CLASS_NAME_ID_ALL));
  return exportActionableDynamicQuery;
}
