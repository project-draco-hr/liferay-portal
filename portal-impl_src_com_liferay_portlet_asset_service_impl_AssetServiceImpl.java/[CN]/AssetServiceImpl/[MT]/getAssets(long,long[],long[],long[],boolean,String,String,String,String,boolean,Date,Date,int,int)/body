{
  long[][] viewableTagIds=getViewableTagIds(tagIds,notTagIds);
  tagIds=viewableTagIds[0];
  notTagIds=viewableTagIds[1];
  return assetLocalService.getAssets(groupId,classNameIds,tagIds,notTagIds,andOperator,orderByCol1,orderByCol2,orderByType1,orderByType2,excludeZeroViewCount,publishDate,expirationDate,start,end);
}
