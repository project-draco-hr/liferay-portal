{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Google Gadget Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Google Gadget Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals("Do you often need to convert currencies?\nTry the new Currency Converter Gadget here on iGoogle!",selenium.getText("message"));
}
