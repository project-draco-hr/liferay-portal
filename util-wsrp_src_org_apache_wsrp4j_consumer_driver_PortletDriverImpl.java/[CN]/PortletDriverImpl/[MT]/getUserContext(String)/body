{
  UserContext userContext=null;
  if (userID != null) {
    User user=consumerEnv.getUserRegistry().getUser(userID);
    if (user != null) {
      userContext=user.getUserContext();
    }
  }
  if (userContext == null) {
    userContext=new UserContext();
    userContext.setUserContextKey("dummyUserContextKey");
  }
  return userContext;
}
