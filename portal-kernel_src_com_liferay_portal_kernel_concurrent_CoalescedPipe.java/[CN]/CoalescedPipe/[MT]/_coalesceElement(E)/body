{
  try {
    _takeLock.lockInterruptibly();
    try {
      ElementLink<E> currentElementLink=_headElementLink._nextElementLink;
      if (_comparator != null) {
        while (currentElementLink != null) {
          if (_comparator.compare(currentElementLink._element,e) == 0) {
            _coalescedCount.incrementAndGet();
            return true;
          }
 else {
            currentElementLink=currentElementLink._nextElementLink;
          }
        }
      }
 else {
        while (currentElementLink != null) {
          if (currentElementLink._element.equals(e)) {
            _coalescedCount.incrementAndGet();
            return true;
          }
 else {
            currentElementLink=currentElementLink._nextElementLink;
          }
        }
      }
    }
  finally {
      _takeLock.unlock();
    }
  }
 catch (  InterruptedException ie) {
  }
  return false;
}
