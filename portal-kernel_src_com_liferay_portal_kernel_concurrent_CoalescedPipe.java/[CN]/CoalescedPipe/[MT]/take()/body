{
  E item;
  _takeLock.lockInterruptibly();
  try {
    while (_pendingCount.get() == 0) {
      _notEmpty.await();
    }
    Element<E> garbageELement=_head;
    _head=_head.next;
    garbageELement.next=null;
    item=_head.item;
    _head.item=null;
    int pendingElements=_pendingCount.getAndDecrement();
    if (pendingElements > 1) {
      _notEmpty.signal();
    }
  }
  finally {
    _takeLock.unlock();
  }
  return item;
}
