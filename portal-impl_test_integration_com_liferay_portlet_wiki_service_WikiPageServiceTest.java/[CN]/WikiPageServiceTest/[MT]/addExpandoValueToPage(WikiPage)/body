{
  ExpandoValue value=ExpandoTestUtil.addValue(PortalUtil.getClassNameId(WikiPage.class),page.getPrimaryKey(),ServiceTestUtil.randomString());
  ExpandoBridge expandoBridge=page.getExpandoBridge();
  ExpandoColumn column=value.getColumn();
  expandoBridge.addAttribute(column.getName(),ExpandoColumnConstants.STRING,value.getString());
}
