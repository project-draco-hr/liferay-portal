{
  String className=PortalUtil.getClassName(resourceClassNameId);
  ServiceWrapper<DDMTemplatePermissionSupport> ddmTemplatePermissionSupportServiceWrapper=_ddmTemplatePermissionSupportServiceTrackerMap.getService(className);
  if (ddmTemplatePermissionSupportServiceWrapper == null) {
    throw new PortalException("The model does not support DDMTemplate permission checking " + className);
  }
  return ddmTemplatePermissionSupportServiceWrapper;
}
