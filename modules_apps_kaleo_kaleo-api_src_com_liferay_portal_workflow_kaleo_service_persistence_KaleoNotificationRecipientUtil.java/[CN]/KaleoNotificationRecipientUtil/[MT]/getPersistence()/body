{
  if (_persistence == null) {
    _persistence=(KaleoNotificationRecipientPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoNotificationRecipientPersistence.class.getName());
