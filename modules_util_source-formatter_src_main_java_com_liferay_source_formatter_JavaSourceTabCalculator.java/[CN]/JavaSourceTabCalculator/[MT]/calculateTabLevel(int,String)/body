{
  level=BaseSourceProcessor.adjustTabLevel(level,text,StringPool.OPEN_CURLY_BRACE,1);
  level=BaseSourceProcessor.adjustTabLevel(level,text,StringPool.CLOSE_CURLY_BRACE,-1);
  level=BaseSourceProcessor.adjustTabLevel(level,text,StringPool.OPEN_PARENTHESIS,1);
  level=BaseSourceProcessor.adjustTabLevel(level,text,StringPool.CLOSE_PARENTHESIS,-1);
  return level;
}
