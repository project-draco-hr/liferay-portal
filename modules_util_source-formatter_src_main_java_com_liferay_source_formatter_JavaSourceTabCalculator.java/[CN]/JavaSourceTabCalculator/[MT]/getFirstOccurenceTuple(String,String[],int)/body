{
  String matchingText=null;
  int firstIndexOf=-1;
  for (  String text : texts) {
    int x=s.indexOf(text,startIndex);
    if (x == -1) {
      continue;
    }
    if ((firstIndexOf == -1) || (x < firstIndexOf)) {
      firstIndexOf=x;
      matchingText=text;
    }
  }
  return new Tuple(matchingText,firstIndexOf);
}
