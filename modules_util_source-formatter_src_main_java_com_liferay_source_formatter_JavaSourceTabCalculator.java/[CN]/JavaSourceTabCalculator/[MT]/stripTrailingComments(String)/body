{
  Matcher matcher=_trailingCommentPattern.matcher(content);
  while (matcher.find()) {
    String match=matcher.group();
    if (!ToolsUtil.isInsideQuotes(match,matcher.start(2) - matcher.start())) {
      String replacement=StringUtil.replace(match,matcher.group(2),StringPool.BLANK);
      content=StringUtil.replace(content,match,replacement);
    }
  }
  return content;
}
