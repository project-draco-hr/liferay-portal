{
  String[] lines=StringUtil.splitLines(text);
  forLoop:   for (  String line : lines) {
    line=StringUtil.trim(line);
    if (line.startsWith("//")) {
      continue;
    }
    int x=-1;
    while (true) {
      x=line.indexOf(s,x + 1);
      if (x == -1) {
        continue forLoop;
      }
      if (!ToolsUtil.isInsideQuotes(line,x)) {
        level+=diff;
      }
    }
  }
  return level;
}
