{
  _vocabulary=_addVocabularyAssociatedToAsset(1,true);
  AssetCategory category=AssetTestUtil.addCategory(_group.getGroupId(),_vocabulary.getVocabularyId());
  long[] selectedCategories=new long[]{category.getCategoryId()};
  Assert.assertFalse(_vocabulary.isMissingRequiredCategory(1,selectedCategories));
}
