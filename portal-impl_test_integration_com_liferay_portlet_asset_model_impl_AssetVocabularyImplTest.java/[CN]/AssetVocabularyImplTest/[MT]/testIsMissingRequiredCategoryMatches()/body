{
  _vocabulary=_addVocabularyAssociatedToAssetRendererFactory(1,true);
  AssetCategory category=AssetTestUtil.addCategory(_group.getGroupId(),_vocabulary.getVocabularyId());
  long[] selectedCategoryIds=new long[]{category.getCategoryId()};
  Assert.assertFalse(_vocabulary.isMissingRequiredCategory(1,selectedCategoryIds));
}
