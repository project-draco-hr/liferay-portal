{
  _vocabulary=_addVocabularyAssociatedToAssetRendererFactory(1,true);
  AssetCategory category1=AssetTestUtil.addCategory(_group.getGroupId(),_vocabulary.getVocabularyId());
  AssetCategory category2=AssetTestUtil.addCategory(_group.getGroupId(),_vocabulary.getVocabularyId());
  long[] selectedCategoryIds=new long[]{category1.getCategoryId(),category2.getCategoryId()};
  Assert.assertTrue(_vocabulary.hasMoreThanOneCategorySelected(selectedCategoryIds));
}
