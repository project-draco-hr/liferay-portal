{
  _vocabulary=_addVocabularyAssociatedToAsset(1,true);
  AssetCategory category1=AssetTestUtil.addCategory(_group.getGroupId(),_vocabulary.getVocabularyId());
  AssetTestUtil.addCategory(_group.getGroupId(),_vocabulary.getVocabularyId());
  long[] selectedCategoryIds=new long[]{category1.getCategoryId()};
  Assert.assertFalse(_vocabulary.hasMoreThanOneCategorySelected(selectedCategoryIds));
}
