{
  AssetVocabulary vocabulary=AssetTestUtil.addVocabulary(_group.getGroupId(),AssetCategoryConstants.ALL_CLASS_NAME_IDS,AssetCategoryConstants.ALL_CLASS_TYPE_PKS,true);
  Assert.assertTrue(vocabulary.isAssociatedToClassNameId(1));
  vocabulary=AssetTestUtil.addVocabulary(_group.getGroupId(),1,AssetCategoryConstants.ALL_CLASS_TYPE_PKS,true);
  Assert.assertTrue(vocabulary.isAssociatedToClassNameId(1));
  Assert.assertFalse(vocabulary.isAssociatedToClassNameId(2));
}
