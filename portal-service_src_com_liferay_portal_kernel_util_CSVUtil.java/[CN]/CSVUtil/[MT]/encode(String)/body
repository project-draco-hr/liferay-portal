{
  if (s == null) {
    return null;
  }
  if ((s.indexOf(StringPool.COMMA) < 0) && (s.indexOf(StringPool.QUOTE) < 0) && (s.indexOf(StringPool.NEW_LINE) < 0)&& (s.indexOf(StringPool.RETURN) < 0)) {
    return s;
  }
  StringBundler sb=new StringBundler(3);
  sb.append(StringPool.QUOTE);
  sb.append(StringUtil.replace(s,StringPool.QUOTE,StringPool.DOUBLE_QUOTE));
  sb.append(StringPool.QUOTE);
  return sb.toString();
}
