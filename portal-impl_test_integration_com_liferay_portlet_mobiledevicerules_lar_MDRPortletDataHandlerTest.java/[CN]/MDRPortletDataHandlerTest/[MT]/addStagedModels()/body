{
  Layout layout=LayoutTestUtil.addLayout(stagingGroup.getGroupId(),RandomTestUtil.randomString());
  MDRRuleGroup ruleGroup=MDRTestUtil.addRuleGroup(stagingGroup.getGroupId());
  MDRRuleGroupInstance ruleGroupInstance=MDRTestUtil.addRuleGroupInstance(stagingGroup.getGroupId(),Layout.class.getName(),layout.getPlid(),ruleGroup.getRuleGroupId());
  MDRTestUtil.addRule(ruleGroup.getRuleGroupId());
  MDRTestUtil.addAction(ruleGroupInstance.getRuleGroupInstanceId());
  ruleGroupInstance=MDRTestUtil.addRuleGroupInstance(stagingGroup.getGroupId(),ruleGroup.getRuleGroupId());
  MDRTestUtil.addAction(ruleGroupInstance.getRuleGroupInstanceId());
}
