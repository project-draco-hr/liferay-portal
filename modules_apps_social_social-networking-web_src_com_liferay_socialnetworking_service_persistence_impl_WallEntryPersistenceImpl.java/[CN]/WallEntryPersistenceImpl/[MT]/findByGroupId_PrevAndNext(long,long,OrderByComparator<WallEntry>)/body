{
  WallEntry wallEntry=findByPrimaryKey(wallEntryId);
  Session session=null;
  try {
    session=openSession();
    WallEntry[] array=new WallEntryImpl[3];
    array[0]=getByGroupId_PrevAndNext(session,wallEntry,groupId,orderByComparator,true);
    array[1]=wallEntry;
    array[2]=getByGroupId_PrevAndNext(session,wallEntry,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
