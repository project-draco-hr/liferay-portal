{
  Session session=null;
  try {
    session=openSession();
    session.delete(blogsEntry);
    session.flush();
    return blogsEntry;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(BlogsEntry.class.getName());
  }
}
