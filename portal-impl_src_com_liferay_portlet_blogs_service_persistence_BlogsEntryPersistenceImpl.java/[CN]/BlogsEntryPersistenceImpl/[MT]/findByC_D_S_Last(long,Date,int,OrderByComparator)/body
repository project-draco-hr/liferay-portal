{
  int count=countByC_D_S(companyId,displayDate,status);
  List<BlogsEntry> list=findByC_D_S(companyId,displayDate,status,count - 1,count,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler();
    msg.append("No BlogsEntry exists with the key {");
    msg.append("companyId=" + companyId);
    msg.append(", ");
    msg.append("displayDate=" + displayDate);
    msg.append(", ");
    msg.append("status=" + status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchEntryException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
