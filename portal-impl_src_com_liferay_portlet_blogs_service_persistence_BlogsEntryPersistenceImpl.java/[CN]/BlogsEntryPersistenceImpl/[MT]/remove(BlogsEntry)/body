{
  Session session=null;
  try {
    session=openSession();
    blogsEntry=(BlogsEntry)session.merge(blogsEntry);
    session.delete(blogsEntry);
    session.flush();
    return blogsEntry;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(BlogsEntry.class.getName());
  }
}
