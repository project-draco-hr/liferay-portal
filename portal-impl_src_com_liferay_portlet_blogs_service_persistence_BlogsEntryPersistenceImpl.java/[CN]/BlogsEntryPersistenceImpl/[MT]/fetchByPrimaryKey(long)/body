{
  BlogsEntry blogsEntry=(BlogsEntry)EntityCacheUtil.getResult(BlogsEntryModelImpl.ENTITY_CACHE_ENABLED,BlogsEntryImpl.class,entryId);
  if (blogsEntry == _nullBlogsEntry) {
    return null;
  }
  if (blogsEntry == null) {
    Session session=null;
    try {
      session=openSession();
      blogsEntry=(BlogsEntry)session.get(BlogsEntryImpl.class,Long.valueOf(entryId));
      if (blogsEntry != null) {
        cacheResult(blogsEntry);
      }
 else {
        EntityCacheUtil.putResult(BlogsEntryModelImpl.ENTITY_CACHE_ENABLED,BlogsEntryImpl.class,entryId,_nullBlogsEntry);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(BlogsEntryModelImpl.ENTITY_CACHE_ENABLED,BlogsEntryImpl.class,entryId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return blogsEntry;
}
