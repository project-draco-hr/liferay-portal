{
  try {
    URL url=getResourceURL(request);
    if (url != null) {
      URLConnection urlConnection=url.openConnection();
      return urlConnection.getLastModified();
    }
 else {
      return super.getLastModified(request);
    }
  }
 catch (  IOException e) {
    return super.getLastModified(request);
  }
}
