{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Blogs Permissions Page",RuntimeVariables.replace("Blogs Permissions Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Blog Entry']",RuntimeVariables.replace("Add Blog Entry"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_33_title']",RuntimeVariables.replace("Permissions Blogs Test Entry"));
  selenium.waitForElementPresent("//textarea[@id='_33_editor' and @style='display: none;']");
  assertEquals(RuntimeVariables.replace("Source"),selenium.getText("//span[.='Source']"));
  selenium.clickAt("//span[.='Source']",RuntimeVariables.replace("Source"));
  selenium.waitForVisible("//a[@class='cke_button_source cke_on']");
  selenium.waitForVisible("//td[@id='cke_contents__33_editor']/textarea");
  selenium.type("//td[@id='cke_contents__33_editor']/textarea",RuntimeVariables.replace("This is a permissions blogs test entry"));
  assertEquals(RuntimeVariables.replace("Source"),selenium.getText("//span[.='Source']"));
  selenium.clickAt("//span[.='Source']",RuntimeVariables.replace("Source"));
  selenium.waitForElementPresent("//textarea[@id='_33_editor' and @style='display: none;']");
  selenium.waitForVisible("//td[@id='cke_contents__33_editor']/iframe");
  selenium.selectFrame("//td[@id='cke_contents__33_editor']/iframe");
  selenium.waitForText("//body","This is a permissions blogs test entry");
  selenium.selectFrame("relative=top");
  selenium.clickAt("//input[@value='Publish']",RuntimeVariables.replace("Publish"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertTrue(selenium.isElementPresent("//div[@class='entry-title']/h2/a"));
}
