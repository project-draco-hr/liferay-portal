{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Announcements Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Announcements Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//div/h3","Announcements Entry Title"));
  assertEquals(RuntimeVariables.replace("General Announcements Entry Content"),selenium.getText("//p"));
  selenium.click(RuntimeVariables.replace("//td[2]/span/a"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this[\\s\\S]$"));
  assertFalse(selenium.isTextPresent("Announcements Entry Title"));
  assertFalse(selenium.isTextPresent("General Announcements Entry Content"));
}
