{
  JSONObject toolbarsJSONObject=getToolbarsJSONObject(showSource);
  String toolbarSet=(String)inputEditorTaglibAttributes.get("liferay-ui:input-editor:toolbarSet");
  String currentToolbarSet=TextFormatter.format(HtmlUtil.escapeJS(toolbarSet),TextFormatter.M);
  if (BrowserSnifferUtil.isMobile(themeDisplay.getRequest())) {
    currentToolbarSet="phone";
  }
  JSONArray currentToolbarJSONArray=toolbarsJSONObject.getJSONArray(currentToolbarSet);
  if (currentToolbarJSONArray == null) {
    currentToolbarJSONArray=toolbarsJSONObject.getJSONArray("liferay");
  }
  return currentToolbarJSONArray;
}
