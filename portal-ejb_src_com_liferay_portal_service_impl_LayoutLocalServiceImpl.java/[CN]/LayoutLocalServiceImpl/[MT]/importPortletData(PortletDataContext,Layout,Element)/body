{
  Iterator itr=parentEl.elements("portlet-data").iterator();
  while (itr.hasNext()) {
    Element el=(Element)itr.next();
    String portletId=el.attributeValue("portlet-id");
    try {
      PortletPreferences portletPreferences=PortletPreferencesUtil.findByO_L_P(layout.getOwnerId(),layout.getLayoutId(),portletId);
      String preferences=importPortletData(context,portletId,portletPreferences,el);
      if (preferences != null) {
        portletPreferences.setPreferences(preferences);
        PortletPreferencesUtil.update(portletPreferences);
      }
    }
 catch (    NoSuchPortletPreferencesException nsppe) {
    }
  }
}
