{
  try {
    UploadPortletRequest uploadReq=PortalUtil.getUploadPortletRequest(req);
    String ownerId=ParamUtil.getString(uploadReq,"ownerId");
    File file=uploadReq.getFile("importFileName");
    LayoutServiceUtil.importLayouts(ownerId,req.getParameterMap(),file);
    SessionMessages.add(req,"request_processed");
  }
 catch (  Exception e) {
    _log.error(e,e);
    SessionErrors.add(req,LayoutImportException.class.getName());
  }
}
