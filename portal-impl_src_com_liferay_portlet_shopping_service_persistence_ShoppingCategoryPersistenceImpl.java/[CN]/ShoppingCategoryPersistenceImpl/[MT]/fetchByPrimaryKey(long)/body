{
  ShoppingCategory shoppingCategory=(ShoppingCategory)EntityCacheUtil.getResult(ShoppingCategoryModelImpl.ENTITY_CACHE_ENABLED,ShoppingCategoryImpl.class,categoryId);
  if (shoppingCategory == _nullShoppingCategory) {
    return null;
  }
  if (shoppingCategory == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      shoppingCategory=(ShoppingCategory)session.get(ShoppingCategoryImpl.class,Long.valueOf(categoryId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (shoppingCategory != null) {
        cacheResult(shoppingCategory);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(ShoppingCategoryModelImpl.ENTITY_CACHE_ENABLED,ShoppingCategoryImpl.class,categoryId,_nullShoppingCategory);
      }
      closeSession(session);
    }
  }
  return shoppingCategory;
}
