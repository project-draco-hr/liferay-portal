{
  ShoppingCategory result=(ShoppingCategory)EntityCacheUtil.getResult(ShoppingCategoryModelImpl.ENTITY_CACHE_ENABLED,ShoppingCategory.class,categoryId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      ShoppingCategory shoppingCategory=(ShoppingCategory)session.get(ShoppingCategoryImpl.class,new Long(categoryId));
      if (shoppingCategory != null) {
        cacheResult(shoppingCategory);
      }
      return shoppingCategory;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (ShoppingCategory)result;
  }
}
