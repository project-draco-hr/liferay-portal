{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(shoppingCategory);
    }
 else {
      if (shoppingCategory.isNew()) {
        session.save(shoppingCategory);
      }
 else {
        session.update(shoppingCategory);
      }
    }
    session.flush();
    shoppingCategory.setNew(false);
    return shoppingCategory;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(ShoppingCategory.class.getName());
  }
}
