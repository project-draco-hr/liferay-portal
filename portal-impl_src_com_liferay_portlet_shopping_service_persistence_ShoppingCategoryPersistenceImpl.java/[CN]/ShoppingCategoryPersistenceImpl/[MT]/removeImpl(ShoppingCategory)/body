{
  Session session=null;
  try {
    session=openSession();
    if (BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(ShoppingCategoryImpl.class,shoppingCategory.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(shoppingCategory);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  ShoppingCategoryModelImpl shoppingCategoryModelImpl=(ShoppingCategoryModelImpl)shoppingCategory;
  EntityCacheUtil.removeResult(ShoppingCategoryModelImpl.ENTITY_CACHE_ENABLED,ShoppingCategory.class,shoppingCategory.getPrimaryKey());
  return shoppingCategory;
}
