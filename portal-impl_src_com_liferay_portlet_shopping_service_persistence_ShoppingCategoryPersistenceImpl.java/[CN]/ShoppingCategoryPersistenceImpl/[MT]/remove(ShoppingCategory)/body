{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(shoppingCategory);
    }
  }
  shoppingCategory=removeImpl(shoppingCategory);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(shoppingCategory);
    }
  }
  return shoppingCategory;
}
