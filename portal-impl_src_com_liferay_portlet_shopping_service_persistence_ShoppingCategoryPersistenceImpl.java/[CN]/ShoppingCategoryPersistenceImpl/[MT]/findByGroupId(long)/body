{
  Object[] finderArgs=new Object[]{new Long(groupId)};
  List<ShoppingCategory> list=(List<ShoppingCategory>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_GROUPID,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portlet.shopping.model.ShoppingCategory WHERE ");
      query.append("groupId = ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("parentCategoryId ASC, ");
      query.append("name ASC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<ShoppingCategory>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_GROUPID,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
