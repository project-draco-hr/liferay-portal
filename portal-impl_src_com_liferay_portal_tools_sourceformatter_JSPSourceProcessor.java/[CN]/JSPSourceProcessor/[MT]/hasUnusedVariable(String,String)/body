{
  if (line.contains(": ")) {
    return false;
  }
  String variableName=getVariableName(line);
  if (Validator.isNull(variableName) || variableName.equals("false") || variableName.equals("true")) {
    return false;
  }
  Set<String> includeFileNames=new HashSet<String>();
  includeFileNames.add(fileName);
  Set<String> checkedFileNames=new HashSet<String>();
  String regex="[^A-Za-z0-9_\"]" + variableName + "[^A-Za-z0-9_\"]";
  return !isJSPTermRequired(fileName,regex,"variable",includeFileNames,checkedFileNames);
}
