{
  Pattern pattern=Pattern.compile("(String redirect = ParamUtil\\.getString\\(request, " + "\"redirect\".*\\);)\n(String backURL = ParamUtil" + "\\.getString\\(request, \"backURL\", redirect\\);)");
  Matcher matcher=pattern.matcher(content);
  String newContent=content;
  while (matcher.find()) {
    newContent=StringUtil.replaceFirst(newContent,matcher.group(),matcher.group(1) + "\n\n" + matcher.group(2),matcher.start());
  }
  return newContent;
}
