{
  String backURLPatternGroup="(String backURL = " + "ParamUtil\\.getString\\(request, \"backURL\", redirect\\);)";
  String redirectPatternGroup="(String redirect = " + "ParamUtil\\.getString\\(request, \"redirect\".*\\);)";
  Pattern pattern=Pattern.compile(redirectPatternGroup + "\n" + backURLPatternGroup);
  Matcher matcher=pattern.matcher(content);
  String newContent=content;
  while (matcher.find()) {
    newContent=StringUtil.replaceFirst(newContent,matcher.group(),matcher.group(1) + "\n\n" + matcher.group(2),matcher.start());
  }
  return newContent;
}
