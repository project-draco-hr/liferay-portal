{
  Pattern pattern=Pattern.compile("String redirect = ParamUtil\\.getString\\(request, " + "\"redirect\".*\\);\nString backURL = ParamUtil\\.getString" + "\\(request, \"backURL\", redirect\\);");
  Matcher matcher=pattern.matcher(content);
  if (matcher.find()) {
    content=StringUtil.replace(content,matcher.group(),matcher.group(1) + "\n\n" + matcher.group(2),matcher.start());
  }
  return content;
}
