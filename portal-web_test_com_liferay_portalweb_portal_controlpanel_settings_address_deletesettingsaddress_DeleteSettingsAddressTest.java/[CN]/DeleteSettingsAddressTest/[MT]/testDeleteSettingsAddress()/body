{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Control Panel");
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Portal Settings",RuntimeVariables.replace("Portal Settings"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//a[@id='_130_addressesLink']","Addresses"));
  selenium.clickAt("//a[@id='_130_addressesLink']",RuntimeVariables.replace("Addresses"));
  assertEquals("123. Liferay Ln.",selenium.getValue("//input[@id='_130_addressStreet1_0']"));
  selenium.waitForElementPresent("//button[2]");
  selenium.clickAt("//button[2]",RuntimeVariables.replace("Delete Row"));
  assertFalse(selenium.isTextPresent("123. Liferay Ln."));
}
