{
  com.udojava.evalex.Expression expression=new com.udojava.evalex.Expression(expressionString);
  List<String> variableNames=_variableNamesExtractor.extract(expressionString);
  for (  String variableName : variableNames) {
    Variable variable=_variables.get(variableName);
    BigDecimal variableValue=getVariableValue(variable);
    expression.setVariable(variableName,variableValue);
  }
  return expression;
}
