{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  MBThreadFlag mbThreadFlag : mbThreadFlags) {
    EntityCacheUtil.removeResult(MBThreadFlagModelImpl.ENTITY_CACHE_ENABLED,MBThreadFlagImpl.class,mbThreadFlag.getPrimaryKey());
    clearUniqueFindersCache((MBThreadFlagModelImpl)mbThreadFlag);
  }
}
