{
  StringBundler sb=new StringBundler();
  sb.append("<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n");
  sb.append("<methodResponse>\n");
  sb.append("<params>\n");
  sb.append("<param>\n");
  sb.append(XmlRpcUtil.wrapValue(_description) + "\n");
  sb.append("</param>\n");
  sb.append("</params>\n");
  sb.append("</methodResponse>");
  return sb.toString();
}
