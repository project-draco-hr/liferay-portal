{
  if (StringPool.BLANK.equals(userUuid)) {
    return _currentUser.getUserId();
  }
  List users=UserUtil.findByUuid(userUuid);
  Iterator itr=users.iterator();
  if (itr.hasNext()) {
    User user=(User)itr.next();
    return user.getUserId();
  }
 else {
    return _currentUser.getUserId();
  }
}
