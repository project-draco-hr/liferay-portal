{
  DDMForm ddmForm=DDMStructureTestUtil.getSampleDDMForm("name",_dataType,_indexType,_repeatable,_type,new Locale[]{LocaleUtil.US},LocaleUtil.US);
  setDDMFormFieldOptions(ddmForm);
  return DDMStructureTestUtil.addStructure(_serviceContext.getScopeGroupId(),getSearchableAssetEntryStructureClassName(),ddmForm);
