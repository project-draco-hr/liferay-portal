{
  AssetEntryQuery assetEntryQuery=AssetEntryQueryTestUtil.createAssetEntryQuery(_group.getGroupId(),new String[]{getSearchableAssetEntryClassName()});
  String orderByCol1=DDMIndexerUtil.encodeName(ddmStructure.getStructureId(),"name");
  assetEntryQuery.setOrderByCol1(orderByCol1);
  assetEntryQuery.setOrderByType1("asc");
  return assetEntryQuery;
}
