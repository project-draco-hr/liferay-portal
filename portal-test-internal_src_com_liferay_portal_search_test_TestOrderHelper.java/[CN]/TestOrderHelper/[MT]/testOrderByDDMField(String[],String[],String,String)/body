{
  DDMStructure ddmStructure=addDDMStructure(dataType,type);
  DDMTemplate ddmTemplate=addDDMTemplate(ddmStructure);
  addSearchableAssetEntries(unsortedValues,ddmStructure,ddmTemplate);
  final AssetEntryQuery assetEntryQuery=createAssetEntryQuery(ddmStructure);
  final AssetRendererFactory assetRendererFactory=AssetRendererFactoryRegistryUtil.getAssetRendererFactoryByClassName(getSearchableAssetEntryClassName());
  IdempotentRetryAssert.retryAssert(3,TimeUnit.SECONDS,new Callable<Void>(){
    @Override public Void call() throws Exception {
      assertArrayEquals(sortedValues,assetEntryQuery,assetRendererFactory);
      return null;
    }
  }
);
}
