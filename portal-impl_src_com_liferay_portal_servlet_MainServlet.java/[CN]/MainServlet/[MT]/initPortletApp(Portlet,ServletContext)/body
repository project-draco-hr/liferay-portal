{
  PortletApp portletApp=portlet.getPortletApp();
  List<PortletFilter> portletFilters=portletApp.getPortletFilters();
  for (  PortletFilter portletFilter : portletFilters) {
    PortletConfig portletConfig=PortletConfigFactory.create(portlet,ctx);
    PortletContext portletCtx=portletConfig.getPortletContext();
    PortletFilterFactory.create(portletFilter,portletCtx);
  }
  List<PortletURLListener> portletURLListeners=portletApp.getPortletURLListeners();
  for (  PortletURLListener portletURLListener : portletURLListeners) {
    PortletURLListenerFactory.create(portletURLListener);
  }
}
