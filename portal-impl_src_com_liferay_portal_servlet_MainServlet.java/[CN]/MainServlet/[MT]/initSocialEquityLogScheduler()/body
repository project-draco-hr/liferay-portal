{
  _socialEquityLogSchedulerEntry=new SchedulerEntryImpl();
  _socialEquityLogSchedulerEntry.setEventListenerClass(CheckEquityLogMessageListener.class.getName());
  _socialEquityLogSchedulerEntry.setTimeUnit(TimeUnit.MINUTE);
  _socialEquityLogSchedulerEntry.setTriggerType(TriggerType.SIMPLE);
  _socialEquityLogSchedulerEntry.setTriggerValue(PropsValues.SOCIAL_EQUITY_EQUITY_LOG_CHECK_INTERVAL);
  SchedulerEngineUtil.schedule(_socialEquityLogSchedulerEntry,PortalClassLoaderUtil.getClassLoader(),JobType.PERMAENT);
}
