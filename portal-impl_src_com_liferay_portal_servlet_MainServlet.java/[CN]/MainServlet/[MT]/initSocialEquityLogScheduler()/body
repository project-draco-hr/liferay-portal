{
  SchedulerEntry socialEquityLogSchedulerEntry=new SchedulerEntryImpl();
  socialEquityLogSchedulerEntry.setEventListenerClass(CheckEquityLogMessageListener.class.getName());
  socialEquityLogSchedulerEntry.setTimeUnit(TimeUnit.MINUTE);
  socialEquityLogSchedulerEntry.setTriggerType(TriggerType.SIMPLE);
  socialEquityLogSchedulerEntry.setTriggerValue(PropsValues.SOCIAL_EQUITY_EQUITY_LOG_CHECK_INTERVAL);
  SchedulerEngineUtil.schedule(socialEquityLogSchedulerEntry,StorageType.MEMORY_CLUSTERED,PortalClassLoaderUtil.getClassLoader(),0);
}
