{
  SchedulerEntry schedulerEntry=new SchedulerEntryImpl();
  schedulerEntry.setEventListenerClass(CheckEquityLogMessageListener.class.getName());
  schedulerEntry.setTimeUnit(TimeUnit.MINUTE);
  schedulerEntry.setTriggerType(TriggerType.SIMPLE);
  schedulerEntry.setTriggerValue(PropsValues.SOCIAL_EQUITY_EQUITY_LOG_CHECK_INTERVAL);
  SchedulerEngineUtil.schedule(schedulerEntry,PortalClassLoaderUtil.getClassLoader());
}
