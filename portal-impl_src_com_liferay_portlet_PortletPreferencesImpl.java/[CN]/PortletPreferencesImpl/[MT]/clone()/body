{
  Map<String,Preference> preferencesClone=new ConcurrentHashMap<String,Preference>();
  for (  Map.Entry<String,Preference> entry : _preferences.entrySet()) {
    String key=entry.getKey();
    Preference preference=entry.getValue();
    preferencesClone.put(key,(Preference)preference.clone());
  }
  return new PortletPreferencesImpl(_companyId,_ownerId,_ownerType,_plid,_portletId,preferencesClone);
}
