{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ClassName.class,ClassName.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("classNameId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("classNameId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
