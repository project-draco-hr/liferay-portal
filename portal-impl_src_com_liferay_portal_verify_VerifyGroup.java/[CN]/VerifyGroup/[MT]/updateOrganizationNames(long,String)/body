{
  Connection con=null;
  PreparedStatement ps=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("update Group_ set name = ? where groupID = ?");
    ps.setString(1,newName);
    ps.setLong(2,groupId);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
}
