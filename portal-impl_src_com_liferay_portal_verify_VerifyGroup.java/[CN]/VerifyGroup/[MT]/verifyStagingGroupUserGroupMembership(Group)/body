{
  List<UserGroup> stagingUserGroups=UserGroupLocalServiceUtil.getGroupUserGroups(stagingGroup.getGroupId());
  if (ListUtil.isEmpty(stagingUserGroups)) {
    return;
  }
  List<UserGroup> liveUserGroups=UserGroupLocalServiceUtil.getGroupUserGroups(stagingGroup.getLiveGroupId());
  for (  UserGroup stagingUserGroup : stagingUserGroups) {
    if (!liveUserGroups.contains(stagingUserGroup)) {
      UserGroupLocalServiceUtil.addGroupUserGroup(stagingGroup.getLiveGroupId(),stagingUserGroup);
    }
  }
  UserGroupLocalServiceUtil.clearGroupUserGroups(stagingGroup.getGroupId());
}
