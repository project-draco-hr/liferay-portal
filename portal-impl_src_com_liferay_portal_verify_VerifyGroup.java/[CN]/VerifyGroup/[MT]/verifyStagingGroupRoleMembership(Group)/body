{
  List<Role> stagingRoles=RoleLocalServiceUtil.getGroupRoles(stagingGroup.getGroupId());
  if (ListUtil.isEmpty(stagingRoles)) {
    return;
  }
  List<Role> liveRoles=RoleLocalServiceUtil.getGroupRoles(stagingGroup.getLiveGroupId());
  for (  Role stagingGroupRole : stagingRoles) {
    if (!liveRoles.contains(stagingGroupRole)) {
      RoleLocalServiceUtil.addGroupRole(stagingGroup.getLiveGroupId(),stagingGroupRole);
    }
  }
  RoleLocalServiceUtil.clearGroupRoles(stagingGroup.getGroupId());
}
