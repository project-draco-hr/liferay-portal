{
  List<Organization> staged=OrganizationLocalServiceUtil.getGroupOrganizations(stagingGroup.getGroupId());
  if (staged.isEmpty()) {
    return;
  }
  List<Organization> live=OrganizationLocalServiceUtil.getGroupOrganizations(stagingGroup.getLiveGroupId());
  for (  Organization organization : staged) {
    if (!live.contains(organization)) {
      OrganizationLocalServiceUtil.addGroupOrganization(stagingGroup.getLiveGroupId(),organization);
    }
  }
  OrganizationLocalServiceUtil.clearGroupOrganizations(stagingGroup.getGroupId());
}
