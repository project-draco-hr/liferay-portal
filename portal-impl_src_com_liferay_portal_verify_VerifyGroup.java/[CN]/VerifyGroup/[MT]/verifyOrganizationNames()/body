{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select groupID, name from Group_ where name like ? and " + "name not like ?");
    ps.setString(1,"%LFR_ORGANIZATION%");
    ps.setString(2,"%LFR_ORGANIZATION ");
    rs=ps.executeQuery();
    while (rs.next()) {
      long groupId=rs.getLong("groupID");
      String newName=rs.getString("name").replaceFirst("(.*)LFR_ORGANIZATION ","") + " LFR_ORGANIZATION ";
      updateOrganizationNames(groupId,newName);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
