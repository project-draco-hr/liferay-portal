{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMTemplate.class,DDMTemplate.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("templateId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("templateId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
