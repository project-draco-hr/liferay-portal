{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMTemplate.class,DDMTemplate.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("templateId",ServiceTestUtil.nextLong()));
  List<DDMTemplate> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
