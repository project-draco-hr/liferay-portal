{
  long nodeId=ParamUtil.getLong(actionRequest,"nodeId");
  List<WikiPage> wikiPages=WikiPageLocalServiceUtil.getPages(nodeId,QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  for (  WikiPage wikiPage : wikiPages) {
    if (wikiPage.getDeletedAttachmentsFilesCount() > 0) {
      WikiPageServiceUtil.deletePageAttachmentsInTrash(nodeId,wikiPage.getTitle());
    }
  }
}
