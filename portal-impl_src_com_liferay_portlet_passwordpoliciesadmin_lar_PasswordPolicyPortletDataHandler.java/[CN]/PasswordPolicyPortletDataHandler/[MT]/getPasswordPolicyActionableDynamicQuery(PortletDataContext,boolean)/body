{
  return new PasswordPolicyExportActionableDynamicQuery(portletDataContext){
    @Override protected void addCriteria(    DynamicQuery dynamicQuery){
      portletDataContext.addDateRangeCriteria(dynamicQuery,"modifiedDate");
    }
    @Override protected void performAction(    Object object) throws PortalException {
      if (!export) {
        return;
      }
      PasswordPolicy passwordPolicy=(PasswordPolicy)object;
      StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,passwordPolicy);
    }
  }
;
}
