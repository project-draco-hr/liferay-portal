{
  if (portletDataContext.addPrimaryKey(PasswordPolicyPortletDataHandler.class,"deleteData")) {
    return portletPreferences;
  }
  List<PasswordPolicy> passwordPolicies=PasswordPolicyLocalServiceUtil.getPasswordPolicies(portletDataContext.getCompanyId());
  for (  PasswordPolicy passwordPolicy : passwordPolicies) {
    if (!passwordPolicy.isDefaultPolicy()) {
      PasswordPolicyLocalServiceUtil.deletePasswordPolicy(passwordPolicy);
    }
  }
  return portletPreferences;
}
