{
  User user=UserUtil.findByPrimaryKey(userId);
  Date now=new Date();
  validate(title);
  WikiPagePK pk=new WikiPagePK(nodeId,title,WikiPageImpl.DEFAULT_VERSION);
  WikiPage page=WikiPageUtil.create(pk);
  page.setCompanyId(user.getCompanyId());
  page.setUserId(user.getUserId());
  page.setUserName(user.getFullName());
  page.setCreateDate(now);
  page.setFormat(WikiPageImpl.DEFAULT_FORMAT);
  page.setHead(true);
  WikiPageUtil.update(page);
  addPageResources(page.getNode(),page,true,true);
  return page;
}
