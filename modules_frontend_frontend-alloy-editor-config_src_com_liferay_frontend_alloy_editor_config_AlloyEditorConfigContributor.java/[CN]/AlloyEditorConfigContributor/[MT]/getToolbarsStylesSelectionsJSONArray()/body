    toolbarsStylesSelectionsLinkJSONObject.put("buttons",JSONFactoryUtil.createJSONArray("['linkEdit']"));
    toolbarsStylesSelectionsLinkJSONObject.put("name","link");
    toolbarsStylesSelectionsLinkJSONObject.put("test","link");
