{
  Branch branch=(Branch)EntityCacheUtil.getResult(BranchModelImpl.ENTITY_CACHE_ENABLED,BranchImpl.class,branchId,this);
  if (branch == null) {
    Session session=null;
    try {
      session=openSession();
      branch=(Branch)session.get(BranchImpl.class,new Long(branchId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (branch != null) {
        cacheResult(branch);
      }
      closeSession(session);
    }
  }
  return branch;
}
