{
  Branch branch=findByPrimaryKey(branchId);
  Session session=null;
  try {
    session=openSession();
    Branch[] array=new BranchImpl[3];
    array[0]=getByG_PrevAndNext(session,branch,groupId,orderByComparator,true);
    array[1]=branch;
    array[2]=getByG_PrevAndNext(session,branch,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
