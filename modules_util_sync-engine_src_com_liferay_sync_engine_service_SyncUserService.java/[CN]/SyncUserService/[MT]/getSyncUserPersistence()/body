{
  if (_syncUserPersistence != null) {
    return _syncUserPersistence;
  }
  try {
    _syncUserPersistence=new SyncUserPersistence();
    return _syncUserPersistence;
  }
 catch (  SQLException sqle) {
    if (_logger.isDebugEnabled()) {
      _logger.debug(sqle.getMessage(),sqle);
    }
    return null;
  }
}
