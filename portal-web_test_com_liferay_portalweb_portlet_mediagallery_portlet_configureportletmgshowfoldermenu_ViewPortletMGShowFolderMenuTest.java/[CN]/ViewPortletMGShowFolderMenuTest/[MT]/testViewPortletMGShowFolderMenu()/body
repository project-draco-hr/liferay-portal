{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Media Gallery Test Page",RuntimeVariables.replace("Media Gallery Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isVisible("//div[@class='lfr-asset-summary']"));
  assertEquals(RuntimeVariables.replace("Access from Desktop"),selenium.getText("//div[@class='lfr-component lfr-menu-list lfr-menu-expanded align-right null']/ul/li[1]/a[contains(.,'Access from Desktop')]"));
}
