{
  String bitMode=OSDetector.getBitmode();
  if (bitMode.equals("32")) {
    bitMode="x86";
  }
 else {
    bitMode="x86_64";
  }
  String os="linux";
  if (OSDetector.isApple()) {
    os="macosx";
  }
 else   if (OSDetector.isWindows()) {
    os="windows";
  }
  String classifier=os + "-" + bitMode;
  GradleUtil.addDependency(project,_POSHI_RUNNER_CONFIGURATION_NAME,"org.bytedeco.javacpp-presets","opencv","2.4.9-0.9",classifier);
}
