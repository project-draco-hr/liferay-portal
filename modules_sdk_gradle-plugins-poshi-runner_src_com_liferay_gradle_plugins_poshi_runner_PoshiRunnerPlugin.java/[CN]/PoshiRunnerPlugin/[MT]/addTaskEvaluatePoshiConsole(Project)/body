{
  JavaExec javaExec=GradleUtil.addTask(project,EVALUATE_POSHI_CONSOLE_TASK_NAME,JavaExec.class);
  javaExec.setClasspath(getPoshiRunnerClasspath(project));
  javaExec.setDescription("Evaluate the console output errors.");
  javaExec.setGroup("verification");
  javaExec.setMain("com.liferay.poshi.runner.PoshiRunnerConsoleEvaluator");
  return javaExec;
}
