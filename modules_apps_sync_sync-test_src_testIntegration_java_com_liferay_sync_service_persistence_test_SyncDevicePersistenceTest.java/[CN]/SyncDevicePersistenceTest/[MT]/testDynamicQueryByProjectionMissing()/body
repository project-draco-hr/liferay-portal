{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SyncDevice.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("syncDeviceId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("syncDeviceId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
