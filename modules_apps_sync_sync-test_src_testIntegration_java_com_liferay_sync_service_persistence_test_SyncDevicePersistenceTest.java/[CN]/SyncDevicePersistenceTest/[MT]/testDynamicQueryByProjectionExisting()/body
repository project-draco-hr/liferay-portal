{
  SyncDevice newSyncDevice=addSyncDevice();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SyncDevice.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("syncDeviceId"));
  Object newSyncDeviceId=newSyncDevice.getSyncDeviceId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("syncDeviceId",new Object[]{newSyncDeviceId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Object existingSyncDeviceId=result.get(0);
  Assert.assertEquals(existingSyncDeviceId,newSyncDeviceId);
}
