{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SyncDevice.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("syncDeviceId",RandomTestUtil.nextLong()));
  List<SyncDevice> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
