{
  try {
    InitColumnProcessor processor=new InitColumnProcessor();
    VelocityContext context=new VelocityContext();
    context.put("processor",processor);
    Velocity.evaluate(context,new PrintWriter(new StringWriter()),LayoutTemplateLocalServiceImpl.class.getName(),content);
    return ListUtil.sort(processor.getColumns());
  }
 catch (  Exception e) {
    _log.error(e);
    return new ArrayList<String>();
  }
}
