{
  try {
    InitColumnProcessor processor=new InitColumnProcessor();
    Template template=TemplateManagerUtil.getTemplate(TemplateManager.VELOCITY,new StringTemplateResource(velocityTemplateId,velocityTemplateContent),TemplateContextType.STANDARD);
    template.put("processor",processor);
    template.processTemplate(new DummyWriter());
    return ListUtil.sort(processor.getColumns());
  }
 catch (  Exception e) {
    _log.error(e);
    return new ArrayList<String>();
  }
}
