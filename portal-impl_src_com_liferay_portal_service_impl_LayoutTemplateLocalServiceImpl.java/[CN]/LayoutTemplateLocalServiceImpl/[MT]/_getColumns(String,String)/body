{
  try {
    InitColumnProcessor processor=new InitColumnProcessor();
    VelocityContext velocityContext=VelocityEngineUtil.getStandardToolsContext();
    velocityContext.put("processor",processor);
    VelocityEngineUtil.mergeTemplate(velocityTemplateId,velocityTemplateContent,velocityContext,new DummyWriter());
    return ListUtil.sort(processor.getColumns());
  }
 catch (  Exception e) {
    _log.error(e);
    return new ArrayList<String>();
  }
}
