{
  Set<ObjectValuePair<String,Boolean>> layoutTemplateIdOVPs=new HashSet<ObjectValuePair<String,Boolean>>();
  if (xml == null) {
    return layoutTemplateIdOVPs;
  }
  Document document=SAXReaderUtil.read(xml,true);
  Element rootElement=document.getRootElement();
  Element standardElement=rootElement.element("standard");
  if (standardElement != null) {
    readLayoutTemplate(servletContextName,servletContext,layoutTemplateIdOVPs,standardElement,true,null,pluginPackage);
  }
  Element customElement=rootElement.element("custom");
  if (customElement != null) {
    readLayoutTemplate(servletContextName,servletContext,layoutTemplateIdOVPs,customElement,false,null,pluginPackage);
  }
  return layoutTemplateIdOVPs;
}
