{
  List<ObjectValuePair<String,Boolean>> layoutTemplateIdOVPs=new ArrayList<ObjectValuePair<String,Boolean>>();
  try {
    for (int i=0; i < xmls.length; i++) {
      Set<ObjectValuePair<String,Boolean>> curLayoutTemplateIdOVPs=_readLayoutTemplates(servletContextName,servletContext,xmls[i],pluginPackage);
      for (      ObjectValuePair<String,Boolean> layoutTemplateIdOVP : curLayoutTemplateIdOVPs) {
        if (!layoutTemplateIdOVPs.contains(layoutTemplateIdOVP)) {
          layoutTemplateIdOVPs.add(layoutTemplateIdOVP);
        }
      }
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return layoutTemplateIdOVPs;
}
