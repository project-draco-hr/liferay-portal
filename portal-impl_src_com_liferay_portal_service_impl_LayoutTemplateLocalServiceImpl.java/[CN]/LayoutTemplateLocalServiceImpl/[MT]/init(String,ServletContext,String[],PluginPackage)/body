{
  List<LayoutTemplate> layoutTemplates=new ArrayList<LayoutTemplate>();
  try {
    for (    String xml : xmls) {
      layoutTemplates.addAll(_readLayoutTemplates(servletContextName,servletContext,xml,pluginPackage));
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return ListUtil.unique(layoutTemplates);
}
