{
  List<ObjectValuePair<String,Boolean>> layoutTemplateIds=new ArrayList<ObjectValuePair<String,Boolean>>();
  try {
    for (int i=0; i < xmls.length; i++) {
      Set<ObjectValuePair<String,Boolean>> curLayoutTemplateIds=_readLayoutTemplates(servletContextName,servletContext,xmls[i],pluginPackage);
      for (      ObjectValuePair<String,Boolean> ovp : curLayoutTemplateIds) {
        if (!layoutTemplateIds.contains(ovp)) {
          layoutTemplateIds.add(ovp);
        }
      }
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return layoutTemplateIds;
}
