{
  for (  Map.Entry<String,LayoutTemplate> entry : _getThemesStandard(themeId).entrySet()) {
    VelocityEngineUtil.flushTemplate(themeId + "_STANDARD_" + entry.getValue().getLayoutTemplateId());
  }
  _getThemesStandard(themeId).clear();
  for (  Map.Entry<String,LayoutTemplate> entry : _getThemesCustom(themeId).entrySet()) {
    VelocityEngineUtil.flushTemplate(themeId + "_CUSTOM_" + entry.getValue().getLayoutTemplateId());
  }
  _getThemesCustom(themeId).clear();
}
