{
  if (standard) {
    VelocityEngineUtil.flushTemplate("null" + LayoutTemplateConstants.STANDARD_SEPARATOR + layoutTemplateId);
    _warStandard.remove(layoutTemplateId);
  }
 else {
    VelocityEngineUtil.flushTemplate("null" + LayoutTemplateConstants.CUSTOM_SEPARATOR + layoutTemplateId);
    _warCustom.remove(layoutTemplateId);
  }
}
