{
  Serializable payload=clusterRequest.getPayload();
  if (payload instanceof ClusterNodeStatus) {
    _memberJoined((ClusterNodeStatus)payload);
    return ClusterNodeResponse.createResultClusterNodeResponse(_localClusterNodeStatus.getClusterNode(),clusterRequest.getUuid(),_localClusterNodeStatus);
  }
  ClusterNodeResponse clusterNodeResponse=executeClusterRequest(clusterRequest);
  if (clusterRequest.isFireAndForget()) {
    return null;
  }
  return clusterNodeResponse;
}
