{
  InputStream is=part.getInputStream();
  try {
    ByteArrayOutputStream baos=new ByteArrayOutputStream();
    byte[] buffer=new byte[8192];
    int count=0;
    while ((count=is.read(buffer)) >= 0) {
      baos.write(buffer,0,count);
    }
    return baos.toByteArray();
  }
  finally {
    is.close();
  }
}
