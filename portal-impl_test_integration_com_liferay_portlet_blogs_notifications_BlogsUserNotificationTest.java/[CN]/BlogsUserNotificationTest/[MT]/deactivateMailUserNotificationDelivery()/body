{
  List<UserNotificationDelivery> userNotificationDeliveries=addUserNotificationDeliveries();
  if (userNotificationDeliveries.isEmpty()) {
    Assert.fail("userNotificationDelivery could not be empty");
  }
  for (  UserNotificationDelivery userNotificationDelivery : userNotificationDeliveries) {
    int deliveryType=userNotificationDelivery.getDeliveryType();
    if (deliveryType == UserNotificationDeliveryConstants.TYPE_EMAIL) {
      UserNotificationDeliveryLocalServiceUtil.updateUserNotificationDelivery(userNotificationDelivery.getUserNotificationDeliveryId(),false);
    }
  }
}
