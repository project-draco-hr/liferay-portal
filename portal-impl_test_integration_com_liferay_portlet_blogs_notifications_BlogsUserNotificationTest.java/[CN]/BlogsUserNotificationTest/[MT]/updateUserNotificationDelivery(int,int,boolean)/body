{
  for (  UserNotificationDelivery userNotificationDelivery : _userNotificationDeliveries) {
    if ((userNotificationDelivery.getDeliveryType() != deliveryType) || (userNotificationDelivery.getNotificationType() != notificationType)) {
      continue;
    }
    UserNotificationDeliveryLocalServiceUtil.updateUserNotificationDelivery(userNotificationDelivery.getUserNotificationDeliveryId(),deliver);
    return;
  }
  Assert.fail("User notification does not exist");
}
