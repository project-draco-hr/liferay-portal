{
  long diff=(startDate.getTime().getTime() - now.getTime().getTime()) / _CALENDAR_EVENT_CHECK_INTERVAL;
  if ((diff == (event.getFirstReminder() / _CALENDAR_EVENT_CHECK_INTERVAL)) || (diff == (event.getSecondReminder() / _CALENDAR_EVENT_CHECK_INTERVAL))) {
    remindUser(event,user,startDate);
  }
}
