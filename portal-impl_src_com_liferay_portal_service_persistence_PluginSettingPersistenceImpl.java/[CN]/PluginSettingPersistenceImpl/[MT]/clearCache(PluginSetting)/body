{
  EntityCacheUtil.removeResult(PluginSettingModelImpl.ENTITY_CACHE_ENABLED,PluginSettingImpl.class,pluginSetting.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_I_T,new Object[]{Long.valueOf(pluginSetting.getCompanyId()),pluginSetting.getPluginId(),pluginSetting.getPluginType()});
}
