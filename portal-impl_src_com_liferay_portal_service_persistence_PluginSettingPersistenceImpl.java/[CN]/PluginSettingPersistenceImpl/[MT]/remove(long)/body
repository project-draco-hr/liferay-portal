{
  Session session=null;
  try {
    session=openSession();
    PluginSetting pluginSetting=(PluginSetting)session.get(PluginSettingImpl.class,new Long(pluginSettingId));
    if (pluginSetting == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + pluginSettingId);
      }
      throw new NoSuchPluginSettingException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + pluginSettingId);
    }
    return pluginSettingPersistence.remove(pluginSetting);
  }
 catch (  NoSuchPluginSettingException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
