{
  int number=0;
  int numberCovered=0;
  for (  CoverageData coverageData : children.values()) {
    number+=coverageData.getNumberOfValidBranches();
    numberCovered+=coverageData.getNumberOfCoveredBranches();
  }
  if (number == 0) {
    return 1d;
  }
  return (double)numberCovered / number;
}
