{
  if (_coordinatorAddress == null) {
    _coordinatorAddress=coordinatorAddress;
    return;
  }
  Address oldCoordinatorAddress=null;
  try {
    _countDownLatch.await();
    oldCoordinatorAddress=_coordinatorAddress;
    _coordinatorAddress=coordinatorAddress;
    if (_hasDoCoordinatorAddressUpdated) {
      _executorService.execute(new CoordinatorAddressUpdatedRunnable(oldCoordinatorAddress,coordinatorAddress));
    }
  }
 catch (  InterruptedException ie) {
    _log.error("Latch opened prematurely by interruption. Dependence may " + "not be ready.");
  }
catch (  RejectedExecutionException ree) {
    _log.error("Unable to handle coordinator address update from " + oldCoordinatorAddress + " to "+ coordinatorAddress,ree);
  }
}
