{
  if (portletDataContext.addPrimaryKey(JournalPortletDataHandler.class,"deleteData")) {
    return portletPreferences;
  }
  JournalArticleLocalServiceUtil.deleteArticles(portletDataContext.getScopeGroupId());
  JournalFolderLocalServiceUtil.deleteFolders(portletDataContext.getGroupId());
  DDMTemplateLocalServiceUtil.deleteTemplates(portletDataContext.getScopeGroupId(),PortalUtil.getClassNameId(DDMStructure.class));
  DDMStructureLocalServiceUtil.deleteStructures(portletDataContext.getScopeGroupId(),PortalUtil.getClassNameId(JournalArticle.class));
  return portletPreferences;
}
