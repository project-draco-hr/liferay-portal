{
  Object value=_attributes.get(name);
  if (ServerDetector.isWebLogic()) {
    value=NonSerializableObjectHandler.getValue(value);
  }
  if (value == _nullValue) {
    return null;
  }
  if (value != null) {
    return value;
  }
  value=super.getAttribute(name);
  if (ServerDetector.isWebLogic()) {
    value=NonSerializableObjectHandler.getValue(value);
  }
  return value;
}
