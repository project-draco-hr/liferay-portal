{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalArticleLocalization.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("articleLocalizationId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("articleLocalizationId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
