{
  String userDomain=domain;
  if (username.contains("@")) {
    String[] matches=username.split("@");
    username=matches[0];
    userDomain=matches[1];
  }
  URL url=new URL(Constants.PROTOCOL + "://" + Constants.APPS_APIS_DOMAIN+ Constants.APPS_APIS_URL+ "/"+ userDomain+ "/"+ username+ "/"+ setting);
  return url;
}
