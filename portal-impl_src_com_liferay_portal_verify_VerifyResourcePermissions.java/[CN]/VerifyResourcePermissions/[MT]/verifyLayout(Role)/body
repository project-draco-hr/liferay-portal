{
  try (LoggingTimer loggingTimer=new LoggingTimer()){
    List<Layout> layouts=LayoutLocalServiceUtil.getNoPermissionLayouts(role.getRoleId());
    int total=layouts.size();
    for (int i=0; i < total; i++) {
      Layout layout=layouts.get(i);
      verifyResourcedModel(role.getCompanyId(),Layout.class.getName(),layout.getPlid(),role,0,i,total);
    }
  }
 }
