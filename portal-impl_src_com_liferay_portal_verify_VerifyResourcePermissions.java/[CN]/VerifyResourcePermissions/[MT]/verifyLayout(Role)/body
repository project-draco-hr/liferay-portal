{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select * from Layout where companyId = " + role.getCompanyId());
    rs=ps.executeQuery();
    while (rs.next()) {
      long plid=rs.getLong("plid");
      verifyModel(role.getCompanyId(),Layout.class.getName(),plid,role,0);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
