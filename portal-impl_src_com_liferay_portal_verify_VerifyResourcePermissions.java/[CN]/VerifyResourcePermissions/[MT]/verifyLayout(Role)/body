{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    String[] MODEL=_LAYOUT_MODELS[0];
    long companyId=role.getCompanyId();
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select * from " + MODEL[_INDEX_MODEL_NAME] + " where companyId = "+ companyId);
    rs=ps.executeQuery();
    while (rs.next()) {
      long primKey=rs.getLong(MODEL[_INDEX_PK_COL_NAME]);
      verifyModel(companyId,MODEL[_INDEX_NAME],primKey,role,0);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
