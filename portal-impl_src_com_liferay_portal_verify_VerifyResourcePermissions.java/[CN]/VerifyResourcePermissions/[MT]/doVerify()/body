{
  long[] companyIds=PortalInstances.getCompanyIdsBySQL();
  for (  long companyId : companyIds) {
    Role role=RoleLocalServiceUtil.getRole(companyId,RoleConstants.OWNER);
    for (    String[] model : _MODELS) {
      verifyModel(role,model[0],model[1],model[2]);
    }
    for (    ModelResource modelResource : _modelResources) {
      verifyModel(role,modelResource.getName(),modelResource.getModelName(),modelResource.getPkColumnName());
    }
    verifyLayout(role);
  }
}
