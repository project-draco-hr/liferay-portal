{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  int total=0;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select count(*) from " + modelName + " where companyId = "+ role.getCompanyId());
    rs=ps.executeQuery();
    if (rs.next()) {
      total=rs.getInt(1);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select " + pkColumnName + ", userId from "+ modelName+ " where companyId = "+ role.getCompanyId());
    rs=ps.executeQuery();
    for (int i=0; rs.next(); i++) {
      long primKey=rs.getLong(pkColumnName);
      long userId=rs.getLong("userId");
      verifyModel(role.getCompanyId(),name,primKey,role,userId,i,total);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
