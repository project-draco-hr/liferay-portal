{
  if (Validator.isNotNull(xml) && !xml.equals(_EMPTY_ROOT_NODE)) {
synchronized (_cache) {
      Map<Tuple,String> map=_cache.get(xml);
      if (map == null) {
        map=new HashMap<>();
      }
      Tuple subkey=new Tuple(useDefault,requestedLanguageId);
      map.put(subkey,value);
      _cache.put(xml,map);
    }
  }
}
