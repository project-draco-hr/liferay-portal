{
  Map<Locale,String> map=new HashMap<>();
  for (  Locale locale : LanguageUtil.getAvailableLocales()) {
    String localizedParameter=getLocalizedName(parameter,LocaleUtil.toLanguageId(locale));
    map.put(locale,ParamUtil.getString(portletRequest,localizedParameter,defaultValues.get(locale)));
  }
  return map;
}
