{
  JournalArticle latestArticle=_journalArticleLocalService.fetchLatestArticle(article.getResourcePrimKey(),new int[]{WorkflowConstants.STATUS_APPROVED,WorkflowConstants.STATUS_IN_TRASH});
  if ((latestArticle != null) && (article.getId() == latestArticle.getId())) {
    return true;
  }
  return false;
}
