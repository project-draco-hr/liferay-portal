{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      selenium.open("/web/guest/home/");
    for (int second=0; ; second++) {
      if (second >= 90) {
        fail("timeout");
      }
      try {
        if (selenium.isVisible("link=Page Comments Test Page")) {
          break;
        }
      }
 catch (      Exception e) {
      }
      Thread.sleep(1000);
    }
  selenium.clickAt("link=Page Comments Test Page",RuntimeVariables.replace("Page Comments Test Page"));
selenium.waitForPageToLoad("30000");
boolean pageComment1Present=selenium.isElementPresent("//li[4]/span/a/span");
if (!pageComment1Present) {
label=2;
continue;
}
assertEquals(RuntimeVariables.replace("Delete"),selenium.getText("//li[4]/span/a/span"));
selenium.click("//li[4]/span/a/span");
Thread.sleep(5000);
assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this[\\s\\S]$"));
case 2:
