{
  PortalCache<Serializable,Serializable> portalCache=_portalCaches.get(name);
  if (portalCache != null) {
    return portalCache;
  }
synchronized (_cacheManager) {
    portalCache=_portalCaches.get(name);
    if (portalCache == null) {
      if (!_cacheManager.cacheExists(name)) {
        return null;
      }
      Cache cache=_cacheManager.getCache(name);
      portalCache=new EhcachePortalCache<Serializable,Serializable>(cache);
      _portalCaches.put(name,portalCache);
    }
  }
  return portalCache;
}
