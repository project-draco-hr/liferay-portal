{
  PortalCache<Serializable,Serializable> portalCache=_hibernatePortalCacheManager.getCache(regionName);
  Ehcache ehcache=EhcacheUnwrapUtil.getEhcache(portalCache);
  if (!(ehcache instanceof ModifiableEhcacheWrapper)) {
    Ehcache modifiableEhcacheWrapper=new ModifiableEhcacheWrapper(ehcache);
    manager.replaceCacheWithDecoratedCache(ehcache,modifiableEhcacheWrapper);
  }
}
