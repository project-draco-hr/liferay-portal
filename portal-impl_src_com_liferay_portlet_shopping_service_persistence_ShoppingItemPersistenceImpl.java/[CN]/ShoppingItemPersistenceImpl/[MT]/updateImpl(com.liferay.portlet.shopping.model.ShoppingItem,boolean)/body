{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,shoppingItem,merge);
    shoppingItem.setNew(false);
    return shoppingItem;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ShoppingItem.class.getName());
  }
}
