{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(shoppingItem);
    }
  }
  shoppingItem=removeImpl(shoppingItem);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(shoppingItem);
    }
  }
  return shoppingItem;
}
