{
  shoppingItem=toUnwrappedModel(shoppingItem);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,shoppingItem);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(shoppingItem);
  return shoppingItem;
}
