{
  String finderClassName=ShoppingItem.class.getName();
  String finderMethodName="fetchByC_S";
  Object[] finderArgs=new Object[]{new Long(companyId),sku};
  Object result=FinderCache.getResult(finderClassName,finderMethodName,finderArgs);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringMaker query=new StringMaker();
      query.append("FROM com.liferay.portlet.shopping.model.ShoppingItem WHERE ");
      query.append("companyId = ?");
      query.append(" AND ");
      if (sku == null) {
        query.append("sku IS NULL");
      }
 else {
        query.append("sku = ?");
      }
      query.append(" ");
      query.append("ORDER BY ");
      query.append("itemId ASC");
      Query q=session.createQuery(query.toString());
      int queryPos=0;
      q.setLong(queryPos++,companyId);
      if (sku != null) {
        q.setString(queryPos++,sku);
      }
      List list=q.list();
      if (list.size() == 0) {
        return null;
      }
      ShoppingItem shoppingItem=(ShoppingItem)list.get(0);
      FinderCache.putResult(finderClassName,finderMethodName,finderArgs,shoppingItem);
      return shoppingItem;
    }
 catch (    Exception e) {
      throw HibernateUtil.processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (ShoppingItem)result;
  }
}
