{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Words Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Words Test Page",RuntimeVariables.replace("Words Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("More\u2026"),selenium.getText("//a[@id='_145_addApplication']"));
  selenium.clickAt("//a[@id='_145_addApplication']",RuntimeVariables.replace("More\u2026"));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//input[@id='layout_configuration_content']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.typeKeys("//input[@id='layout_configuration_content']",RuntimeVariables.replace("w"));
  selenium.saveScreenShotAndSource();
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@title='Words']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  assertFalse(selenium.isVisible("//div[@title='Words']/p/a"));
}
