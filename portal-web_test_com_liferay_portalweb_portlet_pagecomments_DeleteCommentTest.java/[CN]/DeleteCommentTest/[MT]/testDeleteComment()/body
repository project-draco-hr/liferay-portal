{
  selenium.click("link=Delete");
  assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this[\\s\\S]$"));
  verifyTrue(selenium.isTextPresent("Your request processed successfully."));
  verifyFalse(selenium.isTextPresent("This is a test page comment!"));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//a[@id=\"my-community-private-pages\"]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
