{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select distinct(groupId) from SocialActivity");
    rs=ps.executeQuery();
    while (rs.next()) {
      long groupId=rs.getLong("groupId");
      updateGroupId(groupId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
}
