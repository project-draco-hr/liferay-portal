{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.MEMBERSHIP_POLICY_SITES);
  }
  ClassLoader classLoader=ClassLoaderUtil.getPortalClassLoader();
  _originalMembershipPolicy=(SiteMembershipPolicy)InstanceFactory.newInstance(classLoader,PropsValues.MEMBERSHIP_POLICY_SITES);
  _membershipPolicy=_originalMembershipPolicy;
}
