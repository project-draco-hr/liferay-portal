{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.MEMBERSHIP_POLICY_SITES);
  }
  ClassLoader classLoader=ClassLoaderUtil.getPortalClassLoader();
  _originalSiteMembershipPolicy=(SiteMembershipPolicy)InstanceFactory.newInstance(classLoader,PropsValues.MEMBERSHIP_POLICY_SITES);
  _siteMembershipPolicy=_originalSiteMembershipPolicy;
}
