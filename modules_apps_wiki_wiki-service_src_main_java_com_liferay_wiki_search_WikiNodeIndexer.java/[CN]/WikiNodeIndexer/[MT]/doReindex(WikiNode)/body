{
  Document document=getDocument(wikiNode);
  if (!wikiNode.isInTrash()) {
    SearchEngineHelperUtil.deleteDocument(getSearchEngineId(),wikiNode.getCompanyId(),document.get(Field.UID),isCommitImmediately());
    return;
  }
  SearchEngineHelperUtil.updateDocument(getSearchEngineId(),wikiNode.getCompanyId(),document,isCommitImmediately());
}
