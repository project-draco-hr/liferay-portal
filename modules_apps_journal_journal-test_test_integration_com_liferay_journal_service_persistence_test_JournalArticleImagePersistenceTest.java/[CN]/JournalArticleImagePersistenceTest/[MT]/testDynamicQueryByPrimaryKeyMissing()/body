{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalArticleImage.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("articleImageId",RandomTestUtil.nextLong()));
  List<JournalArticleImage> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
