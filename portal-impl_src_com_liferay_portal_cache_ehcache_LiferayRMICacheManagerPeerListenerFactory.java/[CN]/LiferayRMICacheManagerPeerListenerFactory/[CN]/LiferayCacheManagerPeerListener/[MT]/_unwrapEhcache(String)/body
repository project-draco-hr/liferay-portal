{
  Ehcache ehcache=_cacheManager.getEhcache(cacheName);
  if (!(ehcache instanceof LiferayCacheDecorator)) {
    return;
  }
  RegisteredEventListeners registeredEventListeners=ehcache.getCacheEventNotificationService();
  LiferayCacheDecorator liferayCacheDecorator=(LiferayCacheDecorator)ehcache;
  _cacheManager.replaceCacheWithDecoratedCache(liferayCacheDecorator,liferayCacheDecorator.getUnderlyingCache());
  registeredEventListeners.unregisterListener(_cacheReplicator);
}
