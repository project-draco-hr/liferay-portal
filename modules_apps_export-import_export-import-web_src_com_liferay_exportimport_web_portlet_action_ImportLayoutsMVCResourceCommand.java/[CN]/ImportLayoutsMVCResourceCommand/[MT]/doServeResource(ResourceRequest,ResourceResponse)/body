{
  String cmd=ParamUtil.getString(resourceRequest,Constants.CMD);
  PortletContext portletContext=getPortletConfig(resourceRequest).getPortletContext();
  PortletRequestDispatcher portletRequestDispatcher=null;
  if (cmd.equals(Constants.IMPORT)) {
    portletRequestDispatcher=portletContext.getRequestDispatcher("/import_layouts_processes.jsp");
  }
 else {
    portletRequestDispatcher=portletContext.getRequestDispatcher("/import_layouts_resources.jsp");
  }
  portletRequestDispatcher.include(resourceRequest,resourceResponse);
}
