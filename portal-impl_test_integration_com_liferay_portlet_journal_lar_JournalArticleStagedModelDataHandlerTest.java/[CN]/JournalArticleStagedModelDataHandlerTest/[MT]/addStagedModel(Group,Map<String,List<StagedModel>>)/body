{
  List<StagedModel> folderDependentStagedModels=dependentStagedModelsMap.get(JournalFolder.class.getSimpleName());
  JournalFolder folder=(JournalFolder)folderDependentStagedModels.get(0);
  List<StagedModel> ddmStructureDependentStagedModels=dependentStagedModelsMap.get(DDMStructure.class.getSimpleName());
  DDMStructure ddmStructure=(DDMStructure)ddmStructureDependentStagedModels.get(0);
  List<StagedModel> ddmTemplateDependentStagedModels=dependentStagedModelsMap.get(DDMTemplate.class.getSimpleName());
  DDMTemplate ddmTemplate=(DDMTemplate)ddmTemplateDependentStagedModels.get(0);
  return JournalTestUtil.addArticleWithXMLContent(group.getGroupId(),folder.getFolderId(),JournalArticleConstants.CLASSNAME_ID_DEFAULT,DDMStructureTestUtil.getSampleStructuredContent(),ddmStructure.getStructureKey(),ddmTemplate.getTemplateKey());
}
