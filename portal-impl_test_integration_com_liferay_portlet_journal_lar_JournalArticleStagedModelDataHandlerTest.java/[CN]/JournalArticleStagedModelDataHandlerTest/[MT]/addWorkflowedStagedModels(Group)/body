{
  List<StagedModel> stagedModels=new ArrayList<StagedModel>();
  stagedModels.add(JournalTestUtil.addArticleWithWorkflow(group.getGroupId(),true));
  stagedModels.add(JournalTestUtil.addArticleWithWorkflow(group.getGroupId(),false));
  JournalArticle expiredArticle=JournalTestUtil.addArticleWithWorkflow(group.getGroupId(),true);
  expiredArticle=JournalArticleLocalServiceUtil.expireArticle(TestPropsValues.getUserId(),group.getGroupId(),expiredArticle.getArticleId(),expiredArticle.getVersion(),expiredArticle.getUrlTitle(),ServiceTestUtil.getServiceContext());
  stagedModels.add(expiredArticle);
  return stagedModels;
}
