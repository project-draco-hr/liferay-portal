{
  List<DLFileEntry> dlFileEntries=DLFileEntryLocalServiceUtil.getDLFileEntriesByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<DLFileEntry>());
  List<FileEntry> fileEntries=new ArrayList<>();
  for (  DLFileEntry dlFileEntry : dlFileEntries) {
    fileEntries.add(new LiferayFileEntry(dlFileEntry));
  }
  return fileEntries;
}
