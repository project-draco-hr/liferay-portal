{
  String name=queryRule.getName();
  if (name.equals("assetTags")) {
    AssetTagLocalServiceUtil.checkTags(userId,groupId,queryRule.getValues());
  }
  if (queryRules.isEmpty()) {
    return;
  }
  for (  AssetQueryRule addedAssetQueryRule : queryRules) {
    if (addedAssetQueryRule.equals(queryRule)) {
      throw new DuplicateAssetQueryRuleException(queryRule.isAndOperator(),queryRule.isContains(),queryRule.getName());
    }
  }
}
