{
  Collection<PortletStatistics> portletsStats=_portletStatisticsByPortletId.values();
  if (portletsStats.isEmpty()) {
    return Collections.EMPTY_LIST;
  }
  List<RequestStatistics> actionStatistics=new ArrayList<RequestStatistics>(portletsStats.size());
  for (  PortletStatistics portletStats : portletsStats) {
    actionStatistics.add(portletStats.getActionStatistics());
  }
  return actionStatistics;
}
