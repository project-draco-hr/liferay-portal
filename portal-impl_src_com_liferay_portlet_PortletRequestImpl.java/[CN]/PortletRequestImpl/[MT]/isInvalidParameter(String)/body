{
  if (Validator.isNull(name) || name.startsWith(PortletQName.PUBLIC_RENDER_PARAMETER_NAMESPACE) || name.startsWith(PortletQName.REMOVE_PUBLIC_RENDER_PARAMETER_NAMESPACE)|| PortalUtil.isReservedParameter(name)|| (_strutsBasedPortlet && _STRUTS_PORTLET_IGNORED_PARAMETERS_REGEXP.matcher(name).matches())) {
    return true;
  }
 else {
    return false;
  }
}
