{
  if (name == null) {
    throw new IllegalArgumentException();
  }
  if (name.equals(PortletRequest.CCPP_PROFILE)) {
    return getCCPPProfile();
  }
 else   if (name.equals(PortletRequest.USER_INFO)) {
    Object value=getUserInfo();
    if (value != null) {
      return value;
    }
  }
  return _req.getAttribute(name);
}
