{
  String[] values=request.getParameterValues(name);
  QName qName=PortletQNameUtil.getQName(name);
  if (qName != null) {
    PublicRenderParameter publicRenderParameter=_portlet.getPublicRenderParameter(qName.getNamespaceURI(),qName.getLocalPart());
    if (publicRenderParameter != null) {
      if (name.startsWith(PortletQName.PUBLIC_RENDER_PARAMETER_NAMESPACE)) {
        _publicRenderParameters.put(PortletQNameUtil.getKey(qName),values);
      }
 else {
        _publicRenderParameters.remove(PortletQNameUtil.getKey(qName));
      }
    }
    return null;
  }
  if (themeDisplay.isLifecycleRender()) {
    renderParameters.put(name,values);
  }
  return values;
}
