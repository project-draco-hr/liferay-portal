{
  Enumeration<String> names=req.getParameterNames();
  while (names.hasMoreElements()) {
    String name=names.nextElement();
    QName qName=QNameUtil.getQName(name);
    if (qName == null) {
      continue;
    }
    PublicRenderParameter publicRenderParameter=_portlet.getPublicRenderParameter(qName.getNamespaceURI(),qName.getLocalPart());
    if (publicRenderParameter != null) {
      renderParameters.put(publicRenderParameter.getIdentifier(),req.getParameterValues(name));
    }
  }
}
