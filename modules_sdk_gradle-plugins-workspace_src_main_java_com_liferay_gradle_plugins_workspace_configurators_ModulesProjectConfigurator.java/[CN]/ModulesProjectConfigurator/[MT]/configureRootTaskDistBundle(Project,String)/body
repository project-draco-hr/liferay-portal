{
  CopySpec copySpec=(CopySpec)GradleUtil.getTask(project.getRootProject(),rootTaskName);
  copySpec.into("deploy",new Closure<Void>(null){
    @SuppressWarnings("unused") public void doCall(    CopySourceSpec copySourceSpec){
      Task jarTask=GradleUtil.getTask(project,JavaPlugin.JAR_TASK_NAME);
      copySourceSpec.from(jarTask);
    }
  }
);
}
