{
  String pk=_instance._getByC_U_C_C(companyId,userId,className,classPK);
  if (_log.isInfoEnabled()) {
    _log.info("Get C_U_C_C" + " " + companyId.toString() + " "+ userId.toString()+ " "+ className.toString()+ " "+ classPK.toString()+ " is "+ ((pk == null) ? "NOT " : "")+ "in cache");
  }
  return pk;
}
