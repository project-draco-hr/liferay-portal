{
  if (!_cacheable) {
    return null;
  }
 else {
    String pk=null;
    String key=_encodeKeyC_U_C_C(companyId,userId,className,classPK);
    try {
      pk=(String)_cache.getFromCache(key);
    }
 catch (    NeedsRefreshException nfe) {
    }
 finally {
      if (pk == null) {
        _cache.cancelUpdate(key);
      }
    }
    return pk;
  }
}
