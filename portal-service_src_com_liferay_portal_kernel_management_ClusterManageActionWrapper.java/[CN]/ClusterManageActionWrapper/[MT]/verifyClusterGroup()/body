{
  List<ClusterNode> clusterNodes=ClusterExecutorUtil.getClusterNodes();
  String[] requiredClusterNodesIds=_clusterGroup.getClusterNodeIdsArray();
  for (  String requiredClusterNodeId : requiredClusterNodesIds) {
    for (    ClusterNode clusterNode : clusterNodes) {
      if (clusterNode.getClusterNodeId().equals(requiredClusterNodeId)) {
        clusterNodes.remove(clusterNode);
        continue;
      }
    }
    throw new ManageActionException("ClusterNode:" + requiredClusterNodeId + " is not available currently");
  }
}
