{
  List<ClusterNode> clusterNodes=ClusterExecutorUtil.getClusterNodes();
  String[] requiredClusterNodesIds=_clusterGroup.getClusterNodeIdsArray();
  Required:   for (  String requiredClusterNodeId : requiredClusterNodesIds) {
    Iterator<ClusterNode> iterator=clusterNodes.iterator();
    while (iterator.hasNext()) {
      ClusterNode clusterNode=iterator.next();
      String clusterNodeId=clusterNode.getClusterNodeId();
      if (clusterNodeId.equals(requiredClusterNodeId)) {
        clusterNodes.remove(clusterNode);
        continue Required;
      }
    }
    throw new ManageActionException("Cluster node " + requiredClusterNodeId + " is not available");
  }
}
