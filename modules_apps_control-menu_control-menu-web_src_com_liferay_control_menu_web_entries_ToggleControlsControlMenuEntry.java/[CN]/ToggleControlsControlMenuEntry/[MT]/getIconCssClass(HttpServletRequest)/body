{
  String toggleControlsState=GetterUtil.getString(SessionClicks.get(request,"com.liferay.frontend.js.web_toggleControls","visible"));
  String stateCss=null;
  if (toggleControlsState.equals("visible")) {
    stateCss="icon-eye-open";
  }
 else {
    stateCss="icon-eye-close";
  }
  return "toggle-controls controls-state-icon " + stateCss;
}
