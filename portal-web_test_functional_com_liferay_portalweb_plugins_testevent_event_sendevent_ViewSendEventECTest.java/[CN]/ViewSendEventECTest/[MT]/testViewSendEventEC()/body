{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Event Consumer Test Page",RuntimeVariables.replace("Event Consumer Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("PASSED"),selenium.getText("//div[@class='portlet-body']"));
}
