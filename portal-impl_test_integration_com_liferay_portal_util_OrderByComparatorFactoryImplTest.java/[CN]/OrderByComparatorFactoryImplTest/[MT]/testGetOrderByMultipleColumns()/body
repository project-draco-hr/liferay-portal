{
  OrderByComparator obc=OrderByComparatorFactoryUtil.create(EmailAddressModelImpl.TABLE_NAME,"address",true,"createDate",false);
  assertEquals("EmailAddress.address ASC,EmailAddress.createDate DESC",obc.getOrderBy());
  obc=OrderByComparatorFactoryUtil.create(EmailAddressModelImpl.TABLE_NAME,"address",false,"createDate",true);
  assertEquals("EmailAddress.address DESC,EmailAddress.createDate ASC",obc.getOrderBy());
}
