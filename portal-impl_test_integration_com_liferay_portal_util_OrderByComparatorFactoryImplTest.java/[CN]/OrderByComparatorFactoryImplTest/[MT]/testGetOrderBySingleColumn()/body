{
  OrderByComparator obc=OrderByComparatorFactoryUtil.create(EmailAddressModelImpl.TABLE_NAME,"address",true);
  assertEquals("EmailAddress.address ASC",obc.getOrderBy());
  obc=OrderByComparatorFactoryUtil.create(EmailAddressModelImpl.TABLE_NAME,"address",false);
  assertEquals("EmailAddress.address DESC",obc.getOrderBy());
}
