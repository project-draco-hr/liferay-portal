{
  JournalImageContent journalImageContent=(JournalImageContent)binaryContent;
  JournalArticleImage journalImage=journalImageContent.getJournalImage();
  try {
    OptionalJournalImageCriteria journalCriteria=(OptionalJournalImageCriteria)criteria;
    String finder=_getValue(journalCriteria,OptionalJournalImageCriteria.FINDER);
    if (OptionalJournalImageCriteria.REMOVE_BY_ID.equals(finder)) {
      _deleteArticleImage(journalImage.getArticleImageId());
    }
 else     if (OptionalJournalImageCriteria.REMOVE_BY_G_A_V_E_L.equals(finder)) {
      _deleteArticleImage(journalImage.getGroupId(),journalImage.getArticleId(),journalImage.getVersion(),journalImage.getElName(),journalImage.getLanguageId());
    }
  }
 catch (  SystemException ex) {
    throw new CMSException(ex.getMessage(),ex);
  }
}
