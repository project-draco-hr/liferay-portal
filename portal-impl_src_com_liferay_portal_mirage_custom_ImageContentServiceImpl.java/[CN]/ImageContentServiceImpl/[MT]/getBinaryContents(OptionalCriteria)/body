{
  List<BinaryContent> binaryContents=new ArrayList<BinaryContent>();
  try {
    OptionalJournalImageCriteria journalCriteria=(OptionalJournalImageCriteria)criteria;
    String groupId=_getValue(journalCriteria,OptionalJournalImageCriteria.GROUP_ID);
    List<JournalArticleImage> journalImages=_getArticleImages(Long.parseLong(groupId));
    _populateBinaryContents(binaryContents,journalImages);
    return binaryContents;
  }
 catch (  SystemException ex) {
    throw new CMSException(ex.getMessage(),ex);
  }
}
