{
  Template template=TemplateManagerUtil.getTemplate(TemplateConstants.LANG_TYPE_SOY,_templateResource,false);
  template.put(TemplateConstants.NAMESPACE,"ddm.pages");
  Map<String,String> renderedDDMFormFieldsMap=getRenderedDDMFormFieldsMap(ddmForm,ddmFormRenderingContext);
  List<Object> pages=getPages(ddmFormLayout,renderedDDMFormFieldsMap,ddmFormRenderingContext.getLocale());
  template.put("pages",pages);
  return render(template);
}
