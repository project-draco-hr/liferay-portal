{
  long actionId=ParamUtil.getLong(resourceRequest,"actionId");
  MDRAction action=MDRActionServiceUtil.fetchAction(actionId);
  resourceRequest.setAttribute(MDRWebKeys.MOBILE_DEVICE_RULES_RULE_GROUP_ACTION,action);
  String type=ParamUtil.getString(resourceRequest,"type");
  ActionUtil.includeEditorJSP(getPortletConfig(resourceRequest),resourceRequest,resourceResponse,ActionUtil.getActionEditorJSP(type));
}
