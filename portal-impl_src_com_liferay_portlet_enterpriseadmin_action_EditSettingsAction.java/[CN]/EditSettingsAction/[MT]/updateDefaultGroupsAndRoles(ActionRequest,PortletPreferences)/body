{
  String defaultGroupNames=ParamUtil.getString(req,"defaultGroupNames");
  String defaultRoleNames=ParamUtil.getString(req,"defaultRoleNames");
  String defaultUserGroupNames=ParamUtil.getString(req,"defaultUserGroupNames");
  prefs.setValue(PropsUtil.ADMIN_DEFAULT_GROUP_NAMES,defaultGroupNames);
  prefs.setValue(PropsUtil.ADMIN_DEFAULT_ROLE_NAMES,defaultRoleNames);
  prefs.setValue(PropsUtil.ADMIN_DEFAULT_USER_GROUP_NAMES,defaultUserGroupNames);
}
