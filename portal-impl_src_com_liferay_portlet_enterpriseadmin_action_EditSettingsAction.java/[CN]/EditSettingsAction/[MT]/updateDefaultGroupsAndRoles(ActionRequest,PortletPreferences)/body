{
  String defaultGroupNames=ParamUtil.getString(actionRequest,"defaultGroupNames");
  String defaultRoleNames=ParamUtil.getString(actionRequest,"defaultRoleNames");
  String defaultUserGroupNames=ParamUtil.getString(actionRequest,"defaultUserGroupNames");
  preferences.setValue(PropsKeys.ADMIN_DEFAULT_GROUP_NAMES,defaultGroupNames);
  preferences.setValue(PropsKeys.ADMIN_DEFAULT_ROLE_NAMES,defaultRoleNames);
  preferences.setValue(PropsKeys.ADMIN_DEFAULT_USER_GROUP_NAMES,defaultUserGroupNames);
}
