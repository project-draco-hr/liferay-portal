{
  boolean enabled=ParamUtil.getBoolean(actionRequest,"enabled");
  boolean importFromLdap=ParamUtil.getBoolean(actionRequest,"importFromLdap");
  String userHeader=ParamUtil.getString(actionRequest,"userHeader");
  prefs.setValue(PropsKeys.SITEMINDER_AUTH_ENABLED,String.valueOf(enabled));
  prefs.setValue(PropsKeys.SITEMINDER_IMPORT_FROM_LDAP,String.valueOf(importFromLdap));
  prefs.setValue(PropsKeys.SITEMINDER_USER_HEADER,userHeader);
  prefs.store();
}
