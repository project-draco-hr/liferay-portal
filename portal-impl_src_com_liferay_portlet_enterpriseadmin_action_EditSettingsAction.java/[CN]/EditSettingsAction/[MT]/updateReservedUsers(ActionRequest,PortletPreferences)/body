{
  String reservedScreenNames=ParamUtil.getString(req,"reservedScreenNames");
  String reservedEmailAddresses=ParamUtil.getString(req,"reservedEmailAddresses");
  prefs.setValue(PropsKeys.ADMIN_RESERVED_SCREEN_NAMES,reservedScreenNames);
  prefs.setValue(PropsKeys.ADMIN_RESERVED_EMAIL_ADDRESSES,reservedEmailAddresses);
}
