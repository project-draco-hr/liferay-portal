{
  String reservedScreenNames=ParamUtil.getString(actionRequest,"reservedScreenNames");
  String reservedEmailAddresses=ParamUtil.getString(actionRequest,"reservedEmailAddresses");
  preferences.setValue(PropsKeys.ADMIN_RESERVED_SCREEN_NAMES,reservedScreenNames);
  preferences.setValue(PropsKeys.ADMIN_RESERVED_EMAIL_ADDRESSES,reservedEmailAddresses);
  preferences.store();
}
