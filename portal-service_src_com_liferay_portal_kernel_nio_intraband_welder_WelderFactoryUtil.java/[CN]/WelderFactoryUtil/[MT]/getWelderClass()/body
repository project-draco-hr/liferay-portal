{
  if (Validator.isNotNull(INTRABAND_WELDER_IMPL)) {
    try {
      return (Class<? extends Welder>)Class.forName(INTRABAND_WELDER_IMPL);
    }
 catch (    ClassNotFoundException cnfe) {
      throw new RuntimeException("Unable to load class with name " + INTRABAND_WELDER_IMPL,cnfe);
    }
  }
 else {
    if (!OSDetector.isWindows() && FIFOUtil.isFIFOSupported()) {
      return FIFOWelder.class;
    }
 else {
      return SocketWelder.class;
    }
  }
}
