{
  StringBundler sb=new StringBundler(5);
  sb.append("SELECT * FROM someTable WHERE someColumn = ");
  sb.append(getDB().getTemplateFalse());
  sb.append(" AND anotherColumn = ");
  sb.append(getDB().getTemplateTrue());
  sb.append(StringPool.NEW_LINE);
  Assert.assertEquals(sb.toString(),buildSQL(QUERY_WITH_BOOLEAN_LITERAL));
  Assert.assertEquals(QUERY_WITH_BOOLEAN_PATTERN + StringPool.NEW_LINE,buildSQL(QUERY_WITH_BOOLEAN_PATTERN));
}
