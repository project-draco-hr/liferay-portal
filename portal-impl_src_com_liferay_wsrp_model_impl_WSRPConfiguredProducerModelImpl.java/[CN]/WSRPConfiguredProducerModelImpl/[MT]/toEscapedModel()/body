{
  if (isEscapedModel()) {
    return (WSRPConfiguredProducer)this;
  }
 else {
    WSRPConfiguredProducer model=new WSRPConfiguredProducerImpl();
    model.setNew(isNew());
    model.setEscapedModel(true);
    model.setConfiguredProducerId(getConfiguredProducerId());
    model.setName(HtmlUtil.escape(getName()));
    model=(WSRPConfiguredProducer)Proxy.newProxyInstance(WSRPConfiguredProducer.class.getClassLoader(),new Class[]{WSRPConfiguredProducer.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
