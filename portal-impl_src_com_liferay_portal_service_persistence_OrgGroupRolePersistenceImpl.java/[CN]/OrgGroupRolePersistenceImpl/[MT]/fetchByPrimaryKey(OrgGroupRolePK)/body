    try {
      session=openSession();
      orgGroupRole=(OrgGroupRole)session.get(OrgGroupRoleImpl.class,orgGroupRolePK);
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
