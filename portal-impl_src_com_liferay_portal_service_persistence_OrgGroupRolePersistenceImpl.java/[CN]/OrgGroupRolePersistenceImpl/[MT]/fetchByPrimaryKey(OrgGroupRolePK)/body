{
  OrgGroupRole orgGroupRole=(OrgGroupRole)EntityCacheUtil.getResult(OrgGroupRoleModelImpl.ENTITY_CACHE_ENABLED,OrgGroupRoleImpl.class,orgGroupRolePK,this);
  if (orgGroupRole == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (orgGroupRole == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      orgGroupRole=(OrgGroupRole)session.get(OrgGroupRoleImpl.class,orgGroupRolePK);
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (orgGroupRole == null)) {
        EntityCacheUtil.putResult(OrgGroupRoleModelImpl.ENTITY_CACHE_ENABLED,OrgGroupRoleImpl.class,orgGroupRolePK,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(orgGroupRole);
      }
      closeSession(session);
    }
  }
  return orgGroupRole;
}
