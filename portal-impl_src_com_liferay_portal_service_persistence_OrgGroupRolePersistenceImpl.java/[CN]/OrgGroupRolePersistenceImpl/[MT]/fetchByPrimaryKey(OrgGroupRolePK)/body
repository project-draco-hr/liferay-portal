{
  OrgGroupRole orgGroupRole=(OrgGroupRole)EntityCacheUtil.getResult(OrgGroupRoleModelImpl.ENTITY_CACHE_ENABLED,OrgGroupRoleImpl.class,orgGroupRolePK);
