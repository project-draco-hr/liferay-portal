{
  OrgGroupRole orgGroupRole=(OrgGroupRole)EntityCacheUtil.getResult(OrgGroupRoleModelImpl.ENTITY_CACHE_ENABLED,OrgGroupRoleImpl.class,orgGroupRolePK,this);
  if (orgGroupRole == null) {
    Session session=null;
    try {
      session=openSession();
      orgGroupRole=(OrgGroupRole)session.get(OrgGroupRoleImpl.class,orgGroupRolePK);
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (orgGroupRole != null) {
        cacheResult(orgGroupRole);
      }
      closeSession(session);
    }
  }
  return orgGroupRole;
}
