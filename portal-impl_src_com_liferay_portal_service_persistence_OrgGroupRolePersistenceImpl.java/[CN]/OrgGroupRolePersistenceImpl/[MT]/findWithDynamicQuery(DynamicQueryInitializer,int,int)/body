{
  Session session=null;
  try {
    session=openSession();
    DynamicQuery query=queryInitializer.initialize(session);
    query.setLimit(begin,end);
    return query.list();
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
