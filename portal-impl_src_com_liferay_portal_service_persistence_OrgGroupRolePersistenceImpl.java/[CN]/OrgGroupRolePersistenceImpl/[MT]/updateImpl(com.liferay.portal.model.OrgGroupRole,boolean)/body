{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,orgGroupRole,merge);
    orgGroupRole.setNew(false);
    return orgGroupRole;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(OrgGroupRole.class.getName());
  }
}
