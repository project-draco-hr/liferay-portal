{
  DDMStorageLink ddmStorageLink=findByPrimaryKey(storageLinkId);
  Session session=null;
  try {
    session=openSession();
    DDMStorageLink[] array=new DDMStorageLinkImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,ddmStorageLink,uuid,companyId,orderByComparator,true);
    array[1]=ddmStorageLink;
    array[2]=getByUuid_C_PrevAndNext(session,ddmStorageLink,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
