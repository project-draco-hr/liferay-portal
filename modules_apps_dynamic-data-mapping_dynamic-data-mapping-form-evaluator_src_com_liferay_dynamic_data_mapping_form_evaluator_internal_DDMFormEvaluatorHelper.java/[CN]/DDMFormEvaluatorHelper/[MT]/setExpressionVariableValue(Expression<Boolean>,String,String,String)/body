{
  if (variableType.equals("boolean")) {
    expression.setBooleanVariableValue(variableName,Boolean.valueOf(variableValue));
  }
 else   if (variableType.equals("integer")) {
    expression.setIntegerVariableValue(variableName,Integer.valueOf(variableValue));
  }
 else   if (variableType.equals("string")) {
    expression.setStringVariableValue(variableName,variableValue);
  }
}
