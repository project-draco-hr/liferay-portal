{
  for (  MicroblogsEntry microblogsEntry : microblogsEntries) {
    if (EntityCacheUtil.getResult(MicroblogsEntryModelImpl.ENTITY_CACHE_ENABLED,MicroblogsEntryImpl.class,microblogsEntry.getPrimaryKey()) == null) {
      cacheResult(microblogsEntry);
    }
 else {
      microblogsEntry.resetOriginalValues();
    }
  }
}
