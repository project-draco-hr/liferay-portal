{
  UnsyncStringReader usr=new UnsyncStringReader("abc");
  assertEquals('a',usr.read());
  usr.mark(-1);
  assertEquals('b',usr.read());
  assertEquals('c',usr.read());
  assertEquals(-1,usr.read());
  usr.reset();
  assertEquals('b',usr.read());
  assertEquals('c',usr.read());
  assertEquals(-1,usr.read());
}
