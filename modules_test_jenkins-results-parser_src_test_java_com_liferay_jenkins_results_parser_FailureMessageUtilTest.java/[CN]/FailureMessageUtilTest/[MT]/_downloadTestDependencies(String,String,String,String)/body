{
  String jenkinsReportURLTemplate="https://${hostName}.liferay.com/userContent/jobs/${jobName}/" + "/builds/${buildNumber}/jenkins-report.html";
  String jenkinsReportURLString=_replaceToken(jenkinsReportURLTemplate,"buildNumber",buildNumber);
  jenkinsReportURLString=_replaceToken(jenkinsReportURLString,"hostName",hostName);
  jenkinsReportURLString=_replaceToken(jenkinsReportURLString,"jobName",jobName);
  URL jenkinsReportURL=_createURL(jenkinsReportURLString);
  String jenkinsReportName=description + "_" + hostName+ "_"+ buildNumber;
  _downloadTestGroupData(jenkinsReportName,jenkinsReportURL);
}
