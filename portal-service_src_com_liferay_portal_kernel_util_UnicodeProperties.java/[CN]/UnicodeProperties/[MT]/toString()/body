{
  StringBuilder sb=new StringBuilder(_length);
  for (  String key : keySet()) {
    String value=get(key);
    if (Validator.isNotNull(value)) {
      if (_safe) {
        value=_encode(value);
      }
      sb.append(key);
      sb.append(StringPool.EQUAL);
      sb.append(value);
      sb.append(StringPool.NEW_LINE);
    }
  }
  return sb.toString();
}
