{
  UploadPortletRequest uploadReq=UploadRequestUtil.getUploadPortletRequest(req);
  ThemeDisplay themeDisplay=(ThemeDisplay)req.getAttribute(WebKeys.THEME_DISPLAY);
  long nodeId=ParamUtil.getLong(uploadReq,"nodeId");
  File pagesFile=uploadReq.getFile("pagesFile");
  WikiNodePermission.check(themeDisplay.getPermissionChecker(),nodeId,ActionKeys.UPDATE);
  WikiNodeServiceUtil.importPages(themeDisplay.getUserId(),nodeId,pagesFile);
}
