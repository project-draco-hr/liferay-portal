{
  try {
    PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
    if (DLFolderPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),_folder.getFolderId(),ActionKeys.VIEW) && portletDisplay.isWebDAVEnabled() && (_folder.getRepositoryId() == themeDisplay.getScopeGroupId())) {
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
