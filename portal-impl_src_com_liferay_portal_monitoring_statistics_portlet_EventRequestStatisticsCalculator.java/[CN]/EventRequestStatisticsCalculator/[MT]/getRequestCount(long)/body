{
  CompanyStatistics companyStatistics=_ServerStatistics.getPortletContainerStatistics(companyId);
  long numRequests=0;
  Collection<RequestStatistics> portletsStatistics=companyStatistics.getEventStatistics();
  for (  RequestStatistics portletStatistics : portletsStatistics) {
    numRequests+=portletStatistics.getNumRequests();
  }
  return numRequests;
}
