{
  request.setAttribute(ITEM_SELECTED_EVENT_NAME,itemSelectedEventName);
  request.setAttribute(PORTLET_URL,portletURL);
  request.setAttribute(WIKI_ATTACHMENT_ITEM_SELECTOR_CRITERION,wikiAttachmentItemSelectorCriterion);
  RequestDispatcher requestDispatcher=request.getRequestDispatcher("/o/wiki-web/html/item/selector/attachments.jsp");
  requestDispatcher.include(request,response);
}
