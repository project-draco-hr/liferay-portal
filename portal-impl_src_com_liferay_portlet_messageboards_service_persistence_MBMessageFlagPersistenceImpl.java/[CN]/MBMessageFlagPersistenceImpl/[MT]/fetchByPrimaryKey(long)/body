{
  MBMessageFlag mbMessageFlag=(MBMessageFlag)EntityCacheUtil.getResult(MBMessageFlagModelImpl.ENTITY_CACHE_ENABLED,MBMessageFlagImpl.class,messageFlagId,this);
  if (mbMessageFlag == _nullMBMessageFlag) {
    return null;
  }
  if (mbMessageFlag == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      mbMessageFlag=(MBMessageFlag)session.get(MBMessageFlagImpl.class,Long.valueOf(messageFlagId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (!hasException && (mbMessageFlag == null)) {
        EntityCacheUtil.putResult(MBMessageFlagModelImpl.ENTITY_CACHE_ENABLED,MBMessageFlagImpl.class,messageFlagId,_nullMBMessageFlag);
      }
 else {
        cacheResult(mbMessageFlag);
      }
      closeSession(session);
    }
  }
  return mbMessageFlag;
}
