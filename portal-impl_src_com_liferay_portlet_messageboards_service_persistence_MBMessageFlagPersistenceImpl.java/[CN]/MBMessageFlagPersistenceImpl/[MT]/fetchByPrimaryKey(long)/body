{
  MBMessageFlag result=(MBMessageFlag)EntityCacheUtil.getResult(MBMessageFlagModelImpl.ENTITY_CACHE_ENABLED,MBMessageFlagImpl.class,messageFlagId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      MBMessageFlag mbMessageFlag=(MBMessageFlag)session.get(MBMessageFlagImpl.class,new Long(messageFlagId));
      if (mbMessageFlag != null) {
        cacheResult(mbMessageFlag);
      }
      return mbMessageFlag;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (MBMessageFlag)result;
  }
}
