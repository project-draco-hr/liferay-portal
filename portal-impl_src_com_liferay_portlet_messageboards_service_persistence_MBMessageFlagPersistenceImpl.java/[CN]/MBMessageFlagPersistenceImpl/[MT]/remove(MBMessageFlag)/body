{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(mbMessageFlag);
    }
  }
  mbMessageFlag=removeImpl(mbMessageFlag);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(mbMessageFlag);
    }
  }
  return mbMessageFlag;
}
