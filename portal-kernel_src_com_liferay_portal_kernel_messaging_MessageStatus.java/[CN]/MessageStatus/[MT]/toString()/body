{
  StringBuilder sb=new StringBuilder();
  sb.append("{startTime=");
  sb.append(_startTime);
  sb.append(", endTime=");
  sb.append(_endTime);
  sb.append(", payload=");
  sb.append(_payload);
  sb.append(", errorMessage=");
  sb.append(_exceptionMessage);
  sb.append(", errorStackTrace=");
  sb.append(_exceptionStackTrace);
  sb.append("}");
  return sb.toString();
}
