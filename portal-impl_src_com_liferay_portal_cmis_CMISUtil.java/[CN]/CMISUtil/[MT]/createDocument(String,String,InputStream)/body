{
  Entry entry=_abdera.newEntry();
  entry.setTitle(name);
  if (is == null) {
    CMISObject object=entry.addExtension(_constants.OBJECT);
    object.setValue(_constants.PROPERTY_NAME_OBJECT_TYPE_ID,_constants.BASE_TYPE_FOLDER);
  }
 else {
    entry.setContent(is);
    CMISObject object=entry.addExtension(_constants.OBJECT);
    object.setValue(_constants.PROPERTY_NAME_OBJECT_TYPE_ID,_constants.BASE_TYPE_DOCUMENT);
  }
  ClientResponse resp=_getClient().post(parentChildrenUrl,entry);
  if (ResponseType.select(resp.getStatus()) != ResponseType.SUCCESS) {
    throw new CMISException("Error creating " + name + " "+ resp.getStatusText());
  }
  return (Entry)resp.getDocument().getRoot();
}
