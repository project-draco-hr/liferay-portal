{
  long[] pageIds=ParamUtil.getLongValues(portletRequest,"rowIdsWikiPage");
  if (pageIds.length == 0) {
    return Collections.emptyList();
  }
  List<WikiPage> pages=new ArrayList<>();
  for (  long pageId : pageIds) {
    if (pageId != 0) {
      pages.add(WikiPageServiceUtil.getPage(pageId));
    }
  }
  return pages;
}
