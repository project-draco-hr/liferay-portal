{
  int count=countByNamespace(namespace);
  List<WSRPProducer> list=findByNamespace(namespace,count - 1,count,obc);
  if (list.size() == 0) {
    StringBuilder msg=new StringBuilder();
    msg.append("No WSRPProducer exists with the key {");
    msg.append("namespace=" + namespace);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchProducerException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
