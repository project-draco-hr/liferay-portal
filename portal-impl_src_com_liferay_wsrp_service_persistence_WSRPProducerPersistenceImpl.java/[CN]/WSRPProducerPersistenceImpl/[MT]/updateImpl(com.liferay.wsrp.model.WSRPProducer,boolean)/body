{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,wsrpProducer,merge);
    wsrpProducer.setNew(false);
    return wsrpProducer;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(WSRPProducer.class.getName());
  }
}
