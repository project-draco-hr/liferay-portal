{
  Session session=null;
  try {
    session=openSession();
    WSRPProducer wsrpProducer=(WSRPProducer)session.get(WSRPProducerImpl.class,new Long(producerId));
    if (wsrpProducer == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No WSRPProducer exists with the primary key " + producerId);
      }
      throw new NoSuchProducerException("No WSRPProducer exists with the primary key " + producerId);
    }
    return remove(wsrpProducer);
  }
 catch (  NoSuchProducerException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
