{
  String fullName=_defaultDefaultFullNameGenerator.getFullName("ThisShouldBeAVeryLongName","ThisShouldBeAVeryLongMiddleName","ThisShouldBeAVeryLongLastName");
  Assert.assertTrue(fullName.length() < UserConstants.FULL_NAME_MAX_LENGTH);
  Assert.assertEquals("T T ThisShouldBeAVeryLongLastName",fullName);
}
