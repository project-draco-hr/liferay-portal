{
  if (publicLayoutSetPrototypeId > 0) {
    LayoutSetPrototype layoutSetPrototype=LayoutSetPrototypeLocalServiceUtil.getLayoutSetPrototype(publicLayoutSetPrototypeId);
    LayoutSet publicLayoutSet=group.getPublicLayoutSet();
    copyLayoutSet(layoutSetPrototype.getLayoutSet(),publicLayoutSet,serviceContext);
  }
  if (privateLayoutSetPrototypeId > 0) {
    LayoutSetPrototype layoutSetPrototype=LayoutSetPrototypeLocalServiceUtil.getLayoutSetPrototype(privateLayoutSetPrototypeId);
    LayoutSet privateLayoutSet=group.getPrivateLayoutSet();
    copyLayoutSet(layoutSetPrototype.getLayoutSet(),privateLayoutSet,serviceContext);
  }
}
