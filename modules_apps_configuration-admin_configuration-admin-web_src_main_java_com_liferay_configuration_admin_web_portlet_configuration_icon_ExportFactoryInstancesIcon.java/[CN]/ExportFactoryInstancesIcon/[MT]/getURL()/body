{
  LiferayPortletURL liferayPortletURL=(LiferayPortletURL)PortalUtil.getControlPanelPortletURL(portletRequest,ConfigurationAdminPortletKeys.CONFIGURATION_ADMIN,PortletRequest.RESOURCE_PHASE);
  liferayPortletURL.setResourceID("export");
  ConfigurationModel factoryConfigurationModel=(ConfigurationModel)portletRequest.getAttribute(ConfigurationAdminWebKeys.FACTORY_CONFIGURATION_MODEL);
  liferayPortletURL.setParameter("factoryPid",factoryConfigurationModel.getFactoryPid());
  return liferayPortletURL.toString();
}
