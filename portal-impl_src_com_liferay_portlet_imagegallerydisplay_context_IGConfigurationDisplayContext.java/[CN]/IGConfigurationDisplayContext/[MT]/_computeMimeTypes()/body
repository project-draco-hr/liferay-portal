{
  String[] mediaGalleryMimeTypes=_dlSettings.getMediaGalleryMimeTypes();
  _currentMimeTypes=new ArrayList<KeyValuePair>();
  for (  String mimeType : mediaGalleryMimeTypes) {
    _currentMimeTypes.add(new KeyValuePair(mimeType,LanguageUtil.get(_request,mimeType)));
  }
  _availableMimeTypes=new ArrayList<KeyValuePair>();
  Set<String> allMediaGalleryMimeTypes=DLUtil.getAllMediaGalleryMimeTypes();
  for (  String mimeType : allMediaGalleryMimeTypes) {
    if (Arrays.binarySearch(mediaGalleryMimeTypes,mimeType) < 0) {
      _availableMimeTypes.add(new KeyValuePair(mimeType,LanguageUtil.get(_request,mimeType)));
    }
  }
}
