{
  try {
    for (    Dictionary<String,Object> properties : getProperties(hotDeployEvent)) {
      String portletId=(String)properties.get("panel.app.portlet.id");
      ServiceRegistration<PanelApp> serviceRegistration=_bundleContext.registerService(PanelApp.class,new PortletPanelAppAdapter(portletId),properties);
      _serviceRegistrations.put(portletId,serviceRegistration);
    }
  }
 catch (  DocumentException|IOException e) {
    throw new HotDeployException(e);
  }
}
