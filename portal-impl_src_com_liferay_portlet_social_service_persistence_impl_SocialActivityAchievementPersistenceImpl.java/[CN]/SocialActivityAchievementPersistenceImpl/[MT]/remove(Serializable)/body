{
  Session session=null;
  try {
    session=openSession();
    SocialActivityAchievement socialActivityAchievement=(SocialActivityAchievement)session.get(SocialActivityAchievementImpl.class,primaryKey);
    if (socialActivityAchievement == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchActivityAchievementException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(socialActivityAchievement);
  }
 catch (  NoSuchActivityAchievementException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
