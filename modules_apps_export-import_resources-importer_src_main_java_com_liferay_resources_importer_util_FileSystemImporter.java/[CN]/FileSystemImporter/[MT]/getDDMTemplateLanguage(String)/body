{
  String extension=FileUtil.getExtension(fileName);
  if (extension.equals(TemplateConstants.LANG_TYPE_CSS) || extension.equals(TemplateConstants.LANG_TYPE_FTL) || extension.equals(TemplateConstants.LANG_TYPE_VM)|| extension.equals(TemplateConstants.LANG_TYPE_XSL)) {
    return extension;
  }
  return TemplateConstants.LANG_TYPE_VM;
}
