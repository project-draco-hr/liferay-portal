{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      boolean InControlPanel=selenium.isElementPresent("link=Back to My Community");
    if (!InControlPanel) {
      label=2;
      continue;
    }
  selenium.click(RuntimeVariables.replace("link=Back to My Community"));
selenium.waitForPageToLoad("30000");
Thread.sleep(5000);
case 2:
selenium.click(RuntimeVariables.replace("link=Welcome - Liferay"));
selenium.waitForPageToLoad("30000");
case 100:
label=-1;
}
}
}
