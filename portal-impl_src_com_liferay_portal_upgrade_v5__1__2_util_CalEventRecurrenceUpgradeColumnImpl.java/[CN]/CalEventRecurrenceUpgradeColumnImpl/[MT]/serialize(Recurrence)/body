{
  JSONObject recurrenceJSON=JSONFactoryUtil.createJSONObject(JSONFactoryUtil.serialize(recurrence));
  recurrenceJSON.put("javaClass",TZSRecurrence.class.getName());
  TimeZone timeZone=TimeZoneUtil.getTimeZone(StringPool.UTC);
  JSONObject timeZoneJSON=JSONFactoryUtil.createJSONObject(JSONFactoryUtil.serialize(timeZone));
  recurrenceJSON.put("timeZone",timeZoneJSON);
  return recurrenceJSON.toString();
}
