{
  if (cacheConfiguration == null) {
    return;
  }
  resolvePortalProperty(cacheConfiguration.getBootstrapCacheLoaderFactoryConfiguration());
  List<FactoryConfiguration<?>> factoryConfigurations=cacheConfiguration.getCacheEventListenerConfigurations();
  for (  FactoryConfiguration<?> factoryConfiguration : factoryConfigurations) {
    resolvePortalProperty(factoryConfiguration);
  }
}
