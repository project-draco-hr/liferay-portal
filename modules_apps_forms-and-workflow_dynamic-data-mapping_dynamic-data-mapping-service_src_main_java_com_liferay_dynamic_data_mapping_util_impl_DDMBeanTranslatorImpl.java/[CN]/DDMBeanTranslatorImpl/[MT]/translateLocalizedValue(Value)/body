{
  if (value == null) {
    return null;
  }
  com.liferay.dynamic.data.mapping.kernel.LocalizedValue translatedLocalizedValue=new com.liferay.dynamic.data.mapping.kernel.LocalizedValue();
  translatedLocalizedValue.setDefaultLocale(value.getDefaultLocale());
  Map<Locale,String> values=value.getValues();
  for (  Map.Entry<Locale,String> entry : values.entrySet()) {
    translatedLocalizedValue.addString(entry.getKey(),entry.getValue());
  }
  return translatedLocalizedValue;
}
