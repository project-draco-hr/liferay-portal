{
  ActionableDynamicQuery actionableDynamicQuery=new BookmarksFolderActionableDynamicQuery(){
    @Override protected void performAction(    Object object) throws PortalException, SystemException {
      BookmarksFolder folder=(BookmarksFolder)object;
      long groupId=folder.getGroupId();
      long folderId=folder.getFolderId();
      reindexEntries(companyId,groupId,folderId);
    }
  }
;
  actionableDynamicQuery.setCompanyId(companyId);
  actionableDynamicQuery.performActions();
}
