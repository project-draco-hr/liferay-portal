{
  _sourceStore=StoreFactory.getInstance();
  String targetStoreClassName=getTargetStoreClassName();
  _targetStore=(Store)InstanceFactory.newInstance(ClassLoaderUtil.getPortalClassLoader(),targetStoreClassName);
  migratePortlets();
  StoreFactory.setInstance(_targetStore);
  MaintenanceUtil.appendStatus("Please set " + PropsKeys.DL_STORE_IMPL + " in your portal-ext.properties to use "+ targetStoreClassName);
  PropsValues.DL_STORE_IMPL=targetStoreClassName;
}
