{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId(),TestPropsValues.getUserId());
  _message=MBTestUtil.addMessage(group.getGroupId());
  MBCategory category=MBCategoryServiceUtil.addCategory(TestPropsValues.getUserId(),MBCategoryConstants.DEFAULT_PARENT_CATEGORY_ID,RandomTestUtil.randomString(),StringPool.BLANK,serviceContext);
  _submessage=MBTestUtil.addMessage(group.getGroupId(),category.getCategoryId());
}
