{
  if (_portletFilters.size() > _pos) {
    ResourceFilter resourceFilter=(ResourceFilter)_portletFilters.get(_pos++);
    resourceFilter.doFilter(resourceRequest,resourceResponse,this);
  }
 else {
    ResourceServingPortlet resourceServingPortlet=(ResourceServingPortlet)_portlet;
    resourceServingPortlet.serveResource(resourceRequest,resourceResponse);
  }
}
