{
  List<String> wars=new ArrayList<String>();
  List<String> jars=new ArrayList<String>();
  for (int i=0; i < args.length; i++) {
    if (args[i].endsWith(".war")) {
      wars.add(args[i]);
    }
 else     if (args[i].endsWith(".jar")) {
      jars.add(args[i]);
    }
  }
  new LayoutTemplateDeployer(wars,jars);
}
