{
  if (element == null) {
    return false;
  }
  _fullyLock();
  try {
    for (Node<E> previous=_head, current=previous._next; current != null; previous=current, current=current._next) {
      if (element.equals(current._element)) {
        _unlink(current,previous);
        return true;
      }
    }
    return false;
  }
  finally {
    _fullyUnlock();
  }
}
