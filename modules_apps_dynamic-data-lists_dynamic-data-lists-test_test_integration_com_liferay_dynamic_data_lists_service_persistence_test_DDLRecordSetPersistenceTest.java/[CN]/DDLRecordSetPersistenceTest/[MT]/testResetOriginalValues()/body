{
  DDLRecordSet newDDLRecordSet=addDDLRecordSet();
  _persistence.clearCache();
  DDLRecordSet existingDDLRecordSet=_persistence.findByPrimaryKey(newDDLRecordSet.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingDDLRecordSet.getUuid(),ReflectionTestUtil.invoke(existingDDLRecordSet,"getOriginalUuid",new Class<?>[0])));
  Assert.assertEquals(Long.valueOf(existingDDLRecordSet.getGroupId()),ReflectionTestUtil.<Long>invoke(existingDDLRecordSet,"getOriginalGroupId",new Class<?>[0]));
  Assert.assertEquals(Long.valueOf(existingDDLRecordSet.getGroupId()),ReflectionTestUtil.<Long>invoke(existingDDLRecordSet,"getOriginalGroupId",new Class<?>[0]));
  Assert.assertTrue(Validator.equals(existingDDLRecordSet.getRecordSetKey(),ReflectionTestUtil.invoke(existingDDLRecordSet,"getOriginalRecordSetKey",new Class<?>[0])));
}
