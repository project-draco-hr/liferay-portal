{
  long passwordPolicyId=ParamUtil.getLong(req,"passwordPolicyId");
  long[] addGroupIds=StringUtil.split(ParamUtil.getString(req,"addGroupIds"),0L);
  long[] removeGroupIds=StringUtil.split(ParamUtil.getString(req,"removeGroupIds"),0L);
  GroupServiceUtil.addPasswordPolicyGroups(passwordPolicyId,addGroupIds);
  GroupServiceUtil.unsetPasswordPolicyGroups(passwordPolicyId,removeGroupIds);
}
