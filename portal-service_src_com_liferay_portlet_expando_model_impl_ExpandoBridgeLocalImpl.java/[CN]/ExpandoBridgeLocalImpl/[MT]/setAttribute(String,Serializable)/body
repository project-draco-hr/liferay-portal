{
  if (super.getClassPK() <= 0) {
    throw new UnsupportedOperationException();
  }
  try {
    ExpandoValueLocalServiceUtil.addValue(super.getClassName(),ExpandoTableConstants.DEFAULT_TABLE_NAME,name,super.getClassPK(),value);
    checkIndex(name);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
