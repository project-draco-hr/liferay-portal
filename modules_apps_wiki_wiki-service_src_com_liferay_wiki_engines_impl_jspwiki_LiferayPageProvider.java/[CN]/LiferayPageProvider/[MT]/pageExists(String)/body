{
  if (_log.isDebugEnabled()) {
    _log.debug("Invoking pageExists(" + title + ")");
  }
  try {
    int count=WikiPageLocalServiceUtil.getPagesCount(_nodeId,JSPWikiEngine.decodeJSPWikiName(title),true);
    if (count > 0) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return false;
}
