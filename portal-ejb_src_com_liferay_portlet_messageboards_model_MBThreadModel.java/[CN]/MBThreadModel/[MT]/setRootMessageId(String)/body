{
  if (((rootMessageId == null) && (_rootMessageId != null)) || ((rootMessageId != null) && (_rootMessageId == null)) || ((rootMessageId != null) && (_rootMessageId != null) && !rootMessageId.equals(_rootMessageId))) {
    if (!XSS_ALLOW_ROOTMESSAGEID) {
      rootMessageId=XSSUtil.strip(rootMessageId);
    }
    _rootMessageId=rootMessageId;
    setModified(true);
  }
}
