{
  if (obj == null) {
    return -1;
  }
  MBThread mbThread=(MBThread)obj;
  int value=0;
  if (getPriority() < mbThread.getPriority()) {
    value=-1;
  }
 else   if (getPriority() > mbThread.getPriority()) {
    value=1;
  }
 else {
    value=0;
  }
  value=value * -1;
  if (value != 0) {
    return value;
  }
  value=DateUtil.compareTo(getLastPostDate(),mbThread.getLastPostDate());
  value=value * -1;
  if (value != 0) {
    return value;
  }
  return 0;
}
