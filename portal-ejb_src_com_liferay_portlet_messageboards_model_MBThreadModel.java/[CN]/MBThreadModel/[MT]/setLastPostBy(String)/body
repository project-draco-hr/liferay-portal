{
  if (((lastPostBy == null) && (_lastPostBy != null)) || ((lastPostBy != null) && (_lastPostBy == null)) || ((lastPostBy != null) && (_lastPostBy != null) && !lastPostBy.equals(_lastPostBy))) {
    if (!XSS_ALLOW_LASTPOSTBY) {
      lastPostBy=XSSUtil.strip(lastPostBy);
    }
    _lastPostBy=lastPostBy;
  }
}
