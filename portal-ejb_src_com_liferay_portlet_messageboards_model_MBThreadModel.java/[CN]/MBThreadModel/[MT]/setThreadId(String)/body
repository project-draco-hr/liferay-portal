{
  if (((threadId == null) && (_threadId != null)) || ((threadId != null) && (_threadId == null)) || ((threadId != null) && (_threadId != null) && !threadId.equals(_threadId))) {
    if (!XSS_ALLOW_THREADID) {
      threadId=XSSUtil.strip(threadId);
    }
    _threadId=threadId;
    setModified(true);
  }
}
