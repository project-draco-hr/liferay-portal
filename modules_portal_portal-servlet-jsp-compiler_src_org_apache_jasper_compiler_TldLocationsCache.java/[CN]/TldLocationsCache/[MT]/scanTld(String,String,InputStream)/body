{
  try {
    String uri=getUriFromTld(resourcePath,stream);
    if (uri != null && (mappings.get(uri) == null || systemUris.contains(uri) || (systemUrisJsf.contains(uri) && !useMyFaces))) {
      mappings.put(uri,new String[]{resourcePath,entryName});
    }
  }
  finally {
    if (stream != null) {
      try {
        stream.close();
      }
 catch (      Throwable t) {
      }
    }
  }
}
