{
  if (initialized)   return;
  HashMap tldUriToLocationMap=(HashMap)ctxt.getAttribute(Constants.JSP_TLD_URI_TO_LOCATION_MAP);
  if (tldUriToLocationMap != null) {
    mappings=tldUriToLocationMap;
  }
 else {
    mappings=new HashMap();
  }
  try {
    if (!localTldsProcessed) {
      processWebDotXml();
    }
    scanJars();
    if (!localTldsProcessed) {
      processTldsInFileSystem("/WEB-INF/");
    }
    initialized=true;
  }
 catch (  Exception ex) {
    throw new JasperException(Localizer.getMessage("jsp.error.internal.tldinit"),ex);
  }
}
