{
  if (!_BROWSER_TYPE.equals("*chrome") && !_BROWSER_TYPE.equals("*firefox") && !_BROWSER_TYPE.equals("*iehta")&& !_BROWSER_TYPE.equals("*iexplore")) {
    return;
  }
  try {
    Thread.sleep(5000);
    Runtime runtime=Runtime.getRuntime();
    String command=RuntimeVariables.replace(TestPropsValues.SELENIUM_BROWSER_COMMANDS_DIR + TestPropsValues.SELENIUM_DOWNLOAD_FILE);
    runtime.exec(command);
    Thread.sleep(30000);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
