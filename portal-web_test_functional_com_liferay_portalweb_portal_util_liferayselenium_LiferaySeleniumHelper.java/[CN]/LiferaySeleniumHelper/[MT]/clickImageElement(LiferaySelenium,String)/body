{
  if (OSDetector.isWindows()) {
    _imageDirectory=StringUtil.replace(_imageDirectory,"//","\\");
    _imageDirectory=StringUtil.replace(_imageDirectory,"LinuxImages","WindowsImages");
  }
  Match match=screen.exists(liferaySelenium.getProjectDir() + _imageDirectory + image);
  if ((match == null) && image.equals("typeFileName.png")) {
    return;
  }
  screen.click(liferaySelenium.getProjectDir() + _imageDirectory + image);
}
