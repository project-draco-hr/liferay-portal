{
  _screen.click(liferaySelenium.getProjectDirName() + liferaySelenium.getSikuliImagesDirName() + image);
  _screen.type("a",Key.CTRL);
  String slash="/";
  if (OSDetector.isWindows()) {
    slash="\\";
  }
  sikuliType(liferaySelenium,image,liferaySelenium.getOutputDirName() + slash + value);
  _screen.type(Key.ENTER);
}
