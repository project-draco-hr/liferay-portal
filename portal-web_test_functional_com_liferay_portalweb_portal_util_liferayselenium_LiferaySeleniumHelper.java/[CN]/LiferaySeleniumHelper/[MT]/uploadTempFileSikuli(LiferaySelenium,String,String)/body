{
  Screen screen=new Screen();
  String slash="/";
  if (OSDetector.isWindows()) {
    slash="\\";
  }
  Match match=screen.exists(liferaySelenium.getProjectDir() + liferaySelenium.getSikuliImagesDir() + image);
  liferaySelenium.pause("1000");
  if (match == null) {
    return;
  }
  screen.click(liferaySelenium.getProjectDir() + liferaySelenium.getSikuliImagesDir() + image);
  screen.type(liferaySelenium.getOutputDir() + slash + value);
}
