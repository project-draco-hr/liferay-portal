{
  for (int second=0; ; second++) {
    if (second >= TestPropsValues.TIMEOUT_EXPLICIT_WAIT) {
      liferaySelenium.assertSelectedLabel(selectLocator,pattern);
    }
    try {
      if (liferaySelenium.isSelectedLabel(selectLocator,pattern)) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
