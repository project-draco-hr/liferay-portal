{
  for (int second=0; ; second++) {
    if (second >= TestPropsValues.TIMEOUT_EXPLICIT_WAIT) {
      return liferaySelenium.isElementPresent(locator);
    }
    if (liferaySelenium.isElementPresent(locator)) {
      break;
    }
    Thread.sleep(1000);
  }
  return liferaySelenium.isElementPresent(locator);
}
