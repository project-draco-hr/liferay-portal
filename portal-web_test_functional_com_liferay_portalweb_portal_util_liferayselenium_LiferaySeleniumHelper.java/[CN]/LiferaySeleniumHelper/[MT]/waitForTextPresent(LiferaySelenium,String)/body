{
  value=RuntimeVariables.replace(value);
  for (int second=0; ; second++) {
    if (second >= TestPropsValues.TIMEOUT_EXPLICIT_WAIT) {
      BaseTestCase.fail("Timeout: unable to find the text \"" + value + "\"");
    }
    try {
      if (liferaySelenium.isTextPresent(value)) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
