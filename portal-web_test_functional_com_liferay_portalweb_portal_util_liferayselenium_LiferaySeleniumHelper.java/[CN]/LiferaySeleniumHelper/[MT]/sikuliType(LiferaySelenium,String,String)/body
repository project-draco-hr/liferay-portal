{
  Screen screen=new Screen();
  Match match=screen.exists(liferaySelenium.getProjectDirName() + liferaySelenium.getSikuliImagesDirName() + image);
  liferaySelenium.pause("1000");
  if (match == null) {
    return;
  }
  screen.click(liferaySelenium.getProjectDirName() + liferaySelenium.getSikuliImagesDirName() + image);
  screen.type(value);
}
