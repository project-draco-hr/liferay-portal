{
  populateRelatedPages();
  movePageToTrash(_relatedPages.getParentPage());
  WikiPage page=WikiPageLocalServiceUtil.getPage(_relatedPages.getPageResourcePrimKey());
  WikiPage newParentPage=WikiTestUtil.addPage(_group.getGroupId(),_node.getNodeId(),true);
  movePage(page,newParentPage);
  page=WikiPageLocalServiceUtil.getPage(_relatedPages.getPageResourcePrimKey());
  Assert.assertFalse(page.isInTrash());
  Assert.assertEquals(newParentPage.getTitle(),page.getParentTitle());
}
