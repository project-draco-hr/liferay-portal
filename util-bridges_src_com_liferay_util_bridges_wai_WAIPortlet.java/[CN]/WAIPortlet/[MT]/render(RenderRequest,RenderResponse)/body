{
  Map<String,String[]> params=new HashMap<String,String[]>(renderRequest.getParameterMap());
  String appURL=ParamUtil.getString(renderRequest,_APP_URL,renderRequest.getContextPath());
  params.remove(_APP_URL);
  appURL=appURL.concat(HttpUtil.parameterMapToString(params));
  renderRequest.setAttribute(_APP_URL,appURL);
  PortletContext portletContext=getPortletContext();
  PortletRequestDispatcher portletRequestDispatcher=portletContext.getRequestDispatcher(_JSP_IFRAME);
  try {
    portletRequestDispatcher.include(renderRequest,renderResponse);
  }
 catch (  IOException ioe) {
    _log.error(ioe,ioe);
  }
}
