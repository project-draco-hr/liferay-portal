{
  if (dataSample.getCompanyId() != _companyId) {
    return;
  }
  String portletId=dataSample.getPortletId();
  PortletStatistics portletStatistics=_companyPortletStatistics.get(portletId);
  if (portletStatistics == null) {
    portletStatistics=new PortletStatistics(portletId,dataSample.getName(),dataSample.getDisplayName());
    _companyPortletStatistics.put(portletId,portletStatistics);
  }
  portletStatistics.processDataSample(dataSample);
}
