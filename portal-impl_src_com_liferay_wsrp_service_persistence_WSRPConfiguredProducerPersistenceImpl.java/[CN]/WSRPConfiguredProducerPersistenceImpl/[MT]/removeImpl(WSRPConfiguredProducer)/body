{
  Session session=null;
  try {
    session=openSession();
    session.delete(wsrpConfiguredProducer);
    session.flush();
    return wsrpConfiguredProducer;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(WSRPConfiguredProducer.class.getName());
  }
}
