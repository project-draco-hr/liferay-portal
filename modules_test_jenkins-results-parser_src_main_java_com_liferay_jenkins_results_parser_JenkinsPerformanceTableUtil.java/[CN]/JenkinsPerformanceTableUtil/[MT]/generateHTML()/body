{
  List<JenkinsPerformanceDataUtil.Result> results=JenkinsPerformanceDataUtil.getSlowestResults();
  if (results == null) {
    return "";
  }
  Element element=new DefaultElement("table");
  element.addAttribute("border","1");
  element.addAttribute("cellspacing","0");
  element.add(_createRowElement("th","Axis","Class Name","Duration (Seconds)","Job Name","Name","Status",null));
  for (  JenkinsPerformanceDataUtil.Result result : results) {
    element.add(_createRowElement("td",result.getAxis(),result.getClassName(),Float.toString(result.getDuration()),result.getJobName(),result.getName(),result.getStatus(),result.getUrl()));
  }
  JenkinsPerformanceDataUtil.reset();
  StringBuilder sb=new StringBuilder();
  sb.append(JenkinsResultsParserUtil.format(element));
  return sb.toString();
}
