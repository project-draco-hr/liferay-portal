{
  if ((url == null) || (url.length() == 0)) {
    return createCell(name,tag,width);
  }
  Element cell=new DefaultElement(tag);
  cell.addAttribute("width",width);
  Element anchor=new DefaultElement("a");
  anchor.addAttribute("href",url);
  anchor.addText(name);
  cell.add(anchor);
  return cell;
}
