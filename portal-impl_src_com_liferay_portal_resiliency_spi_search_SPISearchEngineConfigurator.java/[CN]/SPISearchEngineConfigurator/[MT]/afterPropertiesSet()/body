{
  if (!SPIUtil.isSPI()) {
    return;
  }
  Set<String> searchEngineIds=SearchEngineUtil.getSearchEngineIds();
  for (  String searchEngineId : searchEngineIds) {
    String searchWriterDestinationName=SearchEngineUtil.getSearchWriterDestinationName(searchEngineId);
    Destination searchWriterDestination=_messageBus.getDestination(searchWriterDestinationName);
    searchWriterDestination.unregisterMessageListeners();
    SPI spi=SPIUtil.getSPI();
    searchWriterDestination.register(new IntrabandBridgeMessageListener(spi.getRegistrationReference()));
  }
}
