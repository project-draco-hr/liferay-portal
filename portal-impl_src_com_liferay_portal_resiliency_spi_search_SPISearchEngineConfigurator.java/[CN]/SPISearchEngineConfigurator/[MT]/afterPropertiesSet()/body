{
  if (!SPIUtil.isSPI()) {
    return;
  }
  Set<String> searchEngineIds=SearchEngineUtil.getSearchEngineIds();
  for (  String searchEngineId : searchEngineIds) {
    String destinationName=SearchEngineUtil.getSearchWriterDestinationName(searchEngineId);
    Destination destination=MessageBusUtil.getDestination(destinationName);
    destination.unregisterMessageListeners();
    SPI spi=SPIUtil.getSPI();
    destination.register(new IntrabandBridgeMessageListener(spi.getRegistrationReference()));
  }
}
