{
  try {
    ActionUtil.getFolder(req);
  }
 catch (  Exception e) {
    if (e != null && e instanceof NoSuchFolderException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.image_gallery.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward("portlet.image_gallery.view_slide_show");
}
