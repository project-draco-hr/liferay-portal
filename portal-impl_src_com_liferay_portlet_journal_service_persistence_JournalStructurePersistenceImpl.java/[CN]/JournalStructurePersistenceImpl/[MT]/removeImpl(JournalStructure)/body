{
  Session session=null;
  try {
    session=openSession();
    session.delete(journalStructure);
    session.flush();
    return journalStructure;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(JournalStructure.class.getName());
  }
}
