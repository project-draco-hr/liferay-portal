{
  Session session=null;
  try {
    session=openSession();
    StringMaker query=new StringMaker();
    query.append("FROM com.liferay.portlet.journal.model.JournalStructure WHERE ");
    query.append("groupId = ?");
    query.append(" AND ");
    if (structureId == null) {
      query.append("structureId IS NULL");
    }
 else {
      query.append("structureId = ?");
    }
    query.append(" ");
    query.append("ORDER BY ");
    query.append("structureId ASC");
    Query q=session.createQuery(query.toString());
    q.setCacheable(true);
    int queryPos=0;
    q.setLong(queryPos++,groupId);
    if (structureId != null) {
      q.setString(queryPos++,structureId);
    }
    List list=q.list();
    if (list.size() == 0) {
      return null;
    }
    JournalStructure journalStructure=(JournalStructure)list.get(0);
    return journalStructure;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
