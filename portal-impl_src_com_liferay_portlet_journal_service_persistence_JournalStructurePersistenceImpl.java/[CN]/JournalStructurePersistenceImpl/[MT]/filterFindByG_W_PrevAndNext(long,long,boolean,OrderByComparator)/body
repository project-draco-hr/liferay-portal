{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByG_W_PrevAndNext(id,groupId,webContentType,orderByComparator);
  }
  JournalStructure journalStructure=findByPrimaryKey(id);
  Session session=null;
  try {
    session=openSession();
    JournalStructure[] array=new JournalStructureImpl[3];
    array[0]=filterGetByG_W_PrevAndNext(session,journalStructure,groupId,webContentType,orderByComparator,true);
    array[1]=journalStructure;
    array[2]=filterGetByG_W_PrevAndNext(session,journalStructure,groupId,webContentType,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
