{
  Session session=null;
  try {
    session=openSession();
    JournalStructure journalStructure=(JournalStructure)session.get(JournalStructureImpl.class,journalStructurePK);
    if (journalStructure == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No JournalStructure exists with the primary key " + journalStructurePK);
      }
      throw new NoSuchStructureException("No JournalStructure exists with the primary key " + journalStructurePK);
    }
    return remove(journalStructure);
  }
 catch (  NoSuchStructureException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
