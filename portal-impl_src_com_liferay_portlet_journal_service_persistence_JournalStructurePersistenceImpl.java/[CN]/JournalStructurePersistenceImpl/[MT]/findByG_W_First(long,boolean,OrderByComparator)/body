{
  List<JournalStructure> list=findByG_W(groupId,webContentType,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(6);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("groupId=");
    msg.append(groupId);
    msg.append(", webContentType=");
    msg.append(webContentType);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchStructureException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
