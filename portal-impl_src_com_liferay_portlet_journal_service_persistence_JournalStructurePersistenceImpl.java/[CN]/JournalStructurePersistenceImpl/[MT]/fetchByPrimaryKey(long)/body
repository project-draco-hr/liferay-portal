{
  JournalStructure result=(JournalStructure)EntityCacheUtil.getResult(JournalStructureModelImpl.ENTITY_CACHE_ENABLED,JournalStructure.class,id,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      JournalStructure journalStructure=(JournalStructure)session.get(JournalStructureImpl.class,new Long(id));
      cacheResult(journalStructure);
      return journalStructure;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (JournalStructure)result;
  }
}
