{
  JournalStructure journalStructure=(JournalStructure)EntityCacheUtil.getResult(JournalStructureModelImpl.ENTITY_CACHE_ENABLED,JournalStructureImpl.class,id,this);
  if (journalStructure == null) {
    Session session=null;
    try {
      session=openSession();
      journalStructure=(JournalStructure)session.get(JournalStructureImpl.class,Long.valueOf(id));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (journalStructure != null) {
        cacheResult(journalStructure);
      }
      closeSession(session);
    }
  }
  return journalStructure;
}
