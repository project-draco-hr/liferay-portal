{
  selenium.open("http://www.able.com:8080/");
  Thread.sleep(5000);
  assertEquals(RuntimeVariables.replace("http://www.able.com:8080/"),selenium.getLocation());
  assertEquals(RuntimeVariables.replace("Virtual Hosting Community"),selenium.getText("//li[2]/span/a"));
  assertTrue(selenium.isElementPresent("link=Public Page"));
  selenium.clickAt("link=Public Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Public Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("http://www.able.com:8080/public-page"),selenium.getLocation());
}
