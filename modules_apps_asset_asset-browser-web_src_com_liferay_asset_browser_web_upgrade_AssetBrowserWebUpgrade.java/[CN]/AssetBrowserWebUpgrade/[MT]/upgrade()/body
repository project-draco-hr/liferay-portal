{
  UpgradePortletId upgradePortletId=new UpgradePortletId(){
    @Override protected String[][] getRenamePortletIdsArray(){
      return new String[][]{new String[]{"172",AssetBrowserPortletKeys.ASSET_BROWSER}};
    }
  }
;
  _releaseLocalService.updateRelease("com.liferay.asset.browser.web",Collections.<UpgradeProcess>singletonList(upgradePortletId),1,0,false);
}
