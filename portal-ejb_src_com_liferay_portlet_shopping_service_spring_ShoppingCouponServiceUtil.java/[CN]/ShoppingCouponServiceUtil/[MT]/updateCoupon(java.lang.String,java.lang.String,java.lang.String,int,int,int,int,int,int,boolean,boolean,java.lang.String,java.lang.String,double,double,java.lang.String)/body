{
  try {
    ShoppingCouponService shoppingCouponService=ShoppingCouponServiceFactory.getService();
    return shoppingCouponService.updateCoupon(couponId,name,description,startMonth,startDay,startYear,endMonth,endDay,endYear,neverExpires,active,limitCategories,limitSkus,minOrder,discount,discountType);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
