{
  ShoppingCouponService shoppingCouponService=ShoppingCouponServiceFactory.getService();
  return shoppingCouponService.updateCoupon(plid,couponId,name,description,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,endDateHour,endDateMinute,neverExpire,active,limitCategories,limitSkus,minOrder,discount,discountType);
