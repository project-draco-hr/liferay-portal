{
  try {
    LayoutExporter layoutExporter=LayoutExporter.getInstance();
    return layoutExporter.exportLayoutsAsFile(exportImportConfiguration);
  }
 catch (  PortalException pe) {
    throw pe;
  }
catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new SystemException(e);
  }
}
