{
  ValueMapper dlFolderIdMapper=AvailableMappersUtil.getDLFolderIdMapper();
  content=IdReplacer.replaceLongIds(content,"/document_library/get_file?folderId=",dlFolderIdMapper);
  content=IdReplacer.replaceLongIds(content,"_20_struts_action=%2Fdocument_library%2Fget_file&_20_folderId=",dlFolderIdMapper);
  ValueMapper igImageIdMapper=AvailableMappersUtil.getIGImageIdMapper();
  content=IdReplacer.replaceLongIds(content,"/image_gallery?img_id=",igImageIdMapper);
  return content;
}
