{
  if (((recipientName == null) && (_recipientName != null)) || ((recipientName != null) && (_recipientName == null)) || ((recipientName != null) && (_recipientName != null) && !recipientName.equals(_recipientName))) {
    if (!XSS_ALLOW_RECIPIENTNAME) {
      recipientName=XSSUtil.strip(recipientName);
    }
    _recipientName=recipientName;
  }
}
