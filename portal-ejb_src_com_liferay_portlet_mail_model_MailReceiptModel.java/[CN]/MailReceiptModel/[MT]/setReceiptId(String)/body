{
  if (((receiptId == null) && (_receiptId != null)) || ((receiptId != null) && (_receiptId == null)) || ((receiptId != null) && (_receiptId != null) && !receiptId.equals(_receiptId))) {
    if (!XSS_ALLOW_RECEIPTID) {
      receiptId=XSSUtil.strip(receiptId);
    }
    _receiptId=receiptId;
    setModified(true);
  }
}
