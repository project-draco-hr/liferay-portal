{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByGroupId_PrevAndNext(id,groupId,orderByComparator);
  }
  JournalFeed journalFeed=findByPrimaryKey(id);
  Session session=null;
  try {
    session=openSession();
    JournalFeed[] array=new JournalFeedImpl[3];
    array[0]=filterGetByGroupId_PrevAndNext(session,journalFeed,groupId,orderByComparator,true);
    array[1]=journalFeed;
    array[2]=filterGetByGroupId_PrevAndNext(session,journalFeed,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
