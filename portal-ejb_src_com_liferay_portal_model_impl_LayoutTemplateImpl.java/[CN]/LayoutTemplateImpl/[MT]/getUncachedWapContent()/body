{
  if (_ctx == null) {
    if (_log.isDebugEnabled()) {
      _log.debug("Cannot get latest WAP content for " + _servletContextName + " "+ getWapTemplatePath()+ " because the servlet context is null");
    }
    return _wapContent;
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Getting latest WAP content for " + _servletContextName + " "+ getWapTemplatePath());
  }
  String wapContent=Http.URLtoString(_ctx.getResource(getWapTemplatePath()));
  setWapContent(wapContent);
  return wapContent;
}
