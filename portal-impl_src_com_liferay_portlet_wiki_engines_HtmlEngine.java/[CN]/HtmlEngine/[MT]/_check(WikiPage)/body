{
  if (Validator.isNull(_friendlyURLMapping) || Validator.isNull(_router)) {
    Portlet portlet=PortletLocalServiceUtil.getPortletById(page.getCompanyId(),PortletKeys.WIKI);
    _friendlyURLMapping=Portal.FRIENDLY_URL_SEPARATOR + portlet.getFriendlyURLMapping();
    _router=portlet.getFriendlyURLMapperInstance().getRouter();
  }
}
