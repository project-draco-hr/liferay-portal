{
  String path=getPath(renderRequest);
  if (Validator.equals(path,"/edit_workflow_instance.jsp")) {
    renderRequest.setAttribute(WebKeys.DISPLAY_CONTEXT,createWorkflowInstanceEditDisplayContext(renderRequest,renderResponse));
    return;
  }
  renderRequest.setAttribute(WebKeys.DISPLAY_CONTEXT,createWorkflowInstanceViewDisplayContext(renderRequest,renderResponse));
}
