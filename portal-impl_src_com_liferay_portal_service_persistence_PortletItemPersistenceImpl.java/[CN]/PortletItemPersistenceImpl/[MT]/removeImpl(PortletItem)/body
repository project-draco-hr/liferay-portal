{
  portletItem=toUnwrappedModel(portletItem);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,portletItem);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(portletItem);
  return portletItem;
}
