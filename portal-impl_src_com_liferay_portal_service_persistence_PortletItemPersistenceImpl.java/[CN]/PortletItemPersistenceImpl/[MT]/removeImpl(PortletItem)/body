{
  Session session=null;
  try {
    session=openSession();
    if (BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(PortletItemImpl.class,portletItem.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(portletItem);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  PortletItemModelImpl portletItemModelImpl=(PortletItemModelImpl)portletItem;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_N_P_C,new Object[]{new Long(portletItemModelImpl.getOriginalGroupId()),portletItemModelImpl.getOriginalName(),portletItemModelImpl.getOriginalPortletId(),new Long(portletItemModelImpl.getOriginalClassNameId())});
  EntityCacheUtil.removeResult(PortletItemModelImpl.ENTITY_CACHE_ENABLED,PortletItem.class,portletItem.getPrimaryKey());
  return portletItem;
}
