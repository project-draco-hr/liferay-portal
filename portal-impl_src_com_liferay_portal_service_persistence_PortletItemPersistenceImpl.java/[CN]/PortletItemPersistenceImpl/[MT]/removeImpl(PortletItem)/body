{
  Session session=null;
  try {
    session=openSession();
    session.delete(portletItem);
    session.flush();
    return portletItem;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(PortletItem.class.getName());
  }
}
