{
  portletItem=toUnwrappedModel(portletItem);
  Session session=null;
  try {
    session=openSession();
    if (portletItem.isCachedModel()) {
      portletItem=(PortletItem)session.get(PortletItemImpl.class,portletItem.getPrimaryKeyObj());
    }
    if (portletItem != null) {
      session.delete(portletItem);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (portletItem != null) {
    clearCache(portletItem);
  }
  return portletItem;
}
