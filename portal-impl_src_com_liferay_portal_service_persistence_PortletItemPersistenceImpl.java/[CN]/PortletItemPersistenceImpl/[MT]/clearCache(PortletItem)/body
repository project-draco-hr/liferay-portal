{
  EntityCacheUtil.removeResult(PortletItemModelImpl.ENTITY_CACHE_ENABLED,PortletItemImpl.class,portletItem.getPrimaryKey());
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_N_P_C,new Object[]{new Long(portletItem.getGroupId()),portletItem.getName(),portletItem.getPortletId(),new Long(portletItem.getClassNameId())});
}
