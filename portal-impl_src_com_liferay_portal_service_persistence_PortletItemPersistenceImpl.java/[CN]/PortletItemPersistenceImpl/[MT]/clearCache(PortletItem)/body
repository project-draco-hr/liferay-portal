{
  EntityCacheUtil.removeResult(PortletItemModelImpl.ENTITY_CACHE_ENABLED,PortletItemImpl.class,portletItem.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_N_P_C,new Object[]{Long.valueOf(portletItem.getGroupId()),portletItem.getName(),portletItem.getPortletId(),Long.valueOf(portletItem.getClassNameId())});
}
