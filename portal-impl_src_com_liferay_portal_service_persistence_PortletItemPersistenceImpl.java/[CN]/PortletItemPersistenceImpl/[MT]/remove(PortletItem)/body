{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(portletItem);
    }
  }
  portletItem=removeImpl(portletItem);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(portletItem);
    }
  }
  return portletItem;
}
