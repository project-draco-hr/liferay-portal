{
  PortletItem portletItem=(PortletItem)EntityCacheUtil.getResult(PortletItemModelImpl.ENTITY_CACHE_ENABLED,PortletItemImpl.class,portletItemId,this);
  if (portletItem == null) {
    Session session=null;
    try {
      session=openSession();
      portletItem=(PortletItem)session.get(PortletItemImpl.class,Long.valueOf(portletItemId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (portletItem != null) {
        cacheResult(portletItem);
      }
      closeSession(session);
    }
  }
  return portletItem;
}
