{
  PortletItem portletItem=(PortletItem)EntityCacheUtil.getResult(PortletItemModelImpl.ENTITY_CACHE_ENABLED,PortletItemImpl.class,portletItemId);
  if (portletItem == _nullPortletItem) {
    return null;
  }
  if (portletItem == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      portletItem=(PortletItem)session.get(PortletItemImpl.class,Long.valueOf(portletItemId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (portletItem != null) {
        cacheResult(portletItem);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(PortletItemModelImpl.ENTITY_CACHE_ENABLED,PortletItemImpl.class,portletItemId,_nullPortletItem);
      }
      closeSession(session);
    }
  }
  return portletItem;
}
