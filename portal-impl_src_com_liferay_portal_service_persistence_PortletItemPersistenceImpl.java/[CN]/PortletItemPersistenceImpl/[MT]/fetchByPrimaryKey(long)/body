{
  PortletItem result=(PortletItem)EntityCacheUtil.getResult(PortletItemModelImpl.ENTITY_CACHE_ENABLED,PortletItemImpl.class,portletItemId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      PortletItem portletItem=(PortletItem)session.get(PortletItemImpl.class,new Long(portletItemId));
      if (portletItem != null) {
        cacheResult(portletItem);
      }
      return portletItem;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (PortletItem)result;
  }
}
