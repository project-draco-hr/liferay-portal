{
  EntityCacheUtil.putResult(PortletItemModelImpl.ENTITY_CACHE_ENABLED,PortletItemImpl.class,portletItem.getPrimaryKey(),portletItem);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_N_P_C,new Object[]{new Long(portletItem.getGroupId()),portletItem.getName(),portletItem.getPortletId(),new Long(portletItem.getClassNameId())},portletItem);
}
