{
  PortletItem portletItem=fetchByPrimaryKey(portletItemId);
  if (portletItem == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No PortletItem exists with the primary key " + portletItemId);
    }
    throw new NoSuchPortletItemException("No PortletItem exists with the primary key " + portletItemId);
  }
  return portletItem;
}
