{
  if (_localClusterNodeId == null) {
    _localClusterNodeId=ClusterExecutorUtil.getLocalClusterNode().getClusterNodeId();
  }
  boolean[] isNewLock=new boolean[1];
  Lock lock=LockLocalServiceUtil.lock(_classNameForLock,_classNameForLock,_localClusterNodeId,_retrieveFromCache,replaceOldLock,isNewLock,oldOwner);
  return lock;
}
