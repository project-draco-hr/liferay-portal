{
  if (_localClusterNodeId == null) {
    ClusterNode clusterNode=ClusterExecutorUtil.getLocalClusterNode();
    _localClusterNodeId=clusterNode.getClusterNodeId();
  }
  Lock lock=LockLocalServiceUtil.lock(_classNameForLock,_classNameForLock,oldOwner,_localClusterNodeId,_retrieveFromCache,replaceOldLock);
  return lock;
}
