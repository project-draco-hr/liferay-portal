{
  ClusterEventType clusterEventType=clusterEvent.getClusterEventType();
  if (!clusterEventType.equals(ClusterEventType.DEPART)) {
    return;
  }
  try {
    updateMemorySchedulerClusterMaster();
  }
 catch (  Exception ex) {
    if (_log.isErrorEnabled()) {
      _log.error("Update memeory scheduler cluster lock failed",ex);
    }
  }
}
