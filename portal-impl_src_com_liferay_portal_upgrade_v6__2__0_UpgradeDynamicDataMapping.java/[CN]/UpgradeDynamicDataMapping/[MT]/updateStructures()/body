{
  try (PreparedStatement ps=connection.prepareStatement("select structureId, structureKey, xsd from DDMStructure");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long structureId=rs.getLong("structureId");
      String structureKey=rs.getString("structureKey");
      String xsd=rs.getString("xsd");
      if (Validator.isNull(structureKey)) {
        structureKey=String.valueOf(System.currentTimeMillis());
      }
 else {
        structureKey=StringUtil.toUpperCase(structureKey.trim());
      }
      updateStructure(structureId,structureKey,updateXSD(xsd,structureKey));
    }
  }
 }
