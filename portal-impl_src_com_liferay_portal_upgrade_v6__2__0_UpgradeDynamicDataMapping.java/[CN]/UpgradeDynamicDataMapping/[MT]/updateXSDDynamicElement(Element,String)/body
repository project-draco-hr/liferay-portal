{
  Element metadataElement=element.element("meta-data");
  updateMetadataElement(metadataElement,new String[]{"multiple","name","readOnly","repeatable","required","showLabel","type","width"},new String[]{"acceptFiles","displayChildLabelAsValue","fieldCssClass","folder"});
  if (StringUtil.equalsIgnoreCase(structureKey,RawMetadataProcessor.TIKA_RAW_METADATA)) {
    element.addAttribute("indexType","text");
  }
  List<Element> dynamicElementElements=element.elements("dynamic-element");
  for (  Element dynamicElementElement : dynamicElementElements) {
    updateXSDDynamicElement(dynamicElementElement,structureKey);
  }
}
