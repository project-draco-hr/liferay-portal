{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("update DDMTemplate set templateKey = ?, script = ? where " + "templateId = ?");
    ps.setString(1,templateKey);
    ps.setString(2,script);
    ps.setLong(3,templateId);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
