{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select * from DDMTemplate");
    rs=ps.executeQuery();
    while (rs.next()) {
      long templateId=rs.getLong("templateId");
      String templateKey=rs.getString("templateKey");
      templateKey=templateKey.trim().toUpperCase();
      runSQL("update DDMTemplate set templateKey = '" + templateKey + "' where templateId = "+ templateId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
