{
  try (PreparedStatement ps=connection.prepareStatement("update DDMStructure set structureKey = ?, xsd = ? where " + "structureId = ?")){
    ps.setString(1,structureKey);
    ps.setString(2,xsd);
    ps.setLong(3,structureId);
    ps.executeUpdate();
  }
 catch (  SQLException sqle) {
    if (_log.isWarnEnabled()) {
      _log.warn(sqle,sqle);
    }
  }
}
