{
  try {
    Group group=getGroup();
    if (group == null) {
      return false;
    }
    LayoutSet layoutSet=LayoutSetLocalServiceUtil.getLayoutSet(LayoutImpl.PUBLIC + group.getGroupId());
    if (layoutSet.getPageCount() > 0) {
      return true;
    }
  }
 catch (  Exception e) {
    _log.error(e);
  }
  return false;
}
