{
  if (activity.isClassName(BookmarksEntry.class.getName())) {
    return BookmarksEntryPermission.contains(permissionChecker,activity.getClassPK(),actionId);
  }
 else   if (activity.isClassName(BookmarksFolder.class.getName())) {
    return BookmarksFolderPermission.contains(permissionChecker,activity.getGroupId(),activity.getClassPK(),actionId);
  }
  return false;
}
