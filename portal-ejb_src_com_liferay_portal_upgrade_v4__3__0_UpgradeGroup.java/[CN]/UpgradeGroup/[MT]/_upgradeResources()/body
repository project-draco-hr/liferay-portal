{
  TempScopeUpgradeColumnImpl upgradeScopeColumn=new TempScopeUpgradeColumnImpl();
  UpgradeColumn upgradePrimKeyColumn=new PrimKeyUpgradeColumnImpl(upgradeScopeColumn,_groupIdMapper,_ownerIdMapper);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(ResourceImpl.TABLE_NAME,ResourceImpl.TABLE_COLUMNS,upgradeScopeColumn,upgradePrimKeyColumn);
  upgradeTable.updateTable();
