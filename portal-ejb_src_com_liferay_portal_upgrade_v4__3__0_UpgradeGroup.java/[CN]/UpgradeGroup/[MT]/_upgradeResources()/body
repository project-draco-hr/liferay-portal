{
  TempUpgradeColumnImpl codeColumn=new TempUpgradeColumnImpl("code");
  UpgradeColumn upgradePrimKeyColumn=new PrimKeyUpgradeColumnImpl(codeColumn,_groupIdMapper,_ownerIdMapper);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(ResourceImpl.TABLE_NAME,ResourceImpl.TABLE_COLUMNS,codeColumn,upgradePrimKeyColumn);
