{
  TempUpgradeColumnImpl codeIdColumn=new TempUpgradeColumnImpl("codeId");
  UpgradeColumn upgradePrimKeyColumn=new PrimKeyUpgradeColumnImpl(codeIdColumn,_groupIdMapper,_ownerIdMapper);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(ResourceImpl.TABLE_NAME,ResourceImpl.TABLE_COLUMNS,codeIdColumn,upgradePrimKeyColumn);
  upgradeTable.updateTable();
