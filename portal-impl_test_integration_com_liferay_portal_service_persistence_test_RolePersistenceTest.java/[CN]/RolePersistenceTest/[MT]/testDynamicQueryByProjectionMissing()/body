{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Role.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("roleId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("roleId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
