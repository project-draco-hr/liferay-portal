{
  TimeZone timeZone=TimeZoneUtil.getTimeZone(timeZoneId);
  if (allDay) {
    timeZone=TimeZone.getTimeZone(StringPool.UTC);
  }
  java.util.Calendar startTimeJCalendar=JCalendarUtil.getJCalendar(startTimeYear,startTimeMonth,startTimeDay,startTimeHour,startTimeMinute,0,0,timeZone);
  java.util.Calendar endTimeJCalendar=JCalendarUtil.getJCalendar(endTimeYear,endTimeMonth,endTimeDay,endTimeHour,endTimeMinute,0,0,timeZone);
  return calendarBookingService.addCalendarBooking(calendarId,childCalendarIds,parentCalendarBookingId,titleMap,descriptionMap,location,startTimeJCalendar.getTimeInMillis(),endTimeJCalendar.getTimeInMillis(),allDay,recurrence,firstReminder,firstReminderType,secondReminder,secondReminderType,serviceContext);
}
