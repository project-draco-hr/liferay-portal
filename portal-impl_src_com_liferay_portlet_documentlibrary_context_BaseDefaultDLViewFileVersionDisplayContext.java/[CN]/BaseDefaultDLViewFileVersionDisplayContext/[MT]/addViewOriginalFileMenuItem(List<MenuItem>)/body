{
  if (dlFileShortcut == null) {
    return;
  }
  PortletURL portletURL=_getRenderURL("/document_library/view_file_entry");
  portletURL.setParameter("redirect",_getCurrentURL());
  portletURL.setParameter("fileEntryId",String.valueOf(dlFileShortcut.getToFileEntryId()));
  _addURLUIItem(new URLMenuItem(),menuItems,"icon-search",DLUIItemKeys.VIEW_ORIGINAL_FILE,"view-original-file",portletURL.toString());
}
