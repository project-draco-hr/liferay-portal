{
  if (!_fileEntryDisplayContextHelper.isCheckoutDocumentActionAvailable()) {
    return;
  }
  _addJavascriptUIItem(new JavascriptToolbarItem(),toolbarItems,"icon-unlock",DLUIItemKeys.CHECKOUT,UnicodeLanguageUtil.get(request,"checkout[document]"),getSubmitFormJavascript(Constants.CHECKOUT,null));
}
