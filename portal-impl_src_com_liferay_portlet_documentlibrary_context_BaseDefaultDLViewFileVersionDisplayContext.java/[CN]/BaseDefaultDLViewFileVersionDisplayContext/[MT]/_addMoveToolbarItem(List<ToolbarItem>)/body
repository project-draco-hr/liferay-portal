{
  if (!_fileEntryDisplayContextHelper.isMoveActionAvailable()) {
    return;
  }
  PortletURL portletURL=_getRenderURL("/document_library/move_entry");
  _addURLUIItem(new URLToolbarItem(),toolbarItems,"icon-move",DLUIItemKeys.MOVE,UnicodeLanguageUtil.get(request,"move"),portletURL.toString());
}
