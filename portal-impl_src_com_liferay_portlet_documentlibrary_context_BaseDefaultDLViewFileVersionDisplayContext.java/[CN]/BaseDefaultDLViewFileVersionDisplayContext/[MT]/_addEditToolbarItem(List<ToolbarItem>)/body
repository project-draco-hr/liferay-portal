{
  if (!_fileEntryDisplayContextHelper.isEditActionAvailable()) {
    return;
  }
  PortletURL portletURL=_getRenderURL("/document_library/edit_file_entry");
  _addURLUIItem(new URLToolbarItem(),toolbarItems,"icon-edit",DLUIItemKeys.EDIT,UnicodeLanguageUtil.get(_request,"edit"),portletURL.toString());
}
