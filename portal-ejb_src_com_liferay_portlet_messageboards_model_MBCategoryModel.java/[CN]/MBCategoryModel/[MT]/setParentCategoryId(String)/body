{
  if (((parentCategoryId == null) && (_parentCategoryId != null)) || ((parentCategoryId != null) && (_parentCategoryId == null)) || ((parentCategoryId != null) && (_parentCategoryId != null) && !parentCategoryId.equals(_parentCategoryId))) {
    if (!XSS_ALLOW_PARENTCATEGORYID) {
      parentCategoryId=XSSUtil.strip(parentCategoryId);
    }
    _parentCategoryId=parentCategoryId;
  }
}
