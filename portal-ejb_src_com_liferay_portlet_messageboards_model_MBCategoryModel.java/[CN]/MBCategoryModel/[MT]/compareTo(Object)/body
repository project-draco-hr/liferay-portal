{
  if (obj == null) {
    return -1;
  }
  MBCategory mbCategory=(MBCategory)obj;
  int value=0;
  value=getParentCategoryId().compareTo(mbCategory.getParentCategoryId());
  if (value != 0) {
    return value;
  }
  value=getName().toLowerCase().compareTo(mbCategory.getName().toLowerCase());
  if (value != 0) {
    return value;
  }
  return 0;
}
