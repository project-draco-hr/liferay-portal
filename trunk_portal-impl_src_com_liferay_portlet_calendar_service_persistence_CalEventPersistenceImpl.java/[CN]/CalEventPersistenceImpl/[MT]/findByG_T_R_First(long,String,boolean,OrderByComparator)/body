{
  List<CalEvent> list=findByG_T_R(groupId,type,repeating,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(8);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("groupId=");
    msg.append(groupId);
    msg.append(", type=");
    msg.append(type);
    msg.append(", repeating=");
    msg.append(repeating);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchEventException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
