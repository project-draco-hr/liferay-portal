{
  DDMStructure structure=addStructure(_classNameId,"Test Structure 1");
  addStructure(_classNameId,"Test Structure 2");
  List<DDMStructure> structures=DDMStructureLocalServiceUtil.search(structure.getCompanyId(),new long[]{structure.getGroupId()},new long[]{structure.getClassNameId()},null,null,structure.getStorageType(),structure.getType(),false,0,1,null);
  Assert.assertEquals(1,structures.size());
}
