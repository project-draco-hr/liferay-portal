{
  String structureKey=RandomTestUtil.randomString();
  String storageType=StorageType.XML.getValue();
  try {
    addStructure(_classNameId,structureKey,"Test Structure 1",getTestStructureDefinition(storageType),storageType,DDMStructureConstants.TYPE_DEFAULT);
    addStructure(_classNameId,structureKey,"Test Structure 2",getTestStructureDefinition(storageType),storageType,DDMStructureConstants.TYPE_DEFAULT);
    Assert.fail();
  }
 catch (  StructureDuplicateStructureKeyException sdske) {
  }
}
