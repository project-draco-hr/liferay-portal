{
  int initialCount=DDMStructureLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),new long[]{TestPropsValues.getGroupId()},new long[]{_classNameId},"Test Structure",null,null,DDMStructureConstants.TYPE_DEFAULT,false);
  addStructure(_classNameId,"Test Structure");
  int count=DDMStructureLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),new long[]{TestPropsValues.getGroupId()},new long[]{_classNameId},"Test Structure",null,null,DDMStructureConstants.TYPE_DEFAULT,false);
  Assert.assertEquals(initialCount + 1,count);
}
