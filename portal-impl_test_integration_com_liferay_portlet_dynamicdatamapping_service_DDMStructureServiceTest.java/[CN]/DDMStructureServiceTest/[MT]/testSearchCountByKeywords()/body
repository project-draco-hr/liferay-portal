{
  int initialCount=DDMStructureLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),new long[]{group.getGroupId()},new long[]{_classNameId},null);
  addStructure(_classNameId,"Test Structure");
  int count=DDMStructureLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),new long[]{group.getGroupId()},new long[]{_classNameId},null);
  Assert.assertEquals(initialCount + 1,count);
}
