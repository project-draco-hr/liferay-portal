{
  int initialCount=DDMStructureLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),new long[]{group.getGroupId()},new long[]{_CLASS_NAME_ID},null);
  addStructure(_CLASS_NAME_ID,"Test Structure");
  int count=DDMStructureLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),new long[]{group.getGroupId()},new long[]{_CLASS_NAME_ID},null);
  Assert.assertEquals(initialCount + 1,count);
}
