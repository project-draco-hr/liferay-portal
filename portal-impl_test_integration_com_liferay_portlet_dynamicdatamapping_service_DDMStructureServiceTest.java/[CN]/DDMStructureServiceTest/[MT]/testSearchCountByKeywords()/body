{
  long companyId=TestPropsValues.getCompanyId();
  long[] groupIds=new long[]{TestPropsValues.getGroupId()};
  long[] classNameIds=new long[]{_testClassNameId};
  int initCount=DDMStructureLocalServiceUtil.searchCount(companyId,groupIds,classNameIds,null);
  addStructure(_testClassNameId,"Test Structure");
  int count=DDMStructureLocalServiceUtil.searchCount(companyId,groupIds,classNameIds,null);
  Assert.assertEquals(initCount + 1,count);
}
