{
  int initialCount=DDMTemplateLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),group.getGroupId(),_classNameId,0,0,null,null,null);
  addDisplayTemplate(_classNameId,0,_resourceClassNameId,"Test Template","Test Template");
  int count=DDMTemplateLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),group.getGroupId(),_classNameId,0,_resourceClassNameId,"Test",null,null);
  Assert.assertEquals(initialCount + 1,count);
}
