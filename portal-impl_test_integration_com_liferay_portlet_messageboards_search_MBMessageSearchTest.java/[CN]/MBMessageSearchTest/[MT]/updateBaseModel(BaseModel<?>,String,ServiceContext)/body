{
  MBMessage message=(MBMessage)baseModel;
  ServiceContext updateServiceContext=ServiceContextTestUtil.getServiceContext(message.getGroupId(),TestPropsValues.getUserId());
  updateServiceContext.setWorkflowAction(WorkflowConstants.ACTION_PUBLISH);
  return MBMessageLocalServiceUtil.updateMessage(TestPropsValues.getUserId(),message.getMessageId(),keywords,updateServiceContext);
}
