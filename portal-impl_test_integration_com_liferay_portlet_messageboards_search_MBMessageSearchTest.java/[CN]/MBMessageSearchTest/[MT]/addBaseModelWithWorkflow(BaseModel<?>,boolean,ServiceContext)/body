{
  MBCategory category=(MBCategory)parentBaseModel;
  MBMessage message=MBMessageLocalServiceUtil.addMessage(TestPropsValues.getUserId(),ServiceTestUtil.randomString(),category.getCategoryId(),getSearchKeywords(),getSearchKeywords(),serviceContext);
  if (!approved) {
    message=MBMessageLocalServiceUtil.updateStatus(message.getStatusByUserId(),message.getMessageId(),WorkflowConstants.STATUS_DRAFT,serviceContext);
  }
  return message;
}
