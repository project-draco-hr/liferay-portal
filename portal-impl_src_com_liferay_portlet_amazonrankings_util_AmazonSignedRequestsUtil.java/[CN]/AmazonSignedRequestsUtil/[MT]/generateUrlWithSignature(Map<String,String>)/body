{
  SortedMap<String,String> sortedParameters=new TreeMap<String,String>(parameters);
  String canonicalizedParameters=_canonicalizeParameters(sortedParameters);
  String signature=_generateSignature("GET\necs.amazonaws.com\n/onca/xml\n" + canonicalizedParameters);
  return "http://ecs.amazonaws.com/onca/xml?" + canonicalizedParameters + "&Signature="+ signature;
}
