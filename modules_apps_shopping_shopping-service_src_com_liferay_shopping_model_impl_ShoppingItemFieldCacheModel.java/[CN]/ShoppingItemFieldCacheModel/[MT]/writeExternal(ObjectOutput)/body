{
  objectOutput.writeLong(itemFieldId);
  objectOutput.writeLong(itemId);
  if (name == null) {
    objectOutput.writeUTF(StringPool.BLANK);
  }
 else {
    objectOutput.writeUTF(name);
  }
  if (values == null) {
    objectOutput.writeUTF(StringPool.BLANK);
  }
 else {
    objectOutput.writeUTF(values);
  }
  if (description == null) {
    objectOutput.writeUTF(StringPool.BLANK);
  }
 else {
    objectOutput.writeUTF(description);
  }
}
