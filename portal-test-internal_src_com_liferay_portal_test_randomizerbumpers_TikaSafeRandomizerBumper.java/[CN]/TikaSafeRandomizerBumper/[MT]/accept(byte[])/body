{
  Metadata metadata=new Metadata();
  try {
    Parser parser=new AutoDetectParser(new TikaConfig());
    ParseContext parserContext=new ParseContext();
    parserContext.set(Parser.class,parser);
    parser.parse(new ByteArrayInputStream(randomValue),new WriteOutContentHandler(new DummyWriter()),metadata,parserContext);
    String contentType=metadata.get("Content-Type");
    return contentType.contains(_contentType);
  }
 catch (  Exception e) {
    return false;
  }
}
