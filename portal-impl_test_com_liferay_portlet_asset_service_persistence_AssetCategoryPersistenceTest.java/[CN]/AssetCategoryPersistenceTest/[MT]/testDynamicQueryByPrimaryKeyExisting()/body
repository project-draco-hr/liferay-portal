{
  AssetCategory newAssetCategory=addAssetCategory();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AssetCategory.class,AssetCategory.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("categoryId",newAssetCategory.getCategoryId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  AssetCategory existingAssetCategory=(AssetCategory)result.get(0);
  assertEquals(existingAssetCategory,newAssetCategory);
}
