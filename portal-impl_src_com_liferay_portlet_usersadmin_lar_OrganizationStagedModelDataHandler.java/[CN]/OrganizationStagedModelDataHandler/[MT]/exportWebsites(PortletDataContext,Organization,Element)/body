{
  List<Website> websites=WebsiteLocalServiceUtil.getWebsites(organization.getCompanyId(),organization.getModelClassName(),organization.getOrganizationId());
  for (  Website website : websites) {
    StagedModelDataHandlerUtil.exportReferenceStagedModel(portletDataContext,organization,website,PortletDataContext.REFERENCE_TYPE_EMBEDDED);
  }
}
