{
  List<Address> addresses=AddressLocalServiceUtil.getAddresses(organization.getCompanyId(),organization.getModelClassName(),organization.getOrganizationId());
  for (  Address address : addresses) {
    portletDataContext.addReferenceElement(organization,element,address,PortletDataContext.REFERENCE_TYPE_EMBEDDED,false);
    StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,address);
  }
}
