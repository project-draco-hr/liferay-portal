{
  selenium.click("//input[@value='Add Subfolder']");
  selenium.waitForPageToLoad("30000");
  selenium.type("_28_name","Test Subfolder");
  selenium.typeKeys("_28_description","This is a test subfolder!");
  selenium.click("//input[@value='Save']");
  selenium.waitForPageToLoad("30000");
  selenium.click("//b");
  selenium.waitForPageToLoad("30000");
}
