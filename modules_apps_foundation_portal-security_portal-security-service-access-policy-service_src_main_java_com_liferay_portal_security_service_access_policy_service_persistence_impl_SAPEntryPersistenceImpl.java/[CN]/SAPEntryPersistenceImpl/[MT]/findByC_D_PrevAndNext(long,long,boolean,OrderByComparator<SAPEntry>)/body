{
  SAPEntry sapEntry=findByPrimaryKey(sapEntryId);
  Session session=null;
  try {
    session=openSession();
    SAPEntry[] array=new SAPEntryImpl[3];
    array[0]=getByC_D_PrevAndNext(session,sapEntry,companyId,defaultSAPEntry,orderByComparator,true);
    array[1]=sapEntry;
    array[2]=getByC_D_PrevAndNext(session,sapEntry,companyId,defaultSAPEntry,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
