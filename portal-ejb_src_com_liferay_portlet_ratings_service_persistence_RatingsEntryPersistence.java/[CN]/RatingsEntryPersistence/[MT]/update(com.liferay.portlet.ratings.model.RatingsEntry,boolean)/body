{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(ratingsEntry);
    }
 else {
      if (ratingsEntry.isNew()) {
        session.save(ratingsEntry);
      }
    }
    session.flush();
    ratingsEntry.setNew(false);
    return ratingsEntry;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
