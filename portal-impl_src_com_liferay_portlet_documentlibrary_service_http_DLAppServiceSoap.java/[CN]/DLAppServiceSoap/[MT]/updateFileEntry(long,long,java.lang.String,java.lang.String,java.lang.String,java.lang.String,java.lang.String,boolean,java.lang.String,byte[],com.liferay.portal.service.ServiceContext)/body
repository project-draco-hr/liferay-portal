{
  try {
    com.liferay.portlet.documentlibrary.model.DLFileEntry returnValue=DLAppServiceUtil.updateFileEntry(groupId,folderId,name,sourceFileName,title,description,changeLog,majorVersion,extraSettings,bytes,serviceContext);
