{
  try {
    java.util.List<com.liferay.portal.kernel.repository.model.Folder> returnValue=DLAppServiceUtil.getFolders(repositoryId,parentFolderId,status,includeMountFolders,start,end,obc);
    return returnValue.toArray(new com.liferay.portal.kernel.repository.model.Folder[returnValue.size()]);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
