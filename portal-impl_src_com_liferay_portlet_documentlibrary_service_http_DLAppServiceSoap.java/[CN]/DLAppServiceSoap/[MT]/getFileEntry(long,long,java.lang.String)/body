{
  try {
    com.liferay.portlet.documentlibrary.model.DLFileEntry returnValue=DLAppServiceUtil.getFileEntry(groupId,folderId,name);
    return com.liferay.portlet.documentlibrary.model.DLFileEntrySoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
