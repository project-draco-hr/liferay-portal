{
  JournalArticle latestIndexableArticle=JournalArticleLocalServiceUtil.fetchLatestArticle(resourcePrimKey,new int[]{WorkflowConstants.STATUS_APPROVED,WorkflowConstants.STATUS_IN_TRASH});
  if (latestIndexableArticle == null) {
    latestIndexableArticle=JournalArticleLocalServiceUtil.fetchLatestArticle(resourcePrimKey);
  }
  return latestIndexableArticle;
}
