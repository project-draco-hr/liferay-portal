{
  InternetAddress to=mailMessage.getTo()[0];
  String processedSubject=StringUtil.replace(mailMessage.getSubject(),new String[]{"[$TO_ADDRESS$]","[$TO_NAME$]"},new String[]{to.getAddress(),GetterUtil.getString(to.getPersonal(),to.getAddress())});
  mailMessage.setSubject(processedSubject);
  String processedBody=StringUtil.replace(mailMessage.getBody(),new String[]{"[$TO_ADDRESS$]","[$TO_NAME$]"},new String[]{to.getAddress(),GetterUtil.getString(to.getPersonal(),to.getAddress())});
  mailMessage.setBody(processedBody);
}
