{
  Class<?> clazz=getClass();
  InputStream inputStream=clazz.getResourceAsStream("dependencies/liferay-social.xml");
  String xml=new String(FileUtil.getBytes(inputStream));
  SocialConfigurationUtil.read(clazz.getClassLoader(),new String[]{xml});
  _activityDefinition=SocialConfigurationUtil.getActivityDefinition(TEST_MODEL,1);
  _group=GroupLocalServiceUtil.getGroup(PortalUtil.getDefaultCompanyId(),GroupConstants.GUEST);
}
