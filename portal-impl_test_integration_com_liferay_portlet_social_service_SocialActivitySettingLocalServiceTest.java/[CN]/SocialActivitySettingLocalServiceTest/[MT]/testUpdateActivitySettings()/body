{
  long classNameId=PortalUtil.getClassNameId(TEST_MODEL);
  Assert.assertFalse(SocialActivitySettingLocalServiceUtil.isEnabled(_group.getGroupId(),classNameId));
  SocialActivitySettingLocalServiceUtil.updateActivitySetting(_group.getGroupId(),TEST_MODEL,true);
  Assert.assertTrue(SocialActivitySettingLocalServiceUtil.isEnabled(_group.getGroupId(),classNameId));
}
