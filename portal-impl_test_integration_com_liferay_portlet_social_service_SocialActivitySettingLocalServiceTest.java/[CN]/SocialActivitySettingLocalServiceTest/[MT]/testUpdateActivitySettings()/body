{
  long classNameId=PortalUtil.getClassNameId(TEST_MODEL);
  boolean enabled=SocialActivitySettingLocalServiceUtil.isEnabled(_guestGroup.getGroupId(),classNameId);
  Assert.assertFalse(enabled);
  SocialActivitySettingLocalServiceUtil.updateActivitySetting(_guestGroup.getGroupId(),TEST_MODEL,true);
  enabled=SocialActivitySettingLocalServiceUtil.isEnabled(_guestGroup.getGroupId(),classNameId);
  Assert.assertTrue(enabled);
}
