{
  Image image=(Image)EntityCacheUtil.getResult(ImageModelImpl.ENTITY_CACHE_ENABLED,ImageImpl.class,imageId,this);
  if (image == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (image == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      image=(Image)session.get(ImageImpl.class,Long.valueOf(imageId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (image == null)) {
        EntityCacheUtil.putResult(ImageModelImpl.ENTITY_CACHE_ENABLED,ImageImpl.class,imageId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(image);
      }
      closeSession(session);
    }
  }
  return image;
}
