{
  image=toUnwrappedModel(image);
  Session session=null;
  try {
    session=openSession();
    if (image.isCachedModel()) {
      image=(Image)session.get(ImageImpl.class,image.getPrimaryKeyObj());
    }
    if (image != null) {
      session.delete(image);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (image != null) {
    clearCache(image);
  }
  return image;
}
