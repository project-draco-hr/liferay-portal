{
  Session session=null;
  try {
    session=openSession();
    if (image.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(ImageImpl.class,image.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(image);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.removeResult(ImageModelImpl.ENTITY_CACHE_ENABLED,ImageImpl.class,image.getPrimaryKey());
  return image;
}
