{
  image=toUnwrappedModel(image);
  Session session=null;
  try {
    session=openSession();
    if (image.isCachedModel()) {
      image=(Image)session.get(ImageImpl.class,image.getPrimaryKeyObj());
    }
    session.delete(image);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(image);
  return image;
}
