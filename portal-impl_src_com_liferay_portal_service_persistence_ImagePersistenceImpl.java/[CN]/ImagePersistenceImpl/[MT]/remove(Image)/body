{
  Session session=null;
  try {
    session=openSession();
    session.delete(image);
    session.flush();
    return image;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Image.class.getName());
  }
}
