{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,image,merge);
    image.setNew(false);
    return image;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Image.class.getName());
  }
}
