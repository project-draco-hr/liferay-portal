{
  Image image=findByPrimaryKey(imageId);
  Session session=null;
  try {
    session=openSession();
    Image[] array=new ImageImpl[3];
    array[0]=getBySize_PrevAndNext(session,image,size,orderByComparator,true);
    array[1]=image;
    array[2]=getBySize_PrevAndNext(session,image,size,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
