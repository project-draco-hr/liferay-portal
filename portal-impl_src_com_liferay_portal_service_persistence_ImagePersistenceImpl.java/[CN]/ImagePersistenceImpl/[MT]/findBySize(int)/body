{
  Object[] finderArgs=new Object[]{new Integer(size)};
  Object result=FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_SIZE,finderArgs,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portal.model.Image WHERE ");
      query.append("size_ < ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("imageId ASC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(size);
      List<Image> list=q.list();
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_SIZE,finderArgs,list);
      cacheResult(list);
      return list;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (List<Image>)result;
  }
}
