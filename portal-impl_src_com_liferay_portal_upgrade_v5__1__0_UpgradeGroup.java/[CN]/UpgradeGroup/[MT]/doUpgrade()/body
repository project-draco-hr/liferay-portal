{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select companyId, groupId, classPK, name from Group_ " + "where friendlyURL = ''");
    rs=ps.executeQuery();
    while (rs.next()) {
      long companyId=rs.getLong("companyId");
      long groupId=rs.getLong("groupId");
      long classPK=rs.getLong("classPK");
      String name=rs.getString("name");
      String friendlyURL=getFriendlyURL(companyId,groupId,classPK,name);
      runSQL("update Group_ set friendlyURL = '" + friendlyURL + "'"+ " where groupId = "+ groupId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
