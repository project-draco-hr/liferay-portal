{
  if (jsrAuthInfo == null) {
    return WSRP_NONE;
  }
 else   if (jsrAuthInfo == PortletRequest.BASIC_AUTH || jsrAuthInfo == PortletRequest.FORM_AUTH || jsrAuthInfo.equals(PortletRequest.BASIC_AUTH) || jsrAuthInfo.equals(PortletRequest.FORM_AUTH)) {
    return WSRP_PASSWD;
  }
 else   if (jsrAuthInfo == PortletRequest.CLIENT_CERT_AUTH || jsrAuthInfo.equals(PortletRequest.CLIENT_CERT_AUTH)) {
    return WSRP_CERT;
  }
 else {
    return jsrAuthInfo;
  }
}
