{
  Object object=null;
  try {
    object=super.getAttribute(name);
  }
 catch (  Exception e) {
    if (e instanceof NotSerializableException) {
      String message=e.getMessage();
      if ((message != null) && message.contains("BEA-101362")) {
      }
 else {
        _log.error(e,e);
      }
    }
  }
  object=NonSerializableObjectHandler.getValue(object);
  return object;
}
