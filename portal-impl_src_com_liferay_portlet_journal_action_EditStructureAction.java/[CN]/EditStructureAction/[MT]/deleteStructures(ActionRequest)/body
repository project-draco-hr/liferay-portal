{
  long groupId=ParamUtil.getLong(req,"groupId");
  String[] deleteStructureIds=StringUtil.split(ParamUtil.getString(req,"deleteStructureIds"));
  for (int i=0; i < deleteStructureIds.length; i++) {
    JournalStructureServiceUtil.deleteStructure(groupId,deleteStructureIds[i]);
    JournalUtil.removeRecentStructure(req,deleteStructureIds[i]);
  }
}
