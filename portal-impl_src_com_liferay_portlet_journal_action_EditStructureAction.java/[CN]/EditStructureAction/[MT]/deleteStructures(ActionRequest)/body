{
  long groupId=ParamUtil.getLong(actionRequest,"groupId");
  String[] deleteStructureIds=StringUtil.split(ParamUtil.getString(actionRequest,"deleteStructureIds"));
  for (int i=0; i < deleteStructureIds.length; i++) {
    JournalStructureServiceUtil.deleteStructure(groupId,deleteStructureIds[i]);
    JournalUtil.removeRecentStructure(actionRequest,deleteStructureIds[i]);
  }
}
