{
  Registry registry=RegistryUtil.getRegistry();
  for (  Class<?> serviceClass : serviceClasses) {
    ServiceDependency serviceDependency=new ServiceDependency(serviceClass);
    _serviceDependencies.add(serviceDependency);
    ServiceTracker<Object,Object> serviceTracker=registry.trackServices((Class<Object>)serviceClass,new ServiceDependencyServiceTrackerCustomizer(serviceDependency));
    serviceDependency.setServiceTracker(serviceTracker);
  }
}
