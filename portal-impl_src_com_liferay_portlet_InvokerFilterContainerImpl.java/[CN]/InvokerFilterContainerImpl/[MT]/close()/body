{
  for (  ServiceRegistrationTuple serviceRegistrationTuple : _serviceRegistrationTuples) {
    PortletFilterFactory.destroy(serviceRegistrationTuple.getPortletFilterModel());
    ServiceRegistration<PortletFilter> serviceRegistration=serviceRegistrationTuple.getServiceRegistration();
    serviceRegistration.unregister();
  }
  _serviceRegistrationTuples.clear();
  _serviceTracker.close();
  _actionFilters.clear();
  _eventFilters.clear();
  _renderFilters.clear();
  _resourceFilters.clear();
}
