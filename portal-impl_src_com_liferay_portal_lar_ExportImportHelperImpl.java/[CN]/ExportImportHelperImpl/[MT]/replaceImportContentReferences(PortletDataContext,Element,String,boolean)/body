{
  content=ExportImportHelperUtil.replaceImportLayoutReferences(portletDataContext,content,importReferencedContent);
  content=ExportImportHelperUtil.replaceImportLinksToLayouts(portletDataContext,content,importReferencedContent);
  content=ExportImportHelperUtil.replaceImportDLReferences(portletDataContext,entityElement,content,importReferencedContent);
  return content;
}
