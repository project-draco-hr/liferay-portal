{
  boolean importCurPortletData=getImportPortletData(companyId,portletId,parameterMap,portletDataElement);
  Map<String,Boolean> importPortletControlsMap=new HashMap<String,Boolean>();
  importPortletControlsMap.put(PortletDataHandlerKeys.PORTLET_DATA,importCurPortletData);
  importPortletControlsMap.putAll(getImportPortletSetupControlsMap(companyId,portletId,parameterMap,manifestSummary));
  return importPortletControlsMap;
}
