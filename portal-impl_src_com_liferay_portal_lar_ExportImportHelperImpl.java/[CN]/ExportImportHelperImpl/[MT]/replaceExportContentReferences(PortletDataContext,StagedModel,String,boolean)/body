{
  content=replaceExportLayoutReferences(portletDataContext,content);
  content=replaceExportLinksToLayouts(portletDataContext,entityStagedModel,content);
  content=replaceExportDLReferences(portletDataContext,entityStagedModel,content,exportReferencedContent);
  String className=ExportImportClassedModelUtil.getClassName(entityStagedModel);
  if (!className.equals(JournalArticle.class.getName())) {
    content=StringUtil.replace(content,StringPool.AMPERSAND_ENCODED,StringPool.AMPERSAND);
  }
  return content;
}
