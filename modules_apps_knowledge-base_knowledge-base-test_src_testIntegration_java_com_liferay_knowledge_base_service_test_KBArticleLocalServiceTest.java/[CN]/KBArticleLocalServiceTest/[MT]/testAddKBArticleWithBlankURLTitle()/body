{
  String urlTitle=StringPool.BLANK;
  KBArticle kbArticle=KBArticleLocalServiceUtil.addKBArticle(_user.getUserId(),_kbFolderClassNameId,KBFolderConstants.DEFAULT_PARENT_FOLDER_ID,StringUtil.randomString(),urlTitle,StringUtil.randomString(),StringUtil.randomString(),null,null,null,_serviceContext);
  Assert.assertTrue(Validator.isNotNull(kbArticle.getUrlTitle()));
}
