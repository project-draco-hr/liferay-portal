{
  KBArticle parentKBArticle=KBArticleLocalServiceUtil.addKBArticle(_user.getUserId(),_kbFolderClassNameId,KBFolderConstants.DEFAULT_PARENT_FOLDER_ID,StringUtil.randomString(),StringUtil.randomString(),StringUtil.randomString(),StringUtil.randomString(),null,null,null,_serviceContext);
  KBArticle childKBArticle=KBArticleLocalServiceUtil.addKBArticle(_user.getUserId(),_kbArticleClassNameId,parentKBArticle.getResourcePrimKey(),StringUtil.randomString(),StringUtil.randomString(),StringUtil.randomString(),StringUtil.randomString(),null,null,null,_serviceContext);
  KBArticle grandChildKBArticle=KBArticleLocalServiceUtil.addKBArticle(_user.getUserId(),_kbArticleClassNameId,childKBArticle.getResourcePrimKey(),StringUtil.randomString(),StringUtil.randomString(),StringUtil.randomString(),StringUtil.randomString(),null,null,null,_serviceContext);
  KBArticleLocalServiceUtil.moveKBArticle(_user.getUserId(),parentKBArticle.getResourcePrimKey(),_kbArticleClassNameId,grandChildKBArticle.getResourcePrimKey(),grandChildKBArticle.getPriority());
}
