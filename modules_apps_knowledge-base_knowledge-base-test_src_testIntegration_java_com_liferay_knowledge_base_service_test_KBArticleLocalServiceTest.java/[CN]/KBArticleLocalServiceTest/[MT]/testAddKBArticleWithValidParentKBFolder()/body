{
  KBFolder kbFolder=KBFolderLocalServiceUtil.addKBFolder(_user.getUserId(),_group.getGroupId(),_kbFolderClassNameId,KBFolderConstants.DEFAULT_PARENT_FOLDER_ID,StringUtil.randomString(),StringUtil.randomString(),_serviceContext);
  KBArticleLocalServiceUtil.addKBArticle(_user.getUserId(),_kbFolderClassNameId,kbFolder.getPrimaryKey(),StringUtil.randomString(),StringUtil.randomString(),StringUtil.randomString(),StringUtil.randomString(),null,null,null,_serviceContext);
}
