{
  try (PreparedStatement ps=connection.prepareStatement("select count(*) from DLFileEntry where groupId = ? and " + "folderId = ? and ((fileEntryId <> ? and title = ?) or " + "fileName = ?)")){
    ps.setLong(1,groupId);
    ps.setLong(2,folderId);
    ps.setLong(3,fileEntryId);
    ps.setString(4,title);
    ps.setString(5,fileName);
    try (ResultSet rs=ps.executeQuery()){
      while (rs.next()) {
        int count=rs.getInt(1);
        if (count > 0) {
          return true;
        }
      }
      return false;
    }
   }
 }
