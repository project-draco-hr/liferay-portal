{
  try (PreparedStatement ps=connection.prepareStatement("select companyId, groupId from Group_ where classNameId = " + "?")){
    long classNameId=PortalUtil.getClassNameId(Company.class);
    ps.setLong(1,classNameId);
    try (ResultSet rs=ps.executeQuery()){
      while (rs.next()) {
        long companyId=rs.getLong(1);
        long groupId=rs.getLong(2);
        updateFileEntryTypeNamesAndDescriptions(companyId,groupId);
      }
    }
   }
 }
