  }
 catch (  SQLException sqle) {
    upgradeTable(DLFileVersionTable.TABLE_NAME,DLFileVersionTable.TABLE_COLUMNS,DLFileVersionTable.TABLE_SQL_CREATE,DLFileVersionTable.TABLE_SQL_ADD_INDEXES);
  }
  updateFileVersionFilenames();
}
