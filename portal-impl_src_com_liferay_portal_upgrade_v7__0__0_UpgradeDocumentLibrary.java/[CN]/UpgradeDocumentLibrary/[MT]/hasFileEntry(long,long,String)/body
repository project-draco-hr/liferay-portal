{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select count(*) from DLFileEntry where groupId = ? and " + "folderId = ? and fileName = ?");
    ps.setLong(1,groupId);
    ps.setLong(2,folderId);
    ps.setString(3,fileName);
    rs=ps.executeQuery();
    while (rs.next()) {
      int count=rs.getInt(1);
      if (count > 0) {
        return true;
      }
    }
    return false;
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
