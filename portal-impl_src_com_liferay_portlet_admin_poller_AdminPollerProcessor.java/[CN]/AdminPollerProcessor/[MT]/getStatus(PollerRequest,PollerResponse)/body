{
  HttpServletRequest originalRequest=pollerRequest.getOriginalRequest();
  HttpSession session=originalRequest.getSession();
  XugglerInstallStatus xugglerInstallStatus=(XugglerInstallStatus)session.getAttribute("xugglerInstallStatus");
  String status="unknown";
  if (xugglerInstallStatus != null) {
    status=xugglerInstallStatus.getCurrentStatus();
  }
  boolean success=false;
  if (!status.equals("unknown")) {
    success=true;
  }
  JSONObject jsonObject=JSONFactoryUtil.createJSONObject();
  jsonObject.put("success",success);
  jsonObject.put("status",status);
  pollerResponse.setParameter("status",jsonObject);
  pollerResponse.setParameter(PollerResponse.POLLER_HINT_HIGH_CONNECTIVITY,Boolean.TRUE.toString());
}
