      if (selenium.isElementPresent("//textarea")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.selectFrame("//iframe[@id=\"_15_editor\"]");
  selenium.selectFrame("//iframe[@id=\"FCKeditor1___Frame\"]");
  selenium.selectFrame("//iframe");
  selenium.typeKeys("//body",RuntimeVariables.replace("This is a WCD Setup Test Entr!"));
  selenium.type("//body",RuntimeVariables.replace("This is a WCD Setup Test Entry!"));
