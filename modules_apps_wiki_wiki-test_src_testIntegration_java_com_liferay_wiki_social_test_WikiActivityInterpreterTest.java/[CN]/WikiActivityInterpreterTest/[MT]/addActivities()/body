{
  WikiNode wikiNode=WikiTestUtil.addNode(group.getGroupId());
  _page=WikiTestUtil.addPage(group.getGroupId(),wikiNode.getNodeId(),true);
  _attachmentFileName=RandomTestUtil.randomString() + ".docx";
  WikiTestUtil.addWikiAttachment(_page.getUserId(),_page.getNodeId(),_page.getTitle(),_attachmentFileName,getClass());
}
