{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select groupId, layoutSetBranchId, privateLayout from " + "LayoutSetBranch");
    rs=ps.executeQuery();
    while (rs.next()) {
      long layoutSetBranchId=rs.getLong("layoutSetBranchId");
      long groupId=rs.getLong("groupId");
      boolean privateLayout=rs.getBoolean("privateLayout");
      upgradeLayoutSetBranch(layoutSetBranchId,groupId,privateLayout);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
