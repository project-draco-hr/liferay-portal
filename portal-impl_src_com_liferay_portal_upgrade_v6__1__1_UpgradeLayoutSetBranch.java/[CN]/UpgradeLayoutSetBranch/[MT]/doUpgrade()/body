{
  try (PreparedStatement ps=connection.prepareStatement("select groupId, layoutSetBranchId, privateLayout from " + "LayoutSetBranch");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long layoutSetBranchId=rs.getLong("layoutSetBranchId");
      long groupId=rs.getLong("groupId");
      boolean privateLayout=rs.getBoolean("privateLayout");
      upgradeLayoutSetBranch(layoutSetBranchId,groupId,privateLayout);
    }
  }
 }
