{
  Session session=null;
  try {
    session=openSession();
    session.delete(announcementsEntry);
    session.flush();
    return announcementsEntry;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(AnnouncementsEntry.class.getName());
  }
}
