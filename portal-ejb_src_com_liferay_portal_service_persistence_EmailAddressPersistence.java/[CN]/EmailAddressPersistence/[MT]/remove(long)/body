{
  Session session=null;
  try {
    session=openSession();
    EmailAddress emailAddress=(EmailAddress)session.get(EmailAddressImpl.class,new Long(emailAddressId));
    if (emailAddress == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No EmailAddress exists with the primary key " + emailAddressId);
      }
      throw new NoSuchEmailAddressException("No EmailAddress exists with the primary key " + emailAddressId);
    }
    return remove(emailAddress);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
