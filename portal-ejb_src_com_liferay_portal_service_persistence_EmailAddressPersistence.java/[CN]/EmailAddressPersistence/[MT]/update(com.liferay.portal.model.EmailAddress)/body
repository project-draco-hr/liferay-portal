{
  Session session=null;
  try {
    session=openSession();
    if (emailAddress.isNew()) {
      session.save(emailAddress);
    }
    session.flush();
    emailAddress.setNew(false);
    return emailAddress;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
