{
  RepositoryEntry repositoryEntry=(RepositoryEntry)entityCache.getResult(RepositoryEntryModelImpl.ENTITY_CACHE_ENABLED,RepositoryEntryImpl.class,primaryKey);
  if (repositoryEntry == _nullRepositoryEntry) {
    return null;
  }
  if (repositoryEntry == null) {
    Session session=null;
    try {
      session=openSession();
      repositoryEntry=(RepositoryEntry)session.get(RepositoryEntryImpl.class,primaryKey);
      if (repositoryEntry != null) {
        cacheResult(repositoryEntry);
      }
 else {
        entityCache.putResult(RepositoryEntryModelImpl.ENTITY_CACHE_ENABLED,RepositoryEntryImpl.class,primaryKey,_nullRepositoryEntry);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(RepositoryEntryModelImpl.ENTITY_CACHE_ENABLED,RepositoryEntryImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return repositoryEntry;
}
