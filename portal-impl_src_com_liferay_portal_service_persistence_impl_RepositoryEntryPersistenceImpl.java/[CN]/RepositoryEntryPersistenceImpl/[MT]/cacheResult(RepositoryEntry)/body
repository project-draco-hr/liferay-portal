{
  entityCache.putResult(RepositoryEntryModelImpl.ENTITY_CACHE_ENABLED,RepositoryEntryImpl.class,repositoryEntry.getPrimaryKey(),repositoryEntry);
  finderCache.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{repositoryEntry.getUuid(),repositoryEntry.getGroupId()},repositoryEntry);
  finderCache.putResult(FINDER_PATH_FETCH_BY_R_M,new Object[]{repositoryEntry.getRepositoryId(),repositoryEntry.getMappedId()},repositoryEntry);
  repositoryEntry.resetOriginalValues();
}
