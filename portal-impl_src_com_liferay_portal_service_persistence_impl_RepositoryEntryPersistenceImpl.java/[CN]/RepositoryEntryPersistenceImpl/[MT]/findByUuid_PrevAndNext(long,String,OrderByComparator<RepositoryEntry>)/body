{
  RepositoryEntry repositoryEntry=findByPrimaryKey(repositoryEntryId);
  Session session=null;
  try {
    session=openSession();
    RepositoryEntry[] array=new RepositoryEntryImpl[3];
    array[0]=getByUuid_PrevAndNext(session,repositoryEntry,uuid,orderByComparator,true);
    array[1]=repositoryEntry;
    array[2]=getByUuid_PrevAndNext(session,repositoryEntry,uuid,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
