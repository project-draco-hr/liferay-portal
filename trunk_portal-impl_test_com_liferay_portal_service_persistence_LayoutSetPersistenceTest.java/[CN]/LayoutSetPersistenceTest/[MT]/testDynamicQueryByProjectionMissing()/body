{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(LayoutSet.class,LayoutSet.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("layoutSetId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("layoutSetId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
