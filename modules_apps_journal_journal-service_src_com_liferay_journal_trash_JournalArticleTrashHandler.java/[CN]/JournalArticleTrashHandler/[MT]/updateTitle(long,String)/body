{
  JournalArticle article=JournalArticleLocalServiceUtil.getLatestArticle(classPK);
  article.setArticleId(name);
  JournalArticleLocalServiceUtil.updateJournalArticle(article);
  JournalArticleResource articleResource=JournalArticleResourceLocalServiceUtil.getArticleResource(article.getResourcePrimKey());
  articleResource.setArticleId(name);
  JournalArticleResourceLocalServiceUtil.updateJournalArticleResource(articleResource);
}
