{
  List<StagedModel> dependentStagedModels=dependentStagedModelsMap.get(DDMStructure.class.getSimpleName());
  Assert.assertEquals(1,dependentStagedModels.size());
  DDMStructure ddmStructure=(DDMStructure)dependentStagedModels.get(0);
  DDMStructureLocalServiceUtil.getDDMStructureByUuidAndGroupId(ddmStructure.getUuid(),group.getGroupId());
  dependentStagedModels=dependentStagedModelsMap.get(DDMTemplate.class.getSimpleName());
  Assert.assertEquals(2,dependentStagedModels.size());
  for (  StagedModel dependentStagedModel : dependentStagedModels) {
    DDMTemplateLocalServiceUtil.getDDMTemplateByUuidAndGroupId(dependentStagedModel.getUuid(),group.getGroupId());
  }
}
