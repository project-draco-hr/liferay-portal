{
  ResourceBlockPermission newResourceBlockPermission=addResourceBlockPermission();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ResourceBlockPermission.class,ResourceBlockPermission.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("id.resourceBlockId"));
  Object newResourceBlockId=newResourceBlockPermission.getResourceBlockId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("id.resourceBlockId",new Object[]{newResourceBlockId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Object existingResourceBlockId=result.get(0);
  assertEquals(existingResourceBlockId,newResourceBlockId);
}
