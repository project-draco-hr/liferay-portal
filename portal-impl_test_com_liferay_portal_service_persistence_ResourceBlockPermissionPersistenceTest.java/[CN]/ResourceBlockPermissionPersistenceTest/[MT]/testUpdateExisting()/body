{
  ResourceBlockPermissionPK pk=new ResourceBlockPermissionPK(nextLong(),nextLong());
  ResourceBlockPermission newResourceBlockPermission=_persistence.create(pk);
  newResourceBlockPermission.setActionIds(nextLong());
  _persistence.update(newResourceBlockPermission,false);
  ResourceBlockPermission existingResourceBlockPermission=_persistence.findByPrimaryKey(newResourceBlockPermission.getPrimaryKey());
  assertEquals(existingResourceBlockPermission.getResourceBlockId(),newResourceBlockPermission.getResourceBlockId());
  assertEquals(existingResourceBlockPermission.getRoleId(),newResourceBlockPermission.getRoleId());
  assertEquals(existingResourceBlockPermission.getActionIds(),newResourceBlockPermission.getActionIds());
}
