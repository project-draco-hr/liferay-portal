{
  RepositoryConfiguration repositoryConfiguration=getRepositoryConfiguration(classNameId);
  RepositoryCreator repositoryCreator=repositoryConfiguration.getRepositoryCreator();
  LocalRepository localRepository=repositoryCreator.createLocalRepository(repositoryId);
  return new CapabilityLocalRepository(localRepository,getExternalSupportedCapabilities(),getExternalExportedCapabilityClasses());
}
