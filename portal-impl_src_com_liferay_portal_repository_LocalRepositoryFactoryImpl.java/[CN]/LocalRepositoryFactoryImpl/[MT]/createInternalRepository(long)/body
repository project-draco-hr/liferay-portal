{
  RepositoryConfiguration repositoryConfiguration=getRepositoryConfiguration(getDefaultClassNameId());
  RepositoryCreator repositoryCreator=repositoryConfiguration.getRepositoryCreator();
  LocalRepository localRepository=repositoryCreator.createLocalRepository(repositoryId);
  return new CapabilityLocalRepository(localRepository,getInternalSupportedCapabilities(),getInternalExportedCapabilityClasses());
}
