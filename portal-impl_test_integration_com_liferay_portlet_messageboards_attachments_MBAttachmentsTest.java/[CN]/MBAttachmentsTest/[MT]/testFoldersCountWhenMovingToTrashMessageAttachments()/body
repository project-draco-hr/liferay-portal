{
  int initialFoldersCount=DLFolderLocalServiceUtil.getDLFoldersCount();
  addMessageAttachment();
  int folderCount=DLFolderLocalServiceUtil.getDLFoldersCount();
  Assert.assertEquals(initialFoldersCount + 3,folderCount);
  MBMessageLocalServiceUtil.moveMessageAttachmentToTrash(TestPropsValues.getUserId(),_message.getMessageId(),"company_logo.png");
  Assert.assertNotEquals(initialFoldersCount,DLFolderLocalServiceUtil.getDLFoldersCount());
}
