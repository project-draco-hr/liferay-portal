{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(RatingsEntry.class,RatingsEntry.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("entryId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("entryId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
