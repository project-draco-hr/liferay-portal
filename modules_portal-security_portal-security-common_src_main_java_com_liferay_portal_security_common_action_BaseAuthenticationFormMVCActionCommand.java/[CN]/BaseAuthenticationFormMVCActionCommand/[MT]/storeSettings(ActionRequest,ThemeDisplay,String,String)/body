{
  Settings settings=SettingsFactoryUtil.getSettings(new CompanyServiceSettingsLocator(themeDisplay.getCompanyId(),serviceName));
  ModifiableSettings modifiableSettings=settings.getModifiableSettings();
  SettingsDescriptor settingsDescriptor=SettingsFactoryUtil.getSettingsDescriptor(serviceName);
  for (  String name : settingsDescriptor.getAllKeys()) {
    String value=ParamUtil.getString(actionRequest,shortNamespace + name);
    String oldValue=settings.getValue(name,null);
    if (!value.equals(oldValue)) {
      modifiableSettings.setValue(name,value);
    }
  }
  modifiableSettings.store();
}
