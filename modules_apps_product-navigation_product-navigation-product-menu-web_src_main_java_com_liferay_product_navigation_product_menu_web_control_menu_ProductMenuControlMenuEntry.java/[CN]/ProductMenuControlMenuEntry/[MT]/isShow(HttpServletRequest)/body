{
  if (_panelCategoryRegistry == null) {
    return false;
  }
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  List<PanelCategory> panelCategories=_panelCategoryRegistry.getChildPanelCategories(PanelCategoryKeys.ROOT,themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroup());
  if (panelCategories.size() <= 1) {
    return false;
  }
  return super.isShow(request);
}
