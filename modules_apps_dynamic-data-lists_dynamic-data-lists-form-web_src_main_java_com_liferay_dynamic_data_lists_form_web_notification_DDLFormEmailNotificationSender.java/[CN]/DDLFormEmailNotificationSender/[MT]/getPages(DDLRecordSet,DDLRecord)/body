{
  List<Object> pages=new ArrayList<>();
  DDMFormLayout ddmFormLayout=getDDMFormLayout(recordSet);
  for (  DDMFormLayoutPage ddmFormLayoutPage : ddmFormLayout.getDDMFormLayoutPages()) {
    Map<String,Object> page=getPage(ddmFormLayoutPage,getDDMFormFieldValuesMap(record),getLocale(recordSet));
    pages.add(page);
  }
  return pages;
}
