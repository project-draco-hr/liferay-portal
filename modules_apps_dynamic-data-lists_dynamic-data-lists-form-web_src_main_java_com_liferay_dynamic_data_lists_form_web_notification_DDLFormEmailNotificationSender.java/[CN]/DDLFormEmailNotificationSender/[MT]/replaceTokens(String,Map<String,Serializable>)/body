{
  return StringUtil.replace(notificationTemplate,new String[]{"[$FORM_FIELDS_VALUES$]","[$FORM_NAME$]","[$FROM_ADDRESS$]","[$FROM_NAME$]","[$PORTAL_URL$]","[$SITE_NAME$]"},new String[]{GetterUtil.getString(context.get("formFieldsValues")),GetterUtil.getString(context.get("formName")),GetterUtil.getString(context.get("fromAddress")),GetterUtil.getString(context.get("fromName")),GetterUtil.getString(context.get("portalUrl")),GetterUtil.getString(context.get("siteName"))});
}
