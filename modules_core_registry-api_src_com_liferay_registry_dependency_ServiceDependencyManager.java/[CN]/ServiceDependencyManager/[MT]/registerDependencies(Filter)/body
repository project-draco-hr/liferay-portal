{
  Registry registry=RegistryUtil.getRegistry();
  for (  Filter filter : filters) {
    ServiceDependency serviceDependency=new ServiceDependency(filter);
    _serviceDependencies.add(serviceDependency);
    ServiceTracker serviceTracker=registry.trackServices(filter,new ServiceDependencyServiceTrackerCustomizer(serviceDependency));
    serviceTracker.open();
    _serviceTrackers.add(serviceTracker);
  }
}
