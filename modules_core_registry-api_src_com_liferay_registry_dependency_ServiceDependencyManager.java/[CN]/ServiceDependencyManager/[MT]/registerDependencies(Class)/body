{
  Registry registry=RegistryUtil.getRegistry();
  for (  Class serviceClass : serviceClasses) {
    ServiceDependency serviceDependency=new ServiceDependency(serviceClass);
    _serviceDependencies.add(serviceDependency);
    ServiceTracker serviceTracker=registry.trackServices(serviceClass,new ServiceDependencyServiceTrackerCustomizer(serviceDependency));
    serviceTracker.open();
    _serviceTrackers.add(serviceTracker);
  }
}
