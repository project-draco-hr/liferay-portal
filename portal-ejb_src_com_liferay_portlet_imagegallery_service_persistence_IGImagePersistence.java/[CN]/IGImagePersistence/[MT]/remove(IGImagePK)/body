{
  Session session=null;
  try {
    session=openSession();
    IGImageHBM igImageHBM=(IGImageHBM)session.get(IGImageHBM.class,igImagePK);
    if (igImageHBM == null) {
      _log.warn("No IGImage exists with the primary key " + igImagePK.toString());
      throw new NoSuchImageException("No IGImage exists with the primary key " + igImagePK.toString());
    }
    session.delete(igImageHBM);
    session.flush();
    return IGImageHBMUtil.model(igImageHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
