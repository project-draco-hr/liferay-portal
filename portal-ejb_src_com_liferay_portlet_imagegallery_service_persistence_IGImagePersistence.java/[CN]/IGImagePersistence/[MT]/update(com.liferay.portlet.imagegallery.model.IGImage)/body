{
  Session session=null;
  try {
    session=openSession();
    if (igImage.isNew()) {
      session.save(igImage);
    }
    session.flush();
    igImage.setNew(false);
    return igImage;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
