{
  Session session=null;
  try {
    session=openSession();
    StringBuffer query=new StringBuffer();
    query.append("FROM IGImage IN CLASS com.liferay.portlet.imagegallery.service.persistence.IGImageHBM WHERE ");
    if (folderId == null) {
      query.append("folderId is null");
    }
 else {
      query.append("folderId = ?");
    }
    query.append(" ");
    query.append("ORDER BY ");
    query.append("imageId ASC");
    Query q=session.createQuery(query.toString());
    int queryPos=0;
    if (folderId != null) {
      q.setString(queryPos++,folderId);
    }
    Iterator itr=q.list().iterator();
    List list=new ArrayList();
    while (itr.hasNext()) {
      IGImageHBM igImageHBM=(IGImageHBM)itr.next();
      list.add(IGImageHBMUtil.model(igImageHBM));
    }
    return list;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
