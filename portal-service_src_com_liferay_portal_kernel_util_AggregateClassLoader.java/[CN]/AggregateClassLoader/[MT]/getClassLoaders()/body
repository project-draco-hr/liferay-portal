{
  List<ClassLoader> classLoaders=new ArrayList<ClassLoader>(_classLoaderReferences.size());
  Iterator<WeakReference<ClassLoader>> itr=_classLoaderReferences.iterator();
  while (itr.hasNext()) {
    WeakReference<ClassLoader> weakReference=itr.next();
    ClassLoader classLoader=weakReference.get();
    if (classLoader == null) {
      itr.remove();
    }
 else {
      classLoaders.add(classLoader);
    }
  }
  return classLoaders;
}
