{
  List<ClassLoader> classLoaders=new ArrayList<ClassLoader>(_classLoaderRefs.size());
  Iterator<WeakReference<ClassLoader>> iterator=_classLoaderRefs.iterator();
  while (iterator.hasNext()) {
    WeakReference<ClassLoader> weakReference=iterator.next();
    ClassLoader classLoader=weakReference.get();
    if (classLoader == null) {
      iterator.remove();
    }
 else {
      classLoaders.add(classLoader);
    }
  }
  return classLoaders;
}
