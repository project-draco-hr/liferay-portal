{
  try {
    return (Enumeration<URL>)_getResourcesMethod.invoke(classLoader,name);
  }
 catch (  InvocationTargetException ite) {
    throw new IOException("Unable to get resources " + name,ite.getTargetException());
  }
catch (  Exception e) {
    throw new IOException("Unable to get resources " + name,e);
  }
}
