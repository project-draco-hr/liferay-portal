{
  DownloadNodeModuleTask downloadNodeModuleTask=GradleUtil.addTask(project,DOWNLOAD_GULP_TASK_NAME,DownloadNodeModuleTask.class);
  downloadNodeModuleTask.setModuleName("gulp");
  downloadNodeModuleTask.setModuleVersion(new Callable<String>(){
    @Override public String call() throws Exception {
      return gulpExtension.getVersion();
    }
  }
);
  return downloadNodeModuleTask;
}
