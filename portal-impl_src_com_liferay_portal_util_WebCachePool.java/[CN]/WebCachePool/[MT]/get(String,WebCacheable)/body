{
  Object obj=SingleVMPoolUtil.get(_cache,key);
  if (obj == null) {
    try {
      obj=wc.convert(key);
      int timeToLive=(int)(wc.getRefreshTime() / Time.SECOND);
      _cache.put(key,obj,timeToLive);
    }
 catch (    ConverterException ce) {
      _log.error(ce.getMessage());
    }
  }
  return obj;
}
