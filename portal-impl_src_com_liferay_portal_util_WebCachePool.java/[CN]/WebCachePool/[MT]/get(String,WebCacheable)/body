{
  Object obj=SingleVMPool.get(_cache,key);
  if (obj == null) {
    try {
      obj=wc.convert(key);
      Element element=new Element(key,obj);
      element.setTimeToLive((int)(wc.getRefreshTime() / Time.SECOND));
      _cache.put(element);
    }
 catch (    ConverterException ce) {
      _log.error(ce.getMessage());
    }
  }
  return obj;
}
