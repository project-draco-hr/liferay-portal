{
  DLEditFileEntryDisplayContext dlEditFileEntryDisplayContext=new DefaultDLEditFileEntryDisplayContext(request,response,dlFileEntryType);
  for (  DLDisplayContextFactoryReference dlDisplayContextFactoryReference : _dlDisplayContextFactoryReferences) {
    DLDisplayContextFactory dlDisplayContextFactory=dlDisplayContextFactoryReference.getDLDisplayContextFactory();
    dlEditFileEntryDisplayContext=dlDisplayContextFactory.getDLEditFileEntryDisplayContext(dlEditFileEntryDisplayContext,request,response,dlFileEntryType);
  }
  return dlEditFileEntryDisplayContext;
}
