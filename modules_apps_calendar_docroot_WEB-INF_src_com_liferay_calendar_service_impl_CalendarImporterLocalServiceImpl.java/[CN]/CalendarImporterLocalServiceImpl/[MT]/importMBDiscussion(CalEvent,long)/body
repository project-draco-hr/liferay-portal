{
  MBDiscussion mbDiscussion=mbDiscussionPersistence.fetchByC_C(classNameLocalService.getClassNameId(CalEvent.class),calEvent.getEventId());
  if (mbDiscussion == null) {
    return;
  }
  long threadId=importMBThread(mbDiscussion.getThreadId(),calendarBookingId);
  addMBDiscussion(PortalUUIDUtil.generate(),counterLocalService.increment(),mbDiscussion.getGroupId(),mbDiscussion.getCompanyId(),mbDiscussion.getUserId(),mbDiscussion.getUserName(),mbDiscussion.getCreateDate(),mbDiscussion.getModifiedDate(),classNameLocalService.getClassNameId(CalendarBooking.class.getName()),calendarBookingId,threadId);
}
