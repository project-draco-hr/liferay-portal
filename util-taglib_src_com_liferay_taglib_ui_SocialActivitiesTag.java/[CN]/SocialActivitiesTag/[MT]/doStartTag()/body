{
  ServletRequest req=pageContext.getRequest();
  req.setAttribute("liferay-ui:social-activities:className",_className);
  req.setAttribute("liferay-ui:social-activities:classPK",String.valueOf(_classPK));
  if (_activities != null) {
    req.setAttribute("liferay-ui:social-activities:activities",_activities);
  }
  return EVAL_BODY_BUFFERED;
}
