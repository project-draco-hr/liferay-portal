{
  List<String> paths=new ArrayList<>();
  int length=folderName.length();
  for (int from=0; from < length; ) {
    int index=folderName.indexOf('/',from);
    if (index == -1) {
      break;
    }
 else {
      String path=folderName.substring(0,index);
      paths.add(0,path);
    }
    from=index + 1;
  }
  return paths;
}
