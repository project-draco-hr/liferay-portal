{
  List<DLFileEntry> fileEntries=DLFileEntryLocalServiceUtil.getDLFileEntriesByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<DLFileEntry>());
  if (ListUtil.isEmpty(fileEntries)) {
    return null;
  }
  return new LiferayFileEntry(fileEntries.get(0));
}
