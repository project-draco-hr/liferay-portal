{
  ThemeDisplay themeDisplay=(ThemeDisplay)liferayPortletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletURL addAssetURL=null;
  if (DLPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),ActionKeys.ADD_DOCUMENT)) {
    addAssetURL=liferayPortletResponse.createRenderURL(PortletKeys.DOCUMENT_LIBRARY);
    addAssetURL.setParameter("struts_action","/document_library/edit_file_entry");
    addAssetURL.setParameter("folderId",String.valueOf(AssetPublisherUtil.getRecentFolderId(liferayPortletRequest,CLASS_NAME)));
    addAssetURL.setParameter("uploader","classic");
  }
  return addAssetURL;
}
