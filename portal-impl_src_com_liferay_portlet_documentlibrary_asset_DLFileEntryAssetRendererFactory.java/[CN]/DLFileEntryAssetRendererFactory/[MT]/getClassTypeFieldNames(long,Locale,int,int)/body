{
  List<Tuple> classTypeFieldNames=new ArrayList<Tuple>();
  DLFileEntryType dlFileEntryType=DLFileEntryTypeLocalServiceUtil.getDLFileEntryType(classTypeId);
  List<DDMStructure> ddmStructures=dlFileEntryType.getDDMStructures();
  for (  DDMStructure ddmStructure : ddmStructures) {
    classTypeFieldNames.addAll(getDDMStructureFieldNames(ddmStructure,locale));
  }
  if ((start != QueryUtil.ALL_POS) || (end != QueryUtil.ALL_POS)) {
    classTypeFieldNames=ListUtil.subList(classTypeFieldNames,start,end);
  }
  return classTypeFieldNames;
}
