{
  Map<Long,String> classTypes=new HashMap<Long,String>();
  List<DLFileEntryType> dlFileEntryTypes=DLFileEntryTypeServiceUtil.getFileEntryTypes(groupIds);
  for (  DLFileEntryType dlFileEntryType : dlFileEntryTypes) {
    classTypes.put(dlFileEntryType.getFileEntryTypeId(),dlFileEntryType.getName());
  }
  return classTypes;
}
