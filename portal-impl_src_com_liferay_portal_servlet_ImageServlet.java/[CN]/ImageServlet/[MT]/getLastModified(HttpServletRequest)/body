{
  try {
    Image image=getImage(request,false);
    if (image == null) {
      return -1;
    }
    Date modifiedDate=image.getModifiedDate();
    if (modifiedDate == null) {
      modifiedDate=PortalUtil.getUptime();
    }
    return (modifiedDate.getTime() / 1000) * 1000;
  }
 catch (  Exception e) {
    _log.error(e,e);
    return -1;
  }
}
