{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByGroupId_PrevAndNext(listId,groupId,orderByComparator);
  }
  DDMList ddmList=findByPrimaryKey(listId);
  Session session=null;
  try {
    session=openSession();
    DDMList[] array=new DDMListImpl[3];
    array[0]=filterGetByGroupId_PrevAndNext(session,ddmList,groupId,orderByComparator,true);
    array[1]=ddmList;
    array[2]=filterGetByGroupId_PrevAndNext(session,ddmList,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
