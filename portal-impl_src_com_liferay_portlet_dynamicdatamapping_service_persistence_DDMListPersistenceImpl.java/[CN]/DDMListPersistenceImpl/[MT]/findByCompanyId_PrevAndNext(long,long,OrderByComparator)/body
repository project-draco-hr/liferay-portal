{
  DDMList ddmList=findByPrimaryKey(listId);
  Session session=null;
  try {
    session=openSession();
    DDMList[] array=new DDMListImpl[3];
    array[0]=getByCompanyId_PrevAndNext(session,ddmList,companyId,orderByComparator,true);
    array[1]=ddmList;
    array[2]=getByCompanyId_PrevAndNext(session,ddmList,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
