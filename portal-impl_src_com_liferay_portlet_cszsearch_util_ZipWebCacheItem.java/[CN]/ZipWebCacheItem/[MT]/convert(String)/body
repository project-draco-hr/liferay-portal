{
  List list=new ArrayList();
  String city=null;
  String state=null;
  String zip=_zip;
  try {
    String text=Http.URLtoString("http://zip4.usps.com/zip4/zcl_3_results.jsp?zip5=" + zip);
    BufferedReader br=new BufferedReader(new StringReader(text));
    String line=null;
    while ((line=br.readLine()) != null) {
      line=line.trim();
      if (!line.equals("")) {
        if (line.indexOf("<h2 style=\"color:#CC0000;\">Not Acceptable") > -1) {
          break;
        }
 else         if (line.indexOf("<td valign=\"top\" class=\"main\"") > -1) {
          String cityAndState=Html.stripHtml(line).trim();
          int pos=cityAndState.indexOf(StringPool.COMMA);
          city=cityAndState.substring(0,pos);
          state=cityAndState.substring(pos + 1,cityAndState.length()).trim();
          if (city != null && state != null) {
            list.add(new CSZAddress(null,TextFormatter.formatName(city),state,zip));
            city=null;
            state=null;
          }
        }
      }
    }
    br.close();
  }
 catch (  Exception e) {
    throw new WebCacheException(e);
  }
  return list;
}
