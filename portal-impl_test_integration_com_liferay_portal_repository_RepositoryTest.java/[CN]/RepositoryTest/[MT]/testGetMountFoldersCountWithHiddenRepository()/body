{
  long classNameId=PortalUtil.getClassNameId(LiferayRepository.class);
  RepositoryLocalServiceUtil.addRepository(TestPropsValues.getUserId(),_group.getGroupId(),classNameId,DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,"Test 1","Test 1",PortletKeys.DOCUMENT_LIBRARY,new UnicodeProperties(),true,new ServiceContext());
  Assert.assertEquals(0,DLFolderServiceUtil.getMountFoldersCount(_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID));
}
