{
  long classNameId=PortalUtil.getClassNameId(LiferayRepository.class);
  RepositoryLocalServiceUtil.addRepository(TestPropsValues.getUserId(),_group.getGroupId(),classNameId,DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,RandomTestUtil.randomString(),RandomTestUtil.randomString(),RandomTestUtil.randomString(),new UnicodeProperties(),false,new ServiceContext());
  Assert.assertEquals(1,DLFolderServiceUtil.getMountFoldersCount(_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID));
}
