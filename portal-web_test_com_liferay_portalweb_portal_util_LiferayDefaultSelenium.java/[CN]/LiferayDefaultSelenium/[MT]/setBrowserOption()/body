{
  String command=null;
  String commandSetBrowserOption=TestPropsValues.SELENIUM_SET_BROWSER_OPTION;
  if (_BROWSER_TYPE.equals("*chrome") || _BROWSER_TYPE.equals("*firefox")) {
    try {
      command=RuntimeVariables.replace(_SELENIUM_EXECUTABLE_DIR + commandSetBrowserOption);
      Runtime.getRuntime().exec(command);
      Thread.sleep(10000);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
