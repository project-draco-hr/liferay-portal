{
  if (!_BROWSER_TYPE.equals("*chrome") && !_BROWSER_TYPE.equals("*firefox") && !_BROWSER_TYPE.equals("*iehta")&& !_BROWSER_TYPE.equals("*iexplore")) {
    return;
  }
  try {
    String[] commands={RuntimeVariables.replace(_SELENIUM_EXECUTABLE_DIR + TestPropsValues.SELENIUM_DOWNLOAD_FILE),TestPropsValues.OUTPUT_DIR + value};
    Runtime runtime=Runtime.getRuntime();
    runtime.exec(commands);
    Thread.sleep(10000);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
