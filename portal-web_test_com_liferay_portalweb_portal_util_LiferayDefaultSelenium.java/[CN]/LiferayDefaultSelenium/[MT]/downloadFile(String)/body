{
  String[] command=null;
  String commandDownloadFile=TestPropsValues.SELENIUM_DOWNLOAD_FILE;
  String downloadFile=TestPropsValues.OUTPUT_DIR + value;
  if (_BROWSER_TYPE.equals("*chrome") || _BROWSER_TYPE.equals("*firefox") || _BROWSER_TYPE.equals("*iehta")|| _BROWSER_TYPE.equals("*iexplore")) {
    try {
      command=new String[]{_SELENIUM_EXECUTABLE_DIR + commandDownloadFile,downloadFile};
      command[0]=RuntimeVariables.replace(command[0]);
      Runtime.getRuntime().exec(command);
      Thread.sleep(10000);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
