{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),PortletKeys.BOOKMARKS,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/bookmarks/edit_folder");
  portletURL.setParameter("folderId",String.valueOf(_folder.getFolderId()));
  return portletURL;
}
