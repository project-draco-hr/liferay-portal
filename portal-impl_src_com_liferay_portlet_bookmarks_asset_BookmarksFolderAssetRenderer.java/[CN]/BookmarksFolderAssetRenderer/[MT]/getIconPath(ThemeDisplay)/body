{
  int foldersCount=0;
  int entriesCount=0;
  try {
    foldersCount=BookmarksFolderServiceUtil.getFoldersCount(_folder.getGroupId(),_folder.getFolderId());
    entriesCount=BookmarksEntryServiceUtil.getEntriesCount(_folder.getGroupId(),_folder.getFolderId());
  }
 catch (  Exception e) {
  }
  if ((foldersCount + entriesCount) > 0) {
    return themeDisplay.getPathThemeImages() + "/common/folder_full_document.png";
  }
  return themeDisplay.getPathThemeImages() + "/common/folder_empty.png";
}
