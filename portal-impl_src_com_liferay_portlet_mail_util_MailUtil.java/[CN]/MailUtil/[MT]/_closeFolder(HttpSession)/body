{
  IMAPFolder folder=(IMAPFolder)ses.getAttribute(WebKeys.MAIL_FOLDER);
  if ((folder != null) && folder.isOpen()) {
    try {
      folder.close(false);
    }
 catch (    MessagingException me) {
      if (_log.isWarnEnabled()) {
        _log.warn(me);
      }
    }
    ses.removeAttribute(WebKeys.MAIL_FOLDER);
  }
}
