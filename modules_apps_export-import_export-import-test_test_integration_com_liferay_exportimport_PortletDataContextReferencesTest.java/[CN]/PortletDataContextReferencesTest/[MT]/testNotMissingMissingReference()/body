{
  Element bookmarksEntryElement=_portletDataContext.getExportDataElement(_bookmarksEntry);
  _portletDataContext.addReferenceElement(_bookmarksEntry,bookmarksEntryElement,_bookmarksFolder,PortletDataContext.REFERENCE_TYPE_PARENT,false);
  _portletDataContext.addReferenceElement(_bookmarksEntry,bookmarksEntryElement,_bookmarksFolder,PortletDataContext.REFERENCE_TYPE_PARENT,true);
  Element missingReferencesElement=_portletDataContext.getMissingReferencesElement();
  List<Element> missingReferenceElements=missingReferencesElement.elements();
  Assert.assertEquals(0,missingReferenceElements.size());
}
