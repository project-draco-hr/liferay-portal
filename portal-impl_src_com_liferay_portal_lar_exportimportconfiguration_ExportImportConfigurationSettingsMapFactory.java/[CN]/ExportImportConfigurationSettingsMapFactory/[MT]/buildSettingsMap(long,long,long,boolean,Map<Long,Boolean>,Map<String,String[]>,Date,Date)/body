{
  Map<String,Serializable> settingsMap=new HashMap<String,Serializable>();
  if (endDate != null) {
    settingsMap.put("endDate",endDate);
  }
  if ((layoutIdMap != null) && layoutIdMap.isEmpty()) {
    HashMap<Long,Boolean> serializableLayoutIdMap=new HashMap<Long,Boolean>(layoutIdMap);
    settingsMap.put("layoutIds",serializableLayoutIdMap);
  }
  if (parameterMap != null) {
    HashMap<String,String[]> serializableParameterMap=new HashMap<String,String[]>(parameterMap);
    settingsMap.put("parameterMap",serializableParameterMap);
  }
  settingsMap.put("privateLayout",privateLayout);
  settingsMap.put("sourceGroupId",sourceGroupId);
  if (startDate != null) {
    settingsMap.put("startDate",startDate);
  }
  if (targetGroupId > 0) {
    settingsMap.put("targetGroupId",targetGroupId);
  }
  settingsMap.put("userId",userId);
  return settingsMap;
}
