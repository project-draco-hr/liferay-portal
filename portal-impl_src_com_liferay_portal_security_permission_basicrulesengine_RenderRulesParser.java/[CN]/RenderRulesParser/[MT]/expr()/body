{
  returnAST=null;
  ASTPair currentAST=new ASTPair();
  AST expr_AST=null;
  try {
    orexp();
    astFactory.addASTChild(currentAST,returnAST);
    expr_AST=(AST)currentAST.root;
  }
 catch (  RecognitionException ex) {
    reportError(ex);
    recover(ex,_tokenSet_0);
  }
  returnAST=expr_AST;
}
