{
  try {
    long imageId=getImageId(req);
    Image image=ImageLocalUtil.getImage(imageId);
    if (image == null) {
      return -1;
    }
    Date modifiedDate=image.getModifiedDate();
    if (modifiedDate == null) {
      return -1;
    }
    return (modifiedDate.getTime() / 1000) * 1000;
  }
 catch (  Exception e) {
    _log.error(e,e);
    return -1;
  }
}
