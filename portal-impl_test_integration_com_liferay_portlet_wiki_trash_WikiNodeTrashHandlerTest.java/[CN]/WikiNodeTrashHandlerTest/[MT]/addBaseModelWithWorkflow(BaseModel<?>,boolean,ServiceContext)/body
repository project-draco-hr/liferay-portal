{
  serviceContext=(ServiceContext)serviceContext.clone();
  serviceContext.setWorkflowAction(WorkflowConstants.ACTION_PUBLISH);
  String title=getSearchKeywords();
  title+=RandomTestUtil.randomString(_NODE_NAME_MAX_LENGTH - title.length());
  return WikiNodeLocalServiceUtil.addNode(TestPropsValues.getUserId(),title,RandomTestUtil.randomString(),serviceContext);
}
