{
  serviceContext=(ServiceContext)serviceContext.clone();
  serviceContext.setWorkflowAction(WorkflowConstants.ACTION_PUBLISH);
  String randomTitle=ServiceTestUtil.randomString(_NODE_NAME_MAX_LENGTH - getSearchKeywords().length());
  String nodeTitle=getSearchKeywords().concat(randomTitle);
  return WikiNodeLocalServiceUtil.addNode(TestPropsValues.getUserId(),nodeTitle,ServiceTestUtil.randomString(),serviceContext);
}
