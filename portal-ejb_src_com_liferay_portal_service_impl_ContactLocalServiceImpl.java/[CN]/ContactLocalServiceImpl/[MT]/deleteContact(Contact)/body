{
  AddressLocalServiceUtil.deleteAddresses(contact.getCompanyId(),Contact.class.getName(),contact.getContactId());
  EmailAddressLocalServiceUtil.deleteEmailAddresses(contact.getCompanyId(),Contact.class.getName(),contact.getContactId());
  PhoneLocalServiceUtil.deletePhones(contact.getCompanyId(),Contact.class.getName(),contact.getContactId());
  WebsiteLocalServiceUtil.deleteWebsites(contact.getCompanyId(),Contact.class.getName(),contact.getContactId());
  ContactUtil.remove(contact.getContactId());
}
