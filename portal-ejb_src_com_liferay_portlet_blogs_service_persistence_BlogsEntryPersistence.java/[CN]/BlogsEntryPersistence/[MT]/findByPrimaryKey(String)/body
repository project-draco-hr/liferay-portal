{
  Session session=null;
  try {
    session=openSession();
    BlogsEntry blogsEntry=(BlogsEntry)session.get(BlogsEntry.class,entryId);
    if (blogsEntry == null) {
      _log.warn("No BlogsEntry exists with the primary key " + entryId.toString());
      throw new NoSuchEntryException("No BlogsEntry exists with the primary key " + entryId.toString());
    }
    return blogsEntry;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
