{
  Session session=null;
  try {
    session=openSession();
    BlogsEntry blogsEntry=(BlogsEntry)session.get(BlogsEntryImpl.class,new Long(entryId));
    if (blogsEntry == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No BlogsEntry exists with the primary key " + entryId);
      }
      throw new NoSuchEntryException("No BlogsEntry exists with the primary key " + entryId);
    }
    return remove(blogsEntry);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
