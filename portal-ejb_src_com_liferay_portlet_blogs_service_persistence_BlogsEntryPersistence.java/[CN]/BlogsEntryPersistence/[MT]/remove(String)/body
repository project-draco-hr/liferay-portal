{
  Session session=null;
  try {
    session=openSession();
    BlogsEntryHBM blogsEntryHBM=(BlogsEntryHBM)session.get(BlogsEntryHBM.class,entryId);
    if (blogsEntryHBM == null) {
      _log.warn("No BlogsEntry exists with the primary key " + entryId.toString());
      throw new NoSuchEntryException("No BlogsEntry exists with the primary key " + entryId.toString());
    }
    session.delete(blogsEntryHBM);
    session.flush();
    return BlogsEntryHBMUtil.model(blogsEntryHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
