{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  TrashVersion newTrashVersion=addTrashVersion();
  _persistence.clearCache();
  TrashVersion existingTrashVersion=_persistence.findByPrimaryKey(newTrashVersion.getPrimaryKey());
  Assert.assertEquals(existingTrashVersion.getClassNameId(),ReflectionTestUtil.invoke(existingTrashVersion,"getOriginalClassNameId",new Class<?>[0]));
  Assert.assertEquals(existingTrashVersion.getClassPK(),ReflectionTestUtil.invoke(existingTrashVersion,"getOriginalClassPK",new Class<?>[0]));
}
