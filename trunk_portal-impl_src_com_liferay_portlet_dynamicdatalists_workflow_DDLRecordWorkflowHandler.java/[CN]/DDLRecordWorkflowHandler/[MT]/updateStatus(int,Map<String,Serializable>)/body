{
  long userId=GetterUtil.getLong((String)workflowContext.get(WorkflowConstants.CONTEXT_USER_ID));
  long recordId=GetterUtil.getLong((String)workflowContext.get(WorkflowConstants.CONTEXT_ENTRY_CLASS_PK));
  ServiceContext serviceContext=(ServiceContext)workflowContext.get("serviceContext");
  return DDLRecordLocalServiceUtil.updateStatus(userId,recordId,status,serviceContext);
}
