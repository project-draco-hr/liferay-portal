{
  selenium.select("_26_id",RuntimeVariables.replace("label=Chinese (Taiwan) to English"));
  selenium.type("_26_text",RuntimeVariables.replace("\u6211\u7684\u540d\u5b57\u662fLiferay \u8b6f\u8005, \u6d41\u5229\u5b8c\u5168\u6210\u529f6 \u901a\u4fe1\u7684\u767e\u842c\u500b\u5f62\u5f0f\u3002"));
  selenium.click(RuntimeVariables.replace("//input[@value='Translate']"));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isTextPresent("My name is the Liferay translator, fluent completely successful 6 correspondence 1,000,000 forms. ")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
