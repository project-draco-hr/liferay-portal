{
  Map<String,Object> parameters=new HashMap<>();
  parameters.put("inline",GetterUtil.getBoolean(ddmFormField.getProperty("inline")));
  parameters.put("showAsSwitcher",GetterUtil.getBoolean(ddmFormField.getProperty("showAsSwitcher")));
  parameters.put("options",getOptions(ddmFormField,ddmFormFieldRenderingContext));
  parameters.put("value",getValue(ddmFormField,ddmFormFieldRenderingContext));
  return parameters;
}
