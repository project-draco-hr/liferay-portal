{
  List<FileEntry> blogsEntryAttachments=new ArrayList<>();
  Pattern pattern=Pattern.compile(EditorConstants.DATA_IMAGE_ID_ATTRIBUTE + "=.(\\d+)");
  Matcher matcher=pattern.matcher(content);
  while (matcher.find()) {
    long fileEntryId=GetterUtil.getLong(matcher.group(1));
    FileEntry blogEntryAttachment=PortletFileRepositoryUtil.getPortletFileEntry(fileEntryId);
    blogsEntryAttachments.add(blogEntryAttachment);
  }
  return blogsEntryAttachments;
}
