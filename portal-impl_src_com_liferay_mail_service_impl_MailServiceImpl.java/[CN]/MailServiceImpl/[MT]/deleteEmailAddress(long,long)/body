{
  if (_log.isDebugEnabled()) {
    _log.debug("deleteEmailAddress");
  }
  MethodWrapper methodWrapper=new MethodWrapper(PropsValues.MAIL_HOOK_IMPL,"deleteEmailAddress",new Object[]{new LongWrapper(companyId),new LongWrapper(userId)});
