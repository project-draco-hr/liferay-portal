{
  if (_log.isDebugEnabled()) {
    _log.debug("addUser");
  }
  MethodWrapper methodWrapper=new MethodWrapper(PropsValues.MAIL_HOOK_IMPL,"addUser",new Object[]{new LongWrapper(companyId),new LongWrapper(userId),password,firstName,middleName,lastName,emailAddress});
  MessageBusUtil.sendMessage(DestinationNames.MAIL,methodWrapper);
}
