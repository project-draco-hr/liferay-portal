{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Asset Publisher Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("AP Setup Test Entry"));
  assertTrue(selenium.isTextPresent("AP Setup Test Bookmark"));
  assertTrue(selenium.isTextPresent("AP Setup Test Document"));
  assertTrue(selenium.isTextPresent("AP Setup Test Article"));
  assertTrue(selenium.isTextPresent("AP Setup MB Test Thread"));
  assertTrue(selenium.isTextPresent("AP Setup Test Wiki Page"));
  assertTrue(selenium.isElementPresent("//img[@alt='Edit Image']"));
}
