{
  ServiceACProfile serviceACProfile=findByPrimaryKey(serviceACProfileId);
  Session session=null;
  try {
    session=openSession();
    ServiceACProfile[] array=new ServiceACProfileImpl[3];
    array[0]=getByCompanyId_PrevAndNext(session,serviceACProfile,companyId,orderByComparator,true);
    array[1]=serviceACProfile;
    array[2]=getByCompanyId_PrevAndNext(session,serviceACProfile,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
