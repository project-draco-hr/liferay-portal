{
  Map<Locale,String> map=LocalizationUtil.getLocalizationMap(preferences,"emailFileEntryUpdatedBody");
  Locale defaultLocale=LocaleUtil.getSiteDefault();
  String defaultValue=map.get(defaultLocale);
  if (Validator.isNotNull(defaultValue)) {
    return map;
  }
  map.put(defaultLocale,ContentUtil.get(PropsUtil.get(PropsKeys.DL_EMAIL_FILE_ENTRY_UPDATED_BODY)));
  return map;
}
