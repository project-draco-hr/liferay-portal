{
  StringBundler sb=new StringBundler(5);
  if (max) {
    sb.append("max-height: ");
  }
 else {
    sb.append("height: ");
  }
  height=height + (2 * margin);
  sb.append(height);
  if (max) {
    sb.append("px; max-width: ");
  }
 else {
    sb.append("px; width: ");
  }
  width=width + (2 * margin);
  sb.append(width);
  sb.append("px;");
  return sb.toString();
}
