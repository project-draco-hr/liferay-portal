{
  List<Subscription> folderSubscriptions=SubscriptionLocalServiceUtil.getUserSubscriptions(userId,Folder.class.getName());
  Set<Long> folderSubscriptionClassPKs=new HashSet<Long>(folderSubscriptions.size());
  for (  Subscription subscription : folderSubscriptions) {
    folderSubscriptionClassPKs.add(subscription.getClassPK());
  }
  return folderSubscriptionClassPKs;
}
