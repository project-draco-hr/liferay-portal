{
  Map<Locale,String> map=LocalizationUtil.getLocalizationMap(preferences,"emailFileEntryAddedSubject");
  Locale defaultLocale=LocaleUtil.getDefault();
  String defaultValue=map.get(defaultLocale);
  if (Validator.isNotNull(defaultValue)) {
    return map;
  }
  map.put(defaultLocale,ContentUtil.get(PropsUtil.get(PropsKeys.DL_EMAIL_FILE_ENTRY_ADDED_SUBJECT)));
  return map;
}
