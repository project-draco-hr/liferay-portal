{
  String uniqueFileName=fileName;
  for (int i=1; ; i++) {
    try {
      DLAppLocalServiceUtil.getFileEntry(groupId,folderId,uniqueFileName);
      uniqueFileName=FileUtil.appendParentheticalSuffix(fileName,String.valueOf(i));
    }
 catch (    Exception e) {
      break;
    }
  }
  return uniqueFileName;
}
