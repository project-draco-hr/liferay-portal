{
  if (StringUtil.equalsIgnoreCase(extension,"doc") || StringUtil.equalsIgnoreCase(extension,"docx") || StringUtil.equalsIgnoreCase(extension,"dot")|| StringUtil.equalsIgnoreCase(extension,"ppt")|| StringUtil.equalsIgnoreCase(extension,"pptx")|| StringUtil.equalsIgnoreCase(extension,"xls")|| StringUtil.equalsIgnoreCase(extension,"xlsx")) {
    return true;
  }
  return false;
}
