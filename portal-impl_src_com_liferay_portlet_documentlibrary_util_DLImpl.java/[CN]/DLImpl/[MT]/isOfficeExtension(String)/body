{
  if (extension.equalsIgnoreCase("doc") || extension.equalsIgnoreCase("docx") || extension.equalsIgnoreCase("dot")|| extension.equalsIgnoreCase("ppt")|| extension.equalsIgnoreCase("pptx")|| extension.equalsIgnoreCase("xls")|| extension.equalsIgnoreCase("xlsx")) {
    return true;
  }
  return false;
}
