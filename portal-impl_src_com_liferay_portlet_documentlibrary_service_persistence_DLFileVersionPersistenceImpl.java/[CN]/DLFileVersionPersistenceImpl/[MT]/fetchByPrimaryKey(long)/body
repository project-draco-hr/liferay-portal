{
  DLFileVersion dlFileVersion=(DLFileVersion)EntityCacheUtil.getResult(DLFileVersionModelImpl.ENTITY_CACHE_ENABLED,DLFileVersionImpl.class,fileVersionId);
  if (dlFileVersion == _nullDLFileVersion) {
    return null;
  }
  if (dlFileVersion == null) {
    Session session=null;
    try {
      session=openSession();
      dlFileVersion=(DLFileVersion)session.get(DLFileVersionImpl.class,Long.valueOf(fileVersionId));
      if (dlFileVersion != null) {
        cacheResult(dlFileVersion);
      }
 else {
        EntityCacheUtil.putResult(DLFileVersionModelImpl.ENTITY_CACHE_ENABLED,DLFileVersionImpl.class,fileVersionId,_nullDLFileVersion);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(DLFileVersionModelImpl.ENTITY_CACHE_ENABLED,DLFileVersionImpl.class,fileVersionId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return dlFileVersion;
}
