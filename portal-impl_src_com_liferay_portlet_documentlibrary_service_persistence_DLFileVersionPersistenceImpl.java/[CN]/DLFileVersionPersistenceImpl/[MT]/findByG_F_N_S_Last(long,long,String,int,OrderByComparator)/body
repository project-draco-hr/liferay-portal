{
  int count=countByG_F_N_S(groupId,folderId,name,status);
  List<DLFileVersion> list=findByG_F_N_S(groupId,folderId,name,status,count - 1,count,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(9);
    msg.append("No DLFileVersion exists with the key {");
    msg.append("groupId=" + groupId);
    msg.append(", ");
    msg.append("folderId=" + folderId);
    msg.append(", ");
    msg.append("name=" + name);
    msg.append(", ");
    msg.append("status=" + status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchFileVersionException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
