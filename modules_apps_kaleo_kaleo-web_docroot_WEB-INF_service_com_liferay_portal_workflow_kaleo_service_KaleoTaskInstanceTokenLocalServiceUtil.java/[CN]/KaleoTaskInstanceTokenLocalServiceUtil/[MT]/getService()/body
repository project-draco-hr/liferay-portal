{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),KaleoTaskInstanceTokenLocalService.class.getName());
    if (invokableLocalService instanceof KaleoTaskInstanceTokenLocalService) {
      _service=(KaleoTaskInstanceTokenLocalService)invokableLocalService;
    }
 else {
      _service=new KaleoTaskInstanceTokenLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(KaleoTaskInstanceTokenLocalServiceUtil.class,"_service");
  }
  return _service;
}
