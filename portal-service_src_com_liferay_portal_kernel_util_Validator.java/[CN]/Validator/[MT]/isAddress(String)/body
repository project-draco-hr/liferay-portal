{
  if (isNull(address)) {
    return false;
  }
  String[] tokens=address.split(StringPool.AT);
  if (tokens.length != 2) {
    return false;
  }
  for (  String token : tokens) {
    for (    char c : token.toCharArray()) {
      if (Character.isWhitespace(c)) {
        return false;
      }
    }
  }
  return true;
}
