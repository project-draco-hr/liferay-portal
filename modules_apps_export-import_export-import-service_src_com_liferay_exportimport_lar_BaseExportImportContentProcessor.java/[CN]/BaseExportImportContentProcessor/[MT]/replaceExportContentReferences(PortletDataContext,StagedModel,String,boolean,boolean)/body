{
  content=replaceExportDLReferences(portletDataContext,stagedModel,content,exportReferencedContent);
  content=replaceExportLayoutReferences(portletDataContext,content);
  content=replaceExportLinksToLayouts(portletDataContext,stagedModel,content);
  if (escapeContent) {
    content=StringUtil.replace(content,StringPool.AMPERSAND_ENCODED,StringPool.AMPERSAND);
  }
  return content;
}
