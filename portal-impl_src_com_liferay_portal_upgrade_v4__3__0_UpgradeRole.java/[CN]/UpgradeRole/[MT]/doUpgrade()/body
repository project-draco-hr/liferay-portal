{
  PKUpgradeColumnImpl upgradePKColumn=new PKUpgradeColumnImpl("roleId",true);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(RoleTable.TABLE_NAME,RoleTable.TABLE_COLUMNS,upgradePKColumn);
  upgradeTable.setCreateSQL(RoleTable.TABLE_SQL_CREATE);
  upgradeTable.updateTable();
  ValueMapper roleIdMapper=upgradePKColumn.getValueMapper();
  AvailableMappersUtil.setRoleIdMapper(roleIdMapper);
}
