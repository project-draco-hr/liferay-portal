{
  PKUpgradeColumnImpl pkUpgradeColumn=new PKUpgradeColumnImpl("roleId",true);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(RoleImpl.TABLE_NAME,RoleImpl.TABLE_COLUMNS,pkUpgradeColumn);
  upgradeTable.updateTable();
  ValueMapper roleIdMapper=pkUpgradeColumn.getValueMapper();
  AvailableMappersUtil.setRoleIdMapper(roleIdMapper);
  runSQL(_UPGRADE_SCHEMA);
}
