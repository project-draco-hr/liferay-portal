{
  List<Integer> list=Arrays.asList(1,2,3,4);
  Assert.assertEquals(Arrays.asList(1,2,3),ListUtil.subList(list,-1,3));
  Assert.assertEquals(list,ListUtil.subList(list,-1,5));
  Assert.assertEquals(list,ListUtil.subList(list,-1,-1));
  Assert.assertEquals(Arrays.asList(2,3),ListUtil.subList(list,1,3));
  Assert.assertSame(Collections.emptyList(),ListUtil.subList(list,1,1));
  Assert.assertSame(Collections.emptyList(),ListUtil.subList(list,2,1));
}
