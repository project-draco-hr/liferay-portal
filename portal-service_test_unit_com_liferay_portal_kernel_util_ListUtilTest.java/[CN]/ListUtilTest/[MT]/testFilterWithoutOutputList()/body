{
  List<String> expectedOutputList=new ArrayList<String>();
  expectedOutputList.add("b");
  PredicateFilter<String> predicateFilter=new PredicateFilter<String>(){
    @Override public boolean filter(    String string){
      return string.equals("b");
    }
  }
;
  List<String> inputList=new ArrayList<String>();
  inputList.add("a");
  inputList.add("b");
  inputList.add("c");
  Collection<String> actualOutputList=ListUtil.filter(inputList,predicateFilter);
  Assert.assertEquals(expectedOutputList,actualOutputList);
}
