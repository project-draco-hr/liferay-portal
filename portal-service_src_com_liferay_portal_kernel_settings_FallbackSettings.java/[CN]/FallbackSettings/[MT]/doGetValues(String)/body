{
  String[] values=parentSettings.getValues(key,null);
  if (values != null) {
    return values;
  }
  String[] fallbackKeysArray=_fallbackKeys.get(key);
  for (  String fallbackKey : fallbackKeysArray) {
    values=parentSettings.getValues(fallbackKey,null);
    if (values != null) {
      return values;
    }
  }
  return null;
}
