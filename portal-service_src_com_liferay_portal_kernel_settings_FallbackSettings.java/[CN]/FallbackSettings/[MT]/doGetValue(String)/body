{
  String value=parentSettings.getValue(key,null);
  if (value != null) {
    return value;
  }
  String[] fallbackKeysArray=_fallbackKeys.get(key);
  for (  String fallbackKey : fallbackKeysArray) {
    value=parentSettings.getValue(fallbackKey,null);
    if (value != null) {
      return value;
    }
  }
  return null;
}
