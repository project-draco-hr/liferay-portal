{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(itemId));
  }
  com.liferay.portlet.shopping.model.ShoppingItem shoppingItem=getPersistence().remove(itemId);
  if (listener != null) {
    listener.onAfterRemove(shoppingItem);
  }
  return shoppingItem;
}
