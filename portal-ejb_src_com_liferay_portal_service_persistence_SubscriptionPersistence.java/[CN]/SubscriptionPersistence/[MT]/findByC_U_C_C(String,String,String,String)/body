{
  Subscription subscription=fetchByC_U_C_C(companyId,userId,className,classPK);
  if (subscription == null) {
    String msg="No Subscription exists with the key ";
    msg+=StringPool.OPEN_CURLY_BRACE;
    msg+="companyId=";
    msg+=companyId;
    msg+=", ";
    msg+="userId=";
    msg+=userId;
    msg+=", ";
    msg+="className=";
    msg+=className;
    msg+=", ";
    msg+="classPK=";
    msg+=classPK;
    msg+=StringPool.CLOSE_CURLY_BRACE;
    if (_log.isWarnEnabled()) {
      _log.warn(msg);
    }
    throw new NoSuchSubscriptionException(msg);
  }
  return subscription;
}
