{
  Session session=null;
  try {
    session=openSession();
    SubscriptionHBM subscriptionHBM=(SubscriptionHBM)session.get(SubscriptionHBM.class,subscriptionId);
    if (subscriptionHBM == null) {
      _log.warn("No Subscription exists with the primary key " + subscriptionId.toString());
      throw new NoSuchSubscriptionException("No Subscription exists with the primary key " + subscriptionId.toString());
    }
    return SubscriptionHBMUtil.model(subscriptionHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
