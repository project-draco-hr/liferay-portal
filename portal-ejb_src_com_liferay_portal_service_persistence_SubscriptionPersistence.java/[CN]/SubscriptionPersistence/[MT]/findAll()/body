{
  Session session=null;
  try {
    session=openSession();
    StringBuffer query=new StringBuffer();
    query.append("FROM Subscription IN CLASS com.liferay.portal.service.persistence.SubscriptionHBM ");
    Query q=session.createQuery(query.toString());
    Iterator itr=q.iterate();
    List list=new ArrayList();
    while (itr.hasNext()) {
      SubscriptionHBM subscriptionHBM=(SubscriptionHBM)itr.next();
      list.add(SubscriptionHBMUtil.model(subscriptionHBM));
    }
    return list;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
