{
  JournalStructure structure1=(JournalStructure)obj1;
  JournalStructure structure2=(JournalStructure)obj2;
  int value=0;
  if (structure1.getId() < structure2.getId()) {
    value=-1;
  }
 else   if (structure1.getId() > structure2.getId()) {
    value=1;
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
