{
  FileUtil.copyDirectory(_getResourcesDir() + "css",_CURRENT_DIR + "/test-results/css");
  String content=(String)_javascriptExecutor.executeScript("return document.getElementsByTagName('html')[0].outerHTML;");
  FileUtil.write(_CURRENT_DIR + "/test-results/html/index.html",content);
  if (isLoggerStarted()) {
    _webDriver.quit();
    _webDriver=null;
  }
}
