{
  if (!isLoggerStarted()) {
    return;
  }
  String escapedAttributeName=StringEscapeUtils.escapeEcmaScript(attributeName);
  String escapedAttributeValue=StringEscapeUtils.escapeEcmaScript(attributeValue);
  _javascriptExecutor.executeScript("setAttribute(" + loggerElement.getID() + ", '"+ escapedAttributeName+ "', '"+ escapedAttributeValue+ "');");
}
