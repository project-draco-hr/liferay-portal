{
  String cmd=ParamUtil.getString(actionRequest,Constants.CMD);
  try {
    if (cmd.equals(Constants.ADD) || cmd.equals(Constants.UPDATE)) {
      updateCategory(actionRequest);
    }
 else     if (cmd.equals(Constants.DELETE)) {
      deleteCategories(actionRequest,false);
    }
 else     if (cmd.equals(Constants.MOVE_TO_TRASH)) {
      deleteCategories(actionRequest,true);
    }
 else     if (cmd.equals(Constants.RESTORE)) {
      restoreTrashEntries(actionRequest);
    }
 else     if (cmd.equals(Constants.SUBSCRIBE)) {
      subscribeCategory(actionRequest);
    }
 else     if (cmd.equals(Constants.UNSUBSCRIBE)) {
      unsubscribeCategory(actionRequest);
    }
    sendRedirect(actionRequest,actionResponse);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchCategoryException || e instanceof PrincipalException) {
      SessionErrors.add(actionRequest,e.getClass());
      actionResponse.setRenderParameter("mvcPath","/html/portlet/message_boards/error.jsp");
    }
 else     if (e instanceof CaptchaConfigurationException || e instanceof CaptchaMaxChallengesException || e instanceof CaptchaTextException|| e instanceof CategoryNameException|| e instanceof MailingListEmailAddressException|| e instanceof MailingListInServerNameException|| e instanceof MailingListInUserNameException|| e instanceof MailingListOutEmailAddressException|| e instanceof MailingListOutServerNameException|| e instanceof MailingListOutUserNameException) {
      SessionErrors.add(actionRequest,e.getClass());
    }
 else {
      throw e;
    }
  }
}
