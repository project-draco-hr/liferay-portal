{
  long fileEntryClassNameId=PortalUtil.getClassNameId(DLFileEntry.class);
  long folderClassNameId=PortalUtil.getClassNameId(DLFolder.class);
  runSQL("update WorkflowDefinitionLink set classNameId = " + folderClassNameId + ", typePK = "+ DLFileEntryTypeConstants.ALL_FILE_ENTRY_TYPES_ID+ " where classNameId = "+ fileEntryClassNameId);
}
