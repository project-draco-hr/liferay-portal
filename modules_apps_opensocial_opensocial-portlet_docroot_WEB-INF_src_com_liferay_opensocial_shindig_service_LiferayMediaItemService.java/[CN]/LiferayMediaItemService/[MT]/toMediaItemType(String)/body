{
  String contentType=MimeTypesUtil.getContentType(fileName);
  if (contentType.startsWith("audio")) {
    return Type.AUDIO;
  }
 else   if (contentType.startsWith("image")) {
    return Type.IMAGE;
  }
 else   if (contentType.startsWith("video")) {
    return Type.VIDEO;
  }
 else {
    return null;
  }
}
