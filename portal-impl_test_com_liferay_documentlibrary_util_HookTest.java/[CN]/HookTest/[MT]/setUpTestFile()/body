{
  for (int i=0; i < _TEST_BYTE_ARRAY.length; i++) {
    _TEST_BYTE_ARRAY[i]=(byte)(i % 128);
  }
  FileWriter fileWriter=null;
  try {
    fileWriter=new FileWriter(_TEST_FILE);
    fileWriter.write(new String(_TEST_BYTE_ARRAY));
    fileWriter.flush();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
 finally {
    try {
      fileWriter.close();
    }
 catch (    IOException ioe) {
      ioe.printStackTrace();
    }
  }
}
