{
synchronized (_eventMap) {
    SyncFile syncFile=(SyncFile)event.getParameterValue("syncFile");
    if (syncFile == null) {
      return;
    }
    Set<Event> events=_eventMap.get(syncFile.getSyncFileId());
    if (events == null) {
      events=new HashSet<>();
      _eventMap.put(syncFile.getSyncFileId(),events);
    }
    events.add(event);
  }
}
