{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  KaleoCondition kaleoCondition : kaleoConditions) {
    EntityCacheUtil.removeResult(KaleoConditionModelImpl.ENTITY_CACHE_ENABLED,KaleoConditionImpl.class,kaleoCondition.getPrimaryKey());
    clearUniqueFindersCache(kaleoCondition);
  }
}
