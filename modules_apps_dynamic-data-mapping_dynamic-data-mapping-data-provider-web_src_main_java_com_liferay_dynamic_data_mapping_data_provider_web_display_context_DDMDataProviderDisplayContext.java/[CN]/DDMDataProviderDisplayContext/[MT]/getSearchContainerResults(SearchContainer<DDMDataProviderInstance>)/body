{
  DDMDataProviderSearchTerms searchTerms=(DDMDataProviderSearchTerms)searchContainer.getSearchTerms();
  if (searchTerms.isAdvancedSearch()) {
    return _ddmDataProviderInstanceService.search(_ddmDataProviderRequestHelper.getCompanyId(),new long[]{_ddmDataProviderRequestHelper.getScopeGroupId()},searchTerms.getName(),searchTerms.getDescription(),searchTerms.isAndOperator(),searchContainer.getStart(),searchContainer.getEnd(),searchContainer.getOrderByComparator());
  }
 else {
    return _ddmDataProviderInstanceService.search(_ddmDataProviderRequestHelper.getCompanyId(),new long[]{_ddmDataProviderRequestHelper.getScopeGroupId()},searchTerms.getKeywords(),searchContainer.getStart(),searchContainer.getEnd(),searchContainer.getOrderByComparator());
  }
}
