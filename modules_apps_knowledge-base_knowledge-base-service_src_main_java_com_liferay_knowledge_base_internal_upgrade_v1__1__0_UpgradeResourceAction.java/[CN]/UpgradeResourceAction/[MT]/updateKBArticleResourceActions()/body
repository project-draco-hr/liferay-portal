{
  runSQL("delete from ResourceAction where name = " + "'com.liferay.knowledgebase.model.KBArticle'");
  runSQL("update ResourceAction set name = " + "'com.liferay.knowledgebase.model.KBArticle' where name = " + "'com.liferay.knowledgebase.model.Article'");
  runSQL("update ResourceAction set actionId = 'MOVE_KB_ARTICLE' where " + "name = 'com.liferay.knowledgebase.model.KBArticle' and " + "actionId = 'MOVE'");
}
