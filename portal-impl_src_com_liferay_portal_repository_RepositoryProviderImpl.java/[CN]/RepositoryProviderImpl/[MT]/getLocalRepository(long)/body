{
  LocalRepository localRepository=_localRepositoriesByRepositoryId.get(repositoryId);
  if (localRepository != null) {
    return localRepository;
  }
  localRepository=_repositoryFactory.createLocalRepository(repositoryId);
  checkRepository(repositoryId);
  checkRepositoryAccess(repositoryId);
  _localRepositoriesByRepositoryId.put(repositoryId,localRepository);
  return localRepository;
}
