{
  try {
    com.liferay.portlet.shopping.model.ShoppingCoupon returnValue=ShoppingCouponServiceUtil.addCoupon(plid,couponId,autoCouponId,name,description,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,endDateHour,endDateMinute,neverExpire,active,limitCategories,limitSkus,minOrder,discount,discountType);
