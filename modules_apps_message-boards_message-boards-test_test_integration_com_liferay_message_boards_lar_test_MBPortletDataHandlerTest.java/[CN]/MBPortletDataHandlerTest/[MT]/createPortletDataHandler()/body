{
  try {
    Registry registry=RegistryUtil.getRegistry();
    Collection<PortletDataHandler> services=registry.getServices(PortletDataHandler.class,"(javax.portlet.name=" + PortletKeys.MESSAGE_BOARDS + ")");
    Iterator<PortletDataHandler> iterator=services.iterator();
    return iterator.next();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
