{
  if (_hiddenVariables != null) {
    return _hiddenVariables;
  }
  _hiddenVariables=_iFramePortletInstanceConfiguration.hiddenVariables();
  if (Validator.isNull(_hiddenVariables)) {
    _hiddenVariables=StringUtil.merge(_iFrameConfiguration.hiddenVariables(),StringPool.SEMICOLON);
  }
  return _hiddenVariables;
}
