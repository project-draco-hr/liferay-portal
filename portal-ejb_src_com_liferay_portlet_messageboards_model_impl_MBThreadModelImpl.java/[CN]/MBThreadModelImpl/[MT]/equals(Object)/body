{
  if (obj == null) {
    return false;
  }
  MBThreadImpl mbThread=null;
  try {
    mbThread=(MBThreadImpl)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  String pk=mbThread.getPrimaryKey();
  if (getPrimaryKey().equals(pk)) {
    return true;
  }
 else {
    return false;
  }
}
