{
  if (((lastPostByUserId == null) && (_lastPostByUserId != null)) || ((lastPostByUserId != null) && (_lastPostByUserId == null)) || ((lastPostByUserId != null) && (_lastPostByUserId != null) && !lastPostByUserId.equals(_lastPostByUserId))) {
    if (!XSS_ALLOW_LASTPOSTBYUSERID) {
      lastPostByUserId=XSSUtil.strip(lastPostByUserId);
    }
    _lastPostByUserId=lastPostByUserId;
  }
}
