{
  if (stepJsonNode == null) {
    return;
  }
  String name=getString(stepJsonNode,"name");
  SyncSystemTestUtil.addUser(name,true,_syncAccount.getSyncAccountId());
  String filePathName=FileUtil.getFilePathName(System.getProperty("user.home"),"liferay-sync-test",name);
  SyncAccount syncAccount=SyncAccountService.addSyncAccount(filePathName,name + "@liferay.com",1,name,"test",5,null,false,"http://localhost:8080");
  SyncAccountService.activateSyncAccount(syncAccount.getSyncAccountId(),false);
  _syncAccountIds.put(name,syncAccount.getSyncAccountId());
}
