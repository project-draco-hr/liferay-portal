{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDLRecordSet.class,DDLRecordSet.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("recordSetId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("recordSetId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
