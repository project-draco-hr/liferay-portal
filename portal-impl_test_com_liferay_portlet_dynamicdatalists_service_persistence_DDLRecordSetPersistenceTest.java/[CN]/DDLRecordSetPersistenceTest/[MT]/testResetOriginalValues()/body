{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  DDLRecordSet newDDLRecordSet=addDDLRecordSet();
  _persistence.clearCache();
  DDLRecordSetModelImpl existingDDLRecordSetModelImpl=(DDLRecordSetModelImpl)_persistence.findByPrimaryKey(newDDLRecordSet.getPrimaryKey());
  assertTrue(Validator.equals(existingDDLRecordSetModelImpl.getUuid(),existingDDLRecordSetModelImpl.getOriginalUuid()));
  assertEquals(existingDDLRecordSetModelImpl.getGroupId(),existingDDLRecordSetModelImpl.getOriginalGroupId());
}
