{
  JavaScriptExecutorConfiguration javaScriptExecutorConfiguration=Configurable.createConfigurable(JavaScriptExecutorConfiguration.class,properties);
  String[] forbiddenClasses=StringUtil.split(javaScriptExecutorConfiguration.forbiddenClasses(),StringPool.COMMA);
  _forbiddenClasses=new HashSet<>(Arrays.asList(forbiddenClasses));
}
