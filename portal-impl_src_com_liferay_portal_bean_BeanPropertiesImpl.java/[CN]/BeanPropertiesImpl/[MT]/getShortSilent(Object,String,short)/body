{
  Short beanValue=null;
  if (bean != null) {
    try {
      Object value=BeanUtil.getProperty(bean,param);
      beanValue=Convert.toShort(value,defaultValue);
    }
 catch (    Exception e) {
    }
  }
  if (beanValue == null) {
    return defaultValue;
  }
 else {
    return beanValue.shortValue();
  }
}
