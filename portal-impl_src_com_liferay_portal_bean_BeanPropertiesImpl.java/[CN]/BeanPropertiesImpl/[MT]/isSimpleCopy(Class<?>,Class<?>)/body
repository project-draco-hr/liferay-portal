{
  return toClazz.isAssignableFrom(fromClazz) && (fromClazz.isPrimitive() || fromClazz.equals(String.class) || fromClazz.equals(Long.class)|| fromClazz.equals(Integer.class)|| fromClazz.equals(Double.class)|| fromClazz.equals(Float.class)|| fromClazz.equals(Boolean.class)|| fromClazz.equals(Locale.class));
}
