{
  if (!targetClazz.isAssignableFrom(sourceClazz)) {
    return false;
  }
  if (sourceClazz.isPrimitive() || sourceClazz.equals(String.class) || sourceClazz.equals(Long.class)|| sourceClazz.equals(Integer.class)|| sourceClazz.equals(Double.class)|| sourceClazz.equals(Float.class)|| sourceClazz.equals(Boolean.class)|| sourceClazz.equals(Locale.class)) {
    return true;
  }
  return false;
}
