{
  Boolean beanValue=null;
  if (bean != null) {
    try {
      beanValue=(Boolean)PropertyUtils.getSimpleProperty(bean,param);
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  if (beanValue == null) {
    return defaultValue;
  }
 else {
    return beanValue.booleanValue();
  }
}
