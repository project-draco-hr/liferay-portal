{
  Registry registry=RegistryUtil.getRegistry();
  RepositoryRegistryPlugin repositoryRegistryPlugin=registry.getService(serviceReference);
  String className=repositoryRegistryPlugin.getClassName();
  if (repositoryRegistryPlugin.isExternalRepository()) {
    _externalRepositoriesClassNames.add(className);
  }
  _repositoryConfigurations.put(className,createRepositoryConfiguration(repositoryRegistryPlugin));
  return repositoryRegistryPlugin;
}
