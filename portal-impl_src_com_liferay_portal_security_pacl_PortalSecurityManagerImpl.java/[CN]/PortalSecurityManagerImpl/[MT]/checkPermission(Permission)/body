{
  boolean loopSupport=false;
  try {
    if ((permission instanceof ReflectPermission) && (permission.getName().equals("suppressAccessChecks")) && (_checkMembersAccessClassLoader.get() != null)) {
      Class<?> stack[]=getClassContext();
      if (_checkMembersAccessClassLoader.get() == stack[2].getClassLoader()) {
        loopSupport=true;
        return;
      }
    }
    java.security.AccessController.checkPermission(permission);
  }
  finally {
    if (!loopSupport) {
      _checkMembersAccessClassLoader.set(null);
    }
  }
}
