{
  List events=getEvents(groupId,cal);
  if (Validator.isNull(type)) {
    return events;
  }
 else {
    events=new ArrayList(events);
    Iterator itr=events.iterator();
    while (itr.hasNext()) {
      CalEvent event=(CalEvent)itr.next();
      if (!event.getType().equals(type)) {
        itr.remove();
      }
    }
    return events;
  }
}
