    sb.append("title=Test\n");
    sb.append("version=1.0");
    String properties=sb.toString();
    zipOutputStream.write(properties.getBytes());
    zipOutputStream.closeEntry();
    zipOutputStream.putNextEntry(new ZipEntry(_SYMBOLIC_NAME.concat("-1.0.0.jar")));
    try (InputStream inputStream=_createJAR();OutputStream outputStream=StreamUtil.uncloseable(zipOutputStream)){
      StreamUtil.transfer(inputStream,outputStream);
    }
     zipOutputStream.closeEntry();
  }
