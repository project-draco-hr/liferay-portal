{
  String className=PortalUtil.getClassName(classNameId);
  ServiceWrapper<DDMStructurePermissionSupport> ddmStructurePermissionSupportServiceWrapper=_ddmStructurePermissionSupportServiceTrackerMap.getService(className);
  if (ddmStructurePermissionSupportServiceWrapper == null) {
    throw new PortalException("The model does not support DDMStructure permission checking " + className);
  }
  return ddmStructurePermissionSupportServiceWrapper;
}
