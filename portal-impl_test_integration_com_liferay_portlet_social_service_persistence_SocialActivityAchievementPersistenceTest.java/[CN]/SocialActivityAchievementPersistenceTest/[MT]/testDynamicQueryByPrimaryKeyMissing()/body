{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SocialActivityAchievement.class,SocialActivityAchievement.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("activityAchievementId",nextLong()));
  List<SocialActivityAchievement> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
