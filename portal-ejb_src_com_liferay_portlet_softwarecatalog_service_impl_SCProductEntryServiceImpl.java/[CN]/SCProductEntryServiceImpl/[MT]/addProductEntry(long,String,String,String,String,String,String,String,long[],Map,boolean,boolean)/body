{
  PortletPermission.check(getPermissionChecker(),plid,PortletKeys.SOFTWARE_CATALOG,ActionKeys.ADD_PRODUCT_ENTRY);
  return SCProductEntryLocalServiceUtil.addProductEntry(getUserId(),plid,name,type,shortDescription,longDescription,pageURL,repoGroupId,repoArtifactId,licenseIds,images,addCommunityPermissions,addGuestPermissions);
}
