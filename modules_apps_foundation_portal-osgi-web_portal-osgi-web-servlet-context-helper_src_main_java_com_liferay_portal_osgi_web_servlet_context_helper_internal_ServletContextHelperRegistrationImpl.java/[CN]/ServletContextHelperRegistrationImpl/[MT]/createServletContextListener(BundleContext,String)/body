{
  Dictionary<String,Object> properties=new Hashtable<>();
  properties.put(HttpWhiteboardConstants.HTTP_WHITEBOARD_CONTEXT_SELECT,createContextSelectFilter(servletContextName));
  properties.put(HttpWhiteboardConstants.HTTP_WHITEBOARD_LISTENER,Boolean.TRUE.toString());
  return bundleContext.registerService(ServletContextListener.class,_customServletContextHelper,properties);
}
