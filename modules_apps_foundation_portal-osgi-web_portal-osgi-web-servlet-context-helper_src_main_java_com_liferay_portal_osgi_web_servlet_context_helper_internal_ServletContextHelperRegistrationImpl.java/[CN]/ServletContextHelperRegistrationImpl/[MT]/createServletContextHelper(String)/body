{
  Dictionary<String,Object> properties=new Hashtable<>();
  properties.put(HttpWhiteboardConstants.HTTP_WHITEBOARD_CONTEXT_NAME,_servletContextName);
  properties.put(HttpWhiteboardConstants.HTTP_WHITEBOARD_CONTEXT_PATH,contextPath);
  properties.put("rtl.required",Boolean.TRUE.toString());
  collectContextInitParams(properties);
  return _bundleContext.registerService(ServletContextHelper.class,_customServletContextHelper,properties);
}
