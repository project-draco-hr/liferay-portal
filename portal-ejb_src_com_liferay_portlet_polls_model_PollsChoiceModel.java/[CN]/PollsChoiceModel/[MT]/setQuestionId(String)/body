{
  if (((questionId == null) && (_questionId != null)) || ((questionId != null) && (_questionId == null)) || ((questionId != null) && (_questionId != null) && !questionId.equals(_questionId))) {
    if (!XSS_ALLOW_QUESTIONID) {
      questionId=XSSUtil.strip(questionId);
    }
    _questionId=questionId;
    setModified(true);
  }
}
