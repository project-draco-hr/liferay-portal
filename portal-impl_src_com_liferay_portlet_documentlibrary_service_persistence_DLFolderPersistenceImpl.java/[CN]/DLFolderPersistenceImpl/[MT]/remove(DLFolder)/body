{
  Session session=null;
  try {
    session=openSession();
    dlFolder=(DLFolder)session.merge(dlFolder);
    session.delete(dlFolder);
    session.flush();
    return dlFolder;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(DLFolder.class.getName());
  }
}
