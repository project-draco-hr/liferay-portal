{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(dlFolder);
    }
  }
  dlFolder=removeImpl(dlFolder);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(dlFolder);
    }
  }
  return dlFolder;
}
