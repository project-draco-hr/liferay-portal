{
  DLFolder dlFolder=(DLFolder)EntityCacheUtil.getResult(DLFolderModelImpl.ENTITY_CACHE_ENABLED,DLFolderImpl.class,folderId);
  if (dlFolder == _nullDLFolder) {
    return null;
  }
  if (dlFolder == null) {
    Session session=null;
    try {
      session=openSession();
      dlFolder=(DLFolder)session.get(DLFolderImpl.class,Long.valueOf(folderId));
      if (dlFolder != null) {
        cacheResult(dlFolder);
      }
 else {
        EntityCacheUtil.putResult(DLFolderModelImpl.ENTITY_CACHE_ENABLED,DLFolderImpl.class,folderId,_nullDLFolder);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(DLFolderModelImpl.ENTITY_CACHE_ENABLED,DLFolderImpl.class,folderId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return dlFolder;
}
