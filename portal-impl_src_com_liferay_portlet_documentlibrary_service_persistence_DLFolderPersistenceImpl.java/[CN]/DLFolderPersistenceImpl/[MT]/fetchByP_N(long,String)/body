{
  String finderClassName=DLFolder.class.getName();
  String finderMethodName="fetchByP_N";
  String[] finderParams=new String[]{Long.class.getName(),String.class.getName()};
  Object[] finderArgs=new Object[]{new Long(parentFolderId),name};
  Object result=FinderCache.getResult(finderClassName,finderMethodName,finderParams,finderArgs,getSessionFactory());
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringMaker query=new StringMaker();
      query.append("FROM com.liferay.portlet.documentlibrary.model.DLFolder WHERE ");
      query.append("parentFolderId = ?");
      query.append(" AND ");
      if (name == null) {
        query.append("name IS NULL");
      }
 else {
        query.append("name = ?");
      }
      query.append(" ");
      query.append("ORDER BY ");
      query.append("parentFolderId ASC").append(", ");
      query.append("name ASC");
      Query q=session.createQuery(query.toString());
      int queryPos=0;
      q.setLong(queryPos++,parentFolderId);
      if (name != null) {
        q.setString(queryPos++,name);
      }
      List list=q.list();
      FinderCache.putResult(finderClassName,finderMethodName,finderParams,finderArgs,list);
      if (list.size() == 0) {
        return null;
      }
 else {
        return (DLFolder)list.get(0);
      }
    }
 catch (    Exception e) {
      throw HibernateUtil.processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    List list=(List)result;
    if (list.size() == 0) {
      return null;
    }
 else {
      return (DLFolder)list.get(0);
    }
  }
}
