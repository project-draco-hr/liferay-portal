{
  List<String[]> mappings=RESTActionsManagerUtil.dumpMappings();
  StringBundler sb=new StringBundler(mappings.size() * 6);
  for (  String[] mapping : mappings) {
    if (mapping[0] == null) {
      sb.append(StringPool.STAR);
    }
 else {
      sb.append(mapping[0]);
    }
    sb.append(CharPool.TAB);
    sb.append(mapping[1]);
    sb.append(" ---> ");
    sb.append(mapping[2]);
    sb.append(CharPool.NEW_LINE);
  }
  response.setContentType(ContentTypes.TEXT_PLAIN);
  response.setHeader(HttpHeaders.CACHE_CONTROL,HttpHeaders.CACHE_CONTROL_NO_CACHE_VALUE);
  PrintWriter printWriter=response.getWriter();
  printWriter.write(sb.toString());
  printWriter.close();
}
