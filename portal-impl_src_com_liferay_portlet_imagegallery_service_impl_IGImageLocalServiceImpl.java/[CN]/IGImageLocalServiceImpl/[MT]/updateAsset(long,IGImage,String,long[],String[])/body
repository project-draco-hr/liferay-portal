{
  Image largeImage=imageLocalService.getImage(image.getLargeImageId());
  if (largeImage == null) {
    return;
  }
  if (contentType == null) {
    contentType=MimeTypesUtil.getContentType(largeImage.getType());
  }
  assetEntryLocalService.updateEntry(userId,image.getGroupId(),IGImage.class.getName(),image.getImageId(),assetCategoryIds,assetTagNames,true,null,null,null,null,contentType,image.getName(),image.getDescription(),null,null,largeImage.getHeight(),largeImage.getWidth(),null,false);
}
