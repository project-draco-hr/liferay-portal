{
  if (image.getFolderId() != folderId) {
    IGFolder oldFolder=igFolderPersistence.findByPrimaryKey(image.getFolderId());
    IGFolder newFolder=igFolderPersistence.fetchByPrimaryKey(folderId);
    if ((newFolder == null) || (oldFolder.getGroupId() != newFolder.getGroupId())) {
      folderId=image.getFolderId();
    }
  }
  return igFolderPersistence.findByPrimaryKey(folderId);
}
