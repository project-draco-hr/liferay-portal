{
  Image largeImage=imageLocalService.getImage(image.getLargeImageId());
  if (largeImage == null) {
    return;
  }
  tagsAssetLocalService.updateAsset(userId,image.getFolder().getGroupId(),IGImage.class.getName(),image.getImageId(),tagsCategories,tagsEntries,true,null,null,null,null,largeImage.getType(),image.getName(),image.getDescription(),null,null,largeImage.getHeight(),largeImage.getWidth(),null,false);
}
