{
  tagsAssetLocalService.deleteAsset(IGImage.class.getName(),image.getImageId());
  resourceLocalService.deleteResource(image.getCompanyId(),IGImage.class.getName(),ResourceImpl.SCOPE_INDIVIDUAL,image.getImageId());
  ImageLocalUtil.deleteImage(image.getSmallImageId());
  ImageLocalUtil.deleteImage(image.getLargeImageId());
  igImagePersistence.remove(image.getPrimaryKey());
}
