{
  TagsAssetLocalServiceUtil.deleteAsset(IGImage.class.getName(),image.getImageId());
  ResourceLocalServiceUtil.deleteResource(image.getCompanyId(),IGImage.class.getName(),ResourceImpl.SCOPE_INDIVIDUAL,image.getImageId());
  ImageLocalUtil.deleteImage(image.getSmallImageId());
  ImageLocalUtil.deleteImage(image.getLargeImageId());
  IGImageUtil.remove(image.getPrimaryKey());
}
