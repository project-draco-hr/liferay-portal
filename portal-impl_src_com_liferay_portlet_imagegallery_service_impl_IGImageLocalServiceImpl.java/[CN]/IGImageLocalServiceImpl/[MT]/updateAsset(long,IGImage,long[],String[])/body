{
  Image largeImage=imageLocalService.getImage(image.getLargeImageId());
  if (largeImage == null) {
    return;
  }
  assetLocalService.updateAsset(userId,image.getGroupId(),IGImage.class.getName(),image.getImageId(),assetCategoryIds,assetTagNames,true,null,null,null,null,largeImage.getType(),image.getName(),image.getDescription(),null,null,largeImage.getHeight(),largeImage.getWidth(),null,false);
}
