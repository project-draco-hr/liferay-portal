{
  String[] paramArray=params.get(name);
  if (paramArray == null) {
    String portletId=PortletProviderUtil.getPortletId(BlogsEntry.class.getName(),PortletProvider.Action.VIEW);
    String namespace=PortalUtil.getPortletNamespace(portletId);
    paramArray=params.get(namespace + name);
  }
  if (ArrayUtil.isNotEmpty(paramArray)) {
    return paramArray[0];
  }
 else {
    return null;
  }
}
