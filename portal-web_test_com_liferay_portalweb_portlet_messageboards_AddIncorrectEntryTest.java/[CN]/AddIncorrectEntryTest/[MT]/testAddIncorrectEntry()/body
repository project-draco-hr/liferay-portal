{
  selenium.click("//a[text()='S\u00e9cond T\u00e9st Subcat\u00e9gory\nThis is a s\u00e9cond t\u00e9st subcat\u00e9gory!']");
  selenium.waitForPageToLoad("30000");
  selenium.click("//input[@value='Post New Thread']");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("_19_subject")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("//input[@value='Save']");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isTextPresent("You have entered invalid data. Please try again.")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("link=\u00ab Back");
  selenium.waitForPageToLoad("30000");
}
