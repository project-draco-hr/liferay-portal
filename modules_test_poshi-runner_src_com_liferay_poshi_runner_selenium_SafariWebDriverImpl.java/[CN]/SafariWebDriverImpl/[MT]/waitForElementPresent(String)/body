{
  if (!(locator.contains(".js"))) {
    for (int second=0; ; second++) {
      if (second >= PropsValues.TIMEOUT_EXPLICIT_WAIT) {
        super.assertElementPresent(locator);
      }
      try {
        if (super.isElementPresent(locator)) {
          break;
        }
      }
 catch (      Exception e) {
      }
      Thread.sleep(1000);
    }
  }
}
