{
  Expression<Double> expression=ExpressionFactoryUtil.createDoubleExpression("sum(var1, var2, var3)");
  double var1=5.5;
  expression.setDoubleVariableValue("var1",var1);
  expression.setExpressionVariableValue("var2",Double.class,"var1 + 3.5");
  expression.setExpressionVariableValue("var3",Double.class,"var2 + var1");
  double var2=var1 + 3.5;
  double var3=var1 + var2;
  double actual=expression.evaluate();
  double expected=MathUtil.sum(var1,var2,var3);
  Assert.assertEquals(actual,expected);
}
