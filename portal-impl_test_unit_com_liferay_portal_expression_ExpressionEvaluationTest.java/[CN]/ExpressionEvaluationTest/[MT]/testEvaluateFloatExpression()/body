{
  Expression<Float> expression=ExpressionFactoryUtil.createFloatExpression("var1 + var2 + var3");
  float var1=5.5f;
  expression.setFloatVariableValue("var1",var1);
  expression.setExpressionStringVariableValue("var2","var1 + 3",Float.class);
  expression.setExpressionStringVariableValue("var3","var2 + var1",Float.class);
  float var2=var1 + 3;
  float var3=var1 + var2;
  Assert.assertEquals(var1 + var2 + var3,expression.evaluate());
}
