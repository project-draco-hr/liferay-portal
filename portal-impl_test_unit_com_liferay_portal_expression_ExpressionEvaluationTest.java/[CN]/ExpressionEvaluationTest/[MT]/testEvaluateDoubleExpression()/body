{
  Expression<Double> expression=ExpressionFactoryUtil.createDoubleExpression("var1 + var2 + var3");
  double var1=5.5;
  expression.setDoubleVariableValue("var1",var1);
  expression.setExpressionStringVariableValue("var2",Double.class,"var1 + 3");
  expression.setExpressionStringVariableValue("var3",Double.class,"var2 + var1");
  double var2=var1 + 3;
  double var3=var1 + var2;
  Assert.assertEquals(var1 + var2 + var3,expression.evaluate());
}
