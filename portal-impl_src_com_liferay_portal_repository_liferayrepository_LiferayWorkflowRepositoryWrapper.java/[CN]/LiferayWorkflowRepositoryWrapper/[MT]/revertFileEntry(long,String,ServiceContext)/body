{
  DLFileVersionReference dlFileVersionReference=_getWorkflowDLFileVersion(fileEntryId,serviceContext);
  super.revertFileEntry(fileEntryId,version,serviceContext);
  _startWorkflowInstance(com.liferay.portal.kernel.repository.util.RepositoryUserUtil.getUserId(),dlFileVersionReference.fetchDLFileVersion(),serviceContext);
}
