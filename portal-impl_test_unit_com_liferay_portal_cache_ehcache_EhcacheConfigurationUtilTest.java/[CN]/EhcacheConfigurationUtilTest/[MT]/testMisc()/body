{
  Configuration configuration=EhcacheConfigurationUtil.getConfiguration("WrongConfigurationPath");
  Assert.assertNull(configuration);
  configuration=EhcacheConfigurationUtil.getConfiguration(StringPool.BLANK,true);
  Assert.assertNull(configuration);
  configuration=EhcacheConfigurationUtil.getConfiguration(StringPool.BLANK,true,true);
  Assert.assertNull(configuration);
  URL configurationURL=EhcacheConfigurationUtilTest.class.getResource("dependencies/test-single-ehcache-config.xml");
  Assert.assertNotNull(configurationURL);
  configuration=EhcacheConfigurationUtil.getConfiguration(configurationURL);
  Assert.assertNotNull(configuration);
  configuration=EhcacheConfigurationUtil.getConfiguration(configurationURL,true);
  Assert.assertNotNull(configuration);
  new EhcacheConfigurationUtil();
}
