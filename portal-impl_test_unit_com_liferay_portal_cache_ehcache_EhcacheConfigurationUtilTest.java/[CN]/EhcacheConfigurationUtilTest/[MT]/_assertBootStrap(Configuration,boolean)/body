{
  List<CacheConfiguration> cacheConfigurations=_getAllCacheConfigurations(configuration);
  for (  CacheConfiguration cacheConfiguration : cacheConfigurations) {
    if (enabled) {
      Assert.assertNotNull(cacheConfiguration.getBootstrapCacheLoaderFactoryConfiguration());
    }
 else {
      Assert.assertNull(cacheConfiguration.getBootstrapCacheLoaderFactoryConfiguration());
    }
  }
}
