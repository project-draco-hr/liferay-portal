{
  File file=new File("portal-impl/test/unit/com/liferay/portal/cache/ehcache/" + "test-multi-ehcache-config.xml");
  try {
    URI uri=file.toURI();
    _configurationURL=uri.toURL();
  }
 catch (  Exception e) {
    Assert.fail();
  }
  _configuration=ConfigurationFactory.parseConfiguration(_configurationURL);
}
