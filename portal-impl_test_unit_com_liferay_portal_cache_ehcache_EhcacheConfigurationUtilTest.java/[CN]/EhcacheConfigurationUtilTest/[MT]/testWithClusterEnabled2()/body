{
  Configuration configuration=EhcacheConfigurationUtil.getConfiguration(_configurationURL,false,false);
  _assertListenerConfigsEquals(_configuration,configuration);
  configuration=EhcacheConfigurationUtil.getConfiguration(_configurationURL,false,true);
  _assertListenerConfigsEquals(_configuration,configuration);
  configuration=EhcacheConfigurationUtil.getConfiguration(_configurationURL,true,false);
  _assertClusterLinkReplicatorConfigs(configuration,false);
  configuration=EhcacheConfigurationUtil.getConfiguration(_configurationURL,true,true);
  _assertClusterLinkReplicatorConfigs(configuration,true);
}
