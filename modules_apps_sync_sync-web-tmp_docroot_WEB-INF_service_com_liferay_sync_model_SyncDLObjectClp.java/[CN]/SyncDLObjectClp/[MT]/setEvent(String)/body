{
  _event=event;
  if (_syncDLObjectRemoteModel != null) {
    try {
      Class<?> clazz=_syncDLObjectRemoteModel.getClass();
      Method method=clazz.getMethod("setEvent",String.class);
      method.invoke(_syncDLObjectRemoteModel,event);
    }
