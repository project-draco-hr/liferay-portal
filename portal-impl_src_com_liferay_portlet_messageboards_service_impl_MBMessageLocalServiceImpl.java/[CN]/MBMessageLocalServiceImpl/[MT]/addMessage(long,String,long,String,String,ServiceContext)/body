{
  MBCategory category=mbCategoryPersistence.findByPrimaryKey(categoryId);
  List<ObjectValuePair<String,InputStream>> inputStreamOVPs=Collections.emptyList();
  return addMessage(userId,userName,category.getGroupId(),categoryId,0,0,subject,body,MBMessageConstants.DEFAULT_FORMAT,inputStreamOVPs,false,0.0,false,serviceContext);
