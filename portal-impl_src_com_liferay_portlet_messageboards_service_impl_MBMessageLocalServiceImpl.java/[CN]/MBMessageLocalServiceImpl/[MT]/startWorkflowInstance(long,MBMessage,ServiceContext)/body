{
  Map<String,Serializable> workflowContext=new HashMap<String,Serializable>();
  workflowContext.put(WorkflowConstants.CONTEXT_URL,getMessageURL(message,serviceContext));
  WorkflowHandlerRegistryUtil.startWorkflowInstance(message.getCompanyId(),message.getGroupId(),userId,message.getWorkflowClassName(),message.getMessageId(),message,serviceContext,workflowContext);
}
