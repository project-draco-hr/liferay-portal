{
  if (message.isDiscussion()) {
    return;
  }
  boolean visible=false;
  if (message.getStatus() == StatusConstants.APPROVED) {
    visible=true;
  }
  assetEntryLocalService.updateEntry(userId,message.getGroupId(),MBMessage.class.getName(),message.getMessageId(),assetCategoryIds,assetTagNames,visible,null,null,null,null,ContentTypes.TEXT_HTML,message.getSubject(),null,null,null,0,0,null,false);
}
