{
  if ((tagsEntries == null) || message.isDiscussion()) {
    return;
  }
  TagsAssetLocalServiceUtil.updateAsset(userId,message.getCategory().getGroupId(),MBMessage.class.getName(),message.getMessageId(),tagsEntries,null,null,null,null,ContentTypes.TEXT_HTML,message.getSubject(),message.getSubject(),message.getSubject(),null,0,0);
}
