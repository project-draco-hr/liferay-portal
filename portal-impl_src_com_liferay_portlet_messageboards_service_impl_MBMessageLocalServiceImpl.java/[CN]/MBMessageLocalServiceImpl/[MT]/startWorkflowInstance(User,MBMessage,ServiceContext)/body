{
  Map<String,Serializable> workflowContext=new HashMap<String,Serializable>();
  workflowContext.put(WorkflowConstants.CONTEXT_URL,getMessageURL(message,serviceContext));
  WorkflowHandlerRegistryUtil.startWorkflowInstance(user.getCompanyId(),message.getGroupId(),user.getUserId(),message.getWorkflowClassName(),message.getMessageId(),message,serviceContext,workflowContext);
}
