{
  if (PropsValues.DISCUSSION_GUEST_NAME_REQUIRED && Validator.isNull(guestName)) {
    throw new RequiredNameException();
  }
  if (PropsValues.DISCUSSION_GUEST_EMAIL_REQUIRED && Validator.isNull(guestEmail)) {
    throw new RequiredEmailException();
  }
  if (PropsValues.DISCUSSION_GUEST_WEB_REQUIRED && Validator.isNull(guestWeb)) {
    throw new RequiredWebException();
  }
}
