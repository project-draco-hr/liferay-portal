{
  Group group1=_userSearchFixture.addGroup();
  _userSearchFixture.addUser(group1,"Liferay " + RandomTestUtil.randomString());
  Group group2=_userSearchFixture.addGroup();
  _userSearchFixture.addUser(group2,"Liferay " + RandomTestUtil.randomString());
  assertSearch("Liferay",2);
  deactivate(group1);
  assertSearch("Liferay",1);
  deactivate(group2);
  assertSearch("Liferay",0);
}
