{
  UserGroupRole userGroupRole=fetchByPrimaryKey(userGroupRolePK);
  if (userGroupRole == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No UserGroupRole exists with the primary key " + userGroupRolePK);
    }
    throw new NoSuchUserGroupRoleException("No UserGroupRole exists with the primary key " + userGroupRolePK);
  }
  return userGroupRole;
}
