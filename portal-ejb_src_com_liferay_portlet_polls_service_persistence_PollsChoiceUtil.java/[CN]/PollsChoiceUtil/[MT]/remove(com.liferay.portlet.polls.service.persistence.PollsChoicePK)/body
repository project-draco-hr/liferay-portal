{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(pollsChoicePK));
  }
  com.liferay.portlet.polls.model.PollsChoice pollsChoice=getPersistence().remove(pollsChoicePK);
  if (listener != null) {
    listener.onAfterRemove(pollsChoice);
  }
  return pollsChoice;
}
