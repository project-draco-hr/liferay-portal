{
  List<UpgradeInfo> upgradeInfos=buildUpgradeInfos(upgradeFromSchemaVersion,upgradeToSchemaVersion,upgradeSteps);
  List<ServiceRegistration<UpgradeStep>> serviceRegistrations=new ArrayList<>();
  for (  UpgradeInfo upgradeInfo : upgradeInfos) {
    ServiceRegistration<UpgradeStep> upgradeStep=_registerUpgradeStep(bundleContext,upgradeBundleSymbolicName,upgradeInfo);
    serviceRegistrations.add(upgradeStep);
  }
  return serviceRegistrations;
}
