{
  UpgradeStepRegistrator upgradeStepRegistrator=_bundleContext.getService(serviceReference);
  if (upgradeStepRegistrator == null) {
    return null;
  }
  final Collection<ServiceRegistration<UpgradeStep>> serviceRegistrations=new ArrayList<>();
  upgradeStepRegistrator.register(new UpgradeStepRegistry(serviceRegistrations));
  return serviceRegistrations;
}
