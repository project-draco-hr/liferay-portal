{
  if (o instanceof ReverseComparable) {
    Comparable inner=((ReverseComparable)o)._inner;
    if (_inner == null) {
      if (inner == null) {
        return 0;
      }
      return 1;
    }
 else     if (inner == null) {
      return -1;
    }
    return -_inner.compareTo(inner);
  }
 else {
    throw new IllegalStateException("expected instanace of " + ReverseComparable.class);
  }
}
