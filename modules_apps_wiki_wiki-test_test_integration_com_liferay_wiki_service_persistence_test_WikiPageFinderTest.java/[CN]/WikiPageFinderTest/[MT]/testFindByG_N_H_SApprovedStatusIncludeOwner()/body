{
  ServiceTestUtil.setUser(TestPropsValues.getUser());
  int count=WikiPageServiceUtil.getPagesCount(_group.getGroupId(),_node.getNodeId(),true,TestPropsValues.getUserId(),true,WorkflowConstants.STATUS_APPROVED);
  List<WikiPage> pages=WikiPageServiceUtil.getPages(_group.getGroupId(),_node.getNodeId(),true,TestPropsValues.getUserId(),true,WorkflowConstants.STATUS_APPROVED,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  Assert.assertEquals(2,count);
  Assert.assertEquals(count,pages.size());
}
