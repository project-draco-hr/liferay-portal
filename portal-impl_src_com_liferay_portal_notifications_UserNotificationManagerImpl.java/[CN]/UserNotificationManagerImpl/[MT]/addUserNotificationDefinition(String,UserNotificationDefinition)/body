{
  List<UserNotificationDefinition> userNotificationDefinitions=_userNotificationDefinitions.get(portletId);
  if (userNotificationDefinitions == null) {
    userNotificationDefinitions=new ArrayList<UserNotificationDefinition>();
    _userNotificationDefinitions.put(portletId,userNotificationDefinitions);
  }
  userNotificationDefinitions.add(userNotificationDefinition);
}
