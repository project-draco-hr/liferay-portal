{
  String markdown="Title [](id=1234)\n=============";
  String fileEntryName="some\\windows\\file";
  HashMap<String,String> metadata=new HashMap<String,String>(){
{
      put("base.source.url","http://baseUrl");
    }
  }
;
  KBArticleMarkdownConverter converter=new KBArticleMarkdownConverter(markdown,fileEntryName,metadata);
  Assert.assertEquals("http://baseUrl/some/windows/file",converter.getSourceURL());
}
