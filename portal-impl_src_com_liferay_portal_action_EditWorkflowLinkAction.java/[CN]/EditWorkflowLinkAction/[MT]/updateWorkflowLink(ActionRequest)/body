{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  for (  String paramName : actionRequest.getParameterMap().keySet()) {
    if (!paramName.startsWith(_PREFIX)) {
      continue;
    }
    String workflowDefinitionName=ParamUtil.getString(actionRequest,paramName);
    long classNameId=GetterUtil.getLong(paramName.substring(_PREFIX.length(),paramName.length()));
    WorkflowLinkLocalServiceUtil.updateWorkflowLink(themeDisplay.getUserId(),themeDisplay.getCompanyId(),0,classNameId,workflowDefinitionName);
  }
}
