{
  EntityCacheUtil.putResult(AssetTagModelImpl.ENTITY_CACHE_ENABLED,AssetTagImpl.class,assetTag.getPrimaryKey(),assetTag);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{assetTag.getUuid(),assetTag.getGroupId()},assetTag);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_N,new Object[]{assetTag.getGroupId(),assetTag.getName()},assetTag);
  assetTag.resetOriginalValues();
}
