{
  content=StringUtil.replace(content,_CSS_PATH_TYPES,_CSS_PATH_PLACEHOLDERS);
  content=StringUtil.replace(content,new String[]{"[$RELATIVE_1$]","[$RELATIVE_2$]","[$RELATIVE_3$]"},new String[]{"url('" + baseURL,"url(\"" + baseURL,"url(" + baseURL});
  content=StringUtil.replace(content,_CSS_PATH_PLACEHOLDERS,_CSS_PATH_TYPES);
  return content;
}
