{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.select("_86_selectionStyle",RuntimeVariables.replace("label=Manual"));
  Thread.sleep(5000);
  assertEquals("Manual",selenium.getSelectedLabel("_86_selectionStyle"));
  assertEquals(RuntimeVariables.replace("Selection"),selenium.getText("//div[@id='assetPublisherSelection']/div[1]/div/span"));
  assertEquals(RuntimeVariables.replace("Display Settings"),selenium.getText("//div[@id='assetPublisherDisplaySettings']/div[1]/div/span"));
}
