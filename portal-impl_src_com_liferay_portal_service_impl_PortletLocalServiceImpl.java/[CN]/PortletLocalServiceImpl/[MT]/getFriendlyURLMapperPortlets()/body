{
  String[] friendlyURLMapperRootPortletIds=_friendlyURLMapperRootPortletIds.get();
  List<Portlet> portlets=new ArrayList<>(friendlyURLMapperRootPortletIds.length);
  for (  String rootPortletId : friendlyURLMapperRootPortletIds) {
    Portlet portlet=_portletsMap.get(rootPortletId);
    if ((portlet == null) || !portlet.isActive() || !portlet.isInclude()|| !portlet.isReady()|| portlet.isUndeployedPortlet()) {
      continue;
    }
    portlets.add(portlet);
  }
  return portlets;
}
