{
  List<Portlet> portlets=new ArrayList<>(_friendlyURLMapperRootPortletIds.size());
  for (  String rootPortletId : _friendlyURLMapperRootPortletIds) {
    Portlet portlet=_portletsMap.get(rootPortletId);
    if ((portlet == null) || !portlet.isActive() || !portlet.isInclude()|| !portlet.isReady()|| portlet.isUndeployedPortlet()) {
      continue;
    }
    portlets.add(portlet);
  }
  return portlets;
}
