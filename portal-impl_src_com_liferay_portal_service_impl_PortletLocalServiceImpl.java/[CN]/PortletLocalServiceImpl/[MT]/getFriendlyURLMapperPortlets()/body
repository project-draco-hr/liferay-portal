{
  List<Portlet> portlets=new ArrayList<Portlet>(_friendlyURLMapperPortlets.size());
  Iterator<Map.Entry<String,Portlet>> itr=_friendlyURLMapperPortlets.entrySet().iterator();
  while (itr.hasNext()) {
    Map.Entry<String,Portlet> entry=itr.next();
    Portlet portlet=entry.getValue();
    FriendlyURLMapper friendlyURLMapper=portlet.getFriendlyURLMapperInstance();
    if (friendlyURLMapper != null) {
      portlets.add(portlet);
    }
  }
  return portlets;
}
