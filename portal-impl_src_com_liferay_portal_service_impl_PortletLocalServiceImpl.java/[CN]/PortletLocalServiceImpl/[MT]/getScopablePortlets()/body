{
  Map<String,Portlet> portletsPool=_getPortletsPool();
  List<Portlet> portlets=ListUtil.fromMapValues(portletsPool);
  Iterator<Portlet> itr=portlets.iterator();
  while (itr.hasNext()) {
    Portlet portlet=itr.next();
    if (!portlet.isScopeable()) {
      itr.remove();
    }
  }
  return portlets;
}
