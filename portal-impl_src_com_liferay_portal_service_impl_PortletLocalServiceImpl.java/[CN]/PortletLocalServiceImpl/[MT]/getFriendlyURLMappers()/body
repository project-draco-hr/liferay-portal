{
  List<FriendlyURLMapper> friendlyURLMappers=new ArrayList<FriendlyURLMapper>();
  for (  Portlet portlet : getPortlets()) {
    if (!portlet.isActive() || !portlet.isInclude() || !portlet.isReady()|| portlet.isUndeployedPortlet()) {
      continue;
    }
    FriendlyURLMapper friendlyURLMapper=portlet.getFriendlyURLMapperInstance();
    if (friendlyURLMapper != null) {
      friendlyURLMappers.add(friendlyURLMapper);
    }
  }
  return friendlyURLMappers;
}
