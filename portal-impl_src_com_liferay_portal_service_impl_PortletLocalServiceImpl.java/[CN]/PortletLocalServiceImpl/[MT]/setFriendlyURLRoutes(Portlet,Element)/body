{
  Element friendlyUrlRoutesElement=portletElement.element("friendly-url-routes");
  if (friendlyUrlRoutesElement == null) {
    return;
  }
  Router router=new Router();
  for (  Element routeElement : friendlyUrlRoutesElement.elements("route")) {
    Route route=new Route();
    String pattern=routeElement.elementText("route-pattern");
    route.setPattern(pattern);
    for (    Element routeDefaultElement : routeElement.elements("route-default")) {
      String defaultParamater=routeDefaultElement.elementText("route-default-parameter");
      String defaultValue=routeDefaultElement.elementText("route-default-value");
      route.addDefaultValue(defaultParamater,defaultValue);
    }
    router.addRoute(route);
  }
}
