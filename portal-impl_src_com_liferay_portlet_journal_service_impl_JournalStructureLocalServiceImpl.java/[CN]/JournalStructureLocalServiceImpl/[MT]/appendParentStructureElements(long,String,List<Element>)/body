{
  if (Validator.isNull(parentStructureId)) {
    return;
  }
  JournalStructure parentStructure=journalStructurePersistence.findByG_S(groupId,parentStructureId);
  appendParentStructureElements(groupId,parentStructure.getParentStructureId(),children);
  Document doc=SAXReaderUtil.read(parentStructure.getXsd());
  Element root=doc.getRootElement();
  children.addAll(root.elements());
}
