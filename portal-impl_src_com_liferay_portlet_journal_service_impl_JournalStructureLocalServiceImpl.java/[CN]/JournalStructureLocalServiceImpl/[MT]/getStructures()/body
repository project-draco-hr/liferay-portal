{
  SearchCriteria criteria=new SearchCriteria();
  List<SearchFieldValue> searchFields=new ArrayList<SearchFieldValue>();
  _addSearchField(searchFields,OptionalJournalStructureCriteria.FINDER,OptionalJournalStructureCriteria.FIND_ALL);
  criteria.setSearchFieldValues(searchFields);
  ContentTypeService ctService=MirageServiceFactory.getContentTypeService();
  try {
    List<ContentType> contentTypes=ctService.searchContentTypes(criteria);
    return _getStructuresFromContentTypes(contentTypes);
  }
 catch (  CMSException ex) {
    throw (SystemException)ex.getCause();
  }
}
