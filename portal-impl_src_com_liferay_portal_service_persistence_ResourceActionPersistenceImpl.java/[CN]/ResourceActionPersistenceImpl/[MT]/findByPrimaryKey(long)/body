{
  ResourceAction resourceAction=fetchByPrimaryKey(resourceActionId);
  if (resourceAction == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ResourceAction exists with the primary key " + resourceActionId);
    }
    throw new NoSuchResourceActionException("No ResourceAction exists with the primary key " + resourceActionId);
  }
  return resourceAction;
}
