{
  resourceAction=toUnwrappedModel(resourceAction);
  Session session=null;
  try {
    session=openSession();
    if (resourceAction.isCachedModel()) {
      resourceAction=(ResourceAction)session.get(ResourceActionImpl.class,resourceAction.getPrimaryKeyObj());
    }
    if (resourceAction != null) {
      session.delete(resourceAction);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (resourceAction != null) {
    clearCache(resourceAction);
  }
  return resourceAction;
}
