{
  ResourceAction resourceAction=(ResourceAction)EntityCacheUtil.getResult(ResourceActionModelImpl.ENTITY_CACHE_ENABLED,ResourceActionImpl.class,resourceActionId,this);
  if (resourceAction == null) {
    Session session=null;
    try {
      session=openSession();
      resourceAction=(ResourceAction)session.get(ResourceActionImpl.class,Long.valueOf(resourceActionId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (resourceAction != null) {
        cacheResult(resourceAction);
      }
      closeSession(session);
    }
  }
  return resourceAction;
}
