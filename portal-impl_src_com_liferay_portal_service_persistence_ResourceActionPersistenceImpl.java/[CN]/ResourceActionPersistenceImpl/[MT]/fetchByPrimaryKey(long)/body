{
  ResourceAction resourceAction=(ResourceAction)EntityCacheUtil.getResult(ResourceActionModelImpl.ENTITY_CACHE_ENABLED,ResourceActionImpl.class,resourceActionId);
  if (resourceAction == _nullResourceAction) {
    return null;
  }
  if (resourceAction == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      resourceAction=(ResourceAction)session.get(ResourceActionImpl.class,Long.valueOf(resourceActionId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (resourceAction != null) {
        cacheResult(resourceAction);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(ResourceActionModelImpl.ENTITY_CACHE_ENABLED,ResourceActionImpl.class,resourceActionId,_nullResourceAction);
      }
      closeSession(session);
    }
  }
  return resourceAction;
}
