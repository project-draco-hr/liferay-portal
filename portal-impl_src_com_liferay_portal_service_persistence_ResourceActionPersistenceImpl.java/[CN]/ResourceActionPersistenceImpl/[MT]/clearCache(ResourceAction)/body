{
  EntityCacheUtil.removeResult(ResourceActionModelImpl.ENTITY_CACHE_ENABLED,ResourceActionImpl.class,resourceAction.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_N_A,new Object[]{resourceAction.getName(),resourceAction.getActionId()});
}
