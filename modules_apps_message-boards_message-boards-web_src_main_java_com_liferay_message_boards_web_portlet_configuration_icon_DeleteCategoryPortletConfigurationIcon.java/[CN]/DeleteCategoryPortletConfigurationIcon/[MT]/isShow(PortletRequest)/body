{
  try {
    if (_category == null) {
      return false;
    }
    ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
    if (MBCategoryPermission.contains(themeDisplay.getPermissionChecker(),_category,ActionKeys.DELETE)) {
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
