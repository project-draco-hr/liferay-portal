{
  try {
    MethodWrapper methodWrapper=new MethodWrapper(IndexerImpl.class.getName(),"reIndex",new Object[]{ids});
    IndexProducer.produce(methodWrapper);
  }
 catch (  Exception e) {
    if (e instanceof SearchException) {
      throw (SearchException)e;
    }
 else {
      _log.error(e);
    }
  }
}
