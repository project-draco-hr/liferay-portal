{
  String buildGradleContent=StringPool.BLANK;
  File file=_baseSourceProcessor.getFile(moduleDirName + "/build.gradle",_baseSourceProcessor.PORTAL_MAX_DIR_LEVEL);
  if (file != null) {
    buildGradleContent=FileUtil.read(file);
  }
  Assert.assertEquals(Arrays.asList(expectedModuleLangDirNames),_baseSourceProcessor.getModuleLangDirNames(moduleDirName,buildGradleContent));
}
