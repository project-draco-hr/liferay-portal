{
  moduleDirName=_TEST_RESOURCES_DIR_NAME + moduleDirName;
  for (int i=0; i < expectedModuleLangDirNames.length; i++) {
    expectedModuleLangDirNames[i]=_TEST_RESOURCES_DIR_NAME + expectedModuleLangDirNames[i];
  }
  String buildGradleContent=StringPool.BLANK;
  File file=_baseSourceProcessor.getFile(moduleDirName + "/build.testgradle",_baseSourceProcessor.PORTAL_MAX_DIR_LEVEL);
  if (file != null) {
    buildGradleContent=FileUtil.read(file);
  }
  Assert.assertEquals(Arrays.asList(expectedModuleLangDirNames),_baseSourceProcessor.getModuleLangDirNames(moduleDirName,buildGradleContent));
}
