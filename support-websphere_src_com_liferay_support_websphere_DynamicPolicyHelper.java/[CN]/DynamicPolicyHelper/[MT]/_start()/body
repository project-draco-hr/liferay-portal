{
  _original=DynamicPolicyFactory.getInstance();
  final DynamicPolicy original=_original;
  DynamicPolicy dynamicPolicy=new DynamicPolicy(){
    @Override public ProtectionDomain getProtectionDomain(    CodeSource codeSource){
      if (original == null) {
        return null;
      }
      return original.getProtectionDomain(codeSource);
    }
    @Override public PermissionCollection getPermissions(    CodeSource codeSource,    Map map){
      return java.security.Policy.getPolicy().getPermissions(codeSource);
    }
    @Override public void getSecurityPolicy(    Map map1,    Map map2){
      if (original == null) {
        return;
      }
      original.getSecurityPolicy(map1,map2);
    }
    @Override public void removePolicy(    Map map){
      if (original == null) {
        return;
      }
      original.removePolicy(map);
    }
    @Override public void setupPolicy(    Map map){
      if (original == null) {
        return;
      }
      original.setupPolicy(map);
    }
  }
;
  DynamicPolicyFactory.setInstance(dynamicPolicy);
}
