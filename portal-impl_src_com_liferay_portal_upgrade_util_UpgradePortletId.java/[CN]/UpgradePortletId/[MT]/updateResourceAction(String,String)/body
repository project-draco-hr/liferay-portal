{
  String selectSQL="select name from ResourceAction where name = '" + newName + "'";
  try (PreparedStatement ps=connection.prepareStatement(selectSQL);ResultSet rs=ps.executeQuery()){
    if (rs.next()) {
      runSQL("delete from ResourceAction where name = '" + oldName + "'");
    }
 else {
      runSQL("update ResourceAction set name = '" + newName + "' where name = '"+ oldName+ "'");
    }
  }
 }
