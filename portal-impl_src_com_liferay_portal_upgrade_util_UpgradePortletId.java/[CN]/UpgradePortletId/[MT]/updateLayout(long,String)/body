{
  PreparedStatement ps=null;
  try {
    ps=connection.prepareStatement("update Layout set typeSettings = ? where plid = " + plid);
    ps.setString(1,typeSettings);
    ps.executeUpdate();
  }
 catch (  SQLException sqle) {
    if (_log.isWarnEnabled()) {
      _log.warn(sqle,sqle);
    }
  }
 finally {
    DataAccess.cleanUp(ps);
  }
}
