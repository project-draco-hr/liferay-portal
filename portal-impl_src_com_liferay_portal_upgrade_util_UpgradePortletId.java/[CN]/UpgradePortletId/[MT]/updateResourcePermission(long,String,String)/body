{
  try (PreparedStatement ps=connection.prepareStatement("update ResourcePermission set name = ?, primKey = ? where " + "resourcePermissionId = " + resourcePermissionId)){
    ps.setString(1,name);
    ps.setString(2,primKey);
    ps.executeUpdate();
  }
 catch (  SQLException sqle) {
    if (_log.isWarnEnabled()) {
      _log.warn(sqle,sqle);
    }
  }
}
