{
  try (PreparedStatement ps=connection.prepareStatement("select plid, groupId, companyId, userId, userName, " + "createDate, modifiedDate, privateLayout, friendlyURL " + "from Layout");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long plid=rs.getLong("plid");
      long groupId=rs.getLong("groupId");
      long companyId=rs.getLong("companyId");
      long userId=rs.getLong("userId");
      String userName=rs.getString("userName");
      Timestamp createDate=rs.getTimestamp("createDate");
      Timestamp modifiedDate=rs.getTimestamp("modifiedDate");
      boolean privateLayout=rs.getBoolean("privateLayout");
      String friendlyURL=rs.getString("friendlyURL");
      addLayoutFriendlyURL(groupId,companyId,userId,userName,createDate,modifiedDate,plid,privateLayout,friendlyURL);
    }
  }
 }
