{
  List<User> users=UserLocalServiceUtil.getGroupUsers(group.getGroupId());
  List<User> parentUsers=UserLocalServiceUtil.getGroupUsers(group.getParentGroupId());
  for (  User user : users) {
    if (!parentUsers.contains(user)) {
      UserLocalServiceUtil.unsetGroupUsers(group.getGroupId(),new long[]{user.getUserId()},null);
    }
  }
}
