{
  try {
    Group group=GroupLocalServiceUtil.getGroup(groupId);
    if (group.isLimitedToParentSiteMembers()) {
      if (!GroupLocalServiceUtil.hasUserGroup(userId,group.getParentGroupId(),false)) {
        return false;
      }
    }
  }
 catch (  Exception e) {
    _log.error(e);
  }
  return true;
}
