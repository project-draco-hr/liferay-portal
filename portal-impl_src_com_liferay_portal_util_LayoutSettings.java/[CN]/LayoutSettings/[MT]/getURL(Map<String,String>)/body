{
  String url=_url;
  Iterator<Map.Entry<String,String>> itr=variables.entrySet().iterator();
  while (itr.hasNext()) {
    Map.Entry<String,String> entry=itr.next();
    String key=entry.getKey();
    String value=entry.getValue();
    url=StringUtil.replace(url,StringPool.DOLLAR_AND_OPEN_CURLY_BRACE.concat(key).concat(StringPool.CLOSE_CURLY_BRACE),value);
  }
  return url;
}
