{
  schedulerEntryImpl.setTrigger(TriggerFactoryUtil.createTrigger(getEventListenerClass(),getEventListenerClass(),ExportImportWebConfigurationValues.DRAFT_EXPORT_IMPORT_CONFIGURATION_CHECK_INTERVAL,TimeUnit.HOUR));
  _schedulerEngineHelper.register(this,schedulerEntryImpl,DestinationNames.SCHEDULER_DISPATCH);
}
