{
  if (((structureId == null) && (_structureId != null)) || ((structureId != null) && (_structureId == null)) || ((structureId != null) && (_structureId != null) && !structureId.equals(_structureId))) {
    if (!XSS_ALLOW_STRUCTUREID) {
      structureId=XSSUtil.strip(structureId);
    }
    _structureId=structureId;
  }
}
