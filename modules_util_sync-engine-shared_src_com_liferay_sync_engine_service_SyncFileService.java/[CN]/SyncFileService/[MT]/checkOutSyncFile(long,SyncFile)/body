{
  syncFile.setUiEvent(SyncFile.UI_EVENT_UPDATED_LOCAL);
  update(syncFile);
  Map<String,Object> parameters=new HashMap<String,Object>();
  parameters.put("fileEntryId",syncFile.getTypePK());
  parameters.put("syncFile",syncFile);
  CheckOutFileEntryEvent checkOutFileEntryEvent=new CheckOutFileEntryEvent(syncAccountId,parameters);
  checkOutFileEntryEvent.run();
  return syncFile;
}
