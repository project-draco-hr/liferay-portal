{
  ModuleHotDeployEvent moduleHotDeployEvent=(ModuleHotDeployEvent)hotDeployEvent;
  if (!moduleHotDeployEvent.hasBundleHeader("Liferay-Service")) {
    return;
  }
  super.invokeDeploy(hotDeployEvent);
  try {
    processServiceResourceActions(hotDeployEvent);
  }
 catch (  SystemException se) {
    throw new HotDeployException("Unable to process service resource actions for " + hotDeployEvent.getServletContextName(),se);
  }
}
