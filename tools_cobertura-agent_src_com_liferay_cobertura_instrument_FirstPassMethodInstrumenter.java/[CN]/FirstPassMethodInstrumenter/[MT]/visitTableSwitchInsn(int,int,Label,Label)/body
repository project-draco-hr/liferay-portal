{
  super.visitTableSwitchInsn(min,max,dflt,labels);
  if (_currentLine != 0) {
    switchLabels.put(dflt,new SwitchHolder(_currentLine,_currentSwitch,-1));
    for (int i=labels.length - 1; i >= 0; i--) {
      switchLabels.put(labels[i],new SwitchHolder(_currentLine,_currentSwitch,i));
    }
    _classData.addLineSwitch(_currentLine,_currentSwitch++,min,max);
  }
}
