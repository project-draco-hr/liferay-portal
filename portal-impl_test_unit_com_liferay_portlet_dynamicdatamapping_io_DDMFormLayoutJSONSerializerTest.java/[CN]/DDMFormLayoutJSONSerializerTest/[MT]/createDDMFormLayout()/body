{
  DDMFormLayout ddmFormLayout=new DDMFormLayout();
  ddmFormLayout.addDDMFormLayoutRow(createDDMFormLayoutRow(createDDMFormLayoutColumns("text1","text2")));
  ddmFormLayout.addDDMFormLayoutRow(createDDMFormLayoutRow(createDDMFormLayoutColumns("text3","text4","text5")));
  ddmFormLayout.addDDMFormLayoutRow(createDDMFormLayoutRow(createDDMFormLayoutColumns("text6")));
  return ddmFormLayout;
}
