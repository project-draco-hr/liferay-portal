{
  selenium.select("_26_id",RuntimeVariables.replace("label=French to English"));
  selenium.type("_26_text",RuntimeVariables.replace("Mon nom est traducteur de Liferay, fluent dans plus de 6 millions de formes de communication."));
  selenium.click(RuntimeVariables.replace("//input[@value='Translate']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("My name is translator of Liferay, flowing in more than 6 million forms of communication. "));
}
