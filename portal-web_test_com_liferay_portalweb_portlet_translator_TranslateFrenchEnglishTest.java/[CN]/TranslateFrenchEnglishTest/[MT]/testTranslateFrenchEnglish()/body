{
  selenium.select("_26_id","label=French to English");
  selenium.type("_26_text","Mon nom est traducteur de Liferay, fluent dans plus de 6 millions de formes de communication.");
  selenium.click("//input[@value='Translate']");
  selenium.waitForPageToLoad("30000");
  verifyTrue(selenium.isTextPresent("My name is a translator of Liferay, flowing in more than 6 million forms of communication."));
}
