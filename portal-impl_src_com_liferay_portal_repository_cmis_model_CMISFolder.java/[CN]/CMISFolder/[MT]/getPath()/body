{
  StringBuilder sb=new StringBuilder();
  try {
    Folder folder=this;
    while (folder != null) {
      sb.insert(0,folder.getName());
      sb.insert(0,StringPool.SLASH);
      folder=folder.getParentFolder();
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return sb.toString();
}
