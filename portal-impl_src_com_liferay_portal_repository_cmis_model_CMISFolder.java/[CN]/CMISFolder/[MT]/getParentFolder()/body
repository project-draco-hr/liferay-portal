{
  Folder parentFolder=null;
  try {
    parentFolder=super.getParentFolder();
    if (parentFolder != null) {
      return parentFolder;
    }
  }
 catch (  Exception e) {
  }
  if (_cmisFolder.isRootFolder()) {
    DLFolder dlFolder=DLRepositoryLocalServiceUtil.getFolderByRepositoryId(getRepositoryId());
    DLFolder parentDLFolder=dlFolder.getParentFolder();
    if (parentDLFolder != null) {
      parentFolder=new LiferayFolder(parentDLFolder);
    }
  }
 else {
    org.apache.chemistry.opencmis.client.api.Folder parentCmisFolder=_cmisFolder.getFolderParent();
    parentFolder=CMISRepositoryLocalServiceUtil.toFolder(getRepositoryId(),parentCmisFolder);
  }
  setParentFolder(parentFolder);
  return parentFolder;
}
