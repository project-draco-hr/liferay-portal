{
  List<org.apache.chemistry.opencmis.client.api.Folder> parentCmisFolders=_cmisFolder.getParents();
  if (parentCmisFolders.isEmpty()) {
    try {
      DLFolder dlFolder=DLRepositoryLocalServiceUtil.getFolderByRepositoryId(getRepositoryId());
      return dlFolder.getName();
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
  return _cmisFolder.getName();
}
