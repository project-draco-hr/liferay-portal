{
  JournalFolder parentFolder=JournalTestUtil.addFolder(_group.getGroupId(),RandomTestUtil.randomString());
  JournalFolder folder=JournalTestUtil.addFolder(_group.getGroupId(),parentFolder.getFolderId(),RandomTestUtil.randomString());
  JournalFolderLocalServiceUtil.moveFolderToTrash(TestPropsValues.getUserId(),folder.getFolderId());
  JournalFolderLocalServiceUtil.deleteFolder(parentFolder.getFolderId(),false);
  doVerify();
}
