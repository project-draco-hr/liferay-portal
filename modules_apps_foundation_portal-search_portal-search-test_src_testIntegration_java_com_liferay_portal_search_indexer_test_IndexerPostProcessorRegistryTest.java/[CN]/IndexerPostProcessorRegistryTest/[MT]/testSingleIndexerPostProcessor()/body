{
  Indexer<Organization> organizationIndexer=IndexerRegistryUtil.getIndexer(Organization.class.getName());
  IndexerPostProcessor[] organizationIndexerPostProcessors=organizationIndexer.getIndexerPostProcessors();
  assertEquals(1,organizationIndexerPostProcessors.length);
  IndexerPostProcessor organizationIndexerPostProcessor=organizationIndexerPostProcessors[0];
  assertNotNull(organizationIndexerPostProcessor);
}
