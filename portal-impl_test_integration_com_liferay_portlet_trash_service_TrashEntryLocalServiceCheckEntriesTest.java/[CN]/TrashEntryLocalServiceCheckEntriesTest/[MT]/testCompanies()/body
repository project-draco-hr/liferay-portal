{
  Long previousCompanyId=CompanyThreadLocal.getCompanyId();
  for (int i=0; i < _COMPANIES_COUNT; i++) {
    long companyId=createCompany();
    CompanyThreadLocal.setCompanyId(companyId);
    Group group=updateTrashEntriesMaxAge(createGroup(companyId),_MAX_AGE);
    createTrashEntries(group);
  }
  TrashEntryLocalServiceUtil.checkEntries();
  Assert.assertEquals(_COMPANIES_COUNT * _NOT_EXPIRED_TRASH_ENTRIES_COUNT,TrashEntryLocalServiceUtil.getTrashEntriesCount());
  CompanyThreadLocal.setCompanyId(previousCompanyId);
}
