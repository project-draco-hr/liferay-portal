{
  _groups=new ArrayList<Group>();
  List<TrashEntry> list=TrashEntryLocalServiceUtil.getTrashEntries(QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  if (list.size() > 0) {
    if (_log.isWarnEnabled()) {
      _log.warn("Cleaning TrashEntries due previous tests did not" + " clean up them properly");
    }
    clearTrashEntries(list);
  }
}
