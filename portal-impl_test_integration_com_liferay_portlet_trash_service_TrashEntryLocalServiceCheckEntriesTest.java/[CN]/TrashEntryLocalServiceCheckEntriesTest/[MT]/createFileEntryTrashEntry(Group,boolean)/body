{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId(),TestPropsValues.getUserId());
  FileEntry fileEntry=DLAppLocalServiceUtil.addFileEntry(TestPropsValues.getUserId(),group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,RandomTestUtil.randomString(),ContentTypes.TEXT_PLAIN,RandomTestUtil.randomBytes(TikaSafeRandomizerBumper.INSTANCE),serviceContext);
  User user=UserTestUtil.getAdminUser(fileEntry.getCompanyId());
  RepositoryTrashUtil.moveFileEntryToTrash(user.getUserId(),fileEntry.getRepositoryId(),fileEntry.getFileEntryId());
  if (expired) {
    int maxAge=TrashUtil.getMaxAge(group);
    TrashEntry trashEntry=TrashEntryLocalServiceUtil.getEntry(DLFileEntry.class.getName(),fileEntry.getFileEntryId());
    Date createDate=trashEntry.getCreateDate();
    trashEntry.setCreateDate(new Date(createDate.getTime() - maxAge * Time.MINUTE - Time.DAY));
    TrashEntryLocalServiceUtil.updateTrashEntry(trashEntry);
  }
}
