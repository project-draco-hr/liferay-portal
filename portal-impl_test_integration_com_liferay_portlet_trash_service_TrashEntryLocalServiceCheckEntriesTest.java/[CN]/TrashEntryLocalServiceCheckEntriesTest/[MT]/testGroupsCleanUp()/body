{
  for (int i=0; i < _groupsCount; i++) {
    Group group=updateTrashEntriesMaxAge(createGroup(TestPropsValues.getCompanyId()),_maxAgeDay);
    createTrashEntries(group);
  }
  TrashEntryLocalServiceUtil.checkEntries();
  Assert.assertEquals(_groupsCount * _notExpiredTrashEntriesCount,TrashEntryLocalServiceUtil.getTrashEntriesCount());
}
