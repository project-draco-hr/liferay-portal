{
  for (int i=0; i < _companiesCount; i++) {
    Group group=setTrashEntriesMaxAge(createGroup(createCompany()),_maxAgeDay);
    createTrashEntries(group);
  }
  TrashEntryLocalServiceUtil.checkEntries();
  Assert.assertEquals(_companiesCount * _notExpiredTrashEntriesCount,TrashEntryLocalServiceUtil.getTrashEntriesCount());
}
