{
  if (_fileShortcut == null) {
    return;
  }
  PortletURL portletURL=_getRenderURL("/document_library/view_file_entry");
  portletURL.setParameter("redirect",_getCurrentURL());
  portletURL.setParameter("fileEntryId",String.valueOf(_fileShortcut.getToFileEntryId()));
  _addURLUIItem(new URLMenuItem(),menuItems,DLUIItemKeys.VIEW_ORIGINAL_FILE,"view-original-file",portletURL.toString());
}
