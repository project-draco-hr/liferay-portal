{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ExpandoColumn.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("columnId",RandomTestUtil.nextLong()));
  List<ExpandoColumn> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
