{
  OrgLabor orgLabor=findByPrimaryKey(orgLaborId);
  Session session=null;
  try {
    session=openSession();
    OrgLabor[] array=new OrgLaborImpl[3];
    array[0]=getByOrganizationId_PrevAndNext(session,orgLabor,organizationId,orderByComparator,true);
    array[1]=orgLabor;
    array[2]=getByOrganizationId_PrevAndNext(session,orgLabor,organizationId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
