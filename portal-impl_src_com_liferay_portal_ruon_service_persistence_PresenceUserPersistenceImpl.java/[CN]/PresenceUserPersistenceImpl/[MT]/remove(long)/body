{
  Session session=null;
  try {
    session=openSession();
    PresenceUser presenceUser=(PresenceUser)session.get(PresenceUserImpl.class,new Long(presenceUserId));
    if (presenceUser == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No PresenceUser exists with the primary key " + presenceUserId);
      }
      throw new NoSuchPresenceUserException("No PresenceUser exists with the primary key " + presenceUserId);
    }
    return remove(presenceUser);
  }
 catch (  NoSuchPresenceUserException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
