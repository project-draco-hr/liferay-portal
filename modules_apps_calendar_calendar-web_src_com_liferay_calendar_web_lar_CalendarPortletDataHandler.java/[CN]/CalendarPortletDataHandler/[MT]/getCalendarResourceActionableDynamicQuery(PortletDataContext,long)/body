{
  ExportActionableDynamicQuery exportActionableDynamicQuery=CalendarResourceLocalServiceUtil.getExportActionableDynamicQuery(portletDataContext);
  exportActionableDynamicQuery.setStagedModelType(new StagedModelType(PortalUtil.getClassNameId(CalendarResource.class),referrerClassNameId));
