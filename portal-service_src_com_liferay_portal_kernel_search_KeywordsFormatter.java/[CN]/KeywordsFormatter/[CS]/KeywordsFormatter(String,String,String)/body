{
  Pattern keywordsDelimiterRegExpPattern=Pattern.compile(keywordsDelimiterRegExp);
  if (Validator.isNull(suggestedKeywords)) {
    _suggestedKeywords=Collections.emptyList();
  }
 else {
    _suggestedKeywords=Arrays.asList(keywordsDelimiterRegExpPattern.split(suggestedKeywords));
  }
  if (Validator.isNull(originalKeywords)) {
    _originalKeywords=Collections.emptyList();
  }
 else {
    _originalKeywords=Arrays.asList(keywordsDelimiterRegExpPattern.split(originalKeywords));
  }
}
