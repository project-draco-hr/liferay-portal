{
  long receipt=MailboxUtil.depositMail(datagram.getDataByteBuffer());
  byte[] data=new byte[8];
  BigEndianCodec.putLong(data,0,receipt);
  Intraband intraband=registrationReference.getIntraband();
  intraband.sendDatagram(registrationReference,Datagram.createResponseDatagram(datagram,ByteBuffer.wrap(data)));
}
