{
  Map<String,Serializable> workflowContext=new HashMap<>();
  workflowContext.put(WorkflowConstants.CONTEXT_URL,"http://localhost");
  workflowContext.put("event",DLSyncConstants.EVENT_ADD);
  DLAppHelperLocalServiceUtil.updateStatus(userId,fileEntry,fileEntry.getLatestFileVersion(),WorkflowConstants.STATUS_PENDING,WorkflowConstants.STATUS_APPROVED,serviceContext,workflowContext);
}
