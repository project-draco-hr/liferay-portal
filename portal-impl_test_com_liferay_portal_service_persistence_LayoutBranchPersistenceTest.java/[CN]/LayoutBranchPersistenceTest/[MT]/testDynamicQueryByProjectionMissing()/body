{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(LayoutBranch.class,LayoutBranch.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("LayoutBranchId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("LayoutBranchId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
