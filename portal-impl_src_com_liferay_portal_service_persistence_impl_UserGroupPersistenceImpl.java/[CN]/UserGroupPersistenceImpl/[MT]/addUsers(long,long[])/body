{
  long companyId=0;
  UserGroup userGroup=fetchByPrimaryKey(pk);
  if (userGroup == null) {
    companyId=companyProvider.getCompanyId();
  }
 else {
    companyId=userGroup.getCompanyId();
  }
  for (  long userPK : userPKs) {
    userGroupToUserTableMapper.addTableMapping(companyId,pk,userPK);
  }
}
