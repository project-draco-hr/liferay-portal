{
  long companyId=0;
  UserGroup userGroup=fetchByPrimaryKey(pk);
  if (userGroup == null) {
    companyId=companyProvider.getCompanyId();
  }
 else {
    companyId=userGroup.getCompanyId();
  }
  for (  long groupPK : groupPKs) {
    userGroupToGroupTableMapper.addTableMapping(companyId,pk,groupPK);
  }
}
