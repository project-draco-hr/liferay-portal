{
  long companyId=0;
  UserGroup userGroup=fetchByPrimaryKey(pk);
  if (userGroup == null) {
    companyId=companyProvider.getCompanyId();
  }
 else {
    companyId=userGroup.getCompanyId();
  }
  for (  long teamPK : teamPKs) {
    userGroupToTeamTableMapper.addTableMapping(companyId,pk,teamPK);
  }
}
