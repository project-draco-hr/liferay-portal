{
  Session session=null;
  try {
    session=openSession();
    UserGroup userGroup=(UserGroup)session.get(UserGroupImpl.class,primaryKey);
    if (userGroup == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchUserGroupException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(userGroup);
  }
 catch (  NoSuchUserGroupException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
