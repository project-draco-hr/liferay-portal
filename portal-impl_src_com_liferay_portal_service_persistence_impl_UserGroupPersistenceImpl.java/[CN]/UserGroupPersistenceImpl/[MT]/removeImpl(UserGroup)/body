{
  userGroup=toUnwrappedModel(userGroup);
  userGroupToGroupTableMapper.deleteLeftPrimaryKeyTableMappings(companyProvider.getCompanyId(),userGroup.getPrimaryKey());
  userGroupToTeamTableMapper.deleteLeftPrimaryKeyTableMappings(companyProvider.getCompanyId(),userGroup.getPrimaryKey());
  userGroupToUserTableMapper.deleteLeftPrimaryKeyTableMappings(companyProvider.getCompanyId(),userGroup.getPrimaryKey());
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(userGroup)) {
      userGroup=(UserGroup)session.get(UserGroupImpl.class,userGroup.getPrimaryKeyObj());
    }
    if (userGroup != null) {
      session.delete(userGroup);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (userGroup != null) {
    clearCache(userGroup);
  }
  return userGroup;
}
