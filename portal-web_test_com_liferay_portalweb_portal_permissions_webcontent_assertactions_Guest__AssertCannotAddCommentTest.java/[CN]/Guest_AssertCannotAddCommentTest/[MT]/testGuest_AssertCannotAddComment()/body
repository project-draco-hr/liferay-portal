{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Display Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Web Content Display Permissions Test Page"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isElementPresent("link=Add Comment"));
}
