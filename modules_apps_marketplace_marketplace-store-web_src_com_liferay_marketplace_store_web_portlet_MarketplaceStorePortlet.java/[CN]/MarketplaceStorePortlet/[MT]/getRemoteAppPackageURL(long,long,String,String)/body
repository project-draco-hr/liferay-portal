{
  String encodedClientId=MarketplaceUtil.encodeClientId(companyId,userId,token);
  String portletNameSpace=PortalUtil.getPortletNamespace(OSB_PORTLET_ID);
  url=HttpUtil.addParameter(url,portletNameSpace.concat("clientId"),encodedClientId);
  url=HttpUtil.addParameter(url,portletNameSpace.concat("token"),token);
  return url;
}
