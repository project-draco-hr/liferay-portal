{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Shopping Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace("Shopping Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Cart",RuntimeVariables.replace("Cart"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Shopping Category Item Name\nShopping Category Item Description\n\nAvailability: In Stock\n\n\nPrice for 1 Items and Above:$9.99"),selenium.getText("//td[2]/a"));
  selenium.clickAt("//input[@value='Empty Cart']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals(RuntimeVariables.replace("Your cart is empty."),selenium.getText("//div[@class='portlet-msg-info']"));
  assertTrue(selenium.isTextPresent("Your request completed successfully."));
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals(RuntimeVariables.replace("Your cart is empty."),selenium.getText("//div[@class='portlet-msg-info']"));
  assertFalse(selenium.isTextPresent("Shopping Category Item Name\nShopping Category Item Description\n\nAvailability: In Stock\n\n\nPrice for 1 Items and Above:$9.99"));
}
