{
  if (type == SocialEquitySettingConstants.TYPE_INFORMATION) {
    return equals(equityActionMapping.getInformationDailyLimit(),equityActionMapping.getInformationLifespan(),type,equityActionMapping.isInformationUnique(),equityActionMapping.getInformationValue());
  }
 else {
    return equals(equityActionMapping.getParticipationDailyLimit(),equityActionMapping.getParticipationLifespan(),type,equityActionMapping.isParticipationUnique(),equityActionMapping.getParticipationValue());
  }
}
