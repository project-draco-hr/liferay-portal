{
  if (type == SocialEquitySettingConstants.TYPE_INFORMATION) {
    if ((_informationDailyLimit != dailyLimit) || (_informationLifespan != lifeSpan) || (_informationUnique == unique)|| (_informationValue != value)) {
      return false;
    }
  }
 else {
    if ((_participationDailyLimit != dailyLimit) || (_participationLifespan != lifeSpan) || (_participationUnique == unique)|| (_participationValue != value)) {
      return false;
    }
  }
  return true;
}
