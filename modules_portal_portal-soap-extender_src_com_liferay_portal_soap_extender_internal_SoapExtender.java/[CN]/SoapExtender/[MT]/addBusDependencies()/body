{
  SoapExtenderConfiguration soapExtenderConfiguration=getSoapExtenderConfiguration();
  String[] contextPaths=soapExtenderConfiguration.contextPaths();
  if (contextPaths == null) {
    return;
  }
  for (  String contextPath : contextPaths) {
    String contextPathFilterString="(" + HttpWhiteboardConstants.HTTP_WHITEBOARD_CONTEXT_PATH + "="+ contextPath+ ")";
    addTCCLDependency(true,Bus.class,contextPathFilterString,"addBus","removeBus");
  }
}
