{
  selenium.click("//div[@id='navigation']/ul/li[1]/a/span");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Manage Pages");
  selenium.waitForPageToLoad("30000");
  selenium.click("//div[@id='_88_layoutsTreeOutput']/ul/li[2]/ul/li[3]/a/span");
  selenium.waitForPageToLoad("30000");
  selenium.click("//input[@value='Delete']");
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete the selected page[\\s\\S]$"));
  selenium.click("link=Return to Full Page");
  selenium.waitForPageToLoad("30000");
}
