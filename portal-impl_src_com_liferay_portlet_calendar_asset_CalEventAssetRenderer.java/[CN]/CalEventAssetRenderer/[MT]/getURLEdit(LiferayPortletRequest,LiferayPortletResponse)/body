{
  HttpServletRequest request=liferayPortletRequest.getHttpServletRequest();
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  PortletURL portletURL=PortletURLFactoryUtil.create(request,PortletKeys.CALENDAR,getControlPanelPlid(themeDisplay),PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/calendar/edit_event");
  portletURL.setParameter("eventId",String.valueOf(_event.getEventId()));
  return portletURL;
}
