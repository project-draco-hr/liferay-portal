{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMDataProviderInstanceLink.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("dataProviderInstanceLinkId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("dataProviderInstanceLinkId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
