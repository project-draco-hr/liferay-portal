{
  DDMDataProviderInstanceLink newDDMDataProviderInstanceLink=addDDMDataProviderInstanceLink();
  _persistence.clearCache();
  DDMDataProviderInstanceLink existingDDMDataProviderInstanceLink=_persistence.findByPrimaryKey(newDDMDataProviderInstanceLink.getPrimaryKey());
  Assert.assertEquals(Long.valueOf(existingDDMDataProviderInstanceLink.getDataProviderInstanceId()),ReflectionTestUtil.<Long>invoke(existingDDMDataProviderInstanceLink,"getOriginalDataProviderInstanceId",new Class<?>[0]));
  Assert.assertEquals(Long.valueOf(existingDDMDataProviderInstanceLink.getStructureId()),ReflectionTestUtil.<Long>invoke(existingDDMDataProviderInstanceLink,"getOriginalStructureId",new Class<?>[0]));
}
