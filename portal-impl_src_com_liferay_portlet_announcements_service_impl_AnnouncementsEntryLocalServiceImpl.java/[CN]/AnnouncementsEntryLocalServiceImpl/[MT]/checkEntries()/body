{
  Date now=new Date();
  List<AnnouncementsEntry> entries=announcementsEntryFinder.findByDisplayDate(now,new Date(now.getTime() - _ANNOUNCEMENTS_ENTRY_CHECK_INTERVAL));
  if (_log.isDebugEnabled()) {
    _log.debug("Processing " + entries.size() + " entries");
  }
  for (  AnnouncementsEntry entry : entries) {
    notifyUsers(entry);
  }
}
