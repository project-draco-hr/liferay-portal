{
  Date now=new Date();
  List<AnnouncementsEntry> entries=announcementsEntryFinder.findByDisplayDate(now,new Date(now.getTime() - _entryCheckInterval));
  if (_log.isDebugEnabled()) {
    _log.debug("Processing " + entries.size() + " entries");
  }
  for (  AnnouncementsEntry entry : entries) {
    try {
      notifyUsers(entry);
    }
 catch (    IOException ioe) {
      throw new SystemException(ioe);
    }
  }
}
