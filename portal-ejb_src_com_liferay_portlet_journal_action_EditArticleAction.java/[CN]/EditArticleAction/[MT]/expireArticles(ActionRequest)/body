{
  String companyId=PortalUtil.getCompanyId(req);
  String articleId=ParamUtil.getString(req,"articleId");
  double version=ParamUtil.getDouble(req,"version");
  if (Validator.isNotNull(articleId)) {
    String articleURL=ParamUtil.getString(req,"articleURL");
    JournalArticleServiceUtil.expireArticle(companyId,articleId,version,articleURL,req.getPreferences());
  }
 else {
    String[] expireArticleIds=StringUtil.split(ParamUtil.getString(req,"expireArticleIds"));
    for (int i=0; i < expireArticleIds.length; i++) {
      int pos=expireArticleIds[i].lastIndexOf(VERSION_SEPARATOR);
      articleId=expireArticleIds[i].substring(0,pos);
      version=GetterUtil.getDouble(expireArticleIds[i].substring(pos + VERSION_SEPARATOR.length()));
      JournalArticleServiceUtil.expireArticle(companyId,articleId,version,null,null);
    }
  }
}
