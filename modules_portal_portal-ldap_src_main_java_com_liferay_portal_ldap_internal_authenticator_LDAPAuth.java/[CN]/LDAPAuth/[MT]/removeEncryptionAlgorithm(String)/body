{
  int openBrace=ldapPassword.indexOf(StringPool.OPEN_CURLY_BRACE);
  if (openBrace == -1) {
    return ldapPassword;
  }
  int closeBrace=ldapPassword.indexOf(StringPool.CLOSE_CURLY_BRACE);
  if (closeBrace == -1) {
    return ldapPassword;
  }
  return ldapPassword.substring(openBrace,closeBrace + 1);
}
