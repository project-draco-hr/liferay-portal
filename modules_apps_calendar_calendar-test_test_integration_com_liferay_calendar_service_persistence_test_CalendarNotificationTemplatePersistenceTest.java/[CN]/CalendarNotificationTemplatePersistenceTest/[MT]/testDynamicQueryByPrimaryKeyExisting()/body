{
  CalendarNotificationTemplate newCalendarNotificationTemplate=addCalendarNotificationTemplate();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(CalendarNotificationTemplate.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("calendarNotificationTemplateId",newCalendarNotificationTemplate.getCalendarNotificationTemplateId()));
  List<CalendarNotificationTemplate> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  CalendarNotificationTemplate existingCalendarNotificationTemplate=result.get(0);
  Assert.assertEquals(existingCalendarNotificationTemplate,newCalendarNotificationTemplate);
}
