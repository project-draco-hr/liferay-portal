{
  request.setAttribute("wikiAttachmentItemSelectorCriterion",wikiAttachmentItemSelectorCriterion);
  request.setAttribute("portletURL",portletURL);
  request.setAttribute("itemSelectedCallback",itemSelectedCallback);
  RequestDispatcher requestDispatcher=request.getRequestDispatcher("/o/wiki-item-selector/attachments.jsp");
  requestDispatcher.include(request,response);
}
