{
  Logger logger=Logger.getLogger("");
  logger.setLevel(Level.FINE);
  try {
    ServerThread serverThread=new ServerThread(Thread.currentThread(),"Parent Server Thread",_serverPort);
    serverThread.start();
    Constructor<ProcessCallable<?>> constructor=_processCallableClass.getConstructor(int.class);
    ProcessExecutor processExecutor=new LocalProcessExecutor();
    processExecutor.execute(_createJPDAProcessConfig(_JPDA_OPTIONS2),constructor.newInstance(_serverPort));
  }
 catch (  Exception e) {
    throw new ProcessException(e);
  }
  try {
    Thread.sleep(Long.MAX_VALUE);
  }
 catch (  InterruptedException ie) {
  }
  return null;
}
