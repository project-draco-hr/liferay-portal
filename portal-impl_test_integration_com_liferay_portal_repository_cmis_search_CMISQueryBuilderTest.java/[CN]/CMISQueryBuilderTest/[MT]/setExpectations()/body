{
  when(_repositoryEntry.getMappedId()).thenReturn("1000");
  when(_repositoryEntryLocalService.fetchRepositoryEntry(Matchers.eq(1000l))).thenReturn(_repositoryEntry);
  when(_beanLocatorProxy.locate(RepositoryEntryLocalService.class.getName())).thenReturn(_repositoryEntryLocalService);
  when(_beanLocatorProxy.locate(DLAppService.class.getName())).thenReturn(_dlAppService);
}
