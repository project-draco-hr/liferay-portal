{
  if (clusterAware && clusterEnabled) {
    return true;
  }
  if (cacheConfiguration.isOverflowToDisk() || cacheConfiguration.isOverflowToOffHeap() || cacheConfiguration.isDiskPersistent()) {
    return true;
  }
  PersistenceConfiguration persistenceConfiguration=cacheConfiguration.getPersistenceConfiguration();
  if (persistenceConfiguration != null) {
    PersistenceConfiguration.Strategy strategy=persistenceConfiguration.getStrategy();
    if (!strategy.equals(PersistenceConfiguration.Strategy.NONE)) {
      return true;
    }
  }
  return false;
}
