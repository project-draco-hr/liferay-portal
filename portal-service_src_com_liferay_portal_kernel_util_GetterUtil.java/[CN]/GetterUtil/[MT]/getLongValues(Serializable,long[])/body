{
  Class clazz=value.getClass();
  if (clazz.isArray()) {
    if (clazz.getComponentType().isAssignableFrom(String.class)) {
      return getLongValues((String[])value,defaultValue);
    }
 else     if (clazz.getComponentType().isAssignableFrom(Long.class)) {
      return (long[])value;
    }
  }
  return defaultValue;
}
