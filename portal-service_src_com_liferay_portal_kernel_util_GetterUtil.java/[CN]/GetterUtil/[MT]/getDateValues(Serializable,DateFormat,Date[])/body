{
  Class clazz=value.getClass();
  if (clazz.isArray()) {
    if (clazz.getComponentType().isAssignableFrom(String.class)) {
      return getDateValues((String[])value,dateFormat,defaultValue);
    }
 else     if (clazz.getComponentType().isAssignableFrom(Date.class)) {
      return (Date[])value;
    }
  }
  return defaultValue;
}
