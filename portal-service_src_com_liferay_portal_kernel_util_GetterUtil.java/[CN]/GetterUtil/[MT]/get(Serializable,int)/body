{
  if (value == null) {
    return defaultValue;
  }
  if (value instanceof String) {
    return get((String)value,defaultValue);
  }
 else   if (value.getClass().isAssignableFrom(Integer.class)) {
    return (Integer)value;
  }
 else   if (value instanceof Number) {
    return ((Number)value).intValue();
  }
  return defaultValue;
}
