{
  if (value == null) {
    return defaultValue;
  }
  if (value instanceof String) {
    return get((String)value,defaultValue);
  }
 else   if (value.getClass().isAssignableFrom(Long.class)) {
    return (Long)value;
  }
 else   if (value instanceof Number) {
    return ((Number)value).longValue();
  }
  return defaultValue;
}
