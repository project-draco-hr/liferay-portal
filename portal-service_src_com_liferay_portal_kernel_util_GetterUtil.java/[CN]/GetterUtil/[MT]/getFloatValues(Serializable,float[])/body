{
  Class clazz=value.getClass();
  if (clazz.isArray()) {
    if (clazz.getComponentType().isAssignableFrom(String.class)) {
      return getFloatValues((String[])value,defaultValue);
    }
 else     if (clazz.getComponentType().isAssignableFrom(Float.class)) {
      return (float[])value;
    }
  }
  return defaultValue;
}
