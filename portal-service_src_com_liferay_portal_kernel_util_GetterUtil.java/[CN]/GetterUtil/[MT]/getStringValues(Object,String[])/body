{
  if (value == null) {
    return defaultValue;
  }
  Class<?> clazz=value.getClass();
  if (clazz.isArray()) {
    Class<?> componentType=clazz.getComponentType();
    if (String.class.isAssignableFrom(componentType)) {
      return getStringValues((String[])value,defaultValue);
    }
  }
  return defaultValue;
}
