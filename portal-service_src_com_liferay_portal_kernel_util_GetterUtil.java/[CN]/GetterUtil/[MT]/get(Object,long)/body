{
  if (value == null) {
    return defaultValue;
  }
  if (value instanceof String) {
    return get((String)value,defaultValue);
  }
  Class<?> clazz=value.getClass();
  if (Long.class.isAssignableFrom(clazz)) {
    return (Long)value;
  }
  if (value instanceof Number) {
    Number number=(Number)value;
    return number.longValue();
  }
  return defaultValue;
}
