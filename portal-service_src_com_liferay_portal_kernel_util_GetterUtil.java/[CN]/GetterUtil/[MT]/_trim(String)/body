{
  value=value.trim();
  int length=value.length();
  StringBuilder sb=new StringBuilder(length);
  for (int i=0; i < length; i++) {
    char c=value.charAt(i);
    if ((Character.isDigit(c)) || ((c == CharPool.DASH) && (i == 0)) || (c == CharPool.PERIOD)|| (c == CharPool.UPPER_CASE_E)|| (c == CharPool.LOWER_CASE_E)) {
      sb.append(c);
    }
  }
  return sb.toString();
}
