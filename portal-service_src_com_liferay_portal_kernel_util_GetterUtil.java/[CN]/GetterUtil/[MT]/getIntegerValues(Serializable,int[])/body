{
  Class clazz=value.getClass();
  if (clazz.isArray()) {
    if (clazz.getComponentType().isAssignableFrom(String.class)) {
      return getIntegerValues((String[])value,defaultValue);
    }
 else     if (clazz.getComponentType().isAssignableFrom(Integer.class)) {
      return (int[])value;
    }
  }
  return defaultValue;
}
