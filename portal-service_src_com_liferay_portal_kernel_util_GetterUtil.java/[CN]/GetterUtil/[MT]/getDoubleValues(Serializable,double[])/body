{
  Class clazz=value.getClass();
  if (clazz.isArray()) {
    if (clazz.getComponentType().isAssignableFrom(String.class)) {
      return getDoubleValues((String[])value,defaultValue);
    }
 else     if (clazz.getComponentType().isAssignableFrom(Double.class)) {
      return (double[])value;
    }
  }
  return defaultValue;
}
