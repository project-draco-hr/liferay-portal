{
  if (value == null) {
    return defaultValue;
  }
  value=value.trim();
  value=value.toLowerCase();
  if (value.equals(BOOLEANS[0]) || value.equals(BOOLEANS[1]) || value.equals(BOOLEANS[2])|| value.equals(BOOLEANS[3])|| value.equals(BOOLEANS[4])) {
    return true;
  }
 else {
    return false;
  }
}
