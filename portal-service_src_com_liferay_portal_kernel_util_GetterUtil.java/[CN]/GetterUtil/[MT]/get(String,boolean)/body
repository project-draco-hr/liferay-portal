{
  if (value == null) {
    return defaultValue;
  }
  try {
    value=value.trim();
    if (value.equalsIgnoreCase(BOOLEANS[0]) || value.equalsIgnoreCase(BOOLEANS[1]) || value.equalsIgnoreCase(BOOLEANS[2])|| value.equalsIgnoreCase(BOOLEANS[3])|| value.equalsIgnoreCase(BOOLEANS[4])) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  Exception e) {
  }
  return defaultValue;
}
