{
  if (value == null) {
    return defaultValue;
  }
  try {
    value=value.trim().toLowerCase();
    if (value.equals(BOOLEANS[0]) || value.equals(BOOLEANS[1]) || value.equals(BOOLEANS[2])|| value.equals(BOOLEANS[3])|| value.equals(BOOLEANS[4])) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  Exception e) {
  }
  return defaultValue;
}
