{
  int size=10;
  UnsyncBufferedInputStream unsyncBufferedInputStream=new UnsyncBufferedInputStream(new ByteArrayInputStream(new byte[size]));
  assertEquals(size,unsyncBufferedInputStream.available());
  unsyncBufferedInputStream=new UnsyncBufferedInputStream(new ByteArrayInputStream(new byte[size]),_SIZE);
  assertEquals(size,unsyncBufferedInputStream.available());
}
