{
  if (this == obj) {
    return true;
  }
  if (!(obj instanceof Tuple)) {
    return false;
  }
  Tuple tuple=(Tuple)obj;
  if (tuple._array.length != _array.length) {
    return false;
  }
  for (int i=0; i < _array.length; i++) {
    if ((tuple._array != null) && (_array[i] != null) && !_array[i].equals(tuple._array[i])) {
      return false;
    }
