{
  if (_hasAddDDMTemplatePermission != null) {
    return _hasAddDDMTemplatePermission;
  }
  _hasAddDDMTemplatePermission=Boolean.FALSE;
  DDLRecordSet recordSet=getRecordSet();
  if (recordSet == null) {
    return _hasAddDDMTemplatePermission;
  }
  _hasAddDDMTemplatePermission=DDMTemplatePermission.containsAddTemplatePermission(getPermissionChecker(),getScopeGroupId(),getStructureTypeClassNameId(),getStructureTypeClassNameId());
  return _hasAddDDMTemplatePermission;
}
