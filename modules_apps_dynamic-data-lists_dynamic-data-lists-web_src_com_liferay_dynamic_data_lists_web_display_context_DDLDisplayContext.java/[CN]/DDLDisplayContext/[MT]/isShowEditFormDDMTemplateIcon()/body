{
  if (_hasEditFormDDMTemplatePermission != null) {
    return _hasEditFormDDMTemplatePermission;
  }
  _hasEditFormDDMTemplatePermission=Boolean.FALSE;
  if (getFormDDMTemplateId() == 0) {
    return _hasEditFormDDMTemplatePermission;
  }
  _hasEditFormDDMTemplatePermission=DDMTemplatePermission.contains(getPermissionChecker(),getScopeGroupId(),getFormDDMTemplateId(),DDLPortletKeys.DYNAMIC_DATA_LISTS,ActionKeys.UPDATE);
  return _hasEditFormDDMTemplatePermission;
}
