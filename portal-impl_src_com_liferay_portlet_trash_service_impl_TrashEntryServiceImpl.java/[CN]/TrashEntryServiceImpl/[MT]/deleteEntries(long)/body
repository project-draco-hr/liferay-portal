{
  boolean error=false;
  List<TrashEntry> entries=trashEntryPersistence.findByGroupId(groupId);
  for (  TrashEntry entry : entries) {
    try {
      deleteEntry(entry);
    }
 catch (    TrashPermissionException tpe) {
      error=true;
    }
catch (    Exception e) {
      _log.error(e,e);
    }
  }
  if (error) {
    throw new TrashPermissionException(TrashPermissionException.ERROR_EMPTY_TRASH);
  }
}
