{
  boolean error=false;
  List<TrashEntry> entries=trashEntryLocalService.getEntries(groupId);
  for (  TrashEntry entry : entries) {
    try {
      deleteEntry(entry);
    }
 catch (    Exception e) {
      error=true;
      _log.error(e);
    }
  }
  if (error) {
    throw new PrincipalException("trash.empty.error");
  }
}
