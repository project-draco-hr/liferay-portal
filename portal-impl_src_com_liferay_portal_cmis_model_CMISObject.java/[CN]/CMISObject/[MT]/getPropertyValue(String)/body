{
  String text=null;
  Element element=getFirstChild(_constants.PROPERTIES);
  List<Element> properties=element.getElements();
  for (  Element property : properties) {
    String attributeValue=property.getAttributeValue(_constants.PROPERTY_NAME);
    if (propertyName.equals(attributeValue)) {
      Element value=property.getFirstChild(_constants.PROPERTY_VALUE);
      if (value != null) {
        text=value.getText();
      }
      break;
    }
  }
  return text;
}
