{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SocialActivity.class,SocialActivity.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("activityId",RandomTestUtil.nextLong()));
  List<SocialActivity> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
