{
  User user=userPersistence.findByPrimaryKey(userId);
  if (user.isDefaultUser()) {
    return true;
  }
  int readThreadMessageCount=mbMessageFlagFinder.countByU_T_F(userId,threadId,MBMessageFlagImpl.READ_FLAG);
  if (readThreadMessageCount != threadMessageCount) {
    return false;
  }
 else {
    return true;
  }
}
