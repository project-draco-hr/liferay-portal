{
  User user=userPersistence.findByPrimaryKey(userId);
  if (user.isDefaultUser()) {
    return true;
  }
  int total=mbMessagePersistence.countByThreadId(threadId);
  int read=mbMessageFlagFinder.countByU_T(userId,threadId);
  if (total != read) {
    return false;
  }
 else {
    return true;
  }
}
