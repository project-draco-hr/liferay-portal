{
  if (userId <= 0) {
    return mbThreadPersistence.findByGroupId(groupId,start,end);
  }
 else {
    if (subscribed) {
      return mbThreadFinder.findByS_G_U(groupId,userId,start,end);
    }
 else {
      List<Long> threadIds=null;
      if (includeAnonymous) {
        threadIds=mbMessageFinder.findByG_U(groupId,userId,start,end);
      }
 else {
        threadIds=mbMessageFinder.findByG_U_A(groupId,userId,false,start,end);
      }
      List<MBThread> threads=new ArrayList<MBThread>(threadIds.size());
      for (      long threadId : threadIds) {
        MBThread thread=mbThreadPersistence.findByPrimaryKey(threadId);
        threads.add(thread);
      }
      return threads;
    }
  }
}
