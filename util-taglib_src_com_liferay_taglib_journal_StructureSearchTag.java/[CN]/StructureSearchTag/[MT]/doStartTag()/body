{
  try {
    List<JournalStructure> structures=JournalStructureLocalServiceUtil.search(_companyId,_groupId,_structureId,_name,_description,_andOperator,_start,_end,_obc);
    pageContext.setAttribute(_var,structures);
    return SKIP_BODY;
  }
 catch (  Exception e) {
