{
  List<JournalStructure> structures;
  try {
    structures=JournalStructureLocalServiceUtil.search(_companyId,_groupId,_structureId,_name,_description,_andOperator,_start,_end,_obc);
  }
 catch (  SystemException se) {
    throw new JspException(se);
  }
  pageContext.setAttribute(_var,structures);
  return SKIP_BODY;
}
