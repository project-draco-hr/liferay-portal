{
  List<MDRRuleGroupInstance> ruleGroupInstances=MDRRuleGroupInstanceLocalServiceUtil.getMDRRuleGroupInstancesByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<MDRRuleGroupInstance>());
  if (ListUtil.isEmpty(ruleGroupInstances)) {
    return null;
  }
  return ruleGroupInstances.get(0);
}
