{
  PortletURL editLayoutURL=PortalUtil.getControlPanelPortletURL(liferayPortletRequest,LayoutAdminPortletKeys.GROUP_PAGES,PortletRequest.RENDER_PHASE);
  if (selPlid >= LayoutConstants.DEFAULT_PLID) {
    editLayoutURL.setParameter("selPlid",String.valueOf(selPlid));
  }
  if (privateLayout != null) {
    editLayoutURL.setParameter("privateLayout",String.valueOf(privateLayout));
  }
  Group liveGroup=getLiveGroup();
  editLayoutURL.setParameter("groupId",String.valueOf(liveGroup.getGroupId()));
  editLayoutURL.setParameter("viewLayout",Boolean.TRUE.toString());
  return editLayoutURL;
}
