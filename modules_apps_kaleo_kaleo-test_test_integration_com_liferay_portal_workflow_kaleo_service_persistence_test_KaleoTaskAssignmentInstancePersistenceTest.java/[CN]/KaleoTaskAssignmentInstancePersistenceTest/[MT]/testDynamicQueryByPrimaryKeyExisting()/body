{
  KaleoTaskAssignmentInstance newKaleoTaskAssignmentInstance=addKaleoTaskAssignmentInstance();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(KaleoTaskAssignmentInstance.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("kaleoTaskAssignmentInstanceId",newKaleoTaskAssignmentInstance.getKaleoTaskAssignmentInstanceId()));
  List<KaleoTaskAssignmentInstance> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  KaleoTaskAssignmentInstance existingKaleoTaskAssignmentInstance=result.get(0);
  Assert.assertEquals(existingKaleoTaskAssignmentInstance,newKaleoTaskAssignmentInstance);
}
