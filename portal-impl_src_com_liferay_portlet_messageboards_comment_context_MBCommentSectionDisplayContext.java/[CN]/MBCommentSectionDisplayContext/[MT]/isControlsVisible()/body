{
  if (_discussionRequestHelper.isHideControls()) {
    return false;
  }
  return MBDiscussionPermission.contains(_discussionRequestHelper.getPermissionChecker(),_discussionRequestHelper.getCompanyId(),_discussionRequestHelper.getScopeGroupId(),_discussionRequestHelper.getPermissionClassName(),_discussionRequestHelper.getPermissionClassPK(),_discussionRequestHelper.getUserId(),ActionKeys.ADD_DISCUSSION);
}
