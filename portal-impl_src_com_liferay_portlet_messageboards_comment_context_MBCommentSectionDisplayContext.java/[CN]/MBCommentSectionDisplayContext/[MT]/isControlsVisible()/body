{
  if (_discussionTaglibHelper.isHideControls()) {
    return false;
  }
  return MBDiscussionPermission.contains(_discussionRequestHelper.getPermissionChecker(),_discussionRequestHelper.getCompanyId(),_discussionRequestHelper.getScopeGroupId(),_discussionTaglibHelper.getPermissionClassName(),_discussionTaglibHelper.getPermissionClassPK(),_discussionTaglibHelper.getUserId(),ActionKeys.ADD_DISCUSSION);
}
