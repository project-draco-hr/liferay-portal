{
  if (kaleoTask.isNew()) {
    Object[] args=new Object[]{kaleoTask.getKaleoNodeId()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_KALEONODEID,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_KALEONODEID,args,kaleoTask);
  }
 else {
    KaleoTaskModelImpl kaleoTaskModelImpl=(KaleoTaskModelImpl)kaleoTask;
    if ((kaleoTaskModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_KALEONODEID.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{kaleoTask.getKaleoNodeId()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_KALEONODEID,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_KALEONODEID,args,kaleoTask);
    }
  }
}
