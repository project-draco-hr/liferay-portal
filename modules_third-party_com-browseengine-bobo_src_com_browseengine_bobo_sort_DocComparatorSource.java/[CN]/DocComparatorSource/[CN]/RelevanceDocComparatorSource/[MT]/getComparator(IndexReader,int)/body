{
  return new DocComparator(){
    public int compare(    ScoreDoc doc1,    ScoreDoc doc2){
      if (doc1.score < doc2.score) {
        return -1;
      }
 else       if (doc1.score > doc2.score) {
        return 1;
      }
 else {
        return 0;
      }
    }
    public Float value(    ScoreDoc doc){
      return Float.valueOf(doc.score);
    }
  }
;
}
