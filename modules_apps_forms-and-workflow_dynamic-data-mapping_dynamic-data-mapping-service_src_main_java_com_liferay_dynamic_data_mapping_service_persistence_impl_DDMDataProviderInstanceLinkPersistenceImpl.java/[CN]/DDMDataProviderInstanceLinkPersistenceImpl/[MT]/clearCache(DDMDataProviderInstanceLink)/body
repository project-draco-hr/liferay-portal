{
  entityCache.removeResult(DDMDataProviderInstanceLinkModelImpl.ENTITY_CACHE_ENABLED,DDMDataProviderInstanceLinkImpl.class,ddmDataProviderInstanceLink.getPrimaryKey());
  finderCache.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  finderCache.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
