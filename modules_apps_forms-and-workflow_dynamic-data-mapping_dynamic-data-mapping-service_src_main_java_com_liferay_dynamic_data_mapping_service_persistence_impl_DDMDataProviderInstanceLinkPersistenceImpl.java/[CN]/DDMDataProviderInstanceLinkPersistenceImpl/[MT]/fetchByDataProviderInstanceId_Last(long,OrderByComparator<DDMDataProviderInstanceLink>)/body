{
  int count=countByDataProviderInstanceId(dataProviderInstanceId);
  if (count == 0) {
    return null;
  }
  List<DDMDataProviderInstanceLink> list=findByDataProviderInstanceId(dataProviderInstanceId,count - 1,count,orderByComparator);
  if (!list.isEmpty()) {
    return list.get(0);
  }
  return null;
}
