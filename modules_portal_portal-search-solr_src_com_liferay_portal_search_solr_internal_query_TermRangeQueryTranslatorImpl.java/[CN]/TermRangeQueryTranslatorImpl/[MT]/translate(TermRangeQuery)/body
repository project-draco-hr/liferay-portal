{
  org.apache.lucene.search.TermRangeQuery luceneTermRangeQuery=org.apache.lucene.search.TermRangeQuery.newStringRange(termRangeQuery.getField(),termRangeQuery.getLowerTerm(),termRangeQuery.getUpperTerm(),termRangeQuery.includesLower(),termRangeQuery.includesUpper());
  if (!termRangeQuery.isDefaultBoost()) {
    luceneTermRangeQuery.setBoost(termRangeQuery.getBoost());
  }
  return luceneTermRangeQuery;
}
