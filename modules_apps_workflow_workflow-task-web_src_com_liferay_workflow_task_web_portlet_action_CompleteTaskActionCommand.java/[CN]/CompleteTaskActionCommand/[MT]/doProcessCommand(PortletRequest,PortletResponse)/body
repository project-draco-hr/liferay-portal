{
  long workflowTaskId=ParamUtil.getLong(portletRequest,WorkflowTaskConstants.WORKFLOW_TASK_ID);
  String transitionName=ParamUtil.getString(portletRequest,WorkflowTaskConstants.TRANSITION_NAME);
  String comment=ParamUtil.getString(portletRequest,WorkflowTaskConstants.COMMENT);
  WorkflowTaskManagerUtil.completeWorkflowTask(getCompanyId(portletRequest),getUserId(portletRequest),workflowTaskId,transitionName,comment,null);
  super.doProcessCommand(portletRequest,portletResponse);
}
