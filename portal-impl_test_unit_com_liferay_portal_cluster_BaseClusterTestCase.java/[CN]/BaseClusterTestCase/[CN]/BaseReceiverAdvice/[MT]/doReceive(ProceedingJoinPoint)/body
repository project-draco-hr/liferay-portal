{
  Receiver receiver=(Receiver)proceedingJoinPoint.getThis();
  org.jgroups.Message jGroupsMessage=(org.jgroups.Message)proceedingJoinPoint.getArgs()[0];
  List<org.jgroups.Message> jGroupsMessageList=_jGroupsMessages.get(receiver);
  if (jGroupsMessageList == null) {
    jGroupsMessageList=new ArrayList<org.jgroups.Message>();
    List<org.jgroups.Message> previousJgroupsMessageList=_jGroupsMessages.putIfAbsent(receiver,jGroupsMessageList);
    if (previousJgroupsMessageList != null) {
      jGroupsMessageList=previousJgroupsMessageList;
    }
  }
  jGroupsMessageList.add(jGroupsMessage);
  _countDownLatch.countDown();
}
