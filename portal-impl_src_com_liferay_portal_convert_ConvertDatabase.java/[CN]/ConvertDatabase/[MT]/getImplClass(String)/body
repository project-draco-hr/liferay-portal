{
  try {
    ClassLoader classLoader=ClassLoaderUtil.getPortalClassLoader();
    return classLoader.loadClass(implClassName);
  }
 catch (  Exception e) {
  }
  for (  String servletContextName : ServletContextPool.keySet()) {
    try {
      ServletContext servletContext=ServletContextPool.get(servletContextName);
      ClassLoader classLoader=(ClassLoader)servletContext.getAttribute(PluginContextListener.PLUGIN_CLASS_LOADER);
      return classLoader.loadClass(implClassName);
    }
 catch (    Exception e) {
    }
  }
  return null;
}
