{
  if (values == null || values.size() == 0) {
    return null;
  }
  StringBuffer sb=new StringBuffer();
  boolean prependComma=false;
  for (int i=0; i < values.size(); i++) {
    if (prependComma) {
      sb.append(",");
    }
    sb.append(values.get(i).toString());
    prependComma=true;
  }
  return sb.toString();
}
