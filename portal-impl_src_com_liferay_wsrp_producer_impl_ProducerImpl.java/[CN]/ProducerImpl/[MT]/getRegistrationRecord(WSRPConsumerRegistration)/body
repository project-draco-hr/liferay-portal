{
  try {
    RegistrationData registrationData=WSRPFactory.getInstance().getRegistrationData(consumerRegistration.getRegistrationData());
    RegistrationRecord registrationRecord=new RegistrationRecord(consumerRegistration.getRegistrationHandle(),consumerRegistration.getStatus(),registrationData,null);
    return registrationRecord;
  }
 catch (  Exception e) {
    throw new ProducerException(e);
  }
}
