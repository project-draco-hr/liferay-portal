{
  Field field=null;
  try {
    field=ReflectionUtil.getDeclaredField(tableMapper.getClass(),name);
    field.setAccessible(true);
    PortalCache<Long,long[]> portalCache=(PortalCache<Long,long[]>)field.get(tableMapper);
    portalCache.removeAll();
  }
  finally {
    field.setAccessible(false);
  }
}
