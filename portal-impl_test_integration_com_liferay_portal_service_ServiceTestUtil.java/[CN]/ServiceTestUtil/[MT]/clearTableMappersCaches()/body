{
  Field field=null;
  try {
    field=ReflectionUtil.getDeclaredField(TableMapperFactory.class,"tableMappers");
    field.setAccessible(true);
    Map<String,TableMapper<?,?>> tableMappers=(Map<String,TableMapper<?,?>>)field.get(null);
    for (    TableMapper<?,?> tableMapper : tableMappers.values()) {
      _clearCache(tableMapper,"leftToRightPortalCache");
      _clearCache(tableMapper,"rightToLeftPortalCache");
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
 finally {
    field.setAccessible(false);
  }
}
