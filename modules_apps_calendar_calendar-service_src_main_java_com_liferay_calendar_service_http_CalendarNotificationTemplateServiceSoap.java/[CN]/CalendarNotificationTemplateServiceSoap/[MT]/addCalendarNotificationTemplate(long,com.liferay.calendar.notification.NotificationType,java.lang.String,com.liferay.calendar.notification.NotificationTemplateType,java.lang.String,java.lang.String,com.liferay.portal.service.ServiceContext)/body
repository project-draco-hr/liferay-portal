{
  try {
    com.liferay.calendar.model.CalendarNotificationTemplate returnValue=CalendarNotificationTemplateServiceUtil.addCalendarNotificationTemplate(calendarId,notificationType,notificationTypeSettings,notificationTemplateType,subject,body,serviceContext);
