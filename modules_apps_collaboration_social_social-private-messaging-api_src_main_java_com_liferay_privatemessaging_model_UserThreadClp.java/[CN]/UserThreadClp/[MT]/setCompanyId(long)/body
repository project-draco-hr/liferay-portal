{
  _companyId=companyId;
  if (_userThreadRemoteModel != null) {
    try {
      Class<?> clazz=_userThreadRemoteModel.getClass();
      Method method=clazz.getMethod("setCompanyId",long.class);
      method.invoke(_userThreadRemoteModel,companyId);
