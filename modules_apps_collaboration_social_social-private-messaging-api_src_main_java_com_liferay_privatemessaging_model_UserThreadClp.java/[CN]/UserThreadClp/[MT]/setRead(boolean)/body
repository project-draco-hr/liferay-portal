{
  _read=read;
  if (_userThreadRemoteModel != null) {
    try {
      Class<?> clazz=_userThreadRemoteModel.getClass();
      Method method=clazz.getMethod("setRead",boolean.class);
      method.invoke(_userThreadRemoteModel,read);
