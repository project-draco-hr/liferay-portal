{
  _mockOriginalServletRequest.setParameter("blog_name","__blogName__");
  _mockOriginalServletRequest.setParameter("title","__title__");
  _mockOriginalServletRequest.setParameter("excerpt","__excerpt__");
  initValidUrl();
  addTrackback();
  assertSuccess();
  verify(_trackbacks).addTrackback(same(_blogsEntry),same(_themeDisplay),eq("__excerpt__"),eq("__url__"),eq("__blogName__"),eq("__title__"),(Function<String,ServiceContext>)any());
}
