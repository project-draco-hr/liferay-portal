{
  List<com.liferay.dynamic.data.mapping.model.DDMTemplate> ddmTemplates=_ddmStructure.getTemplates();
  List<DDMTemplate> templates=new ArrayList<>();
  for (  com.liferay.dynamic.data.mapping.model.DDMTemplate ddmTemplate : ddmTemplates) {
    templates.add(new DDMTemplateImpl(ddmTemplate));
  }
  return templates;
}
