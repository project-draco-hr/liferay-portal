{
  if (!roleLocalService.hasUserRole(getUserId(),companyId,RoleConstants.ADMINISTRATOR,true)) {
    throw new PrincipalException();
  }
  return companyLocalService.updateCompany(companyId,virtualHost,mx,homeURL,name,legalName,legalId,legalType,sicCode,tickerSymbol,industry,type,size,logo,logoBytes);
}
