{
  VerifyProcess verifyProcess=_bundleContext.getService(serviceReference);
  String verifyProcessName=getVerifyProcessName(serviceReference);
  if (Validator.isNull(verifyProcessName)) {
    return null;
  }
  _verifyProcesses.put(verifyProcessName,verifyProcess);
  if (_log.isDebugEnabled()) {
    Bundle bundle=_bundleContext.getBundle();
    _log.debug("Executing the verify process " + ClassUtil.getClassName(verifyProcess.getClass()) + " for "+ bundle.getSymbolicName());
  }
  try {
    verifyProcess.verify();
  }
 catch (  VerifyException ve) {
    _log.error("A verify exception was thrown while executing the verify " + "process " + ClassUtil.getClassName(verifyProcess.getClass()),ve);
  }
  return verifyProcess;
}
