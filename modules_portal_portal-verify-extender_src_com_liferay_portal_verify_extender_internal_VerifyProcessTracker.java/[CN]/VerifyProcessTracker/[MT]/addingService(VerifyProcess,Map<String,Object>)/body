{
  String verifyProcessName=(String)properties.get("verify.process.name");
  _verifyProcesses.put(verifyProcessName,verifyProcess);
  if (_log.isDebugEnabled()) {
    _log.debug("Executing the verify process " + ClassUtil.getClassName(verifyProcess.getClass()));
  }
  try {
    verifyProcess.verify();
  }
 catch (  VerifyException ve) {
    _log.error("A verify exception was thrown while executing the verify " + "process " + ClassUtil.getClassName(verifyProcess.getClass()),ve);
  }
}
