{
  long groupId=ParamUtil.getLong(actionRequest,"groupId");
  String[] expireArticleIds=StringUtil.split(ParamUtil.getString(actionRequest,"expireArticleIds"));
  ServiceContext serviceContext=ServiceContextFactory.getInstance(JournalArticle.class.getName(),actionRequest);
  for (int i=0; i < expireArticleIds.length; i++) {
    int pos=expireArticleIds[i].lastIndexOf(VERSION_SEPARATOR);
    String articleId=expireArticleIds[i];
    String articleURL=ParamUtil.getString(actionRequest,"articleURL");
    double version=0;
    if (pos == -1) {
      JournalArticleServiceUtil.expireArticle(groupId,articleId,articleURL,serviceContext);
    }
 else {
      articleId=articleId.substring(0,pos);
      version=GetterUtil.getDouble(expireArticleIds[i].substring(pos + VERSION_SEPARATOR.length()));
      JournalArticleServiceUtil.expireArticle(groupId,articleId,version,articleURL,serviceContext);
    }
  }
}
