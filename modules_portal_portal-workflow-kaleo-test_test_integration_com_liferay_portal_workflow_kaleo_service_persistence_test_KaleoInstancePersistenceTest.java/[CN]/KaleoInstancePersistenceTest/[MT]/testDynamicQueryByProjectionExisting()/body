{
  KaleoInstance newKaleoInstance=addKaleoInstance();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(KaleoInstance.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("kaleoInstanceId"));
  Object newKaleoInstanceId=newKaleoInstance.getKaleoInstanceId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("kaleoInstanceId",new Object[]{newKaleoInstanceId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Object existingKaleoInstanceId=result.get(0);
  Assert.assertEquals(existingKaleoInstanceId,newKaleoInstanceId);
}
