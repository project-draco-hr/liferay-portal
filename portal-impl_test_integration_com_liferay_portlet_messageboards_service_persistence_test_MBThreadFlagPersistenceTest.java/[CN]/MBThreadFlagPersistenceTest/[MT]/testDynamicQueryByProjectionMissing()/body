{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBThreadFlag.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("threadFlagId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("threadFlagId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
