{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MDRRule.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("ruleId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("ruleId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
