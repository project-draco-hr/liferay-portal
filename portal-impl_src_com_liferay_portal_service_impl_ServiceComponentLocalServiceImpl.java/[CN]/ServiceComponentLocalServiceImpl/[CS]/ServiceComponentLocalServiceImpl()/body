{
  Registry registry=RegistryUtil.getRegistry();
  Filter filter=registry.getFilter("(&(&(upgrade.from.schema.version=0.0.0)" + "(objectClass=" + UpgradeStep.class.getName() + "))"+ "(upgrade.initial.database.creation=true))");
  _serviceTrackerList=ServiceTrackerCollections.openList(UpgradeStep.class,filter);
  _serviceTrackerList.open();
}
