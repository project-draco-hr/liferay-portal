{
  Registry registry=RegistryUtil.getRegistry();
  Filter filter=registry.getFilter("(&(objectClass=" + UpgradeStep.class.getName() + ")(upgrade.from.schema.version=0.0.0)(upgrade.initial."+ "database.creation=true))");
  _serviceTrackerList=ServiceTrackerCollections.openList(UpgradeStep.class,filter);
