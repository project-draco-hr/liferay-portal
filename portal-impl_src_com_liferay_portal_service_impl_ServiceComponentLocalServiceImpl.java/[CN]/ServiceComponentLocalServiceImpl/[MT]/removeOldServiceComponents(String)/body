{
  int serviceComponentsCount=serviceComponentPersistence.countByBuildNamespace(buildNamespace);
  if (serviceComponentsCount < _SERVICE_COMPONENTS_MAX) {
    return;
  }
  List<ServiceComponent> serviceComponents=serviceComponentPersistence.findByBuildNamespace(buildNamespace,_SERVICE_COMPONENTS_MAX,serviceComponentsCount);
  for (int i=0; i < serviceComponents.size(); i++) {
    ServiceComponent serviceComponent=serviceComponents.get(i);
    serviceComponentPersistence.remove(serviceComponent);
  }
}
