{
  int serviceComponentsCount=serviceComponentPersistence.countByBuildNamespace(buildNamespace);
  if (serviceComponentsCount < _MAX_SERVICE_COMPONENTS) {
    return;
  }
  List serviceComponents=serviceComponentPersistence.findByBuildNamespace(buildNamespace,_MAX_SERVICE_COMPONENTS,serviceComponentsCount);
  for (int i=0; i < serviceComponents.size(); i++) {
    ServiceComponent serviceComponent=(ServiceComponent)serviceComponents.get(i);
    serviceComponentPersistence.remove(serviceComponent);
  }
}
