{
  ResourcePermission newResourcePermission=addResourcePermission();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ResourcePermission.class,ResourcePermission.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("resourcePermissionId"));
  Object newResourcePermissionId=newResourcePermission.getResourcePermissionId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("resourcePermissionId",new Object[]{newResourcePermissionId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Object existingResourcePermissionId=result.get(0);
  assertEquals(existingResourcePermissionId,newResourcePermissionId);
}
