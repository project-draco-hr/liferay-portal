{
  if (((approvedByUserId == null) && (_approvedByUserId != null)) || ((approvedByUserId != null) && (_approvedByUserId == null)) || ((approvedByUserId != null) && (_approvedByUserId != null) && !approvedByUserId.equals(_approvedByUserId))) {
    if (!XSS_ALLOW_APPROVEDBYUSERID) {
      approvedByUserId=XSSUtil.strip(approvedByUserId);
    }
    _approvedByUserId=approvedByUserId;
  }
}
