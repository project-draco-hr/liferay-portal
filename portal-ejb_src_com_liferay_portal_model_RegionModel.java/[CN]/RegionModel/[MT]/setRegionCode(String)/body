{
  if (((regionCode == null) && (_regionCode != null)) || ((regionCode != null) && (_regionCode == null)) || ((regionCode != null) && (_regionCode != null) && !regionCode.equals(_regionCode))) {
    if (!XSS_ALLOW_REGIONCODE) {
      regionCode=XSSUtil.strip(regionCode);
    }
    _regionCode=regionCode;
    setModified(true);
  }
}
