{
  if (!_executing) {
    try {
      _executing=true;
      if (_log.isDebugEnabled()) {
        _log.debug("Executing");
        pollPopServer();
      }
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
 finally {
      _executing=false;
    }
  }
 else {
    if (_log.isDebugEnabled()) {
      _log.debug("Not executing");
    }
  }
}
