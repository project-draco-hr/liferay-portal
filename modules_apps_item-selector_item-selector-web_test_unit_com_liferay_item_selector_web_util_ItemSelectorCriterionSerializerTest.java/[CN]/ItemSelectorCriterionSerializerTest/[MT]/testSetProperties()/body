{
  Map<String,String[]> properties=new HashMap<>();
  ItemSelectorReturnType testURLItemSelectorReturnType=new TestURLItemSelectorReturnType();
  properties.put(_PREFIX + ItemSelectorCriterionSerializer.JSON,new String[]{"{\"desiredItemSelectorReturnTypes\":[\"" + testURLItemSelectorReturnType.getName() + "\"],\""+ "tags\":[\"tag1\",\"tag2\",\"tag3\"],\"user\":\""+ "Joe Bloggs\"}"});
  _itemSelectorCriterionSerializer.setProperties(_flickrItemSelectorCriterion,_PREFIX,properties);
  Assert.assertEquals("Joe Bloggs",_flickrItemSelectorCriterion.getUser());
  Assert.assertArrayEquals(new String[]{"tag1","tag2","tag3"},_flickrItemSelectorCriterion.getTags());
  Set<ItemSelectorReturnType> expectedDesiredItemSelectorReturnTypes=new HashSet<>();
  expectedDesiredItemSelectorReturnTypes.add(testURLItemSelectorReturnType);
  Assert.assertEquals(expectedDesiredItemSelectorReturnTypes,_flickrItemSelectorCriterion.getDesiredItemSelectorReturnTypes());
}
