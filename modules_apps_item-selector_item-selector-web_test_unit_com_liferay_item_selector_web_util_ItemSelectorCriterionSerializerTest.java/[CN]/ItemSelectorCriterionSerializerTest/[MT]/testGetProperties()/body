{
  Map<String,String[]> properties=_itemSelectorCriterionSerializer.getProperties();
  Assert.assertEquals(2,properties.size());
  Assert.assertEquals(_flickrItemSelectorCriterion.getUser(),properties.get("prefix_user")[0]);
  Assert.assertEquals(StringUtil.merge(_flickrItemSelectorCriterion.getTags()),properties.get("prefix_tags")[0]);
}
