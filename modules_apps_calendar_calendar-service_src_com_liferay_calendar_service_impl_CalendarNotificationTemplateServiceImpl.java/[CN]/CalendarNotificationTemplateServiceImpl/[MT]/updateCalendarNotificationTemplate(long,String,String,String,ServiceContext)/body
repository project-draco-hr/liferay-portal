{
  CalendarNotificationTemplate calendarNotificationTemplate=calendarNotificationTemplatePersistence.findByPrimaryKey(calendarNotificationTemplateId);
  CalendarPermission.check(getPermissionChecker(),calendarNotificationTemplate.getCalendarId(),CalendarActionKeys.UPDATE);
  return calendarNotificationTemplateLocalService.updateCalendarNotificationTemplate(calendarNotificationTemplateId,notificationTypeSettings,subject,body,serviceContext);
}
