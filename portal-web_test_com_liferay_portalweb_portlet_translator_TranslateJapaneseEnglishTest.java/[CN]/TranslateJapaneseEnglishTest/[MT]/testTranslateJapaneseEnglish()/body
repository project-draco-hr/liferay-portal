{
  selenium.select("_26_id",RuntimeVariables.replace("label=Japanese to English"));
  selenium.type("_26_text",RuntimeVariables.replace("???????????????6,000,000 ???????Liferay ???????"));
  selenium.click(RuntimeVariables.replace("//input[@value='Translate']"));
