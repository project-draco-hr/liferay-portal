{
  UploadException uploadException=(UploadException)request.getAttribute(WebKeys.UPLOAD_EXCEPTION);
  if (uploadException != null) {
    if (uploadException.isExceededFileSizeLimit() || uploadException.isExceededRequestContentLengthLimit()) {
      throw new LARFileSizeException(uploadException.getCause());
    }
    throw new PortalException(uploadException.getCause());
  }
}
