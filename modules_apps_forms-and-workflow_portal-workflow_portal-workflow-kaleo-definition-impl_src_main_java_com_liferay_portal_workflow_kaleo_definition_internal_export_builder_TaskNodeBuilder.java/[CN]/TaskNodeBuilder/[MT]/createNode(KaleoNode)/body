{
  KaleoTask kaleoTask=_kaleoTaskLocalService.getKaleoNodeKaleoTask(kaleoNode.getKaleoNodeId());
  Task task=new Task(kaleoNode.getName(),kaleoNode.getDescription());
  Set<Assignment> assignments=buildAssigments(KaleoTask.class.getName(),kaleoTask.getKaleoTaskId());
  task.setAssignments(assignments);
  Set<TaskForm> taskForms=buildTaskForms(KaleoTask.class.getName(),kaleoTask.getKaleoTaskId());
  task.addTaskForms(taskForms);
  return task;
}
