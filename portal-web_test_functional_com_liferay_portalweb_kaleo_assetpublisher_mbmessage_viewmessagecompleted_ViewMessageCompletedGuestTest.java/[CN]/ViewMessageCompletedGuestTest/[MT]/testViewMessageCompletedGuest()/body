{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace("Asset Publisher Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("//section"));
  assertEquals(RuntimeVariables.replace("Asset Publisher"),selenium.getText("//h1/span[contains(.,'Asset Publisher')]"));
  assertEquals(RuntimeVariables.replace("Message Boards Message Subject"),selenium.getText("//div[@class='asset-abstract ']/h3/a"));
  assertEquals(RuntimeVariables.replace("Message Boards Message Body"),selenium.getText("//div[@class='asset-summary']"));
  assertTrue(selenium.isPartialText("//div[contains(.,'Read More')]/a","Read More"));
  assertFalse(selenium.isTextPresent("There are no results."));
  selenium.clickAt("//div[contains(.,'Read More')]/a",RuntimeVariables.replace("Read More"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Message Boards Message Subject"),selenium.getText("//div[@class='taglib-header ']/h1/span[contains(.,'Message Boards Message Subject')]"));
  assertTrue(selenium.isPartialText("//td[@class='lfr-top']/div[contains(.,'Message Boards Message Body')]","Message Boards Message Body"));
}
