{
  ServletRequest servletRequest=pageContext.getRequest();
  ScriptData scriptData=(ScriptData)servletRequest.getAttribute(ScriptTag.class.getName());
  if (scriptData == null) {
    scriptData=(ScriptData)servletRequest.getAttribute(WebKeys.AUI_SCRIPT_DATA);
    if (scriptData != null) {
      servletRequest.removeAttribute(WebKeys.AUI_SCRIPT_DATA);
    }
  }
  if (scriptData != null) {
    ScriptTag scriptTag=new ScriptTag();
    scriptTag.setPageContext(pageContext);
    scriptTag.processEndTag(scriptData);
  }
}
