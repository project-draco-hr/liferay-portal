{
  ScriptTag scriptTag=new ScriptTag();
  scriptTag.setPageContext(pageContext);
  scriptTag._position=position;
  scriptTag._use=use;
  BodyContent bodyContent=pageContext.pushBody();
  scriptTag.setBodyContent(bodyContent);
  bodyContent.write(body);
  pageContext.popBody();
  scriptTag.doEndTag();
  scriptTag.release();
}
