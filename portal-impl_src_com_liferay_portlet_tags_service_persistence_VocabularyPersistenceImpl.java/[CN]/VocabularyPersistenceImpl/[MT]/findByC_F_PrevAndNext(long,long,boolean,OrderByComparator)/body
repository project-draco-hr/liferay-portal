{
  Vocabulary vocabulary=findByPrimaryKey(vocabularyId);
  int count=countByC_F(companyId,folksonomy);
  Session session=null;
  try {
    session=openSession();
    StringBuilder query=new StringBuilder();
    query.append("FROM com.liferay.portlet.tags.model.Vocabulary WHERE ");
    query.append("companyId = ?");
    query.append(" AND ");
    query.append("folksonomy = ?");
    query.append(" ");
    if (obc != null) {
      query.append("ORDER BY ");
      query.append(obc.getOrderBy());
    }
    Query q=session.createQuery(query.toString());
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(companyId);
    qPos.add(folksonomy);
    Object[] objArray=QueryUtil.getPrevAndNext(q,count,obc,vocabulary);
    Vocabulary[] array=new VocabularyImpl[3];
    array[0]=(Vocabulary)objArray[0];
    array[1]=(Vocabulary)objArray[1];
    array[2]=(Vocabulary)objArray[2];
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
