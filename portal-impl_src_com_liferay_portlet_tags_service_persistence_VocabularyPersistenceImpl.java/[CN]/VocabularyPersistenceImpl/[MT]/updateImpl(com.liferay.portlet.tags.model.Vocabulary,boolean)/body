{
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(vocabulary);
    }
 else {
      if (vocabulary.isNew()) {
        session.save(vocabulary);
      }
    }
    session.flush();
    vocabulary.setNew(false);
    return vocabulary;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Vocabulary.class.getName());
  }
}
