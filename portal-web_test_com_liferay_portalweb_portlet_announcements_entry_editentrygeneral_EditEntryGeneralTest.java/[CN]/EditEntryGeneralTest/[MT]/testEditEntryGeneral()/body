{
  selenium.open("/web/guest/home/");
  Thread.sleep(5000);
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Announcements Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Announcements Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Edit",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(5000);
  selenium.type("_84_title",RuntimeVariables.replace("This Test General Annoucement has been edited."));
  selenium.type("_84_content",RuntimeVariables.replace("Hello Everyone! This is test has been edited."));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(5000);
  assertTrue(selenium.isElementPresent("link=This Test General Annoucement has been edited."));
  assertTrue(selenium.isTextPresent("Hello Everyone! This is test has been edited."));
}
