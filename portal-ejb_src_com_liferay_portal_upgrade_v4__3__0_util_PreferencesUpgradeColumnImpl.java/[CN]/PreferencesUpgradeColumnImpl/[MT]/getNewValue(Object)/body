{
  String xml=(String)oldValue;
  PortletPreferencesImpl prefs=(PortletPreferencesImpl)PortletPreferencesSerializer.fromDefaultXML(xml);
  String groupId=(String)prefs.getValue("group-id",null);
  if (Validator.isNotNull(groupId)) {
    Long newGroupId=(Long)_groupIdMapper.getNewValue(new Long(groupId));
    prefs.setValue("group-id",newGroupId.toString());
    xml=PortletPreferencesSerializer.toXML(prefs);
  }
  return xml;
}
