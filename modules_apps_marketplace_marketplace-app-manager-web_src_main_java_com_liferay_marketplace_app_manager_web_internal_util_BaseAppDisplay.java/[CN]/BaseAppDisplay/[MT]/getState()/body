{
  List<Bundle> _bundles=getBundles();
  if (_bundles.isEmpty()) {
    return Bundle.UNINSTALLED;
  }
  int state=Bundle.ACTIVE;
  for (  Bundle bundle : _bundles) {
    if (BundleUtil.isFragment(bundle)) {
      continue;
    }
    int bundleState=bundle.getState();
    if (state > bundleState) {
      state=bundleState;
    }
  }
  return state;
}
