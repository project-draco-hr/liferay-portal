{
  if (PropsValues.RSS_FEEDS_DISABLED) {
    PortalUtil.sendError(HttpServletResponse.SC_NOT_FOUND,new NoSuchFeedException(),request,response);
    return null;
  }
  try {
    ServletResponseUtil.sendFile(request,response,null,getRSS(request),ContentTypes.TEXT_XML_UTF8);
    return mapping.findForward(ActionConstants.COMMON_NULL);
  }
 catch (  Exception e) {
    PortalUtil.sendError(e,request,response);
    return null;
  }
}
