{
  if (req == null) {
    return false;
  }
  String accept=req.getHeader(HttpHeaders.ACCEPT);
  if (accept == null) {
    return false;
  }
  accept=accept.toLowerCase();
  if ((accept.indexOf("application/xhtml+xml") == -1) && (accept.indexOf("wap.xhtml") != -1)) {
    return true;
  }
 else {
    return false;
  }
}
