{
  Collection<UserNotificationEvent> userNotificationEvents=new ArrayList<UserNotificationEvent>(notificationEvents.size());
  for (  NotificationEvent notificationEvent : notificationEvents) {
    UserNotificationEvent userNotificationEvent=addUserNotificationEvent(companyId,userId,notificationEvent);
    userNotificationEvents.add(userNotificationEvent);
  }
  return userNotificationEvents;
}
