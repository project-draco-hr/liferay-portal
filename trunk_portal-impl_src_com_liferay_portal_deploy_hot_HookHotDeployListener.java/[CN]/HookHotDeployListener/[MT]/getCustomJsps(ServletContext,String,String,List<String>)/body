{
  Set<String> resourcePaths=servletContext.getResourcePaths(resourcePath);
  for (  String curResourcePath : resourcePaths) {
    if (curResourcePath.endsWith(StringPool.SLASH)) {
      getCustomJsps(servletContext,webDir,curResourcePath,customJsps);
    }
 else {
      String customJsp=webDir + curResourcePath;
      customJsp=StringUtil.replace(customJsp,StringPool.DOUBLE_SLASH,StringPool.SLASH);
      customJsps.add(customJsp);
    }
  }
}
