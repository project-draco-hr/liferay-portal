{
  ServiceBag serviceBag=_serviceBags.get(serviceType);
  if (serviceBag == null) {
    serviceBag=new ServiceBag(wrappedService);
    _serviceBags.put(serviceType,serviceBag);
  }
  serviceBag.addCustomServiceConstructor(servletContextName,portletClassLoader,serviceTypeClass,serviceImplConstructor);
}
