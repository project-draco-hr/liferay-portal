{
  List<ServiceConstructor> serviceConstructors=_serviceConstructors.get(servletContextName);
  if (serviceConstructors == null) {
    serviceConstructors=new ArrayList<ServiceConstructor>();
    _serviceConstructors.put(servletContextName,serviceConstructors);
  }
  ServiceConstructor serviceConstructor=new ServiceConstructor(portletClassLoader,serviceTypeClass,serviceImplConstructor);
  serviceConstructors.add(serviceConstructor);
}
