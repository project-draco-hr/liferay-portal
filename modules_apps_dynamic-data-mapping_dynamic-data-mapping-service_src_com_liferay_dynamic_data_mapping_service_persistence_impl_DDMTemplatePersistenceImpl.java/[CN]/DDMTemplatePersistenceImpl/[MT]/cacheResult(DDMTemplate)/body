{
  EntityCacheUtil.putResult(DDMTemplateModelImpl.ENTITY_CACHE_ENABLED,DDMTemplateImpl.class,ddmTemplate.getPrimaryKey(),ddmTemplate);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddmTemplate.getUuid(),ddmTemplate.getGroupId()},ddmTemplate);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_SMALLIMAGEID,new Object[]{ddmTemplate.getSmallImageId()},ddmTemplate);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_C_T,new Object[]{ddmTemplate.getGroupId(),ddmTemplate.getClassNameId(),ddmTemplate.getTemplateKey()},ddmTemplate);
  ddmTemplate.resetOriginalValues();
}
