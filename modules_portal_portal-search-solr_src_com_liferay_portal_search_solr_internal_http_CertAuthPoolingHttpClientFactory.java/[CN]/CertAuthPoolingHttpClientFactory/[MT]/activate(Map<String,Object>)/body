{
  _solrConfiguration=Configurable.createConfigurable(SolrConfiguration.class,properties);
  int defaultMaxConnectionsPerRoute=_solrConfiguration.defaultMaxConnectionsPerRoute();
  setDefaultMaxConnectionsPerRoute(defaultMaxConnectionsPerRoute);
  int maxTotalConnections=_solrConfiguration.maxTotalConnections();
  setMaxTotalConnections(maxTotalConnections);
}
