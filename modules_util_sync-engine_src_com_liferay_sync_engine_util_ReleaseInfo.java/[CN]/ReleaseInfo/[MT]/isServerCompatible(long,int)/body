{
  Boolean serverCompatible=_serverCompatibilityMap.get(syncAccountId + "#" + minimumVersion);
  if (serverCompatible != null) {
    return serverCompatible;
  }
  SyncAccount syncAccount=SyncAccountService.fetchSyncAccount(syncAccountId);
  serverCompatible=isServerCompatible(syncAccount.getPluginVersion(),minimumVersion);
  _serverCompatibilityMap.put(syncAccountId + "#" + minimumVersion,serverCompatible);
  return serverCompatible;
}
