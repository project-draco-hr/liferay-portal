{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Country.class,Country.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("countryId",ServiceTestUtil.nextLong()));
  List<Country> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
