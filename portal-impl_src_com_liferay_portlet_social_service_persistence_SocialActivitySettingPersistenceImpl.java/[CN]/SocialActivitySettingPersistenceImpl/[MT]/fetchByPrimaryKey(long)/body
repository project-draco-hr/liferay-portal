{
  SocialActivitySetting socialActivitySetting=(SocialActivitySetting)EntityCacheUtil.getResult(SocialActivitySettingModelImpl.ENTITY_CACHE_ENABLED,SocialActivitySettingImpl.class,activitySettingId);
  if (socialActivitySetting == _nullSocialActivitySetting) {
    return null;
  }
  if (socialActivitySetting == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      socialActivitySetting=(SocialActivitySetting)session.get(SocialActivitySettingImpl.class,Long.valueOf(activitySettingId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (socialActivitySetting != null) {
        cacheResult(socialActivitySetting);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(SocialActivitySettingModelImpl.ENTITY_CACHE_ENABLED,SocialActivitySettingImpl.class,activitySettingId,_nullSocialActivitySetting);
      }
      closeSession(session);
    }
  }
  return socialActivitySetting;
}
