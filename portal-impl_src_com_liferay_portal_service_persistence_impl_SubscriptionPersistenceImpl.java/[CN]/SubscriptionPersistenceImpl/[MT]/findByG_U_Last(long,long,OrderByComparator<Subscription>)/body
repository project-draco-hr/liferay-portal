{
  Subscription subscription=fetchByG_U_Last(groupId,userId,orderByComparator);
  if (subscription != null) {
    return subscription;
  }
  StringBundler msg=new StringBundler(6);
  msg.append(_NO_SUCH_ENTITY_WITH_KEY);
  msg.append("groupId=");
  msg.append(groupId);
  msg.append(", userId=");
  msg.append(userId);
