{
  Session session=null;
  try {
    session=openSession();
    Subscription subscription=(Subscription)session.get(SubscriptionImpl.class,primaryKey);
    if (subscription == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchSubscriptionException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(subscription);
  }
 catch (  NoSuchSubscriptionException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
