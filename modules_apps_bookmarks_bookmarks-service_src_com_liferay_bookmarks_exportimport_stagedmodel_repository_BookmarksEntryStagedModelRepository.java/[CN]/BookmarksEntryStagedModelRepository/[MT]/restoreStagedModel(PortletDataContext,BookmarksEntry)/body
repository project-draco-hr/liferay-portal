{
  long userId=portletDataContext.getUserId(entry.getUserUuid());
  BookmarksEntry existingEntry=fetchStagedModelByUuidAndGroupId(entry.getUuid(),portletDataContext.getScopeGroupId());
  if ((existingEntry == null) || !isStagedModelInTrash(existingEntry)) {
    return;
  }
  TrashHandler trashHandler=existingEntry.getTrashHandler();
  try {
    if (trashHandler.isRestorable(existingEntry.getEntryId())) {
      trashHandler.restoreTrashEntry(userId,existingEntry.getEntryId());
    }
  }
 catch (  PortalException pe) {
    throw new PortletDataException(pe);
  }
}
