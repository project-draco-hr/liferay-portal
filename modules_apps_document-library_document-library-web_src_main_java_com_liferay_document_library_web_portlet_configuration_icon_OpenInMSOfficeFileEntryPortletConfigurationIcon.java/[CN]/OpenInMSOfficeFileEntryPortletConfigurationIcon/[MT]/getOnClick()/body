{
  StringBundler sb=new StringBundler(4);
  try {
    String webDavURL=DLUtil.getWebDavURL(themeDisplay,_fileEntry.getFolder(),_fileEntry,PropsValues.DL_FILE_ENTRY_OPEN_IN_MS_OFFICE_MANUAL_CHECK_IN_REQUIRED);
    PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
    sb.append(portletDisplay.getNamespace());
    sb.append("openDocument('");
    sb.append(webDavURL);
    sb.append("');");
  }
 catch (  PortalException pe) {
  }
  return sb.toString();
}
