{
  try {
    Map<Locale,String> titleMap=LocalizationUtil.getLocalizationMap(titleMapLanguageIds,titleMapValues);
    com.liferay.portal.security.service.access.policy.model.SAPEntry returnValue=SAPEntryServiceUtil.addSAPEntry(allowedServiceSignatures,defaultSAPEntry,enabled,name,titleMap,serviceContext);
    return com.liferay.portal.security.service.access.policy.model.SAPEntrySoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
