{
  String value=null;
  try {
    java.util.Properties propsFromDatabase=PropertiesLocalServiceUtil.getPortalProperties(companyId);
    value=propsFromDatabase.getProperty(key);
  }
 catch (  SystemException se) {
  }
  if (value == null) {
    ExtPropertiesLoader propsFromFile=ExtPropertiesLoader.getInstance(PropsFiles.PORTAL,companyId);
    value=propsFromFile.get(key);
  }
  if (value == null) {
    value=PropsUtil.get(key);
  }
  return value;
}
