{
  try {
    List<?> list=sessionImpl.list(queryString,positionalParameterMap,namedParameterMap,strictName,firstResult,maxResults,flushModeType,lockModeType,sqlQuery,entityClass);
    if ((entityClass == null) && !list.isEmpty()) {
      list=_transformList(list);
    }
    if (unmodifiable) {
      list=new UnmodifiableList<Object>(list);
    }
 else     if (copy) {
      list=ListUtil.copy(list);
    }
    return list;
  }
 catch (  Exception e) {
    throw ExceptionTranslator.translate(e);
  }
}
