{
  try {
    Object object=_sessionImpl.uniqueResult(_queryString,_parameterMap,_firstResult,_maxResults,_flushMode,_sqlQuery,_entityClass);
    if (object instanceof Collection && (((Collection)object).size() == 1)) {
      object=((Collection)object).iterator().next();
    }
    return object;
  }
 catch (  Exception e) {
    throw ExceptionTranslator.translate(e);
  }
}
