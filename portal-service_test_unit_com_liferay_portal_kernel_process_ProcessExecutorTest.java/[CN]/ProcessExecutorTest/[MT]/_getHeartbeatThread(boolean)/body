{
  Field field=ReflectionUtil.getDeclaredField(ProcessContext.class,"_heartbeatThreadReference");
  AtomicReference<? extends Thread> heartbeatThreadReference=(AtomicReference<? extends Thread>)field.get(null);
  if (remove) {
    return heartbeatThreadReference.getAndSet(null);
  }
 else {
    return heartbeatThreadReference.get();
  }
}
