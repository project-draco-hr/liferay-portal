{
  byte[] reallyLargePayload=new byte[100 * 1024 * 1024];
  Random random=new Random();
  random.nextBytes(reallyLargePayload);
  EchoPayloadProcessCallable echoPayloadProcessCallable=new EchoPayloadProcessCallable(reallyLargePayload);
  Future<byte[]> future=ProcessExecutor.execute(_classPath,_classPath,_createArguments(_JPDA_OPTIONS1),echoPayloadProcessCallable);
  Assert.assertArrayEquals(reallyLargePayload,future.get());
  Assert.assertFalse(future.isCancelled());
  Assert.assertTrue(future.isDone());
}
