{
  UserGroupRole userGroupRole=findByPrimaryKey(userGroupRolePK);
  Session session=null;
  try {
    session=openSession();
    UserGroupRole[] array=new UserGroupRoleImpl[3];
    array[0]=getByG_R_PrevAndNext(session,userGroupRole,groupId,roleId,orderByComparator,true);
    array[1]=userGroupRole;
    array[2]=getByG_R_PrevAndNext(session,userGroupRole,groupId,roleId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
