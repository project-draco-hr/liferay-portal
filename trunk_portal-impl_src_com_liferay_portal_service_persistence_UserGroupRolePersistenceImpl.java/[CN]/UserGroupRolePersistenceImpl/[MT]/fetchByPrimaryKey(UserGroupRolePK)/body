{
  UserGroupRole userGroupRole=(UserGroupRole)EntityCacheUtil.getResult(UserGroupRoleModelImpl.ENTITY_CACHE_ENABLED,UserGroupRoleImpl.class,userGroupRolePK,this);
  if (userGroupRole == null) {
    Session session=null;
    try {
      session=openSession();
      userGroupRole=(UserGroupRole)session.get(UserGroupRoleImpl.class,userGroupRolePK);
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (userGroupRole != null) {
        cacheResult(userGroupRole);
      }
      closeSession(session);
    }
  }
  return userGroupRole;
}
