{
  if (Validator.isNull(line)) {
    return;
  }
  _line=line;
  Matcher javaMatcher=_javaImportPattern.matcher(line);
  if (javaMatcher.find()) {
    _import=javaMatcher.group(1);
  }
  Matcher jspMatcher=_jspImportPattern.matcher(line);
  if (jspMatcher.find()) {
    _import=jspMatcher.group(1);
  }
}
