{
  List<Layout> layouts=LayoutLocalServiceUtil.getLayouts(layoutPrototype.getGroupId(),true,LayoutConstants.DEFAULT_PARENT_LAYOUT_ID);
  for (  Layout layout : layouts) {
    StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,layout);
    portletDataContext.addReferenceElement(layoutPrototype,layoutPrototypeElement,layout,PortletDataContext.REFERENCE_TYPE_EMBEDDED,false);
  }
}
