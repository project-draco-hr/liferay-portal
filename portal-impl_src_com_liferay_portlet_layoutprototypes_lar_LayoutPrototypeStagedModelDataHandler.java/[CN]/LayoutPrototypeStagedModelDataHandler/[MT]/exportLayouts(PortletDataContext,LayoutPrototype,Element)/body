{
  List<Layout> layouts=LayoutLocalServiceUtil.getLayouts(layoutPrototype.getGroupId(),true,LayoutConstants.DEFAULT_PARENT_LAYOUT_ID);
  for (  Layout layout : layouts) {
    StagedModelDataHandlerUtil.exportReferencedStagedModel(portletDataContext,layoutPrototype,layout,PortletDataContext.REFERENCE_TYPE_EMBEDDED);
  }
}
