{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  boolean subscribed=false;
  try {
    WikiNode node=ActionUtil.getNode(portletRequest);
    subscribed=_subscriptionLocalService.isSubscribed(themeDisplay.getCompanyId(),themeDisplay.getUserId(),WikiNode.class.getName(),node.getNodeId());
  }
 catch (  Exception e) {
  }
  return subscribed;
}
