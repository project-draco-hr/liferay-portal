{
  List<DLFileEntry> list=findByG_U_F(groupId,userId,folderId,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(8);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("groupId=");
    msg.append(groupId);
    msg.append(", userId=");
    msg.append(userId);
    msg.append(", folderId=");
    msg.append(folderId);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchFileEntryException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
