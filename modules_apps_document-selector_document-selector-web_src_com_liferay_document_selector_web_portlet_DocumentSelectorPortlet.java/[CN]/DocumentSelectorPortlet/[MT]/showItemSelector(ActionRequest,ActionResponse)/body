{
  ItemSelectorRendering itemSelectorRendering=_itemSelector.getItemSelectorRendering(actionRequest.getParameterMap());
  LocalizedItemSelectorRendering localizedItemSelectorRendering=new LocalizedItemSelectorRendering(actionRequest.getLocale(),itemSelectorRendering);
  localizedItemSelectorRendering.store(actionRequest);
  actionResponse.setRenderParameter("mvcPath","/item_selector.jsp");
}
