{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBThread.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("threadId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("threadId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
