{
  MBThread newMBThread=addMBThread();
  _persistence.clearCache();
  MBThread existingMBThread=_persistence.findByPrimaryKey(newMBThread.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingMBThread.getUuid(),ReflectionTestUtil.invoke(existingMBThread,"getOriginalUuid",new Class<?>[0])));
  Assert.assertEquals(existingMBThread.getGroupId(),ReflectionTestUtil.invoke(existingMBThread,"getOriginalGroupId",new Class<?>[0]));
  Assert.assertEquals(existingMBThread.getRootMessageId(),ReflectionTestUtil.invoke(existingMBThread,"getOriginalRootMessageId",new Class<?>[0]));
}
