{
  Calendar calendar=CalendarFactoryUtil.getCalendar();
  calendar.add(Calendar.DAY_OF_MONTH,-1);
  Date yesterday=calendar.getTime();
  calendar.add(Calendar.DAY_OF_MONTH,-2);
  Date dayBeforeYesterday=calendar.getTime();
  _assetEntry1=AssetTestUtil.addAssetEntry(_group.getGroupId(),dayBeforeYesterday);
  _ratingStats1=RatingsTestUtil.addStats(_assetEntry1.getClassName(),_assetEntry1.getClassPK(),2000);
  _assetEntry2=AssetTestUtil.addAssetEntry(_group.getGroupId(),dayBeforeYesterday);
  _ratingStats2=RatingsTestUtil.addStats(_assetEntry2.getClassName(),_assetEntry2.getClassPK(),1000);
  _assetEntry3=AssetTestUtil.addAssetEntry(_group.getGroupId(),yesterday);
  _ratingStats3=RatingsTestUtil.addStats(_assetEntry3.getClassName(),_assetEntry3.getClassPK(),3000);
  List<AssetEntry> entries=new ArrayList<AssetEntry>(3);
  entries.add(_assetEntry3);
  entries.add(_assetEntry1);
  entries.add(_assetEntry2);
  return entries;
}
