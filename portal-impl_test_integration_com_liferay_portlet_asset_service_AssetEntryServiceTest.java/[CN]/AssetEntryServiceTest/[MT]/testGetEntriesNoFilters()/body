{
  AssetEntryQuery assetEntryQuery=new AssetEntryQuery();
  assetEntryQuery.setGroupIds(new long[]{_group.getGroupId()});
  List<AssetEntry> initialAssetEntries=AssetEntryLocalServiceUtil.getEntries(assetEntryQuery);
  AssetTestUtil.addAssetEntry(_group.getGroupId());
  assetEntryQuery.setGroupIds(new long[]{_group.getGroupId()});
  List<AssetEntry> assetEntries=AssetEntryLocalServiceUtil.getEntries(assetEntryQuery);
  Assert.assertEquals(initialAssetEntries.size() + 1,assetEntries.size());
  AssetEntry assetEntry=assetEntries.get(0);
  Assert.assertTrue(assetEntries.contains(assetEntry));
}
