{
  if (((listTypeId == null) && (_listTypeId != null)) || ((listTypeId != null) && (_listTypeId == null)) || ((listTypeId != null) && (_listTypeId != null) && !listTypeId.equals(_listTypeId))) {
    if (!XSS_ALLOW_LISTTYPEID) {
      listTypeId=XSSUtil.strip(listTypeId);
    }
    _listTypeId=listTypeId;
    setModified(true);
  }
