{
  MethodKey methodKey=new MethodKey(_adaptedInstance.getClass(),methodName,parameterTypes);
  if (_methodsCache.containsKey(methodKey)) {
    return _methodsCache.get(methodKey);
  }
  Method method=ReflectionUtil.getDeclaredMethod(_adaptedInstance.getClass(),methodName,parameterTypes);
  _methodsCache.put(methodKey,method);
  return method;
}
