{
  MethodKey methodKey=new MethodKey(_adaptedObject.getClass(),methodName,parameterTypes);
  if (_methods.containsKey(methodKey)) {
    return _methods.get(methodKey);
  }
  Method method=ReflectionUtil.getDeclaredMethod(_adaptedObject.getClass(),methodName,parameterTypes);
  _methods.put(methodKey,method);
  return method;
}
