{
  String cmd=ParamUtil.getString(resourceRequest,Constants.CMD);
  PortletContext portletContext=portletConfig.getPortletContext();
  PortletRequestDispatcher portletRequestDispatcher=null;
  if (cmd.equals(Constants.IMPORT)) {
    portletRequestDispatcher=portletContext.getRequestDispatcher("/html/portlet/export_import/import_layouts_processes.jsp");
  }
 else {
    portletRequestDispatcher=portletContext.getRequestDispatcher("/html/portlet/export_import/import_layouts_resources.jsp");
  }
  portletRequestDispatcher.include(resourceRequest,resourceResponse);
}
