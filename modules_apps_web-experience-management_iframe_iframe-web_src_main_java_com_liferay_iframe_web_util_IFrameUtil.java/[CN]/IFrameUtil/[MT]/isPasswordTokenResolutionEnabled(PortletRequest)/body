{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  Layout layout=themeDisplay.getLayout();
  if (layout.isPrivateLayout() && layout.getGroup().isUser() && (themeDisplay.getRealUserId() != layout.getGroup().getClassPK())) {
    return false;
  }
  return true;
}
