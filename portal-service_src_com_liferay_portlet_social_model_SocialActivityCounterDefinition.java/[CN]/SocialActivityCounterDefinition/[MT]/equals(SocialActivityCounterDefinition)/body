{
  if (Validator.equals(_increment,activityCounterDefinition._increment) && Validator.equals(_limitValue,activityCounterDefinition._limitValue) && Validator.equals(_limitPeriod,activityCounterDefinition._limitPeriod)&& Validator.equals(_name,activityCounterDefinition._name)&& Validator.equals(_ownerType,activityCounterDefinition._ownerType)) {
    return true;
  }
  return false;
}
