{
  try {
    renderRequest.setAttribute(AnnouncementsWebKeys.ANNOUNCEMENTS_ENTRY,_getAnnouncementsEntry(renderRequest));
    renderRequest.setAttribute(AnnouncementsWebKeys.VIEW_ENTRY_FLAG_VALUE,AnnouncementsFlagConstants.NOT_HIDDEN);
  }
 catch (  PortalException pe) {
    return "/announcements/error.jsp";
  }
  return "/announcements/view_entry.jsp";
}
