{
  BrowserTracker browserTracker=fetchByPrimaryKey(browserTrackerId);
  if (browserTracker == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No BrowserTracker exists with the primary key " + browserTrackerId);
    }
    throw new NoSuchBrowserTrackerException("No BrowserTracker exists with the primary key " + browserTrackerId);
  }
  return browserTracker;
}
