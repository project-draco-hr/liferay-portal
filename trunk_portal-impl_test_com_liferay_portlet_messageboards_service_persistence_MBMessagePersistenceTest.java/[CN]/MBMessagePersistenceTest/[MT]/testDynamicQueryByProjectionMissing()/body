{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBMessage.class,MBMessage.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("messageId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("messageId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
