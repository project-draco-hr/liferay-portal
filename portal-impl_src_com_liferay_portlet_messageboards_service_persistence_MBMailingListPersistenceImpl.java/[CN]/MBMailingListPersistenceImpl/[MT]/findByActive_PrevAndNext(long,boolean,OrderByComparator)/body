{
  MBMailingList mbMailingList=findByPrimaryKey(mailingListId);
  Session session=null;
  try {
    session=openSession();
    MBMailingList[] array=new MBMailingListImpl[3];
    array[0]=getByActive_PrevAndNext(session,mbMailingList,active,orderByComparator,true);
    array[1]=mbMailingList;
    array[2]=getByActive_PrevAndNext(session,mbMailingList,active,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
