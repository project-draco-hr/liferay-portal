{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(mbMailingList);
    }
  }
  mbMailingList=removeImpl(mbMailingList);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(mbMailingList);
    }
  }
  return mbMailingList;
}
