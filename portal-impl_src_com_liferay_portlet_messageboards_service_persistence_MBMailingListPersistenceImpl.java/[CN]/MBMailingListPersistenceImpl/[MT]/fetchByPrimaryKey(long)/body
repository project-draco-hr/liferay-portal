{
  MBMailingList mbMailingList=(MBMailingList)EntityCacheUtil.getResult(MBMailingListModelImpl.ENTITY_CACHE_ENABLED,MBMailingListImpl.class,mailingListId);
  if (mbMailingList == _nullMBMailingList) {
    return null;
  }
  if (mbMailingList == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      mbMailingList=(MBMailingList)session.get(MBMailingListImpl.class,Long.valueOf(mailingListId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (mbMailingList != null) {
        cacheResult(mbMailingList);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(MBMailingListModelImpl.ENTITY_CACHE_ENABLED,MBMailingListImpl.class,mailingListId,_nullMBMailingList);
      }
      closeSession(session);
    }
  }
  return mbMailingList;
}
