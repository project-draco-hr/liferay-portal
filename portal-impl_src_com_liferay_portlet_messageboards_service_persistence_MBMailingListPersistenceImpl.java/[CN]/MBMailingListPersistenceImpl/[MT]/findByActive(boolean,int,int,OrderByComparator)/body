{
  int count=countByActive(active);
  if (count == 0) {
    return Collections.EMPTY_LIST;
  }
  Object[] finderArgs=new Object[]{Boolean.valueOf(active),String.valueOf(start),String.valueOf(end),String.valueOf(obc)};
  List<MBMailingList> list=(List<MBMailingList>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_OBC_ACTIVE,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portlet.messageboards.model.MBMailingList WHERE ");
      query.append("active_ = ?");
      query.append(" ");
      if (obc != null) {
        query.append("ORDER BY ");
        query.append(obc.getOrderBy());
      }
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(active);
      list=(List<MBMailingList>)QueryUtil.list(q,getDialect(),start,end);
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<MBMailingList>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_OBC_ACTIVE,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
