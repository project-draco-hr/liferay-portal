{
  if (!routeParameters.containsKey(name)) {
    return;
  }
  String nodeName=routeParameters.get(name);
  if (escape) {
    nodeName=StringUtil.replace(nodeName,_UNESCAPED_CHARS,_ESCAPED_CHARS);
  }
 else {
    nodeName=StringUtil.replace(nodeName,_ESCAPED_CHARS,_UNESCAPED_CHARS);
  }
  routeParameters.put(name,nodeName);
}
