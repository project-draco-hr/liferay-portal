{
  if (!isCancelCheckoutDocumentButtonVisible()) {
    return;
  }
  _addJavascriptUIItem(new JavascriptToolbarItem(),toolbarItems,DLToolbarItemKeys.CANCEL_CHECKOUT,"icon-undo",UnicodeLanguageUtil.get(_request,"cancel-checkout[document]"),getSubmitFormJavascript(Constants.CANCEL_CHECKOUT,null));
}
