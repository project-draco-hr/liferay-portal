{
  if (!isMoveToTheRecycleBinButtonVisible()) {
    return;
  }
  LiferayPortletResponse liferayPortletResponse=_getLiferayPortletResponse();
  PortletURL portletURL=liferayPortletResponse.createRenderURL();
  portletURL.setParameter("struts_action","/document_library/view");
  portletURL.setParameter("folderId",String.valueOf(_fileEntry.getFolderId()));
  _addJavascriptToolbarItem(toolbarItems,DLToolbarItemKeys.MOVE_TO_THE_RECYCLE_BIN,"icon-trash",UnicodeLanguageUtil.get(_request,"move-to-the-recycle-bin"),getSubmitFormJavascript(Constants.MOVE_TO_TRASH,portletURL.toString()));
}
