{
  if (!_fileEntryDisplayContextHelper.isCancelCheckoutDocumentButtonVisible()) {
    return;
  }
  URLMenuItem urlMenuItem=new URLMenuItem();
  urlMenuItem.setIconCssClass("icon-remove");
  urlMenuItem.setKey(DLMenuItemKeys.CANCEL_CHECKOUT);
  urlMenuItem.setMessage("cancel-checkout[document]");
  urlMenuItem.setURL(_getActionURL("/document_library/edit_file_entry",Constants.CANCEL_CHECKOUT));
  menuItems.add(urlMenuItem);
}
