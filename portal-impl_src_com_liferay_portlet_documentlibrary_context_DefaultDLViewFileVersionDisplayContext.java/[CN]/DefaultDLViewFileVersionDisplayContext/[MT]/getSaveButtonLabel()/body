{
  String saveButtonLabel="save";
  FileVersion fileVersion=_defaultDLFileEntryActionsDisplayContextHelper.getFileVersion();
  if ((fileVersion == null) || _defaultDLFileEntryActionsDisplayContextHelper.isApproved() || _defaultDLFileEntryActionsDisplayContextHelper.isDraft()) {
    saveButtonLabel="save-as-draft";
  }
  return saveButtonLabel;
}
