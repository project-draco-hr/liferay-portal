{
  if (!isMoveButtonVisible()) {
    return;
  }
  LiferayPortletResponse liferayPortletResponse=getLiferayPortletResponse();
  PortletURL portletURL=liferayPortletResponse.createRenderURL();
  portletURL.setParameter("struts_action","/document_library/move_entry");
  PortletURL redirectURL=liferayPortletResponse.createRenderURL();
  redirectURL.setParameter("struts_action","/document_library/view");
  redirectURL.setParameter("folderId",String.valueOf(_folderId));
  portletURL.setParameter("redirect",redirectURL.toString());
  portletURL.setParameter("fileEntryIds",String.valueOf(_fileEntry.getFileEntryId()));
  addURLMenuItem(menuItems,"icon-move",DLMenuItemKeys.MOVE,"move",portletURL.toString());
}
