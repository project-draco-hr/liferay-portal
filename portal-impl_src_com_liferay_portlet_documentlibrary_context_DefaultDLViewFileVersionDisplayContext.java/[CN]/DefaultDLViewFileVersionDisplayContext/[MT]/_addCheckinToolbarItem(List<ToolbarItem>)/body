{
  if (!isCheckinButtonVisible()) {
    return;
  }
  JavascriptToolbarItem javascriptToolbarItem=new JavascriptToolbarItem();
  javascriptToolbarItem.setKey(DLToolbarItemKeys.CHECKIN);
  javascriptToolbarItem.setIcon("icon-lock");
  javascriptToolbarItem.setLabel(UnicodeLanguageUtil.get(_request,"checkin"));
  javascriptToolbarItem.setOnClick(getSubmitFormJavascript(Constants.CHECKIN,null));
  toolbarItems.add(javascriptToolbarItem);
}
