{
  Session session=null;
  try {
    session=openSession();
    WikiPage wikiPage=(WikiPage)session.get(WikiPage.class,wikiPagePK);
    if (wikiPage == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No WikiPage exists with the primary key " + wikiPagePK.toString());
      }
      throw new NoSuchPageException("No WikiPage exists with the primary key " + wikiPagePK.toString());
    }
    session.delete(wikiPage);
    session.flush();
    return wikiPage;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
