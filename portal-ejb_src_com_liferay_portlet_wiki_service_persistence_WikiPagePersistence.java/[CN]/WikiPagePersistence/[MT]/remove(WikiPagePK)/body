{
  Session session=null;
  try {
    session=openSession();
    WikiPage wikiPage=(WikiPage)session.get(WikiPageImpl.class,wikiPagePK);
    if (wikiPage == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No WikiPage exists with the primary key " + wikiPagePK);
      }
      throw new NoSuchPageException("No WikiPage exists with the primary key " + wikiPagePK);
    }
    return remove(wikiPage);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
