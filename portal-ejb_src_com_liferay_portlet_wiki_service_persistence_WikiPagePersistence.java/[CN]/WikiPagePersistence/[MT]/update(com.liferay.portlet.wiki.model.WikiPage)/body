{
  Session session=null;
  try {
    session=openSession();
    if (wikiPage.isNew()) {
      session.save(wikiPage);
    }
    session.flush();
    wikiPage.setNew(false);
    return wikiPage;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
