for (int second=0; ; second++) {
if (second >= 60) {
fail("timeout");
}
try {
if (selenium.isElementPresent("_86_showBreadcrumbsCheckbox")) {
break;
}
}
 catch (Exception e) {
}
Thread.sleep(1000);
