{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      for (int second=0; ; second++) {
        if (second >= 60) {
          fail("timeout");
        }
        try {
          if (selenium.isElementPresent("link=DLD Setup Test Page")) {
            break;
          }
        }
 catch (        Exception e) {
        }
        Thread.sleep(1000);
      }
    selenium.click(RuntimeVariables.replace("link=DLD Setup Test Page"));
  selenium.waitForPageToLoad("30000");
