{
  Map<String,List<StagedModel>> dependentStagedModelsMap=new HashMap<String,List<StagedModel>>();
  Layout parentLayout=LayoutTestUtil.addLayout(group.getGroupId(),RandomTestUtil.randomString());
  addDependentStagedModel(dependentStagedModelsMap,Layout.class,parentLayout);
  addDependentLayoutFriendlyURLs(dependentStagedModelsMap,parentLayout);
  return dependentStagedModelsMap;
}
