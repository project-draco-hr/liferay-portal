{
  Session session=null;
  try {
    session=openSession();
    KaleoInstanceToken kaleoInstanceToken=(KaleoInstanceToken)session.get(KaleoInstanceTokenImpl.class,primaryKey);
    if (kaleoInstanceToken == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchInstanceTokenException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(kaleoInstanceToken);
  }
 catch (  NoSuchInstanceTokenException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
