{
  if (_phoneNumberFormat == null) {
    try {
      String className=GetterUtil.getString(SystemProperties.get(PhoneNumberFormat.class.getName()),USAPhoneNumberFormat.class.getName());
      _phoneNumberFormat=(PhoneNumberFormat)InstanceFactory.newInstance(className);
    }
 catch (    Exception e) {
      _log.error(e,e);
      _phoneNumberFormat=new USAPhoneNumberFormat();
    }
  }
  return _phoneNumberFormat;
}
