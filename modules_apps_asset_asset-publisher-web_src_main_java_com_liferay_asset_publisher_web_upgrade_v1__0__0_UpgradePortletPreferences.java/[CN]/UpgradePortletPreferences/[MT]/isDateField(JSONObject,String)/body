{
  JSONArray fieldsJSONArray=ddmStructureJSONObject.getJSONArray("fields");
  JSONObject fieldJSONObject=getFieldJSONObject(fieldsJSONArray,selectedFieldName);
  if ((fieldJSONObject != null) && Validator.equals(fieldJSONObject.getString("type"),"ddm-date")) {
    return true;
  }
  return false;
}
