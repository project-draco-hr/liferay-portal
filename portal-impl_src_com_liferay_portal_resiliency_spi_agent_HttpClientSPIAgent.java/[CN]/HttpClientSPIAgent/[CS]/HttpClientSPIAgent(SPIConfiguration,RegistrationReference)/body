{
  this.registrationReference=registrationReference;
  socketAddress=new InetSocketAddress(InetAddressUtil.getLoopbackInetAddress(),spiConfiguration.getConnectorPort());
  socketPool=new ArrayBlockingQueue<Socket>(PropsValues.PORTAL_RESILIENCY_SPI_AGENT_HTTPCLIENTSPIAGENT_SOCKETPOOL_MAX_SIZE);
  String httpRequestContentString="POST " + AGENT_CONTEXT_PATH + MAPPING_PATTERN+ " HTTP/1.1\r\nHost: localhost:"+ spiConfiguration.getConnectorPort()+ "\r\n"+ "Content-Length: 8\r\n\r\n";
  httpRequestContent=httpRequestContentString.getBytes(Charset.forName("US-ASCII"));
}
