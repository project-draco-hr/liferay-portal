{
  this.registrationReference=registrationReference;
  socketAddress=new InetSocketAddress(InetAddressUtil.getLoopbackInetAddress(),spiConfiguration.getConnectorPort());
  socketBlockingQueue=new ArrayBlockingQueue<>(PropsValues.PORTAL_RESILIENCY_SPI_AGENT_CLIENT_POOL_MAX_SIZE);
