{
  boolean pooled=false;
  if (!forceCloseSocket && socket.isConnected() && !socket.isInputShutdown()&& !socket.isOutputShutdown()) {
    pooled=socketPool.offer(socket);
  }
  if (!pooled) {
    try {
      socket.close();
    }
 catch (    IOException ioe) {
      if (_log.isWarnEnabled()) {
        _log.warn(ioe,ioe);
      }
    }
  }
}
