{
  if (nodeId == newNodeId) {
    return;
  }
  checkNodeChange(nodeId,title,newNodeId);
  WikiPage oldPage=getPage(nodeId,title);
  oldPage.setParentTitle(StringPool.BLANK);
  serviceContext.setCommand(Constants.MOVE);
  updatePage(userId,oldPage,newNodeId,oldPage.getTitle(),oldPage.getContent(),oldPage.getSummary(),oldPage.getMinorEdit(),oldPage.getFormat(),oldPage.getParentTitle(),oldPage.getRedirectTitle(),serviceContext);
}
