{
  List<WikiPage> redirectPages=wikiPagePersistence.findByN_R(nodeId,title);
  for (  WikiPage redirectPage : redirectPages) {
    redirectPage=doChangeNode(userId,nodeId,redirectPage.getTitle(),newNodeId,serviceContext);
    Indexer indexer=IndexerRegistryUtil.nullSafeGetIndexer(WikiPage.class);
    indexer.reindex(redirectPage);
  }
}
