{
  for (  BundleContext bundleContext : _sqlPoolMaps.keySet()) {
    if (bundleContext == null) {
      continue;
    }
    Map<String,String> sqlPoolMap=_sqlPoolMaps.get(bundleContext);
    if (sqlPoolMap == null) {
      continue;
    }
    String content=sqlPoolMap.get(id);
    if (content != null) {
      return content;
    }
  }
  return null;
}
