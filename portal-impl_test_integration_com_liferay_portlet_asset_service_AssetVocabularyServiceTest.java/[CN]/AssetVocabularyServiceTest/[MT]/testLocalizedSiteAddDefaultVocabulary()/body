{
  Group group=GroupTestUtil.addGroup();
  LocaleThreadLocal.setSiteDefaultLocale(new Locale("es","ES"));
  AssetVocabulary assetVocabulary=AssetVocabularyLocalServiceUtil.addDefaultVocabulary(group.getGroupId());
  Assert.assertEquals(PropsValues.ASSET_VOCABULARY_DEFAULT,assetVocabulary.getTitle(Locale.US,true));
}
