{
  _request=request;
  _navigationMenuWebConfiguration=navigationMenuWebConfiguration;
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  _navigationMenuPortletInstanceConfiguration=portletDisplay.getPortletInstanceConfiguration(NavigationMenuPortletInstanceConfiguration.class);
}
