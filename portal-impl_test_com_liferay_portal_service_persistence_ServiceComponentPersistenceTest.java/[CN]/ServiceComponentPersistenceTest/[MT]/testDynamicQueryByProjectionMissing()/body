{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ServiceComponent.class,ServiceComponent.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("serviceComponentId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("serviceComponentId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
