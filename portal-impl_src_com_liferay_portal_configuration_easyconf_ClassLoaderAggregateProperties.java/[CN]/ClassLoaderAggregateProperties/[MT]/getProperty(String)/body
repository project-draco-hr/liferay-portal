{
  Object value=null;
  if (value == null) {
    value=System.getProperty(_getPrefix().concat(key));
  }
  if (value == null) {
    value=_globalCompositeConfiguration.getProperty(_getPrefix().concat(key));
  }
  if (value == null) {
    value=_globalCompositeConfiguration.getProperty(key);
  }
  if (value == null) {
    value=_baseCompositeConfiguration.getProperty(key);
  }
  if (value == null) {
    value=super.getProperty(key);
  }
  if (value == null) {
    value=System.getProperty(key);
  }
  if ((value == null) && key.equals(Conventions.COMPANY_ID_PROPERTY)) {
    value=_companyId;
  }
  if ((value == null) && key.equals(Conventions.COMPONENT_NAME_PROPERTY)) {
    value=_componentName;
  }
  return value;
}
