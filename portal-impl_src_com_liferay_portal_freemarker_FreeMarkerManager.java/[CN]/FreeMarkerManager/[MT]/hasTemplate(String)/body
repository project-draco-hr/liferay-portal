{
  try {
    freemarker.template.Template template=_configuration.getTemplate(templateId);
    if (template != null) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  IOException ioe) {
    if (_log.isWarnEnabled()) {
      _log.warn(ioe,ioe);
    }
    return false;
  }
}
