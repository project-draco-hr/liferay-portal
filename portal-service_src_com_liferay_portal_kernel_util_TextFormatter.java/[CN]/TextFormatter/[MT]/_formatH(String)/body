{
  StringBuilder sb=new StringBuilder(s.length() * 2);
  for (int i=0; i < s.length(); i++) {
    char c=s.charAt(i);
    if (Character.isUpperCase(c)) {
      sb.append(CharPool.SPACE);
      sb.append(Character.toLowerCase(c));
    }
 else {
      sb.append(c);
    }
  }
  return sb.toString().trim();
}
