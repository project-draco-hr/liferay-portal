{
  Constructor publicDefaultConstructor=null;
  try {
    publicDefaultConstructor=clazz.getConstructor();
  }
 catch (  Exception e) {
  }
  if ((jsonClass == null || jsonClass == JSONObject.class) && Serializable.class.isAssignableFrom(clazz) && (publicDefaultConstructor != null)) {
    return true;
  }
  return false;
}
