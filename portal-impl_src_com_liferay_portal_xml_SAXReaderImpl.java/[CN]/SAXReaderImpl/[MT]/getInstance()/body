{
  if (_instance != null) {
    return _instance;
  }
synchronized (SAXReaderImpl.class) {
    if (_instance != null) {
      return _instance;
    }
    _instance=new SAXReaderImpl(true);
    SecureXMLFactoryProvider secureXMLFactoryProvider=new SecureXMLFactoryProviderImpl();
    _instance.setSecureXMLFactoryProvider(secureXMLFactoryProvider);
  }
  return _instance;
}
