{
  if (_instance != null) {
    return _instance;
  }
synchronized (SAXReaderImpl.class) {
    if (_instance != null) {
      return _instance;
    }
    _instance=new SAXReaderImpl();
    _instance.setSecure(false);
    _instance.setSecureXMLFactoryProvider(new SecureXMLFactoryProviderImpl());
  }
  return _instance;
}
