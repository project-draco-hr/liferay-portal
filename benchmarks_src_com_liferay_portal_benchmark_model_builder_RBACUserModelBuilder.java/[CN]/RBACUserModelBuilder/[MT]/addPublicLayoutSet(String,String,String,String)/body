{
  validate();
  if ((_user == null) || (_user.getPrivateGroup() == null)) {
    throw new IllegalStateException("Must create a user and private group first");
  }
  _user.setPublicLayoutSet(new LayoutSet(_generator.generate(),_user.getPrivateGroup().getGroupId(),_companyId,false,themeId,themeColorSchemeId,wapThemeId,wapColorSchemeId));
}
