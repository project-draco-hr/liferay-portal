{
  ProcessOutputStream processOutputStream=ProcessContext.getProcessOutputStream();
  try {
    processOutputStream.writeProcessCallable(new ProcessCallable<Serializable>(){
      @Override public Serializable call(){
        ReflectionTestUtil.invoke(BridgeJUnitTestRunner.getRunNotifier(testClass),methodName,new Class<?>[]{argument.getClass()},argument);
        return null;
      }
      private static final long serialVersionUID=1L;
    }
);
  }
 catch (  IOException ioe) {
    ReflectionUtil.throwException(ioe);
  }
}
