  _defaultCompanyLogo=new ImageImpl();
  _defaultCompanyLogo.setTextObj(ImageUtil.read(classLoader,PropsUtil.get(PropsUtil.IMAGE_DEFAULT_COMPANY_LOGO)));
  _defaultUserPortrait=new ImageImpl();
  _defaultUserPortrait.setTextObj(ImageUtil.read(classLoader,PropsUtil.get(PropsUtil.IMAGE_DEFAULT_USER_PORTRAIT)));
}
