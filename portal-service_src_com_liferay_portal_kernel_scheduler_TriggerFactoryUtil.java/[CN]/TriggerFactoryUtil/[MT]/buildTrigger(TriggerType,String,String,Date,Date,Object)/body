{
  if (triggerType == TriggerType.CRON) {
    return new CronTrigger(jobName,groupName,startDate,endDate,String.valueOf(triggerContent));
  }
  if (triggerType == TriggerType.SIMPLE) {
    ObjectValuePair<Long,TimeUnit> objectValuePair=(ObjectValuePair<Long,TimeUnit>)triggerContent;
    return new IntervalTrigger(jobName,groupName,startDate,endDate,objectValuePair.getKey(),objectValuePair.getValue());
  }
  throw new IllegalArgumentException("Unknown trigger type " + triggerType);
}
