{
  if (triggerType == TriggerType.CRON) {
    return new CronTrigger(jobName,groupName,startDate,endDate,String.valueOf(triggerContent));
  }
 else   if (triggerType == TriggerType.SIMPLE) {
    Number number=(Number)triggerContent;
    return new IntervalTrigger(jobName,groupName,startDate,endDate,number.longValue());
  }
 else {
    throw new SchedulerException("Unknown trigger type " + triggerType);
  }
}
