{
  entityCache.putResult(KBArticleModelImpl.ENTITY_CACHE_ENABLED,KBArticleImpl.class,kbArticle.getPrimaryKey(),kbArticle);
  finderCache.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{kbArticle.getUuid(),kbArticle.getGroupId()},kbArticle);
  finderCache.putResult(FINDER_PATH_FETCH_BY_R_V,new Object[]{kbArticle.getResourcePrimKey(),kbArticle.getVersion()},kbArticle);
  finderCache.putResult(FINDER_PATH_FETCH_BY_R_G_V,new Object[]{kbArticle.getResourcePrimKey(),kbArticle.getGroupId(),kbArticle.getVersion()},kbArticle);
  kbArticle.resetOriginalValues();
}
