{
  KBArticle kbArticle=(KBArticle)entityCache.getResult(KBArticleModelImpl.ENTITY_CACHE_ENABLED,KBArticleImpl.class,primaryKey);
  if (kbArticle == _nullKBArticle) {
    return null;
  }
  if (kbArticle == null) {
    Session session=null;
    try {
      session=openSession();
      kbArticle=(KBArticle)session.get(KBArticleImpl.class,primaryKey);
      if (kbArticle != null) {
        cacheResult(kbArticle);
      }
 else {
        entityCache.putResult(KBArticleModelImpl.ENTITY_CACHE_ENABLED,KBArticleImpl.class,primaryKey,_nullKBArticle);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(KBArticleModelImpl.ENTITY_CACHE_ENABLED,KBArticleImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return kbArticle;
}
