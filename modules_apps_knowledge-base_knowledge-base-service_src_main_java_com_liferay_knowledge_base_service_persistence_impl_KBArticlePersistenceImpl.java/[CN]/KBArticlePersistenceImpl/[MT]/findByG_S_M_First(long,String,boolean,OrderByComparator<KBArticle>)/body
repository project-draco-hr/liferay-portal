{
  KBArticle kbArticle=fetchByG_S_M_First(groupId,sections,main,orderByComparator);
  if (kbArticle != null) {
    return kbArticle;
  }
  StringBundler msg=new StringBundler(8);
  msg.append(_NO_SUCH_ENTITY_WITH_KEY);
  msg.append("groupId=");
  msg.append(groupId);
  msg.append(", sections=");
  msg.append(sections);
  msg.append(", main=");
  msg.append(main);
  msg.append(StringPool.CLOSE_CURLY_BRACE);
  throw new NoSuchArticleException(msg.toString());
}
