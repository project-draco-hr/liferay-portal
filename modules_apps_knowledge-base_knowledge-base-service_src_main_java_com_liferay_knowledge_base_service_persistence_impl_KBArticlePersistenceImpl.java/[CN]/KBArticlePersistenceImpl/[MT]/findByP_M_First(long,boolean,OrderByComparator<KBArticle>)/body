{
  KBArticle kbArticle=fetchByP_M_First(parentResourcePrimKey,main,orderByComparator);
  if (kbArticle != null) {
    return kbArticle;
  }
  StringBundler msg=new StringBundler(6);
  msg.append(_NO_SUCH_ENTITY_WITH_KEY);
