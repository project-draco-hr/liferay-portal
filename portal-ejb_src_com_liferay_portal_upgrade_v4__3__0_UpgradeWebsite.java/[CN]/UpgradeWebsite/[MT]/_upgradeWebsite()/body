{
  List websites=WebsiteLocalServiceUtil.getWebsites();
  WebsiteLocalServiceUtil.deleteWebsites();
  for (int i=0; i < websites.size(); i++) {
    Website website=(Website)websites.get(i);
    WebsiteLocalServiceUtil.addWebsite(website.getUserId(),website.getClassName(),website.getClassPK(),website.getUrl(),website.getTypeId(),website.getPrimary());
  }
}
