{
  if (_arrayIndex == 0) {
    return StringPool.BLANK;
  }
  String s=null;
  if (_arrayIndex <= 3) {
    s=_array[0];
    for (int i=1; i < _arrayIndex; i++) {
      s=s.concat(_array[i]);
    }
  }
 else {
    int length=0;
    for (int i=0; i < _arrayIndex; i++) {
      length+=_array[i].length();
    }
    StringBuilder sb=new StringBuilder(length);
    for (int i=0; i < _arrayIndex; i++) {
      sb.append(_array[i]);
    }
    s=sb.toString();
  }
  return s;
}
