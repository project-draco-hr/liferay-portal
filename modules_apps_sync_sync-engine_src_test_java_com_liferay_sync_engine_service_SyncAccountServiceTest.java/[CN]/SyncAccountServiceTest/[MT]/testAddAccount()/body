{
  String targetFilePathName=FileUtil.getFilePathName(System.getProperty("user.home"),"liferay-sync-test3");
  SyncAccount syncAccount2=SyncAccountService.addSyncAccount(targetFilePathName,"test3@liferay.com",1,"","",false,"","","test",5,Collections.<SyncSite,List<SyncFile>>emptyMap(),null,false,"http://localhost:8080");
  syncAccount2=SyncAccountService.fetchSyncAccount(syncAccount2.getSyncAccountId());
  Assert.assertNotNull(syncAccount2);
  SyncFile syncFile=SyncFileService.fetchSyncFile(targetFilePathName);
  Assert.assertTrue(FileKeyUtil.hasFileKey(Paths.get(targetFilePathName),syncFile.getSyncFileId()));
  SyncAccountService.deleteSyncAccount(syncAccount2.getSyncAccountId());
}
