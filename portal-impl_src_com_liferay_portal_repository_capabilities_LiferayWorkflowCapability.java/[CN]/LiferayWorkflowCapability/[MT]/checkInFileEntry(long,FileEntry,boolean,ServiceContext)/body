{
  boolean keepFileVersionLabel=_dlFileEntryServiceAdapter.isKeepFileVersionLabel(fileEntry.getFileEntryId(),majorVersion,serviceContext);
  if ((serviceContext.getWorkflowAction() == WorkflowConstants.ACTION_PUBLISH) && !keepFileVersionLabel) {
    DLFileVersion latestDLFileVersion=_dlFileVersionServiceAdapter.getLatestFileVersion(fileEntry.getFileEntryId(),false);
    DLUtil.startWorkflowInstance(userId,latestDLFileVersion,DLSyncConstants.EVENT_UPDATE,serviceContext);
  }
}
