{
  int count=_counter++;
  if (count == 0) {
    return new MockAddMappingSqlUpdate(dataSource,sql,types);
  }
  if (count == 1) {
    return new MockDeleteMappingSqlUpdate(dataSource,sql,types);
  }
  if (count == 2) {
    return new MockDeleteLeftPrimaryKeyTableMappingsSqlUpdate(dataSource,sql,types);
  }
  if (count == 3) {
    return new MockDeleteRightPrimaryKeyTableMappingsSqlUpdate(dataSource,sql,types);
  }
  return null;
}
