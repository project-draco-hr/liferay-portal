{
  File poshiRunnerPropertiesFile=poshiRunnerExtension.getPoshiRunnerProperties();
  Properties poshiRunnerProperties=GUtil.loadProperties(poshiRunnerPropertiesFile);
  GradleUtil.addDependency(project,POSHI_RUNNER_CONFIGURATION_NAME,poshiRunnerPropertiesFile);
  GradleUtil.addDependency(project,POSHI_RUNNER_CONFIGURATION_NAME,"com.liferay","com.liferay.poshi.runner",poshiRunnerExtension.getVersion());
}
