{
  List<Company> companies=_companyLocaService.getCompanies();
  long classNameId=classNameLocalService.getClassNameId(Group.class);
  for (  Company company : companies) {
    long adminUserId=getCompanyAdminUserId(company);
    long defaultUserId=_userLocalService.getDefaultUserId(company.getCompanyId());
    upgradeCalendarResourceUserId(classNameId,defaultUserId,adminUserId);
    updateCalendarUserIds(classNameId,defaultUserId,adminUserId);
  }
}
