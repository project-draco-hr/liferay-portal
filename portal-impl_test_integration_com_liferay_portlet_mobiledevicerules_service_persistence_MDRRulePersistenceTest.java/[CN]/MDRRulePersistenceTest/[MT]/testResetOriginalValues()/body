{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  MDRRule newMDRRule=addMDRRule();
  _persistence.clearCache();
  MDRRuleModelImpl existingMDRRuleModelImpl=(MDRRuleModelImpl)_persistence.findByPrimaryKey(newMDRRule.getPrimaryKey());
  assertTrue(Validator.equals(existingMDRRuleModelImpl.getUuid(),existingMDRRuleModelImpl.getOriginalUuid()));
  assertEquals(existingMDRRuleModelImpl.getGroupId(),existingMDRRuleModelImpl.getOriginalGroupId());
}
