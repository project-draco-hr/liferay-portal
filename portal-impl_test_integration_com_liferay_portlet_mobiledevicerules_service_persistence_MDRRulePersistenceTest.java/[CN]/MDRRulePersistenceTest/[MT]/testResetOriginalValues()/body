{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  MDRRule newMDRRule=addMDRRule();
  _persistence.clearCache();
  MDRRule existingMDRRule=_persistence.findByPrimaryKey(newMDRRule.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingMDRRule.getUuid(),ReflectionTestUtil.invoke(existingMDRRule,"getOriginalUuid",new Class<?>[0])));
  Assert.assertEquals(existingMDRRule.getGroupId(),ReflectionTestUtil.invoke(existingMDRRule,"getOriginalGroupId",new Class<?>[0]));
}
