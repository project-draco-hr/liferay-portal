{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MDRRule.class,MDRRule.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("ruleId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("ruleId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
