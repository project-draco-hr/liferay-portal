{
  if (ClusterInvokeThreadLocal.isEnabled()) {
    return false;
  }
  boolean pluginReady=(Boolean)context.get(ClusterSchedulerEngine.PLUGIN_READY);
  if (!pluginReady) {
    return false;
  }
  boolean portalReady=(Boolean)context.get(ClusterSchedulerEngine.PORTAL_READY);
  if (!portalReady) {
    return false;
  }
  StorageType storageType=(StorageType)context.get(SchedulerEngine.STORAGE_TYPE);
  if (storageType == StorageType.PERSISTED) {
    return false;
  }
  return true;
}
