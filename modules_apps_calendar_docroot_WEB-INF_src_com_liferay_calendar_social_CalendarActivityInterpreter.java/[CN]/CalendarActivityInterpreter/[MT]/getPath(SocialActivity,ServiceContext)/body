{
  long plid=PortalUtil.getPlidFromPortletId(serviceContext.getScopeGroupId(),PortletKeys.CALENDAR);
  PortletURL portletURL=PortletURLFactoryUtil.create(serviceContext.getRequest(),PortletKeys.CALENDAR,plid,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/view_calendar_booking.jsp");
  portletURL.setParameter("backURL",serviceContext.getCurrentURL());
  portletURL.setParameter("calendarBookingId",String.valueOf(activity.getClassPK()));
  return portletURL.toString();
}
