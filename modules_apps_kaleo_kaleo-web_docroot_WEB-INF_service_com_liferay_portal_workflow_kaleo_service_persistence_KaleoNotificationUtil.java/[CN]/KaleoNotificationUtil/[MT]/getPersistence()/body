{
  if (_persistence == null) {
    _persistence=(KaleoNotificationPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoNotificationPersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoNotificationUtil.class,"_persistence");
  }
  return _persistence;
}
