{
  UpgradeColumn upgradeUserIdColumn=new SwapUpgradeColumnImpl("userId",new Integer(Types.VARCHAR),AvailableMappersUtil.getUserIdMapper());
  UpgradeTable upgradeTable=UpgradeTableFactoryUtil.getUpgradeTable(UserIdMapperTable.TABLE_NAME,UserIdMapperTable.TABLE_COLUMNS,new PKUpgradeColumnImpl("userIdMapperId",false),upgradeUserIdColumn);
  upgradeTable.setCreateSQL(UserIdMapperTable.TABLE_SQL_CREATE);
  upgradeTable.setIndexesSQL(UserIdMapperTable.TABLE_SQL_ADD_INDEXES);
  upgradeTable.updateTable();
}
