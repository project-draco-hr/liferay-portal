{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Chat Test Page");
  selenium.click(RuntimeVariables.replace("link=Chat Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Settings"),selenium.getText("//li[contains(@class,'chat-settings')]/div/span"));
  assertEquals(RuntimeVariables.replace("Online Friends (0)"),selenium.getText("//li[contains(@class,'buddy-list')]/div/span"));
}
