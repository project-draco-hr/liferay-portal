{
  String userToken=null;
  HttpSession session=getSession();
  if (session != null) {
    Long userId=(Long)session.getAttribute(WebKeys.USER_ID);
    User user=null;
    if (userId != null) {
      user=UserLocalServiceUtil.fetchUser(userId);
    }
    if (user != null) {
      userToken=user.getLogin();
    }
  }
  return userToken;
}
