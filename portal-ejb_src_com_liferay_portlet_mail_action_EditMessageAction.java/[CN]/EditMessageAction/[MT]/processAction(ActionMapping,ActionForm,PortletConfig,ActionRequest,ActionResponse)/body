{
  try {
    completeMessage(req);
    sendRedirect(req,res);
  }
 catch (  Exception e) {
    if (e instanceof RecipientException) {
      SessionErrors.add(req,e.getClass().getName());
      req.setAttribute(WebKeys.MAIL_MESSAGE_ATTACHMENTS,getRemoteAttachments(req));
    }
 else {
      throw e;
    }
  }
}
