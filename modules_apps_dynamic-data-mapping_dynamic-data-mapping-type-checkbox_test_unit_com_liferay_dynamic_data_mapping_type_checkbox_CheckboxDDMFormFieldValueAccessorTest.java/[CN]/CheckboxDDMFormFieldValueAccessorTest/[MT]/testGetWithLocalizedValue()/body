{
  Value value=new LocalizedValue(LocaleUtil.US);
  value.addString(LocaleUtil.BRAZIL,"true");
  value.addString(LocaleUtil.US,"false");
  DDMFormFieldValue ddmFormFieldValue=DDMFormValuesTestUtil.createDDMFormFieldValue("Checkbox",value);
  CheckboxDDMFormFieldValueAccessor checkboxDDMFormFieldValueAccessor=new CheckboxDDMFormFieldValueAccessor(LocaleUtil.BRAZIL);
  Assert.assertEquals(Boolean.TRUE,checkboxDDMFormFieldValueAccessor.get(ddmFormFieldValue));
  checkboxDDMFormFieldValueAccessor=new CheckboxDDMFormFieldValueAccessor(LocaleUtil.US);
  Assert.assertEquals(Boolean.FALSE,checkboxDDMFormFieldValueAccessor.get(ddmFormFieldValue));
}
