{
  Set<Header> values=_headers.get(name);
  if (values == null) {
    values=new HashSet<Header>();
    _headers.put(name,values);
  }
  Header header=new Header(value);
  values.add(header);
  if (name.equals(HttpHeaders.CONTENT_TYPE)) {
    setContentType(value);
  }
  super.addHeader(name,value);
}
