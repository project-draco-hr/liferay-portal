{
  ResourceBundle resourceBundleA=_createResourceBundle("keyA","valueA");
  ResourceBundle resourceBundleB=_createResourceBundle("keyB","valueB");
  CompositeResourceBundle compositeResourceBundle=new CompositeResourceBundle(resourceBundleA,resourceBundleB);
  Assert.assertEquals("valueA",compositeResourceBundle.getString("keyA"));
  Assert.assertEquals("valueB",compositeResourceBundle.getString("keyB"));
  Assert.assertEquals(new HashSet<String>(){
{
      add("keyA");
      add("keyB");
    }
  }
,compositeResourceBundle.keySet());
}
