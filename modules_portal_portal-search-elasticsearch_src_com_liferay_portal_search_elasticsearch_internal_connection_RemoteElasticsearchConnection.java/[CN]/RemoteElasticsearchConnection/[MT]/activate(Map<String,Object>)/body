{
  _elasticsearchConfiguration=Configurable.createConfigurable(ElasticsearchConfiguration.class,properties);
  setClusterName(_elasticsearchConfiguration.clusterName());
  setConfigFileName(MapUtil.getString(properties,"configFileName"));
  setTestConfigFileName(MapUtil.getString(properties,"testConfigFileName"));
  List<String> transportAddresses=StringPlus.asList(properties.get("transportAddresses"));
  setTransportAddresses(new HashSet<>(transportAddresses));
}
