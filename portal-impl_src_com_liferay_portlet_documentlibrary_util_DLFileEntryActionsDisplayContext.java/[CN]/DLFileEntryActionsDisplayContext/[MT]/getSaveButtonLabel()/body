{
  String saveButtonLabel="save";
  FileVersion fileVersion=_dlFileEntryActionsDisplayContextHelper.getFileVersion();
  if ((fileVersion == null) || _dlFileEntryActionsDisplayContextHelper.isApproved() || _dlFileEntryActionsDisplayContextHelper.isDraft()) {
    saveButtonLabel="save-as-draft";
  }
  return saveButtonLabel;
}
