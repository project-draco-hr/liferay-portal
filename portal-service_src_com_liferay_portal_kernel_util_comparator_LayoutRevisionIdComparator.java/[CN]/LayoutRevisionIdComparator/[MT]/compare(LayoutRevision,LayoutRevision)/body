{
  int value=0;
  if (layoutRevision1.getLayoutRevisionId() > layoutRevision2.getLayoutRevisionId()) {
    value=1;
  }
 else   if (layoutRevision1.getLayoutRevisionId() < layoutRevision2.getLayoutRevisionId()) {
    value=-1;
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
