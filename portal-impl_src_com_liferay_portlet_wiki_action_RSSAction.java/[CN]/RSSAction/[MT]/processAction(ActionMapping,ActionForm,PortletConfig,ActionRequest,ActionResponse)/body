{
  if (PropsValues.RSS_FEEDS_DISABLED) {
    PortalUtil.sendError(HttpServletResponse.SC_NOT_FOUND,new NoSuchFeedException(),actionRequest,actionResponse);
    return;
  }
  try {
    HttpServletRequest request=PortalUtil.getHttpServletRequest(actionRequest);
    HttpServletResponse response=PortalUtil.getHttpServletResponse(actionResponse);
    ServletResponseUtil.sendFile(request,response,null,getRSS(request),ContentTypes.TEXT_XML_UTF8);
    setForward(actionRequest,ActionConstants.COMMON_NULL);
  }
 catch (  Exception e) {
    PortalUtil.sendError(e,actionRequest,actionResponse);
  }
}
