{
  Object[] group=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select companyId from Group_ where groupId = ?");
    ps.setLong(1,groupId);
    rs=ps.executeQuery();
    while (rs.next()) {
      long companyId=rs.getLong("companyId");
      group=new Object[]{groupId,companyId};
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
  return group;
}
