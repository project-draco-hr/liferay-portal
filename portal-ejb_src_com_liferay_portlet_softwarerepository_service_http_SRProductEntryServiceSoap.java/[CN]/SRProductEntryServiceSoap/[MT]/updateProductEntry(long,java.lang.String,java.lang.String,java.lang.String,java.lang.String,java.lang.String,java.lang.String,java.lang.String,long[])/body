{
  try {
    com.liferay.portlet.softwarerepository.model.SRProductEntry returnValue=SRProductEntryServiceUtil.updateProductEntry(productEntryId,name,type,shortDescription,longDescription,pageURL,repoGroupId,repoArtifactId,licenseIds);
