{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/user/joebloggs/so/dashboard/");
  assertEquals(RuntimeVariables.replace("Sites"),selenium.getText("//div[@id='so-sidebar']/h3"));
  assertTrue(selenium.isVisible("//input[@class='search-input']"));
  selenium.type("//input[@class='search-input']",RuntimeVariables.replace("Open"));
  Thread.sleep(1000);
  assertEquals(RuntimeVariables.replace("Open Site Name"),selenium.getText("//li[contains(@class, 'social-office-enabled')]/span[2]/a"));
  selenium.clickAt("//li[contains(@class, 'social-office-enabled')]/span[2]/a",RuntimeVariables.replace("Open Site Name"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Wiki"),selenium.getText("//nav/ul/li[contains(.,'Wiki')]/a/span"));
  selenium.clickAt("//nav/ul/li[contains(.,'Wiki')]/a/span",RuntimeVariables.replace("Wiki"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isVisible("//div[@class='page-ratings']"));
  assertTrue(selenium.isElementPresent("//div[@class='page-ratings']/div/div/div/a[5]"));
  selenium.clickAt("//div[@class='page-ratings']/div/div/div/a[5]",RuntimeVariables.replace("5 stars out of 5"));
  selenium.waitForElementPresent("//img[@alt='The average rating is 5 stars out of 5.']");
  assertTrue(selenium.isElementPresent("//img[@alt='The average rating is 5 stars out of 5.']"));
  assertEquals(RuntimeVariables.replace("Average (1 Vote)"),selenium.getText("xPath=(//div[@class='aui-rating-label-element'])[2]"));
}
