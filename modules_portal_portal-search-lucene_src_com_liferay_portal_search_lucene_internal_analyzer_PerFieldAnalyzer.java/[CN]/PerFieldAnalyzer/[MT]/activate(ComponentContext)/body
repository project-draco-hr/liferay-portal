{
  Dictionary<String,Object> dictionary=componentContext.getProperties();
  _version=Version.valueOf(GetterUtil.getString(dictionary.get("version"),"LUCENE_35"));
  BundleContext bundleContext=componentContext.getBundleContext();
  _serviceRegistration=bundleContext.registerService(Version.class,_version,new Hashtable<String,Object>());
  initializeAnalyzers();
}
