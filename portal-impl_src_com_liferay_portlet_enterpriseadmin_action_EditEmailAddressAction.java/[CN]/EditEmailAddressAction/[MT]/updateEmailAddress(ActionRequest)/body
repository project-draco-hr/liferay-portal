{
  long emailAddressId=ParamUtil.getLong(actionRequest,"emailAddressId");
  String className=ParamUtil.getString(actionRequest,"className");
  long classPK=ParamUtil.getLong(actionRequest,"classPK");
  String address=ParamUtil.getString(actionRequest,"address");
  int typeId=ParamUtil.getInteger(actionRequest,"typeId");
  boolean primary=ParamUtil.getBoolean(actionRequest,"primary");
  if (emailAddressId <= 0) {
    EmailAddressServiceUtil.addEmailAddress(className,classPK,address,typeId,primary);
  }
 else {
    EmailAddressServiceUtil.updateEmailAddress(emailAddressId,address,typeId,primary);
  }
}
