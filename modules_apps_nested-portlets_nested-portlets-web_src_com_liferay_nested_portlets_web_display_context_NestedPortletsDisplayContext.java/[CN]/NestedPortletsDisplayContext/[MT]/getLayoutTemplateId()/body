{
  if (_layoutTemplateId != null) {
    return _layoutTemplateId;
  }
  _layoutTemplateId=_nestedPortletsPortletInstanceConfiguration.layoutTemplateId();
  if (Validator.isNull(_layoutTemplateId)) {
    _layoutTemplateId=_nestedPortletsConfiguration.layoutTemplateDefault();
  }
  return _layoutTemplateId;
}
