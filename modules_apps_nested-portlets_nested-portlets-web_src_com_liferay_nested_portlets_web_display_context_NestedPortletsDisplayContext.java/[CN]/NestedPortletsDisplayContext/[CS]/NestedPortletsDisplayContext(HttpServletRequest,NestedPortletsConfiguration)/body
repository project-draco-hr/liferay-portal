{
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(com.liferay.portal.util.WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  _nestedPortletsPortletInstanceConfiguration=portletDisplay.getPortletInstanceConfiguration(NestedPortletsPortletInstanceConfiguration.class);
  _request=request;
  _nestedPortletsConfiguration=nestedPortletsConfiguration;
}
