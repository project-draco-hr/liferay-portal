{
  PollsQuestion question=PollsTestUtil.addQuestion(stagingGroup.getGroupId());
  PollsChoice choice=PollsTestUtil.addChoice(stagingGroup.getGroupId(),question.getQuestionId());
  PollsChoiceUtil.update(choice);
  PollsTestUtil.addVote(stagingGroup.getGroupId(),question.getQuestionId(),choice.getChoiceId());
}
