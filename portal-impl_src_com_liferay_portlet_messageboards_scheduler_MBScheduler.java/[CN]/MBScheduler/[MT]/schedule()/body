{
  if (PropsValues.MESSAGE_BOARDS_EXPIRE_BAN_INTERVAL <= 0) {
    if (_log.isDebugEnabled()) {
      _log.debug("Auto expire of banned message board users is disabled");
    }
    return;
  }
  String description="This is the scheduler of message boards";
  JSONObject jsonObj=JSONFactoryUtil.createJSONObject();
  jsonObj.put("Scheduler_Key",MBScheduler_Key);
  String messageBody=jsonObj.toString();
  try {
    SchedulerEngineUtil.schedule(GroupName,_interval,null,null,description,DestinationNames.SCHEDULER_COMMON,messageBody);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
