{
  try {
    if (PropsValues.MESSAGE_BOARDS_EXPIRE_BAN_INTERVAL <= 0) {
      if (_log.isDebugEnabled()) {
        _log.debug("Auto expire of banned message board users is " + "disabled");
      }
      return;
    }
    SchedulerEngineUtil.unschedule(GroupName,GroupName);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
