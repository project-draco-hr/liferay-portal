{
  int initialTrashVersionCount=TrashVersionLocalServiceUtil.getTrashVersionsCount();
  RelatedPages relatedPages=buildRelatedPages();
  WikiPage page=relatedPages.getPage();
  page=WikiPageLocalServiceUtil.movePageToTrash(TestPropsValues.getUserId(),_node.getNodeId(),page.getTitle());
  restoreFromTrash(page);
  Assert.assertEquals(initialTrashVersionCount,TrashVersionLocalServiceUtil.getTrashVersionsCount());
}
