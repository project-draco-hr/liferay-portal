{
synchronized (System.out) {
    if (index > 0) {
      byte[] bytes=toByteArray();
      reset();
      byte[] logData=new byte[_logPrefix.length + bytes.length];
      System.arraycopy(_logPrefix,0,logData,0,_logPrefix.length);
      System.arraycopy(bytes,0,logData,_logPrefix.length,bytes.length);
      _objectOutputStream.writeObject(new LoggingProcessCallable(logData,_error));
    }
    _objectOutputStream.flush();
    _objectOutputStream.reset();
  }
}
