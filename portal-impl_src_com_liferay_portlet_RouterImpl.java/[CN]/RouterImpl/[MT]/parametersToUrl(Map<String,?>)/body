{
  for (  Route route : _routes) {
    String url=route.parametersToUrl(parameters);
    if (url != null) {
      if (!parameters.isEmpty()) {
        url=url.concat(StringPool.QUESTION).concat(parametersToQueryString(parameters));
      }
      return url;
    }
  }
  throw new RouteNotFoundException("No route could be found to use for parameters " + MapUtil.toString(parameters));
}
