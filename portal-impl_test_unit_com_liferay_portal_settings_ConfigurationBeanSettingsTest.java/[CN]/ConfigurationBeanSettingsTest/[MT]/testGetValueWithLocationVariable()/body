{
  when(_mockLocationVariableResolver.isLocationVariable(_configurationBean.locationVariableValue())).thenReturn(true);
  final String expectedValue="Once upon a time...";
  when(_mockLocationVariableResolver.resolve(_configurationBean.locationVariableValue())).thenReturn(expectedValue);
  Assert.assertEquals(expectedValue,_configurationBeanSettings.getValue("locationVariableValue","defaultValue"));
}
