{
  _priority=priority;
  if (_kaleoActionRemoteModel != null) {
    try {
      Class<?> clazz=_kaleoActionRemoteModel.getClass();
      Method method=clazz.getMethod("setPriority",int.class);
      method.invoke(_kaleoActionRemoteModel,priority);
