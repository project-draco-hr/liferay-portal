{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(RatingsStats.class,RatingsStats.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("statsId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("statsId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
