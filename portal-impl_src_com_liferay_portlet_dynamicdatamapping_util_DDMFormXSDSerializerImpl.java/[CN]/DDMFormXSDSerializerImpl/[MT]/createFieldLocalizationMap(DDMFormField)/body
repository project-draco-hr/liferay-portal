{
  Map<Locale,Map<String,String>> localizationMap=new HashMap<Locale,Map<String,String>>();
  addMetadataEntry(localizationMap,"label",ddmFormField.getLabel());
  addMetadataEntry(localizationMap,"predefinedValue",ddmFormField.getPredefinedValue());
  addMetadataEntry(localizationMap,"tip",ddmFormField.getTip());
  return localizationMap;
}
