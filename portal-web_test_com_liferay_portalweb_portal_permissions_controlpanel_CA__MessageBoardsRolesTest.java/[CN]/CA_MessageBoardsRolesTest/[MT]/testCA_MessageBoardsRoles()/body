  selenium.select("_128_scopecom.liferay.portlet.messageboards.model.MBCategoryADD_MESSAGE",RuntimeVariables.replace("label=Portal"));
  selenium.select("_128_scopecom.liferay.portlet.messageboards.model.MBCategoryADD_SUBCATEGORY",RuntimeVariables.replace("label=Portal"));
