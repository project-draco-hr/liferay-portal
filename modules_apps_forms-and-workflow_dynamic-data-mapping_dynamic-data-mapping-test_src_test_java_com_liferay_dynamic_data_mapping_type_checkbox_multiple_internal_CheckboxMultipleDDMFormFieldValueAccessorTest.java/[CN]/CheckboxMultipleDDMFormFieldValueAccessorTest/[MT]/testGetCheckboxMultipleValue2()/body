{
  DDMFormFieldValue ddmFormFieldValue=DDMFormValuesTestUtil.createDDMFormFieldValue("CheckboxMultiple",new UnlocalizedValue(StringPool.BLANK));
  JSONArray actualJSONArray=_checkboxMultipleDDMFormFieldValueAccessor.getValue(ddmFormFieldValue,LocaleUtil.US);
  Assert.assertTrue(actualJSONArray.length() == 0);
