{
  DDMFormFieldValue ddmFormFieldValue=DDMFormValuesTestUtil.createDDMFormFieldValue("CheckboxMultiple",new UnlocalizedValue("value 1"));
  JSONArray actualJSONArray=_checkboxMultipleDDMFormFieldValueAccessor.getValue(ddmFormFieldValue,LocaleUtil.US);
  Assert.assertEquals("value 1",actualJSONArray.getString(0));
}
