{
  ModelListener[] userListeners=userPersistence.getListeners();
  List<com.liferay.portal.model.User> users=null;
  if ((listeners.length > 0) || (userListeners.length > 0)) {
    users=getUsers(permissionId);
    for (    com.liferay.portal.model.User user : users) {
      for (      ModelListener listener : listeners) {
        listener.onBeforeRemoveAssociation(permissionId,com.liferay.portal.model.User.class.getName(),user.getPrimaryKey());
      }
      for (      ModelListener listener : userListeners) {
        listener.onBeforeRemoveAssociation(user.getPrimaryKey(),Permission.class.getName(),permissionId);
      }
    }
  }
  _sqlUpdate.update(new Object[]{new Long(permissionId)});
  if ((listeners.length > 0) || (userListeners.length > 0)) {
    for (    com.liferay.portal.model.User user : users) {
      for (      ModelListener listener : listeners) {
        listener.onAfterRemoveAssociation(permissionId,com.liferay.portal.model.User.class.getName(),user.getPrimaryKey());
      }
      for (      ModelListener listener : userListeners) {
        listener.onBeforeRemoveAssociation(user.getPrimaryKey(),Permission.class.getName(),permissionId);
      }
    }
  }
