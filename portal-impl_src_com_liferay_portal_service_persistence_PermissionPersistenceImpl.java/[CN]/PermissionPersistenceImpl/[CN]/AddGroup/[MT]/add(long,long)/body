{
  if (!_persistenceImpl.containsGroup.contains(permissionId,groupId)) {
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onBeforeAddAssociation(permissionId,com.liferay.portal.model.Group.class.getName(),groupId);
      }
    }
    _sqlUpdate.update(new Object[]{new Long(permissionId),new Long(groupId)});
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onAfterAddAssociation(permissionId,com.liferay.portal.model.Group.class.getName(),groupId);
      }
    }
  }
}
