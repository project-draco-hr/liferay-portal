{
  if (_persistenceImpl.containsGroup.contains(permissionId,groupId)) {
    ModelListener<com.liferay.portal.model.Group>[] groupListeners=groupPersistence.getListeners();
    for (    ModelListener<Permission> listener : listeners) {
      listener.onBeforeRemoveAssociation(permissionId,com.liferay.portal.model.Group.class.getName(),groupId);
    }
    for (    ModelListener<com.liferay.portal.model.Group> listener : groupListeners) {
      listener.onBeforeRemoveAssociation(groupId,Permission.class.getName(),permissionId);
    }
    _sqlUpdate.update(new Object[]{new Long(permissionId),new Long(groupId)});
    for (    ModelListener<Permission> listener : listeners) {
      listener.onAfterRemoveAssociation(permissionId,com.liferay.portal.model.Group.class.getName(),groupId);
    }
    for (    ModelListener<com.liferay.portal.model.Group> listener : groupListeners) {
      listener.onAfterRemoveAssociation(groupId,Permission.class.getName(),permissionId);
    }
  }
}
