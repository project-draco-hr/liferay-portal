{
  if (_persistenceImpl.containsUser.contains(permissionId,userId)) {
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onBeforeRemoveAssociation(permissionId,com.liferay.portal.model.User.class.getName(),userId);
      }
    }
    _sqlUpdate.update(new Object[]{new Long(permissionId),new Long(userId)});
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onAfterRemoveAssociation(permissionId,com.liferay.portal.model.User.class.getName(),userId);
      }
    }
  }
}
