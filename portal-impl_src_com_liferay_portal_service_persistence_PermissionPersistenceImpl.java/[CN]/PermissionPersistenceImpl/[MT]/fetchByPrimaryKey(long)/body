{
  Permission permission=(Permission)EntityCacheUtil.getResult(PermissionModelImpl.ENTITY_CACHE_ENABLED,PermissionImpl.class,permissionId,this);
  if (permission == null) {
    Session session=null;
    try {
      session=openSession();
      permission=(Permission)session.get(PermissionImpl.class,new Long(permissionId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (permission != null) {
        cacheResult(permission);
      }
      closeSession(session);
    }
  }
  return permission;
}
