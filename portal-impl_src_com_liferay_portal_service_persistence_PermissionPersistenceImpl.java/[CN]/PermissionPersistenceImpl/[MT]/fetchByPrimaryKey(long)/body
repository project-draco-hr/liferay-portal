{
  Permission result=(Permission)EntityCacheUtil.getResult(PermissionModelImpl.ENTITY_CACHE_ENABLED,Permission.class,permissionId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      Permission permission=(Permission)session.get(PermissionImpl.class,new Long(permissionId));
      if (permission != null) {
        cacheResult(permission);
      }
      return permission;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (Permission)result;
  }
}
