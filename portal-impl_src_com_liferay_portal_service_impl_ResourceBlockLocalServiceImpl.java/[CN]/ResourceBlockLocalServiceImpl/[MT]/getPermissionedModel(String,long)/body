{
  PersistedModelLocalService persistedModelLocalService=PersistedModelLocalServiceRegistryUtil.getPersistedModelLocalService(name);
  if (persistedModelLocalService == null) {
    throw new ResourceBlocksNotSupportedException();
  }
  PersistedModel persistedModel=persistedModelLocalService.getPersistedModel(primKey);
  try {
    return (PermissionedModel)persistedModel;
  }
 catch (  ClassCastException cce) {
    throw new ResourceBlocksNotSupportedException();
  }
}
