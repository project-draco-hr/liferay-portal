{
  List<ResourceTypePermission> resourceTypePermissions=resourceTypePermissionLocalService.getGroupScopeResourceTypePermissions(companyId,name,roleId);
  for (  ResourceTypePermission resourceTypePermission : resourceTypePermissions) {
    removeGroupScopePermissions(companyId,resourceTypePermission.getGroupId(),name,roleId,actionIdsLong);
  }
}
