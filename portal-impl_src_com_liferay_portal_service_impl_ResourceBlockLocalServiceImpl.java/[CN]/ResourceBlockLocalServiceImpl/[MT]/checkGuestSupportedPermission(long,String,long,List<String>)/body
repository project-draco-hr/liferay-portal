{
  if (!isGuestRole(companyId,roleId)) {
    return;
  }
  List<String> unsupportedActionIds=ResourceActionsUtil.getResourceGuestUnsupportedActions(name,name);
  for (  String actionId : actionIds) {
    if (unsupportedActionIds.contains(actionId)) {
      throw new PrincipalException(actionId + "is not supported by role " + roleId);
    }
  }
}
