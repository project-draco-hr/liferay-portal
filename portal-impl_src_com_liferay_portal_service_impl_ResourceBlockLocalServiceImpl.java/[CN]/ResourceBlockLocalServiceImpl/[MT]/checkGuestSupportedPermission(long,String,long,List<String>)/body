{
  if (!isGuestRoleId(companyId,roleId)) {
    return;
  }
  List<String> unsupportedActions=ResourceActionsUtil.getResourceGuestUnsupportedActions(name,name);
  for (  String actionId : actionIds) {
    if (unsupportedActions.contains(actionId)) {
      throw new PrincipalException(actionId + "is not supported by role " + roleId);
    }
  }
}
