{
  ModelListener listener=null;
  if (Validator.isNotNull(_LISTENER)) {
    try {
      listener=(ModelListener)Class.forName(_LISTENER).newInstance();
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(folderId));
  }
  com.liferay.portlet.imagegallery.model.IGFolder igFolder=getPersistence().remove(folderId);
  if (listener != null) {
    listener.onAfterRemove(igFolder);
  }
  return igFolder;
}
