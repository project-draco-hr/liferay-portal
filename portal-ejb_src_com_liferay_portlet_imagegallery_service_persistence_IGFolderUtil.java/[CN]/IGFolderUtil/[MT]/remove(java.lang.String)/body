{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(folderId));
  }
  com.liferay.portlet.imagegallery.model.IGFolder igFolder=getPersistence().remove(folderId);
  if (listener != null) {
    listener.onAfterRemove(igFolder);
  }
  return igFolder;
}
