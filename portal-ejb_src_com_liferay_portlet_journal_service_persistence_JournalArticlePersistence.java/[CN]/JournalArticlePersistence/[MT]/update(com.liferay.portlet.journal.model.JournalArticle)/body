{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(journalArticle);
    session.flush();
    journalArticle.setNew(false);
    return journalArticle;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
