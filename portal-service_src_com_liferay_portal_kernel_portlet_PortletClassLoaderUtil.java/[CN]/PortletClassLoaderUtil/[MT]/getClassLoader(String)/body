{
  PortletBag portletBag=PortletBagPool.get(portletId);
  if (portletBag == null) {
    return null;
  }
  ServletContext servletContext=portletBag.getServletContext();
  ClassLoader portletClassLoader=(ClassLoader)servletContext.getAttribute(PortletServlet.PORTLET_CLASS_LOADER);
  return portletClassLoader;
}
