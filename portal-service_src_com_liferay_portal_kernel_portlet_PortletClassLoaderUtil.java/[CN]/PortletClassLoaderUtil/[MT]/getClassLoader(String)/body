{
  SecurityManager sm=System.getSecurityManager();
  if (sm != null) {
    sm.checkPermission(new RuntimePermission(PACLConstants.GET_CLASSLOADER.concat(StringPool.PERIOD).concat(portletId)));
  }
  PortletBag portletBag=PortletBagPool.get(portletId);
  if (portletBag == null) {
    return null;
  }
  ServletContext servletContext=portletBag.getServletContext();
  return (ClassLoader)servletContext.getAttribute(PluginContextListener.PLUGIN_CLASS_LOADER);
}
