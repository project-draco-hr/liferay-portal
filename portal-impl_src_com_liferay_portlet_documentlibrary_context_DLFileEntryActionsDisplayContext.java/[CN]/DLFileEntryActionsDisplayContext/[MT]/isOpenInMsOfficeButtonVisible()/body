{
  DLContext dlDisplayContext=_dlActionsDisplayContext.getDLDisplayContext();
  if (_dlFileEntryActionsDisplayContextHelper.hasViewPermission() && _dlFileEntryActionsDisplayContextHelper.isOfficeDoc() && dlDisplayContext.isWebDAVEnabled()&& dlDisplayContext.isIEOnWin32()) {
    return true;
  }
  return false;
}
