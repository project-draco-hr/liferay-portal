{
  Api api=new MarketplaceApi();
  OAuthConfig oAuthConfig=new OAuthConfig(MarketplaceStoreWebConfigurationValues.MARKETPLACE_KEY,MarketplaceStoreWebConfigurationValues.MARKETPLACE_SECRET,MarketplaceStoreWebConfigurationValues.MARKETPLACE_URL,SignatureType.Header,null,null);
  return api.createService(oAuthConfig);
}
