{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SCProductVersion.class,SCProductVersion.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("productVersionId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("productVersionId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
