{
  if (roleName == null) {
    return false;
  }
  roleName=roleName.trim();
  int pos1=Arrays.binarySearch(_sortedSystemRoles,roleName,new StringComparator());
  int pos2=Arrays.binarySearch(_sortedSystemCommunityRoles,roleName,new StringComparator());
  if ((pos1 >= 0) || (pos2 >= 0)) {
    return true;
  }
 else {
    return false;
  }
}
