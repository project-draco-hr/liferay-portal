{
  String companyId=getCompanyId(req);
  if (companyId == null) {
    return null;
  }
  Company company=(Company)req.getAttribute(WebKeys.COMPANY);
  if (company == null) {
    company=CompanyLocalServiceUtil.getCompany(companyId);
    req.setAttribute(WebKeys.COMPANY,company);
  }
  return company;
}
