{
  for (  SettingsDefinition<?,?> settingsDefinition : _settingsDefinitionLifecycleHandlers.keySet()) {
    SettingsDefinitionLifecycleHandler<?,?> settingsDefinitionLifecycleHandler=_settingsDefinitionLifecycleHandlers.get(settingsDefinition);
    settingsDefinitionLifecycleHandler.stop();
  }
  _settingsDefinitionLifecycleHandlers.clear();
}
