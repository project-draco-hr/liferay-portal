{
  Country newCountry=addCountry();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Country.class,Country.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("countryId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("countryId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
