{
  String companyIdString=String.valueOf(CompanyThreadLocal.getCompanyId());
  Map liveUsers=(Map)WebAppPool.get(companyIdString,WebKeys.LIVE_USERS);
  if (liveUsers == null) {
    liveUsers=CollectionFactory.getSyncHashMap();
    WebAppPool.put(companyIdString,WebKeys.LIVE_USERS,liveUsers);
  }
  return liveUsers;
}
