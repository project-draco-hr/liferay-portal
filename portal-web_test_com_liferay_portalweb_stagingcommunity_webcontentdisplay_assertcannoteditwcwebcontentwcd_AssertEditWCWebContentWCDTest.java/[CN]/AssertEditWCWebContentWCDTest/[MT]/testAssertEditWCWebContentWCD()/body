{
  selenium.open("/web/guest/home");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Site Name")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Site Name",RuntimeVariables.replace("Site Name"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Web Content Display Test Page",RuntimeVariables.replace("Web Content Display Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("WCD Web Content Content"),selenium.getText("//div[@class='journal-content-article']/p"));
  assertTrue(selenium.isVisible("//img[@alt='Edit Web Content']"));
  assertTrue(selenium.isVisible("//img[@alt='Select Web Content']"));
  assertTrue(selenium.isVisible("//img[@alt='Add Web Content']"));
}
