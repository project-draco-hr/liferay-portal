{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps=connection.prepareStatement("select groupId, portletId from JournalContentSearch group " + "by groupId, portletId having count(groupId) > 1 and " + "count(portletId) > 1");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long groupId=rs.getLong("groupId");
      String portletId=rs.getString("portletId");
      updateContentSearch(groupId,portletId);
    }
  }
 }
