{
  PKUpgradeColumnImpl pkUpgradeColumn=new PKUpgradeColumnImpl(0,true);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(BookmarksFolderImpl.TABLE_NAME,BookmarksFolderImpl.TABLE_COLUMNS,pkUpgradeColumn);
  upgradeTable.updateTable();
  _folderIdMapper=pkUpgradeColumn.getValueMapper();
  _folderIdMapper.appendException(new Long(BookmarksFolderImpl.DEFAULT_PARENT_FOLDER_ID));
  upgradeTable=new DefaultUpgradeTableImpl(BookmarksFolderImpl.TABLE_NAME,BookmarksFolderImpl.TABLE_COLUMNS,new SwapUpgradeColumnImpl("parentFolderId",_folderIdMapper));
  upgradeTable.updateTable();
  pkUpgradeColumn=new PKUpgradeColumnImpl(0,true);
  upgradeTable=new DefaultUpgradeTableImpl(BookmarksEntryImpl.TABLE_NAME,BookmarksEntryImpl.TABLE_COLUMNS,pkUpgradeColumn,new SwapUpgradeColumnImpl("folderId",_folderIdMapper));
  upgradeTable.updateTable();
  _entryIdMapper=pkUpgradeColumn.getValueMapper();
}
