{
  for (  String columnId : getColumns()) {
    if (isColumnCustomizable(columnId)) {
      boolean customizableColumn=GetterUtil.getBoolean(getTypeSettingsProperty(CustomizedPages.namespaceColumnId(columnId)));
      if (customizableColumn) {
        UnicodeProperties typeSettingsProperties=getTypeSettingsProperties();
        typeSettingsProperties.remove(CustomizedPages.namespaceColumnId(columnId));
      }
    }
  }
}
