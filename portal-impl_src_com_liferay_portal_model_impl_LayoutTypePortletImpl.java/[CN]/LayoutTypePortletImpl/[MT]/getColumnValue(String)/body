{
  Boolean customizable=null;
  Boolean columnDisabled=null;
  if (hasTemplate() && (customizable=isCustomizable()) && (columnDisabled=isColumnDisabled(columnId))) {
    return getTemplateProperty(columnId);
  }
  if (hasUserPreferences() && (customizable == null ? isCustomizable() : customizable) && (columnDisabled == null ? !isColumnDisabled(columnId) : columnDisabled)) {
    return getUserPreference(columnId);
  }
  return getTypeSettingsProperties().getProperty(columnId);
}
