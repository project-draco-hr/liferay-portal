{
  String nestedColumnIds=getTypeSettingsProperties().getProperty(LayoutTypePortletConstants.NESTED_COLUMN_IDS,StringPool.BLANK);
  if (nestedColumnIds.indexOf(columnId) == -1) {
    nestedColumnIds=StringUtil.add(nestedColumnIds,columnId);
    getTypeSettingsProperties().setProperty(LayoutTypePortletConstants.NESTED_COLUMN_IDS,nestedColumnIds);
  }
}
