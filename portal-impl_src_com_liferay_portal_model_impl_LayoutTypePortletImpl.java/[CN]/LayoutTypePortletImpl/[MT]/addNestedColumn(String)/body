{
  String nestedColumnIds=getTypeSettingsProperty(LayoutTypePortletConstants.NESTED_COLUMN_IDS,StringPool.BLANK);
  if (nestedColumnIds.indexOf(columnId) == -1) {
    nestedColumnIds=StringUtil.add(nestedColumnIds,columnId);
    setTypeSettingsProperty(LayoutTypePortletConstants.NESTED_COLUMN_IDS,nestedColumnIds);
  }
}
