{
  String nestedColumnIds=getTypeSettingsProperties().getProperty(NESTED_COLUMN_IDS);
  String[] nestedColumnIdsArray=StringUtil.split(nestedColumnIds);
  nestedColumnIdsArray=ArrayUtil.removeByPrefix(nestedColumnIdsArray,portletId);
  getTypeSettingsProperties().setProperty(NESTED_COLUMN_IDS,StringUtil.merge(nestedColumnIdsArray));
  reorganizePortlets(newColumns,oldColumns);
}
