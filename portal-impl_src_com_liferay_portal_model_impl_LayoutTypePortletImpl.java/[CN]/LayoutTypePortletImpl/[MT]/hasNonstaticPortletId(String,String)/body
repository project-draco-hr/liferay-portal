{
  String columnValue=getColumnValue(columnId);
  String[] columnValues=StringUtil.split(columnValue);
  for (  String nonstaticPortletId : columnValues) {
    if (nonstaticPortletId.equals(portletId) || nonstaticPortletId.startsWith(portletId.concat(PortletConstants.INSTANCE_SEPARATOR))) {
      return true;
    }
  }
  return false;
}
