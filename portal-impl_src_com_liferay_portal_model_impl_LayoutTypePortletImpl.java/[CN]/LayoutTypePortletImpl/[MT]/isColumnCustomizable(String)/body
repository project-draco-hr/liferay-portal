{
  String customizableString=getTypeSettingsProperty(CustomizedPages.namespaceColumnId(columnId));
  boolean customizable=GetterUtil.getBoolean(customizableString);
  if (customizable) {
    if (isLayoutSetPrototype()) {
      return false;
    }
    return true;
  }
 else {
    if (hasUserPreferences()) {
      String columnValue=_portalPreferences.getValue(CustomizedPages.namespacePlid(getPlid()),columnId,StringPool.NULL);
      if (!Objects.equals(columnValue,StringPool.NULL)) {
        setUserPreference(columnId,null);
      }
    }
    return false;
  }
}
