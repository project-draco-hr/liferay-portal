{
  try {
    List list=PortletPreferencesLocalServiceUtil.getPortletPreferences(getLayout().getPlid(),portletId);
    for (int i=0; i < list.size(); i++) {
      PortletPreferences portletPreferences=(PortletPreferences)list.get(i);
      PortletPreferencesLocalServiceUtil.deletePortletPreferences(portletPreferences.getPortletPreferencesId());
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
