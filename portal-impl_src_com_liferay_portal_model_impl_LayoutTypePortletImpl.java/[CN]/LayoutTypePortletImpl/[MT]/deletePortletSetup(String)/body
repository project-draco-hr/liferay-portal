{
  try {
    List<PortletPreferences> portletPreferencesList=PortletPreferencesLocalServiceUtil.getPortletPreferences(getLayout().getPlid(),portletId);
    for (    PortletPreferences portletPreferences : portletPreferencesList) {
      PortletPreferencesLocalServiceUtil.deletePortletPreferences(portletPreferences.getPortletPreferencesId());
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
