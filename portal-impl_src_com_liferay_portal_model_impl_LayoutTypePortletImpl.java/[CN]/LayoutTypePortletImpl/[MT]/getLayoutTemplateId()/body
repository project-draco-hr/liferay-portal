{
  String layoutTemplateId=StringPool.BLANK;
  if (hasSourcePrototypeLayout()) {
    layoutTemplateId=getSourcePrototypeLayoutProperty(LayoutTypePortletConstants.LAYOUT_TEMPLATE_ID);
  }
 else {
    layoutTemplateId=getTypeSettingsProperty(LayoutTypePortletConstants.LAYOUT_TEMPLATE_ID);
  }
  if (Validator.isNull(layoutTemplateId)) {
    layoutTemplateId=StringPool.BLANK;
  }
  return layoutTemplateId;
}
