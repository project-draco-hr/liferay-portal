{
  String layoutTemplateId=getTypeSettingsProperties().getProperty(LAYOUT_TEMPLATE_ID);
  if (Validator.isNull(layoutTemplateId)) {
    layoutTemplateId="2_columns_ii";
    getTypeSettingsProperties().setProperty(LAYOUT_TEMPLATE_ID,layoutTemplateId);
    _log.warn("Layout template id for layout " + getLayout().getPrimaryKey() + " is null, setting it to 2_columns_ii");
  }
  return layoutTemplateId;
}
