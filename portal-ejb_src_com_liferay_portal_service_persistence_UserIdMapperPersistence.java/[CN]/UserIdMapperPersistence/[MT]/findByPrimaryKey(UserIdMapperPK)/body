{
  Session session=null;
  try {
    session=openSession();
    UserIdMapper userIdMapper=(UserIdMapper)session.get(UserIdMapper.class,userIdMapperPK);
    if (userIdMapper == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No UserIdMapper exists with the primary key " + userIdMapperPK.toString());
      }
      throw new NoSuchUserIdMapperException("No UserIdMapper exists with the primary key " + userIdMapperPK.toString());
    }
    return userIdMapper;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
