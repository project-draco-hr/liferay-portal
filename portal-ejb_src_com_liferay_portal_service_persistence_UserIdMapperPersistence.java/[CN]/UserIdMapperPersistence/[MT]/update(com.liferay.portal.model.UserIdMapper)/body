{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(userIdMapper);
    session.flush();
    userIdMapper.setNew(false);
    return userIdMapper;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
