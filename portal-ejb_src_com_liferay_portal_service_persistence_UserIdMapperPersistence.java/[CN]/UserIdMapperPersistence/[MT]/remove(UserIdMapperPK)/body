{
  Session session=null;
  try {
    session=openSession();
    UserIdMapper userIdMapper=(UserIdMapper)session.get(UserIdMapperImpl.class,userIdMapperPK);
    if (userIdMapper == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No UserIdMapper exists with the primary key " + userIdMapperPK);
      }
      throw new NoSuchUserIdMapperException("No UserIdMapper exists with the primary key " + userIdMapperPK);
    }
    return remove(userIdMapper);
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
