{
  Session session=null;
  try {
    session=openSession();
    StringBuffer query=new StringBuffer();
    query.append("FROM UserIdMapper IN CLASS com.liferay.portal.service.persistence.UserIdMapperHBM ");
    Query q=session.createQuery(query.toString());
    Iterator itr=q.iterate();
    List list=new ArrayList();
    while (itr.hasNext()) {
      UserIdMapperHBM userIdMapperHBM=(UserIdMapperHBM)itr.next();
      list.add(UserIdMapperHBMUtil.model(userIdMapperHBM));
    }
    return list;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
