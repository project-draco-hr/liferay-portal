{
  UserIdMapper userIdMapper=fetchByPrimaryKey(userIdMapperId);
  if (userIdMapper == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No UserIdMapper exists with the primary key " + userIdMapperId);
    }
    throw new NoSuchUserIdMapperException("No UserIdMapper exists with the primary key " + userIdMapperId);
  }
  return userIdMapper;
}
