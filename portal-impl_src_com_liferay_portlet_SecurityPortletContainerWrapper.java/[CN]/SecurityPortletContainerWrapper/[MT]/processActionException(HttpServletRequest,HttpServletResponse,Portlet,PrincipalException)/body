{
  if (_log.isDebugEnabled()) {
    _log.debug(pe);
  }
  String url=getOriginalURL(request);
  if (_log.isWarnEnabled()) {
    _log.warn(String.format("Action denied for user %s with url %s and portlet %s",PortalUtil.getUserId(request),url,portlet.getPortletId()));
  }
  return ActionResult.EMPTY_ACTION_RESULT;
}
