{
  String type=_getProperty(bundle,"Liferay-Theme-Contributor-Type","themeContributorType");
  if (type == null) {
    return null;
  }
  BundleWebResourcesImpl bundleWebResources=_scanForResources(bundle);
  if (bundleWebResources == null) {
    return null;
  }
  int themeContributorWeight=GetterUtil.getInteger(_getProperty(bundle,"Liferay-Theme-Contributor-Weight","themeContributorWeight"));
  return new ThemeContributorExtension(bundle,bundleWebResources,themeContributorWeight);
}
