{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Second Edited Wiki Test")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("link=Second Edited Wiki Test");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
