      if (selenium.isElementPresent("link=Second Edited Wiki Test")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Second Edited Wiki Test"));
  selenium.waitForPageToLoad("30000");
