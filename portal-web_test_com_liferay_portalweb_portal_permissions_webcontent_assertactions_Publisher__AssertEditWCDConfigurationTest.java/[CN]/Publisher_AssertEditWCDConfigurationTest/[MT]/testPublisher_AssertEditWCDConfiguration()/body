{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Display Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Web Content Display Permissions Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=Configuration"));
  selenium.click(RuntimeVariables.replace("link=Configuration"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=Setup"));
  assertTrue(selenium.isElementPresent("link=Supported Clients"));
  assertTrue(selenium.isElementPresent("link=Permissions"));
  assertTrue(selenium.isElementPresent("link=Sharing"));
}
