{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Web Content Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertTrue(selenium.isElementPresent("link=Configuration"));
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertTrue(selenium.isElementPresent("link=Setup"));
  assertTrue(selenium.isElementPresent("link=Supported Clients"));
  assertTrue(selenium.isElementPresent("link=Permissions"));
  assertTrue(selenium.isElementPresent("link=Sharing"));
}
