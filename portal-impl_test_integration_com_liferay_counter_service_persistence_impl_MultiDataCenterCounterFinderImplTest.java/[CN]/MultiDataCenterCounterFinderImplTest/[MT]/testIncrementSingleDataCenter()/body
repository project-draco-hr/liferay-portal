{
  long value=Long.MAX_VALUE;
  MultiDataCenterCounterFinderImpl multiDataCenterCounterFinderImpl=new MultiDataCenterCounterFinderImpl(1,0);
  Assert.assertEquals(0,multiDataCenterCounterFinderImpl.getMultiClusterSafeValue(0));
  Assert.assertEquals(value,multiDataCenterCounterFinderImpl.getMultiClusterSafeValue(value));
}
