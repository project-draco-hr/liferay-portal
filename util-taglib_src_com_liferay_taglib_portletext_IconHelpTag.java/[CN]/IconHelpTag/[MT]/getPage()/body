{
  if (FileAvailabilityUtil.isAvailable(servletContext,_PAGE)) {
    return _PAGE;
  }
  PortletDisplay portletDisplay=(PortletDisplay)pageContext.getAttribute("portletDisplay");
  if (!portletDisplay.isShowHelpIcon()) {
    return null;
  }
  setCssClass("portlet-help portlet-help-icon");
  setImage("../aui/question-sign");
  setMessage("help");
  setToolTip(false);
  setUrl(portletDisplay.getURLHelp());
  return super.getPage();
}
