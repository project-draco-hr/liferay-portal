{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PortletItem.class,PortletItem.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("portletItemId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("portletItemId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
