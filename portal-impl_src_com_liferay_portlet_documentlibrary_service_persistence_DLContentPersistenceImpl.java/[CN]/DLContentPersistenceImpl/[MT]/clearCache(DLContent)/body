{
  EntityCacheUtil.removeResult(DLContentModelImpl.ENTITY_CACHE_ENABLED,DLContentImpl.class,dlContent.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_R_P_V,new Object[]{Long.valueOf(dlContent.getCompanyId()),Long.valueOf(dlContent.getRepositoryId()),dlContent.getPath(),dlContent.getVersion()});
}
