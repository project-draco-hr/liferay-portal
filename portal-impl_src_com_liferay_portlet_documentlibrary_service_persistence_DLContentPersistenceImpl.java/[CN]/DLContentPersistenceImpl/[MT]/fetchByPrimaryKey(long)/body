{
  DLContent dlContent=(DLContent)EntityCacheUtil.getResult(DLContentModelImpl.ENTITY_CACHE_ENABLED,DLContentImpl.class,contentId,this);
  if (dlContent == _nullDLContent) {
    return null;
  }
  if (dlContent == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      dlContent=(DLContent)session.get(DLContentImpl.class,Long.valueOf(contentId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (!hasException && (dlContent == null)) {
        EntityCacheUtil.putResult(DLContentModelImpl.ENTITY_CACHE_ENABLED,DLContentImpl.class,contentId,_nullDLContent);
      }
 else {
        cacheResult(dlContent);
      }
      closeSession(session);
    }
  }
  return dlContent;
}
