{
  dlContent=toUnwrappedModel(dlContent);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,dlContent);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  DLContentModelImpl dlContentModelImpl=(DLContentModelImpl)dlContent;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_R_P_V,new Object[]{Long.valueOf(dlContentModelImpl.getCompanyId()),Long.valueOf(dlContentModelImpl.getRepositoryId()),dlContentModelImpl.getPath(),dlContentModelImpl.getVersion()});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_P_R_P_V,new Object[]{Long.valueOf(dlContentModelImpl.getCompanyId()),dlContentModelImpl.getPortletId(),Long.valueOf(dlContentModelImpl.getRepositoryId()),dlContentModelImpl.getPath(),dlContentModelImpl.getVersion()});
  EntityCacheUtil.removeResult(DLContentModelImpl.ENTITY_CACHE_ENABLED,DLContentImpl.class,dlContent.getPrimaryKey());
  return dlContent;
}
