{
  dlContent=toUnwrappedModel(dlContent);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,dlContent);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(dlContent);
  return dlContent;
}
