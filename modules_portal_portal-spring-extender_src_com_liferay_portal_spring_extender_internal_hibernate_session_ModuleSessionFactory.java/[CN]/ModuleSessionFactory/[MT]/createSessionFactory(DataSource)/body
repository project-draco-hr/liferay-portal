{
  ModuleHibernateConfiguration moduleHibernateConfiguration=new ModuleHibernateConfiguration(_classLoader);
  moduleHibernateConfiguration.setBeanFactory(getBeanFactory());
  moduleHibernateConfiguration.setDataSource(dataSource);
  try {
    return moduleHibernateConfiguration.buildSessionFactory();
  }
 catch (  Exception e) {
    return null;
  }
}
