{
  int size=1;
  for (  LinkedHashMap<String,Object> param : params) {
    if (param != null) {
      size+=param.size() * 2;
    }
  }
  StringBundler sb=new StringBundler(size);
  for (  LinkedHashMap<String,Object> param : params) {
    if (param != null) {
      for (      String key : param.keySet()) {
        sb.append(key);
        sb.append(StringPool.COMMA);
      }
    }
  }
  sb.append(obc.getOrderBy());
  return sb.toString();
}
