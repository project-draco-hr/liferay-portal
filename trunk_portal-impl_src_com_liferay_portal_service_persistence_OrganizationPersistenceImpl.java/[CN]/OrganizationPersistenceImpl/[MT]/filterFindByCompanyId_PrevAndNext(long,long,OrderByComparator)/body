{
  if (!InlineSQLHelperUtil.isEnabled()) {
    return findByCompanyId_PrevAndNext(organizationId,companyId,orderByComparator);
  }
  Organization organization=findByPrimaryKey(organizationId);
  Session session=null;
  try {
    session=openSession();
    Organization[] array=new OrganizationImpl[3];
    array[0]=filterGetByCompanyId_PrevAndNext(session,organization,companyId,orderByComparator,true);
    array[1]=organization;
    array[2]=filterGetByCompanyId_PrevAndNext(session,organization,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
