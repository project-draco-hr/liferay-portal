{
  byte[] bytes={Byte.MIN_VALUE,Byte.MAX_VALUE};
  InputStream is=new ByteArrayInputStream(bytes);
  _portletDataContext.addZipEntry(_PATH,is);
  Mockito.verify(_zipWriter,Mockito.times(1)).addEntry(_PATH,is);
  _portletDataContext.addZipEntry(_PATH,is);
  Mockito.verify(_zipWriter,Mockito.times(1)).addEntry(_PATH,is);
}
