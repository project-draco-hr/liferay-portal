{
  event.getHttpServletResponse().setContentType(ContentTypes.TEXT_PLAIN_UTF8);
  CatalinaCometRequest cometRequest=new CatalinaCometRequest(event.getHttpServletRequest(),System.currentTimeMillis());
  CatalinaCometResponse cometResponse=new CatalinaCometResponse(event);
  CatalinaCometSession cometSession=new CatalinaCometSession(sessionId,cometRequest,cometResponse,event);
  CometHandler cometHandler=_cometHandler.clone();
  _cometHandlerPool.startCometHandler(cometSession,cometHandler);
}
