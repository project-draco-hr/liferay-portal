{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select threadFlagId, userId, threadId from MBThreadFlag");
    rs=ps.executeQuery();
    while (rs.next()) {
      long threadFlagId=rs.getLong("threadFlagId");
      long userId=rs.getLong("userId");
      long threadId=rs.getLong("threadId");
      String fullName=getFullName(userId);
      long[] groupCompanyIdsArray=getGroupCompanyIdsArray(threadId);
      updateThreadFlag(threadFlagId,groupCompanyIdsArray[0],groupCompanyIdsArray[1],fullName);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
