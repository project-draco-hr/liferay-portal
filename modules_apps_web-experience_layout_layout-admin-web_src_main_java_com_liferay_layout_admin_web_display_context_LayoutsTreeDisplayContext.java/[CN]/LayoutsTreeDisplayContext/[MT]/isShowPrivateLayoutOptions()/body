{
  if (!isPrivateLayout()) {
    return false;
  }
  if (isShowAddRootLayoutButton()) {
    return true;
  }
  if (isShowAddBothRootLayoutButtons()) {
    return true;
  }
  if (isShowEditLayoutSetButton()) {
    return true;
  }
  if (isShowExpandLayoutSetButton(false)) {
    return true;
  }
  if (isShowAddBothRootLayoutButtons()) {
    return true;
  }
  return false;
}
