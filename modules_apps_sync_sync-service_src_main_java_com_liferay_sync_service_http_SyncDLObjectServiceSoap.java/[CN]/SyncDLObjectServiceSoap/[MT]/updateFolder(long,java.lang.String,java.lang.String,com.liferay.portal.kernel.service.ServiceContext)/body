{
  try {
    com.liferay.sync.model.SyncDLObject returnValue=SyncDLObjectServiceUtil.updateFolder(folderId,name,description,serviceContext);
    return com.liferay.sync.model.SyncDLObjectSoap.toSoapModel(returnValue);
