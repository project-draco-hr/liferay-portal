{
  try {
    DDLRecordSet recordSet=_ddlRecordSetLocalService.getRecordSet(recordSetId);
    DDMStructure ddmStructure=recordSet.getDDMStructure();
    String ddmStructureName=ddmStructure.getName(locale);
    String recordSetName=recordSet.getName(locale);
    return LanguageUtil.format(locale,"new-x-for-list-x",new Object[]{ddmStructureName,recordSetName},false);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return StringPool.BLANK;
}
