{
  try {
    List<KaleoLog> kaleoLogs=KaleoLogLocalServiceUtil.getKaleoInstanceKaleoLogs(workflowInstanceId,logTypes,start,end,KaleoLogOrderByComparator.getOrderByComparator(orderByComparator));
    return toWorkflowLogs(kaleoLogs);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
