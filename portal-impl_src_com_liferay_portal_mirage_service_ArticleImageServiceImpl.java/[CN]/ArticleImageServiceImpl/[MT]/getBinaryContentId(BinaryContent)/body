{
  MirageArticleImage mirageArticleImage=(MirageArticleImage)binaryContent;
  JournalArticleImage articleImage=mirageArticleImage.getJournalImage();
  try {
    return getArticleImageId(articleImage.getGroupId(),articleImage.getArticleId(),articleImage.getVersion(),articleImage.getElName(),articleImage.getLanguageId(),articleImage.isTempImage());
  }
 catch (  SystemException se) {
    throw new CMSException(se.getMessage(),se);
  }
}
