{
  MirageArticleImage mirageArticleImage=(MirageArticleImage)binaryContent;
  JournalArticleImage articleImage=mirageArticleImage.getJournalImage();
  try {
    addArticleImageId(articleImage.getArticleImageId(),articleImage.getGroupId(),articleImage.getArticleId(),articleImage.getVersion(),articleImage.getElName(),articleImage.getLanguageId());
  }
 catch (  PortalException pe) {
    throw new CMSException(pe.getMessage(),pe);
  }
catch (  SystemException se) {
    throw new CMSException(se.getMessage(),se);
  }
}
