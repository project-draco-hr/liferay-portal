{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(liferayPortletRequest,BlogsPortletKeys.BLOGS,0,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcRenderCommandName","/blogs/edit_entry");
  portletURL.setParameter("entryId",String.valueOf(_entry.getEntryId()));
