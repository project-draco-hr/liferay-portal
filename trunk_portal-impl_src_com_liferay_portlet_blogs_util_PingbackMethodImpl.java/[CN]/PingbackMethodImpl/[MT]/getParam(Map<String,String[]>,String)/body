{
  String[] paramArray=params.get(name);
  if (paramArray == null) {
    String namespace=PortalUtil.getPortletNamespace(PortletKeys.BLOGS);
    paramArray=params.get(namespace + name);
  }
  if ((paramArray != null) && (paramArray.length > 0)) {
    return paramArray[0];
  }
 else {
    return null;
  }
}
