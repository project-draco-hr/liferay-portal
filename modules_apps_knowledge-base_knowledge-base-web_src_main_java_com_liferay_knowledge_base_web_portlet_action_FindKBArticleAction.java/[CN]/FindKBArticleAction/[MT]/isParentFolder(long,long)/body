{
  if (resourcePrimKey == kbFolderId) {
    return true;
  }
  while (kbFolderId != KBFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    if (resourcePrimKey == kbFolderId) {
      return true;
    }
    KBFolder kbFolder=KBFolderLocalServiceUtil.getKBFolder(kbFolderId);
    kbFolderId=kbFolder.getParentKBFolderId();
  }
  return false;
}
