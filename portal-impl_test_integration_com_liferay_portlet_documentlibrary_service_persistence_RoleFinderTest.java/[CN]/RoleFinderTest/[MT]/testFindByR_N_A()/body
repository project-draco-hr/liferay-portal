{
  List<Role> roles=RoleFinderUtil.findByR_N_A(_resourceBlock.getResourceBlockId(),_resourceBlock.getName(),_bookmarkFolderResourceAction.getActionId());
  for (  Role curRole : roles) {
    if (curRole.getRoleId() == _arbitraryRole.getRoleId()) {
      return;
    }
  }
  Assert.fail("The method findByR_N_A should have returned the role " + _arbitraryRole.getRoleId());
}
