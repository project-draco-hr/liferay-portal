{
  if (!_paclPolicy.isActive()) {
    return true;
  }
  if (permission instanceof PACLUtil.Permission) {
    throw new PACLUtil.Exception(_paclPolicy);
  }
  if (_permissionCollection.implies(permission)) {
    return true;
  }
  if (!_paclPolicy.implies(permission)) {
    return false;
  }
  return true;
}
