{
  String serializedDDMFormValues=read("ddm-form-values-json-deserializer-repeatable-field.json");
  DDMFormValues ddmFormValues=DDMFormValuesJSONDeserializerUtil.deserialize(serializedDDMFormValues);
  List<DDMFormFieldValue> ddmFormFieldValues=ddmFormValues.getDDMFormFieldValues();
  Assert.assertEquals(1,ddmFormFieldValues.size());
  testTextDDMFormFieldValue(ddmFormFieldValues.get(0));
}
