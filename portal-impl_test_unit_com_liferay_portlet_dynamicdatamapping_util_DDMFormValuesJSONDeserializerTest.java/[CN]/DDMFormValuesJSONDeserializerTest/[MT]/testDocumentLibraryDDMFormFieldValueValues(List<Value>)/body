{
  JSONObject expectedJSONObject=JSONFactoryUtil.createJSONObject();
  expectedJSONObject.put("groupId",10192);
  expectedJSONObject.put("uuid","c8acdf70-e101-46a6-83e5-c5f5e087b0dc");
  expectedJSONObject.put("version",1.0);
  Value value=values.get(0);
  JSONAssert.assertEquals(expectedJSONObject.toString(),value.getValue(LocaleUtil.US),false);
  JSONAssert.assertEquals(expectedJSONObject.toString(),value.getValue(LocaleUtil.BRAZIL),false);
}
