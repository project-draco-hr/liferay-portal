{
  Iterable<Long[]> companyAndGroupIds=getCompanyAndGroupIds(PortalUtil.getClassNameId(Company.class));
  for (  Long[] companyAndGroupId : companyAndGroupIds) {
    Long companyId=companyAndGroupId[0];
    Long groupId=companyAndGroupId[1];
    for (    TranslationUpdater translationUpdater : getTranslationUpdaters()) {
      translationUpdater.update(companyId,groupId);
    }
  }
}
