{
  AssetEntryLocalServiceUtil.deleteEntry(JournalFolder.class.getName(),_folderB.getFolderId());
  LastSessionRecorderUtil.syncLastSessionState();
  List<JournalFolder> folders=JournalFolderFinderUtil.findF_ByNoAssets();
  Assert.assertEquals(1,folders.size());
  JournalFolder folder=folders.get(0);
  Assert.assertEquals(_folderB.getFolderId(),folder.getFolderId());
}
