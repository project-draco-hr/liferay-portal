{
  QueryDefinition<Object> queryDefinition=new QueryDefinition<Object>();
  queryDefinition.setStatus(WorkflowConstants.STATUS_ANY);
  Assert.assertEquals(3,JournalFolderFinderUtil.countF_A_ByG_F(_group.getGroupId(),_folder1.getFolderId(),queryDefinition));
  queryDefinition.setStatus(WorkflowConstants.STATUS_IN_TRASH);
  Assert.assertEquals(1,JournalFolderFinderUtil.countF_A_ByG_F(_group.getGroupId(),_folder1.getFolderId(),queryDefinition));
  queryDefinition.setStatus(WorkflowConstants.STATUS_IN_TRASH,true);
  Assert.assertEquals(2,JournalFolderFinderUtil.countF_A_ByG_F(_group.getGroupId(),_folder1.getFolderId(),queryDefinition));
}
