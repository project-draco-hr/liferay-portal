{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(RatingsEntry.class,RatingsEntry.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("entryId",ServiceTestUtil.nextLong()));
  List<RatingsEntry> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
