{
  int count=countByFileEntryTypeId(fileEntryTypeId);
  List<DLFileEntryMetadata> list=findByFileEntryTypeId(fileEntryTypeId,count - 1,count,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(4);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("fileEntryTypeId=");
    msg.append(fileEntryTypeId);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchFileEntryMetadataException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
