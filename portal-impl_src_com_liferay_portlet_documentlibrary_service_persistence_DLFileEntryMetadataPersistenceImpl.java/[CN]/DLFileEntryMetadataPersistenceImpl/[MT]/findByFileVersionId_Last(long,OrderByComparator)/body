{
  int count=countByFileVersionId(fileVersionId);
  List<DLFileEntryMetadata> list=findByFileVersionId(fileVersionId,count - 1,count,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(4);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("fileVersionId=");
    msg.append(fileVersionId);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchFileEntryMetadataException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
