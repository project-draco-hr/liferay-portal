{
  DLFileEntryMetadata dlFileEntryMetadata=fetchByPrimaryKey(fileEntryMetadataId);
  if (dlFileEntryMetadata == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + fileEntryMetadataId);
    }
    throw new NoSuchFileEntryMetadataException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + fileEntryMetadataId);
  }
  return dlFileEntryMetadata;
}
