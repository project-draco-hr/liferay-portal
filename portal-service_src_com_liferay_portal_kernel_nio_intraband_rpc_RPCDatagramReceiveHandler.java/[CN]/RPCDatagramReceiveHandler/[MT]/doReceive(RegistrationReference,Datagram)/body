{
  Deserializer deserializer=new Deserializer(datagram.getDataByteBuffer());
  Serializer serializer=new Serializer();
  try {
    ProcessCallable<? extends Serializable> processCallable=deserializer.readObject();
    serializer.writeObject(new RPCResponse(processCallable.call()));
  }
 catch (  Exception e) {
    serializer.writeObject(new RPCResponse(e));
  }
  Intraband intraband=registrationReference.getIntraband();
  intraband.sendDatagram(registrationReference,Datagram.createResponseDatagram(datagram,serializer.toByteBuffer()));
}
