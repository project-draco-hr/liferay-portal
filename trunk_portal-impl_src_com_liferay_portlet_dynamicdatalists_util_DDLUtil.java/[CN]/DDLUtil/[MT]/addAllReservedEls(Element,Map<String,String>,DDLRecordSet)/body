{
  JournalUtil.addReservedEl(rootElement,tokens,DDLConstants.RESERVED_RECORD_SET_ID,String.valueOf(recordSet.getRecordSetId()));
  JournalUtil.addReservedEl(rootElement,tokens,DDLConstants.RESERVED_RECORD_SET_NAME,recordSet.getName());
  JournalUtil.addReservedEl(rootElement,tokens,DDLConstants.RESERVED_RECORD_SET_DESCRIPTION,recordSet.getDescription());
  JournalUtil.addReservedEl(rootElement,tokens,DDLConstants.RESERVED_DDM_STRUCTURE_ID,String.valueOf(recordSet.getDDMStructureId()));
}
