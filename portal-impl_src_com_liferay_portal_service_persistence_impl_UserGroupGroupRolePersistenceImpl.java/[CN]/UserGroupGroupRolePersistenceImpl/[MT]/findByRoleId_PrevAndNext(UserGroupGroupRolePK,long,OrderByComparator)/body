{
  UserGroupGroupRole userGroupGroupRole=findByPrimaryKey(userGroupGroupRolePK);
  Session session=null;
  try {
    session=openSession();
    UserGroupGroupRole[] array=new UserGroupGroupRoleImpl[3];
    array[0]=getByRoleId_PrevAndNext(session,userGroupGroupRole,roleId,orderByComparator,true);
    array[1]=userGroupGroupRole;
    array[2]=getByRoleId_PrevAndNext(session,userGroupGroupRole,roleId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
