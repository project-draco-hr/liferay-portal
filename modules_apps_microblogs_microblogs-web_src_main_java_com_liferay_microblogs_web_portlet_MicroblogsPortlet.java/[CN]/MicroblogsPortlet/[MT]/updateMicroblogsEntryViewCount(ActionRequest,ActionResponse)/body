{
  long microblogsEntryId=ParamUtil.getLong(actionRequest,"microblogsEntryId");
  MicroblogsEntry microblogsEntry=_microblogsEntryLocalService.fetchMicroblogsEntry(microblogsEntryId);
  if (microblogsEntry == null) {
    return;
  }
  _assetEntryLocalService.incrementViewCounter(0,MicroblogsEntry.class.getName(),microblogsEntryId,1);
}
