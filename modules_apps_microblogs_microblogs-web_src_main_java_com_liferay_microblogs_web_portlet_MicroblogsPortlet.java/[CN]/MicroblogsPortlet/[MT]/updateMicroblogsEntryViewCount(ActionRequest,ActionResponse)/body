{
  long microblogsEntryId=ParamUtil.getLong(actionRequest,"microblogsEntryId");
  MicroblogsEntry microblogsEntry=microblogsEntryLocalService.fetchMicroblogsEntry(microblogsEntryId);
  if (microblogsEntry == null) {
    return;
  }
  assetEntryLocalService.incrementViewCounter(0,MicroblogsEntry.class.getName(),microblogsEntryId,1);
}
