{
  if (service == null) {
    return;
  }
  EntryWrapper entryWrapper=new EntryWrapper(serviceReference,service);
synchronized (_services) {
    int position=Collections.binarySearch(_services,entryWrapper);
    if (remove) {
      if (position >= 0) {
        _services.remove(position);
      }
    }
 else     if (position < 0) {
      _services.add(((-position) - 1),entryWrapper);
    }
  }
}
