{
  try {
    Project project=getProject();
    JavadocFormatter javadocFormatter=JavadocFormatterInvoker.invoke(project.getBaseDir(),_javadocFormatterArgs);
    Set<String> modifiedFileNames=javadocFormatter.getModifiedFileNames();
    project.addIdReference(JavadocFormatter.OUTPUT_KEY_MODIFIED_FILES,modifiedFileNames);
  }
 catch (  Exception e) {
    throw new BuildException(e);
  }
}
