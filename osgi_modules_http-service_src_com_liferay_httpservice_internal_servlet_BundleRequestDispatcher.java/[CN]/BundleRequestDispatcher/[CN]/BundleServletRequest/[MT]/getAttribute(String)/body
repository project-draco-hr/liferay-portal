{
  if ((name.equals(INCLUDE_SERVLET_PATH) || name.equals(WebKeys.INVOKER_FILTER_URI)) && _attributes.containsKey(WebKeys.SERVLET_PATH)) {
    return _attributes.get(WebKeys.SERVLET_PATH);
  }
  if (ArrayUtil.contains(_MASKED_ATTRIBUTES,name)) {
    return _attributes.get(name);
  }
  return super.getAttribute(name);
}
