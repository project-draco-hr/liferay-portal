{
  if (!fileName.endsWith(".jsp") && !fileName.endsWith(".jspf")) {
    return content;
  }
  _jspSourceProcessor=jspSourceProcessor;
  String originalContent=content;
  while (true) {
    String newContent=_calculateTabs(fileName,content,originalContent);
    if (newContent.equals(content) || newContent.equals(originalContent)) {
      return newContent;
    }
    content=newContent;
  }
}
