{
  while (true) {
    index++;
    if (index >= jspLines.size()) {
      return null;
    }
    JSPLine jspLine=jspLines.get(index);
    if (!jspLine.isCloseTag()) {
      continue;
    }
    String openTagLine=openTagJSPLine.getLine();
    if (openTagLine.matches("\t*<%!?")) {
      String line=jspLine.getLine();
      if (line.matches("\t*%>")) {
        return jspLine;
      }
      continue;
    }
    if (jspLine.isCloseTag() && (openTagJSPLine.getTabLevel() == (jspLine.getLineTabLevel() + jspLine.getTabLevel()))) {
      return jspLine;
    }
  }
}
