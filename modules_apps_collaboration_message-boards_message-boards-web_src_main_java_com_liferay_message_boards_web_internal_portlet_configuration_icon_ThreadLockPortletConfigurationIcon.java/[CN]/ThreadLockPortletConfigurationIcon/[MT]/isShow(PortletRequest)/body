{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    MBMessage message=ActionUtil.getMessage(portletRequest);
    return MBCategoryPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),message.getCategoryId(),ActionKeys.LOCK_THREAD);
  }
 catch (  Exception e) {
  }
  return false;
}
