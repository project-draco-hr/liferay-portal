{
  if (attributeName.equals(JavaConstants.JAVAX_SERVLET_INCLUDE_SERVLET_PATH)) {
    return StringPool.BLANK;
  }
 else   if (attributeName.equals(JavaConstants.JAVAX_SERVLET_INCLUDE_PATH_INFO)) {
    String includeServletPath=(String)super.getAttribute(JavaConstants.JAVAX_SERVLET_INCLUDE_SERVLET_PATH);
    if (includeServletPath.contains(_servletPath)) {
      includeServletPath=includeServletPath.substring(_servletPath.length());
    }
    return includeServletPath;
  }
  return super.getAttribute(attributeName);
}
