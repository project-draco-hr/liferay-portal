{
  WSRPConsumer wsrpConsumer=WSRPConsumerLocalServiceUtil.getWSRPConsumer(wsrpConsumerPortlet.getWsrpConsumerId());
  StagedModelDataHandlerUtil.exportReferenceStagedModel(portletDataContext,wsrpConsumerPortlet,wsrpConsumer,PortletDataContext.REFERENCE_TYPE_STRONG);
  Element wsrpConsumerPortletElement=portletDataContext.getExportDataElement(wsrpConsumerPortlet);
  portletDataContext.addClassedModel(wsrpConsumerPortletElement,ExportImportPathUtil.getModelPath(wsrpConsumerPortlet),wsrpConsumerPortlet);
}
