{
  String[] excludes=JSONIncludesManagerUtil.lookupExcludes(ExtendsTwo.class);
  Assert.assertEquals(1,excludes.length);
  Assert.assertEquals("*",excludes[0]);
  String[] includes=JSONIncludesManagerUtil.lookupIncludes(ExtendsTwo.class);
  Assert.assertEquals(1,includes.length);
  Assert.assertEquals("ftwo",includes[0]);
}
