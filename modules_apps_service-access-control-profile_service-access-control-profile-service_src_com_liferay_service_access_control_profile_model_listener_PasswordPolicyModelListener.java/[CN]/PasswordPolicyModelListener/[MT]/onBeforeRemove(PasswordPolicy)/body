{
  if (!passwordPolicy.isDefaultPolicy()) {
    return;
  }
  try {
    for (    SACPEntry sacpEntry : _sacpEntryLocalService.getCompanySACPEntries(passwordPolicy.getCompanyId(),QueryUtil.ALL_POS,QueryUtil.ALL_POS)) {
      _sacpEntryLocalService.deleteSACPEntry(sacpEntry);
    }
  }
 catch (  PortalException pe) {
    throw new ModelListenerException(pe);
  }
}
