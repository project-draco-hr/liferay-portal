{
  final Statement statement=super.methodBlock(frameworkMethod);
  if (!TestPropsValues.ASSERT_LOGS) {
    return statement;
  }
  return new Statement(){
    @Override public void evaluate() throws Throwable {
      ConcurrentAssertUtil.startAssert();
      CaptureAppender captureAppender=ExpectedLogsUtil.startAssert(frameworkMethod.getMethod());
      try {
        LogAssertionUtil.enableLogAssertion();
        statement.evaluate();
      }
  finally {
        ExpectedLogsUtil.endAssert(frameworkMethod.getMethod(),captureAppender);
        ConcurrentAssertUtil.endAssert();
      }
    }
  }
;
}
