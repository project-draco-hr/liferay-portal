{
  Object value=super.getAttribute(name);
  if (name == null) {
    return value;
  }
  if (!ServerDetector.isWebSphere()) {
    return value;
  }
  if (name.equals(JavaConstants.JAVAX_SERVLET_INCLUDE_CONTEXT_PATH)) {
    value=_portletReq.getContextPath();
  }
 else   if (name.equals(JavaConstants.JAVAX_SERVLET_INCLUDE_PATH_INFO)) {
    value=_pathInfo;
  }
 else   if (name.equals(JavaConstants.JAVAX_SERVLET_INCLUDE_QUERY_STRING)) {
    value=_queryString;
  }
 else   if (name.equals(JavaConstants.JAVAX_SERVLET_INCLUDE_REQUEST_URI)) {
    value=_requestURI;
  }
 else   if (name.equals(JavaConstants.JAVAX_SERVLET_INCLUDE_SERVLET_PATH)) {
    value=_servletPath;
  }
  if ((name.startsWith(JavaConstants.JAVAX_SERVLET_INCLUDE)) && (value == null)) {
    value=StringPool.BLANK;
  }
  return value;
}
