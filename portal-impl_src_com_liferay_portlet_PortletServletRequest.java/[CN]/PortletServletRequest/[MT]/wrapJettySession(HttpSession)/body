{
  ClassLoader classLoader=ClassLoaderUtil.getPortalClassLoader();
  Class<?> jettyHttpSessionWrapperClass=classLoader.loadClass("com.liferay.portal.servlet.JettyHttpSessionWrapper");
  Constructor<?> constructor=jettyHttpSessionWrapperClass.getConstructor(new Class[]{HttpSession.class});
  return (HttpSession)constructor.newInstance(new Object[]{session});
}
