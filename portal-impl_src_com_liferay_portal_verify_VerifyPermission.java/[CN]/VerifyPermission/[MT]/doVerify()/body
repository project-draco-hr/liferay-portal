{
  StopWatch stopWatch=StopWatchLoggingHelper.startLogging(_log,"VerifyPermission.deleteDefaultPrivateLayoutPermissions");
  deleteDefaultPrivateLayoutPermissions();
  StopWatchLoggingHelper.endLogging(stopWatch,_log,"VerifyPermission.deleteDefaultPrivateLayoutPermissions");
  stopWatch=StopWatchLoggingHelper.startLogging(_log,"VerifyPermission.checkPermissions");
  checkPermissions();
  StopWatchLoggingHelper.endLogging(stopWatch,_log,"VerifyPermission.checkPermissions");
  stopWatch=StopWatchLoggingHelper.startLogging(_log,"VerifyPermission.fixOrganizationRolePermissions");
  fixOrganizationRolePermissions();
  StopWatchLoggingHelper.endLogging(stopWatch,_log,"VerifyPermission.fixOrganizationRolePermissions");
  stopWatch=StopWatchLoggingHelper.startLogging(_log,"VerifyPermission.fixUserDefaultRolePermissions");
  fixUserDefaultRolePermissions();
  StopWatchLoggingHelper.endLogging(stopWatch,_log,"VerifyPermission.fixUserDefaultRolePermissions");
}
