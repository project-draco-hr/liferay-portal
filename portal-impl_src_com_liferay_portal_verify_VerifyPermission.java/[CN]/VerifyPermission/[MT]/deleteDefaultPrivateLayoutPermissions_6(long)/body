{
  Role defaultRole=RoleLocalServiceUtil.getRole(companyId,RoleConstants.GUEST);
  List<ResourcePermission> resourcePermissions=ResourcePermissionLocalServiceUtil.getRoleResourcePermissions(defaultRole.getRoleId());
  for (  ResourcePermission resourcePermission : resourcePermissions) {
    if (isPrivateLayout(resourcePermission.getName(),resourcePermission.getPrimKey())) {
      ResourcePermissionLocalServiceUtil.deleteResourcePermission(resourcePermission.getResourcePermissionId());
    }
  }
}
