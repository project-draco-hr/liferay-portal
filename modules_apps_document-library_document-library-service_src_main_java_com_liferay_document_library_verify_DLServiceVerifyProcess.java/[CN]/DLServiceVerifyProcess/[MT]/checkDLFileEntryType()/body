{
  DLFileEntryType dlFileEntryType=_dlFileEntryTypeLocalService.fetchDLFileEntryType(DLFileEntryTypeConstants.FILE_ENTRY_TYPE_ID_BASIC_DOCUMENT);
  if (dlFileEntryType != null) {
    return;
  }
  dlFileEntryType=_dlFileEntryTypeLocalService.createDLFileEntryType(DLFileEntryTypeConstants.FILE_ENTRY_TYPE_ID_BASIC_DOCUMENT);
  dlFileEntryType.setFileEntryTypeKey(StringUtil.toUpperCase(DLFileEntryTypeConstants.NAME_BASIC_DOCUMENT));
  dlFileEntryType.setName(DLFileEntryTypeConstants.NAME_BASIC_DOCUMENT,LocaleUtil.getDefault());
  _dlFileEntryTypeLocalService.updateDLFileEntryType(dlFileEntryType);
}
