{
  super(String.format("User %s must have %s permission for %s %s",Validator.isNull(userId) ? "" : userId,StringUtil.merge(actionIds,","),resourceName,Validator.isNull(resourceId) ? "" : resourceId));
  this.actionId=actionIds;
  this.resourceName=resourceName;
  this.resourceId=resourceId;
  this.userId=userId;
}
