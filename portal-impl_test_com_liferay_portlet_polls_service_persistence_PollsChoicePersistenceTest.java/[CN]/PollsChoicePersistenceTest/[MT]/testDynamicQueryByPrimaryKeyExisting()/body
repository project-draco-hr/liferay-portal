{
  PollsChoice newPollsChoice=addPollsChoice();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PollsChoice.class,PollsChoice.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("choiceId",newPollsChoice.getChoiceId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  PollsChoice existingPollsChoice=(PollsChoice)result.get(0);
  assertEquals(existingPollsChoice,newPollsChoice);
}
