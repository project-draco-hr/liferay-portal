{
  if (!GetterUtil.getBoolean(_props.get(PropsKeys.INDEX_ON_STARTUP)) || !_clusterMasterExecutor.isMaster()) {
    return;
  }
  try {
    _indexWriterHelper.reindex(UserConstants.USER_ID_DEFAULT,SearchConstants.INDEX_ON_ACTIVATE_BACKGROUND_TASK_NAME + "." + _className,new long[]{company.getCompanyId()},_className,null);
  }
 catch (  SearchException se) {
    if (_log.isErrorEnabled()) {
      _log.error("Unable to reindex on activation",se);
    }
  }
}
