{
  List<StagedModel> dependentStagedModels=dependentStagedModelsMap.get(MDRRuleGroup.class.getName());
  MDRRuleGroup ruleGroup=(MDRRuleGroup)dependentStagedModels.get(0);
  return MDRTestUtil.addRuleGroupInstance(group.getGroupId(),Layout.class.getName(),layout.getPlid(),ruleGroup.getRuleGroupId());
}
