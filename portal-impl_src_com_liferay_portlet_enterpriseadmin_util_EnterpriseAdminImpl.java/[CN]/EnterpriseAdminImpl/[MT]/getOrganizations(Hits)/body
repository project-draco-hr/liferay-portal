{
  List<Organization> organizations=new ArrayList<Organization>();
  List<Document> documents=hits.toList();
  boolean indexCorrupt=false;
  for (  Document document : documents) {
    long organizationId=GetterUtil.getLong(document.get(Field.ORGANIZATION_ID));
    try {
      Organization organization=OrganizationLocalServiceUtil.getOrganization(organizationId);
      organizations.add(organization);
    }
 catch (    NoSuchOrganizationException nsoe) {
      indexCorrupt=true;
      _log.error("Organization " + organizationId + " does not exist in "+ "the search index");
    }
  }
  return new Tuple(organizations,indexCorrupt);
}
