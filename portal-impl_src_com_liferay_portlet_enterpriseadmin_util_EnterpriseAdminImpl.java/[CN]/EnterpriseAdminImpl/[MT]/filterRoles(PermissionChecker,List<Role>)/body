{
  List<Role> filteredRoles=ListUtil.copy(roles);
  Iterator<Role> itr=filteredRoles.iterator();
  while (itr.hasNext()) {
    Role role=itr.next();
    String name=role.getName();
    if (name.equals(RoleConstants.SITE_MEMBER) || name.equals(RoleConstants.GUEST) || name.equals(RoleConstants.OWNER)|| name.equals(RoleConstants.ORGANIZATION_MEMBER)|| name.equals(RoleConstants.USER)) {
      itr.remove();
    }
  }
  if (permissionChecker.isCompanyAdmin()) {
    return filteredRoles;
  }
  itr=filteredRoles.iterator();
  while (itr.hasNext()) {
    Role role=itr.next();
    if (!RolePermissionUtil.contains(permissionChecker,role.getRoleId(),ActionKeys.ASSIGN_MEMBERS)) {
      itr.remove();
    }
  }
  return filteredRoles;
}
