{
  long trashEntryId=ParamUtil.getLong(actionRequest,"trashEntryId");
  if (trashEntryId > 0) {
    deleteTrashEntry(trashEntryId);
  }
 else {
    long[] deleteTrashEntryIds=StringUtil.split(ParamUtil.getString(actionRequest,"deleteEntryIds"),0L);
    for (int i=0; i < deleteTrashEntryIds.length; i++) {
      deleteTrashEntry(deleteTrashEntryIds[i]);
    }
  }
}
