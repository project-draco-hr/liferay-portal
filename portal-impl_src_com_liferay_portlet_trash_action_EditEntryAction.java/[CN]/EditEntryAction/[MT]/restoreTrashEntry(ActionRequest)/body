{
  long trashEntryId=ParamUtil.getLong(actionRequest,"trashEntryId");
  TrashEntry trashEntry=TrashEntryLocalServiceUtil.getTrashEntry(trashEntryId);
  TrashHandler trashHandler=TrashHandlerRegistryUtil.getTrashHandler(trashEntry.getClassName());
  trashHandler.restoreTrashEntry(trashEntry.getClassPK());
}
