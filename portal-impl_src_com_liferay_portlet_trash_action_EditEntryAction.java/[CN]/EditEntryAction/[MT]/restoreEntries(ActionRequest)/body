{
  long trashEntryId=ParamUtil.getLong(actionRequest,"trashEntryId");
  List<ObjectValuePair<String,Long>> entries=new ArrayList<ObjectValuePair<String,Long>>();
  if (trashEntryId > 0) {
    entries=restoreEntry(trashEntryId);
  }
 else {
    long[] restoreEntryIds=StringUtil.split(ParamUtil.getString(actionRequest,"restoreTrashEntryIds"),0L);
    for (int i=0; i < restoreEntryIds.length; i++) {
      entries.addAll(restoreEntry(restoreEntryIds[i]));
    }
  }
  return entries;
}
