{
  if (_delta != null) {
    return _delta;
  }
  _delta=GetterUtil.getInteger(_portletPreferences.getValue("delta",null),SearchContainer.DEFAULT_DELTA);
  String portletName=getPortletName();
  if (portletName.equals(AssetPublisherPortletKeys.RECENT_CONTENT)) {
    _delta=PropsValues.RECENT_CONTENT_MAX_DISPLAY_ITEMS;
  }
  return _delta;
}
