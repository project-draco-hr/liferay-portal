{
  final Project project=getProject();
  project.javaexec(new Closure<Void>(null){
    @SuppressWarnings("unused") public void doCall(    JavaExecSpec javaExecSpec){
      javaExecSpec.setClasspath(getClasspath());
      javaExecSpec.setMain("com.liferay.xml.formatter.XMLFormatter");
      javaExecSpec.setSystemProperties(getSystemProperties(file));
      javaExecSpec.setWorkingDir(project.getProjectDir());
    }
  }
);
}
