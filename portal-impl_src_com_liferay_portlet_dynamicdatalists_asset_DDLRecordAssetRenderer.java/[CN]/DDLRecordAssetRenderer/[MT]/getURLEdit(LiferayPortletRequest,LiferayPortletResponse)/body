{
  HttpServletRequest request=liferayPortletRequest.getHttpServletRequest();
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  PortletURL portletURL=PortletURLFactoryUtil.create(request,PortletKeys.DYNAMIC_DATA_LISTS,getControlPanelPlid(themeDisplay),PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/dynamic_data_lists/edit_record");
  portletURL.setParameter("recordId",String.valueOf(_record.getRecordId()));
  return portletURL;
}
