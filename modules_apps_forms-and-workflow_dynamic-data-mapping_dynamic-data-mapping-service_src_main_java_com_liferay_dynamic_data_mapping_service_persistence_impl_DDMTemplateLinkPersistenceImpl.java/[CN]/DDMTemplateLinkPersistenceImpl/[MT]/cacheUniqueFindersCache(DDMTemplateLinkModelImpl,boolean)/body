{
  if (isNew) {
    Object[] args=new Object[]{ddmTemplateLinkModelImpl.getClassNameId(),ddmTemplateLinkModelImpl.getClassPK()};
    finderCache.putResult(FINDER_PATH_COUNT_BY_C_C,args,Long.valueOf(1));
    finderCache.putResult(FINDER_PATH_FETCH_BY_C_C,args,ddmTemplateLinkModelImpl);
  }
 else {
    if ((ddmTemplateLinkModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_C_C.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{ddmTemplateLinkModelImpl.getClassNameId(),ddmTemplateLinkModelImpl.getClassPK()};
      finderCache.putResult(FINDER_PATH_COUNT_BY_C_C,args,Long.valueOf(1));
      finderCache.putResult(FINDER_PATH_FETCH_BY_C_C,args,ddmTemplateLinkModelImpl);
    }
  }
}
