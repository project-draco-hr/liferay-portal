{
  Boolean hasPermission=StagingPermissionUtil.hasPermission(permissionChecker,documentType.getGroupId(),DLDocumentType.class.getName(),documentType.getDocumentTypeId(),PortletKeys.DOCUMENT_LIBRARY,actionId);
  if (hasPermission != null) {
    return hasPermission.booleanValue();
  }
  if (permissionChecker.hasOwnerPermission(documentType.getCompanyId(),DLDocumentType.class.getName(),documentType.getDocumentTypeId(),documentType.getUserId(),actionId)) {
    return true;
  }
  return permissionChecker.hasPermission(documentType.getGroupId(),DLDocumentType.class.getName(),documentType.getDocumentTypeId(),actionId);
}
