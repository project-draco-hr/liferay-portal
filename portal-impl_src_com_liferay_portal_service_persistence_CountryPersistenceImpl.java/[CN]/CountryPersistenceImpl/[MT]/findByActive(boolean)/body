{
  Object[] finderArgs=new Object[]{Boolean.valueOf(active)};
  Object result=FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_ACTIVE,finderArgs,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portal.model.Country WHERE ");
      query.append("active_ = ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("name ASC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(active);
      List<Country> list=q.list();
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_ACTIVE,finderArgs,list);
      return list;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (List<Country>)result;
  }
}
