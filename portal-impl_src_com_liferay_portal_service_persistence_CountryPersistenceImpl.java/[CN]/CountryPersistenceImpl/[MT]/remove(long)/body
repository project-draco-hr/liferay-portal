{
  Session session=null;
  try {
    session=openSession();
    Country country=(Country)session.get(CountryImpl.class,new Long(countryId));
    if (country == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + countryId);
      }
      throw new NoSuchCountryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + countryId);
    }
    return remove(country);
  }
 catch (  NoSuchCountryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
