{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,country,merge);
    country.setNew(false);
    return country;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Country.class.getName());
  }
}
