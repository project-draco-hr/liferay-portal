{
  Session session=null;
  try {
    session=openSession();
    country=(Country)session.merge(country);
    session.delete(country);
    session.flush();
    return country;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Country.class.getName());
  }
}
