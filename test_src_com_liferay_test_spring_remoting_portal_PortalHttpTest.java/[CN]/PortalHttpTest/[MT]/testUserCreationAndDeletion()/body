{
  UserService userService=getUserService();
  User user=userService.addUser(TestConstants.COMPANY_ID,autoPassword,password1,password2,passwordReset,autoScreenName,screenName,emailAddress,locale,firstName,middleName,lastName,nickName,prefixId,suffixId,male,birthdayMonth,birthdayDay,birthdayYear,jobTitle,organizationId,locationId,false);
  user=userService.getUserByEmailAddress(TestConstants.COMPANY_ID,emailAddress);
  userService.deleteUser(user.getUserId());
}
