{
  if (SessionErrors.contains(renderRequest,NoSuchGroupException.class.getName()) || SessionErrors.contains(renderRequest,NoSuchBackgroundTaskException.class.getName()) || SessionErrors.contains(renderRequest,PrincipalException.class.getName())) {
    include("/error.jsp",renderRequest,renderResponse);
  }
 else {
    super.doDispatch(renderRequest,renderResponse);
  }
}
