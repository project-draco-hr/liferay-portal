{
  UserGroupGroupRole userGroupGroupRole=fetchByPrimaryKey(userGroupGroupRolePK);
  if (userGroupGroupRole == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + userGroupGroupRolePK);
    }
    throw new NoSuchUserGroupGroupRoleException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + userGroupGroupRolePK);
  }
  return userGroupGroupRole;
}
