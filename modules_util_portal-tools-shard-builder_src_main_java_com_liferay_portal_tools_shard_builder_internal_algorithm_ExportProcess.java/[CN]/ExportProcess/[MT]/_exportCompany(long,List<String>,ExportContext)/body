{
  String outputFileName=exportContext.getSchema() + "-" + companyId+ ".sql";
  File outputFile=new File(exportContext.getOutputFolder(),outputFileName);
  OutputStream outputStream=new BufferedOutputStream(new FileOutputStream(outputFile));
  for (  String tableName : tables) {
    _dbProvider.write(tableName,outputStream);
  }
  outputStream.flush();
  outputStream.close();
}
