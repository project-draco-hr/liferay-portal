{
  List<String> partitionedTables=_dbProvider.getPartitionedTables(exportContext.getSchema());
  List<String> controlTables=_dbProvider.getControlTables(exportContext.getSchema());
  List<Long> companyIds=exportContext.getCompanyIds();
  for (  Long companyId : companyIds) {
    _exportCompany(companyId,partitionedTables,exportContext);
    _exportCompany(companyId,controlTables,exportContext);
  }
}
