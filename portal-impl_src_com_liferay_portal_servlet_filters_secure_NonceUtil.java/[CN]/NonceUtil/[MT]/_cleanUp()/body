{
  long expired=System.currentTimeMillis() - _NONCE_EXPIRATION;
  List<Long> times=new ArrayList<Long>();
synchronized (_nonceMap) {
    for (    long time : _nonceMap.keySet()) {
      if (time <= expired) {
        times.add(time);
      }
 else {
        break;
      }
    }
  }
  for (  long time : times) {
    _nonceMap.remove(time);
  }
}
