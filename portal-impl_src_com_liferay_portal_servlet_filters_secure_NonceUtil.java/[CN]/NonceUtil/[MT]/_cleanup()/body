{
  long expired=System.currentTimeMillis() - _NONCE_EXPIRATION;
  List<Long> expiredTimes=new ArrayList<Long>();
  for (  long time : _cache.keySet()) {
    if (time <= expired) {
      expiredTimes.add(time);
    }
 else {
      break;
    }
  }
  for (  long time : expiredTimes) {
    _cache.remove(time);
  }
}
