{
  PropsValues.SPRING_HIBERNATE_SESSION_DELEGATED=false;
  try {
    Field annotationsField=ReflectionUtil.getDeclaredField(ServiceBeanAopCacheManager.class,"_annotations");
    annotationsField.set(null,new HashMap<MethodInvocation,Annotation[]>(){
      @Override public Annotation[] get(      Object key){
        return _mockAnnotations;
      }
      private final Annotation[] _mockAnnotations=new Annotation[]{new Skip(){
        public Class<? extends Annotation> annotationType(){
          return Skip.class;
        }
      }
};
    }
);
  }
 catch (  Exception e) {
    throw new VerifyException(e);
  }
}
