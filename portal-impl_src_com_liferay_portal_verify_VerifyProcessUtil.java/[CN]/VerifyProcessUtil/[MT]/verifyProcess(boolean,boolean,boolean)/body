{
  int verifyFrequency=GetterUtil.getInteger(PropsUtil.get(PropsKeys.VERIFY_FREQUENCY));
  if ((verifyFrequency == VerifyProcess.ALWAYS) || ((verifyFrequency == VerifyProcess.ONCE) && !verified) || ranUpgradeProcess|| versionChanged) {
    return _verifyProcess(ranUpgradeProcess);
  }
  return false;
}
