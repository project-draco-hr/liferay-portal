{
  if (_cacheListenersMap.containsKey(cacheListener)) {
    return;
  }
  CacheEventListener cacheEventListener=new WrappedCacheEventListener(cacheListener,this);
  _cacheListenersMap.put(cacheListener,cacheEventListener);
  NotificationScope notificationScope=convertCacheListenerScope(cacheListenerScope);
  RegisteredEventListeners registeredEventListeners=_cache.getCacheEventNotificationService();
  registeredEventListeners.registerListener(cacheEventListener,notificationScope);
}
