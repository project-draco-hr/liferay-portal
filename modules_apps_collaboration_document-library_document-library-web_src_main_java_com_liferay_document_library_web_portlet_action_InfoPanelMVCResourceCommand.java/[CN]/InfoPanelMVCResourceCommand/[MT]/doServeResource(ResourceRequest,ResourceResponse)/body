{
  resourceRequest.setAttribute(WebKeys.DOCUMENT_LIBRARY_FILE_ENTRIES,ActionUtil.getFileEntries(resourceRequest));
  resourceRequest.setAttribute(WebKeys.DOCUMENT_LIBRARY_FILE_SHORTCUTS,ActionUtil.getFileShortcuts(resourceRequest));
  resourceRequest.setAttribute(WebKeys.DOCUMENT_LIBRARY_FOLDERS,ActionUtil.getFolders(resourceRequest));
  include(resourceRequest,resourceResponse,"/document_library/info_panel.jsp");
}
