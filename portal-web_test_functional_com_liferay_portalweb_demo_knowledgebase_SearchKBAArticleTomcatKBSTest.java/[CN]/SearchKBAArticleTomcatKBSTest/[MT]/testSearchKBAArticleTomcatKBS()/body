{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Knowledge Base Search Test Page");
  selenium.clickAt("link=Knowledge Base Search Test Page",RuntimeVariables.replace("Knowledge Base Search Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_5_WAR_knowledgebaseportlet_keywords']",RuntimeVariables.replace("tomcat"));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace("Search"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("The third"),selenium.getText("//td[1]/a"));
  assertEquals(RuntimeVariables.replace("Joe Bloggs"),selenium.getText("//td[2]/a"));
}
