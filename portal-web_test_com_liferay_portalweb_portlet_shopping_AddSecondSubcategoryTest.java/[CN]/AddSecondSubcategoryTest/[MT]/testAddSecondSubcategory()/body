{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Shopping Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//td[1]/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Subcategory']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_34_name",RuntimeVariables.replace("T\u00e9st Subcat\u00e9gory"));
  selenium.type("_34_description",RuntimeVariables.replace("This is a t\u00e9st subcat\u00e9gory!"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertTrue(selenium.isTextPresent("T\u00e9st Subcat\u00e9gory"));
  assertTrue(selenium.isTextPresent("This is a t\u00e9st subcat\u00e9gory!"));
}
