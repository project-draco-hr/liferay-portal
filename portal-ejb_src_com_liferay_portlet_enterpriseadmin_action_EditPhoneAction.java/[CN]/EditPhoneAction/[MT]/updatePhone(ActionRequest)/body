{
  long phoneId=ParamUtil.getLong(req,"phoneId");
  String className=ParamUtil.getString(req,"className");
  String classPK=ParamUtil.getString(req,"classPK");
  String number=ParamUtil.getString(req,"number");
  String extension=ParamUtil.getString(req,"extension");
  String typeId=ParamUtil.getString(req,"typeId");
  boolean primary=ParamUtil.getBoolean(req,"primary");
  if (phoneId <= 0) {
    PhoneServiceUtil.addPhone(className,classPK,number,extension,typeId,primary);
  }
 else {
    PhoneServiceUtil.updatePhone(phoneId,number,extension,typeId,primary);
  }
}
