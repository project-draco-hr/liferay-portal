{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ShoppingOrderItem.class,ShoppingOrderItem.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("orderItemId",ServiceTestUtil.nextLong()));
  List<ShoppingOrderItem> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
