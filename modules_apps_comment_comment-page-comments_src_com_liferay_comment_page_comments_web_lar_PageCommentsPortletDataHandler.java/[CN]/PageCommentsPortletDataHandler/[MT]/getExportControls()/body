{
  DiscussionStagingHandler discussionStagingHandler=CommentManagerUtil.getDiscussionStagingHandler();
  if (discussionStagingHandler == null) {
    return new PortletDataHandlerControl[0];
  }
  PortletDataHandlerBoolean portletDataHandlerBoolean=new PortletDataHandlerBoolean(NAMESPACE,"comment",true,false,null,discussionStagingHandler.getClassName(),StagedModelType.REFERRER_CLASS_NAME_ANY);
  return new PortletDataHandlerControl[]{portletDataHandlerBoolean};
}
