{
  long leftPrimaryKey=1;
  long rightPrimaryKey=2;
  Assert.assertFalse(_tableMappingImpl.containsMapping(leftPrimaryKey,rightPrimaryKey));
  PortalCache<Long,long[]> leftToRightPortalCache=_tableMappingImpl.leftToRightPortalCache;
  leftToRightPortalCache.remove(leftPrimaryKey);
  _mappingStore.put(leftPrimaryKey,new long[]{rightPrimaryKey});
  Assert.assertTrue(_tableMappingImpl.containsMapping(leftPrimaryKey,rightPrimaryKey));
}
