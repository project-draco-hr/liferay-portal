{
  DB db=DBFactoryUtil.getDB();
  String dbType=db.getType();
  if (!dbType.equals(DB.TYPE_MYSQL)) {
    return;
  }
  try (Connection connection=DataAccess.getUpgradeOptimizedConnection();Statement statement=connection.createStatement()){
    verifyTableEngine(statement);
    verifyDatetimePrecision(connection.getMetaData(),statement);
  }
 }
