{
  if (GetterUtil.getFloat(db.getVersionString()) < 5.6F) {
    return;
  }
  try (ResultSet rs=databaseMetaData.getTables(null,null,null,null)){
    while (rs.next()) {
      verifyDatetimePrecisionForTable(databaseMetaData,statement,rs.getString("TABLE_CAT"),rs.getString("TABLE_SCHEM"),rs.getString("TABLE_NAME"));
    }
  }
 }
