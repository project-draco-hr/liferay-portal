{
  Object[] finderArgs=new Object[]{new Long(groupId)};
  List<BlogsStatsUser> list=(List<BlogsStatsUser>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_GROUPID,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT blogsStatsUser FROM BlogsStatsUser blogsStatsUser WHERE ");
      query.append("blogsStatsUser.groupId = ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("blogsStatsUser.entryCount DESC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<BlogsStatsUser>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_GROUPID,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
