{
  Session session=null;
  try {
    session=openSession();
    session.delete(blogsStatsUser);
    session.flush();
    return blogsStatsUser;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(BlogsStatsUser.class.getName());
  }
}
