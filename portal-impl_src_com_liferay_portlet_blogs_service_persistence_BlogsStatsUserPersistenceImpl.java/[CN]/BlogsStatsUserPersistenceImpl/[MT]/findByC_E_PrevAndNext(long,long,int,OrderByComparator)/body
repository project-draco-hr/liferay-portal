{
  BlogsStatsUser blogsStatsUser=findByPrimaryKey(statsUserId);
  Session session=null;
  try {
    session=openSession();
    BlogsStatsUser[] array=new BlogsStatsUserImpl[3];
    array[0]=getByC_E_PrevAndNext(session,blogsStatsUser,companyId,entryCount,orderByComparator,true);
    array[1]=blogsStatsUser;
    array[2]=getByC_E_PrevAndNext(session,blogsStatsUser,companyId,entryCount,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
