{
  Session session=null;
  try {
    session=openSession();
    BlogsStatsUser blogsStatsUser=(BlogsStatsUser)session.get(BlogsStatsUserImpl.class,new Long(statsUserId));
    if (blogsStatsUser == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No BlogsStatsUser exists with the primary key " + statsUserId);
      }
      throw new NoSuchStatsUserException("No BlogsStatsUser exists with the primary key " + statsUserId);
    }
    return remove(blogsStatsUser);
  }
 catch (  NoSuchStatsUserException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
