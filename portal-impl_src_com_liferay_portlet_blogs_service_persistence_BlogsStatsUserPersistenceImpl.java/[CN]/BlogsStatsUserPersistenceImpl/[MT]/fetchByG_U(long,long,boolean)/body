{
  Object[] finderArgs=new Object[]{new Long(groupId),new Long(userId)};
  Object result=FinderCacheUtil.getResult(FINDER_PATH_FETCH_BY_G_U,finderArgs,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portlet.blogs.model.BlogsStatsUser WHERE ");
      query.append("groupId = ?");
      query.append(" AND ");
      query.append("userId = ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("entryCount DESC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      qPos.add(userId);
      List<BlogsStatsUser> list=q.list();
      BlogsStatsUser blogsStatsUser=null;
      if (list.isEmpty()) {
        if (cacheEmptyResult) {
          FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_U,finderArgs,list);
        }
      }
 else {
        blogsStatsUser=list.get(0);
        cacheResult(blogsStatsUser);
      }
      return blogsStatsUser;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    if (result instanceof List) {
      return null;
    }
 else {
      return (BlogsStatsUser)result;
    }
  }
}
