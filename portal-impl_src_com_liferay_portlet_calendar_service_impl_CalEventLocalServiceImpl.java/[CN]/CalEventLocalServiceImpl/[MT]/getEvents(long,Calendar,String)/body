{
  List<CalEvent> events=getEvents(groupId,cal);
  if (Validator.isNull(type)) {
    return events;
  }
 else {
    events=new ArrayList<CalEvent>(events);
    Iterator<CalEvent> itr=events.iterator();
    while (itr.hasNext()) {
      CalEvent event=itr.next();
      if (!event.getType().equals(type)) {
        itr.remove();
      }
    }
    return events;
  }
}
