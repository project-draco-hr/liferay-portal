{
  calEventPersistence.remove(event);
  resourceLocalService.deleteResource(event.getCompanyId(),CalEvent.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,event.getEventId());
  expandoValueLocalService.deleteValues(CalEvent.class.getName(),event.getEventId());
  socialActivityLocalService.deleteActivities(CalEvent.class.getName(),event.getEventId());
  CalEventLocalUtil.clearEventsPool(event.getGroupId());
}
