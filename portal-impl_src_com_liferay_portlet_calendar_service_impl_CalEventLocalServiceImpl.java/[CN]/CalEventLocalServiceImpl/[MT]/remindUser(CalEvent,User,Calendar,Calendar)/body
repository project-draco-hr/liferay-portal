{
  Date startDate=startCalendar.getTime();
  long startTime=startDate.getTime();
  Date nowDate=nowCalendar.getTime();
  long nowTime=nowDate.getTime();
  if (startTime < nowTime) {
    return;
  }
  long diff=(startTime - nowTime) / _CALENDAR_EVENT_CHECK_INTERVAL;
  if ((diff == (event.getFirstReminder() / _CALENDAR_EVENT_CHECK_INTERVAL)) || (diff == (event.getSecondReminder() / _CALENDAR_EVENT_CHECK_INTERVAL))) {
    remindUser(event,user,startCalendar);
  }
}
