{
  AssetEntry assetEntry=assetEntryLocalService.updateEntry(userId,event.getGroupId(),CalEvent.class.getName(),event.getEventId(),event.getUuid(),0,assetCategoryIds,assetTagNames,true,null,null,null,null,ContentTypes.TEXT_HTML,event.getTitle(),event.getDescription(),null,null,null,0,0,null,false);
  assetLinkLocalService.updateLinks(userId,assetEntry.getEntryId(),assetLinkEntryIds,AssetLinkConstants.TYPE_RELATED);
}
