{
  int count=0;
  List<CalEvent> localEvents=getEvents(groupId,type,QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  List<CalEvent> remoteEvents=getAllRemoteEvents(groupId,type,extCalServerUrl,extCalId,extCalUserId,extCalPassword,extCalSessionId);
  List<CalEvent> events=removeDuplicateEvents(localEvents,remoteEvents);
  if (events != null) {
    count=events.size();
  }
  return count;
}
