{
  DDMDisplay ddmDisplay=DDMUtil.getDDMDisplay(resourceClassNameId);
  DDMPermission.check(getPermissionChecker(),serviceContext.getScopeGroupId(),ddmDisplay.getResourceName(classNameId),ddmDisplay.getAddTemplateActionId());
  return ddmTemplateLocalService.addTemplate(getUserId(),groupId,classNameId,classPK,resourceClassNameId,null,nameMap,descriptionMap,type,mode,language,script,false,false,null,null,serviceContext);
}
