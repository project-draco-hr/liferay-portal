{
  DDMDisplay ddmDisplay=DDMUtil.getDDMDisplay(classNameId);
  DDMPermission.check(getPermissionChecker(),serviceContext.getScopeGroupId(),ddmDisplay.getResourceName(classNameId),ddmDisplay.getAddTemplateActionId());
  return ddmTemplateLocalService.copyTemplates(getUserId(),classNameId,classPK,newClassPK,type,serviceContext);
}
