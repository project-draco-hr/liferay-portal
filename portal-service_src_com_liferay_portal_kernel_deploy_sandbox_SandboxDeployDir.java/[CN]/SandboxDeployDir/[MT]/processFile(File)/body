{
  String fileName=file.getName();
  if (!file.canRead()) {
    _log.error("Unable to read " + fileName);
    return;
  }
  if (!file.canWrite()) {
    _log.error("Unable to write " + fileName);
    return;
  }
  if (_log.isInfoEnabled()) {
    _log.info("Processing " + fileName);
  }
  try {
    for (    SandboxDeployListener sandboxDeployListener : _sandboxDeployListeners) {
      sandboxDeployListener.deploy(file);
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
