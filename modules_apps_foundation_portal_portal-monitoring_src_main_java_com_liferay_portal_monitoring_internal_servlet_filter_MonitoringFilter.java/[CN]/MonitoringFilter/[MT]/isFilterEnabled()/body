{
  if (!super.isFilterEnabled()) {
    return false;
  }
  if (!_monitorPortalRequest && !_portletMonitoringControl.isMonitorPortletActionRequest() && !_portletMonitoringControl.isMonitorPortletEventRequest()&& !_portletMonitoringControl.isMonitorPortletRenderRequest()&& !_portletMonitoringControl.isMonitorPortletResourceRequest()&& !_serviceMonitoringControl.isMonitorServiceRequest()) {
    return false;
  }
  return true;
}
