{
  if (activity.isClassName(DLFileEntry.class.getName())) {
    return DLFileEntryPermission.contains(permissionChecker,activity.getClassPK(),actionId);
  }
 else   if (activity.isClassName(DLFolder.class.getName())) {
    return DLFolderPermission.contains(permissionChecker,activity.getGroupId(),activity.getClassPK(),actionId);
  }
  return false;
}
