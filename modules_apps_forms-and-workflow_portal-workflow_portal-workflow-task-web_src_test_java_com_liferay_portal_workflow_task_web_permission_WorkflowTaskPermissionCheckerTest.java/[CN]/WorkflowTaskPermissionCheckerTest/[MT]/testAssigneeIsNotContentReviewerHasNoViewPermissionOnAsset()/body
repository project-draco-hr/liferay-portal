{
  PermissionChecker permissionChecker=mockPermissionChecker(false,false,false);
  long[] roleIds=permissionChecker.getRoleIds(RandomTestUtil.randomLong(),RandomTestUtil.randomLong());
  WorkflowTask workflowTask=mockWorkflowTask(Role.class.getName(),roleIds[0]);
  mockWorkflowHandler(false);
  Assert.assertFalse(_workflowTaskPermissionChecker.hasPermission(RandomTestUtil.randomLong(),workflowTask,permissionChecker));
}
