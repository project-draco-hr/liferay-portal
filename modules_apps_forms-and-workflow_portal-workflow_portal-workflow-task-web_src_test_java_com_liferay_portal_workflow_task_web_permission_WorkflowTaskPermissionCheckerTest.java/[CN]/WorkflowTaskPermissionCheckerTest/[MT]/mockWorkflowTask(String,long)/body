{
  WorkflowTaskAssignee workflowTaskAssignee=mockWorkflowTaskAssignee(assigneeClassName,assigneeClassPK);
  WorkflowTask workflowTask=mock(WorkflowTask.class);
  ArrayList<WorkflowTaskAssignee> assignees=new ArrayList<>();
  assignees.add(workflowTaskAssignee);
  when(workflowTask.getWorkflowTaskAssignees()).thenReturn(assignees);
  return workflowTask;
}
