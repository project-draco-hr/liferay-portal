{
  WorkflowTaskAssignee workflowTaskAssignee=mockWorkflowTaskAssignee(assigneeClassName,assigneeClassPK);
  WorkflowTask workflowTask=mock(WorkflowTask.class);
  ArrayList<WorkflowTaskAssignee> workflowTaskAssignees=new ArrayList<>();
  workflowTaskAssignees.add(workflowTaskAssignee);
  when(workflowTask.getWorkflowTaskAssignees()).thenReturn(workflowTaskAssignees);
  return workflowTask;
}
