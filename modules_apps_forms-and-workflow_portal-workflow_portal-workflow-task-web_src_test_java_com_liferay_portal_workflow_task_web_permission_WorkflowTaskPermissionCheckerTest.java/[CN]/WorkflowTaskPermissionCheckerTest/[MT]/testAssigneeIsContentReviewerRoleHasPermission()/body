{
  PermissionChecker permissionChecker=mockContentReviewerPermissionChecker();
  long[] roleIds=permissionChecker.getRoleIds(RandomTestUtil.randomLong(),RandomTestUtil.randomLong());
  WorkflowTask workflowTask=mockWorkflowTask(Role.class.getName(),roleIds[0]);
  Assert.assertTrue(_workflowTaskPermissionChecker.hasPermission(RandomTestUtil.randomLong(),workflowTask,permissionChecker));
}
