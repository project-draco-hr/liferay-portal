{
  long assigneeRoleId=RandomTestUtil.randomLong();
  WorkflowTask workflowTask=mockWorkflowTask(Role.class.getName(),assigneeRoleId);
  PermissionChecker permissionChecker=mockContentReviewerPermissionChecker(RandomTestUtil.randomLong());
  Assert.assertFalse(_workflowTaskPermissionChecker.hasPermission(assigneeRoleId,workflowTask,permissionChecker));
}
