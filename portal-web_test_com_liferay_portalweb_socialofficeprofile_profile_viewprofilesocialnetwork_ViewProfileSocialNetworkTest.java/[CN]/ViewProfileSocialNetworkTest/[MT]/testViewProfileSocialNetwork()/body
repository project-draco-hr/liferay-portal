{
  selenium.open("/web/socialoffice01/profile");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//li[contains(@class, 'selected')]/a/span")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Profile"),selenium.getText("//li[contains(@class, 'selected')]/a/span"));
  assertEquals(RuntimeVariables.replace("Social01 Office01 User01"),selenium.getText("//div[@class='lfr-contact-name']/a"));
  assertEquals(RuntimeVariables.replace("socialoffice01@liferay.com"),selenium.getText("//div[@class='lfr-contact-extra']"));
  assertTrue(selenium.isVisible("//div[@class='section lfr-user-social-network']/h3"));
  assertEquals(RuntimeVariables.replace("Social Network:"),selenium.getText("//div[@class='section lfr-user-social-network']/h3"));
  assertEquals(RuntimeVariables.replace("Facebook"),selenium.getText("//div[@class='section lfr-user-social-network']/ul/li/span"));
  assertEquals(RuntimeVariables.replace("socialoffice01"),selenium.getText("//div[@class='section lfr-user-social-network']/ul/li/span[2]"));
  assertEquals(RuntimeVariables.replace("MySpace"),selenium.getText("//div[@class='section lfr-user-social-network']/ul/li[2]/span"));
  assertEquals(RuntimeVariables.replace("socialoffice01"),selenium.getText("//div[@class='section lfr-user-social-network']/ul/li[2]/span[2]"));
  assertEquals(RuntimeVariables.replace("Twitter"),selenium.getText("//div[@class='section lfr-user-social-network']/ul/li[3]/span"));
  assertEquals(RuntimeVariables.replace("socialoffice01"),selenium.getText("//div[@class='section lfr-user-social-network']/ul/li[3]/span[2]"));
}
