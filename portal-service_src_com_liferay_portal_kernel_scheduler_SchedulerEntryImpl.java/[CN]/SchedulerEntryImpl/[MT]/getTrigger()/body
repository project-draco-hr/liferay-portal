{
  if (_trigger != null) {
    return _trigger;
  }
  if (_triggerType.equals(TriggerType.CRON)) {
    _trigger=new CronTrigger(_eventListenerClass,_eventListenerClass,_triggerValue);
  }
 else   if (_triggerType.equals(TriggerType.SIMPLE)) {
    int interval=GetterUtil.getInteger(_triggerValue);
    _trigger=new IntervalTrigger(_eventListenerClass,_eventListenerClass,interval,_timeUnit);
  }
 else {
    throw new SchedulerException("Unsupport trigger type " + _triggerType);
  }
  return _trigger;
}
