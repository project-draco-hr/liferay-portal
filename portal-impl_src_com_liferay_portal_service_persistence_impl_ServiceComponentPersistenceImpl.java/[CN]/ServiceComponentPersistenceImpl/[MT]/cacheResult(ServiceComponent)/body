{
  entityCache.putResult(ServiceComponentModelImpl.ENTITY_CACHE_ENABLED,ServiceComponentImpl.class,serviceComponent.getPrimaryKey(),serviceComponent);
  finderCache.putResult(FINDER_PATH_FETCH_BY_BNS_BNU,new Object[]{serviceComponent.getBuildNamespace(),serviceComponent.getBuildNumber()},serviceComponent);
  serviceComponent.resetOriginalValues();
}
