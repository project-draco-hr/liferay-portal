{
  String[] allowedContentElementAttributesArray=StringUtil.split(allowedContent,StringPool.SEMICOLON);
  for (  String allowedContentElementAttributes : allowedContentElementAttributesArray) {
    int x=allowedContentElementAttributes.indexOf(StringPool.OPEN_BRACKET);
    int y=allowedContentElementAttributes.indexOf(StringPool.CLOSE_BRACKET);
    String allowedContentElement=allowedContentElementAttributes;
    String[] allowedContentAttributes=new String[0];
    if ((x != -1) && (y != -1)) {
      allowedContentElement=allowedContentElementAttributes.substring(0,x);
      allowedContentAttributes=StringUtil.split(allowedContentElementAttributes.substring(x + 1,y));
    }
    _allowedContentElementAttributes.put(allowedContentElement,allowedContentAttributes);
  }
}
