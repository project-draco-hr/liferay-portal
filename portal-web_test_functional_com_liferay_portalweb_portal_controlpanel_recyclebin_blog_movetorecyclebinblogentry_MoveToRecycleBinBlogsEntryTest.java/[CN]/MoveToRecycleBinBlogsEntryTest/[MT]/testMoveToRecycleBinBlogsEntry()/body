{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Blogs Test Page",RuntimeVariables.replace("Blogs Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForVisible("xPath=(//div[@class='lfr-meta-actions edit-actions entry']/table/tbody/tr/td[3]/span/a)[1]");
  assertEquals(RuntimeVariables.replace("Move to the Recycle Bin"),selenium.getText("xPath=(//div[@class='lfr-meta-actions edit-actions entry']/table/tbody/tr/td[3]/span/a)[1]"));
  selenium.clickAt("xPath=(//div[@class='lfr-meta-actions edit-actions entry']/table/tbody/tr/td[3]/span/a)[1]",RuntimeVariables.replace("Move to the Recycle Bin"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("The selected item was moved to the Recycle Bin. Undo"),selenium.getText("//div[@class='portlet-msg-success taglib-trash-undo']"));
}
