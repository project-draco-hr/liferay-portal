{
  try {
    JSONObject jsonObject=_jsonFactory.createJSONObject(serializedDDMFormValues);
    DDMFormValues ddmFormValues=new DDMFormValues(ddmForm);
    setDDMFormValuesAvailableLocales(jsonObject.getJSONArray("availableLanguageIds"),ddmFormValues);
    setDDMFormValuesDefaultLocale(jsonObject.getString("defaultLanguageId"),ddmFormValues);
    setDDMFormFieldValues(jsonObject.getJSONArray("fieldValues"),ddmFormValues);
    setDDMFormLocalizedValuesDefaultLocale(ddmFormValues);
    return ddmFormValues;
  }
 catch (  JSONException jsone) {
    throw new PortalException(jsone);
  }
}
