{
  for (  MDRRuleGroupInstance mdrRuleGroupInstance : mdrRuleGroupInstances) {
    if (EntityCacheUtil.getResult(MDRRuleGroupInstanceModelImpl.ENTITY_CACHE_ENABLED,MDRRuleGroupInstanceImpl.class,mdrRuleGroupInstance.getPrimaryKey()) == null) {
      cacheResult(mdrRuleGroupInstance);
    }
 else {
      mdrRuleGroupInstance.resetOriginalValues();
    }
  }
}
