{
  MDRRuleGroupInstance mdrRuleGroupInstance=fetchByPrimaryKey(ruleGroupInstanceId);
  if (mdrRuleGroupInstance == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + ruleGroupInstanceId);
    }
    throw new NoSuchRuleGroupInstanceException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + ruleGroupInstanceId);
  }
  return mdrRuleGroupInstance;
}
