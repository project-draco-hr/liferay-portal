  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  MDRRuleGroupInstanceModelImpl mdrRuleGroupInstanceModelImpl=(MDRRuleGroupInstanceModelImpl)mdrRuleGroupInstance;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{mdrRuleGroupInstanceModelImpl.getUuid(),Long.valueOf(mdrRuleGroupInstanceModelImpl.getGroupId())});
