{
  EntityCacheUtil.putResult(MDRRuleGroupInstanceModelImpl.ENTITY_CACHE_ENABLED,MDRRuleGroupInstanceImpl.class,mdrRuleGroupInstance.getPrimaryKey(),mdrRuleGroupInstance);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{mdrRuleGroupInstance.getUuid(),Long.valueOf(mdrRuleGroupInstance.getGroupId())},mdrRuleGroupInstance);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_C_R,new Object[]{Long.valueOf(mdrRuleGroupInstance.getClassNameId()),Long.valueOf(mdrRuleGroupInstance.getClassPK()),Long.valueOf(mdrRuleGroupInstance.getRuleGroupId())},mdrRuleGroupInstance);
  mdrRuleGroupInstance.resetOriginalValues();
}
