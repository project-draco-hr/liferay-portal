{
  Session session=null;
  try {
    session=openSession();
    MDRRuleGroupInstance mdrRuleGroupInstance=(MDRRuleGroupInstance)session.get(MDRRuleGroupInstanceImpl.class,Long.valueOf(ruleGroupInstanceId));
    if (mdrRuleGroupInstance == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + ruleGroupInstanceId);
      }
      throw new NoSuchRuleGroupInstanceException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + ruleGroupInstanceId);
    }
    return mdrRuleGroupInstancePersistence.remove(mdrRuleGroupInstance);
  }
 catch (  NoSuchRuleGroupInstanceException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
