{
  Session session=null;
  try {
    session=openSession();
    SocialActivitySetting socialActivitySetting=(SocialActivitySetting)session.get(SocialActivitySettingImpl.class,primaryKey);
    if (socialActivitySetting == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchActivitySettingException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(socialActivitySetting);
  }
 catch (  NoSuchActivitySettingException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
