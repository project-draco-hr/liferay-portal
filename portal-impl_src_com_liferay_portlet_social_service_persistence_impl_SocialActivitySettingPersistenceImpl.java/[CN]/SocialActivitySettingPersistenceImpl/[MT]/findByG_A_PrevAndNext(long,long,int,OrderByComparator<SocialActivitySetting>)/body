{
  SocialActivitySetting socialActivitySetting=findByPrimaryKey(activitySettingId);
  Session session=null;
  try {
    session=openSession();
    SocialActivitySetting[] array=new SocialActivitySettingImpl[3];
    array[0]=getByG_A_PrevAndNext(session,socialActivitySetting,groupId,activityType,orderByComparator,true);
    array[1]=socialActivitySetting;
    array[2]=getByG_A_PrevAndNext(session,socialActivitySetting,groupId,activityType,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
