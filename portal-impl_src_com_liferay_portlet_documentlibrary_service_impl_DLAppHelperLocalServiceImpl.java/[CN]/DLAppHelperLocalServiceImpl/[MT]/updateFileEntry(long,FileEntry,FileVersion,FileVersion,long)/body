{
  if (!DLAppHelperThreadLocal.isEnabled()) {
    return;
  }
  boolean updateAsset=true;
  if (fileEntry instanceof LiferayFileEntry && fileEntry.getVersion().equals(destinationFileVersion.getVersion())) {
    updateAsset=false;
  }
  if (updateAsset) {
    updateAsset(userId,fileEntry,destinationFileVersion,assetClassPk);
  }
  registerDLProcessorCallback(fileEntry,sourceFileVersion);
  registerDLSyncCallback(DLSyncConstants.EVENT_UPDATE,DLSyncConstants.TYPE_FILE,fileEntry.getFileEntryId());
}
