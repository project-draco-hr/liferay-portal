{
  if (!DLAppHelperThreadLocal.isEnabled()) {
    return;
  }
  registerDLSyncEventCallback(DLSyncConstants.EVENT_DELETE,folder);
  assetEntryLocalService.deleteEntry(DLFolderConstants.getClassName(),folder.getFolderId());
  if (folder.getModel() instanceof DLFolder) {
    trashEntryLocalService.deleteEntry(DLFolderConstants.getClassName(),folder.getFolderId());
  }
}
