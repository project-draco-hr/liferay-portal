{
  if (!DLAppHelperThreadLocal.isEnabled()) {
    return;
  }
  registerDLSyncEventCallback(DLSyncConstants.EVENT_DELETE,folder);
  assetEntryLocalService.deleteEntry(DLFolderConstants.getClassName(),folder.getFolderId());
  if (folder.getModel() instanceof DLFolder) {
    DLFolder dlFolder=(DLFolder)folder.getModel();
    if (dlFolder.isInTrashExplicitly()) {
      trashEntryLocalService.deleteEntry(DLFolderConstants.getClassName(),dlFolder.getFolderId());
    }
 else {
      trashVersionLocalService.deleteTrashVersion(DLFolderConstants.getClassName(),dlFolder.getFolderId());
    }
  }
}
