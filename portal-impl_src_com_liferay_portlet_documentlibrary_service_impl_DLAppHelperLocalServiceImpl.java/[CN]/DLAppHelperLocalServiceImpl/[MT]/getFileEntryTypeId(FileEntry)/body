{
  FileVersion latestFileVersion=null;
  if (fileEntry.getModel() instanceof DLFileEntry) {
    DLFileEntry dlFileEntry=(DLFileEntry)fileEntry.getModel();
    latestFileVersion=new LiferayFileVersion(dlFileEntry.getLatestFileVersion(true));
  }
 else {
    latestFileVersion=fileEntry.getLatestFileVersion();
  }
  LiferayFileEntry liferayFileEntry=(LiferayFileEntry)latestFileVersion.getFileEntry();
  if (liferayFileEntry == null) {
    return 0;
  }
  DLFileEntry dlFileEntry=liferayFileEntry.getDLFileEntry();
  return dlFileEntry.getFileEntryTypeId();
}
