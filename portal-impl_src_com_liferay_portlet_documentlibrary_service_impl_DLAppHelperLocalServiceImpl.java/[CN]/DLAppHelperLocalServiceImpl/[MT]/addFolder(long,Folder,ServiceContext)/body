{
  updateAsset(userId,folder,serviceContext.getAssetCategoryIds(),serviceContext.getAssetTagNames(),serviceContext.getAssetLinkEntryIds());
  if (!isStagingGroup(folder.getGroupId())) {
    dlSyncLocalService.addSync(folder.getFolderId(),folder.getUuid(),folder.getCompanyId(),folder.getRepositoryId(),folder.getParentFolderId(),folder.getName(),folder.getDescription(),DLSyncConstants.TYPE_FOLDER,"-1");
  }
}
