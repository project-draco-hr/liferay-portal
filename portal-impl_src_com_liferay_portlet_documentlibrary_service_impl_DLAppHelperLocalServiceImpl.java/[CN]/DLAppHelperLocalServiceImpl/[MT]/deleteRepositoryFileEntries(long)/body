{
  LocalRepository localRepository=repositoryLocalService.getLocalRepositoryImpl(repositoryId);
  List<FileEntry> fileEntries=localRepository.getRepositoryFileEntries(DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  for (  FileEntry fileEntry : fileEntries) {
    deleteFileEntry(fileEntry);
  }
}
