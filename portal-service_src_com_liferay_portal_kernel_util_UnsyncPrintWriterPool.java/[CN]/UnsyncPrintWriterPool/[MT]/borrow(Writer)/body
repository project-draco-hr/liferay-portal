{
  if (!isEnabled()) {
    return new UnsyncPrintWriter(writer);
  }
  UnsyncPrintWriter unsyncPrintWriter=_unsyncPrintWriterSoftReferencePool.borrowObject(writer);
  List<UnsyncPrintWriter> unsyncPrintWriters=_borrowedUnsyncPrintWritersThreadLocal.get();
  unsyncPrintWriters.add(unsyncPrintWriter);
  return unsyncPrintWriter;
}
