{
  String content="This is a test article";
  JournalArticle journalArticle=JournalTestUtil.addArticle(_group.getGroupId(),"Test Article",content);
  String updatedContent=JournalTestUtil.createLocalizedContent("This is an updated test article",LocaleUtil.getDefault());
  Assert.assertNotNull(JournalTestUtil.updateArticle(journalArticle,journalArticle.getTitle(),updatedContent));
}
