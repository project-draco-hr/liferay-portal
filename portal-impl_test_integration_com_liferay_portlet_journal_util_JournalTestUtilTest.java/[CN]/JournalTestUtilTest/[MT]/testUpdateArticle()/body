{
  JournalArticle article=JournalTestUtil.addArticle(_group.getGroupId(),"Test Article","This is a test article.");
  String localizedContent=JournalTestUtil.createLocalizedContent("This is an updated test article.",LocaleUtil.getDefault());
  Assert.assertNotNull(JournalTestUtil.updateArticle(article,article.getTitle(),localizedContent));
}
