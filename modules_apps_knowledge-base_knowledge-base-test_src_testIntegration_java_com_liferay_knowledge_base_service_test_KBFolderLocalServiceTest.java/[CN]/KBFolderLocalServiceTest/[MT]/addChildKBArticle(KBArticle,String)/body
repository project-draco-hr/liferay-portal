{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group,_user.getUserId());
  return KBArticleLocalServiceUtil.addKBArticle(_user.getUserId(),PortalUtil.getClassNameId(KBArticleConstants.getClassName()),kbArticle.getResourcePrimKey(),title,title,RandomTestUtil.randomString(),RandomTestUtil.randomString(),null,new String[0],new String[0],serviceContext);
}
