{
  KBArticle kbArticle=addKBArticle(KBFolderConstants.DEFAULT_PARENT_FOLDER_ID,RandomTestUtil.randomString());
  updateKBArticle(kbArticle);
  Assert.assertEquals(2,KBFolderLocalServiceUtil.getKBFoldersAndKBArticlesCount(_group.getGroupId(),KBFolderConstants.DEFAULT_PARENT_FOLDER_ID,WorkflowConstants.STATUS_ANY));
}
