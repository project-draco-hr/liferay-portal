{
  KBArticle kbArticle=addKBArticle(KBFolderConstants.DEFAULT_PARENT_FOLDER_ID,RandomTestUtil.randomString());
  List<Object> kbFolderAndKBArticles=KBFolderLocalServiceUtil.getKBFoldersAndKBArticles(_group.getGroupId(),KBFolderConstants.DEFAULT_PARENT_FOLDER_ID,WorkflowConstants.STATUS_ANY,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new KBEntriesTitleComparator<>(false,true));
  KBFolder currentKBFolder=(KBFolder)kbFolderAndKBArticles.get(0);
  KBArticle currentKBArticle=(KBArticle)kbFolderAndKBArticles.get(1);
  Assert.assertEquals(_kbFolder.getKbFolderId(),currentKBFolder.getKbFolderId());
  Assert.assertEquals(kbArticle.getKbArticleId(),currentKBArticle.getKbArticleId());
}
