{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group,_user.getUserId());
  return KBArticleLocalServiceUtil.addKBArticle(_user.getUserId(),PortalUtil.getClassNameId(KBFolderConstants.getClassName()),parentKbFolderId,title,title,RandomTestUtil.randomString(),RandomTestUtil.randomString(),null,new String[0],new String[0],serviceContext);
}
