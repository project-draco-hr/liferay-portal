{
  addKBArticle(KBFolderConstants.DEFAULT_PARENT_FOLDER_ID,RandomTestUtil.randomString());
  List<Object> kbFolderAndArticles=KBFolderLocalServiceUtil.getKBFoldersAndKBArticles(_group.getGroupId(),KBFolderConstants.DEFAULT_PARENT_FOLDER_ID,WorkflowConstants.STATUS_DRAFT,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new KBEntriesTitleComparator<>(false,true));
  KBFolder currentkbFolder=(KBFolder)kbFolderAndArticles.get(0);
  Assert.assertEquals(1,kbFolderAndArticles.size());
  Assert.assertEquals(_kbFolder.getKbFolderId(),currentkbFolder.getKbFolderId());
}
