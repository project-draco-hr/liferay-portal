{
  String strutsAction=ParamUtil.getString(portletRequest,getParameterName(),"-");
  List<PortletToolbarContributor> portletToolbarContributors=_serviceTrackerMap.getService(portletId.concat(StringPool.PERIOD).concat(strutsAction));
  if (ListUtil.isEmpty(portletToolbarContributors)) {
    portletToolbarContributors=_serviceTrackerMap.getService(portletId);
  }
  return portletToolbarContributors;
}
