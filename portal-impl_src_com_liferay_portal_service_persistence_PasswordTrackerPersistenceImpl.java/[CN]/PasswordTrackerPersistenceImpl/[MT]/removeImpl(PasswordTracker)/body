{
  Session session=null;
  try {
    session=openSession();
    session.delete(passwordTracker);
    session.flush();
    return passwordTracker;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(PasswordTracker.class.getName());
  }
}
