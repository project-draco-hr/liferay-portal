{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(passwordTracker);
    }
 else {
      if (passwordTracker.isNew()) {
        session.save(passwordTracker);
      }
 else {
        passwordTracker=(PasswordTracker)session.merge(passwordTracker);
      }
    }
    session.flush();
    passwordTracker.setNew(false);
    return passwordTracker;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(PasswordTracker.class.getName());
  }
}
