{
  PasswordTracker passwordTracker=(PasswordTracker)EntityCacheUtil.getResult(PasswordTrackerModelImpl.ENTITY_CACHE_ENABLED,PasswordTrackerImpl.class,passwordTrackerId,this);
  if (passwordTracker == _nullPasswordTracker) {
    return null;
  }
  if (passwordTracker == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      passwordTracker=(PasswordTracker)session.get(PasswordTrackerImpl.class,Long.valueOf(passwordTrackerId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (!hasException && (passwordTracker == null)) {
        EntityCacheUtil.putResult(PasswordTrackerModelImpl.ENTITY_CACHE_ENABLED,PasswordTrackerImpl.class,passwordTrackerId,_nullPasswordTracker);
      }
 else {
        cacheResult(passwordTracker);
      }
      closeSession(session);
    }
  }
  return passwordTracker;
}
