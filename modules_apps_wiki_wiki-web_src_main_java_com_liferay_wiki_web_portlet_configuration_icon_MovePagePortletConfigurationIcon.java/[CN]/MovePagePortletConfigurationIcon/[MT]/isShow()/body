{
  try {
    if (WikiPagePermissionChecker.contains(themeDisplay.getPermissionChecker(),_page,ActionKeys.UPDATE) || WikiNodePermissionChecker.contains(themeDisplay.getPermissionChecker(),_node.getNodeId(),ActionKeys.ADD_PAGE)) {
      return true;
    }
  }
 catch (  PortalException pe) {
  }
  return false;
}
