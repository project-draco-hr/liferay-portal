{
  EventResponseImpl eventResponseImpl=null;
  if (PropsValues.COMMONS_POOL_ENABLED) {
    eventResponseImpl=(EventResponseImpl)_instance._pool.borrowObject();
  }
 else {
    eventResponseImpl=new EventResponseImpl();
  }
  eventResponseImpl.init(eventRequestImpl,response,portletName,user,layout,windowState,portletMode);
  return eventResponseImpl;
}
