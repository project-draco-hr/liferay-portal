{
  _igRequestHelper=new IGRequestHelper(request);
  _dlPorletInstanceSettingsHelper=new DLPortletInstanceSettingsHelper(_igRequestHelper);
  if (fileShortcut == null) {
    _uiItemsBuilder=new UIItemsBuilder(request,response,fileVersion);
  }
 else {
    _uiItemsBuilder=new UIItemsBuilder(request,response,fileShortcut);
  }
}
