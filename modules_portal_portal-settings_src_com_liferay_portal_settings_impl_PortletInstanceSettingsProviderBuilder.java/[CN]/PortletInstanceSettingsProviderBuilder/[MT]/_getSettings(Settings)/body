{
  Class<?> settingsExtraClass=_settingsDefinition.getSettingsExtraClass();
  Constructor<?> constructor=settingsExtraClass.getConstructor(TypedSettings.class);
  TypedSettings typedSettings=new TypedSettings(settings);
  Object settingsExtraInstance=constructor.newInstance(typedSettings);
  SettingsInvocationHandler<P,C> settingsInvocationHandler=new SettingsInvocationHandler<>(_settingsDefinition.getSettingsClass(),settingsExtraInstance,typedSettings);
  return settingsInvocationHandler.createProxy();
}
