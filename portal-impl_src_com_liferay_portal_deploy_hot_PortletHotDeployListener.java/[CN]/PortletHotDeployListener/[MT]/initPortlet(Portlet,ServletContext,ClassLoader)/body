{
  PortletBag portletBag=PortletBagFactory.create(portlet,servletContext,portletClassLoader);
  if (portletBag == null) {
    return null;
  }
  javax.portlet.Portlet portletInstance=portletBag.getPortletInstance();
  if (ClassUtil.isSubclass(portletInstance.getClass(),StrutsPortlet.class.getName())) {
    _strutsBridges=true;
  }
  return portletBag;
}
