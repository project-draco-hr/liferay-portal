{
  if (((updatedBy == null) && (_updatedBy != null)) || ((updatedBy != null) && (_updatedBy == null)) || ((updatedBy != null) && (_updatedBy != null) && !updatedBy.equals(_updatedBy))) {
    if (!XSS_ALLOW_UPDATEDBY) {
      updatedBy=XSSUtil.strip(updatedBy);
    }
    _updatedBy=updatedBy;
  }
}
