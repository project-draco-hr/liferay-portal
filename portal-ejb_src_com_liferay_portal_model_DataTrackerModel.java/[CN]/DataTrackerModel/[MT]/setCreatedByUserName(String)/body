{
  if (((createdByUserName == null) && (_createdByUserName != null)) || ((createdByUserName != null) && (_createdByUserName == null)) || ((createdByUserName != null) && (_createdByUserName != null) && !createdByUserName.equals(_createdByUserName))) {
    if (!XSS_ALLOW_CREATEDBYUSERNAME) {
      createdByUserName=XSSUtil.strip(createdByUserName);
    }
    _createdByUserName=createdByUserName;
  }
}
