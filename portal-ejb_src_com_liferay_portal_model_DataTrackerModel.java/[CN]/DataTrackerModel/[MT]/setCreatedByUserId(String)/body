{
  if (((createdByUserId == null) && (_createdByUserId != null)) || ((createdByUserId != null) && (_createdByUserId == null)) || ((createdByUserId != null) && (_createdByUserId != null) && !createdByUserId.equals(_createdByUserId))) {
    if (!XSS_ALLOW_CREATEDBYUSERID) {
      createdByUserId=XSSUtil.strip(createdByUserId);
    }
    _createdByUserId=createdByUserId;
    setModified(true);
  }
}
