{
  String organizationId=ParamUtil.getString(req,"organizationId");
  String parentOrganizationId=ParamUtil.get(req,"parentOrganizationId",OrganizationImpl.DEFAULT_PARENT_ORGANIZATION_ID);
  String name=ParamUtil.getString(req,"name");
  int statusId=ParamUtil.getInteger(req,"statusId");
  String regionId=ParamUtil.getString(req,"regionId");
  String countryId=ParamUtil.getString(req,"countryId");
  Organization organization=null;
  if (Validator.isNull(organizationId)) {
    organization=OrganizationServiceUtil.addOrganization(parentOrganizationId,name,regionId,countryId,statusId,isLocation());
  }
 else {
    organization=OrganizationServiceUtil.updateOrganization(organizationId,parentOrganizationId,name,regionId,countryId,statusId,isLocation());
  }
  return organization;
}
