{
  Session session=null;
  try {
    session=openSession();
    MBBan mbBan=(MBBan)session.get(MBBanImpl.class,Long.valueOf(banId));
    if (mbBan == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + banId);
      }
      throw new NoSuchBanException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + banId);
    }
    return mbBanPersistence.remove(mbBan);
  }
 catch (  NoSuchBanException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
