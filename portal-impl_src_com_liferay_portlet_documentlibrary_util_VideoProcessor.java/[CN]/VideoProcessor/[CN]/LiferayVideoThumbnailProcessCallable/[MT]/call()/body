{
  Log4JUtil.initLog4J(_serverId,_liferayHome,this.getClass().getClassLoader(),new Log4jLogFactoryImpl(),_customLogSettings);
  try {
    LiferayConverter liferayConverter=new LiferayVideoThumbnailConverter(_inputURL,_outputFile,_extension,_height,_width,_percentage);
    liferayConverter.convert();
  }
 catch (  Exception e) {
    throw new ProcessException(e);
  }
  return StringPool.BLANK;
}
