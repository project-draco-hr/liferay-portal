{
  String id=DLUtil.getTempFileId(fileEntry.getFileEntryId(),fileEntry.getVersion());
  File previewFile=_getPreviewFile(id);
  File thumbnailFile=_getThumbnailFile(id);
  if (PropsValues.DL_FILE_ENTRY_PREVIEW_ENABLED && PropsValues.DL_FILE_ENTRY_THUMBNAIL_ENABLED) {
    if (previewFile.exists() && thumbnailFile.exists()) {
      return true;
    }
  }
 else   if (PropsValues.DL_FILE_ENTRY_PREVIEW_ENABLED && previewFile.exists()) {
    return true;
  }
 else   if (PropsValues.DL_FILE_ENTRY_THUMBNAIL_ENABLED && thumbnailFile.exists()) {
    return true;
  }
  return false;
}
