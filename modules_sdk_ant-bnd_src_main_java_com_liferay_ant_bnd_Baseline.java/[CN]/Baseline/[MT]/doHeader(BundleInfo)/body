{
  if (!bundleInfo.mismatch) {
    return;
  }
  String output="[Baseline Report] Mode: ";
  if (_reportDiff) {
    output+="diff";
  }
 else {
    output+="standard";
  }
  if (_logFile != null) {
    output+=" (persisted)";
  }
  log(output);
  output="[Baseline Warning] Bundle Version Change Recommended: " + bundleInfo.suggestedVersion;
  log(output);
  persistLog(output);
}
