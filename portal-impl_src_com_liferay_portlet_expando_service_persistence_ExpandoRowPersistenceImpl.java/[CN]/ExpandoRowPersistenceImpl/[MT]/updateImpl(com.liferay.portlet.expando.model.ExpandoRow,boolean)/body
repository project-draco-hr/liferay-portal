{
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(expandoRow);
    }
 else {
      if (expandoRow.isNew()) {
        session.save(expandoRow);
      }
    }
    session.flush();
    expandoRow.setNew(false);
    return expandoRow;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(ExpandoRow.class.getName());
  }
}
