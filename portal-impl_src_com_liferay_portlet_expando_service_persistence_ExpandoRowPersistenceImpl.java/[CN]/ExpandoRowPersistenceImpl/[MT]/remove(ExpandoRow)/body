{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(expandoRow);
    }
  }
  expandoRow=removeImpl(expandoRow);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(expandoRow);
    }
  }
  return expandoRow;
}
