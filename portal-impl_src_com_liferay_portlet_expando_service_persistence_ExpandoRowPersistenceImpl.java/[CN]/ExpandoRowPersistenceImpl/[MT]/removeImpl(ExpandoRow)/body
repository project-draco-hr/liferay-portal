{
  Session session=null;
  try {
    session=openSession();
    session.delete(expandoRow);
    session.flush();
    return expandoRow;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ExpandoRow.class.getName());
  }
}
