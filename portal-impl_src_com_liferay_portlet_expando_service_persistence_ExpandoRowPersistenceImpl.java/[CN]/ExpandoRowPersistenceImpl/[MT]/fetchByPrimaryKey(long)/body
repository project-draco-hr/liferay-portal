{
  ExpandoRow expandoRow=(ExpandoRow)EntityCacheUtil.getResult(ExpandoRowModelImpl.ENTITY_CACHE_ENABLED,ExpandoRowImpl.class,rowId,this);
  if (expandoRow == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (expandoRow == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      expandoRow=(ExpandoRow)session.get(ExpandoRowImpl.class,Long.valueOf(rowId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (expandoRow == null)) {
        EntityCacheUtil.putResult(ExpandoRowModelImpl.ENTITY_CACHE_ENABLED,ExpandoRowImpl.class,rowId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(expandoRow);
      }
      closeSession(session);
    }
  }
  return expandoRow;
}
