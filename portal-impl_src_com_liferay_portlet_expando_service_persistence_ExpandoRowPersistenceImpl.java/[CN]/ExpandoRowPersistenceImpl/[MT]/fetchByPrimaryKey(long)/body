{
  ExpandoRow expandoRow=(ExpandoRow)EntityCacheUtil.getResult(ExpandoRowModelImpl.ENTITY_CACHE_ENABLED,ExpandoRowImpl.class,rowId);
  if (expandoRow == _nullExpandoRow) {
    return null;
  }
  if (expandoRow == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      expandoRow=(ExpandoRow)session.get(ExpandoRowImpl.class,Long.valueOf(rowId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (expandoRow != null) {
        cacheResult(expandoRow);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(ExpandoRowModelImpl.ENTITY_CACHE_ENABLED,ExpandoRowImpl.class,rowId,_nullExpandoRow);
      }
      closeSession(session);
    }
  }
  return expandoRow;
}
