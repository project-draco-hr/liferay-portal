{
  ExpandoRow expandoRow=fetchByPrimaryKey(rowId);
  if (expandoRow == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ExpandoRow exists with the primary key " + rowId);
    }
    throw new NoSuchRowException("No ExpandoRow exists with the primary key " + rowId);
  }
  return expandoRow;
}
