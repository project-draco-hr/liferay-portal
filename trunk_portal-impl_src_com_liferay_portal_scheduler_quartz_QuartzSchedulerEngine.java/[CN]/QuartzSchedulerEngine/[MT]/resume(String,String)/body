{
  if (!PropsValues.SCHEDULER_ENABLED) {
    return;
  }
  try {
    Scheduler scheduler=getScheduler(groupName);
    resume(scheduler,jobName,groupName);
  }
 catch (  Exception e) {
    throw new SchedulerException("Unable to resume job {jobName=" + jobName + ", groupName="+ groupName+ "}",e);
  }
}
