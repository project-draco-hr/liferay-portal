{
  ShoppingCategory shoppingCategory=(ShoppingCategory)entityCache.getResult(ShoppingCategoryModelImpl.ENTITY_CACHE_ENABLED,ShoppingCategoryImpl.class,primaryKey);
  if (shoppingCategory == _nullShoppingCategory) {
    return null;
  }
  if (shoppingCategory == null) {
    Session session=null;
    try {
      session=openSession();
      shoppingCategory=(ShoppingCategory)session.get(ShoppingCategoryImpl.class,primaryKey);
      if (shoppingCategory != null) {
        cacheResult(shoppingCategory);
      }
 else {
        entityCache.putResult(ShoppingCategoryModelImpl.ENTITY_CACHE_ENABLED,ShoppingCategoryImpl.class,primaryKey,_nullShoppingCategory);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(ShoppingCategoryModelImpl.ENTITY_CACHE_ENABLED,ShoppingCategoryImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return shoppingCategory;
}
