{
  LayoutPermissionUtil.check(getPermissionChecker(),groupId,privateLayout,layoutId,ActionKeys.UPDATE);
  return layoutLocalService.updateLayout(groupId,privateLayout,layoutId,parentLayoutId,localeNamesMap,localeTitlesMap,descriptionMap,keywordsMap,robotsMap,type,hidden,friendlyURL,iconImage,iconBytes,serviceContext);
}
