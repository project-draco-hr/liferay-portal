{
  Map<Locale,String> friendlyURLMap=new HashMap<Locale,String>();
  friendlyURLMap.put(LocaleUtil.getDefault(),friendlyURL);
  return updateLayout(groupId,privateLayout,layoutId,parentLayoutId,localeNamesMap,localeTitlesMap,descriptionMap,keywordsMap,robotsMap,type,hidden,friendlyURLMap,iconImage,iconBytes,serviceContext);
}
