{
  PermissionChecker permissionChecker=getPermissionChecker();
  if (!GroupPermissionUtil.contains(permissionChecker,groupId,ActionKeys.MANAGE_LAYOUTS) && !GroupPermissionUtil.contains(permissionChecker,groupId,ActionKeys.PUBLISH_STAGING)) {
    throw new PrincipalException();
  }
  layoutLocalService.importLayouts(getUserId(),groupId,privateLayout,parameterMap,file);
}
