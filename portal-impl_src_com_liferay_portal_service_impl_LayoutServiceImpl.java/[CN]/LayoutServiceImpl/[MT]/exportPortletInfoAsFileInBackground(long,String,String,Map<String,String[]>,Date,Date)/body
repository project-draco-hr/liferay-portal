{
  User user=userPersistence.findByPrimaryKey(userId);
  Group companyGroup=groupLocalService.getCompanyGroup(user.getCompanyId());
  GroupPermissionUtil.check(getPermissionChecker(),companyGroup.getGroupId(),ActionKeys.EXPORT_IMPORT_PORTLET_INFO);
  return layoutLocalService.exportPortletInfoAsFileInBackground(userId,taskName,portletId,parameterMap,startDate,endDate);
}
