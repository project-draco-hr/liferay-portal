{
  Map<Locale,String> localeNamesMap=new HashMap<Locale,String>();
  Locale defaultLocale=LocaleUtil.getDefault();
  localeNamesMap.put(defaultLocale,name);
  return addLayout(groupId,privateLayout,parentLayoutId,localeNamesMap,new HashMap<Locale,String>(),new HashMap<Locale,String>(),new HashMap<Locale,String>(),new HashMap<Locale,String>(),type,hidden,friendlyURL,locked,serviceContext);
}
