{
  LayoutPermissionUtil.check(getPermissionChecker(),groupId,privateLayout,layoutId,ActionKeys.UPDATE);
  pluginSettingLocalService.checkPermission(getUserId(),themeId,ThemeImpl.PLUGIN_TYPE);
  return layoutLocalService.updateLookAndFeel(groupId,privateLayout,layoutId,themeId,colorSchemeId,css,wapTheme);
}
