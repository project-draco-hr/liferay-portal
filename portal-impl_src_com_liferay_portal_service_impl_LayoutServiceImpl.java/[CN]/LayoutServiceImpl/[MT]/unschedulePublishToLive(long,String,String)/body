{
  PermissionChecker permissionChecker=getPermissionChecker();
  if (!GroupPermissionUtil.contains(permissionChecker,groupId,ActionKeys.MANAGE_STAGING) && !GroupPermissionUtil.contains(permissionChecker,groupId,ActionKeys.PUBLISH_STAGING)) {
    throw new PrincipalException();
  }
  SchedulerEngineUtil.unschedule(jobName,groupName);
}
