{
  DDMFormLayoutColumn ddmFormLayoutColumn1=createDDMFormLayoutColumn("field1",6);
  DDMFormLayoutColumn ddmFormLayoutColumn2=createDDMFormLayoutColumn("field2",7);
  DDMFormLayoutRow ddmFormLayoutRow=createDDMFormLayoutRow(ddmFormLayoutColumn1);
  ddmFormLayoutRow.addDDMFormLayoutColumn(ddmFormLayoutColumn2);
  LocalizedValue pageTitle=createPageTitle(LocaleUtil.US,"Page1");
  DDMFormLayoutPage ddmFormLayoutPage=createDDMFormLayoutPage(ddmFormLayoutRow,pageTitle);
  DDMFormLayout ddmFormLayout=createDDMFormLayout(ddmFormLayoutPage,LocaleUtil.US);
  _ddmFormLayoutValidator.validate(ddmFormLayout);
}
