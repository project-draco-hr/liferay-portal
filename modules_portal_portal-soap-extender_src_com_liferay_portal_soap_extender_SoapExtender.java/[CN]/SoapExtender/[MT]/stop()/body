{
  try {
    _serverServiceTracker.close();
  }
 catch (  Exception e) {
    if (_logger.isWarnEnabled()) {
      _logger.warn("Unable to close server service tracker " + _serverServiceTracker);
    }
  }
  try {
    _servletServiceRegistration.unregister();
  }
 catch (  Exception e) {
    if (_logger.isWarnEnabled()) {
      _logger.warn("Unable to unregister servlet " + _servletServiceRegistration);
    }
  }
  try {
    _servletContextHelperRegistration.unregister();
  }
 catch (  Exception e) {
    if (_logger.isWarnEnabled()) {
      _logger.warn("Unable to unregister servlet context " + _serverServiceTracker);
    }
  }
}
