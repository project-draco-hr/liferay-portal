{
  try {
    _serverServiceTracker.close();
  }
 catch (  Exception e) {
    if (_logger.isWarnEnabled()) {
      _logger.warn("Could not close servicetracker " + _serverServiceTracker);
    }
  }
  try {
    _servletServiceRegistration.unregister();
  }
 catch (  Exception e) {
    if (_logger.isWarnEnabled()) {
      _logger.warn("Could not unregister CXF servlet " + _servletServiceRegistration);
    }
  }
  try {
    _servletContextHelperRegistration.unregister();
  }
 catch (  Exception e) {
    if (_logger.isWarnEnabled()) {
      _logger.warn("Could not unregister Servlet context " + _serverServiceTracker);
    }
  }
}
