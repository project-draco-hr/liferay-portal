{
  Handler<?> handler=_bundleContext.getService(serviceReference);
  JaxWsEndpointImpl jaxWsEndpoint=(JaxWsEndpointImpl)_server.getEndpoint();
  Binding binding=jaxWsEndpoint.getJaxwsBinding();
  @SuppressWarnings("rawtypes") List<Handler> handlers=binding.getHandlerChain();
  handlers.add(handler);
  binding.setHandlerChain(handlers);
  return handler;
}
