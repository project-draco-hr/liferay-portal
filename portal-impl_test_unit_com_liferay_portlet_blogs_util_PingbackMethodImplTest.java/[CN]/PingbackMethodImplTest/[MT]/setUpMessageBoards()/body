{
  MBMessageDisplay mbMessageDisplay=Mockito.mock(MBMessageDisplay.class);
  when(_mbMessageLocalService.getDiscussionMessageDisplay(Matchers.anyLong(),Matchers.anyLong(),Matchers.eq(BlogsEntry.class.getName()),Matchers.anyLong(),Matchers.eq(WorkflowConstants.STATUS_APPROVED))).thenReturn(mbMessageDisplay);
  MBThread mbThread=Mockito.mock(MBThread.class);
  when(mbMessageDisplay.getThread()).thenReturn(mbThread);
  when(mbThread.getRootMessageId()).thenReturn(_PARENT_MESSAGE_ID);
  when(mbThread.getThreadId()).thenReturn(_THREAD_ID);
  mockStatic(MBMessageLocalServiceUtil.class,Mockito.CALLS_REAL_METHODS);
  stub(method(MBMessageLocalServiceUtil.class,"getService")).toReturn(_mbMessageLocalService);
}
