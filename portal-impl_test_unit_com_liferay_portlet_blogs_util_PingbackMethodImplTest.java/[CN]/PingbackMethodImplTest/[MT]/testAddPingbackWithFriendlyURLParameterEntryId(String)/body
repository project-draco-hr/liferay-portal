{
  when(_blogsEntryLocalService.getEntry(Matchers.anyLong())).thenReturn(_blogsEntry);
  String name;
  if (namespace == null) {
    name="entryId";
  }
 else {
    name=namespace + "entryId";
  }
  long entryId=RandomTestUtil.randomLong();
  whenFriendlyURLMapperPopulateParamsPut("",name,String.valueOf(entryId));
  execute();
  verifySuccess();
  Mockito.verify(_blogsEntryLocalService).getEntry(entryId);
}
