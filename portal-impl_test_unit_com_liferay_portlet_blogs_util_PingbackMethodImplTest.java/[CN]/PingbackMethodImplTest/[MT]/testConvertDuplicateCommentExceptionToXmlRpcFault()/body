{
  Mockito.doThrow(DuplicateCommentException.class).when(_commentManager).addComment(Mockito.anyLong(),Mockito.anyLong(),Mockito.anyString(),Mockito.anyLong(),Mockito.anyString(),(ServiceContext)Mockito.any());
  execute();
  verifyFault(PingbackMethodImpl.PINGBACK_ALREADY_REGISTERED,"Pingback previously registered");
}
