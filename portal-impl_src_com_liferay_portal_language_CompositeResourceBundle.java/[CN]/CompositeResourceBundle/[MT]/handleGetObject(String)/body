{
  for (  ResourceBundle delegateResourceBundle : _delegateResourceBundles) {
    Object object=null;
    try {
      object=delegateResourceBundle.getObject(key);
    }
 catch (    MissingResourceException e) {
      continue;
    }
    if (object != null) {
      return object;
    }
  }
  throw new MissingResourceException("Could not find resource",CompositeResourceBundle.class.getName(),key);
}
