{
  for (  ResourceBundle resourceBundle : _resourceBundles) {
    Object object=null;
    try {
      object=resourceBundle.getObject(key);
    }
 catch (    MissingResourceException mre) {
      continue;
    }
    if (object != null) {
      return object;
    }
  }
  throw new MissingResourceException("Unable to find resource",CompositeResourceBundle.class.getName(),key);
}
