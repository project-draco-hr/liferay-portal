{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Communities")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Communities"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//strong/span");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Assign Members")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Assign Members"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("link=Available"));
  selenium.waitForPageToLoad("30000");
  selenium.typeKeys("_134_keywords",RuntimeVariables.replace("content"));
  selenium.type("_134_keywords",RuntimeVariables.replace("content"));
  selenium.click(RuntimeVariables.replace("//input[@value='Search']"));
  selenium.waitForPageToLoad("30000");
  selenium.click("_134_allRowIds");
  selenium.click(RuntimeVariables.replace("//input[@value='Update Associations']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
}
