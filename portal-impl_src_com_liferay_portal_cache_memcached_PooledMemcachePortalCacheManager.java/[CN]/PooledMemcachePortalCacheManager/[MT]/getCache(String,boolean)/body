{
  PooledMemcachePortalCache portalCache=_portalCaches.get(name);
  if (portalCache == null) {
    portalCache=new PooledMemcachePortalCache(name,_memcachedClientFactory,_timeout,_timeoutTimeUnit);
    portalCache.setDebug(_debug);
    _portalCaches.put(name,portalCache);
  }
  return portalCache;
}
