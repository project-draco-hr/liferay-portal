{
  String[] paramArray=params.get(name);
  if (paramArray == null) {
    String namesapce=PortalUtil.getPortletNamespace(PortletKeys.BLOGS);
    paramArray=params.get(namesapce + name);
  }
  if (paramArray != null && paramArray.length > 0) {
    return paramArray[0];
  }
 else {
    return null;
  }
}
