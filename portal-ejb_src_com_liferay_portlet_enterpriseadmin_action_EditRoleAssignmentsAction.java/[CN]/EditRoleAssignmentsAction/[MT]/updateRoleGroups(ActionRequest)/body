{
  long roleId=ParamUtil.getLong(req,"roleId");
  long[] addGroupIds=StringUtil.split(ParamUtil.getString(req,"addGroupIds"),0L);
  long[] removeGroupIds=StringUtil.split(ParamUtil.getString(req,"removeGroupIds"),0L);
  GroupServiceUtil.addRoleGroups(roleId,addGroupIds);
  GroupServiceUtil.unsetRoleGroups(roleId,removeGroupIds);
}
