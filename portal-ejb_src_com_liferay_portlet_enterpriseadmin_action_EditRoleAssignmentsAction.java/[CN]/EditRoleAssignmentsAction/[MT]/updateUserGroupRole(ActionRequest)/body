{
  String roleId=ParamUtil.getString(req,"roleId");
  long groupId=ParamUtil.getLong(req,"roleGroupId");
  String[] addUserIds=StringUtil.split(ParamUtil.getString(req,"addUserIds"));
  String[] removeUserIds=StringUtil.split(ParamUtil.getString(req,"removeUserIds"));
  UserGroupRoleServiceUtil.addUserGroupRoles(addUserIds,groupId,roleId);
  UserGroupRoleServiceUtil.deleteUserGroupRoles(removeUserIds,groupId,roleId);
}
