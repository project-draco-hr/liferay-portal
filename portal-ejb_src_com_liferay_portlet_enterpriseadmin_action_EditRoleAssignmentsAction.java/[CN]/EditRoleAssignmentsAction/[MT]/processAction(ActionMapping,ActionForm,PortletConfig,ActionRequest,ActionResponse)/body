{
  String cmd=ParamUtil.getString(req,Constants.CMD);
  try {
    if (cmd.equals("role_groups")) {
      updateRoleGroups(req);
    }
 else     if (cmd.equals("role_users")) {
      updateRoleUsers(req);
    }
 else     if (cmd.equals("users_role_group")) {
      updateUsersRoleGroup(req);
    }
    if (Validator.isNotNull(cmd)) {
      sendRedirect(req,res);
    }
  }
 catch (  Exception e) {
    if (e instanceof NoSuchRoleException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      setForward(req,"portlet.enterprise_admin.error");
    }
 else {
      throw e;
    }
  }
}
