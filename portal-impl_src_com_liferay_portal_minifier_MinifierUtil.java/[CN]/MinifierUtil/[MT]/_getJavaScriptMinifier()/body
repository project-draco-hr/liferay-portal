{
  try {
    Class<JavaScriptMinifier> javaScriptMinifierClass=(Class<JavaScriptMinifier>)Class.forName(PropsValues.MINIFIER_JAVASCRIPT);
    return javaScriptMinifierClass.newInstance();
  }
 catch (  Exception e) {
    if (_log.isErrorEnabled()) {
      _log.error("Could not instantiate " + PropsValues.MINIFIER_JAVASCRIPT + ". Returning GoogleClosureCompileJavaScriptMinifier as default");
    }
    return new GoogleJavaScriptMinifier();
  }
}
