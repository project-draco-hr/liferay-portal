{
  if (!_DIRECT_SERVLET_CONTEXT_ENABLED) {
    return servletRequest.getRequestDispatcher(path);
  }
  ServletContext servletContext=(ServletContext)servletRequest.getAttribute(WebKeys.CTX);
  if (servletContext == null) {
    throw new IllegalStateException("Cannot find servlet context in request attributes");
  }
  return getRequestDispatcher(servletContext,path);
}
