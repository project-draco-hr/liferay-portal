{
  Session session=null;
  try {
    session=openSession();
    Phone phone=(Phone)session.get(Phone.class,phoneId);
    if (phone == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No Phone exists with the primary key " + phoneId.toString());
      }
      throw new NoSuchPhoneException("No Phone exists with the primary key " + phoneId.toString());
    }
    return phone;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
