{
  Session session=null;
  try {
    session=openSession();
    if (phone.isNew()) {
      session.save(phone);
    }
    session.flush();
    phone.setNew(false);
    return phone;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
