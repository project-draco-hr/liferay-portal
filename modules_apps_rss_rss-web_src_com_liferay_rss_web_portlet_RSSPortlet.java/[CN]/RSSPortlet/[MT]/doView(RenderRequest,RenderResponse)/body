{
  ThemeDisplay themeDisplay=(ThemeDisplay)renderRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  renderRequest.setAttribute(RSSWebConfiguration.class.getName(),_rssWebConfiguration);
  try {
    RSSPortletInstanceSettings rssPortletInstanceSettings=_settingsFactory.getSettings(RSSPortletInstanceSettings.class,new PortletInstanceSettingsLocator(themeDisplay.getLayout(),portletDisplay.getId()));
    renderRequest.setAttribute(RSSPortletInstanceSettings.class.getName(),rssPortletInstanceSettings);
  }
 catch (  PortalException pe) {
    throw new SystemException(pe);
  }
  super.doView(renderRequest,renderResponse);
}
