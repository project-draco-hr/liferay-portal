{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Web Content Display Permissions Page",RuntimeVariables.replace("Web Content Display Permissions Page"));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(5000);
  assertEquals(RuntimeVariables.replace("Options"),selenium.getText("//span[@title='Options']/ul/li/strong/a"));
  selenium.clickAt("//span[@title='Options']/ul/li/strong/a",RuntimeVariables.replace("Options"));
  selenium.waitForVisible("//div[@class='lfr-component lfr-menu-list']/ul/li[contains(.,'Configuration')]/a");
  assertEquals(RuntimeVariables.replace("Configuration"),selenium.getText("//div[@class='lfr-component lfr-menu-list']/ul/li[contains(.,'Configuration')]/a"));
  selenium.click("//div[@class='lfr-component lfr-menu-list']/ul/li[contains(.,'Configuration')]/a");
  selenium.waitForVisible("//iframe[contains(@id,'configurationIframeDialog')]");
  selenium.selectFrame("//iframe[contains(@id,'configurationIframeDialog')]");
  selenium.waitForElementPresent("//script[contains(@src,'/liferay/navigation_interaction.js')]");
  selenium.waitForVisible("link=Setup");
  assertTrue(selenium.isVisible("link=Setup"));
  assertTrue(selenium.isVisible("link=Supported Clients"));
  assertTrue(selenium.isVisible("link=Sharing"));
  assertTrue(selenium.isVisible("link=Scope"));
  selenium.selectFrame("relative=top");
}
