{
  long[] notPermittedEntryIds=getNotPermittedEntryIds(entryIds);
  notEntryIds=ArrayUtil.append(notEntryIds,notPermittedEntryIds);
  entryIds=ArrayUtil.toArray(getPermittedEntryIds(entryIds,notPermittedEntryIds));
  return tagsAssetLocalService.getAssetsCount(groupId,classNameIds,entryIds,notEntryIds,andOperator,excludeZeroViewCount,publishDate,expirationDate);
}
