{
  float value=Float.valueOf(s).floatValue();
  char c=_operator.charAt(0);
  if (c == '=') {
    _value=value;
  }
 else   if (c == '+') {
    _value+=value;
  }
 else   if (c == '-') {
    _value-=value;
  }
 else   if (c == '*') {
    _value*=value;
  }
 else   if (c == '/') {
    _value/=value;
  }
  _inputLabel.setText(Float.toString(_value));
}
