{
  if (obj == null) {
    return false;
  }
  OrgGroupRolePK pk=null;
  try {
    pk=(OrgGroupRolePK)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  if ((organizationId.equals(pk.organizationId)) && (groupId == pk.groupId) && (roleId.equals(pk.roleId))) {
    return true;
  }
 else {
    return false;
  }
}
