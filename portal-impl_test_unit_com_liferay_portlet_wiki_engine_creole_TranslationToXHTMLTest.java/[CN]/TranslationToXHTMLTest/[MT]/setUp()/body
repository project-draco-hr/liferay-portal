{
  mockStatic(HtmlUtil.class);
  mockStatic(PropsUtil.class);
  when(HtmlUtil.escape(Mockito.anyString())).then(new ReturnArgumentCalledAnswer<String>(0));
  when(PropsUtil.getArray(PropsKeys.WIKI_PARSERS_CREOLE_SUPPORTED_PROTOCOLS)).thenReturn(new String[]{"http://","https://","ftp://","mailto:","mms://"});
}
