{
  URL url=null;
  if (url == null && path.endsWith(".portal.jsp")) {
    path=getOriginalResourcePath(path);
    url=getExtension(path,true);
  }
 else   if (url == null && path.endsWith(".portal.jspf")) {
    path=getOriginalResourcePath(path);
    url=getExtension(path,true);
  }
 else {
    url=getResource(path);
  }
  if (url == null) {
    return null;
  }
  try {
    return url.openStream();
  }
 catch (  IOException ioe) {
    return null;
  }
}
