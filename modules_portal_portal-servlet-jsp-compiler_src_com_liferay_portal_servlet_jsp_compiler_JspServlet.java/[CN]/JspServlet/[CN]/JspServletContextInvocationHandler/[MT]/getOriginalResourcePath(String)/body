{
  if (path.endsWith(".portal.jsp")) {
    int endIndex=path.indexOf(".portal.jsp");
    path=path.substring(0,endIndex);
    path=path + ".jsp";
  }
 else   if (path.endsWith(".portal.jspf")) {
    int endIndex=path.indexOf(".portal.jspf");
    path=path.substring(0,endIndex);
    path=path + ".jspf";
  }
  return path;
}
