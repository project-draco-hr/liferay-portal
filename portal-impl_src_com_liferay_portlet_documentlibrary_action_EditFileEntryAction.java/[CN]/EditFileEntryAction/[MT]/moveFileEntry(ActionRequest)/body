{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long groupId=themeDisplay.getScopeGroupId();
  long folderId=ParamUtil.getLong(actionRequest,"folderId");
  long newFolderId=ParamUtil.getLong(actionRequest,"newFolderId");
  String name=ParamUtil.getString(actionRequest,"name");
  ServiceContext serviceContext=ServiceContextFactory.getInstance(DLFileEntry.class.getName(),actionRequest);
  DLFileEntry fileEntry=DLFileEntryServiceUtil.getFileEntry(groupId,folderId,name);
  DLFileEntryServiceUtil.updateFileEntry(groupId,folderId,newFolderId,name,null,fileEntry.getTitle(),fileEntry.getDescription(),null,false,fileEntry.getExtraSettings(),null,serviceContext);
}
