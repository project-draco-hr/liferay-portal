{
  long fileEntryId=ParamUtil.getLong(actionRequest,"fileEntryId");
  long newFolderId=ParamUtil.getLong(actionRequest,"newFolderId");
  ServiceContext serviceContext=ServiceContextFactory.getInstance(DLFileEntry.class.getName(),actionRequest);
  long[] fileEntryIds=null;
  if (fileEntryId > 0) {
    fileEntryIds=new long[]{fileEntryId};
  }
 else {
    fileEntryIds=StringUtil.split(ParamUtil.getString(actionRequest,"fileEntryIds"),0L);
  }
  for (  long moveFileEntryId : fileEntryIds) {
    if (moveFromTrash) {
      DLAppServiceUtil.moveFileEntryFromTrash(moveFileEntryId,newFolderId,serviceContext);
    }
 else {
      DLAppServiceUtil.moveFileEntry(moveFileEntryId,newFolderId,serviceContext);
    }
  }
}
