{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(userTrackerPath);
    session.flush();
    userTrackerPath.setNew(false);
    return userTrackerPath;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
