{
  Connection con=null;
  PreparedStatement ps=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("update CalEvent set recurrence = ? where eventId = ?");
    ps.setString(1,recurrence);
    ps.setLong(2,eventId);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
}
