{
  Connection con=null;
  PreparedStatement ps=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("update CalEvent set endDate = null " + "where (recurrence like '%\"until\":null%' " + "or recurrence = 'null') "+ "and enddate is not null");
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
}
