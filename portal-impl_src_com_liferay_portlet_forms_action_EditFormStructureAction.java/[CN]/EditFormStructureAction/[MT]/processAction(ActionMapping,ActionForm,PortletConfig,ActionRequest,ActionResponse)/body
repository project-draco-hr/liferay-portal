{
  String cmd=ParamUtil.getString(actionRequest,Constants.CMD);
  FormStructure formStructure=null;
  try {
    if (cmd.equals(Constants.ADD) || cmd.equals(Constants.UPDATE)) {
      formStructure=updateFormStructure(actionRequest);
    }
    if (Validator.isNotNull(cmd)) {
      String redirect=ParamUtil.getString(actionRequest,"redirect");
      if (formStructure != null) {
        boolean saveAndContinue=ParamUtil.getBoolean(actionRequest,"saveAndContinue");
        if (saveAndContinue) {
          redirect=getSaveAndContinueRedirect(portletConfig,actionRequest,formStructure,redirect);
        }
      }
      sendRedirect(actionRequest,actionResponse,redirect);
    }
  }
 catch (  Exception e) {
    if (e instanceof NoSuchFormStructureException || e instanceof PrincipalException) {
      SessionErrors.add(actionRequest,e.getClass().getName());
      setForward(actionRequest,"portlet.forms.error");
    }
 else     if (e instanceof FormDuplicateStructureElementException || e instanceof FormDuplicateStructureIdException || e instanceof FormStructureIdException|| e instanceof FormStructureNameException|| e instanceof FormStructureXsdException) {
      SessionErrors.add(actionRequest,e.getClass().getName(),e);
    }
 else {
      throw e;
    }
  }
  if (SessionErrors.isEmpty(actionRequest)) {
    SessionMessages.add(actionRequest,"request_processed");
  }
}
