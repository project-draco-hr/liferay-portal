{
  dlFileEntry=toUnwrappedModel(dlFileEntry);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,dlFileEntry);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(dlFileEntry);
  return dlFileEntry;
}
