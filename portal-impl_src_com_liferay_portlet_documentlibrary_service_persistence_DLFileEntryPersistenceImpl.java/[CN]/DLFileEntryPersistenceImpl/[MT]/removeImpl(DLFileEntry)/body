{
  Session session=null;
  try {
    session=openSession();
    session.delete(dlFileEntry);
    session.flush();
    return dlFileEntry;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(DLFileEntry.class.getName());
  }
}
