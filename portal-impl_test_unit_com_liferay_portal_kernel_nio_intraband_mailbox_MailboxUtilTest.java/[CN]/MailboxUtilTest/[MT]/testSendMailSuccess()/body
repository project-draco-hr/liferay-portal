{
  final long receipt=100;
  MockIntraband mockIntraband=new MockIntraband(){
    @Override protected void doSendDatagram(    RegistrationReference registrationReference,    Datagram datagram){
      byte[] data=new byte[8];
      BigEndianCodec.putLong(data,0,receipt);
      CompletionHandler<?> completionHandler=DatagramHelper.getCompletionHandler(datagram);
      completionHandler.replied(null,Datagram.createResponseDatagram(datagram,ByteBuffer.wrap(data)));
    }
  }
;
  Assert.assertEquals(receipt,MailboxUtil.sendMail(new MockRegistrationReference(mockIntraband),ByteBuffer.allocate(0)));
}
