{
  if (_stringCount == 0) {
    return _EMPTY_STRING;
  }
  String result=null;
  if (_stringCount <= 3) {
    result=_strings[0];
    for (int i=1; i < _stringCount; i++) {
      result=result.concat(_strings[i]);
    }
  }
 else {
    int length=getStringLength();
    StringBuilder sb=new StringBuilder(length);
    for (int i=0; i < _stringCount; i++) {
      sb.append(_strings[i]);
    }
    result=sb.toString();
  }
  return result;
}
