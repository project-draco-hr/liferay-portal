{
  if ((_type == TYPE_CLASS_PUBLIC) || (_type == TYPE_CLASS_PUBLIC_STATIC) || (_type == TYPE_CONSTRUCTOR_PUBLIC)|| (_type == TYPE_METHOD_PUBLIC)|| (_type == TYPE_METHOD_PUBLIC_STATIC)|| (_type == TYPE_VARIABLE_PUBLIC)|| (_type == TYPE_VARIABLE_PUBLIC_STATIC)) {
    return true;
  }
 else {
    return false;
  }
}
