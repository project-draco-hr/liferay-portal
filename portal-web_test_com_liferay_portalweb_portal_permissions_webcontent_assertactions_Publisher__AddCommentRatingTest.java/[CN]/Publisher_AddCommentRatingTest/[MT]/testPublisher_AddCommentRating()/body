{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Display Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Web Content Display Permissions Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//td[1]/ul/li[2]/a[1]");
  Thread.sleep(5000);
  assertTrue(selenium.isTextPresent("+1"));
  selenium.click("//td[1]/ul/li[2]/a[2]");
  Thread.sleep(5000);
  assertTrue(selenium.isTextPresent("-1"));
  selenium.click("//td[1]/ul/li[2]/a[2]");
  Thread.sleep(5000);
  assertTrue(selenium.isTextPresent("\u00b10"));
}
