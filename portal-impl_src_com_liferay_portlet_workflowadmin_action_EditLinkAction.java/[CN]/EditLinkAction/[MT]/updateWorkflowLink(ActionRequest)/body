{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  for (  String name : actionRequest.getParameterNames()) {
    if (!name.startsWith(_PREFIX)) {
      continue;
    }
    String workflowDefinitionName=ParamUtil.getString(actionRequest,name);
    long classNameId=GetterUtil.getLong(name.substring(_PREFIX.length(),name.length()));
    WorkflowLinkLocalServiceUtil.updateWorkflowLink(themeDisplay.getUserId(),themeDisplay.getCompanyId(),0,classNameId,workflowDefinitionName);
  }
}
