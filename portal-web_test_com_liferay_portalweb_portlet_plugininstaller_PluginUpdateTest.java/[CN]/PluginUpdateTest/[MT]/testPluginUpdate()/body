{
  selenium.click("link=Plugin Installer Test Page");
  selenium.waitForPageToLoad("30000");
  selenium.type("_111_keywords",RuntimeVariables.replace("google"));
  selenium.click("//input[@value='Search Plugins']");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Google Gadget 4.4.0.1");
  selenium.waitForPageToLoad("30000");
  selenium.click("//input[@value='Install']");
  selenium.waitForPageToLoad("30000");
  Thread.sleep(10000);
  verifyTrue(selenium.isTextPresent("Your request processed successfully."));
  selenium.click("link=Return to Full Page");
  selenium.waitForPageToLoad("30000");
  selenium.click("//input[@value='Refresh']");
  selenium.waitForPageToLoad("30000");
  selenium.click("//input[@value='Refresh']");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Plugins");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Plugin Installer Test Page");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Updates are available for Liferay. Click here to open the Update Manager for details.")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("link=Updates are available for Liferay. Click here to open the Update Manager for details.");
  selenium.waitForPageToLoad("30000");
  verifyTrue(selenium.isTextPresent("Update"));
  selenium.click("Link=Update");
  selenium.waitForPageToLoad("30000");
  Thread.sleep(10000);
  selenium.click("//input[@value='Refresh']");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Plugin Installer Test Page");
  selenium.waitForPageToLoad("30000");
  verifyFalse(selenium.isTextPresent("Updates are available for Liferay. Click here to open the Update Manager for details."));
}
