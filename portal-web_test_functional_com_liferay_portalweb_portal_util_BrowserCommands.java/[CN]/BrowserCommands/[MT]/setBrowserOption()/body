{
  if (!_BROWSER_TYPE.equals("*chrome") && !_BROWSER_TYPE.equals("*firefox")) {
    return;
  }
  try {
    Runtime runtime=Runtime.getRuntime();
    String[] commands={_BROWSER_COMMANDS_DIR + "set_browser_option.exe",_OUTPUT_DIR};
    runtime.exec(commands);
    Thread.sleep(10000);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
