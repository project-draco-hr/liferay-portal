{
  List<JournalFolder> folders=journalFolderPersistence.filterFindByG_P(groupId,folderId);
  for (  JournalFolder folder : folders) {
    if (folder.isInTrash() || folder.isInTrashContainer()) {
      continue;
    }
    folderIds.add(folder.getFolderId());
    getSubfolderIds(folderIds,folder.getGroupId(),folder.getFolderId());
  }
}
