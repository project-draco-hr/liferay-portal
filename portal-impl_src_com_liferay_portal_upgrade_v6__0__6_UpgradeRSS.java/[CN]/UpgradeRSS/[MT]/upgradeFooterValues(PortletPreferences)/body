{
  String[] footerArticleResouceValues=portletPreferences.getValues("footer-article-resource-values",new String[]{"0",""});
  long footerArticleResourcePrimKey=GetterUtil.getLong(footerArticleResouceValues[0]);
  String[] values=getArticleValues(footerArticleResourcePrimKey);
  portletPreferences.setValues("footer-article-values",values);
  portletPreferences.reset("footer-article-resource-values");
}
