{
  long groupId=0;
  String articleId=StringPool.BLANK;
  try (PreparedStatement ps=connection.prepareStatement("select groupId, articleId from JournalArticle where " + "resourcePrimKey = ?")){
    ps.setLong(1,resourcePrimKey);
    try (ResultSet rs=ps.executeQuery()){
      rs.next();
      groupId=rs.getLong("groupId");
      articleId=rs.getString("articleId");
    }
   }
 catch (  Exception e) {
  }
  return new String[]{String.valueOf(groupId),articleId};
}
