{
  String[] headerArticleResouceValues=portletPreferences.getValues("header-article-resource-values",new String[]{"0",""});
  long headerArticleResourcePrimKey=GetterUtil.getLong(headerArticleResouceValues[0]);
  String[] values=getArticleValues(headerArticleResourcePrimKey);
  portletPreferences.setValues("header-article-values",values);
  portletPreferences.reset("header-article-resource-values");
}
