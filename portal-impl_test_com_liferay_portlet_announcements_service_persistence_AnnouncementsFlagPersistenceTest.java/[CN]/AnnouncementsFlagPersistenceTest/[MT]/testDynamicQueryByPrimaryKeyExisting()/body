{
  AnnouncementsFlag newAnnouncementsFlag=addAnnouncementsFlag();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AnnouncementsFlag.class,AnnouncementsFlag.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("flagId",newAnnouncementsFlag.getFlagId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  AnnouncementsFlag existingAnnouncementsFlag=(AnnouncementsFlag)result.get(0);
  assertEquals(existingAnnouncementsFlag,newAnnouncementsFlag);
}
