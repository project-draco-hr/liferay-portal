{
  if (_group != null) {
    GroupLocalServiceUtil.deleteGroup(_group);
    _group=null;
  }
  if (_wikiPage != null) {
    WikiPageLocalServiceUtil.deletePage(_wikiPage);
    _wikiPage=null;
  }
  if (_wikiNode != null) {
    WikiNodeLocalServiceUtil.deleteNode(_wikiNode);
    _wikiNode=null;
  }
}
