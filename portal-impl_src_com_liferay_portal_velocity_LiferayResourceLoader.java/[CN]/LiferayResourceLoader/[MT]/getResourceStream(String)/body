{
  InputStream is=doGetResourceStream(source);
  if (is == null) {
    if (_log.isDebugEnabled()) {
      _log.debug("Unable to find " + source);
    }
    throw new ResourceNotFoundException(source);
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Successfully found " + source);
  }
  return is;
}
