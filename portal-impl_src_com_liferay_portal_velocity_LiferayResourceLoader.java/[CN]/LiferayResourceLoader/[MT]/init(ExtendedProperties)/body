{
  boolean cachingOn=GetterUtil.getBoolean(PropsUtil.get(PropsKeys.VELOCITY_ENGINE_RESOURCE_MANAGER_CACHE_ENABLED));
  int modificationCheckInterval=GetterUtil.getInteger(PropsUtil.get(PropsKeys.VELOCITY_ENGINE_RESOURCE_MANAGER_MODIFICATION_CHECK_INTERVAL));
  setCachingOn(cachingOn);
  setModificationCheckInterval(modificationCheckInterval);
}
