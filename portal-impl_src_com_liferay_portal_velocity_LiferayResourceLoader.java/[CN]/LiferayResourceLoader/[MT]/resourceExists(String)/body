{
  InputStream is=null;
  try {
    is=getResourceStream(resourceName);
  }
 catch (  ResourceNotFoundException e) {
    if (_log.isDebugEnabled()) {
      _log.debug("Could not load resource '" + resourceName + "' from ResourceLoader "+ getClass().getName()+ ": ",e);
    }
  }
 finally {
    try {
      if (is != null) {
        is.close();
      }
    }
 catch (    IOException e) {
    }
  }
  if (is != null) {
    return true;
  }
 else {
    return false;
  }
}
