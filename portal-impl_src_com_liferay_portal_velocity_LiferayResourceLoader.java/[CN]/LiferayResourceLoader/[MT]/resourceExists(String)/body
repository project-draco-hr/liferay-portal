{
  InputStream is=null;
  try {
    is=getResourceStream(resourceName);
  }
 catch (  ResourceNotFoundException rnfe) {
    if (_log.isDebugEnabled()) {
      _log.debug("Could not load resource " + resourceName + " from "+ getClass(),rnfe);
    }
  }
 finally {
    try {
      if (is != null) {
        is.close();
      }
    }
 catch (    IOException ioe) {
    }
  }
  if (is != null) {
    return true;
  }
 else {
    return false;
  }
}
