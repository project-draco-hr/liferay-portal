{
  String[] parameters=StringUtil.split(queryString,CharPool.AMPERSAND);
  StringBundler sb=new StringBundler(2 + parameters.length * 4);
  sb.append("p_p_id=");
  sb.append(portletId);
  for (  String parameter : parameters) {
    sb.append("&_");
    sb.append(portletId);
    sb.append("_");
    sb.append(parameter);
  }
  return sb.toString();
}
