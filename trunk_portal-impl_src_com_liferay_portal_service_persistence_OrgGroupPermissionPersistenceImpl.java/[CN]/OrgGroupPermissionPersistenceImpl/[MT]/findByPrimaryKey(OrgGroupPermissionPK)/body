{
  OrgGroupPermission orgGroupPermission=fetchByPrimaryKey(orgGroupPermissionPK);
  if (orgGroupPermission == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + orgGroupPermissionPK);
    }
    throw new NoSuchOrgGroupPermissionException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + orgGroupPermissionPK);
  }
  return orgGroupPermission;
}
