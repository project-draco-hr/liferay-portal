{
  HttpAuthorizationHeader httpAuthorizationHeader=new HttpAuthorizationHeader(HttpAuthorizationHeader.SCHEME_BASIC);
  httpAuthorizationHeader.setAuthParameter(HttpAuthorizationHeader.AUTH_PARAMETER_NAME_REALM,Portal.PORTAL_REALM);
  Assert.assertEquals(httpAuthorizationHeader.toString(),"Basic realm=\"" + Portal.PORTAL_REALM + "\"");
}
