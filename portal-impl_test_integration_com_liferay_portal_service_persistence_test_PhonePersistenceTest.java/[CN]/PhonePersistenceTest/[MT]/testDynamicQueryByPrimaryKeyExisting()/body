{
  Phone newPhone=addPhone();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Phone.class,Phone.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("phoneId",newPhone.getPhoneId()));
  List<Phone> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Phone existingPhone=result.get(0);
  Assert.assertEquals(existingPhone,newPhone);
}
