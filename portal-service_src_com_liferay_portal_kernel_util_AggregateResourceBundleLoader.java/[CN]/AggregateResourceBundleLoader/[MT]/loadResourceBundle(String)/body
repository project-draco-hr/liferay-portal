{
  ArrayList<ResourceBundle> resourceBundles=new ArrayList<>();
  for (  ResourceBundleLoader resourceBundleLoader : _resourceBundleLoaders) {
    try {
      ResourceBundle resourceBundle=resourceBundleLoader.loadResourceBundle(languageId);
      if (resourceBundle != null) {
        resourceBundles.add(resourceBundle);
      }
    }
 catch (    Exception e) {
    }
  }
  if (resourceBundles.isEmpty()) {
    throw new MissingResourceException("ResourceBundleLoader " + this + " failed to load "+ "ResourceBundle for "+ languageId,"",languageId);
  }
  if (resourceBundles.size() == 1) {
    return resourceBundles.get(0);
  }
  return new AggregateResourceBundle(resourceBundles.toArray(new ResourceBundle[0]));
}
