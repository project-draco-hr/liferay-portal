{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MDRRuleGroupInstance.class,MDRRuleGroupInstance.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("ruleGroupInstanceId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("ruleGroupInstanceId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
