{
  JournalPermission.check(getPermissionChecker(),groupId,ActionKeys.ADD_FEED);
  return journalFeedLocalService.addFeed(getUserId(),groupId,feedId,autoFeedId,name,description,ddmStructureKey,ddmTemplateKey,ddmRendererTemplateKey,delta,orderByCol,orderByType,targetLayoutFriendlyUrl,targetPortletId,contentField,feedType,feedVersion,serviceContext);
}
