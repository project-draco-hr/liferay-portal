{
  PortletPermissionUtil.check(getPermissionChecker(),plid,PortletKeys.JOURNAL,ActionKeys.ADD_FEED);
  return journalFeedLocalService.addFeed(getUserId(),plid,feedId,autoFeedId,name,description,type,structureId,templateId,rendererTemplateId,delta,orderByCol,orderByType,targetLayoutFriendlyUrl,targetPortletId,contentField,feedType,feedVersion,addCommunityPermissions,addGuestPermissions);
}
