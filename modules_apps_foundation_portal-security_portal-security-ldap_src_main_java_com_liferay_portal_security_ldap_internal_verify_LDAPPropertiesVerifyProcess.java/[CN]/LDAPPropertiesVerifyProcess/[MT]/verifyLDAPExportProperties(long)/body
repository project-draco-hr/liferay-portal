{
  Dictionary<String,Object> dictionary=new HashMapDictionary<>();
  dictionary.put(LDAPConstants.EXPORT_ENABLED,_prefsProps.getBoolean(companyId,LegacyLDAPPropsKeys.LDAP_EXPORT_ENABLED,false));
  dictionary.put(LDAPConstants.EXPORT_GROUP_ENABLED,_prefsProps.getBoolean(companyId,LegacyLDAPPropsKeys.LDAP_EXPORT_GROUP_ENABLED,false));
  if (_log.isInfoEnabled()) {
    _log.info("Adding LDAP export configuration for company " + companyId + " with properties: "+ dictionary);
  }
  _ldapExportConfigurationProvider.updateProperties(companyId,dictionary);
}
