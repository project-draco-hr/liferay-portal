{
  Ticket ticket=findByPrimaryKey(ticketId);
  Session session=null;
  try {
    session=openSession();
    Ticket[] array=new TicketImpl[3];
    array[0]=getByC_C_C_PrevAndNext(session,ticket,companyId,classNameId,classPK,orderByComparator,true);
    array[1]=ticket;
    array[2]=getByC_C_C_PrevAndNext(session,ticket,companyId,classNameId,classPK,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
