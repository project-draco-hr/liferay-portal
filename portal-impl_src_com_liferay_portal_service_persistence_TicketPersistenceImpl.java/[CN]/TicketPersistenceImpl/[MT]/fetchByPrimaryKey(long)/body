{
  Ticket ticket=(Ticket)EntityCacheUtil.getResult(TicketModelImpl.ENTITY_CACHE_ENABLED,TicketImpl.class,ticketId);
  if (ticket == _nullTicket) {
    return null;
  }
  if (ticket == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      ticket=(Ticket)session.get(TicketImpl.class,Long.valueOf(ticketId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (ticket != null) {
        cacheResult(ticket);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(TicketModelImpl.ENTITY_CACHE_ENABLED,TicketImpl.class,ticketId,_nullTicket);
      }
      closeSession(session);
    }
  }
  return ticket;
}
