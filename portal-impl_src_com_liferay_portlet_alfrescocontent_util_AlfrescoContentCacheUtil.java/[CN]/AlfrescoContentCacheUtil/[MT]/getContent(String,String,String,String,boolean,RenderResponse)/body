{
  String content=null;
  if (Validator.isNull(uuid)) {
    return null;
  }
  String key=_encodeKey(uuid);
  content=(String)ClusterPool.get(_cache,key);
  if (content == null) {
    try {
      content=AlfrescoContentUtil.getContent(userId,password,uuid,path,maximizeLinks,res);
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn(e.getMessage());
      }
    }
    if (content != null) {
      ClusterPool.put(_cache,key,content);
    }
  }
  return content;
}
