{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(folderId));
  }
  com.liferay.portlet.documentlibrary.model.DLFolder dlFolder=getPersistence().remove(folderId);
  if (listener != null) {
    listener.onAfterRemove(dlFolder);
  }
  return dlFolder;
}
