{
  MBDiscussion mbDiscussion=findByPrimaryKey(discussionId);
  Session session=null;
  try {
    session=openSession();
    MBDiscussion[] array=new MBDiscussionImpl[3];
    array[0]=getByUuid_PrevAndNext(session,mbDiscussion,uuid,orderByComparator,true);
    array[1]=mbDiscussion;
    array[2]=getByUuid_PrevAndNext(session,mbDiscussion,uuid,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
