{
  mbDiscussion=toUnwrappedModel(mbDiscussion);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,mbDiscussion);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  MBDiscussionModelImpl mbDiscussionModelImpl=(MBDiscussionModelImpl)mbDiscussion;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_THREADID,new Object[]{Long.valueOf(mbDiscussionModelImpl.getThreadId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_C,new Object[]{Long.valueOf(mbDiscussionModelImpl.getClassNameId()),Long.valueOf(mbDiscussionModelImpl.getClassPK())});
  EntityCacheUtil.removeResult(MBDiscussionModelImpl.ENTITY_CACHE_ENABLED,MBDiscussionImpl.class,mbDiscussion.getPrimaryKey());
  return mbDiscussion;
}
