{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(mbDiscussion);
    }
  }
  mbDiscussion=removeImpl(mbDiscussion);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(mbDiscussion);
    }
  }
  return mbDiscussion;
}
