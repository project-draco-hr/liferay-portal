{
  KaleoTimerInstanceToken kaleoTimerInstanceToken=findByPrimaryKey(kaleoTimerInstanceTokenId);
  Session session=null;
  try {
    session=openSession();
    KaleoTimerInstanceToken[] array=new KaleoTimerInstanceTokenImpl[3];
    array[0]=getByKaleoInstanceId_PrevAndNext(session,kaleoTimerInstanceToken,kaleoInstanceId,orderByComparator,true);
    array[1]=kaleoTimerInstanceToken;
    array[2]=getByKaleoInstanceId_PrevAndNext(session,kaleoTimerInstanceToken,kaleoInstanceId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
