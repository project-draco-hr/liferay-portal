{
  Class<?> superClass=ReflectionUtil.getGenericSuperType(persistenceImpl.getClass());
  Class<?> key=ReflectionUtil.getGenericSuperType(superClass.getClass());
  if (key == null) {
    return (ModelListener<T>[])_instance._getModelListeners(superClass);
  }
  return (ModelListener<T>[])_instance._getModelListeners(key);
}
