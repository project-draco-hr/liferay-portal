{
  String sql="update Group_ set typeSettings = ? where groupId = " + groupId;
  try (PreparedStatement ps=connection.prepareStatement(sql)){
    ps.setString(1,typeSettings);
    ps.executeUpdate();
  }
 catch (  SQLException sqle) {
    if (_log.isWarnEnabled()) {
      _log.warn(sqle,sqle);
    }
  }
}
