{
  List<Company> companies=_companyLocalService.getCompanies();
  for (  Company company : companies) {
    long defaultUserId=_userLocalService.getDefaultUserId(company.getCompanyId());
    List<LayoutPrototype> layoutPrototypes=_layoutPrototypeLocalService.search(company.getCompanyId(),null,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
    addWikiPage(company.getCompanyId(),defaultUserId,layoutPrototypes);
  }
}
