{
  Serializable serializable=entityCache.getResult(ResourceActionModelImpl.ENTITY_CACHE_ENABLED,ResourceActionImpl.class,primaryKey);
  if (serializable == nullModel) {
    return null;
  }
  ResourceAction resourceAction=(ResourceAction)serializable;
  if (resourceAction == null) {
    Session session=null;
    try {
      session=openSession();
      resourceAction=(ResourceAction)session.get(ResourceActionImpl.class,primaryKey);
      if (resourceAction != null) {
        cacheResult(resourceAction);
      }
 else {
        entityCache.putResult(ResourceActionModelImpl.ENTITY_CACHE_ENABLED,ResourceActionImpl.class,primaryKey,nullModel);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(ResourceActionModelImpl.ENTITY_CACHE_ENABLED,ResourceActionImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return resourceAction;
}
