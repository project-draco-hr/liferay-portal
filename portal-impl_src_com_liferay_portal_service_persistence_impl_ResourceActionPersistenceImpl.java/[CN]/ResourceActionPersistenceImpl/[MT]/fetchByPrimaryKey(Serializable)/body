{
  ResourceAction resourceAction=(ResourceAction)entityCache.getResult(ResourceActionModelImpl.ENTITY_CACHE_ENABLED,ResourceActionImpl.class,primaryKey);
  if (resourceAction == _nullResourceAction) {
    return null;
  }
  if (resourceAction == null) {
    Session session=null;
    try {
      session=openSession();
      resourceAction=(ResourceAction)session.get(ResourceActionImpl.class,primaryKey);
      if (resourceAction != null) {
        cacheResult(resourceAction);
      }
 else {
        entityCache.putResult(ResourceActionModelImpl.ENTITY_CACHE_ENABLED,ResourceActionImpl.class,primaryKey,_nullResourceAction);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(ResourceActionModelImpl.ENTITY_CACHE_ENABLED,ResourceActionImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return resourceAction;
}
