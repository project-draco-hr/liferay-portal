{
  Filter filter=bundleContext.createFilter(filterString);
  ServiceTracker<?,?> serviceTracker=new ServiceTracker<>(bundleContext,filter,null);
  serviceTracker.open();
  Object object=serviceTracker.waitForService(timeout);
  serviceTracker.close();
  if (object == null) {
    return false;
  }
  return true;
}
