{
  if (ArrayUtil.isEmpty(array)) {
    return true;
  }
  int i=0;
  for (i=0; i < array.length; i++) {
    int val=0;
    if (allowNegative && (array[i] < 0)) {
      int max=candidate.getActualMaximum(field);
      val=(max + 1) + array[i];
    }
 else {
      val=array[i];
    }
    if (val == candidate.get(field)) {
      return true;
    }
  }
  return false;
}
