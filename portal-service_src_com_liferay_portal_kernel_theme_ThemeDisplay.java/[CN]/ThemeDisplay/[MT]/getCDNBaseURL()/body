{
  if (_cdnBaseURL != null) {
    return _cdnBaseURL;
  }
  String host=getCDNHost();
  if (Validator.isNull(host)) {
    String portalURL=getPortalURL();
    try {
      portalURL=PortalUtil.getPortalURL(getLayout(),this);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
    host=portalURL;
  }
  _cdnBaseURL=host;
  return _cdnBaseURL;
}
