{
  Group group=_groupLocalService.fetchFriendlyURLGroup(company.getCompanyId(),"/forms");
  if (group == null) {
    group=addFormsGroup(company.getCompanyId());
  }
  Layout layout=_layoutLocalService.fetchLayoutByFriendlyURL(group.getGroupId(),false,"/shared");
  if (layout == null) {
    addSharedLayout(company.getCompanyId(),group.getGroupId());
  }
}
