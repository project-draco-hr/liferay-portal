{
  if (!_privateLayout) {
    return true;
  }
 else {
    Group group=getGroup();
    if (group.getClassNameId() > 0) {
      long userClassNameId=PortalUtil.getClassNameId(User.class);
      if (group.getClassNameId() == userClassNameId) {
        return false;
      }
    }
    return true;
  }
}
