{
  List layouts=getChildren();
  Iterator itr=layouts.iterator();
  while (itr.hasNext()) {
    Layout layout=(Layout)itr.next();
    if (layout.isHidden() || !LayoutPermission.contains(permissionChecker,layout.getLayoutId(),layout.getOwnerId(),ActionKeys.VIEW)) {
      itr.remove();
    }
  }
  return layouts;
}
