{
  ServiceContext serviceContext=executionContext.getServiceContext();
  KaleoTimerInstanceToken kaleoTimerInstanceToken=executionContext.getKaleoTimerInstanceToken();
  KaleoTimer kaleoTimer=kaleoTimerInstanceToken.getKaleoTimer();
  kaleoActionExecutor.executeKaleoActions(KaleoTimer.class.getName(),kaleoTimer.getKaleoTimerId(),ExecutionType.ON_TIMER,executionContext);
  notificationHelper.sendKaleoNotifications(KaleoTimer.class.getName(),kaleoTimer.getKaleoTimerId(),ExecutionType.ON_TIMER,executionContext);
  doExecuteTimer(currentKaleoNode,kaleoTimer,executionContext);
  if (!kaleoTimer.isRecurring()) {
    kaleoTimerInstanceTokenLocalService.completeKaleoTimerInstanceToken(kaleoTimerInstanceToken.getKaleoTimerInstanceTokenId(),serviceContext);
  }
}
