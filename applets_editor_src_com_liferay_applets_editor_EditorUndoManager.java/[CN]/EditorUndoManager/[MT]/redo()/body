{
  boolean stop=false;
  super.redo();
  do {
    UndoableEdit edit=editToBeRedone();
    if (edit != null) {
      String presName=edit.getPresentationName();
      if (presName.toLowerCase().indexOf("style") != -1) {
        super.redo();
      }
 else {
        stop=true;
      }
    }
 else {
      stop=true;
    }
  }
 while (!stop);
}
