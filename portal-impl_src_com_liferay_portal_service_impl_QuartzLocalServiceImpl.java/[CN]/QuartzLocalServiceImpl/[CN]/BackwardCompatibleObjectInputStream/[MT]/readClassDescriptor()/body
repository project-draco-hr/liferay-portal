{
  ObjectStreamClass objectStreamClass=super.readClassDescriptor();
  String name=objectStreamClass.getName();
  if (name.equals(Message.class.getName())) {
    return ObjectStreamClass.lookup(Message.class);
  }
 else   if (name.equals(com.liferay.portlet.communities.messaging.LayoutsLocalPublisherRequest.class.getName())) {
    return ObjectStreamClass.lookup(LayoutsLocalPublisherRequest.class);
  }
 else   if (name.equals(com.liferay.portlet.communities.messaging.LayoutsRemotePublisherRequest.class.getName())) {
    return ObjectStreamClass.lookup(LayoutsRemotePublisherRequest.class);
  }
 else {
    return objectStreamClass;
  }
}
