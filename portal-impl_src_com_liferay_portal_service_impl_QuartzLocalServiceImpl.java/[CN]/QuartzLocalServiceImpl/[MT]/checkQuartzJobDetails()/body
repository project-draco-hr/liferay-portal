{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select JOB_NAME, JOB_GROUP, JOB_DATA from QUARTZ_JOB_DETAILS " + "for update",ResultSet.TYPE_FORWARD_ONLY,ResultSet.CONCUR_UPDATABLE);
    rs=ps.executeQuery();
    while (rs.next()) {
      Blob jobData=rs.getBlob("JOB_DATA");
      boolean update=convertMessageToJSON(jobData);
      if (update) {
        rs.updateBlob("JOB_DATA",jobData);
        rs.updateRow();
      }
    }
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn(e,e);
    }
  }
 finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
