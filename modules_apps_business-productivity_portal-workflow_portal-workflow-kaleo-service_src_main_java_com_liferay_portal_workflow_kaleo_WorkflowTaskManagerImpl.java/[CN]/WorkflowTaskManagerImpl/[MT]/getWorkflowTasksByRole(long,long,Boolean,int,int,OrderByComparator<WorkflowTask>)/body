{
  try {
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setCompanyId(companyId);
    List<KaleoTaskInstanceToken> kaleoTaskInstanceTokens=KaleoTaskInstanceTokenLocalServiceUtil.getKaleoTaskInstanceTokens(Role.class.getName(),roleId,completed,start,end,KaleoTaskInstanceTokenOrderByComparator.getOrderByComparator(orderByComparator,_kaleoWorkflowModelConverter),serviceContext);
    return toWorkflowTasks(kaleoTaskInstanceTokens);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
