{
  UpgradeColumn locationColumn=new TempUpgradeColumnImpl("location",new Integer(Types.BOOLEAN));
  UpgradeColumn typeColumn=new OrganizationTypeUpgradeColumnImpl(locationColumn);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(OrganizationImpl.TABLE_NAME,OrganizationImpl.TABLE_COLUMNS,locationColumn,typeColumn);
