{
  UpgradeColumn locationColumn=new TempUpgradeColumnImpl("location",new Integer(Types.BOOLEAN));
  UpgradeColumn typeColumn=new OrganizationTypeUpgradeColumnImpl(locationColumn);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(OrganizationTable.TABLE_NAME,OrganizationTable.TABLE_COLUMNS,locationColumn,typeColumn);
  upgradeTable.updateTable();
  updateLocationResources();
}
