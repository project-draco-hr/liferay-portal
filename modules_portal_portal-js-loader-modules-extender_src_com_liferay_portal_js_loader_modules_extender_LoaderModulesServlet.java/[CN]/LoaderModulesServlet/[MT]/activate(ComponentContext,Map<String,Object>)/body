{
  if (_serviceTracker != null) {
    _serviceTracker.close();
  }
  setDetails(Converter.cnv(Details.class,properties));
  Filter filter=FrameworkUtil.createFilter("(&(objectClass=" + ServletContext.class.getName() + ")(osgi.web.contextpath=*))");
  _serviceTracker=new ServiceTracker<>(componentContext.getBundleContext(),filter,this);
  _serviceTracker.open();
}
