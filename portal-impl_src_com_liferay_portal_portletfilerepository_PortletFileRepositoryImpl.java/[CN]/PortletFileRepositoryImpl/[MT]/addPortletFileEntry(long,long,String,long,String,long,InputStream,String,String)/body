{
  if (inputStream == null) {
    return null;
  }
  File file=null;
  try {
    file=FileUtil.createTempFile(inputStream);
    return addPortletFileEntry(groupId,userId,className,classPK,portletId,folderId,file,fileName,mimeType);
  }
 catch (  IOException ioe) {
    throw new SystemException("Unable to write temporary file",ioe);
  }
 finally {
    FileUtil.delete(file);
  }
}
