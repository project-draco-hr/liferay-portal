{
  Class<?> AbstractAutowireCapableBeanFactoryClass=null;
  AbstractAutowireCapableBeanFactoryClass=Class.forName("org.springframework.beans.factory.support." + "AbstractAutowireCapableBeanFactory");
  Field filteredPropertyDescriptorsCacheField=null;
  filteredPropertyDescriptorsCacheField=AbstractAutowireCapableBeanFactoryClass.getDeclaredField("filteredPropertyDescriptorsCache");
  Class<?> filteredPropertyDescriptorsCacheClass=filteredPropertyDescriptorsCacheField.getType();
  if (!Map.class.isAssignableFrom(filteredPropertyDescriptorsCacheClass)) {
    Assert.fail(filteredPropertyDescriptorsCacheClass.getClass().getName() + " is not " + Map.class.getName());
  }
}
