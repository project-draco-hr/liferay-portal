{
  Class<?> aspectJExpressionPointcutClass=null;
  try {
    aspectJExpressionPointcutClass=Class.forName("org.springframework.aop.aspectj.AspectJExpressionPointcut");
  }
 catch (  Exception e) {
    Assert.fail(e.getMessage());
  }
  Field shadowMatchCacheField=null;
  try {
    shadowMatchCacheField=aspectJExpressionPointcutClass.getDeclaredField("shadowMatchCache");
  }
 catch (  Exception e) {
    Assert.fail(e.getMessage());
  }
  Class<?> shadowMatchCacheClass=shadowMatchCacheField.getType();
  if (!Map.class.isAssignableFrom(shadowMatchCacheClass)) {
    Assert.fail(shadowMatchCacheClass.getClass().getName() + " is not " + Map.class.getName());
  }
}
