{
  Class<?> jdkDynamicAopProxyClass=Class.forName("org.springframework.aop.framework.JdkDynamicAopProxy");
  Field advisedField=jdkDynamicAopProxyClass.getDeclaredField("advised");
  Class<?> advisedSupportClass=advisedField.getType();
  Class<?> clazz=advisedSupportClass.getClass();
  Assert.assertTrue(clazz.getName() + " is not " + AdvisedSupport.class.getName(),advisedSupportClass.equals(AdvisedSupport.class));
}
