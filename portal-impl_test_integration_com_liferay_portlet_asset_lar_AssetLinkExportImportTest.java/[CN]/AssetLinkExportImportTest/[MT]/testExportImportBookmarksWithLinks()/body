{
  long groupId=group.getGroupId();
  BookmarksEntry bookmarksEntry=BookmarksTestUtil.addEntry(groupId,true);
  BookmarksEntry relatedBookmarkEntry1=BookmarksTestUtil.addEntry(groupId,true);
  BookmarksEntry relatedBookmarkEntry2=BookmarksTestUtil.addEntry(groupId,true);
  addAssetLink(groupId,bookmarksEntry.getUuid(),relatedBookmarkEntry1.getUuid(),1);
  addAssetLink(groupId,bookmarksEntry.getUuid(),relatedBookmarkEntry2.getUuid(),2);
  doExportImportPortlet(PortletKeys.BOOKMARKS);
  BookmarksEntry importedBookmarksEntry=BookmarksEntryLocalServiceUtil.getBookmarksEntryByUuidAndGroupId(bookmarksEntry.getUuid(),importedGroup.getGroupId());
  Assert.assertNotNull(importedBookmarksEntry);
  validateImportedLinks(bookmarksEntry.getUuid());
}
