{
  for (  AssetLink originLink : assetLinks) {
    AssetEntry sourceAssetEntry=AssetEntryLocalServiceUtil.getEntry(originLink.getEntryId1());
    AssetEntry targetAssetEntry=AssetEntryLocalServiceUtil.getEntry(originLink.getEntryId2());
    AssetLink importedLink=AssetLinkLocalServiceUtil.getLink(importedGroupId,sourceAssetEntry.getClassUuid(),targetAssetEntry.getClassUuid(),originLink.getType());
    Assert.assertEquals(originLink.getType(),importedLink.getType());
    Assert.assertEquals(originLink.getWeight(),importedLink.getWeight());
  }
}
