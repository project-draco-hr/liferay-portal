{
  AssetEntry originAssetEntry=AssetEntryLocalServiceUtil.getEntry(groupId,sourceEntryUUID);
  AssetEntry targetAssetEntry=AssetEntryLocalServiceUtil.getEntry(groupId,targetEntryUUID);
  return AssetLinkLocalServiceUtil.addLink(TestPropsValues.getUserId(),originAssetEntry.getEntryId(),targetAssetEntry.getEntryId(),0,weight);
}
