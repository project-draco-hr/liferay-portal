{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Bookmarks Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Bookmarks Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//div[@class='lfr-component lfr-menu-list right ']/ul/li[2]/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_28_name",RuntimeVariables.replace(""));
  selenium.type("_28_description",RuntimeVariables.replace("This is a null name folder."));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have entered invalid data. Please try again."));
  assertTrue(selenium.isTextPresent("Please enter a valid name."));
}
