{
  if (obj == null) {
    return false;
  }
  Portlet portlet=null;
  try {
    portlet=(Portlet)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=portlet.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
