{
  if (isEscapedModel()) {
    return (Portlet)this;
  }
 else {
    Portlet model=new PortletImpl();
    model.setNew(isNew());
    model.setEscapedModel(true);
    model.setId(getId());
    model.setCompanyId(getCompanyId());
    model.setPortletId(HtmlUtil.escape(getPortletId()));
    model.setRoles(HtmlUtil.escape(getRoles()));
    model.setActive(getActive());
    model=(Portlet)Proxy.newProxyInstance(Portlet.class.getClassLoader(),new Class[]{Portlet.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
