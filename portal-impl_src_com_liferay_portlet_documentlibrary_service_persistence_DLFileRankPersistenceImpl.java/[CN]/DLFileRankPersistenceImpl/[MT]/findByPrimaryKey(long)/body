{
  DLFileRank dlFileRank=fetchByPrimaryKey(fileRankId);
  if (dlFileRank == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No DLFileRank exists with the primary key " + fileRankId);
    }
    throw new NoSuchFileRankException("No DLFileRank exists with the primary key " + fileRankId);
  }
  return dlFileRank;
}
