{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,dlFileRank,merge);
    dlFileRank.setNew(false);
    return dlFileRank;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(DLFileRank.class.getName());
  }
}
