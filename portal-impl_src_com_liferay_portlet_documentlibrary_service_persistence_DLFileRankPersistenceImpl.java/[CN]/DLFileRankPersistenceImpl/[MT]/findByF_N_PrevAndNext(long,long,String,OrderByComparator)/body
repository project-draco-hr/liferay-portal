{
  DLFileRank dlFileRank=findByPrimaryKey(fileRankId);
  Session session=null;
  try {
    session=openSession();
    DLFileRank[] array=new DLFileRankImpl[3];
    array[0]=getByF_N_PrevAndNext(session,dlFileRank,folderId,name,orderByComparator,true);
    array[1]=dlFileRank;
    array[2]=getByF_N_PrevAndNext(session,dlFileRank,folderId,name,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
