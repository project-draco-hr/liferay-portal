{
  try {
    _ddmStructureLocalService=ddmStructureLocalService;
    _dlFileEntryMetadataLocalService=dlFileEntryMetadataLocalService;
    _storageEngine=storageEngine;
    _dlFileVersion=dlFileEntry.getFileVersion();
