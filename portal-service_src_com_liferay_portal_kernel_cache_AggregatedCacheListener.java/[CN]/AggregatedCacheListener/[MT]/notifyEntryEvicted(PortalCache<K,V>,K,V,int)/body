{
  if (_skipListenerThreadLocal.get()) {
    return;
  }
  for (  Map.Entry<CacheListener<K,V>,CacheListenerScope> entry : _cacheListeners.entrySet()) {
    CacheListener<K,V> cacheListener=entry.getKey();
    if (_shouldDeliver(cacheListener,entry.getValue())) {
      cacheListener.notifyEntryEvicted(portalCache,key,value,timeToLive);
    }
  }
}
