{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(portletRequest,BookmarksPortletKeys.BOOKMARKS_ADMIN,0,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/bookmarks/view");
  portletURL.setParameter("folderId",String.valueOf(folderId));
  return portletURL.toString();
}
