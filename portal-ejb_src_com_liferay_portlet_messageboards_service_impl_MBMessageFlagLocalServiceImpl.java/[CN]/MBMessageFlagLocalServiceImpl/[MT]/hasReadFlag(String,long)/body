{
  if (userId > 0) {
    return true;
  }
  MBMessageFlagPK messageFlagPK=new MBMessageFlagPK(MBMessageImpl.DEPRECATED_TOPIC_ID,messageId,userId);
  MBMessageFlag messageFlag=MBMessageFlagUtil.fetchByPrimaryKey(messageFlagPK);
  if (messageFlag != null) {
    return true;
  }
 else {
    return false;
  }
}
