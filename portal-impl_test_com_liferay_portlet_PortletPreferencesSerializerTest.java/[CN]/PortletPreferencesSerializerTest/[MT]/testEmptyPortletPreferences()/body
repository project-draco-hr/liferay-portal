{
  String expectedXML="<portlet-preferences></portlet-preferences>";
  PortletPreferencesImpl portletPreferences=new PortletPreferencesImpl();
  String actualXML=PortletPreferencesSerializer.toXML(portletPreferences);
  assertEquals(expectedXML,actualXML);
  portletPreferences=(PortletPreferencesImpl)PortletPreferencesSerializer.fromDefaultXML(expectedXML);
  assertEquals(0,portletPreferences.getPreferences().size());
}
