{
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  if (Validator.isNull(portletDisplay.getId())) {
    return;
  }
  PortletURL portletURL=requestBasedPortletURLFactory.createActionURL(portletDisplay.getId());
  portletURL.setParameter(ActionRequest.ACTION_NAME,"/blogs/upload_editor_image");
  editorOptions.setUploadURL(portletURL.toString());
}
