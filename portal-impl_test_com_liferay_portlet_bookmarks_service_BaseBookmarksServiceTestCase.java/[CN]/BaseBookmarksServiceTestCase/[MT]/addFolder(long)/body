{
  String name="Test Folder";
  String description="This is a test folder.";
  Layout layout=LayoutLocalServiceUtil.getLayout(TestPropsValues.LAYOUT_PLID);
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddCommunityPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  serviceContext.setScopeGroupId(layout.getGroupId());
  serviceContext.setPlid(layout.getPlid());
  return BookmarksFolderServiceUtil.addFolder(parentFolderId,name,description,serviceContext);
}
