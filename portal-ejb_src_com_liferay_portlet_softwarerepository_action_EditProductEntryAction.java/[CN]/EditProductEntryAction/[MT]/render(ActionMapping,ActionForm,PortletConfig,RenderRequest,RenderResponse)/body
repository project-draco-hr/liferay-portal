{
  try {
    ActionUtil.getProductEntry(req);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchProductEntryException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.software_repository.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward(getForward(req,"portlet.software_repository.edit_product_entry"));
}
