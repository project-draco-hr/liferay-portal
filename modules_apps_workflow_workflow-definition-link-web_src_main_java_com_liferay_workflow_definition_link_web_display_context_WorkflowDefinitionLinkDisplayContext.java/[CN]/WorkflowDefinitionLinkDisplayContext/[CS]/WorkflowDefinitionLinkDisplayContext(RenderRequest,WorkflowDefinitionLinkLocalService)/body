{
  _workflowDefinitionLinkLocalService=workflowDefinitionLinkLocalService;
  _workflowDefinitionLinkRequestHelper=new WorkflowDefinitionLinkRequestHelper(renderRequest);
  if (calledFromControlPanel()) {
    _groupId=WorkflowConstants.DEFAULT_GROUP_ID;
  }
 else {
    ThemeDisplay themeDisplay=_workflowDefinitionLinkRequestHelper.getThemeDisplay();
    _groupId=themeDisplay.getSiteGroupIdOrLiveGroupId();
  }
}
