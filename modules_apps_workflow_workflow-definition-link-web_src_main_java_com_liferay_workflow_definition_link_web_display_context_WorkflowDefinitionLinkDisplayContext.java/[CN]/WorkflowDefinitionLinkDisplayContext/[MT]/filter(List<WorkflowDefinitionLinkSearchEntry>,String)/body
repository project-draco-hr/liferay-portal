{
  Iterator<WorkflowDefinitionLinkSearchEntry> iterator=workflowDefinitionLinkSearchEntries.iterator();
  while (iterator.hasNext()) {
    WorkflowDefinitionLinkSearchEntry entry=iterator.next();
    String resource=StringUtil.toLowerCase(entry.getResource());
    String workflowDefinitionName=StringUtil.toLowerCase(entry.getWorkflowDefinitionName());
    if (!resource.contains(keyword) && !workflowDefinitionName.contains(keyword)) {
      iterator.remove();
    }
  }
}
