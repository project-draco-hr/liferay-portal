{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(frameworkVersionId));
  }
  com.liferay.portlet.softwarecatalog.model.SCFrameworkVersion scFrameworkVersion=getPersistence().remove(frameworkVersionId);
  if (listener != null) {
    listener.onAfterRemove(scFrameworkVersion);
  }
  return scFrameworkVersion;
}
