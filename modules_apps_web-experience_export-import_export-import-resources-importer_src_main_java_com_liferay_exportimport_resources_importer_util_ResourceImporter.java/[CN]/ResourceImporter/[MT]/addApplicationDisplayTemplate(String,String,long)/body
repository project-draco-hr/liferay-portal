{
  Set<String> resourcePaths=servletContext.getResourcePaths(buildResourcePath(resourcesDir,parentDirName,StringPool.SLASH,dirName));
  if (resourcePaths == null) {
    return;
  }
  for (  String resourcePath : resourcePaths) {
    URL url=servletContext.getResource(resourcePath);
    URLConnection urlConnection=url.openConnection();
    String script=StringUtil.read(urlConnection.getInputStream());
    if (Validator.isNull(script)) {
      continue;
    }
    File file=new File(resourcePath);
    addApplicationDisplayTemplate(script,file,classNameId);
  }
}
