{
  String[] fileEntryTypeUuids=StringUtil.split(folderElement.attributeValue("fileEntryTypeUuids"));
  String defaultFileEntryTypeUuid=folderElement.attributeValue("defaultFileEntryTypeUuid");
  List<Long> fileEntryTypeIds=new ArrayList<Long>();
  long defaultFileEntryTypeId=0;
  for (  String fileEntryTypeUuid : fileEntryTypeUuids) {
    DLFileEntryType fileEntryType=DLFileEntryTypeUtil.fetchByUUID_G(fileEntryTypeUuid,portletDataContext.getScopeGroupId());
    if (fileEntryType != null) {
      fileEntryTypeIds.add(fileEntryType.getFileEntryTypeId());
      if (fileEntryType.getUuid().equals(defaultFileEntryTypeUuid)) {
        defaultFileEntryTypeId=fileEntryType.getFileEntryTypeId();
      }
    }
  }
  DLFileEntryTypeLocalServiceUtil.updateFolderFileEntryTypes((DLFolder)folder,fileEntryTypeIds,defaultFileEntryTypeId,serviceContext);
}
