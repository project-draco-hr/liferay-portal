{
  if (!portletDataContext.addPrimaryKey(DLPortletDataHandlerImpl.class,"deleteData")) {
    DLAppLocalServiceUtil.deleteFolders(portletDataContext.getScopeGroupId());
    DLAppLocalServiceUtil.deleteFileEntries(portletDataContext.getScopeGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID);
  }
  return null;
}
