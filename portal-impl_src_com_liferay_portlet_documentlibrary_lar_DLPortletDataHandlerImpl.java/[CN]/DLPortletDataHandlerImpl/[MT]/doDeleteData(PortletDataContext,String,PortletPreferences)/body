{
  if (!portletDataContext.addPrimaryKey(DLPortletDataHandlerImpl.class,"deleteData")) {
    DLFolderLocalServiceUtil.deleteFolders(portletDataContext.getScopeGroupId());
    DLFileEntryLocalServiceUtil.deleteFileEntries(portletDataContext.getScopeGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID);
  }
  return null;
}
