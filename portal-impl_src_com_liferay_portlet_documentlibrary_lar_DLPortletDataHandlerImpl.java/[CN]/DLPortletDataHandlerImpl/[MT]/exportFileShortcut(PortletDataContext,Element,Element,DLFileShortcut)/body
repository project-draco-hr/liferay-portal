{
  exportParentFolder(portletDataContext,foldersElement,fileShortcut.getFolderId());
  String path=getFileShortcutPath(portletDataContext,fileShortcut);
  if (portletDataContext.isPathNotProcessed(path)) {
    Element fileShortcutElement=fileShortcutsElement.addElement("file-shortcut");
    fileShortcutElement.addAttribute("path",path);
    fileShortcut.setUserUuid(fileShortcut.getUserUuid());
    portletDataContext.addPermissions(DLFileShortcut.class,fileShortcut.getFileShortcutId());
    portletDataContext.addZipEntry(path,fileShortcut);
  }
}
