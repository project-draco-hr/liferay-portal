{
  long defaultFileEntryType=DLFileEntryTypeLocalServiceUtil.getDefaultFileEntryType(folder.getFolderId());
  List<DLFileEntryType> fileEntryTypes=DLFileEntryTypeLocalServiceUtil.getFolderFileEntryTypes(new long[]{portletDataContext.getScopeGroupId()},folder.getFolderId(),false);
  String[] fileEntryTypeUuids=new String[fileEntryTypes.size()];
  String deaultFileEntryTypeUuid=StringPool.BLANK;
  for (int i=0; i < fileEntryTypes.size(); i++) {
    DLFileEntryType fileEntryType=fileEntryTypes.get(i);
    fileEntryTypeUuids[i]=fileEntryType.getUuid();
    if (fileEntryType.getFileEntryTypeId() == defaultFileEntryType) {
      deaultFileEntryTypeUuid=fileEntryType.getUuid();
    }
    exportFileEntryType(portletDataContext,fileEntryTypesElement,fileEntryType);
  }
  folderElement.addAttribute("fileEntryTypeUuids",StringUtil.merge(fileEntryTypeUuids));
  folderElement.addAttribute("deaultFileEntryTypeUuid",deaultFileEntryTypeUuid);
}
