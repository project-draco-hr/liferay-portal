{
  DLFileEntryType dlFileEntryType=DLFileEntryTypeUtil.fetchByG_N(groupId,name);
  if (dlFileEntryType == null) {
    return name;
  }
  if (Validator.isNotNull(uuid) && uuid.equals(dlFileEntryType.getUuid())) {
    return name;
  }
  name=StringUtil.appendParentheticalSuffix(name,count);
  return getFileEntryTypeName(uuid,companyId,groupId,name,++count);
}
