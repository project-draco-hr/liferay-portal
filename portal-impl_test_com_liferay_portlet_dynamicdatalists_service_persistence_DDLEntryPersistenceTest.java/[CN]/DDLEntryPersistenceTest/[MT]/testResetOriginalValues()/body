{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  DDLEntry newDDLEntry=addDDLEntry();
  _persistence.clearCache();
  DDLEntryModelImpl existingDDLEntryModelImpl=(DDLEntryModelImpl)_persistence.findByPrimaryKey(newDDLEntry.getPrimaryKey());
  assertTrue(Validator.equals(existingDDLEntryModelImpl.getUuid(),existingDDLEntryModelImpl.getOriginalUuid()));
  assertEquals(existingDDLEntryModelImpl.getGroupId(),existingDDLEntryModelImpl.getOriginalGroupId());
  assertEquals(existingDDLEntryModelImpl.getGroupId(),existingDDLEntryModelImpl.getOriginalGroupId());
  assertTrue(Validator.equals(existingDDLEntryModelImpl.getEntryKey(),existingDDLEntryModelImpl.getOriginalEntryKey()));
}
