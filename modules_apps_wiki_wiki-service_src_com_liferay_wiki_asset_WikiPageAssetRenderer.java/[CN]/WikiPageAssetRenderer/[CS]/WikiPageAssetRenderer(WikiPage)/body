{
  _page=page;
  WikiServiceSettingsProvider wikiServiceSettingsProvider=WikiServiceSettingsProvider.getWikiServiceSettingsProvider();
  GroupServiceSettingsProvider<WikiGroupServiceSettings> wikiGroupServiceSettingsProvider=wikiServiceSettingsProvider.getWikiGroupServiceSettingsProvider();
  _wikiGroupServiceSettings=wikiGroupServiceSettingsProvider.getGroupServiceSettings(page.getGroupId());
}
