{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),WikiPortletKeys.WIKI,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/wiki/export_page");
  portletURL.setParameter("nodeId",String.valueOf(_page.getNodeId()));
  portletURL.setParameter("title",_page.getTitle());
  return portletURL;
}
