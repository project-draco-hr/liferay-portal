{
  try (PreparedStatement ps=connection.prepareStatement("insert into PortletPreferences (portletPreferencesId, " + "ownerId, ownerType, plid, portletId, preferences) " + "values (?, ?, ?, ?, ?, ?)")){
    ps.setLong(1,increment());
    ps.setLong(2,ownerId);
    ps.setInt(3,ownerType);
    ps.setLong(4,plid);
    ps.setString(5,portletId);
    ps.setString(6,preferences);
    ps.executeUpdate();
  }
 }
