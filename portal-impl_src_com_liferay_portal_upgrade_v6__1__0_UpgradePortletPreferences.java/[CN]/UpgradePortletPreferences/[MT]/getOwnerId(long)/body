{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select groupId from Layout where plid = " + plid);
    rs=ps.executeQuery();
    if (rs.next()) {
      return rs.getLong("groupId");
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
  return 0;
}
