{
  PreparedStatement ps=null;
  try {
    ps=connection.prepareStatement("insert into PortalPreferences (portalPreferencesId, " + "ownerId, ownerType, preferences) values (?, ?, ?, ?)");
    ps.setLong(1,increment());
    ps.setLong(2,ownerId);
    ps.setInt(3,ownerType);
    ps.setString(4,preferences);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(ps);
  }
}
