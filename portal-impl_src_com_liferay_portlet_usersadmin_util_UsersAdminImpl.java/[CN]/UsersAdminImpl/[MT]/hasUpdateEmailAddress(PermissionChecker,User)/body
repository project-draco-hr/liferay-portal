{
  String[] fieldEditiableUserEmailAddress=PropsValues.FIELD_EDITABLE_COM_LIFERAY_PORTAL_MODEL_USER_EMAILADDRESS;
  if (ArrayUtil.contains(fieldEditiableUserEmailAddress,"administrator") && permissionChecker.isCompanyAdmin()) {
    return true;
  }
  if (ArrayUtil.contains(fieldEditiableUserEmailAddress,"user-with-mx") && user.hasCompanyMx()) {
    return true;
  }
  if (ArrayUtil.contains(fieldEditiableUserEmailAddress,"user-without-mx") && !user.hasCompanyMx()) {
    return true;
  }
  return false;
}
