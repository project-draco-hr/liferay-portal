{
  List<UserGroup> userGroups=new ArrayList<UserGroup>();
  boolean corruptIndex=false;
  List<Document> documents=hits.toList();
  for (  Document document : documents) {
    long userGroupId=GetterUtil.getLong(document.get(Field.USER_GROUP_ID));
    try {
      UserGroup userGroup=UserGroupLocalServiceUtil.getUserGroup(userGroupId);
      userGroups.add(userGroup);
    }
 catch (    NoSuchUserGroupException nsuge) {
      corruptIndex=true;
      Indexer indexer=IndexerRegistryUtil.getIndexer(UserGroup.class);
      long companyId=GetterUtil.getLong(document.get(Field.COMPANY_ID));
      indexer.delete(companyId,document.getUID());
    }
  }
  return new Tuple(userGroups,corruptIndex);
}
