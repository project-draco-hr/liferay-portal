{
  if (addMenuType == AddMenuType.FAVORITE) {
    return "favorite";
  }
 else   if (addMenuType == AddMenuType.PRIMARY) {
    return "primary";
  }
 else   if (addMenuType == AddMenuType.RECENT) {
    return "recent";
  }
 else   if (addMenuType == AddMenuType.DEFAULT) {
    return "default";
  }
  return StringPool.BLANK;
}
