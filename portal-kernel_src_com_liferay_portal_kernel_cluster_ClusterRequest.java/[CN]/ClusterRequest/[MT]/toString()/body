{
  StringBundler sb=new StringBundler(11);
  sb.append("{multicast=");
  sb.append(_multicast);
  sb.append(", payload=");
  sb.append(_payload);
  sb.append(", skipLocal=");
  sb.append(_skipLocal);
  if (!_multicast) {
    sb.append(", _targetClusterNodeIds=");
    sb.append(_targetClusterNodeIds);
  }
  sb.append(", uuid=");
  sb.append(_uuid);
  sb.append("}");
  return sb.toString();
}
