{
  String queryString=url.substring(url.indexOf('?') + 1);
  DLFileEntry entry=null;
  Map parameters=HttpUtil.parameterMapFromString(queryString);
  if (parameters.containsKey("folderId") && parameters.containsKey("name")) {
    try {
      long folderId=Long.parseLong(((String[])parameters.get("folderId"))[0]);
      String name=((String[])parameters.get("name"))[0];
      entry=DLFileEntryLocalServiceUtil.getFileEntry(folderId,name);
    }
 catch (    Exception e) {
    }
  }
 else   if (parameters.containsKey("groupId") && parameters.containsKey("uuid")) {
    try {
      long groupId=Long.parseLong(((String[])parameters.get("groupId"))[0]);
      String uuid=((String[])parameters.get("uuid"))[0];
      entry=DLFileEntryLocalServiceUtil.getFileEntryByUuidAndGroupId(uuid,groupId);
    }
 catch (    Exception e) {
      System.out.println(e.getMessage());
    }
  }
  return entry;
}
