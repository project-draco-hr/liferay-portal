{
  try {
    if (Validator.isNull(_userGroup)) {
      UserGroup userGroup=(UserGroup)obj;
      return UserGroupGroupRoleLocalServiceUtil.hasUserGroupGroupRole(userGroup.getUserGroupId(),_group.getGroupId(),_role.getRoleId());
    }
 else {
      Role role=(Role)obj;
      return UserGroupGroupRoleLocalServiceUtil.hasUserGroupGroupRole(_userGroup.getUserGroupId(),_group.getGroupId(),role.getRoleId());
    }
  }
 catch (  Exception e) {
    _log.error(e);
    return false;
  }
}
