{
  try {
    com.liferay.dynamic.data.mapping.model.DDMStructure ddmStructure=_ddmStructureLocalService.updateStructure(userId,structureId,parentStructureId,nameMap,descriptionMap,DDMBeanCopyUtil.copyDDMForm(ddmForm),DDMBeanCopyUtil.copyDDMFormLayout(ddmFormLayout),serviceContext);
    return new DDMStructureImpl(ddmStructure);
  }
 catch (  PortalException pe) {
    throw translate(pe);
  }
}
