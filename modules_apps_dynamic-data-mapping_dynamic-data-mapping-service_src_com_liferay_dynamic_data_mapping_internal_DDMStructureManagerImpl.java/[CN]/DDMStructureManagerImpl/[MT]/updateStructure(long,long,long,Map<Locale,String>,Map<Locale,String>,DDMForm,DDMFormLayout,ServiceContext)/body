{
  try {
    com.liferay.portlet.dynamicdatamapping.model.DDMStructure structure=_ddmStructureLocalService.updateStructure(userId,structureId,parentStructureId,nameMap,descriptionMap,translate(ddmForm),translate(ddmFormLayout),serviceContext);
    return new DDMStructureImpl(structure);
  }
 catch (  PortalException pe) {
    throw pe;
  }
catch (  Exception e) {
    _log.error(e,e);
  }
  return null;
}
