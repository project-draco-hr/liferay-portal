{
  try {
    com.liferay.portlet.dynamicdatamapping.model.DDMStructure ddmStructure=_ddmStructureLocalService.getStructure(structureId);
    return StagedModelDataHandlerUtil.exportReferenceStagedModel(portletDataContext,referrerStagedModel,ddmStructure,referenceType);
  }
 catch (  PortalException pe) {
    throw new PortletDataException(pe);
  }
}
