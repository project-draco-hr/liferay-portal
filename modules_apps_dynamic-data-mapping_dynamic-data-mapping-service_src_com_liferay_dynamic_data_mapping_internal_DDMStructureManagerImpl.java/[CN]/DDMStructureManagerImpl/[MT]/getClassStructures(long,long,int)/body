{
  List<DDMStructure> ddmStructures=new ArrayList<>();
  for (  com.liferay.portlet.dynamicdatamapping.model.DDMStructure ddmStructure : _ddmStructureLocalService.getClassStructures(companyId,classNameId,getStructureOrderByComparator(structureComparator))) {
    ddmStructures.add(new DDMStructureImpl(ddmStructure));
  }
  return ddmStructures;
}
