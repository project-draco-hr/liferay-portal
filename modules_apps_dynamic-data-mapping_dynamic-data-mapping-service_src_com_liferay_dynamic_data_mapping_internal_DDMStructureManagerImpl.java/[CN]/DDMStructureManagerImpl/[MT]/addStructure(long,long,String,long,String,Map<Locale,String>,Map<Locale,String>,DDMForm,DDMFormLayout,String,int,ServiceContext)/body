{
  try {
    com.liferay.dynamic.data.mapping.model.DDMStructure ddmStructure=_ddmStructureLocalService.addStructure(userId,groupId,parentStructureKey,classNameId,structureKey,nameMap,descriptionMap,DDMBeanCopyUtil.copyDDMForm(ddmForm),DDMBeanCopyUtil.copyDDMFormLayout(ddmFormLayout),storageType,type,serviceContext);
