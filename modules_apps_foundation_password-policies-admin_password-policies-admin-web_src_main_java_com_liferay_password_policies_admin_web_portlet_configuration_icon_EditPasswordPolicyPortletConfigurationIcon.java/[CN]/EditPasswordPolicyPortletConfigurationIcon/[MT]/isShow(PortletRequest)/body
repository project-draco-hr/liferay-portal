{
  try {
    ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
    if (PasswordPolicyPermissionUtil.contains(themeDisplay.getPermissionChecker(),getPasswordPolicyId(portletRequest),ActionKeys.UPDATE)) {
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
