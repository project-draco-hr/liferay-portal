{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.USERS_MEMBERSHIP_POLICY);
  }
  MembershipPolicy membershipPolicy=null;
  ClassLoader classLoader=PACLClassLoaderUtil.getPortalClassLoader();
  try {
    membershipPolicy=(MembershipPolicy)InstanceFactory.newInstance(classLoader,PropsValues.USERS_MEMBERSHIP_POLICY);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return membershipPolicy;
}
