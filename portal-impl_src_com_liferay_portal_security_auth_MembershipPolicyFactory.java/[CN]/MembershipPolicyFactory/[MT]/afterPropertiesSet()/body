{
  if (Validator.isNotNull(PropsValues.USERS_MEMBERSHIP_POLICY)) {
    if (_log.isDebugEnabled()) {
      _log.debug("Instantiate " + PropsValues.USERS_MEMBERSHIP_POLICY);
    }
    ClassLoader classLoader=PACLClassLoaderUtil.getPortalClassLoader();
    try {
      _membershipPolicy=(MembershipPolicy)InstanceFactory.newInstance(classLoader,PropsValues.USERS_MEMBERSHIP_POLICY);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
}
