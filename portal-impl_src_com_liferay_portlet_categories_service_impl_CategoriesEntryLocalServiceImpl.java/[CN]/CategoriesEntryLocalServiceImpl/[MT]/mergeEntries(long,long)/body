{
  List<TagsAsset> assets=categoriesEntryPersistence.getTagsAssets(fromEntryId);
  categoriesEntryPersistence.addTagsAssets(toEntryId,assets);
  List<CategoriesProperty> properties=categoriesPropertyPersistence.findByEntryId(fromEntryId);
  for (  CategoriesProperty fromProperty : properties) {
    CategoriesProperty toProperty=categoriesPropertyPersistence.fetchByE_K(toEntryId,fromProperty.getKey());
    if (toProperty == null) {
      fromProperty.setEntryId(toEntryId);
      categoriesPropertyPersistence.update(fromProperty,false);
    }
  }
  deleteEntry(fromEntryId);
}
