{
  lock.lock();
  try {
    if (branch >= hits.length) {
      throw new IllegalStateException("No instrument data for class " + className + " line "+ lineNumber+ " switch "+ switchNumber+ " branch "+ branch);
    }
    if (branch == -1) {
      defaultHits++;
    }
 else {
      hits[branch]+=new_hits;
    }
  }
  finally {
    lock.unlock();
  }
}
