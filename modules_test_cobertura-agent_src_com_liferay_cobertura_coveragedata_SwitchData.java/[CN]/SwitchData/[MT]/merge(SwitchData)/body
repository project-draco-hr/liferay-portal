{
  AtomicLongArray hitsArray=switchData._hitsArray;
  if (!_className.equals(switchData._className) || (_lineNumber != switchData._lineNumber) || (_switchNumber != switchData._switchNumber)|| (_hitsArray.length() != hitsArray.length())) {
    throw new IllegalArgumentException("Switch data mismatch, left : " + toString() + ", right : "+ switchData);
  }
  for (int i=0; i < _hitsArray.length(); i++) {
    _hitsArray.addAndGet(i,switchData._hitsArray.get(i));
  }
}
