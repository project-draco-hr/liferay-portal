{
  if (branch >= _hitsArray.length()) {
    throw new IllegalStateException("No instrument data for class " + _className + " line "+ _lineNumber+ " switch "+ _switchNumber+ " branch "+ branch);
  }
  if (branch == -1) {
    branch=_hitsArray.length() - 1;
  }
  _hitsArray.addAndGet(branch,hits);
}
