{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Address.class,Address.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("addressId",RandomTestUtil.nextLong()));
  List<Address> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
