{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Address.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("addressId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("addressId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
