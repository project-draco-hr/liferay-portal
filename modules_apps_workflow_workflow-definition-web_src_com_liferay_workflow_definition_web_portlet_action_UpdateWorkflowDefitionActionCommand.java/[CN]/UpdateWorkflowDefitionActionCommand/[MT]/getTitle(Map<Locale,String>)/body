{
  if (Validator.isNull(titleMap)) {
    return null;
  }
  String value=StringPool.BLANK;
  for (  Locale locale : LanguageUtil.getAvailableLocales()) {
    String languageId=LocaleUtil.toLanguageId(locale);
    String title=titleMap.get(locale);
    if (Validator.isNotNull(title)) {
      value=LocalizationUtil.updateLocalization(value,WorkflowDefinitionConstants.TITLE2,title,languageId);
    }
 else {
      value=LocalizationUtil.removeLocalization(value,WorkflowDefinitionConstants.TITLE2,languageId);
    }
  }
  return value;
}
