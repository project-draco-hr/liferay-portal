{
  byte[] layout=null;
  if (Validator.isNull(plid)) {
    return false;
  }
  plid=plid.trim().toUpperCase();
  String key=_encodeKey(companyId,plid + LANGUAGE + languageId);
  try {
    layout=(byte[])_cache.getFromCache(key);
  }
 catch (  NeedsRefreshException nre) {
    return false;
  }
 finally {
    if (layout == null) {
      _cache.cancelUpdate(key);
      return false;
    }
  }
  return true;
}
