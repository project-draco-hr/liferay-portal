{
  Subscription subscription=findByPrimaryKey(subscriptionId);
  Session session=null;
  try {
    session=openSession();
    Subscription[] array=new SubscriptionImpl[3];
    array[0]=getByUserId_PrevAndNext(session,subscription,userId,orderByComparator,true);
    array[1]=subscription;
    array[2]=getByUserId_PrevAndNext(session,subscription,userId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
