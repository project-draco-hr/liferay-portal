{
  FinderCache.clearCache(Subscription.class.getName());
  Session session=null;
  try {
    session=openSession();
    session.delete(subscription);
    session.flush();
    return subscription;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
