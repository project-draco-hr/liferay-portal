{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,subscription,merge);
    subscription.setNew(false);
    return subscription;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Subscription.class.getName());
  }
}
