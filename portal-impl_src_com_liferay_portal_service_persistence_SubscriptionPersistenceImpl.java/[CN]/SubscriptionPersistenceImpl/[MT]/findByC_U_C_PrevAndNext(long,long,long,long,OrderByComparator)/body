{
  Subscription subscription=findByPrimaryKey(subscriptionId);
  int count=countByC_U_C(companyId,userId,classNameId);
  Session session=null;
  try {
    session=openSession();
    StringMaker query=new StringMaker();
    query.append("FROM com.liferay.portal.model.Subscription WHERE ");
    query.append("companyId = ?");
    query.append(" AND ");
    query.append("userId = ?");
    query.append(" AND ");
    query.append("classNameId = ?");
    query.append(" ");
    if (obc != null) {
      query.append("ORDER BY ");
      query.append(obc.getOrderBy());
    }
    Query q=session.createQuery(query.toString());
    int queryPos=0;
    q.setLong(queryPos++,companyId);
    q.setLong(queryPos++,userId);
    q.setLong(queryPos++,classNameId);
    Object[] objArray=QueryUtil.getPrevAndNext(q,count,obc,subscription);
    Subscription[] array=new SubscriptionImpl[3];
    array[0]=(Subscription)objArray[0];
    array[1]=(Subscription)objArray[1];
    array[2]=(Subscription)objArray[2];
    return array;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
