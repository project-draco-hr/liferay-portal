{
  EntityCacheUtil.removeResult(SubscriptionModelImpl.ENTITY_CACHE_ENABLED,SubscriptionImpl.class,subscription.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_U_C_C,new Object[]{Long.valueOf(subscription.getCompanyId()),Long.valueOf(subscription.getUserId()),Long.valueOf(subscription.getClassNameId()),Long.valueOf(subscription.getClassPK())});
}
