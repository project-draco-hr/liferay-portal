{
  InputStream is=null;
  try {
    is=new FileInputStream(file);
    return getContentType(is,file.getName());
  }
 catch (  FileNotFoundException fnfe) {
    return getContentType(file.getName());
  }
 finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
}
