{
  if (file == null || !file.exists()) {
    return getContentType(fileName);
  }
  InputStream is=null;
  try {
    is=TikaInputStream.get(file);
    return getContentType(is,fileName);
  }
 catch (  FileNotFoundException fnfe) {
    return getContentType(fileName);
  }
 finally {
    StreamUtil.cleanUp(is);
  }
}
