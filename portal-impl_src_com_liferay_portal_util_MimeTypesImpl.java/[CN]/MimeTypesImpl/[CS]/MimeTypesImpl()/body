{
  _detector=new DefaultDetector(org.apache.tika.mime.MimeTypes.getDefaultMimeTypes());
  _webImageMimeTypes=SetUtil.fromArray(PropsValues.MIME_TYPES_WEB_IMAGES);
  URL url=org.apache.tika.mime.MimeTypes.class.getResource("tika-mimetypes.xml");
  try {
    read(url.openStream());
  }
 catch (  Exception e) {
    _log.error("Unable to populate extensions map",e);
  }
}
