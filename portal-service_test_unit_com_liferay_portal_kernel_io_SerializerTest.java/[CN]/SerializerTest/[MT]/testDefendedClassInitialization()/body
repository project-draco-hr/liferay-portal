{
  System.setProperty(Serializer.class.getName() + ".threadlocal.buffer.count.limit",String.valueOf(Serializer.MIN_THREADLOCAL_BUFFER_COUNT - 1));
  System.setProperty(Serializer.class.getName() + ".threadlocal.buffer.size.limit",String.valueOf(Serializer.MIN_THREADLOCAL_BUFFER_SIZE - 1));
  Assert.assertEquals(Serializer.MIN_THREADLOCAL_BUFFER_COUNT,Serializer.THREADLOCAL_BUFFER_COUNT_LIMIT);
  Assert.assertEquals(Serializer.MIN_THREADLOCAL_BUFFER_SIZE,Serializer.THREADLOCAL_BUFFER_SIZE_LIMIT);
}
