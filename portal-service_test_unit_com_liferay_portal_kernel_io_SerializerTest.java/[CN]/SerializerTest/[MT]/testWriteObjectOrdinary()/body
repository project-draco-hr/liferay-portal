{
  Serializer serializer=new Serializer();
  Date date=new Date(123456);
  serializer.writeObject(date);
  ByteBuffer byteBuffer=serializer.toByteBuffer();
  UnsyncByteArrayInputStream unsyncByteArrayInputStream=new UnsyncByteArrayInputStream(byteBuffer.array(),6,byteBuffer.remaining());
  ObjectInputStream objectInputStream=new ObjectInputStream(unsyncByteArrayInputStream);
  Object object=objectInputStream.readObject();
  Assert.assertTrue(object instanceof Date);
  Assert.assertEquals(date,object);
}
