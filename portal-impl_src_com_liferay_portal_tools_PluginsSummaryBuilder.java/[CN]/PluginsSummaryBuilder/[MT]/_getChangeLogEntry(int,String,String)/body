{
  StringBundler sb=new StringBundler(8);
  if (changeLogVersion > 1) {
    sb.append("\n\n");
  }
  sb.append("#\n");
  sb.append("# Module Incremental Version ");
  sb.append(changeLogVersion);
  sb.append("\n#\n");
  sb.append(range);
  sb.append("=");
  sb.append(ticketIdsString);
  return sb.toString();
}
