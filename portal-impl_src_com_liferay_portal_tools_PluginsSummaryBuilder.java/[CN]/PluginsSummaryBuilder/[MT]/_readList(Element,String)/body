{
  StringBuilder sb=new StringBuilder();
  if (parentEl != null) {
    Iterator<Element> itr=parentEl.elements(name).iterator();
    while (itr.hasNext()) {
      Element el=itr.next();
      String text=el.getText().trim();
      sb.append(text);
      if (itr.hasNext()) {
        sb.append(", ");
      }
    }
  }
  return sb.toString();
}
