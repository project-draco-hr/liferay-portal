{
  StringMaker sm=new StringMaker();
  if (parentEl != null) {
    Iterator<Element> itr=parentEl.elements(name).iterator();
    while (itr.hasNext()) {
      Element el=itr.next();
      String text=el.getText().trim().toLowerCase();
      sm.append(text);
      if (itr.hasNext()) {
        sm.append(", ");
      }
    }
  }
  return sm.toString();
}
