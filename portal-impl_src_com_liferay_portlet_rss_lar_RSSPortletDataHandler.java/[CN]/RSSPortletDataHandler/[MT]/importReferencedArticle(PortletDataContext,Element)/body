{
  List<Element> referencedStagedModels=portletDataContext.getReferencedDataElements(parentElement,JournalArticle.class);
  if (referencedStagedModels.isEmpty()) {
    return;
  }
  for (  Element element : referencedStagedModels) {
    StagedModelDataHandlerUtil.importStagedModel(portletDataContext,element);
  }
}
