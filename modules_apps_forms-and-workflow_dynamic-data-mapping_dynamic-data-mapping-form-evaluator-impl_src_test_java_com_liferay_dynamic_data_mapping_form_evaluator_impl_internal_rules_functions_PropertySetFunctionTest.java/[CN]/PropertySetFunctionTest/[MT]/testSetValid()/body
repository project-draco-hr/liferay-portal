{
  DDMFormFieldEvaluationResult ddmFormFieldEvaluationResult=new DDMFormFieldEvaluationResult("field",StringUtil.randomString());
  PropertySetFunction propertySetFunction=new PropertySetFunction();
  propertySetFunction.evaluate(ddmFormFieldEvaluationResult,"valid",false,"This field is invalid.");
  Assert.assertFalse(ddmFormFieldEvaluationResult.isValid());
  Assert.assertEquals("This field is invalid.",ddmFormFieldEvaluationResult.getErrorMessage());
}
