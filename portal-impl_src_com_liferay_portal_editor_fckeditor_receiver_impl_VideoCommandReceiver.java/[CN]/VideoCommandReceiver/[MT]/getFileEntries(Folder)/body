{
  List<FileEntry> fileEntries=null;
  String[] videoMimeTypes=getVideoMimeTypes();
  if (videoMimeTypes != null) {
    fileEntries=DLAppServiceUtil.getFileEntries(folder.getRepositoryId(),folder.getFolderId(),videoMimeTypes);
  }
 else {
    fileEntries=DLAppServiceUtil.getFileEntries(folder.getRepositoryId(),folder.getFolderId());
  }
  List<FileEntry> generatedVideoEntries=new ArrayList<FileEntry>();
  for (  FileEntry fileEntry : fileEntries) {
    generatedVideoEntries.add(fileEntry);
  }
  return generatedVideoEntries;
}
