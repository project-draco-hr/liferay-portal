{
  RequestHeaderAutoLoginConfiguration requestHeaderAutoLoginConfiguration=getRequestHeaderAutoLoginConfiguration(companyId);
  if (requestHeaderAutoLoginConfiguration == null) {
    return false;
  }
  String[] hostsAllowedArray=StringUtil.split(requestHeaderAutoLoginConfiguration.authHostsAllowed());
  Set<String> hostsAllowed=new HashSet<>();
  for (int i=0; i < hostsAllowedArray.length; i++) {
    hostsAllowed.add(hostsAllowedArray[i]);
  }
  return SSOUtil.isAccessAllowed(request,hostsAllowed);
}
