{
  try {
    RequestHeaderAutoLoginConfiguration requestHeaderAutoLoginConfiguration=_settingsFactory.getSettings(RequestHeaderAutoLoginConfiguration.class,new CompanyServiceSettingsLocator(companyId,RequestHeaderAutoLoginConstants.SERVICE_NAME));
    return requestHeaderAutoLoginConfiguration;
  }
 catch (  SettingsException se) {
    _log.error("Unable to get request header auto login configuration",se);
  }
  return null;
}
