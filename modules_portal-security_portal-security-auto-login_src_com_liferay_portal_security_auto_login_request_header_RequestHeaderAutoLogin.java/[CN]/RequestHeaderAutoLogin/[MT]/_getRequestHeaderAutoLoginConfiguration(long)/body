{
  try {
    RequestHeaderAutoLoginConfiguration requestHeaderAutoLoginConfiguration=_configurationFactory.getConfiguration(RequestHeaderAutoLoginConfiguration.class,new CompanyServiceSettingsLocator(companyId,RequestHeaderAutoLoginConstants.SERVICE_NAME));
    return requestHeaderAutoLoginConfiguration;
  }
 catch (  ConfigurationException ce) {
    _log.error("Unable to get request header auto login configuration",ce);
  }
  return null;
}
