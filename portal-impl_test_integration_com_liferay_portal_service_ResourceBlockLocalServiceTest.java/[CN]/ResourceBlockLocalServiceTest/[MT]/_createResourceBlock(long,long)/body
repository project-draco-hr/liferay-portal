{
  Connection connection=DataAccess.getConnection();
  PreparedStatement preparedStatement=connection.prepareStatement("INSERT INTO ResourceBlock (resourceBlockId, referenceCount) " + "VALUES (?, ?)");
  preparedStatement.setLong(1,resourceBlockIdId);
  preparedStatement.setLong(2,referenceCount);
  int affectRows=preparedStatement.executeUpdate();
  Assert.assertEquals(1,affectRows);
  DataAccess.cleanUp(connection,preparedStatement);
}
