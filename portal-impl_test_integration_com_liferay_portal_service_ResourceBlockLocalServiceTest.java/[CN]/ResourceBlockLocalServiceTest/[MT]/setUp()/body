{
  Connection connection=DataAccess.getConnection();
  PreparedStatement preparedStatement=connection.prepareStatement("DELETE FROM ResourceBlock WHERE companyId = ? AND groupId " + "= ? AND name = ?");
  preparedStatement.setLong(1,_COMPANY_ID);
  preparedStatement.setLong(2,_GROUP_ID);
  preparedStatement.setString(3,_MODEL_NAME);
  preparedStatement.executeUpdate();
  DataAccess.cleanUp(connection,preparedStatement);
}
