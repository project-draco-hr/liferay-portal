{
  while (true) {
    try {
      ResourceBlockLocalServiceUtil.updateResourceBlockId(_COMPANY_ID,_GROUP_ID,_MODEL_NAME,_permissionedModel,_resourceBlockPermissionsContainer.getPermissionsHash(),_resourceBlockPermissionsContainer);
      if (_semaphore != null) {
        _semaphore.release();
      }
