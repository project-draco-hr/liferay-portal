{
  KaleoTask newKaleoTask=addKaleoTask();
  _persistence.clearCache();
  KaleoTask existingKaleoTask=_persistence.findByPrimaryKey(newKaleoTask.getPrimaryKey());
  Assert.assertEquals(existingKaleoTask.getKaleoNodeId(),ReflectionTestUtil.invoke(existingKaleoTask,"getOriginalKaleoNodeId",new Class<?>[0]));
}
