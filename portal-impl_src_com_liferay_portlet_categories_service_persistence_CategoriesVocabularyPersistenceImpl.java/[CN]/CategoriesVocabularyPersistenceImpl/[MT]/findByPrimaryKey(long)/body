{
  CategoriesVocabulary categoriesVocabulary=fetchByPrimaryKey(vocabularyId);
  if (categoriesVocabulary == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No CategoriesVocabulary exists with the primary key " + vocabularyId);
    }
    throw new NoSuchVocabularyException("No CategoriesVocabulary exists with the primary key " + vocabularyId);
  }
  return categoriesVocabulary;
}
