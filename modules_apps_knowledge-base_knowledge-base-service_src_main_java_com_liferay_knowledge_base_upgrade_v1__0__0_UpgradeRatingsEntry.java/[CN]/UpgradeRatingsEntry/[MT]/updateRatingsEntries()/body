{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    long classNameId=getClassNameId(_ARTICLE_CLASS_NAME);
    ps=con.prepareStatement("select entryId, score from RatingsEntry where classNameId = " + classNameId);
    rs=ps.executeQuery();
    while (rs.next()) {
      long entryId=rs.getLong("entryId");
      double score=rs.getDouble("score");
      StringBundler sb=new StringBundler(4);
      sb.append("update RatingsEntry set score = ");
      sb.append(score * 2);
      sb.append(" where entryId = ");
      sb.append(entryId);
      runSQL(sb.toString());
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
