{
  if (_permissionChecker.isCompanyAdmin(searchContext.getCompanyId())) {
    return true;
  }
  long[] groupIds=searchContext.getGroupIds();
  if (groupIds == null) {
    return false;
  }
  for (  long groupId : groupIds) {
    if (!_permissionChecker.isGroupAdmin(groupId)) {
      return false;
    }
  }
  return true;
}
