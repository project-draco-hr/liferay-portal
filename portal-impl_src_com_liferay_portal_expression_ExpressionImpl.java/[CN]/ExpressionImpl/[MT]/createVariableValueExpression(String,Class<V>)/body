{
  Expression<V> variableValueExpression=new ExpressionImpl<V>(expression,expressionType);
  List<String> variableNames=_variableNamesExtractor.extract(expression);
  for (  String variableName : variableNames) {
    Variable variable=_variablesMap.get(variableName);
    variableValueExpression.setVariableValue(variableName,variable.getType(),getVariableValue(variable));
  }
  return variableValueExpression;
}
