{
  VariableDependencies variableDependencies=variableDependenciesMap.get(variable.getName());
  if (variableDependencies != null) {
    return variableDependencies;
  }
  variableDependencies=new VariableDependencies(variable.getName());
  variableDependenciesMap.put(variable.getName(),variableDependencies);
  if (variable.getValueExpression() != null) {
    List<String> variableNames=_variableNamesExtractor.extract(variable.getValueExpression());
    for (    String variableName : variableNames) {
      VariableDependencies populateVariableDependencies=populateVariableDependenciesMap(_variablesMap.get(variableName),variableDependenciesMap);
      variableDependencies.addRequiredVariable(populateVariableDependencies.getVariableName());
      populateVariableDependencies.addAffectedVariable(variableDependencies.getVariableName());
    }
  }
  return variableDependencies;
}
