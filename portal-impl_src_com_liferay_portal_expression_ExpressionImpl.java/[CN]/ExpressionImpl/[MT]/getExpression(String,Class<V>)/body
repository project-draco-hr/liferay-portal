{
  Expression<V> expression=new ExpressionImpl<V>(expressionString,expressionType);
  List<String> variableNames=_variableNamesExtractor.extract(expressionString);
  for (  String variableName : variableNames) {
    Variable variable=_variables.get(variableName);
    expression.setVariableValue(variableName,getVariableValue(variable),variable.getVariableClass());
  }
  return expression;
}
