{
  com.liferay.portal.search.configuration.QueryPreProcessConfiguration queryPreProcessConfiguration=Configurable.createConfigurable(com.liferay.portal.search.configuration.QueryPreProcessConfiguration.class,properties);
  String[] fieldNamePatterns=queryPreProcessConfiguration.fieldNamePatterns();
  for (  String fieldNamePattern : fieldNamePatterns) {
    _fieldNamePatterns.put(fieldNamePattern,Pattern.compile(fieldNamePattern));
  }
}
