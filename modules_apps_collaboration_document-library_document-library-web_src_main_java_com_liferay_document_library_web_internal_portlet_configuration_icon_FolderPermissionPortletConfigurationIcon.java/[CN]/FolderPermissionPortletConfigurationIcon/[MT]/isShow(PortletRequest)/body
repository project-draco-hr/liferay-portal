{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    Folder folder=ActionUtil.getFolder(portletRequest);
    if (folder != null) {
      return DLFolderPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),folder.getFolderId(),ActionKeys.PERMISSIONS);
    }
  }
 catch (  Exception e) {
  }
  return false;
}
