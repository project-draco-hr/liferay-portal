{
  long difference=notificationEvent2.getDeliverBy() - notificationEvent1.getDeliverBy();
  int intDifference;
  if (difference > Integer.MAX_VALUE || notificationEvent2.getDeliverBy() == 0) {
    intDifference=Integer.MAX_VALUE;
  }
 else   if (difference < Integer.MIN_VALUE || notificationEvent1.getDeliverBy() == 0) {
    intDifference=Integer.MIN_VALUE;
  }
 else {
    intDifference=(int)difference;
  }
  return intDifference;
}
