{
  Set<String> names=getPropertySet("security-manager-properties-read");
  for (  String name : names) {
    Permission permission=new PropertyPermission(name,PROPERTY_PERMISSION_READ);
    _permissions.add(permission);
  }
  names=getPropertySet("security-manager-properties-write");
  for (  String name : names) {
    Permission permission=new PropertyPermission(name,PROPERTY_PERMISSION_WRITE);
    _permissions.add(permission);
  }
}
