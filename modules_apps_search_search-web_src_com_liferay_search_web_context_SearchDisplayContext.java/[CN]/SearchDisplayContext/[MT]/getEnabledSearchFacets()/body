{
  if (_enabledSearchFacets != null) {
    return _enabledSearchFacets;
  }
  _enabledSearchFacets=ListUtil.filter(_searchFacets,new PredicateFilter<SearchFacet>(){
    @Override public boolean filter(    SearchFacet searchFacet){
      return isDisplayFacet(searchFacet.getClassName());
    }
  }
);
  return _enabledSearchFacets;
}
