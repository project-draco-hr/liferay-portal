{
  JournalArticle journalArticle=JournalTestUtil.addArticle(_group.getGroupId(),JournalFolderConstants.DEFAULT_PARENT_FOLDER_ID);
  List<ResourcePermission> resourcePermissions=ResourcePermissionLocalServiceUtil.getResourcePermissions(journalArticle.getCompanyId(),JournalArticle.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,String.valueOf(journalArticle.getResourcePrimKey()));
  for (  ResourcePermission resourcePermission : resourcePermissions) {
    ResourcePermissionLocalServiceUtil.deleteResourcePermission(resourcePermission.getResourcePermissionId());
  }
  List<JournalArticle> journalArticles=JournalArticleLocalServiceUtil.getNoPermissionArticles();
  Assert.assertEquals(1,journalArticles.size());
  Assert.assertEquals(journalArticle,journalArticles.get(0));
}
