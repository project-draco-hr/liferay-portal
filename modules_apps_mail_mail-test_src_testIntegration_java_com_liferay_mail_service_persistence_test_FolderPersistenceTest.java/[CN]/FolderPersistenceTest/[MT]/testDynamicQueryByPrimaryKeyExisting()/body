{
  Folder newFolder=addFolder();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Folder.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("folderId",newFolder.getFolderId()));
  List<Folder> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Folder existingFolder=result.get(0);
  Assert.assertEquals(existingFolder,newFolder);
}
