{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select * from JournalArticle where approved = false");
    rs=ps.executeQuery();
    SmartResultSet srs=new SmartResultSet(rs);
    while (srs.next()) {
      long articleId=srs.getLong("resourcePrimKey");
      ps=con.prepareStatement("update TagsAsset set visible = false where classPK = ?");
      ps.setLong(1,articleId);
      ps.executeUpdate();
      ps.close();
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
