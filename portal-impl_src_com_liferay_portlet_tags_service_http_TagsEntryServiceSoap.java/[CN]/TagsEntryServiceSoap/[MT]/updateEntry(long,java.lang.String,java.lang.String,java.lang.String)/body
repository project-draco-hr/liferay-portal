{
  try {
    com.liferay.portlet.tags.model.TagsEntry returnValue=TagsEntryServiceUtil.updateEntry(entryId,parentEntryName,name,vocabularyName);
    return com.liferay.portlet.tags.model.TagsEntrySoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
