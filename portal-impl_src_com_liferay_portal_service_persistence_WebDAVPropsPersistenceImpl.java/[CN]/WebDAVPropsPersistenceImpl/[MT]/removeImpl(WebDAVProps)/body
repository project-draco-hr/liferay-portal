{
  Session session=null;
  try {
    session=openSession();
    session.delete(webDAVProps);
    session.flush();
    return webDAVProps;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(WebDAVProps.class.getName());
  }
}
