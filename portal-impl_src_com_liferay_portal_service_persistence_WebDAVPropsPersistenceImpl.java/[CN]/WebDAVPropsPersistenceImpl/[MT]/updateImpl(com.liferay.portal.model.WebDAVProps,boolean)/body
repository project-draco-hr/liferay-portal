{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,webDAVProps,merge);
    webDAVProps.setNew(false);
    return webDAVProps;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(WebDAVProps.class.getName());
  }
}
