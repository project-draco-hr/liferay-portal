{
  Session session=null;
  try {
    session=openSession();
    WebDAVProps webDAVProps=(WebDAVProps)session.get(WebDAVPropsImpl.class,new Long(webDavPropsId));
    if (webDAVProps == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No WebDAVProps exists with the primary key " + webDavPropsId);
      }
      throw new NoSuchWebDAVPropsException("No WebDAVProps exists with the primary key " + webDavPropsId);
    }
    return remove(webDAVProps);
  }
 catch (  NoSuchWebDAVPropsException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
