{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace("Asset Publisher Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("//section"));
  assertEquals(RuntimeVariables.replace("Asset Publisher"),selenium.getText("//span[@class='portlet-title-text']"));
  assertEquals(RuntimeVariables.replace("Web Content Name"),selenium.getText("//h3[@class='asset-title']/a"));
  assertTrue(selenium.isPartialText("//div[@class='asset-more']/a","Read More"));
  selenium.clickAt("//div[@class='asset-more']/a",RuntimeVariables.replace("Read More"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Web Content Name"),selenium.getText("//h1[@class='header-title']/span"));
  assertEquals(RuntimeVariables.replace("Web Content Content"),selenium.getText("//div[@class='asset-content']/div"));
}
