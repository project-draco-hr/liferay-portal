{
  AtomEntryContent atomEntryContent=new AtomEntryContent(AtomEntryContent.Type.MEDIA);
  atomEntryContent.setMimeType(dlFileEntry.getMimeType());
  String srcLink=AtomUtil.createEntryLink(atomRequestContext,_COLLECTION_NAME,dlFileEntry.getFileEntryId() + "?media");
  atomEntryContent.setSrcLink(srcLink);
  return atomEntryContent;
}
