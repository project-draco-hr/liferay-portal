{
  _validate(args);
  Properties databaseProperties=PropsReader.read(args[0]);
  ShardExporter shardExporter=ShardExporterFactory.getShardExporter(databaseProperties);
  String schema=args[1];
  List<Long> companyIds=Arrays.asList(new Long[]{Long.parseLong(args[2])});
  String outputFolder=args[3];
  ExportContext exportContext=new ExportContext(companyIds,outputFolder,schema);
  shardExporter.export(exportContext);
}
