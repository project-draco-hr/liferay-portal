{
  entityCache.putResult(DDLRecordSetModelImpl.ENTITY_CACHE_ENABLED,DDLRecordSetImpl.class,ddlRecordSet.getPrimaryKey(),ddlRecordSet);
  finderCache.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddlRecordSet.getUuid(),ddlRecordSet.getGroupId()},ddlRecordSet);
  finderCache.putResult(FINDER_PATH_FETCH_BY_G_R,new Object[]{ddlRecordSet.getGroupId(),ddlRecordSet.getRecordSetKey()},ddlRecordSet);
  ddlRecordSet.resetOriginalValues();
}
