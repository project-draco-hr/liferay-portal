{
  if (ListUtil.isEmpty(exclusionFiles)) {
    return false;
  }
  String absolutePathWithJavaTermName=null;
  if (Validator.isNotNull(javaTermName)) {
    absolutePathWithJavaTermName=absolutePath + StringPool.AT + javaTermName;
  }
  String absolutePathWithLineCount=null;
  if (lineCount > 0) {
    absolutePathWithLineCount=absolutePath + StringPool.AT + lineCount;
  }
  for (  String exclusionFile : exclusionFiles) {
    if (absolutePath.endsWith(exclusionFile) || ((absolutePathWithJavaTermName != null) && absolutePathWithJavaTermName.endsWith(exclusionFile)) || ((absolutePathWithLineCount != null) && absolutePathWithLineCount.endsWith(exclusionFile))) {
      return true;
    }
  }
  return false;
}
