{
  script=getGlobalScript() + script;
  PortletConfig portletConfig=getPortletConfig();
  PortletContext portletContext=getPortletContext();
  Map<String,Object> portletObjects=ScriptingUtil.getPortletObjects(portletConfig,portletContext,portletRequest,portletResponse);
  ScriptingUtil.exec(null,portletObjects,language,script);
}
