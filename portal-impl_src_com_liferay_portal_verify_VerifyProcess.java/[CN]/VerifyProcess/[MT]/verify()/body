{
  long start=System.currentTimeMillis();
  try {
    if (_log.isInfoEnabled()) {
      _log.info("Verifying " + getClass().getName());
    }
    doVerify();
  }
 catch (  Exception e) {
    throw new VerifyException(e);
  }
 finally {
    if (_log.isInfoEnabled()) {
      _log.info("Completed verification: " + getClass().getName() + " in "+ (System.currentTimeMillis() - start)+ "ms");
    }
  }
}
