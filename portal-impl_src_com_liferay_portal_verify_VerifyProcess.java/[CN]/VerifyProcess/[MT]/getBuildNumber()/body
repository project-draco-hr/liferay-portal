{
  String sql="select buildNumber from Release_ where servletContextName = ?";
  try (Connection con=DataAccess.getUpgradeOptimizedConnection();PreparedStatement ps=con.prepareStatement(sql)){
    ps.setString(1,ReleaseConstants.DEFAULT_SERVLET_CONTEXT_NAME);
    try (ResultSet rs=ps.executeQuery()){
      rs.next();
      return rs.getInt(1);
    }
   }
 }
