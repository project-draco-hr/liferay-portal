{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select buildNumber from Release_ where servletContextName " + "= ?");
    ps.setString(1,ReleaseConstants.DEFAULT_SERVLET_CONTEXT_NAME);
    rs=ps.executeQuery();
    rs.next();
    return rs.getInt(1);
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
