{
  if (!PropsValues.LAYOUT_USER_PRIVATE_LAYOUTS_ENABLED && !PropsValues.LAYOUT_USER_PUBLIC_LAYOUTS_ENABLED) {
    return true;
  }
  if ((PropsValues.LAYOUT_USER_PRIVATE_LAYOUTS_POWER_USER_REQUIRED || PropsValues.LAYOUT_USER_PUBLIC_LAYOUTS_POWER_USER_REQUIRED) && !_roleLocalService.hasUserRole(permissionChecker.getUserId(),permissionChecker.getCompanyId(),RoleConstants.POWER_USER,true)) {
    return true;
  }
  return super.hasAccessPermissionDenied(permissionChecker,group,portlet);
}
