{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,tagsProperty,merge);
    tagsProperty.setNew(false);
    return tagsProperty;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(TagsProperty.class.getName());
  }
}
