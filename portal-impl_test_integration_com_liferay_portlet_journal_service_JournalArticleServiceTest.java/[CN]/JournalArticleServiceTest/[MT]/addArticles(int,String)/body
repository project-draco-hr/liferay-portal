{
  List<JournalArticle> articles=new ArrayList<JournalArticle>(count);
  for (int i=0; i < count; i++) {
    JournalArticle article=JournalTestUtil.addArticle(_group.getGroupId(),JournalFolderConstants.DEFAULT_PARENT_FOLDER_ID,ServiceTestUtil.randomString(),content);
    articles.add(article);
  }
  return articles;
}
