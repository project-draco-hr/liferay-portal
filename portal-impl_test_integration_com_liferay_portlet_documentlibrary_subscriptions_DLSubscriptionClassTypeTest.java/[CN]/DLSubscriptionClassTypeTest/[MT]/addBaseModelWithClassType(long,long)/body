{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId(),TestPropsValues.getUserId());
  DLAppTestUtil.populateNotificationsServiceContext(serviceContext,Constants.ADD);
  DLAppTestUtil.populateServiceContext(serviceContext,classTypeId);
  FileEntry fileEntry=DLAppLocalServiceUtil.addFileEntry(TestPropsValues.getUserId(),group.getGroupId(),containerModelId,RandomTestUtil.randomString() + ".txt",ContentTypes.TEXT_PLAIN,RandomTestUtil.randomBytes(),serviceContext);
  return fileEntry.getFileEntryId();
}
