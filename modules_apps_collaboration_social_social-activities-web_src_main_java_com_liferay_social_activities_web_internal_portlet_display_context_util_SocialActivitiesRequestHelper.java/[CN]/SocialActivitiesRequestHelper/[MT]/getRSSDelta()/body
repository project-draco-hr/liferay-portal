{
  if (_rssDelta != null) {
    return _rssDelta;
  }
  LiferayPortletRequest liferayPortletRequest=getLiferayPortletRequest();
  PortletPreferences preferences=liferayPortletRequest.getPreferences();
  _rssDelta=GetterUtil.getInteger(preferences.getValue("rssDelta",StringPool.BLANK),SearchContainer.DEFAULT_DELTA);
  return _rssDelta;
}
