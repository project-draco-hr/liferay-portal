{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  DDLRecordVersion newDDLRecordVersion=addDDLRecordVersion();
  _persistence.clearCache();
  DDLRecordVersionModelImpl existingDDLRecordVersionModelImpl=(DDLRecordVersionModelImpl)_persistence.findByPrimaryKey(newDDLRecordVersion.getPrimaryKey());
  Assert.assertEquals(existingDDLRecordVersionModelImpl.getRecordId(),existingDDLRecordVersionModelImpl.getOriginalRecordId());
  Assert.assertTrue(Validator.equals(existingDDLRecordVersionModelImpl.getVersion(),existingDDLRecordVersionModelImpl.getOriginalVersion()));
}
