{
  BlogsItemSelectorViewDisplayContext blogsItemSelectorViewDisplayContext=new BlogsItemSelectorViewDisplayContext(blogsItemSelectorCriterion,this,itemSelectedEventName,portletURL);
  request.setAttribute(BLOGS_ITEM_SELECTOR_VIEW_DISPLAY_CONTEXT,blogsItemSelectorViewDisplayContext);
  RequestDispatcher requestDispatcher=request.getRequestDispatcher("/o/blogs-item-selector-web/blogs_attachments.jsp");
  requestDispatcher.include(request,response);
}
