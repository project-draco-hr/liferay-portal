{
  request.setAttribute(ITEM_SELECTED_EVENT_NAME,itemSelectedEventName);
  request.setAttribute(PORTLET_URL,portletURL);
  request.setAttribute(BLOGS_ITEM_SELECTOR_CRITERION,blogsItemSelectorCriterion);
  RequestDispatcher requestDispatcher=request.getRequestDispatcher("/o/blogs-item-selector-web/blogs_attachments.jsp");
  requestDispatcher.include(request,response);
}
