{
  LayoutFriendlyURL layoutFriendlyURL=(LayoutFriendlyURL)EntityCacheUtil.getResult(LayoutFriendlyURLModelImpl.ENTITY_CACHE_ENABLED,LayoutFriendlyURLImpl.class,primaryKey);
  if (layoutFriendlyURL == _nullLayoutFriendlyURL) {
    return null;
  }
  if (layoutFriendlyURL == null) {
    Session session=null;
    try {
      session=openSession();
      layoutFriendlyURL=(LayoutFriendlyURL)session.get(LayoutFriendlyURLImpl.class,primaryKey);
      if (layoutFriendlyURL != null) {
        cacheResult(layoutFriendlyURL);
      }
 else {
        EntityCacheUtil.putResult(LayoutFriendlyURLModelImpl.ENTITY_CACHE_ENABLED,LayoutFriendlyURLImpl.class,primaryKey,_nullLayoutFriendlyURL);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(LayoutFriendlyURLModelImpl.ENTITY_CACHE_ENABLED,LayoutFriendlyURLImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return layoutFriendlyURL;
}
