{
  RestExtenderConfiguration restExtenderConfiguration=getRestExtenderConfiguration();
  String[] applicationFilters=restExtenderConfiguration.jaxRsApplicationFilterStrings();
  if (applicationFilters != null) {
    for (    String applicationFilter : applicationFilters) {
      addTCCLDependency(false,Application.class,applicationFilter,"addApplication","removeApplication");
    }
  }
}
