{
  _restExtenderConfiguration=Configurable.createConfigurable(RestExtenderConfiguration.class,properties);
  _dependencyManager=new TCCLDependencyManager(bundleContext);
  _component=_dependencyManager.createComponent();
  CXFJaxRsServiceRegistrator cxfJaxRsServiceRegistrator=new CXFJaxRsServiceRegistrator(properties);
  _component.setImplementation(cxfJaxRsServiceRegistrator);
  addBusDependencies();
  addJaxRsApplicationDependencies();
  addJaxRsProviderServiceDependencies();
  addJaxRsServiceDependencies();
  _dependencyManager.add(_component);
  _component.start();
}
