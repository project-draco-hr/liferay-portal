{
  String[] contextPathStrings=getRestExtenderConfiguration().contextPaths();
  if (contextPathStrings != null) {
    for (    String contextPath : contextPathStrings) {
      addTCCLDependency(true,Bus.class,"(" + HttpWhiteboardConstants.HTTP_WHITEBOARD_CONTEXT_PATH + "="+ contextPath+ ")","addBus","removeBus");
    }
  }
}
