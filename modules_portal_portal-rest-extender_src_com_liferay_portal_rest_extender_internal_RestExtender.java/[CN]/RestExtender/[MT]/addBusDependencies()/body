{
  RestExtenderConfiguration restExtenderConfiguration=getRestExtenderConfiguration();
  String[] contextPaths=restExtenderConfiguration.contextPaths();
  if (contextPaths == null) {
    return;
  }
  for (  String contextPath : contextPaths) {
    addTCCLServiceDependency(true,Bus.class,"(" + HttpWhiteboardConstants.HTTP_WHITEBOARD_CONTEXT_PATH + "="+ contextPath+ ")","addBus","removeBus");
  }
}
