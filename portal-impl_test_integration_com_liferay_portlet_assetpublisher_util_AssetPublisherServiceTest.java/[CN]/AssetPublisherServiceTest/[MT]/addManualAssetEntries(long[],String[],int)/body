{
  List<AssetEntry> assetEntries=new ArrayList<AssetEntry>();
  for (int i=0; i < number; i++) {
    JournalArticle article=JournalTestUtil.addArticle(TestPropsValues.getGroupId(),ServiceTestUtil.randomString(),ServiceTestUtil.randomString(100));
    JournalArticleLocalServiceUtil.updateAsset(TestPropsValues.getUserId(),article,categoryIds,tagNames,null);
    AssetEntry assetEntry=AssetEntryLocalServiceUtil.getEntry(JournalArticle.class.getName(),article.getResourcePrimKey());
    assetEntries.add(assetEntry);
    _formatXml(assetEntry);
  }
  return assetEntries;
}
