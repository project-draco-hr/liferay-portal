{
  addVocabulary();
  long[] allCategoryIds=new long[]{_categoryIds[0],_categoryIds[1],_categoryIds[2]};
  _expectedAssetEntries=addAssetEntries(allCategoryIds,_NO_TAG_NAMES,2,true);
  List<AssetEntry> assetEntries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},_assetEntryXmls,false,false);
  Assert.assertEquals(7,assetEntries.size());
  List<AssetEntry> filteredAsssetEntries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},allCategoryIds,_assetEntryXmls,_NO_TAG_NAMES,false,false);
  Assert.assertEquals(_expectedAssetEntries,filteredAsssetEntries);
}
