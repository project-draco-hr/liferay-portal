{
  addVocabulary();
  long[] allAssetCategoryIds=new long[]{_categoryIds[0],_categoryIds[1],_categoryIds[2]};
  _expectedEntries=addManualAssetEntries(allAssetCategoryIds,_NO_TAGS,2);
  List<AssetEntry> entries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},_assetEntryXmls,false,false);
  Assert.assertEquals(7,entries.size());
  List<AssetEntry> filteredEntries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},allAssetCategoryIds,_assetEntryXmls,_NO_TAGS,false,false);
  Assert.assertEquals(2,filteredEntries.size());
  Assert.assertEquals(_expectedEntries,filteredEntries);
}
