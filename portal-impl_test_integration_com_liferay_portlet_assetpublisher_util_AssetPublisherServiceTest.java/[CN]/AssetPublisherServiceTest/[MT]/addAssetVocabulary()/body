{
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext(TestPropsValues.getGroupId());
  serviceContext.setAddGroupPermissions(false);
  serviceContext.setAddGuestPermissions(false);
  AssetVocabulary assetVocabulary=AssetVocabularyLocalServiceUtil.addVocabulary(TestPropsValues.getUserId(),ServiceTestUtil.randomString(),ServiceTestUtil.getServiceContext(TestPropsValues.getGroupId()));
  addAssetCategories(assetVocabulary.getVocabularyId());
}
