{
  String[] allAssetTagNames=new String[]{_TAG_NAMES[0],_TAG_NAMES[1]};
  _expectedEntries=addManualAssetEntries(_NO_CATEGORY_IDS,allAssetTagNames,2);
  List<AssetEntry> entries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},_assetEntryXmls,false,false);
  Assert.assertEquals(7,entries.size());
  List<AssetEntry> filteredEntries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},_NO_CATEGORY_IDS,_assetEntryXmls,allAssetTagNames,false,false);
  Assert.assertEquals(2,filteredEntries.size());
  Assert.assertEquals(_expectedEntries,filteredEntries);
}
