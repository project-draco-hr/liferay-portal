{
  String[] allTagNames=new String[]{_TAG_NAMES[0],_TAG_NAMES[1]};
  _expectedAssetEntries=addAssetEntries(_NO_CATEGORY_IDS,allTagNames,2,true);
  List<AssetEntry> assetEntries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},_assetEntryXmls,false,false);
  Assert.assertEquals(7,assetEntries.size());
  List<AssetEntry> filteredAssetEntries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},_NO_CATEGORY_IDS,_assetEntryXmls,allTagNames,false,false);
  Assert.assertEquals(_expectedAssetEntries,filteredAssetEntries);
}
