{
  addVocabulary();
  long[] allAssetCategoyIds=new long[]{_categoryIds[0],_categoryIds[1],_categoryIds[2],_categoryIds[3]};
  String[] allAssetTagNames=new String[]{_tagNames[0],_tagNames[1]};
  _expectedEntries=addManualAssetEntries(allAssetCategoyIds,allAssetTagNames,2);
  List<AssetEntry> entries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},_assetEntryXmls,false,false);
  Assert.assertEquals(7,entries.size());
  List<AssetEntry> filteredEntries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},allAssetCategoyIds,_assetEntryXmls,allAssetTagNames,false,false);
  Assert.assertEquals(2,filteredEntries.size());
  Assert.assertEquals(_expectedEntries,filteredEntries);
}
