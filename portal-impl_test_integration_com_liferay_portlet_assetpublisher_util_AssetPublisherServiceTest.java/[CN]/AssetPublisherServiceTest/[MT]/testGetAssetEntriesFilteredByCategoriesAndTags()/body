{
  addVocabulary();
  long[] allCategoyIds={_assetCategoryIds[0],_assetCategoryIds[1],_assetCategoryIds[2],_assetCategoryIds[3]};
  String[] allAssetTagNames={_ASSET_TAG_NAMES[0],_ASSET_TAG_NAMES[1]};
  List<AssetEntry> expectedAssetEntries=addAssetEntries(allCategoyIds,allAssetTagNames,2,true);
  List<AssetEntry> assetEntries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},_assetEntryXmls,false,false);
  Assert.assertEquals(_initialAssetEntries.size() + expectedAssetEntries.size(),assetEntries.size());
  List<AssetEntry> filteredAssetEntries=AssetPublisherUtil.getAssetEntries(new MockPortletRequest(),new MockPortletPreferences(),_permissionChecker,new long[]{TestPropsValues.getGroupId()},allCategoyIds,_assetEntryXmls,allAssetTagNames,false,false);
  Assert.assertEquals(expectedAssetEntries,filteredAssetEntries);
}
