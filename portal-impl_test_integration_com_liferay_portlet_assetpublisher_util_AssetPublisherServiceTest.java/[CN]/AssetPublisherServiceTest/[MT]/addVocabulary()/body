{
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext(TestPropsValues.getGroupId());
  serviceContext.setAddGroupPermissions(false);
  serviceContext.setAddGuestPermissions(false);
  AssetVocabulary vocabulary=AssetVocabularyLocalServiceUtil.addVocabulary(TestPropsValues.getUserId(),ServiceTestUtil.randomString(),ServiceTestUtil.getServiceContext(TestPropsValues.getGroupId()));
  addAssetCategories(vocabulary.getVocabularyId());
}
