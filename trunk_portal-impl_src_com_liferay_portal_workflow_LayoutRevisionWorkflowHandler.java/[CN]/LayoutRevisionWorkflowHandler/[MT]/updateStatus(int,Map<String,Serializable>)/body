{
  long userId=GetterUtil.getLong((String)workflowContext.get(WorkflowConstants.CONTEXT_USER_ID));
  long layoutRevisionId=GetterUtil.getLong((String)workflowContext.get(WorkflowConstants.CONTEXT_ENTRY_CLASS_PK));
  ServiceContext serviceContext=(ServiceContext)workflowContext.get("serviceContext");
  return LayoutRevisionLocalServiceUtil.updateStatus(userId,layoutRevisionId,status,serviceContext);
}
