{
  PortletURL portletURL=liferayPortletResponse.createRenderURL(PortletKeys.IMAGE_GALLERY);
  portletURL.setWindowState(WindowState.MAXIMIZED);
  portletURL.setParameter("struts_action","/image_gallery/view");
  portletURL.setParameter("groupId",String.valueOf(_image.getGroupId()));
  portletURL.setParameter("folderId",String.valueOf(_image.getFolderId()));
  return portletURL.toString();
}
