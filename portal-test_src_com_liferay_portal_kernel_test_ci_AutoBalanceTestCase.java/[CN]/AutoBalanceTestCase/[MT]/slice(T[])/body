{
  int groupSize=array.length / testClassGroupsSize;
  if ((array.length % testClassGroupsSize) != 0) {
    groupSize++;
  }
  int start=groupSize * testClassGroupIndex;
  int end=start + groupSize;
  if (end > array.length) {
    end=array.length;
  }
  return ArrayUtil.subset(array,start,end);
}
