{
  try {
    ActionUtil.getLayoutSetPrototype(renderRequest);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchLayoutSetPrototypeException || e instanceof PrincipalException) {
      SessionErrors.add(renderRequest,e.getClass().getName());
      return mapping.findForward("portlet.layout_set_prototypes.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward(getForward(renderRequest,"portlet.layout_set_prototypes.edit_layout_set_prototype"));
}
