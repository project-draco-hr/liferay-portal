{
  String amazonAccessKeyId=GetterUtil.getString(PropsUtil.get(PropsKeys.AMAZON_ACCESS_KEY_ID));
  String amazonAssociateTag=GetterUtil.getString(PropsUtil.get(PropsKeys.AMAZON_ASSOCIATE_TAG));
  String amazonSecretAccesKey=GetterUtil.getString(PropsUtil.get(PropsKeys.AMAZON_SECRET_ACCESS_KEY));
  if (Validator.isNull(amazonAccessKeyId) || Validator.isNull(amazonAssociateTag) || Validator.isNull(amazonSecretAccesKey)) {
    return false;
  }
  return true;
}
