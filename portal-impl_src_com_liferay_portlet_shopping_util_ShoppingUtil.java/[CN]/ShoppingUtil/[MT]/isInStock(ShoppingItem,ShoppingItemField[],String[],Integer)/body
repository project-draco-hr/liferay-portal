{
  if (!item.isFields()) {
    if (item.getStockQuantity() > 0) {
      return true;
    }
 else {
      return false;
    }
  }
 else {
    int rowPos=getFieldsQuantitiesPos(item,itemFields,fieldsArray);
    String[] fieldsQuantities=item.getFieldsQuantitiesArray();
    int quantityInStock=GetterUtil.getInteger(fieldsQuantities[rowPos]);
    try {
      if ((quantityInStock > 0) && (quantityInStock >= quantityOrdered.intValue())) {
        return true;
      }
    }
 catch (    Exception e) {
    }
    return false;
  }
}
