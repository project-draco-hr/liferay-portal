{
  super(renderRequest,new CouponDisplayTerms(renderRequest),new CouponSearchTerms(renderRequest),DEFAULT_CUR_PARAM,DEFAULT_DELTA,iteratorURL,headerNames,EMPTY_RESULTS_MESSAGE);
  CouponDisplayTerms displayTerms=(CouponDisplayTerms)getDisplayTerms();
  iteratorURL.setParameter(CouponDisplayTerms.CODE,displayTerms.getCode());
  iteratorURL.setParameter(CouponDisplayTerms.DISCOUNT_TYPE,displayTerms.getDiscountType());
  iteratorURL.setParameter(CouponDisplayTerms.ACTIVE,String.valueOf(displayTerms.isActive()));
}
