{
  List<ObjectValuePair<String,InputStream>> inputStreams=new ArrayList<ObjectValuePair<String,InputStream>>(_files.size());
  for (  ObjectValuePair<String,byte[]> ovp : _files) {
    String key=ovp.getKey();
    byte[] bytes=ovp.getValue();
    ByteArrayInputStream bais=new ByteArrayInputStream(bytes);
    inputStreams.add(new ObjectValuePair<String,InputStream>(key,bais));
  }
  return inputStreams;
}
