{
  ResultRow resultRow=(ResultRow)_liferayPortletRequest.getAttribute(WebKeys.SEARCH_CONTAINER_RESULT_ROW);
  if (Validator.isNotNull(resultRow)) {
    return (WorkflowTask)resultRow.getParameter("workflowTask");
  }
 else {
    return (WorkflowTask)_liferayPortletRequest.getAttribute(WebKeys.WORKFLOW_TASK);
  }
}
