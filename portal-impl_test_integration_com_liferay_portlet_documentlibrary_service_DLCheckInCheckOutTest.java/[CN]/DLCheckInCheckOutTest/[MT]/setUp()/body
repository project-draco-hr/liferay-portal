{
  _group=GroupTestUtil.addGroup();
  RoleTestUtil.addResourcePermission(RoleConstants.POWER_USER,DLFolderConstants.getClassName(),ResourceConstants.SCOPE_GROUP_TEMPLATE,String.valueOf(GroupConstants.DEFAULT_PARENT_GROUP_ID),ActionKeys.ADD_DOCUMENT);
  _authorUser=UserTestUtil.addUser("author",_group.getGroupId());
