{
  Role role=ServiceTestUtil.addRole("Overrider",RoleConstants.TYPE_REGULAR,DLFileEntryConstants.getClassName(),ResourceConstants.SCOPE_GROUP_TEMPLATE,String.valueOf(GroupConstants.DEFAULT_PARENT_GROUP_ID),ActionKeys.OVERRIDE_CHECKOUT);
  try {
    UserLocalServiceUtil.setRoleUsers(role.getRoleId(),new long[]{_overriderUser.getUserId()});
    overrideCheckout(_authorUser,_overriderUser,true);
  }
  finally {
    RoleLocalServiceUtil.deleteRole(role.getRoleId());
  }
}
