{
  String newContent=_TEST_CONTENT + "\n" + System.currentTimeMillis();
  InputStream inputStream=new UnsyncByteArrayInputStream(newContent.getBytes());
  return DLAppServiceUtil.updateFileEntry(fileEntryId,"test1.txt",ContentTypes.TEXT_PLAIN,"test1.txt",null,null,false,inputStream,newContent.length(),serviceContext);
}
