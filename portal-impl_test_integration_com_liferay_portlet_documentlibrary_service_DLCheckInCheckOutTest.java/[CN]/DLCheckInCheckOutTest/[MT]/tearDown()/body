{
  DLAppServiceUtil.deleteFolder(_folder.getFolderId());
  RoleTestUtil.removeResourcePermission(RoleConstants.GUEST,DLPermission.RESOURCE_NAME,ResourceConstants.SCOPE_GROUP,String.valueOf(_group.getGroupId()),ActionKeys.VIEW);
  UserLocalServiceUtil.deleteUser(_authorUser.getUserId());
  UserLocalServiceUtil.deleteUser(_overriderUser.getUserId());
}
