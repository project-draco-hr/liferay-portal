{
  try {
    String xml=getUserTasksCountXml(instanceId,taskName,definitionName,assignedTo,createDateGT,createDateLT,startDateGT,startDateLT,endDateGT,endDateLT,hideEndedTasks,andOperator);
    return parseInt(xml,"count");
  }
 catch (  Exception e) {
    throw new WorkflowComponentException(e);
  }
}
