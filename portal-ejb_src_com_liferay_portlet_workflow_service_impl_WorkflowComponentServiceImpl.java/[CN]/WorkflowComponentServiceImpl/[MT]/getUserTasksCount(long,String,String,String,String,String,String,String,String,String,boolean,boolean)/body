{
  try {
    String xml=getUserTasksCountXml(instanceId,taskName,definitionName,assignedTo,gtCreateDate,ltCreateDate,gtStartDate,ltStartDate,gtEndDate,ltEndDate,hideEndedTasks,andOperator);
    return parseInt(xml,"count");
  }
 catch (  Exception e) {
    throw new WorkflowComponentException(e);
  }
}
