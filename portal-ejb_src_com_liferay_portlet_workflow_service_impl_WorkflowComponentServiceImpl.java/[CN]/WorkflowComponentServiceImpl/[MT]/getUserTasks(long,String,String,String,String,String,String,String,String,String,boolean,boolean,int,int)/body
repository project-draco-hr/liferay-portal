{
  try {
    String xml=getUserTasksXml(instanceId,taskName,workflowName,assignedTo,gtCreateDate,ltCreateDate,gtStartDate,ltStartDate,gtEndDate,ltEndDate,hideEndedTasks,andOperator,begin,end);
    return parseList(xml,"tasks");
  }
 catch (  Exception e) {
    throw new WorkflowComponentException(e);
  }
}
