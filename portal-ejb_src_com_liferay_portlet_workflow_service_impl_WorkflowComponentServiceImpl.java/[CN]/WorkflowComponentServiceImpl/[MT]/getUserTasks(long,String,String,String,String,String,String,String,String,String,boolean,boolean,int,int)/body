{
  try {
    String xml=getUserTasksXml(instanceId,taskName,definitionName,assignedTo,createDateGT,createDateLT,startDateGT,startDateLT,endDateGT,endDateLT,hideEndedTasks,andOperator,begin,end);
    return parseList(xml,"tasks");
  }
 catch (  Exception e) {
    throw new WorkflowComponentException(e);
  }
}
