{
  try {
    String xml=getInstancesXml(definitionId,instanceId,definitionName,definitionVersion,startDateGT,startDateLT,endDateGT,endDateLT,hideEndedTasks,andOperator,begin,end);
    return parseList(xml,"instances");
  }
 catch (  Exception e) {
    throw new WorkflowComponentException(e);
  }
}
