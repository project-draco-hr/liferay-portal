{
  _dictionary=CollectionFactory.getHashSet(150000);
  String[] dictionaryWords=StringUtil.split(ContentUtil.get("com/liferay/portlet/words/dependencies/words.txt"),"\n");
  for (int i=0; i < dictionaryWords.length; i++) {
    _dictionary.add(dictionaryWords[i]);
  }
  try {
    _spellDictionary=new SpellDictionaryHashMap();
    String[] dics=new String[]{"center.dic","centre.dic","color.dic","colour.dic","eng_com.dic","english.0","english.1","ise.dic","ize.dic","labeled.dic","labelled.dic","yse.dic","yze.dic"};
    for (int i=0; i < dics.length; i++) {
      _spellDictionary.addDictionary(new StringReader(ContentUtil.get("com/liferay/portlet/words/dependencies/" + dics[i])));
    }
  }
 catch (  IOException ioe) {
    _log.error(ioe);
  }
}
