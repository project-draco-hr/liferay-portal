{
  if (TouchUtil.class.getClassLoader() == ClassLoader.getSystemClassLoader()) {
    ProjectData projectData=ProjectDataUtil.getProjectData();
    ClassData classData=projectData.getOrCreateClassData(className);
    classData.touchJump(lineNumber,branchNumber,branch,1);
    return;
  }
  try {
    _touchJumpMethod.invoke(null,className,lineNumber,branchNumber,branch);
  }
 catch (  InvocationTargetException ite) {
    _throwException(ite.getCause());
  }
catch (  ReflectiveOperationException roe) {
    _throwException(roe);
  }
}
