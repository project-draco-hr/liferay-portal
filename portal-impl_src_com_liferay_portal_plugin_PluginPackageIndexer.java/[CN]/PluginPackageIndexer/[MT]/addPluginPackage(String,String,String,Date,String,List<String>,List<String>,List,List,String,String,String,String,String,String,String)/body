{
  Document doc=getAddPluginPackageDocument(moduleId,name,version,modifiedDate,author,types,tags,licenses,liferayVersions,shortDescription,longDescription,changeLog,pageURL,repositoryURL,status,installedVersion);
  IndexWriter writer=null;
  try {
    writer=LuceneUtil.getWriter(CompanyConstants.SYSTEM);
    writer.addDocument(doc);
  }
  finally {
    if (writer != null) {
      LuceneUtil.write(CompanyConstants.SYSTEM);
    }
  }
}
