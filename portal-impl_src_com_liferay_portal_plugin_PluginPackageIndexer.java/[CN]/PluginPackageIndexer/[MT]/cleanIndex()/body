{
synchronized (PluginPackageIndexer.class) {
    IndexReader reader=null;
    try {
      reader=LuceneUtil.getReader(CompanyImpl.SYSTEM);
      reader.deleteDocuments(new Term(LuceneFields.PORTLET_ID,PluginPackageIndexer.PORTLET_ID));
    }
  finally {
      if (reader != null) {
        reader.close();
      }
    }
  }
}
