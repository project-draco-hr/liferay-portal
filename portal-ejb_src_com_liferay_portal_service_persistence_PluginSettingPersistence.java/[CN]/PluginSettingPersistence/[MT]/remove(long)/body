{
  Session session=null;
  try {
    session=openSession();
    PluginSetting pluginSetting=(PluginSetting)session.get(PluginSettingImpl.class,new Long(pluginSettingId));
    if (pluginSetting == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No PluginSetting exists with the primary key " + pluginSettingId);
      }
      throw new NoSuchPluginSettingException("No PluginSetting exists with the primary key " + pluginSettingId);
    }
    return remove(pluginSetting);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
