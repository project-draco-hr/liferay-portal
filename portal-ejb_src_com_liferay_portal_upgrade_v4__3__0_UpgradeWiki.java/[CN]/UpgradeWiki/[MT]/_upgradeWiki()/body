{
  PKUpgradeColumnImpl pkUpgradeColumn=new PKUpgradeColumnImpl(0,true);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(WikiNodeImpl.TABLE_NAME,WikiNodeImpl.TABLE_COLUMNS,pkUpgradeColumn);
  upgradeTable.updateTable();
  _nodeIdMapper=pkUpgradeColumn.getValueMapper();
  upgradeTable=new DefaultUpgradeTableImpl(WikiPageImpl.TABLE_NAME,WikiPageImpl.TABLE_COLUMNS,new SwapUpgradeColumnImpl("nodeId",_nodeIdMapper));
  upgradeTable.updateTable();
}
