{
  List<AssetLink> list=findByE2(entryId2,0,1,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(4);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("entryId2=");
    msg.append(entryId2);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchLinkException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
