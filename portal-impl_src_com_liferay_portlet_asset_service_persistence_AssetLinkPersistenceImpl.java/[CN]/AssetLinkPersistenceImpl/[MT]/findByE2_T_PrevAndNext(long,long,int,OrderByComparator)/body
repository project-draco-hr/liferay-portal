{
  AssetLink assetLink=findByPrimaryKey(linkId);
  Session session=null;
  try {
    session=openSession();
    AssetLink[] array=new AssetLinkImpl[3];
    array[0]=getByE2_T_PrevAndNext(session,assetLink,entryId2,type,orderByComparator,true);
    array[1]=assetLink;
    array[2]=getByE2_T_PrevAndNext(session,assetLink,entryId2,type,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
