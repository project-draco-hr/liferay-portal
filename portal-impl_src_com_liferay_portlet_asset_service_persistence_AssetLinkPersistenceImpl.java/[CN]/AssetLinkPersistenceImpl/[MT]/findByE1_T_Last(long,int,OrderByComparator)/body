{
  int count=countByE1_T(entryId1,typeId);
  List<AssetLink> list=findByE1_T(entryId1,typeId,count - 1,count,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(6);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("entryId1=");
    msg.append(entryId1);
    msg.append(", typeId=");
    msg.append(typeId);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchLinkException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
