{
  List<AssetLink> list=findByE_E_T(entryId1,entryId2,type,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(8);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("entryId1=");
    msg.append(entryId1);
    msg.append(", entryId2=");
    msg.append(entryId2);
    msg.append(", type=");
    msg.append(type);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchLinkException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
