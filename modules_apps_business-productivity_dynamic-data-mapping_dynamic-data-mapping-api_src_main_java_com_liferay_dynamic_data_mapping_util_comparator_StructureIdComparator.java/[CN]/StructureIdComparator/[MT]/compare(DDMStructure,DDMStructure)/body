{
  long structureId1=structure1.getStructureId();
  long structureId2=structure2.getStructureId();
  int value=0;
  if (structureId1 < structureId2) {
    value=-1;
  }
 else   if (structureId1 > structureId2) {
    value=1;
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
