{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Lock.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("lockId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("lockId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
