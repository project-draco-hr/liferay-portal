{
  List<Role> siteRoles=roleLocalService.getRoles(RoleConstants.TYPE_SITE,StringPool.BLANK);
  for (  long userId : userIds) {
    for (    Role siteRole : siteRoles) {
      UserGroupRolePK pk=new UserGroupRolePK(userId,groupId,siteRole.getRoleId());
      try {
        userGroupRolePersistence.remove(pk);
      }
 catch (      NoSuchUserGroupRoleException nsugre) {
      }
    }
  }
  PermissionCacheUtil.clearCache();
}
