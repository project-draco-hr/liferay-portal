{
  List<Role> roles=rolePersistence.findByT_S(roleType,StringPool.BLANK);
  for (  long userId : userIds) {
    for (    Role role : roles) {
      UserGroupRolePK userGroupRolePK=new UserGroupRolePK(userId,groupId,role.getRoleId());
      try {
        userGroupRolePersistence.remove(userGroupRolePK);
      }
 catch (      NoSuchUserGroupRoleException nsugre) {
      }
    }
  }
  PermissionCacheUtil.clearCache(userIds);
}
