{
  if (Validator.isNotNull(getPortletDataHandlerClass())) {
    if (_portletApp.isWARFile()) {
      PortletBagImpl portletBagImpl=(PortletBagImpl)PortletBagPool.get(getRootPortletId());
      return portletBagImpl.getPortletDataHandlerInstance();
    }
 else {
      return (PortletDataHandler)InstancePool.get(getPortletDataHandlerClass());
    }
  }
  return null;
}
