{
  if (Validator.isNotNull(getPortletDataHandlerClass())) {
    if (isWARFile()) {
      PortletContextWrapper pcw=PortletContextPool.get(getRootPortletId());
      return pcw.getPortletDataHandlerInstance();
    }
 else {
      return (PortletDataHandler)InstancePool.get(getPortletDataHandlerClass());
    }
  }
  return null;
}
