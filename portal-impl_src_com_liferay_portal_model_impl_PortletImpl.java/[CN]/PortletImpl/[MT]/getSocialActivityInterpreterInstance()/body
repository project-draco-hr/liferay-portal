{
  if (Validator.isNull(getSocialActivityInterpreterClass())) {
    return null;
  }
  if (_portletApp.isWARFile()) {
    PortletBagImpl portletBagImpl=(PortletBagImpl)PortletBagPool.get(getRootPortletId());
    return portletBagImpl.getSocialActivityInterpreterInstance();
  }
  return (SocialActivityInterpreter)InstancePool.get(getSocialActivityInterpreterClass());
}
