{
  if (Validator.isNotNull(getSocialActivityInterpreterClass())) {
    if (_portletApp.isWARFile()) {
      PortletBagImpl portletBag=(PortletBagImpl)PortletBagPool.get(getRootPortletId());
      return portletBag.getSocialActivityInterpreterInstance();
    }
 else {
      return (SocialActivityInterpreter)InstancePool.get(getSocialActivityInterpreterClass());
    }
  }
  return null;
}
