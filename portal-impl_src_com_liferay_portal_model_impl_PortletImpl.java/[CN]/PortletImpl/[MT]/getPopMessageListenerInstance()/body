{
  if (Validator.isNotNull(getPopMessageListenerClass())) {
    if (isWARFile()) {
      PortletContextWrapper pcw=PortletContextPool.get(getRootPortletId());
      return pcw.getPopMessageListenerInstance();
    }
 else {
      return (MessageListener)InstancePool.get(getPopMessageListenerClass());
    }
  }
  return null;
}
