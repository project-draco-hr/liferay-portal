{
  if (Validator.isNotNull(getURLEncoderClass())) {
    if (isWARFile()) {
      PortletContextWrapper pcw=PortletContextPool.get(getRootPortletId());
      return pcw.getURLEncoderInstance();
    }
 else {
      return (URLEncoder)InstancePool.get(getURLEncoderClass());
    }
  }
  return null;
}
