{
  for (  Map.Entry<String,PublicRenderParameter> entry : _publicRenderParameters.entrySet()) {
    PublicRenderParameter publicRenderParameter=entry.getValue();
    QName qName=publicRenderParameter.getQName();
    if (qName.getNamespaceURI().equals(uri) && qName.getLocalPart().equals(localPart)) {
      return publicRenderParameter.getIdentifier();
    }
  }
  return null;
}
