{
  if (getWorkflowHandlerClasses().isEmpty()) {
    return null;
  }
  if (_portletApp.isWARFile()) {
    PortletBagImpl portletBagImpl=(PortletBagImpl)PortletBagPool.get(getRootPortletId());
    return portletBagImpl.getWorkflowHandlerInstances();
  }
  List<WorkflowHandler> workflowHandlerInstances=new ArrayList<WorkflowHandler>();
  for (  String workflowHandlerClass : getWorkflowHandlerClasses()) {
    WorkflowHandler workflowHandlerInstance=(WorkflowHandler)InstancePool.get(workflowHandlerClass);
    workflowHandlerInstances.add(workflowHandlerInstance);
  }
  return workflowHandlerInstances;
}
