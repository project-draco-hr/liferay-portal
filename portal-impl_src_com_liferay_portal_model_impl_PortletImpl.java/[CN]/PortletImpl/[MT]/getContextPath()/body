{
  String virtualPath=getVirtualPath();
  if (Validator.isNotNull(virtualPath)) {
    return virtualPath;
  }
  if (isWARFile()) {
    StringMaker sm=new StringMaker();
    sm.append(StringPool.SLASH);
    sm.append(getServletContextName());
    return sm.toString();
  }
 else {
    return PortalUtil.getPathContext();
  }
}
