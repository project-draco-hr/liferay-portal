{
  PortletBag portletBag=PortletBagPool.get(getRootPortletId());
  List<SocialRequestInterpreter> socialRequestInterpreterInstances=portletBag.getSocialRequestInterpreterInstances();
  if (socialRequestInterpreterInstances.isEmpty()) {
    return null;
  }
  return socialRequestInterpreterInstances.get(0);
}
