{
  if (Validator.isNull(getSocialRequestInterpreterClass())) {
    return null;
  }
  if (_portletApp.isWARFile()) {
    PortletBagImpl portletBagImpl=(PortletBagImpl)PortletBagPool.get(getRootPortletId());
    return portletBagImpl.getSocialRequestInterpreterInstance();
  }
  return (SocialRequestInterpreter)InstancePool.get(getSocialRequestInterpreterClass());
}
