{
  if (Validator.isNotNull(getSocialRequestInterpreterClass())) {
    if (_portletApp.isWARFile()) {
      PortletBagImpl portletBag=(PortletBagImpl)PortletBagPool.get(getRootPortletId());
      return portletBag.getSocialRequestInterpreterInstance();
    }
 else {
      return (SocialRequestInterpreter)InstancePool.get(getSocialRequestInterpreterClass());
    }
  }
  return null;
}
