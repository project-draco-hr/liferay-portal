{
  if (getAssetRendererFactoryClasses().isEmpty()) {
    return null;
  }
  if (_portletApp.isWARFile()) {
    PortletBagImpl portletBagImpl=(PortletBagImpl)PortletBagPool.get(getRootPortletId());
    return portletBagImpl.getAssetRendererFactoryInstances();
  }
  List<AssetRendererFactory> assetRendererFactoryInstances=new ArrayList<AssetRendererFactory>();
  for (  String assetRendererFactoryClass : getAssetRendererFactoryClasses()) {
    AssetRendererFactory assetRendererFactoryInstance=(AssetRendererFactory)InstancePool.get(assetRendererFactoryClass);
    assetRendererFactoryInstance.setClassNameId(PortalUtil.getClassNameId(assetRendererFactoryInstance.getClassName()));
    assetRendererFactoryInstance.setPortletId(getPortletId());
    assetRendererFactoryInstances.add(assetRendererFactoryInstance);
  }
  return assetRendererFactoryInstances;
}
