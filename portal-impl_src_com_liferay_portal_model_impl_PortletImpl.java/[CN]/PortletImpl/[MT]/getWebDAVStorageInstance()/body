{
  if (Validator.isNull(getWebDAVStorageClass())) {
    return null;
  }
  if (_portletApp.isWARFile()) {
    PortletBagImpl portletBagImpl=(PortletBagImpl)PortletBagPool.get(getRootPortletId());
    return portletBagImpl.getWebDAVStorageInstance();
  }
  return (WebDAVStorage)InstancePool.get(getWebDAVStorageClass());
}
