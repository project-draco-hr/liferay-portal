{
  Path indexesFilePath=Paths.get(indexesFileName);
  if (Files.exists(indexesFilePath)) {
    return _loadIndexes(indexesFilePath);
  }
  List<URI> uris=new ArrayList<>();
  uris.add(_indexTargetPlatform(moduleFrameworkStaticDirName,moduleFrameworkModulesDirName,moduleFrameworkPortalDirName));
  uris.addAll(_indexLPKGFiles(Utilities.listFiles(moduleFrameworkMarketplaceDirName,"*.lpkg")));
  _saveIndexes(indexesFileName,uris);
  return uris;
}
