{
  if (properties == null) {
    properties=new HashMapDictionary<>();
  }
  _configurationBean=Configurable.createConfigurable(_configurationBeanClass,properties);
  if (_configurationBeanServiceRegistration != null) {
    _configurationBeanServiceRegistration.unregister();
  }
  _configurationBeanServiceRegistration=_bundleContext.registerService(_configurationBeanClass.getName(),_configurationBean,new HashMapDictionary<String,Object>());
}
