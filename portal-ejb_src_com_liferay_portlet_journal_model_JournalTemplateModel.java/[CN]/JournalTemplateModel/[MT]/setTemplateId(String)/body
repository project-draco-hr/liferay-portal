{
  if (((templateId == null) && (_templateId != null)) || ((templateId != null) && (_templateId == null)) || ((templateId != null) && (_templateId != null) && !templateId.equals(_templateId))) {
    if (!XSS_ALLOW_TEMPLATEID) {
      templateId=XSSUtil.strip(templateId);
    }
    _templateId=templateId;
    setModified(true);
  }
}
