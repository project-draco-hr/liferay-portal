{
  try {
    ActionUtil.getGroup(renderRequest);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchGroupException || e instanceof PrincipalException) {
      SessionErrors.add(renderRequest,e.getClass());
      return actionMapping.findForward("portlet.export_configuration.error");
    }
 else {
      throw e;
    }
  }
  return actionMapping.findForward(getForward(renderRequest,"portlet.export_configuration.publish_layouts"));
}
