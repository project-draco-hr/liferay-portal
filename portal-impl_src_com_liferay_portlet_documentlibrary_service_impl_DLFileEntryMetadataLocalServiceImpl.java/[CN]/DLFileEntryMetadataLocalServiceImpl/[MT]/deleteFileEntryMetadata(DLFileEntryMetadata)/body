{
  dlFileEntryMetadataPersistence.remove(fileEntryMetadata);
  StorageEngineUtil.deleteByClass(fileEntryMetadata.getDDMStorageId());
  long classNameId=classNameLocalService.getClassNameId(DLFileEntryMetadata.class);
  ddmStructureLinkLocalService.deleteStructureLinks(classNameId,fileEntryMetadata.getFileEntryMetadataId());
}
