{
  try {
    Class<?> clazz=getClass();
    InputStream inputStream=clazz.getResourceAsStream("/logging.properties");
    if (inputStream != null) {
      LogManager logManager=LogManager.getLogManager();
      logManager.readConfiguration(inputStream);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
