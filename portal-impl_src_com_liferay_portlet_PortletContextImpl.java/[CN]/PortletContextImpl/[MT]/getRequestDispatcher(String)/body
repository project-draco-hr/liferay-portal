{
  RequestDispatcher requestDispatcher=null;
  try {
    requestDispatcher=_servletContext.getRequestDispatcher(path);
  }
 catch (  Throwable t) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to get request dispatcher for path " + path,t);
    }
    return null;
  }
  if (requestDispatcher != null) {
    return DoPrivilegedUtil.wrapWhenActive(new PortletRequestDispatcherImpl(requestDispatcher,false,this,path));
  }
 else {
    return null;
  }
}
