{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Blogs Test Page1",RuntimeVariables.replace("Blogs Test Page1"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@title='Search Entries']",RuntimeVariables.replace("\"Blogs Entry2 Title\""));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace("Search"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isTextPresent("Blogs Entry1 Title"));
  assertEquals(RuntimeVariables.replace("No entries were found that matched the keywords: \"Blogs Entry2 Title\"."),selenium.getText("//div[@class='portlet-msg-info']"));
}
