{
  OrgGroupRole orgGroupRole=fetchByPrimaryKey(orgGroupRolePK);
  if (orgGroupRole == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No OrgGroupRole exists with the primary key " + orgGroupRolePK.toString());
    }
    throw new NoSuchOrgGroupRoleException("No OrgGroupRole exists with the primary key " + orgGroupRolePK.toString());
  }
  return orgGroupRole;
}
