{
  Session session=null;
  try {
    session=openSession();
    StringBuffer query=new StringBuffer();
    query.append("FROM OrgGroupRole IN CLASS com.liferay.portal.service.persistence.OrgGroupRoleHBM ");
    Query q=session.createQuery(query.toString());
    Iterator itr=q.iterate();
    List list=new ArrayList();
    while (itr.hasNext()) {
      OrgGroupRoleHBM orgGroupRoleHBM=(OrgGroupRoleHBM)itr.next();
      list.add(OrgGroupRoleHBMUtil.model(orgGroupRoleHBM));
    }
    return list;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
