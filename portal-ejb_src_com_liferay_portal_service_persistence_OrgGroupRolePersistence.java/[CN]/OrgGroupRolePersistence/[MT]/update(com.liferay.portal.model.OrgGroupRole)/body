{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(orgGroupRole);
    session.flush();
    orgGroupRole.setNew(false);
    return orgGroupRole;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
