{
  Session session=null;
  try {
    session=openSession();
    StringBuffer query=new StringBuffer();
    query.append("FROM OrgGroupRole IN CLASS com.liferay.portal.service.persistence.OrgGroupRoleHBM WHERE ");
    if (roleId == null) {
      query.append("roleId is null");
    }
 else {
      query.append("roleId = ?");
    }
    query.append(" ");
    if (obc != null) {
      query.append("ORDER BY " + obc.getOrderBy());
    }
    Query q=session.createQuery(query.toString());
    int queryPos=0;
    if (roleId != null) {
      q.setString(queryPos++,roleId);
    }
    List list=new ArrayList();
    Iterator itr=QueryUtil.iterate(q,getDialect(),begin,end);
    while (itr.hasNext()) {
      OrgGroupRoleHBM orgGroupRoleHBM=(OrgGroupRoleHBM)itr.next();
      list.add(OrgGroupRoleHBMUtil.model(orgGroupRoleHBM));
    }
    return list;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
