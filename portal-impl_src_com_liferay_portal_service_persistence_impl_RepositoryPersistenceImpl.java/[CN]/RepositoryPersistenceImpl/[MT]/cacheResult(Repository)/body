{
  EntityCacheUtil.putResult(RepositoryModelImpl.ENTITY_CACHE_ENABLED,RepositoryImpl.class,repository.getPrimaryKey(),repository);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{repository.getUuid(),repository.getGroupId()},repository);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_N_P,new Object[]{repository.getGroupId(),repository.getName(),repository.getPortletId()},repository);
  repository.resetOriginalValues();
}
