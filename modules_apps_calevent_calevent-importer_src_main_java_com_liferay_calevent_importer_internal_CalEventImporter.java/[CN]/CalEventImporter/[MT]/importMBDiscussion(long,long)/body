{
  MBDiscussion mbDiscussion=_mbDiscussionLocalService.fetchDiscussion(_CLASS_NAME,eventId);
  if (mbDiscussion == null) {
    return;
  }
  long threadId=importMBThread(mbDiscussion.getThreadId(),calendarBookingId);
  addMBDiscussion(PortalUUIDUtil.generate(),_counterLocalService.increment(),mbDiscussion.getGroupId(),mbDiscussion.getCompanyId(),mbDiscussion.getUserId(),mbDiscussion.getUserName(),mbDiscussion.getCreateDate(),mbDiscussion.getModifiedDate(),_classNameLocalService.getClassNameId(CalendarBooking.class.getName()),calendarBookingId,threadId);
}
