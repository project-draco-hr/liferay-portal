{
  BlogsEntry entry=BlogsTestUtil.addBlogsEntry(TestPropsValues.getUserId(),serviceContext.getScopeGroupId(),getSearchKeywords(),approved);
  if (approved) {
    entry=BlogsEntryLocalServiceUtil.updateStatus(TestPropsValues.getUserId(),entry.getEntryId(),WorkflowConstants.STATUS_APPROVED,serviceContext);
  }
  return entry;
}
