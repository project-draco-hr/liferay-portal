{
  setResponse("dependencies/get_sync_dl_object_update_event_update.json");
  Path sourceFilePath=FileUtil.getFilePath(_syncSite.getFilePathName(),"test.txt");
  Files.createFile(sourceFilePath);
  SyncFileTestUtil.addFileSyncFile(sourceFilePath.toString(),0,_syncSite.getGroupId(),syncAccount.getSyncAccountId(),27382);
  run();
  _syncFiles=SyncFileService.findSyncFiles(syncAccount.getSyncAccountId());
  Assert.assertEquals(3,_syncFiles.size());
  Path targetFilePath=FileUtil.getFilePath(_syncSite.getFilePathName(),"test2.txt");
  Assert.assertTrue(Files.exists(targetFilePath));
  SyncFile syncFile=SyncFileService.fetchSyncFile(targetFilePath.toString());
  Assert.assertEquals("Updated Description",syncFile.getDescription());
}
