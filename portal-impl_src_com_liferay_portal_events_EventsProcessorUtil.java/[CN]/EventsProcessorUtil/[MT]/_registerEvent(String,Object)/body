{
  Registry registry=RegistryUtil.getRegistry();
  Map<String,Object> properties=new HashMap<>();
  properties.put("key",key);
  ServiceRegistration<LifecycleAction> serviceRegistration=registry.registerService(LifecycleAction.class,(LifecycleAction)event,properties);
  Map<Object,ServiceRegistration<LifecycleAction>> serviceRegistrationMap=_serviceRegistrationMaps.get(key);
  if (serviceRegistrationMap == null) {
    _serviceRegistrationMaps.putIfAbsent(key,new ConcurrentHashMap<Object,ServiceRegistration<LifecycleAction>>());
    serviceRegistrationMap=_serviceRegistrationMaps.get(key);
  }
  serviceRegistrationMap.put(event,serviceRegistration);
}
