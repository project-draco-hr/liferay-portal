{
  Collection<LifecycleAction> lifecycleActions=_lifecycleActions.get(key);
  if (lifecycleActions == null) {
    StringBundler sb=new StringBundler(3);
    sb.append("(key=");
    sb.append(key);
    sb.append(")");
    Map<String,Object> map=new HashMap<String,Object>();
    map.put("key",key);
    lifecycleActions=ServiceTrackerCollections.list(LifecycleAction.class,sb.toString(),map);
    _lifecycleActions.putIfAbsent(key,lifecycleActions);
  }
  return lifecycleActions;
}
