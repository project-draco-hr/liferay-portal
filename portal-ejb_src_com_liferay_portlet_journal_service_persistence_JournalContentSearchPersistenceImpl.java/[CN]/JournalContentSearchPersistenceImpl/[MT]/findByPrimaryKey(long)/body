{
  JournalContentSearch journalContentSearch=fetchByPrimaryKey(contentSearchId);
  if (journalContentSearch == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No JournalContentSearch exists with the primary key " + contentSearchId);
    }
    throw new NoSuchContentSearchException("No JournalContentSearch exists with the primary key " + contentSearchId);
  }
  return journalContentSearch;
}
