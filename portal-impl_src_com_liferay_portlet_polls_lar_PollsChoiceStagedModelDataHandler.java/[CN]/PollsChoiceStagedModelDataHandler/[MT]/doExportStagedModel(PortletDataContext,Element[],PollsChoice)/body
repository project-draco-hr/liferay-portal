{
  Element questionsElement=elements[0];
  PollsQuestion question=PollsQuestionLocalServiceUtil.getQuestion(choice.getQuestionId());
  StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,questionsElement,question);
  Element choicesElement=elements[1];
  Element choiceElement=choicesElement.addElement("choice");
  portletDataContext.addClassedModel(choiceElement,StagedModelPathUtil.getPath(choice),choice,PollsPortletDataHandler.NAMESPACE);
}
