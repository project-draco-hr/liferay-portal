{
  if (obc == null) {
    return;
  }
  String[] orderByFields=obc.getOrderByFields();
  for (  String orderByField : orderByFields) {
    if (obc.isAscending(orderByField)) {
      dynamicQuery.addOrder(asc(orderByField));
    }
 else {
      dynamicQuery.addOrder(desc(orderByField));
    }
  }
}
