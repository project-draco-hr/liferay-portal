{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),KaleoDefinitionService.class.getName());
    if (invokableService instanceof KaleoDefinitionService) {
      _service=(KaleoDefinitionService)invokableService;
    }
 else {
      _service=new KaleoDefinitionServiceClp(invokableService);
    }
    ReferenceRegistry.registerReference(KaleoDefinitionServiceUtil.class,"_service");
  }
  return _service;
}
