{
  Session session=null;
  try {
    session=openSession();
    ListType listType=(ListType)session.get(ListType.class,listTypeId);
    if (listType == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No ListType exists with the primary key " + listTypeId.toString());
      }
      throw new NoSuchListTypeException("No ListType exists with the primary key " + listTypeId.toString());
    }
    return listType;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
