{
  Session session=null;
  try {
    session=openSession();
    ListTypeHBM listTypeHBM=(ListTypeHBM)session.get(ListTypeHBM.class,listTypeId);
    if (listTypeHBM == null) {
      _log.warn("No ListType exists with the primary key " + listTypeId.toString());
      throw new NoSuchListTypeException("No ListType exists with the primary key " + listTypeId.toString());
    }
    session.delete(listTypeHBM);
    session.flush();
    return ListTypeHBMUtil.model(listTypeHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
