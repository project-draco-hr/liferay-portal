{
  Session session=null;
  try {
    session=openSession();
    if (listType.isNew()) {
      session.save(listType);
    }
    session.flush();
    listType.setNew(false);
    return listType;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
