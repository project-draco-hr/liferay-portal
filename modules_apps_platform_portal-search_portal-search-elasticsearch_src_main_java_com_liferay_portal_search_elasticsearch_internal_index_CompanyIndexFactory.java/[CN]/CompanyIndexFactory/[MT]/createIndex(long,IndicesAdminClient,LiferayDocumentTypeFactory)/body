{
  CreateIndexRequestBuilder createIndexRequestBuilder=indicesAdminClient.prepareCreate(indexNameBuilder.getIndexName(companyId));
  addMappings(createIndexRequestBuilder);
  setSettings(createIndexRequestBuilder,liferayDocumentTypeFactory);
  liferayDocumentTypeFactory.createRequiredDefaultTypeMappings(createIndexRequestBuilder);
  CreateIndexResponse createIndexResponse=createIndexRequestBuilder.get();
  LogUtil.logActionResponse(_log,createIndexResponse);
}
