{
  IndicesAdminClient indicesAdminClient=adminClient.indices();
  if (!hasIndex(indicesAdminClient,companyId)) {
    return;
  }
  DeleteIndexRequestBuilder deleteIndexRequestBuilder=indicesAdminClient.prepareDelete(indexNameBuilder.getIndexName(companyId));
  DeleteIndexResponse deleteIndexResponse=deleteIndexRequestBuilder.get();
  LogUtil.logActionResponse(_log,deleteIndexResponse);
}
