{
  Session session=null;
  try {
    session=openSession();
    session.delete(bookmarksEntry);
    session.flush();
    return bookmarksEntry;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(BookmarksEntry.class.getName());
  }
}
