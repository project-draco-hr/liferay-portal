{
  BookmarksEntry bookmarksEntry=findByPrimaryKey(entryId);
  Session session=null;
  try {
    session=openSession();
    BookmarksEntry[] array=new BookmarksEntryImpl[3];
    array[0]=getByResourceBlockId_PrevAndNext(session,bookmarksEntry,resourceBlockId,orderByComparator,true);
    array[1]=bookmarksEntry;
    array[2]=getByResourceBlockId_PrevAndNext(session,bookmarksEntry,resourceBlockId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
