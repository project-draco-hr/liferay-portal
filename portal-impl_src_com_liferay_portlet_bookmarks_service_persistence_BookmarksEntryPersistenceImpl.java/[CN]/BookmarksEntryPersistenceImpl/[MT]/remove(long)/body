{
  Session session=null;
  try {
    session=openSession();
    BookmarksEntry bookmarksEntry=(BookmarksEntry)session.get(BookmarksEntryImpl.class,new Long(entryId));
    if (bookmarksEntry == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + entryId);
      }
      throw new NoSuchEntryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + entryId);
    }
    return remove(bookmarksEntry);
  }
 catch (  NoSuchEntryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
