{
  PanelCategory panelCategory=_panelCategoryRegistry.getFirstChildPanelCategory(panelCategoryKey,permissionChecker,group);
  if (panelCategory == null) {
    return null;
  }
  PanelApp panelApp=_panelAppRegistry.getFirstPanelApp(panelCategory,permissionChecker,group);
  if (panelApp == null) {
    return null;
  }
  return panelApp.getPortletId();
}
