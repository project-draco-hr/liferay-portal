{
  if (_portletLocalService == null) {
    throw new IllegalStateException("Portlet local service not initialized");
  }
  MessageListener schedulerEngineMessageListener=(MessageListener)message.get(SchedulerEngine.MESSAGE_LISTENER);
  if (schedulerEngineMessageListener == null) {
    return;
  }
  SchedulerEventMessageListenerWrapper schedulerEventMessageListenerWrapper=new SchedulerEventMessageListenerWrapper();
  schedulerEventMessageListenerWrapper.setMessageListener(schedulerEngineMessageListener);
  schedulerEventMessageListenerWrapper.afterPropertiesSet();
  _messageBus.registerMessageListener(destinationName,schedulerEventMessageListenerWrapper);
  message.put(SchedulerEngine.MESSAGE_LISTENER_UUID,schedulerEventMessageListenerWrapper.getMessageListenerUUID());
}
