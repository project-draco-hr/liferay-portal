{
  try {
    ActionUtil.getCategory(req);
  }
 catch (  Exception e) {
    if (e != null && e instanceof NoSuchCategoryException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.blogs.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward(getForward(req,"portlet.blogs.edit_category"));
}
