{
  int value=0;
  if (getFolderId() < igFolder.getFolderId()) {
    value=-1;
  }
 else   if (getFolderId() > igFolder.getFolderId()) {
    value=1;
  }
 else {
    value=0;
  }
  if (value != 0) {
    return value;
  }
  value=getName().toLowerCase().compareTo(igFolder.getName().toLowerCase());
  if (value != 0) {
    return value;
  }
  return 0;
}
