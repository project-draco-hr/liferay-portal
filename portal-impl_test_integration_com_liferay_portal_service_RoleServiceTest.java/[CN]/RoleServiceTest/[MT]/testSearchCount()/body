{
  int initialCount=RoleServiceUtil.searchCount(group.getCompanyId(),StringPool.BLANK,new Integer[]{RoleConstants.TYPE_REGULAR},new LinkedHashMap<String,Object>());
  removePortletModelViewPermission();
  int count=RoleServiceUtil.searchCount(group.getCompanyId(),StringPool.BLANK,new Integer[]{RoleConstants.TYPE_REGULAR},new LinkedHashMap<String,Object>());
  Assert.assertEquals(initialCount - 1,count);
}
