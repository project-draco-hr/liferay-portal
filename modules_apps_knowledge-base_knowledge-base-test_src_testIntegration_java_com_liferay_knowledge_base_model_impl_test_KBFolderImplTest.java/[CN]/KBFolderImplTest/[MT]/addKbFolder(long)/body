{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group,_user.getUserId());
  return KBFolderLocalServiceUtil.addKBFolder(_user.getUserId(),_group.getGroupId(),PortalUtil.getClassNameId(KBFolderConstants.getClassName()),parentResourcePrimKey,RandomTestUtil.randomString(),RandomTestUtil.randomString(),serviceContext);
}
