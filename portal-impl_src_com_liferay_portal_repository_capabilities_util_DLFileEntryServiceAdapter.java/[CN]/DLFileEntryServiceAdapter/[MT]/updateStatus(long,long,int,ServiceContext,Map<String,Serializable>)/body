{
  DLFileEntry dlFileEntry=null;
  if (_dlFileEntryService != null) {
    dlFileEntry=_dlFileEntryService.updateStatus(userId,fileVersionId,status,serviceContext,workflowContext);
  }
 else {
    dlFileEntry=_dlFileEntryLocalService.updateStatus(userId,fileVersionId,status,serviceContext,workflowContext);
  }
  return dlFileEntry;
}
