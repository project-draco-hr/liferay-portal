{
  List<DLFileEntry> dlFileEntries=null;
  if (_dlFileEntryService != null) {
    dlFileEntries=_dlFileEntryService.getGroupFileEntries(groupId,userId,repositoryId,folderId,null,WorkflowConstants.STATUS_ANY,start,end,obc);
  }
 else {
    dlFileEntries=_dlFileEntryLocalService.getGroupFileEntries(groupId,userId,repositoryId,folderId,start,end,obc);
  }
  return dlFileEntries;
}
