{
  if (!SyncEngine.isRunning()) {
    return;
  }
  _handler=getHandler();
  try {
    if (_logger.isTraceEnabled()) {
      Class<?> clazz=this.getClass();
      _logger.trace("Processing {}",clazz.getSimpleName());
    }
    processRequest();
  }
 catch (  Exception e) {
    _handler.handleException(e);
  }
}
