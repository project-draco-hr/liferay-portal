{
  try (PreparedStatement ps=connection.prepareStatement("select count(*) from " + tableName);ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      int count=rs.getInt(1);
      if (count > 0) {
        return true;
      }
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return false;
}
