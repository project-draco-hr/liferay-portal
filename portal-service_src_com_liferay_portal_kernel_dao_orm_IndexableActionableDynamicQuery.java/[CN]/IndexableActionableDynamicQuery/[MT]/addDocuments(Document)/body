{
  if (ArrayUtil.isEmpty(documents)) {
    return;
  }
  if (_documents == null) {
    if (isParallel()) {
      _documents=new ConcurrentLinkedDeque<>();
    }
 else {
      _documents=new ArrayList<>();
    }
  }
  for (  Document document : documents) {
    if (document != null) {
      _documents.add(document);
    }
  }
  if (_documents.size() >= getInterval()) {
    indexInterval();
  }
}
