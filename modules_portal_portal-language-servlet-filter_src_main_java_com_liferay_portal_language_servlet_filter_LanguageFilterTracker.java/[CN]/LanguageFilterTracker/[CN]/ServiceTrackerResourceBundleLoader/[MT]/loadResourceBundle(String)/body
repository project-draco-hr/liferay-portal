{
  ResourceBundleLoader resourceBundleLoader=_serviceTracker.getService();
  ResourceBundleLoader portalResourceBundleLoader=ResourceBundleLoaderUtil.getPortalResourceBundleLoader();
  if (resourceBundleLoader != null) {
    ResourceBundle resourceBundle=resourceBundleLoader.loadResourceBundle(languageId);
    if (resourceBundle != null) {
      return new AggregateResourceBundle(resourceBundle,portalResourceBundleLoader.loadResourceBundle(languageId));
    }
  }
  return portalResourceBundleLoader.loadResourceBundle(languageId);
}
