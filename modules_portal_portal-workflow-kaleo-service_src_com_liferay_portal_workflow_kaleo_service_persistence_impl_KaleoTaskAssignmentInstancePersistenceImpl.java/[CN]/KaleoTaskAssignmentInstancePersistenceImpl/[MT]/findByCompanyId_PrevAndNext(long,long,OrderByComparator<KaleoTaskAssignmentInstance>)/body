{
  KaleoTaskAssignmentInstance kaleoTaskAssignmentInstance=findByPrimaryKey(kaleoTaskAssignmentInstanceId);
  Session session=null;
  try {
    session=openSession();
    KaleoTaskAssignmentInstance[] array=new KaleoTaskAssignmentInstanceImpl[3];
    array[0]=getByCompanyId_PrevAndNext(session,kaleoTaskAssignmentInstance,companyId,orderByComparator,true);
    array[1]=kaleoTaskAssignmentInstance;
    array[2]=getByCompanyId_PrevAndNext(session,kaleoTaskAssignmentInstance,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
