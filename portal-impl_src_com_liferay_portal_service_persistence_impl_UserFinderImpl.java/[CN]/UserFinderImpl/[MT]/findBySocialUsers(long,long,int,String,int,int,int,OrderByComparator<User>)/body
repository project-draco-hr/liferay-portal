{
  Session session=null;
  try {
    session=openSession();
    String sql=CustomSQLUtil.get(FIND_BY_SOCIAL_USERS);
    sql=StringUtil.replace(sql,"[$SOCIAL_RELATION_TYPE_COMPARATOR$]",socialRelationTypeComparator.equals(StringPool.EQUAL) ? StringPool.EQUAL : StringPool.NOT_EQUAL);
    sql=CustomSQLUtil.replaceOrderBy(sql,obc);
    SQLQuery q=session.createSynchronizedSQLQuery(sql);
    q.addEntity("User_",UserImpl.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(socialRelationType);
    qPos.add(userId);
    qPos.add(companyId);
    qPos.add(Boolean.FALSE);
    qPos.add(status);
    return (List<User>)QueryUtil.list(q,getDialect(),start,end);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
}
