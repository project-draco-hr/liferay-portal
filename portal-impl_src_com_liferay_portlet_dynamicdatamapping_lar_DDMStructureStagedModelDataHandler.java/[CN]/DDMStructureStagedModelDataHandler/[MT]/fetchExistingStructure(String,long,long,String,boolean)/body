{
  DDMStructure existingStructure=null;
  if (!preloaded) {
    existingStructure=DDMStructureLocalServiceUtil.fetchDDMStructureByUuidAndGroupId(uuid,groupId);
  }
 else {
    existingStructure=DDMStructureLocalServiceUtil.fetchStructure(groupId,classNameId,structureKey);
  }
  return existingStructure;
}
