{
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Test Page 8")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Test Page 8"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  selenium.click("link=Layout Template");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("layoutTemplateId1")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  FileUtil.mkdirs(RuntimeVariables.replace("L:\\portal\\build\\portal-web\\test-output\\brochure\\"));
  selenium.captureEntirePageScreenshot(RuntimeVariables.replace("L:\\portal\\build\\portal-web\\test-output\\brochure\\ScreengrabTest09.jpg"),"");
  selenium.click(RuntimeVariables.replace("//input[@value='Save']"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
}
