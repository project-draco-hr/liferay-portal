{
  Company company=findByPrimaryKey(companyId);
  Session session=null;
  try {
    session=openSession();
    Company[] array=new CompanyImpl[3];
    array[0]=getBySystem_PrevAndNext(session,company,system,orderByComparator,true);
    array[1]=company;
    array[2]=getBySystem_PrevAndNext(session,company,system,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
