{
  String controlPanelCategory=themeDisplay.getControlPanelCategory();
  if (controlPanelCategory.equals(PortletCategoryKeys.CONTENT)) {
    return false;
  }
  Group scopeGroup=themeDisplay.getScopeGroup();
  if (scopeGroup.isCompany() || scopeGroup.isUser() || !GroupPermissionUtil.contains(themeDisplay.getPermissionChecker(),scopeGroup.getGroupId(),ActionKeys.ASSIGN_MEMBERS)) {
    return false;
  }
  return super.isVisible(portlet,category,themeDisplay);
}
