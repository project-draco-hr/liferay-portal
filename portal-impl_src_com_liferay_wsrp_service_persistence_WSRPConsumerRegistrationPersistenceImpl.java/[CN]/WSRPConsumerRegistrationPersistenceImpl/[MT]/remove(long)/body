{
  Session session=null;
  try {
    session=openSession();
    WSRPConsumerRegistration wsrpConsumerRegistration=(WSRPConsumerRegistration)session.get(WSRPConsumerRegistrationImpl.class,new Long(consumerRegistrationId));
    if (wsrpConsumerRegistration == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No WSRPConsumerRegistration exists with the primary key " + consumerRegistrationId);
      }
      throw new NoSuchConsumerRegistrationException("No WSRPConsumerRegistration exists with the primary key " + consumerRegistrationId);
    }
    return remove(wsrpConsumerRegistration);
  }
 catch (  NoSuchConsumerRegistrationException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
