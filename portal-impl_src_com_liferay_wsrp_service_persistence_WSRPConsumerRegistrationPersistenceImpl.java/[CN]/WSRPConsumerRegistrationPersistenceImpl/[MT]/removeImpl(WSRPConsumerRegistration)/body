{
  Session session=null;
  try {
    session=openSession();
    if (BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(WSRPConsumerRegistrationImpl.class,wsrpConsumerRegistration.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(wsrpConsumerRegistration);
    session.flush();
    return wsrpConsumerRegistration;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(WSRPConsumerRegistration.class.getName());
  }
}
