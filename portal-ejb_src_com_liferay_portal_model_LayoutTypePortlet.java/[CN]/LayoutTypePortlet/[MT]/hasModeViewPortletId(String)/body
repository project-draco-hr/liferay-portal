{
  if (hasModeAboutPortletId(portletId) || hasModeConfigPortletId(portletId) || hasModeEditPortletId(portletId)|| hasModeEditDefaultsPortletId(portletId)|| hasModeHelpPortletId(portletId)|| hasModePreviewPortletId(portletId)|| hasModePrintPortletId(portletId)) {
    return false;
  }
 else {
    return true;
  }
}
