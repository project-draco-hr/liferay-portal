{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId(),TestPropsValues.getUserId());
  _category=MBCategoryLocalServiceUtil.addCategory(TestPropsValues.getUserId(),MBCategoryConstants.DEFAULT_PARENT_CATEGORY_ID,RandomTestUtil.randomString(),StringPool.BLANK,serviceContext);
  _subcategory=MBCategoryLocalServiceUtil.addCategory(TestPropsValues.getUserId(),_category.getCategoryId(),RandomTestUtil.randomString(),StringPool.BLANK,serviceContext);
}
