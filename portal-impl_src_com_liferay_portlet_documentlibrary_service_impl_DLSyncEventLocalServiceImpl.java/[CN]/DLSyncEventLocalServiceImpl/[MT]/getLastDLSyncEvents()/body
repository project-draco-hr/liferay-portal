{
  DynamicQuery subDynamicQuery=DynamicQueryFactoryUtil.forClass(DLSyncEvent.class);
  subDynamicQuery.setProjection(ProjectionFactoryUtil.max("modifiedDate"));
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLSyncEvent.class);
  dynamicQuery.add(PropertyFactoryUtil.forName("modifiedDate").eq(subDynamicQuery));
  List<DLSyncEvent> dlSyncEvents=(List<DLSyncEvent>)dlSyncEventPersistence.findWithDynamicQuery(dynamicQuery);
  return dlSyncEvents;
}
