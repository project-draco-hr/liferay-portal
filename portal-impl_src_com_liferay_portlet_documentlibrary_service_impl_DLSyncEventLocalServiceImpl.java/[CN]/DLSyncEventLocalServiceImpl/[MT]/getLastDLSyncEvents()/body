{
  DynamicQuery modifiedDateDynamicQuery=DynamicQueryFactoryUtil.forClass(DLSyncEvent.class);
  Projection projection=ProjectionFactoryUtil.max("modifiedDate");
  modifiedDateDynamicQuery.setProjection(projection);
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLSyncEvent.class);
  Property property=PropertyFactoryUtil.forName("modifiedDate");
  dynamicQuery.add(property.eq(modifiedDateDynamicQuery));
  List<DLSyncEvent> dlSyncEvents=(List<DLSyncEvent>)dlSyncEventPersistence.findWithDynamicQuery(dynamicQuery);
  return dlSyncEvents;
}
