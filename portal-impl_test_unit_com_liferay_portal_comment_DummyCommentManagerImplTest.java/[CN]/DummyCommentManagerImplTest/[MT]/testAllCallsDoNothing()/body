{
  long userId=RandomTestUtil.randomLong();
  long groupId=RandomTestUtil.randomLong();
  String className=RandomTestUtil.randomString();
  long classPK=RandomTestUtil.randomLong();
  String userName=RandomTestUtil.randomString();
  String subject=RandomTestUtil.randomString();
  String body=RandomTestUtil.randomString();
  Assert.assertEquals(0,_commentManager.addComment(userId,groupId,className,classPK,userName,subject,body,null));
  _commentManager.addDiscussion(userId,groupId,className,classPK,userName);
  long commentId=RandomTestUtil.randomLong();
  _commentManager.deleteComment(commentId);
  _commentManager.deleteDiscussion(className,classPK);
  Assert.assertEquals(0,_commentManager.getCommentsCount(className,classPK));
}
