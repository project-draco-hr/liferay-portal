  shoppingOrder.setCcName(randomString());
  shoppingOrder.setCcType(randomString());
  shoppingOrder.setCcNumber(randomString());
  shoppingOrder.setCcExpMonth(nextInt());
  shoppingOrder.setCcExpYear(nextInt());
