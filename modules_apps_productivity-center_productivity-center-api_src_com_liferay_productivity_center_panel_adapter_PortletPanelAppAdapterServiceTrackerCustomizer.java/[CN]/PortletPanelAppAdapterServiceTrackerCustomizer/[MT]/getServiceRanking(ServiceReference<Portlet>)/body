{
  String controlPanelEntryWeight=(String)serviceReference.getProperty("com.liferay.portlet.control-panel-entry-weight");
  if (Validator.isNotNull(controlPanelEntryWeight)) {
    try {
      return (int)Math.ceil(Double.parseDouble(controlPanelEntryWeight) * 100);
    }
 catch (    NumberFormatException nfe) {
    }
  }
  return null;
}
