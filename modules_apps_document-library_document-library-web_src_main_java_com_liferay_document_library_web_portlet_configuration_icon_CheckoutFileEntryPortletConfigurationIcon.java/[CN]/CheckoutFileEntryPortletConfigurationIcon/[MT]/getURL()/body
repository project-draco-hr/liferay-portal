{
  PortletURL checkoutURL=PortalUtil.getControlPanelPortletURL(portletRequest,DLPortletKeys.DOCUMENT_LIBRARY_ADMIN,PortletRequest.ACTION_PHASE);
  checkoutURL.setParameter("javax.portlet.action","/document_library/edit_file_entry");
  checkoutURL.setParameter(Constants.CMD,Constants.CHECKOUT);
  checkoutURL.setParameter("redirect",themeDisplay.getURLCurrent());
  checkoutURL.setParameter("fileEntryId",String.valueOf(_fileEntry.getFileEntryId()));
  return checkoutURL.toString();
}
