{
  try {
    Class<?> clazz=getClass();
    String defaultConfiguration="/META-INF/elasticsearch-optional-defaults.yml";
    InputStream inputStream=clazz.getResourceAsStream(defaultConfiguration);
    builder.loadFromStream(defaultConfiguration,inputStream);
  }
 catch (  Exception e) {
    if (_log.isInfoEnabled()) {
      _log.info("Unable to load optional default configurations",e);
    }
  }
}
