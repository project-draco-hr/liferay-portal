{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("//input[@id='_58_login']");
  selenium.type("//input[@id='_58_login']",RuntimeVariables.replace("pm@liferay.com"));
  selenium.type("//input[@id='_58_password']",RuntimeVariables.replace("test"));
  selenium.click("//input[@id='_58_rememberMeCheckbox']");
  selenium.clickAt("//input[@value='Sign In']",RuntimeVariables.replace("Sign In"));
  selenium.waitForPageToLoad("30000");
}
