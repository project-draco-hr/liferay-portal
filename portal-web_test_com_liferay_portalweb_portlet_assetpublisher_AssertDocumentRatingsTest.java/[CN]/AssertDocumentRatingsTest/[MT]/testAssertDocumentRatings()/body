{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Asset Publisher Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("link=AP Setup Test Document"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//img[5]");
  Thread.sleep(5000);
  assertTrue(selenium.isTextPresent("Average (1 Vote)"));
}
