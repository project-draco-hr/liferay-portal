{
  Session session=null;
  try {
    session=HibernateUtil.openSession();
    String createDateComparator=StringPool.GREATER_THAN;
    if (before) {
      createDateComparator=StringPool.LESS_THAN;
    }
    String sql=CustomSQLUtil.get(FIND_BY_CREATEDATE);
    sql=StringUtil.replace(sql,"[$CREATE_DATE_COMPARATOR$]",createDateComparator);
    SQLQuery q=session.createSQLQuery(sql);
    q.addEntity("WikiPage",WikiPageImpl.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(nodeId);
    qPos.add(createDate);
    qPos.add(true);
    return (List<WikiPage>)QueryUtil.list(q,HibernateUtil.getDialect(),begin,end);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    HibernateUtil.closeSession(session);
  }
}
