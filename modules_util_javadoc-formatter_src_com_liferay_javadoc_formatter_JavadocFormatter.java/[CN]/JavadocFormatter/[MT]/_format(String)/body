{
  File file=new File(_inputDirName,fileName);
  String originalContent=_read(file);
  if (fileName.contains("modules/third-party") || fileName.endsWith("Application.java") || fileName.endsWith("JavadocFormatter.java")|| fileName.endsWith("SourceFormatter.java")|| fileName.endsWith("WebProxyPortlet.java")|| _hasGeneratedTag(originalContent)) {
    return;
  }
  String newContent=ToolsUtil.stripFullyQualifiedClassNames(originalContent);
  JavaClass javaClass=_getJavaClass(fileName,new UnsyncStringReader(newContent));
  String javadocLessContent=_removeJavadocFromJava(javaClass,newContent);
  Document document=_getJavadocDocument(javaClass);
  _updateJavadocsXmlFile(fileName,javaClass,document);
  newContent=_getUpdateJavaFromDocument(fileName,javadocLessContent,document);
  if (!originalContent.equals(newContent)) {
    _write(file,newContent);
    _modifiedFileNames.add(file.getAbsolutePath());
    System.out.println("Writing " + file);
  }
}
