{
  DDMFormFieldValue ddmFormFieldValue=DDMFormValuesTestUtil.createDDMFormFieldValue("Text",new UnlocalizedValue("Scott Joplin"));
  TextDDMFormFieldValueAccessor textDDMFormFieldValueAccessor=new TextDDMFormFieldValueAccessor(LocaleUtil.US);
  Assert.assertEquals("Scott Joplin",textDDMFormFieldValueAccessor.get(ddmFormFieldValue));
}
