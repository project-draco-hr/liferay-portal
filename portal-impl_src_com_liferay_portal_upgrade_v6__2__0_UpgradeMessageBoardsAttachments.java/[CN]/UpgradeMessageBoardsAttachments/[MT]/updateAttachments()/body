{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select messageId, groupId, companyId, userId, userName, " + "threadId from MBMessage where classNameId = 0 and " + "classPK = 0");
    rs=ps.executeQuery();
    while (rs.next()) {
      long messageId=rs.getLong("messageId");
      long groupId=rs.getLong("groupId");
      long companyId=rs.getLong("companyId");
      long userId=rs.getLong("userId");
      String userName=rs.getString("userName");
      long threadId=rs.getLong("threadId");
      updateEntryAttachments(companyId,groupId,messageId,threadId,userId,userName);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
