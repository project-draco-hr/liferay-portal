{
  MBMessageServiceSoapServiceLocator locator=new MBMessageServiceSoapServiceLocator();
  MBMessageServiceSoap soap=locator.getPortlet_Message_Boards_MBMessageService(getURL("Portlet_Message_Boards_MBMessageService"));
  MBMessageModel message=soap.addMessage("1","Test Subject","This is a test body.",new Object[0],false,true,true);
  soap.deleteMessage(message.getTopicId(),message.getMessageId());
}
