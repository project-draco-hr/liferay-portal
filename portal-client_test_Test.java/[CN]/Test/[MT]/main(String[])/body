{
  try {
    LayoutServiceSoapServiceLocator locator=new LayoutServiceSoapServiceLocator();
    LayoutServiceSoap soap=locator.getPortal_LayoutService(new URL("http://localhost:8080/tunnel/axis/Portal_LayoutService"));
    LayoutModel[] layouts=soap.getLayouts("liferay.com","56","article-id","PRODUCTS-LICENSING");
    for (int i=0; i < layouts.length; i++) {
      System.out.println(layouts[i].getLayoutId());
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
