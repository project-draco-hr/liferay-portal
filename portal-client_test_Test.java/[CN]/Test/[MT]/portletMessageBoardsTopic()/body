{
  MBTopicServiceSoapServiceLocator locator=new MBTopicServiceSoapServiceLocator();
  MBTopicServiceSoap soap=locator.getPortlet_Message_Boards_MBTopicService(getURL("Portlet_Message_Boards_MBTopicService"));
  MBTopicModel topic=soap.addTopic("1","Test Topic","This is a test topic.",true,true);
  soap.deleteTopic(topic.getTopicId());
}
