{
  MBCategoryServiceSoapServiceLocator locator=new MBCategoryServiceSoapServiceLocator();
  MBCategoryServiceSoap soap=locator.getPortlet_Message_Boards_MBCategoryService(getURL("Portlet_Message_Boards_MBCategoryService"));
  MBCategoryModel category=soap.addCategory("PRI.3.1","-1","Test Category","This is a test category.",true,true);
  soap.deleteCategory(category.getCategoryId());
}
