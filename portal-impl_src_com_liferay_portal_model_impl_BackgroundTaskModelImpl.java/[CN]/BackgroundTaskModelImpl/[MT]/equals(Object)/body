{
  if (this == obj) {
    return true;
  }
  if (!(obj instanceof BackgroundTask)) {
    return false;
  }
  BackgroundTask backgroundTask=(BackgroundTask)obj;
  long primaryKey=backgroundTask.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
