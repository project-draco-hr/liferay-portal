{
  if (!ArrayUtil.contains(PropsValues.JAVASCRIPT_BUNDLE_IDS,bundleId)) {
    return requires;
  }
  String[] modules=PropsUtil.getArray(PropsKeys.JAVASCRIPT_BUNDLE_REQUIRES,new Filter(bundleId));
  for (  String moduleName : modules) {
    String[] dependencies=PropsUtil.getArray(PropsKeys.JAVASCRIPT_BUNDLE_REQUIRES,new Filter(moduleName));
    if (!ArrayUtil.contains(dependencies,bundleId)) {
      _getRequires(moduleName,requires);
    }
    requires.add(moduleName);
  }
  requires.add(bundleId);
  return requires;
}
