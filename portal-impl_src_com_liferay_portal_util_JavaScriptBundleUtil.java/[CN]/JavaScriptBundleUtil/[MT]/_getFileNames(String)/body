{
  String[] result={};
  List<String> requires=JavaScriptBundleUtil.getRequires(bundleId);
  Iterator<String> itr=requires.iterator();
  while (itr.hasNext()) {
    String id=itr.next();
    String[] fileNames=PropsUtil.getArray(id);
    result=ArrayUtil.append(result,fileNames);
  }
  return result;
}
