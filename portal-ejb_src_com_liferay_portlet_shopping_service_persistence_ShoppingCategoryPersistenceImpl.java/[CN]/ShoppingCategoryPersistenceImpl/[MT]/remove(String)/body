{
  Session session=null;
  try {
    session=openSession();
    ShoppingCategory shoppingCategory=(ShoppingCategory)session.get(ShoppingCategoryImpl.class,categoryId);
    if (shoppingCategory == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No ShoppingCategory exists with the primary key " + categoryId);
      }
      throw new NoSuchCategoryException("No ShoppingCategory exists with the primary key " + categoryId);
    }
    return remove(shoppingCategory);
  }
 catch (  NoSuchCategoryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
