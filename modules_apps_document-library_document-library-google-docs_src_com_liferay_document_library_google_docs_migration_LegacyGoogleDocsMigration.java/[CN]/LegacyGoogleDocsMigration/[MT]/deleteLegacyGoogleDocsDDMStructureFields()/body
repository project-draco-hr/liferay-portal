{
  DDMStructure legacyDDMStructure=LegacyGoogleDocsMetadataHelper.getGoogleDocsDDMStructure(_dlFileEntryType);
  String definition=legacyDDMStructure.getDefinition();
  definition=definition.replaceAll("(?s)<dynamic-element[^>]*>.*?</dynamic-element>","");
  com.liferay.dynamic.data.mapping.model.DDMStructure ddmStructure=_ddmStructureLocalService.getDDMStructure(legacyDDMStructure.getStructureId());
  ddmStructure.setDefinition(definition);
  _ddmStructureLocalService.updateDDMStructure(ddmStructure);
}
