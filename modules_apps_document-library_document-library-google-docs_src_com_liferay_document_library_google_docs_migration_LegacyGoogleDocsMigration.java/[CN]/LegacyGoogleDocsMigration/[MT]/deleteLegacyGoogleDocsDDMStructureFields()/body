{
  DDMStructure legacyDDMStructure=LegacyGoogleDocsMetadataHelper.getGoogleDocsDDMStructure(_dlFileEntryType);
  String definition=legacyDDMStructure.getDefinition();
  definition=definition.replaceAll("(?s)<dynamic-element[^>]*>.*?</dynamic-element>","");
  legacyDDMStructure.setDefinition(definition);
  DDMStructureLocalServiceUtil.updateDDMStructure(legacyDDMStructure);
}
