{
  if (_autoImportLibraries == null) {
    return;
  }
  for (  Map.Entry<String,TemplateResource> entry : _autoImportLibraries.entrySet()) {
    String key=entry.getKey();
    TemplateResource templateResource=entry.getValue();
    if (templateResource == null) {
      _log.error("Unable to find template resource " + templateResource.getTemplateId());
      continue;
    }
    Reader reader=templateResource.getReader();
    if (reader == null) {
      _log.error("Unable to find template resource " + templateResource);
      continue;
    }
    readers.add(reader);
    freemarker.template.Template importTemplate=new freemarker.template.Template(templateResource.getTemplateId(),reader,_configuration,TemplateResource.DEFAUT_ENCODING);
    environment.importLib(importTemplate,key);
  }
}
