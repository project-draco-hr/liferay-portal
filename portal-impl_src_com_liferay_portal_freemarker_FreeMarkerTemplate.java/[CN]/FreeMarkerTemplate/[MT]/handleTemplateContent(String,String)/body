{
  if (Validator.isNotNull(templateContent) && (!PropsValues.LAYOUT_TEMPLATE_CACHE_ENABLED || !stringTemplateExists(templateId))) {
    _stringTemplateLoader.putTemplate(templateId,templateContent);
    if (_log.isDebugEnabled()) {
      _log.debug("Added " + templateId + " to the string based FreeMarker "+ "template repository");
    }
  }
}
