{
  super(templateResource,errorTemplateResource,templateContextHelper,_templateResourceLoaderName,PropsValues.FREEMARKER_ENGINE_RESOURCE_MODIFICATION_CHECK_INTERVAL);
  _context=new HashMap<String,Object>();
  if (context != null) {
    for (    Map.Entry<String,Object> entry : context.entrySet()) {
      put(entry.getKey(),entry.getValue());
    }
  }
  _configuration=configuration;
}
