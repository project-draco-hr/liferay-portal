{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//a[@id='my-community-private-pages']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("//a[@id='my-community-private-pages']"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//div[@id='add-page']/a/span");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("new_page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.typeKeys("new_page",RuntimeVariables.replace("World of Liferay Test Page"));
  selenium.type("new_page",RuntimeVariables.replace("World of Liferay Test Page"));
  selenium.click("link=Save");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=World of Liferay Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=World of Liferay Test Page"));
  selenium.waitForPageToLoad("30000");
}
