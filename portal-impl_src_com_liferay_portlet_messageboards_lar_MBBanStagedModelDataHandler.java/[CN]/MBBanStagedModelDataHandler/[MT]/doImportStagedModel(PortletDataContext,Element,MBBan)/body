{
  User user=UserUtil.fetchByUuid_C_First(ban.getBanUserUuid(),portletDataContext.getCompanyId(),null);
  if (user == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to find banned user with uuid " + ban.getBanUserUuid());
    }
    return;
  }
  long userId=portletDataContext.getUserId(ban.getUserUuid());
  ServiceContext serviceContext=portletDataContext.createServiceContext(element,ban,MBPortletDataHandler.NAMESPACE);
  MBBanLocalServiceUtil.addBan(userId,user.getUserId(),serviceContext);
}
