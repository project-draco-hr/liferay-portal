{
  Element userBanElement=portletDataContext.getExportDataElement(ban);
  ban.setBanUserUuid(ban.getBanUserUuid());
  User bannedUser=UserLocalServiceUtil.getUser(ban.getUserId());
  portletDataContext.addReferenceElement(ban,userBanElement,bannedUser,User.class,PortletDataContext.REFERENCE_TYPE_DEPENDENCY_DISPOSABLE,true);
  portletDataContext.addClassedModel(userBanElement,ExportImportPathUtil.getModelPath(ban),ban);
}
