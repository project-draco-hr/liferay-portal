{
  if (SearchEngineUtil.isIndexReadOnly()) {
    return;
  }
synchronized (this) {
    IndexReader indexReader=null;
    try {
      indexReader=IndexReader.open(LuceneHelperUtil.getLuceneDir(companyId),false);
      indexReader.deleteDocuments(term);
    }
  finally {
      if (indexReader != null) {
        indexReader.close();
      }
    }
  }
}
