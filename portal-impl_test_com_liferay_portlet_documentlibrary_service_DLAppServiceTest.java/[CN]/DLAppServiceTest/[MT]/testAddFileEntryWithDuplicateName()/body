{
  addFileEntry(false);
  try {
    addFileEntry(false);
    fail("Able to add two files of the same name");
  }
 catch (  DuplicateFileException dfe) {
  }
  try {
    addFileEntry(true);
  }
 catch (  DuplicateFileException dfe) {
    fail("Unable to add two files of the same name in different " + "folders");
  }
  DLAppServiceUtil.deleteFileEntry(_fileEntry.getFileEntryId());
  _fileEntry=null;
}
