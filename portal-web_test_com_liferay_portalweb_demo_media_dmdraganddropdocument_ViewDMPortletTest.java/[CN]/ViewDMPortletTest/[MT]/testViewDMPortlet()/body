{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Documents and Media Test Page");
  selenium.clickAt("link=Documents and Media Test Page",RuntimeVariables.replace("Documents and Media Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Home"),selenium.getText("//div[@id='_20_folderContainer']/ul/li[1]/a"));
  assertEquals(RuntimeVariables.replace("Recent"),selenium.getText("//div[@id='_20_folderContainer']/ul/li[2]/a"));
  assertEquals(RuntimeVariables.replace("Mine"),selenium.getText("//div[@id='_20_folderContainer']/ul/li[3]/a"));
}
