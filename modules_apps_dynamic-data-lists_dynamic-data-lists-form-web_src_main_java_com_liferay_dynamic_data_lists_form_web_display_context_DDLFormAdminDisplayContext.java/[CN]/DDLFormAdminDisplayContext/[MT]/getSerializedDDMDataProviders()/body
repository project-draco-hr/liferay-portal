{
  ThemeDisplay themeDisplay=_ddlFormAdminRequestHelper.getThemeDisplay();
  List<DDMDataProviderInstance> ddmDataProviderInstances=_ddmDataProviderInstanceLocalService.getDataProviderInstances(PortalUtil.getCurrentAndAncestorSiteGroupIds(themeDisplay.getScopeGroupId()));
  return serialize(ddmDataProviderInstances,themeDisplay.getLocale());
}
