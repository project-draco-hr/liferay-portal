{
  RecordSetSearchTerms searchTerms=(RecordSetSearchTerms)searchContainer.getSearchTerms();
  if (searchTerms.isAdvancedSearch()) {
    return DDLRecordSetServiceUtil.search(_ddlFormAdminRequestHelper.getCompanyId(),_ddlFormAdminRequestHelper.getScopeGroupId(),searchTerms.getName(),searchTerms.getDescription(),DDLRecordSetConstants.SCOPE_FORMS,searchTerms.isAndOperator(),searchContainer.getStart(),searchContainer.getEnd(),searchContainer.getOrderByComparator());
  }
 else {
    return DDLRecordSetServiceUtil.search(_ddlFormAdminRequestHelper.getCompanyId(),_ddlFormAdminRequestHelper.getScopeGroupId(),searchTerms.getKeywords(),DDLRecordSetConstants.SCOPE_FORMS,searchContainer.getStart(),searchContainer.getEnd(),searchContainer.getOrderByComparator());
  }
}
