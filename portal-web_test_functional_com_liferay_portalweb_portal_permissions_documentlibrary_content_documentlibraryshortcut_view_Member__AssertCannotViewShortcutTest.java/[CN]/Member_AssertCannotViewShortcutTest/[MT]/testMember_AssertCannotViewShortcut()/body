{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Control Panel");
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Documents and Media",RuntimeVariables.replace("Documents and Media"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("TestDocument.txt"),selenium.getText("xPath=(//a[contains(@class,'document-link')]/span[@class='entry-title'])[1]"));
  assertTrue(selenium.isElementNotPresent("xPath=(//a[contains(@class,'document-link')]/span[@class='entry-title'])[2]"));
}
