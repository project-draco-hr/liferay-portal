{
  if (obj == null) {
    return false;
  }
  ResourceTypePermissionPK pk=null;
  try {
    pk=(ResourceTypePermissionPK)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  if ((companyId == pk.companyId) && (groupId == pk.groupId) && (name.equals(pk.name))&& (roleId == pk.roleId)) {
    return true;
  }
 else {
    return false;
  }
}
