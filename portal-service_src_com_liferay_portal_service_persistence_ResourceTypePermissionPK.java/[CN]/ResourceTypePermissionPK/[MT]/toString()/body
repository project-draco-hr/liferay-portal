{
  StringBundler sb=new StringBundler(20);
  sb.append(StringPool.OPEN_CURLY_BRACE);
  sb.append("companyId");
  sb.append(StringPool.EQUAL);
  sb.append(companyId);
  sb.append(StringPool.COMMA);
  sb.append(StringPool.SPACE);
  sb.append("groupId");
  sb.append(StringPool.EQUAL);
  sb.append(groupId);
  sb.append(StringPool.COMMA);
  sb.append(StringPool.SPACE);
  sb.append("name");
  sb.append(StringPool.EQUAL);
  sb.append(name);
  sb.append(StringPool.COMMA);
  sb.append(StringPool.SPACE);
  sb.append("roleId");
  sb.append(StringPool.EQUAL);
  sb.append(roleId);
  sb.append(StringPool.CLOSE_CURLY_BRACE);
  return sb.toString();
}
