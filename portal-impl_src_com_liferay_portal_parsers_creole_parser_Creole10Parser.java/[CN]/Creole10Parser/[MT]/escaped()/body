{
  ScapedNode scaped=new ScapedNode();
  Token c=null;
  try {
{
      match(input,ESCAPE,FOLLOW_ESCAPE_in_escaped4807);
      if (failed)       return scaped;
      c=(Token)input.LT(1);
      matchAny(input);
      if (failed)       return scaped;
      if (backtracking == 0) {
        scaped.setContent(c.getText());
      }
    }
  }
 catch (  RecognitionException re) {
    reportError(re);
    recover(input,re);
  }
 finally {
  }
  return scaped;
}
