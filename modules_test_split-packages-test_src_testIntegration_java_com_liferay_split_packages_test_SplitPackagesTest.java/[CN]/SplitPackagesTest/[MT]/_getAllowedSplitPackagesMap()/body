{
  Map<ExportPackage,SplitPackages> allowedSplitPackagesMap=new HashMap<>();
  for (  String splitPackagesLine : StringUtil.splitLines(StringUtil.read(SplitPackagesTest.class.getResourceAsStream("AllowedSplitPackages.txt")))) {
    String[] splitPackagesParts=StringUtil.split(splitPackagesLine,';');
    ExportPackage exportPackage=new ExportPackage(splitPackagesParts[0],splitPackagesParts[1]);
    allowedSplitPackagesMap.put(exportPackage,new SplitPackages(exportPackage,SetUtil.fromArray(StringUtil.split(splitPackagesParts[2]))));
  }
  return allowedSplitPackagesMap;
}
