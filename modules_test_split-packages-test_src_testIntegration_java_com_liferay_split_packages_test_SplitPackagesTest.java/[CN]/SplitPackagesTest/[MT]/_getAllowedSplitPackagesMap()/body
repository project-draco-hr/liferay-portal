{
  Class clazz=this.getClass();
  ClassLoader classLoader=clazz.getClassLoader();
  Map<String,SplitPackages> allowedSplitPackagesMap=new HashMap<>();
  for (  String packages : StringUtil.split(IOUtils.toString(classLoader.getResourceAsStream("SuitableImports"),StringPool.UTF8),'\n')) {
    String[] packageSplit=StringUtil.split(packages,';');
    allowedSplitPackagesMap.put(packageSplit[0],new SplitPackages(new ExportPackage(packageSplit[0],packageSplit[1]),SetUtil.fromArray(StringUtil.split(packageSplit[2]))));
  }
  return allowedSplitPackagesMap;
}
