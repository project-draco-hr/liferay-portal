{
  Map<String,SplitPackages> allowedSplitPackagesMap=new HashMap<>();
  for (  String splitPackagesLine : StringUtil.splitLines(StringUtil.read(SplitPackagesTest.class.getResourceAsStream("AllowedSplitPackages.txt")))) {
    String[] splitPackagesParts=StringUtil.split(splitPackagesLine,';');
    allowedSplitPackagesMap.put(splitPackagesParts[0],new SplitPackages(new ExportPackage(splitPackagesParts[0],splitPackagesParts[1]),SetUtil.fromArray(StringUtil.split(splitPackagesParts[2]))));
  }
  return allowedSplitPackagesMap;
}
