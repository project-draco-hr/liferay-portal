{
  for (  ExportPackage duplicatedExportPackage : duplicatedExportPackages) {
    Set<String> symbolicNames=allowedSplitPackages.get(duplicatedExportPackage);
    if ((symbolicNames == null) || !symbolicNames.contains(currentBundle.getSymbolicName())) {
      Assert.fail("Detected split packages " + duplicatedExportPackage + " in "+ mapBundle+ " and "+ currentBundle);
    }
  }
}
