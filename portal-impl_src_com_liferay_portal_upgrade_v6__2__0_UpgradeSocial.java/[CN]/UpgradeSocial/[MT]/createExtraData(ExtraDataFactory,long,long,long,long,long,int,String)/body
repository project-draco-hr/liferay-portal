{
  if (extraDataGenerator == null) {
    return null;
  }
  String result=null;
  try (PreparedStatement ps=connection.prepareStatement(extraDataGenerator.getEntityQuery())){
    extraDataGenerator.setEntityQueryParameters(ps,groupId,companyId,userId,classNameId,classPK,type,extraData);
    try (ResultSet rs=ps.executeQuery()){
      JSONObject extraDataJSONObject=null;
      while (rs.next()) {
        extraDataJSONObject=extraDataGenerator.createExtraDataJSONObject(rs,extraData);
      }
      result=extraDataJSONObject.toString();
    }
   }
   return result;
}
