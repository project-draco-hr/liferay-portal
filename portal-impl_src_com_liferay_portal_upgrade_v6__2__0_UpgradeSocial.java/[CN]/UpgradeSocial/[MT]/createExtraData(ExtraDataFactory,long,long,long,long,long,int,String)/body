{
  if (extraDataFactory == null) {
    return null;
  }
  try (PreparedStatement ps=connection.prepareStatement(extraDataFactory.getSQL())){
    extraDataFactory.setModelSQLParameters(ps,groupId,companyId,userId,classNameId,classPK,type,extraData);
    try (ResultSet rs=ps.executeQuery()){
      JSONObject extraDataJSONObject=null;
      while (rs.next()) {
        extraDataJSONObject=extraDataFactory.createExtraDataJSONObject(rs,extraData);
      }
      return extraDataJSONObject.toString();
    }
   }
 }
