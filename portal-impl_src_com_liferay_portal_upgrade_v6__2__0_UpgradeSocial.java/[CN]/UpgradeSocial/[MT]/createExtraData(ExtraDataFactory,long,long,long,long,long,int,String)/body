{
  if (extraDataGenerator == null) {
    return null;
  }
  try (PreparedStatement ps=connection.prepareStatement(extraDataGenerator.getSQL())){
    extraDataGenerator.setModelSQLParameters(ps,groupId,companyId,userId,classNameId,classPK,type,extraData);
    try (ResultSet rs=ps.executeQuery()){
      JSONObject extraDataJSONObject=null;
      while (rs.next()) {
        extraDataJSONObject=extraDataGenerator.createExtraDataJSONObject(rs,extraData);
      }
      return extraDataJSONObject.toString();
    }
   }
 }
