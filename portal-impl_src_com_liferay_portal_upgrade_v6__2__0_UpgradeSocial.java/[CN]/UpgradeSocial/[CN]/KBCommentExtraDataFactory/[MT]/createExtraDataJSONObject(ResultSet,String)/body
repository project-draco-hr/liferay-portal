{
  JSONObject extraDataJSONObject=null;
  long classnameId=entityResultSet.getLong("classNameId");
  long classpk=entityResultSet.getLong("classPK");
  ExtraDataFactory extraDataGenerator=null;
  if (classnameId == PortalUtil.getClassNameId(_kbArticleExtraDataGenerator.getActivityClassName())) {
    extraDataGenerator=_kbArticleExtraDataGenerator;
  }
 else   if (classnameId == PortalUtil.getClassNameId(_kbTemplateExtraDataGenerator.getActivityClassName())) {
    extraDataGenerator=_kbTemplateExtraDataGenerator;
  }
  if (extraDataGenerator != null) {
    try (PreparedStatement ps=connection.prepareStatement(extraDataGenerator.getSQL())){
      ps.setLong(1,classpk);
      try (ResultSet rs=ps.executeQuery()){
        while (rs.next()) {
          extraDataJSONObject=extraDataGenerator.createExtraDataJSONObject(rs,StringPool.BLANK);
        }
      }
     }
   }
  return extraDataJSONObject;
}
