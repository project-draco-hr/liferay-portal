{
  Matcher matcher=Pattern.compile("\\[{2}([^\\]]*)\\]{2}",Pattern.DOTALL).matcher(content);
  while (matcher.find()) {
    String linkWithUnderscores=matcher.group(1);
    String link=linkWithUnderscores.replaceAll(StringPool.UNDERLINE,StringPool.SPACE);
    content=content.replaceAll(linkWithUnderscores,link);
  }
  return TABLE_OF_CONTENTS + super.postProcess(content);
}
