{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(BrowserTracker.class,BrowserTracker.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("browserTrackerId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("browserTrackerId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
