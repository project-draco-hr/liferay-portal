{
  InputStream inputStream=null;
  try {
    inputStream=new FileInputStream(_file);
    return extractMetadata(inputStream,_metadata,_parser);
  }
 catch (  IOException ioe) {
    throw new ProcessException(ioe);
  }
 finally {
    if (inputStream != null) {
      try {
        inputStream.close();
      }
 catch (      IOException ioe) {
        throw new ProcessException(ioe);
      }
    }
  }
}
