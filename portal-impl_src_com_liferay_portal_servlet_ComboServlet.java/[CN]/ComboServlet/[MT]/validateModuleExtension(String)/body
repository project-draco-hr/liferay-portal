{
  moduleName=getResourcePath(moduleName);
  int index=moduleName.indexOf(CharPool.QUESTION);
  if (index != -1) {
    moduleName=moduleName.substring(0,index);
  }
  boolean validModuleExtension=false;
  String[] fileExtensions=PrefsPropsUtil.getStringArray(PropsKeys.COMBO_ALLOWED_FILE_EXTENSIONS,StringPool.COMMA);
  for (  String fileExtension : fileExtensions) {
    if (StringPool.STAR.equals(fileExtension) || StringUtil.endsWith(moduleName,fileExtension)) {
      validModuleExtension=true;
      break;
    }
  }
  return validModuleExtension;
}
