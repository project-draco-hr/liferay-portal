{
  StringBundler sb=new StringBundler(4);
  sb.append(VirtualLayoutConstants.CANONICAL_URL_SEPARATOR);
  try {
    Group group=_sourceLayout.getGroup();
    sb.append(group.getFriendlyURL());
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  sb.append(_sourceLayout.getFriendlyURL());
  return sb.toString();
}
