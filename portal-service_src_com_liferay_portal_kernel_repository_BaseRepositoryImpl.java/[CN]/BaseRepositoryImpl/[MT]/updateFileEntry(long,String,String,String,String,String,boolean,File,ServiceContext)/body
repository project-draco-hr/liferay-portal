{
  InputStream is=null;
  long size=0;
  try {
    is=new FileInputStream(file);
    size=file.length();
    return updateFileEntry(fileEntryId,sourceFileName,mimeType,title,description,changeLog,majorVersion,is,size,serviceContext);
  }
 catch (  IOException ioe) {
    throw new SystemException(ioe);
  }
 finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
}
