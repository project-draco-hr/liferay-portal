{
  if (((emailAddressId == null) && (_emailAddressId != null)) || ((emailAddressId != null) && (_emailAddressId == null)) || ((emailAddressId != null) && (_emailAddressId != null) && !emailAddressId.equals(_emailAddressId))) {
    if (!XSS_ALLOW_EMAILADDRESSID) {
      emailAddressId=XSSUtil.strip(emailAddressId);
    }
    _emailAddressId=emailAddressId;
    setModified(true);
  }
}
