{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select plid from Layout where uuid_ is null");
    rs=ps.executeQuery();
    while (rs.next()) {
      long plid=rs.getLong("plid");
      runSQL("update Layout set uuid_ = '" + PortalUUIDUtil.generate() + "' where plid = "+ plid);
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
