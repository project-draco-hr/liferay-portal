{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps=connection.prepareStatement("select plid from Layout where uuid_ is null");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long plid=rs.getLong("plid");
      runSQL("update Layout set uuid_ = '" + PortalUUIDUtil.generate() + "' where plid = "+ plid);
    }
  }
 }
