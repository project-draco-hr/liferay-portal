{
  Folder folder=BlogsEntryLocalServiceUtil.addAttachmentsFolder(themeDisplay.getUserId(),themeDisplay.getScopeGroupId());
  return PortletFileRepositoryUtil.addPortletFileEntry(themeDisplay.getScopeGroupId(),themeDisplay.getUserId(),BlogsEntry.class.getName(),0,BlogsConstants.SERVICE_NAME,folder.getFolderId(),inputStream,uniqueFileName,contentType,true);
}
