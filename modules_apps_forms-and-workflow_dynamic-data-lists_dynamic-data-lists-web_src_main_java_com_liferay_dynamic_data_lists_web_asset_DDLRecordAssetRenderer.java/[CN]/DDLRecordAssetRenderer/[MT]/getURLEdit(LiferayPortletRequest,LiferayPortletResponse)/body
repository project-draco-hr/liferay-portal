{
  Group group=GroupLocalServiceUtil.fetchGroup(_record.getGroupId());
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(liferayPortletRequest,group,DDLPortletKeys.DYNAMIC_DATA_LISTS,0,0,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/edit_record.jsp");
  portletURL.setParameter("recordId",String.valueOf(_record.getRecordId()));
  portletURL.setParameter("version",_recordVersion.getVersion());
  return portletURL;
}
