{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AssetCategory.class,AssetCategory.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("categoryId",ServiceTestUtil.nextLong()));
  List<AssetCategory> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
