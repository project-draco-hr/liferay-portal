{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AssetCategory.class,AssetCategory.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("categoryId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("categoryId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
