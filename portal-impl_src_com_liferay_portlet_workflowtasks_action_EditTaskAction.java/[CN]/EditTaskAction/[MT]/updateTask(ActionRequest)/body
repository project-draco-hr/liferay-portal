{
  long taskIntanceId=ParamUtil.getLong(actionRequest,"taskIntanceId");
  String activityName=ParamUtil.getString(actionRequest,"activityName");
  User user=PortalUtil.getUser(actionRequest);
  String comment=StringPool.BLANK;
  if (Validator.isNull(activityName)) {
    TaskInstanceManagerUtil.completeTaskInstance(taskIntanceId,user.getUserId(),comment,null);
  }
 else {
    TaskInstanceManagerUtil.completeTaskInstance(taskIntanceId,user.getUserId(),activityName,comment,null);
  }
}
