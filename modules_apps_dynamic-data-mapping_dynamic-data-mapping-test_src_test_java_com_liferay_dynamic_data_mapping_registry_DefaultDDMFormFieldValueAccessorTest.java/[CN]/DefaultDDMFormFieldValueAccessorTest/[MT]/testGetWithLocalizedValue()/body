{
  Value value=new LocalizedValue(LocaleUtil.US);
  value.addString(LocaleUtil.BRAZIL,"Portuguese value");
  value.addString(LocaleUtil.US,"English value");
  DDMFormFieldValue ddmFormFieldValue=DDMFormValuesTestUtil.createDDMFormFieldValue("Text",value);
  Assert.assertEquals("Portuguese value",_defaultDDMFormFieldValueAccessor.getValue(ddmFormFieldValue,LocaleUtil.BRAZIL));
  Assert.assertEquals("English value",_defaultDDMFormFieldValueAccessor.getValue(ddmFormFieldValue,LocaleUtil.US));
}
