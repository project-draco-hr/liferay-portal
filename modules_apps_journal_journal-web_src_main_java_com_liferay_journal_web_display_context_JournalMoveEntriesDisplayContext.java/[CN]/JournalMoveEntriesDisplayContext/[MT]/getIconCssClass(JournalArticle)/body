{
  if (_journalArticleAssetRendererFactory == null) {
    _journalArticleAssetRendererFactory=AssetRendererFactoryRegistryUtil.getAssetRendererFactoryByClass(JournalArticle.class);
  }
  AssetRenderer<JournalArticle> assetRenderer=_journalArticleAssetRendererFactory.getAssetRenderer(JournalArticleAssetRenderer.getClassPK(article));
  return assetRenderer.getIconCssClass();
}
