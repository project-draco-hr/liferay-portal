{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//a[@id=\"my-community-private-pages\"]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("//a[@id=\"my-community-private-pages\"]"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//div/a/span");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("new_page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.typeKeys("new_page",RuntimeVariables.replace("Toda in Christian Histor Test Page"));
  selenium.type("new_page",RuntimeVariables.replace("Today in Christian History Test Page"));
  selenium.click("link=Save");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Today in Christian History Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Today in Christian History Test Page"));
  selenium.waitForPageToLoad("30000");
}
