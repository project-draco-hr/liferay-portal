{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(portletRequest,SiteMembershipsPortletKeys.SITE_MEMBERSHIPS_ADMIN,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/view_membership_requests.jsp");
  try {
    portletURL.setWindowState(LiferayWindowState.POP_UP);
  }
 catch (  WindowStateException wse) {
  }
  return portletURL.toString();
}
