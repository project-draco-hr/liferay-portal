{
  try {
    InputStream is=null;
    long size=0;
    if (file == null) {
      is=new UnsyncByteArrayInputStream(new byte[0]);
    }
 else {
      is=new FileInputStream(file);
      size=file.length();
    }
    return addFileEntry(userId,repositoryId,folderId,mimeType,title,description,changeLog,is,size,serviceContext);
  }
 catch (  FileNotFoundException fnfe) {
    throw new FileSizeException();
  }
}
