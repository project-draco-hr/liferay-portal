{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/announcements-entry-community/announcements-entry-page");
  selenium.waitForVisible("link=Announcements Entry Page");
  selenium.clickAt("link=Announcements Entry Page",RuntimeVariables.replace("Announcements Entry Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Announcements Entry Name"),selenium.getText("//h3[@class='entry-title']/a"));
  assertTrue(selenium.isPartialText("//div[contains(@class,'entry-content')]","Announcements Entry Content"));
  selenium.clickAt("//h3[@class='entry-title']/a",RuntimeVariables.replace("Announcements Entry Name"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("//img[@alt='Liferay']"));
}
