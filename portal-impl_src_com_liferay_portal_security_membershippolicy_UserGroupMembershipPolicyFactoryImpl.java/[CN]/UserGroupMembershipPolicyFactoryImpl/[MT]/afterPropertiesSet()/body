{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.MEMBERSHIP_POLICY_USER_GROUPS);
  }
  ClassLoader classLoader=PACLClassLoaderUtil.getPortalClassLoader();
  _originalMembershipPolicy=(UserGroupMembershipPolicy)InstanceFactory.newInstance(classLoader,PropsValues.MEMBERSHIP_POLICY_USER_GROUPS);
  _membershipPolicy=_originalMembershipPolicy;
}
