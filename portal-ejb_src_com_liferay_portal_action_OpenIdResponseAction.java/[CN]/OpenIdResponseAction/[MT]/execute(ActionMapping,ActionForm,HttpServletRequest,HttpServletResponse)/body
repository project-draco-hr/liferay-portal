{
  try {
    User user=_readResponse(req);
  }
 catch (  Exception e) {
    if (e instanceof ConsumerException || e instanceof AssociationException || e instanceof MessageException|| e instanceof DiscoveryException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portal.login");
    }
 else {
      req.setAttribute(PageContext.EXCEPTION,e);
      return mapping.findForward(Constants.COMMON_ERROR);
    }
  }
  ThemeDisplay themeDisplay=(ThemeDisplay)req.getAttribute(WebKeys.THEME_DISPLAY);
  String loginURL=PortalUtil.getPortalURL(req) + themeDisplay.getPathMain() + "/portal/login";
  res.sendRedirect(loginURL);
  return null;
}
