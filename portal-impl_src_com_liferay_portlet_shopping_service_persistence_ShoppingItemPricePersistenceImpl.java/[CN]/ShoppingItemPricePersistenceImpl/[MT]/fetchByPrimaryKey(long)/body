{
  ShoppingItemPrice shoppingItemPrice=(ShoppingItemPrice)EntityCacheUtil.getResult(ShoppingItemPriceModelImpl.ENTITY_CACHE_ENABLED,ShoppingItemPriceImpl.class,itemPriceId,this);
  if (shoppingItemPrice == null) {
    Session session=null;
    try {
      session=openSession();
      shoppingItemPrice=(ShoppingItemPrice)session.get(ShoppingItemPriceImpl.class,new Long(itemPriceId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (shoppingItemPrice != null) {
        cacheResult(shoppingItemPrice);
      }
      closeSession(session);
    }
  }
  return shoppingItemPrice;
}
