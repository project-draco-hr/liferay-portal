{
  ShoppingItemPrice result=(ShoppingItemPrice)EntityCacheUtil.getResult(ShoppingItemPriceModelImpl.ENTITY_CACHE_ENABLED,ShoppingItemPrice.class,itemPriceId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      ShoppingItemPrice shoppingItemPrice=(ShoppingItemPrice)session.get(ShoppingItemPriceImpl.class,new Long(itemPriceId));
      if (shoppingItemPrice != null) {
        cacheResult(shoppingItemPrice);
      }
      return shoppingItemPrice;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (ShoppingItemPrice)result;
  }
}
