{
  FinderCache.clearCache(ShoppingItemPrice.class.getName());
  Session session=null;
  try {
    session=openSession();
    session.delete(shoppingItemPrice);
    session.flush();
    return shoppingItemPrice;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
