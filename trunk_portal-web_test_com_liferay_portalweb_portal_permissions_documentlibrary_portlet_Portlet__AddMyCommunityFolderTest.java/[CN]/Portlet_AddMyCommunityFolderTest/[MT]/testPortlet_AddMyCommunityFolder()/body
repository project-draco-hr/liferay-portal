{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Document Library")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Document Library",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Add Folder"),selenium.getText("//div[@class='lfr-component lfr-menu-list lfr-menu-expanded right null']/ul/li[2]/a"));
  selenium.click(RuntimeVariables.replace("//div[@class='lfr-component lfr-menu-list lfr-menu-expanded right null']/ul/li[2]/a"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.type("_20_name",RuntimeVariables.replace("My1 Community1 Folder1"));
  selenium.saveScreenShotAndSource();
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//section/div/div/div/div[1]"));
  assertEquals(RuntimeVariables.replace("My1 Community1 Folder1"),selenium.getText("//a/strong"));
}
