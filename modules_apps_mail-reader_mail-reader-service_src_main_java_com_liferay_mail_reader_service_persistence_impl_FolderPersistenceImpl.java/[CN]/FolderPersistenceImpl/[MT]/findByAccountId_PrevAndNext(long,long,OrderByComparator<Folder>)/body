{
  Folder folder=findByPrimaryKey(folderId);
  Session session=null;
  try {
    session=openSession();
    Folder[] array=new FolderImpl[3];
    array[0]=getByAccountId_PrevAndNext(session,folder,accountId,orderByComparator,true);
    array[1]=folder;
    array[2]=getByAccountId_PrevAndNext(session,folder,accountId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
