{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Image Gallery Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Image Gallery Permissions Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Portlet2 Temporary2 Folder2",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Add Subfolder"),selenium.getText("//div[2]/ul/li[1]/a"));
  selenium.clickAt("//div[2]/ul/li[1]/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_31_name",RuntimeVariables.replace("Portlet1 Permissions1 Subfolder1"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request completed successfully."));
  assertEquals(RuntimeVariables.replace("Portlet1 Permissions1 Subfolder1"),selenium.getText("//a/strong"));
}
