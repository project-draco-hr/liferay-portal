{
  Folder folder=new LiferayFolder(new DLFolderImpl());
  if (getFolderId() > 0) {
    try {
      folder=DLAppLocalServiceUtil.getFolder(getFolderId());
    }
 catch (    NoSuchFolderException nsfe) {
      try {
        if (!isInTrash()) {
          _log.error(nsfe,nsfe);
        }
      }
 catch (      Exception e) {
        _log.error(e,e);
      }
    }
catch (    Exception e) {
      _log.error(e,e);
    }
  }
  return folder;
}
