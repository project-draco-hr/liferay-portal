{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      Thread.sleep(2500);
    boolean NotInControlPanel=selenium.isElementPresent("//div[4]/ul/li[2]/a/span[1]");
  if (!NotInControlPanel) {
    label=2;
    continue;
  }
selenium.click(RuntimeVariables.replace("//div[@id='_145_myPlacesContainer']/ul/li[2]/a/span[1]"));
selenium.waitForPageToLoad("30000");
case 2:
for (int second=0; ; second++) {
if (second >= 60) {
fail("timeout");
}
try {
if (selenium.isElementPresent("link=Control Panel")) {
  break;
}
}
 catch (Exception e) {
}
Thread.sleep(1000);
}
selenium.clickAt("link=Control Panel",RuntimeVariables.replace(""));
selenium.waitForPageToLoad("30000");
case 100:
label=-1;
}
}
}
