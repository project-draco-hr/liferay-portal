{
  InputTag inputTag=(InputTag)findAncestorWithClass(this,InputTag.class);
  String name=getName();
  _custom=ModelHintsUtil.isCustomValidator(name);
  if (_custom) {
    name=ModelHintsUtil.processCustomValidatorName(name);
  }
  ValidatorTag validatorTag=new ValidatorTagImpl(name,getErrorMessage(),_body,_custom);
  inputTag.addValidatorTag(name,validatorTag);
  return EVAL_BODY_BUFFERED;
}
