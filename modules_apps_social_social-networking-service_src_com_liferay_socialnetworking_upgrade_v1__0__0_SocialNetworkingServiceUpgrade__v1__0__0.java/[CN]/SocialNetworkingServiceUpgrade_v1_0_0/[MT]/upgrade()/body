{
  Release release=_releaseLocalService.fetchRelease("social-networking-portlet");
  if (release.getBuildNumber() >= 100) {
    return;
  }
  List<UpgradeProcess> upgradeProcesses=Collections.<UpgradeProcess>singletonList(new UpgradeNamespace());
  _releaseLocalService.updateRelease("social-networking-portlet",upgradeProcesses,100,0,false);
}
