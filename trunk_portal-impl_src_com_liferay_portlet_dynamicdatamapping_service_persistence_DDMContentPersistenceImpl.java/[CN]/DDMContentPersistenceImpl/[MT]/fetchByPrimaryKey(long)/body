{
  DDMContent ddmContent=(DDMContent)EntityCacheUtil.getResult(DDMContentModelImpl.ENTITY_CACHE_ENABLED,DDMContentImpl.class,contentId,this);
  if (ddmContent == null) {
    Session session=null;
    try {
      session=openSession();
      ddmContent=(DDMContent)session.get(DDMContentImpl.class,Long.valueOf(contentId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (ddmContent != null) {
        cacheResult(ddmContent);
      }
      closeSession(session);
    }
  }
  return ddmContent;
}
