{
  EntityCacheUtil.putResult(DDMContentModelImpl.ENTITY_CACHE_ENABLED,DDMContentImpl.class,ddmContent.getPrimaryKey(),ddmContent);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddmContent.getUuid(),Long.valueOf(ddmContent.getGroupId())},ddmContent);
  ddmContent.resetOriginalValues();
}
