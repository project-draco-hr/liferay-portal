{
  Assert.assertTrue(LanguageValidator.isValid(LanguageConstants.KEY_DIR,"ltr"));
  Assert.assertTrue(LanguageValidator.isValid(LanguageConstants.KEY_DIR,"rtl"));
  Assert.assertFalse(LanguageValidator.isValid(LanguageConstants.KEY_DIR,"tlr"));
  Assert.assertFalse(LanguageValidator.isValid(LanguageConstants.KEY_DIR,"lrt"));
  Assert.assertFalse(LanguageValidator.isValid(LanguageConstants.KEY_DIR,"Any other value"));
}
