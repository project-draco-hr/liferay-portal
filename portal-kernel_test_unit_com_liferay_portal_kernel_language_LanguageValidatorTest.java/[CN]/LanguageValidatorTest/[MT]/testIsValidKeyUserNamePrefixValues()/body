{
  Assert.assertTrue(LanguageValidator.isValid(LanguageConstants.KEY_USER_NAME_PREFIX_VALUES,"Mr,Mrs,Dr"));
  Assert.assertTrue(LanguageValidator.isValid(LanguageConstants.KEY_USER_NAME_PREFIX_VALUES,"Any value at all"));
  Assert.assertFalse(LanguageValidator.isValid(LanguageConstants.KEY_USER_NAME_PREFIX_VALUES,""));
  Assert.assertFalse(LanguageValidator.isValid(LanguageConstants.KEY_USER_NAME_PREFIX_VALUES,null));
}
