{
  _portlet=portlet;
  _portletCtx=(PortletContextImpl)portletCtx;
  _expCache=expCache;
  if (ClassUtil.isSubclass(_portlet.getClass(),Constants.JSF_MYFACES) || ClassUtil.isSubclass(_portlet.getClass(),Constants.JSF_SUN)) {
    _facesPortlet=true;
  }
  _strutsPortlet=ClassUtil.isSubclass(portlet.getClass(),StrutsPortlet.class);
}
