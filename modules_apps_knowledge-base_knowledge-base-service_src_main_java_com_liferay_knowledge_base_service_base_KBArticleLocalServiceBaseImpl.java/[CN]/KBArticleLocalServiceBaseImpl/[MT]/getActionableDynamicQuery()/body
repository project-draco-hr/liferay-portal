{
  ActionableDynamicQuery actionableDynamicQuery=new DefaultActionableDynamicQuery();
  actionableDynamicQuery.setBaseLocalService(com.liferay.knowledge.base.service.KBArticleLocalServiceUtil.getService());
  actionableDynamicQuery.setClassLoader(getClassLoader());
  actionableDynamicQuery.setModelClass(KBArticle.class);
  actionableDynamicQuery.setPrimaryKeyPropertyName("kbArticleId");
  return actionableDynamicQuery;
}
