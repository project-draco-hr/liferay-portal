{
  Project project=npmInstallTask.getProject();
  String removeShrinkwrappedUrls=GradleUtil.getProperty(project,"nodejs.npm.remove.shrinkwrapped.urls",(String)null);
  if (Validator.isNotNull(removeShrinkwrappedUrls)) {
    npmInstallTask.setRemoveShrinkwrappedUrls(Boolean.parseBoolean(removeShrinkwrappedUrls));
  }
  String sassBinarySite=GradleUtil.getProperty(project,"nodejs.npm.sass.binary.site",(String)null);
  if (Validator.isNotNull(sassBinarySite)) {
    setTaskExecuteNodeArgDefault(npmInstallTask,_SASS_BINARY_SITE_ARG,sassBinarySite);
  }
}
