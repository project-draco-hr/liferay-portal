{
  if (actionId.equals(ActionKeys.ADD_LAYOUT_BRANCH) || actionId.equals(ActionKeys.DELETE)) {
    return GroupPermissionUtil.contains(permissionChecker,groupId,ActionKeys.ADD_LAYOUT_BRANCH);
  }
 else {
    LayoutBranch layoutBranch=LayoutBranchLocalServiceUtil.getLayoutBranch(layoutBranchId);
    return contains(permissionChecker,layoutBranch,actionId);
  }
}
