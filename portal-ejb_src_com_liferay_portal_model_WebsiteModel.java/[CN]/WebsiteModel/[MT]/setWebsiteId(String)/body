{
  if (((websiteId == null) && (_websiteId != null)) || ((websiteId != null) && (_websiteId == null)) || ((websiteId != null) && (_websiteId != null) && !websiteId.equals(_websiteId))) {
    if (!XSS_ALLOW_WEBSITEID) {
      websiteId=XSSUtil.strip(websiteId);
    }
    _websiteId=websiteId;
    setModified(true);
  }
}
