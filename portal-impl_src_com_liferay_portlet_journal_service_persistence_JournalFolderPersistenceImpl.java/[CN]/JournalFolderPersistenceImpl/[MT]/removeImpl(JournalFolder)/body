{
  journalFolder=toUnwrappedModel(journalFolder);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,journalFolder);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(journalFolder);
  return journalFolder;
}
