{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MDRRuleGroupInstance.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("ruleGroupInstanceId",RandomTestUtil.nextLong()));
  List<MDRRuleGroupInstance> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
