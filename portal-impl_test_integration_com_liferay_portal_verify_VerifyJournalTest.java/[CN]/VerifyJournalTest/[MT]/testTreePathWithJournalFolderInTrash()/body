{
  Group group=GroupTestUtil.addGroup();
  JournalFolder parentFolder=JournalTestUtil.addFolder(group.getGroupId(),"parentFolder");
  JournalFolder childFolder=JournalTestUtil.addFolder(group.getGroupId(),parentFolder.getFolderId(),"childFolder");
  JournalFolderLocalServiceUtil.moveFolderToTrash(TestPropsValues.getUserId(),childFolder.getFolderId());
  JournalFolderLocalServiceUtil.deleteFolder(parentFolder.getFolderId(),false);
  doVerify();
}
