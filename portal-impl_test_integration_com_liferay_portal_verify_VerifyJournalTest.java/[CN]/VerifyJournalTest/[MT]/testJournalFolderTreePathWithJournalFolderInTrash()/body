{
  Group group=GroupTestUtil.addGroup();
  JournalFolder parentFolder=JournalTestUtil.addFolder(group.getGroupId(),"parentFolder");
  JournalFolder folder=JournalTestUtil.addFolder(group.getGroupId(),parentFolder.getFolderId(),"folder");
  JournalFolderLocalServiceUtil.moveFolderToTrash(TestPropsValues.getUserId(),folder.getFolderId());
  JournalFolderLocalServiceUtil.deleteFolder(parentFolder.getFolderId(),false);
  doVerify();
}
