{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(codeId));
  }
  com.liferay.portal.model.ResourceCode resourceCode=getPersistence().remove(codeId);
  if (listener != null) {
    listener.onAfterRemove(resourceCode);
  }
  return resourceCode;
}
