{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(resourceCodePK));
  }
  com.liferay.portal.model.ResourceCode resourceCode=getPersistence().remove(resourceCodePK);
  if (listener != null) {
    listener.onAfterRemove(resourceCode);
  }
  return resourceCode;
}
