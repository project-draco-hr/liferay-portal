{
  LayoutSet layoutSet=layoutSetPersistence.findByG_P(groupId,privateLayout);
  List layouts=layoutPersistence.findByG_P_P(groupId,privateLayout,LayoutImpl.DEFAULT_PARENT_LAYOUT_ID);
  Iterator itr=layouts.iterator();
  while (itr.hasNext()) {
    Layout layout=(Layout)itr.next();
    try {
      layoutLocalService.deleteLayout(layout,false);
    }
 catch (    NoSuchLayoutException nsle) {
    }
  }
  ImageLocalUtil.deleteImage(layoutSet.getLogoId());
  layoutSetPersistence.removeByG_P(groupId,privateLayout);
}
