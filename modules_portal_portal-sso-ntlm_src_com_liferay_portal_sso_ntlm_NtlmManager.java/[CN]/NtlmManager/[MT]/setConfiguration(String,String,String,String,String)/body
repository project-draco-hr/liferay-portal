{
  _domain=domain;
  _domainController=domainController;
  _domainControllerName=domainControllerName;
  _ntlmServiceAccount=new NtlmServiceAccount(serviceAccount,servicePassword);
  _netlogon=new Netlogon();
  _netlogon.setConfiguration(domainController,domainControllerName,_ntlmServiceAccount);
}
