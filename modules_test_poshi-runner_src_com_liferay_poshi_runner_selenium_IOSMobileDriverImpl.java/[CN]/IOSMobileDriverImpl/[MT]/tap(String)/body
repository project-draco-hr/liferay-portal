{
  TouchAction touchAction=new TouchAction(this);
  int screenPositionX=WebDriverHelper.getElementPositionCenterX(this,locator);
  int navigationBarHeight=50;
  int screenPositionY=WebDriverHelper.getElementPositionCenterY(this,locator) + navigationBarHeight;
  context("NATIVE_APP");
  touchAction.tap(screenPositionX,screenPositionY);
  touchAction.perform();
  context("WEBVIEW_1");
}
