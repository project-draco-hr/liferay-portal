{
  CounterRegister register=_registerLookup.get(name);
  if (register != null) {
    return register;
  }
 else {
synchronized (_registerLookup) {
      register=_registerLookup.get(name);
      if (register == null) {
        register=createCounterRegister(name);
        _registerLookup.put(name,register);
      }
      return register;
    }
  }
}
