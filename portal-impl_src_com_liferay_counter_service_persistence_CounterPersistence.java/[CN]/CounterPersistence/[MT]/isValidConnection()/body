{
  try {
    PreparedStatement preparedStatement=_connection.prepareStatement(_COUNT_COUNTER);
    preparedStatement.execute();
  }
 catch (  SQLException sqle) {
    DataAccess.cleanUp(_connection);
    if (_log.isDebugEnabled()) {
      _log.debug(sqle,sqle);
    }
    return false;
  }
  return true;
}
