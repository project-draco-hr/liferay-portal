{
  Boolean hasPermission=StagingPermissionUtil.hasPermission(permissionChecker,image.getGroupId(),IGImage.class.getName(),image.getImageId(),PortletKeys.IMAGE_GALLERY,actionId);
  if (hasPermission != null) {
    return hasPermission.booleanValue();
  }
  if (PropsValues.PERMISSIONS_VIEW_DYNAMIC_INHERITANCE) {
    if (image.getFolderId() != IGFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
      IGFolder folder=image.getFolder();
      if (!IGFolderPermission.contains(permissionChecker,folder,ActionKeys.ACCESS) && !IGFolderPermission.contains(permissionChecker,folder,ActionKeys.VIEW)) {
        return false;
      }
    }
  }
  if (permissionChecker.hasOwnerPermission(image.getCompanyId(),IGImage.class.getName(),image.getImageId(),image.getUserId(),actionId)) {
    return true;
  }
  return permissionChecker.hasPermission(image.getGroupId(),IGImage.class.getName(),image.getImageId(),actionId);
}
