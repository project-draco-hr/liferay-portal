{
  DDMStructureEntryLink structureEntryLink=ddmStructureEntryLinkPersistence.findByPrimaryKey(structureEntryLinkId);
  structureEntryLink.setStructureId(structureId);
  structureEntryLink.setClassName(className);
  structureEntryLink.setClassPK(classPK);
  return ddmStructureEntryLinkPersistence.update(structureEntryLink,false);
}
