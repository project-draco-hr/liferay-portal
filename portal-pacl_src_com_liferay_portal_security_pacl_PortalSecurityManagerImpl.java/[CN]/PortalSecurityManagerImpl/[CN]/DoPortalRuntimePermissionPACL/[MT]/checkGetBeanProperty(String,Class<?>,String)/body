{
  SecurityManager securityManager=System.getSecurityManager();
  if (securityManager == null) {
    return;
  }
  Class<?> callerClass=Reflection.getCallerClass(5);
  if (clazz == callerClass) {
    return;
  }
  clazz=PACLUtil.getClass(clazz);
  Permission permission=new PortalRuntimePermission(PACLConstants.PORTAL_RUNTIME_PERMISSION_GET_BEAN_PROPERTY,servletContextName,clazz.getName(),property);
  securityManager.checkPermission(permission);
}
