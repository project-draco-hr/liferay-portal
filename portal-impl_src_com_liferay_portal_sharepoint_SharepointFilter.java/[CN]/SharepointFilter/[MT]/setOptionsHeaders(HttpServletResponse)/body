{
  response.setHeader("MS-Author-Via","MS-FP/4.0,DAV");
  response.setHeader("MicrosoftOfficeWebServer","5.0_Collab");
  response.setHeader("MicrosoftSharePointTeamServices",SharepointUtil.VERSION);
  response.setHeader("DAV","1,2");
