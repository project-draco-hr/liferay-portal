{
  Session session=null;
  try {
    session=openSession();
    BookmarksFolderHBM bookmarksFolderHBM=(BookmarksFolderHBM)session.get(BookmarksFolderHBM.class,folderId);
    if (bookmarksFolderHBM == null) {
      _log.warn("No BookmarksFolder exists with the primary key " + folderId.toString());
      throw new NoSuchFolderException("No BookmarksFolder exists with the primary key " + folderId.toString());
    }
    session.delete(bookmarksFolderHBM);
    session.flush();
    return BookmarksFolderHBMUtil.model(bookmarksFolderHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
