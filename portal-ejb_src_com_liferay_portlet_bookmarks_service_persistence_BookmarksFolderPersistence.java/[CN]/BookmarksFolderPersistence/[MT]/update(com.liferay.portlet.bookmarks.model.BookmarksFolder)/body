{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(bookmarksFolder);
    session.flush();
    bookmarksFolder.setNew(false);
    return bookmarksFolder;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
