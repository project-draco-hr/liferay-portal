{
  Session session=null;
  try {
    session=openSession();
    if (bookmarksFolder.isNew()) {
      session.save(bookmarksFolder);
    }
    session.flush();
    bookmarksFolder.setNew(false);
    return bookmarksFolder;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
