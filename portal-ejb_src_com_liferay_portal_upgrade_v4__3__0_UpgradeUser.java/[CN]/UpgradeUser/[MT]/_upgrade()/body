{
  PKUpgradeColumnImpl upgradeColumn=new PKUpgradeColumnImpl(0,new Integer(Types.VARCHAR),true);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(UserImpl.TABLE_NAME,UserImpl.TABLE_COLUMNS,upgradeColumn);
  upgradeTable.updateTable();
  ValueMapper userIdMapper=upgradeColumn.getValueMapper();
  UpgradeColumn upgradeUserIdColumn=new SwapUpgradeColumnImpl("userId",userIdMapper);
  upgradeTable=new DefaultUpgradeTableImpl(WikiPageImpl.TABLE_NAME,UserImpl.TABLE_COLUMNS,upgradeUserIdColumn);
  upgradeTable.updateTable();
  DBUtil.getInstance().executeSQL(_UPGRADE_SCHEMA);
}
