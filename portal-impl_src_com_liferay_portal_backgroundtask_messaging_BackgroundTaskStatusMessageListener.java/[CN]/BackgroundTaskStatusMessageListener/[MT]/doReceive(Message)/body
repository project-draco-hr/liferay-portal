{
  long backgroundTaskId=message.getLong("backgroundTaskId");
  if (backgroundTaskId != _backgroundTaskId) {
    return;
  }
  BackgroundTaskStatus backgroundTaskStatus=BackgroundTaskStatusRegistryUtil.getBackgroundTaskStatus(backgroundTaskId);
  if (backgroundTaskStatus == null) {
    if (_log.isDebugEnabled()) {
      _log.debug("Unable to locate backgroundTaskStatus for : " + backgroundTaskId + " to process: "+ message);
    }
    return;
  }
  _backgroundTaskStatusMessageTranslator.translate(backgroundTaskStatus,message);
}
