{
  if (parameters.length != 2) {
    throw new IllegalArgumentException("One parameter is expected");
  }
  String ddmFormFieldName=parameters[0].toString();
  List<DDMFormFieldEvaluationResult> ddmFormFieldEvaluationResults=getDDMFormFieldEvaluationResults(ddmFormFieldName);
  DDMFormFieldEvaluationResult ddmFormFieldEvaluationResult=ddmFormFieldEvaluationResults.get(0);
  return ddmFormFieldEvaluationResult.getProperty(_propertyName);
}
