{
  for (  DDMDataProvider ddmDataProvider : ddmDataProviders) {
    if (entityCache.getResult(DDMDataProviderModelImpl.ENTITY_CACHE_ENABLED,DDMDataProviderImpl.class,ddmDataProvider.getPrimaryKey()) == null) {
      cacheResult(ddmDataProvider);
    }
 else {
      ddmDataProvider.resetOriginalValues();
    }
  }
}
