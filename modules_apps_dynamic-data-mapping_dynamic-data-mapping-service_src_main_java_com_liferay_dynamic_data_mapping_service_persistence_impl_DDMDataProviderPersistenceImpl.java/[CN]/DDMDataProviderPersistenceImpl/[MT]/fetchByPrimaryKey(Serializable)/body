{
  DDMDataProvider ddmDataProvider=(DDMDataProvider)entityCache.getResult(DDMDataProviderModelImpl.ENTITY_CACHE_ENABLED,DDMDataProviderImpl.class,primaryKey);
  if (ddmDataProvider == _nullDDMDataProvider) {
    return null;
  }
  if (ddmDataProvider == null) {
    Session session=null;
    try {
      session=openSession();
      ddmDataProvider=(DDMDataProvider)session.get(DDMDataProviderImpl.class,primaryKey);
      if (ddmDataProvider != null) {
        cacheResult(ddmDataProvider);
      }
 else {
        entityCache.putResult(DDMDataProviderModelImpl.ENTITY_CACHE_ENABLED,DDMDataProviderImpl.class,primaryKey,_nullDDMDataProvider);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(DDMDataProviderModelImpl.ENTITY_CACHE_ENABLED,DDMDataProviderImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return ddmDataProvider;
}
