{
  entityCache.putResult(DDMDataProviderModelImpl.ENTITY_CACHE_ENABLED,DDMDataProviderImpl.class,ddmDataProvider.getPrimaryKey(),ddmDataProvider);
  finderCache.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddmDataProvider.getUuid(),ddmDataProvider.getGroupId()},ddmDataProvider);
  ddmDataProvider.resetOriginalValues();
}
