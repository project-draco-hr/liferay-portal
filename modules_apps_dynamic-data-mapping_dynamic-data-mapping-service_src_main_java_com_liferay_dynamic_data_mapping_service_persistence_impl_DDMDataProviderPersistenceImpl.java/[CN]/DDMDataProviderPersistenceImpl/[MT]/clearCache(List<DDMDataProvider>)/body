{
  finderCache.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  finderCache.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  DDMDataProvider ddmDataProvider : ddmDataProviders) {
    entityCache.removeResult(DDMDataProviderModelImpl.ENTITY_CACHE_ENABLED,DDMDataProviderImpl.class,ddmDataProvider.getPrimaryKey());
    clearUniqueFindersCache((DDMDataProviderModelImpl)ddmDataProvider);
  }
}
