{
  long groupId=fileEntry.getGroupId();
  long parentFolderId=fileEntry.getFolderId();
  String fileName=fileEntry.getName();
  if (Validator.isNull(lockUuid)) {
    return DLAppServiceUtil.hasFileEntryLock(groupId,parentFolderId,fileName);
  }
 else {
    try {
      boolean verified=DLAppServiceUtil.verifyFileEntryLock(groupId,parentFolderId,fileName,lockUuid);
      return !verified;
    }
 catch (    NoSuchLockException nsle) {
      return false;
    }
  }
}
