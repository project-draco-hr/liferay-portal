{
  try {
    DLFolder folder=DLAppServiceUtil.getFolder(groupId,parentFolderId,name);
    DLAppServiceUtil.deleteFolder(folder.getFolderId());
    return true;
  }
 catch (  NoSuchFolderException nsfe) {
    try {
      DLFileEntry fileEntry=DLAppServiceUtil.getFileEntryByTitle(groupId,parentFolderId,name);
      if (isLocked(fileEntry,lockUuid)) {
        throw new LockException();
      }
      DLAppServiceUtil.deleteFileEntryByTitle(groupId,parentFolderId,name);
      return true;
    }
 catch (    NoSuchFileEntryException nsfee) {
    }
  }
  return false;
}
