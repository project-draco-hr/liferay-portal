{
  Resource resource=getResource(webDavRequest);
  try {
    if (resource instanceof DLFileEntryResourceImpl) {
      DLFileEntry fileEntry=(DLFileEntry)resource.getModel();
      DLFileEntryServiceUtil.unlockFileEntry(fileEntry.getGroupId(),fileEntry.getFolderId(),fileEntry.getName(),token);
    }
 else {
      DLFolder folder=(DLFolder)resource.getModel();
      DLFolderServiceUtil.unlockFolder(folder.getGroupId(),folder.getParentFolderId(),folder.getName(),token);
    }
    return true;
  }
 catch (  Exception e) {
    if (e instanceof InvalidLockException) {
      if (_log.isWarnEnabled()) {
        _log.warn(e.getMessage());
      }
    }
 else {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to unlock file entry",e);
      }
    }
  }
  return false;
}
