{
  String href=getRootPath() + webDavReq.getPath();
  if (appendPath) {
    href+=StringPool.SLASH + folder.getName();
  }
  Resource resource=new BaseResourceImpl(href,folder.getName(),folder.getCreateDate(),folder.getModifiedDate());
  resource.setModel(folder);
  resource.setClassName(DLFolder.class.getName());
  resource.setPrimaryKey(folder.getPrimaryKey());
  return resource;
}
