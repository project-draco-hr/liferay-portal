{
  if (Validator.isNull(lockUuid)) {
    return fileEntry.hasLock();
  }
 else {
    try {
      boolean verified=DLAppServiceUtil.verifyFileEntryCheckOut(fileEntry.getRepositoryId(),fileEntry.getFileEntryId(),lockUuid);
      return !verified;
    }
 catch (    NoSuchLockException nsle) {
      return false;
    }
  }
}
