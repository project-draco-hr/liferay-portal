{
  try {
    com.liferay.portlet.announcements.model.Announcement returnValue=AnnouncementServiceUtil.addAnnouncement(userId,plid,classNameId,classPK,title,content,url,type,displayMonth,displayDay,displayYear,expirationMonth,expirationDay,expirationYear,priority,alert);
