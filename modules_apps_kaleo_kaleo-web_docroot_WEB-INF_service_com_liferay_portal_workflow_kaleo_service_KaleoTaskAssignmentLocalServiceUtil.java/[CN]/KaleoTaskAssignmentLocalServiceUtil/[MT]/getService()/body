{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),KaleoTaskAssignmentLocalService.class.getName());
    if (invokableLocalService instanceof KaleoTaskAssignmentLocalService) {
      _service=(KaleoTaskAssignmentLocalService)invokableLocalService;
    }
 else {
      _service=new KaleoTaskAssignmentLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(KaleoTaskAssignmentLocalServiceUtil.class,"_service");
  }
  return _service;
}
