{
  double informationEquity=0;
  try {
    SocialEquityAssetEntry socialEquityAssetEntry=socialEquityAssetEntryPersistence.findByAssetEntryId(assetEntryId);
    informationEquity=calculateEquity(getEquityDate(new Date()),socialEquityAssetEntry.getInformationK(),socialEquityAssetEntry.getInformationB());
  }
 catch (  NoSuchEquityAssetEntryException nseaee) {
  }
  return informationEquity;
}
