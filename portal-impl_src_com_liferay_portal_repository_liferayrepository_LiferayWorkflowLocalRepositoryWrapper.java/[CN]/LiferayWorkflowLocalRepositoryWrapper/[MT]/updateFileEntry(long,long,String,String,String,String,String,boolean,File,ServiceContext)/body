{
  DLFileVersionReference dlFileVersionReference=_getWorkflowDLFileVersion(fileEntryId,serviceContext);
  super.updateFileEntry(userId,fileEntryId,sourceFileName,mimeType,title,description,changeLog,majorVersion,file,serviceContext);
  _startWorkflowInstance(userId,dlFileVersionReference.fetchDLFileVersion(),serviceContext);
  return super.getFileEntry(fileEntryId);
}
