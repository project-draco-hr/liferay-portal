{
  ServerSocket serverSocket=createServerSocket(PropsValues.EHCACHE_SOCKET_START_PORT);
  List<String> cacheNames=remoteDeferredCacheNames;
  if (synchronizeCaches) {
    cacheNames=diffCacheNamesFromCluster(remoteCacheNames,remoteDeferredCacheNames);
  }
  EhcacheStreamServerThread ehcacheStreamServerThread=new EhcacheStreamServerThread(serverSocket,cacheNames);
  ehcacheStreamServerThread.start();
  return serverSocket.getLocalSocketAddress();
}
