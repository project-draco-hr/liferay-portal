{
  ServerSocketChannel serverSocketChannel=SocketUtil.createServerSocketChannel(ClusterLinkUtil.getBindInetAddress(),PropsValues.EHCACHE_SOCKET_START_PORT,_serverSocketConfigurator);
  ServerSocket serverSocket=serverSocketChannel.socket();
  PortalCacheManager<?,?> portalCacheManager=(PortalCacheManager<?,?>)PortalBeanLocatorUtil.locate(_BEAN_NAME_MULTI_VM_PORTAL_CACHE_MANAGER);
  EhcacheStreamServerThread ehcacheStreamServerThread=new EhcacheStreamServerThread(serverSocket,portalCacheManager,cacheNames);
  ehcacheStreamServerThread.start();
  return serverSocket.getLocalSocketAddress();
}
