{
  try (PreparedStatement ps=connection.prepareStatement("select * from ExpandoRow where tableId = ?")){
    ps.setLong(1,tableId);
    try (ResultSet rs=ps.executeQuery()){
      while (rs.next()) {
        long rowId=rs.getLong("rowId_");
        long companyId=rs.getLong("companyId");
        long classPK=rs.getLong("classPK");
        updateRow(companyId,classPK,tableName,tableId,columnName,rowId);
      }
    }
   }
 }
