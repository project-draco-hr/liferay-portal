{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select * from ExpandoRow where tableId = ?");
    ps.setLong(1,tableId);
    rs=ps.executeQuery();
    while (rs.next()) {
      long rowId=rs.getLong("rowId_");
      long companyId=rs.getLong("companyId");
      long classPK=rs.getLong("classPK");
      List<Long> ids=getJournalArticleIds(classPK);
      for (      Long id : ids) {
        if (!existsRow(companyId,tableId,id)) {
          long targetRowId=increment();
          if (_log.isDebugEnabled()) {
            _log.debug("Copying row " + rowId + " into "+ targetRowId);
          }
          runSQL("insert into ExpandoRow (rowId_, companyId, " + "tableId, classPK) values (" + targetRowId + ", "+ companyId+ ", "+ tableId+ ", "+ id+ ")");
          updateValues(tableId,rowId,targetRowId,classPK,id);
        }
      }
      if (!ids.isEmpty()) {
        runSQL("delete from ExpandoValue where rowId_ = " + rowId);
        runSQL("delete from ExpandoRow where rowId_ = " + rowId);
      }
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
