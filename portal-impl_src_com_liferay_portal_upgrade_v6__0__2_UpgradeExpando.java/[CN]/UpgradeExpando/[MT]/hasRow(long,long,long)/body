{
  try (PreparedStatement ps=connection.prepareStatement("select count(*) from ExpandoRow where companyId = ? and " + "tableId = ? and classPK = ?")){
    ps.setLong(1,companyId);
    ps.setLong(2,tableId);
    ps.setLong(3,classPK);
    try (ResultSet rs=ps.executeQuery()){
      while (rs.next()) {
        int count=rs.getInt(1);
        if (count > 0) {
          return true;
        }
      }
      return false;
    }
   }
 }
