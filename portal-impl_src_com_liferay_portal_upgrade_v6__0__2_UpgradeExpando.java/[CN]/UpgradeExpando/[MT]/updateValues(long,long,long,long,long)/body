{
  try (PreparedStatement ps=connection.prepareStatement("select * from ExpandoValue where tableId = ? and rowId_ = ? " + "and classPK = ?")){
    ps.setLong(1,tableId);
    ps.setLong(2,rowId);
    ps.setLong(3,classPK);
    try (ResultSet rs=ps.executeQuery()){
      while (rs.next()) {
        long companyId=rs.getLong("companyId");
        long columnId=rs.getLong("columnId");
        long classNameId=rs.getLong("classNameId");
        String data=rs.getString("data_");
        if (!hasValue(companyId,tableId,columnId,newRowId)) {
          long newValueId=increment();
          addValue(newValueId,companyId,tableId,columnId,newRowId,classNameId,newClassPK,data);
        }
      }
    }
   }
 }
