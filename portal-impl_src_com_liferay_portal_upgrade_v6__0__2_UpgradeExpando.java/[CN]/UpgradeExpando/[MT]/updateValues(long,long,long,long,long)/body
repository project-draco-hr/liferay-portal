{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select * from ExpandoValue where tableId = ? and rowId_ = ? " + "and classPK = ?");
    ps.setLong(1,tableId);
    ps.setLong(2,rowId);
    ps.setLong(3,classPK);
    rs=ps.executeQuery();
    while (rs.next()) {
      long valueId=rs.getLong("valueId");
      long companyId=rs.getLong("companyId");
      long columnId=rs.getLong("columnId");
      long classNameId=rs.getLong("classNameId");
      String data=rs.getString("data_");
      long targetValueId=increment();
      if (_log.isDebugEnabled()) {
        _log.debug("Copying value " + valueId + " into "+ targetValueId);
      }
      if (!existsValue(companyId,tableId,columnId,targetRowId)) {
        runSQL("insert into ExpandoValue (valueId, companyId, " + "tableId, columnId, rowId_, classNameId, classPK," + " data_) values (" + targetValueId + ", "+ companyId+ ", "+ tableId+ ", "+ columnId+ ", "+ targetRowId+ ", "+ classNameId+ ", "+ targetClassPK+ ", '"+ data+ "')");
      }
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
