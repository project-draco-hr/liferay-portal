{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select * from ExpandoRow where companyId = ? and tableId = ?" + " and classPK = ?");
    ps.setLong(1,companyId);
    ps.setLong(2,tableId);
    ps.setLong(3,classPK);
    rs=ps.executeQuery();
    return rs.next();
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
