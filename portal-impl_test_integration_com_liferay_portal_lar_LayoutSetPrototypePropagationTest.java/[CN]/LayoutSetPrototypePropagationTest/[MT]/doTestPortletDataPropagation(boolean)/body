{
  setLinkEnabled(linkEnabled);
  String content=_layoutSetPrototypeJournalArticle.getContent();
  for (  String languageId : journalArticle.getAvailableLanguageIds()) {
    String localization=LocalizationUtil.getLocalization(content,languageId);
    String importedLocalization=LocalizationUtil.getLocalization(journalArticle.getContent(),languageId);
    Assert.assertEquals(localization,importedLocalization);
  }
  String newContent=DDMStructureTestUtil.getSampleStructuredContent("New Test Content");
  JournalTestUtil.updateArticle(_layoutSetPrototypeJournalArticle,"New Test Title",newContent);
  propagateChanges(group);
  for (  String languageId : journalArticle.getAvailableLanguageIds()) {
    String localization=LocalizationUtil.getLocalization(content,languageId);
    String importedLocalization=LocalizationUtil.getLocalization(journalArticle.getContent(),languageId);
    Assert.assertEquals(localization,importedLocalization);
  }
}
