{
  request.setAttribute("liferay-staging:menu:cssClass",_cssClass);
  request.setAttribute("liferay-staging:menu:layoutSetBranchId",String.valueOf(_layoutSetBranchId));
  request.setAttribute("liferay-staging:menu:onlyActions",String.valueOf(_onlyActions));
  request.setAttribute("liferay-staging:menu:selPlid",String.valueOf(_selPlid));
  request.setAttribute("liferay-staging:menu:showManageBranches",String.valueOf(_showManageBranches));
}
