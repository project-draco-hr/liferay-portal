{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(OrgGroupPermission.class,OrgGroupPermission.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id.organizationId",nextLong()));
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id.groupId",nextLong()));
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id.permissionId",nextLong()));
  List<OrgGroupPermission> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
