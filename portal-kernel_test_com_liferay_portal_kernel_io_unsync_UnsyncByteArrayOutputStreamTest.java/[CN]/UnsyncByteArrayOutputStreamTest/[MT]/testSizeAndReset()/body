{
  UnsyncByteArrayOutputStream ubaos=new UnsyncByteArrayOutputStream();
  assertEquals(0,ubaos.size());
  ubaos.write(0);
  assertEquals(1,ubaos.size());
  ubaos.write(1);
  assertEquals(2,ubaos.size());
  ubaos.reset();
  assertEquals(0,ubaos.size());
  ubaos.write(0);
  assertEquals(1,ubaos.size());
  ubaos.write(1);
  assertEquals(2,ubaos.size());
}
