{
  UnsyncByteArrayOutputStream unsyncBaos=new UnsyncByteArrayOutputStream();
  int size=unsyncBaos.size();
  assertEquals(0,size);
  unsyncBaos.write(0);
  size=unsyncBaos.size();
  assertEquals(1,size);
  unsyncBaos.write(1);
  size=unsyncBaos.size();
  assertEquals(2,size);
  unsyncBaos.reset();
  size=unsyncBaos.size();
  assertEquals(0,size);
  unsyncBaos.write(0);
  size=unsyncBaos.size();
  assertEquals(1,size);
  unsyncBaos.write(1);
  size=unsyncBaos.size();
  assertEquals(2,size);
}
