{
  UnsyncByteArrayOutputStream unsyncBaos=new UnsyncByteArrayOutputStream();
  assertEquals(0,unsyncBaos.size());
  unsyncBaos.write(0);
  assertEquals(1,unsyncBaos.size());
  unsyncBaos.write(1);
  assertEquals(2,unsyncBaos.size());
  unsyncBaos.reset();
  assertEquals(0,unsyncBaos.size());
  unsyncBaos.write(0);
  assertEquals(1,unsyncBaos.size());
  unsyncBaos.write(1);
  assertEquals(2,unsyncBaos.size());
}
