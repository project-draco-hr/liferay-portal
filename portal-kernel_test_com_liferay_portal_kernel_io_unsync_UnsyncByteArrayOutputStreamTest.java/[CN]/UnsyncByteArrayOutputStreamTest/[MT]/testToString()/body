{
  UnsyncByteArrayOutputStream unsyncBaos=new UnsyncByteArrayOutputStream();
  unsyncBaos.write(_BUFFER);
  assertEquals(new String(_BUFFER),unsyncBaos.toString());
  String charsetName1="UTF-16BE";
  String charsetName2="UTF-16LE";
  assertFalse(new String(_BUFFER,charsetName1).equals(unsyncBaos.toString(charsetName2)));
  assertEquals(new String(_BUFFER,charsetName1),unsyncBaos.toString(charsetName1));
  assertEquals(new String(_BUFFER,charsetName2),unsyncBaos.toString(charsetName2));
}
