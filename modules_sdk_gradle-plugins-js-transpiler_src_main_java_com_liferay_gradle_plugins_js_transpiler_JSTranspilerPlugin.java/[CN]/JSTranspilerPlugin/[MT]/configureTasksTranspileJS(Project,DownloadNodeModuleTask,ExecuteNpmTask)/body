{
  TaskContainer taskContainer=project.getTasks();
  taskContainer.withType(TranspileJSTask.class,new Action<TranspileJSTask>(){
    @Override public void execute(    TranspileJSTask transpileJSTask){
      configureTaskTranspileJS(transpileJSTask,downloadMetalCliTask,npmInstallTask);
    }
  }
);
}
