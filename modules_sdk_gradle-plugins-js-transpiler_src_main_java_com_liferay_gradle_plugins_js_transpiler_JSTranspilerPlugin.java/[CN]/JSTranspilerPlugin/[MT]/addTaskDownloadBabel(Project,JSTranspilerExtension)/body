{
  DownloadNodeModuleTask downloadNodeModuleTask=GradleUtil.addTask(project,DOWNLOAD_BABEL_TASK_NAME,DownloadNodeModuleTask.class);
  downloadNodeModuleTask.setModuleName("babel");
  downloadNodeModuleTask.setModuleVersion(new Callable<String>(){
    @Override public String call() throws Exception {
      return jsTranspilerExtension.getBabelVersion();
    }
  }
);
  return downloadNodeModuleTask;
}
