{
  DownloadNodeModuleTask downloadNodeModuleTask=GradleUtil.addTask(project,DOWNLOAD_LFR_AMD_LOADER_TASK_NAME,DownloadNodeModuleTask.class);
  downloadNodeModuleTask.setModuleName("lfr-amd-loader");
  downloadNodeModuleTask.setModuleVersion(new Callable<String>(){
    @Override public String call() throws Exception {
      return jsTranspilerExtension.getLfrAmdLoaderVersion();
    }
  }
);
  return downloadNodeModuleTask;
}
