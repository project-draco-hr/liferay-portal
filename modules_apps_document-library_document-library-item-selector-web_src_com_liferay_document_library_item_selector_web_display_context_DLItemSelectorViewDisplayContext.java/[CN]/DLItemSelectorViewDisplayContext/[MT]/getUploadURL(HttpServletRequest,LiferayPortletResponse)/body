{
  PortletURL portletURL=liferayPortletResponse.createActionURL(PortletKeys.DOCUMENT_LIBRARY);
  portletURL.setParameter(ActionRequest.ACTION_NAME,"/document_library/upload_image");
  portletURL.setParameter("repositoryId",String.valueOf(getRepositoryId(request)));
  portletURL.setParameter("folderId",String.valueOf(getFolderId(request)));
  return portletURL;
}
