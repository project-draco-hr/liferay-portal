{
  long[] folderIds=bookmarksFolderService.getFolderIds(groupId,rootFolderId);
  if (folderIds.length == 0) {
    return 0;
  }
 else   if (userId <= 0) {
    return bookmarksEntryPersistence.filterCountByG_F_S(groupId,folderIds,WorkflowConstants.STATUS_APPROVED);
  }
 else {
    return bookmarksEntryPersistence.filterCountByG_U_F_S(groupId,userId,folderIds,WorkflowConstants.STATUS_APPROVED);
  }
}
