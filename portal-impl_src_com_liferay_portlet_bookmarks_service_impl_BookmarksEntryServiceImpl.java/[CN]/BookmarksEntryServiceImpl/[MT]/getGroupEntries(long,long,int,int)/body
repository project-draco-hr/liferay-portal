{
  OrderByComparator orderByComparator=new EntryModifiedDateComparator();
  if (userId <= 0) {
    return bookmarksEntryPersistence.filterFindByGroupId(groupId,start,end,orderByComparator);
  }
 else {
    return bookmarksEntryPersistence.filterFindByG_U(groupId,userId,start,end,orderByComparator);
  }
}
