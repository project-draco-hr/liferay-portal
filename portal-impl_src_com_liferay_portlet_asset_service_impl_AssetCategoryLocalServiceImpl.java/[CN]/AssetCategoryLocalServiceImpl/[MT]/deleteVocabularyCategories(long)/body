{
  List<AssetCategory> categories=assetCategoryPersistence.findByVocabularyId(vocabularyId);
  for (  AssetCategory category : categories) {
    if (category.getParentCategoryId() == AssetCategoryConstants.DEFAULT_PARENT_CATEGORY_ID) {
      assetCategoryLocalService.deleteCategory(category.getCategoryId());
    }
  }
}
