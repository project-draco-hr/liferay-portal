{
  Locale locale=LocaleUtil.getDefault();
  Map<Locale,String> categoryTitleMap=new HashMap<Locale,String>();
  categoryTitleMap.put(locale,title);
  Map<Locale,String> categoryDescriptionMap=new HashMap<Locale,String>();
  categoryDescriptionMap.put(locale,StringPool.BLANK);
  return addCategory(userId,AssetCategoryConstants.DEFAULT_PARENT_CATEGORY_ID,categoryTitleMap,categoryDescriptionMap,vocabularyId,null,serviceContext);
}
