{
  long blogsEntryId=RandomTestUtil.randomLong();
  Mockito.when(_blogsEntry.getEntryId()).thenReturn(blogsEntryId);
  String userName=RandomTestUtil.randomString();
  Mockito.when(_blogsEntry.getUserName()).thenReturn(userName);
  long userId=RandomTestUtil.randomLong();
  long groupId=RandomTestUtil.randomLong();
  _blogsEntryLocalServiceImpl.addDiscussion(_blogsEntry,userId,groupId);
  Mockito.verify(_commentManager).addDiscussion(userId,groupId,BlogsEntry.class.getName(),blogsEntryId,userName);
}
