{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Repository.class,Repository.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("repositoryId",ServiceTestUtil.nextLong()));
  List<Repository> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
