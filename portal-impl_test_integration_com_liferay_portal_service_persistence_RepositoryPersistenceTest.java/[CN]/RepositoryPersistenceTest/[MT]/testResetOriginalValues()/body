{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  Repository newRepository=addRepository();
  _persistence.clearCache();
  RepositoryModelImpl existingRepositoryModelImpl=(RepositoryModelImpl)_persistence.findByPrimaryKey(newRepository.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingRepositoryModelImpl.getUuid(),existingRepositoryModelImpl.getOriginalUuid()));
  Assert.assertEquals(existingRepositoryModelImpl.getGroupId(),existingRepositoryModelImpl.getOriginalGroupId());
}
