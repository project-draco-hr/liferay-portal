{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      selenium.open("/web/guest/home/");
    for (int second=0; ; second++) {
      if (second >= 60) {
        fail("timeout");
      }
      try {
        if (selenium.isElementPresent("link=Control Panel")) {
          break;
        }
      }
 catch (      Exception e) {
      }
      Thread.sleep(1000);
    }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace(""));
selenium.waitForPageToLoad("30000");
selenium.clickAt("link=Organizations",RuntimeVariables.replace(""));
selenium.waitForPageToLoad("30000");
boolean advancedPresent=selenium.isVisible("link=Advanced \u00bb");
if (!advancedPresent) {
label=2;
continue;
}
selenium.clickAt("link=Advanced \u00bb",RuntimeVariables.replace(""));
case 2:
selenium.type("_126_zip",RuntimeVariables.replace("41111"));
selenium.clickAt("//div[2]/span[2]/span/input",RuntimeVariables.replace("Search"));
selenium.waitForPageToLoad("30000");
selenium.type("_126_zip",RuntimeVariables.replace(""));
assertEquals(RuntimeVariables.replace("Selenium"),selenium.getText("//td[2]/a"));
boolean basicPresent=selenium.isVisible("link=\u00ab Basic");
if (!basicPresent) {
label=3;
continue;
}
selenium.clickAt("link=\u00ab Basic",RuntimeVariables.replace(""));
case 3:
case 100:
label=-1;
}
}
}
