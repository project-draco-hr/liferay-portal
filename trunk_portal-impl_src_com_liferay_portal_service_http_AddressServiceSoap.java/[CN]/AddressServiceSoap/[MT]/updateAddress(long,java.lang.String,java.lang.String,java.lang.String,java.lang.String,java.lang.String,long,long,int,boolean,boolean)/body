{
  try {
    com.liferay.portal.model.Address returnValue=AddressServiceUtil.updateAddress(addressId,street1,street2,street3,city,zip,regionId,countryId,typeId,mailing,primary);
    return com.liferay.portal.model.AddressSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
