{
  BlogsEntry entry=blogsEntryPersistence.findByPrimaryKey(entryId);
  long groupId=entry.getGroupId();
  Date displayDate=entry.getDisplayDate();
  BlogsEntry[] blogsEntries=blogsEntryPersistence.findByG_S_D_PrevAndNext(entryId,groupId,WorkflowConstants.STATUS_APPROVED,displayDate,new EntryIdComparator(true));
  return blogsEntries;
}
