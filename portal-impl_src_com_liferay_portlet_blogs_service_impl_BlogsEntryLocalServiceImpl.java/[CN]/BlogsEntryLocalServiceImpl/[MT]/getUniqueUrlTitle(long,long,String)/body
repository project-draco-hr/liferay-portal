{
  String urlTitle=getUrlTitle(entryId,title);
  for (int i=1; ; i++) {
    BlogsEntry entry=blogsEntryPersistence.fetchByG_UT(groupId,urlTitle);
    if ((entry == null) || (entry.getEntryId() == entryId)) {
      break;
    }
 else {
      urlTitle=urlTitle + StringPool.DASH + i;
    }
  }
  return urlTitle;
}
