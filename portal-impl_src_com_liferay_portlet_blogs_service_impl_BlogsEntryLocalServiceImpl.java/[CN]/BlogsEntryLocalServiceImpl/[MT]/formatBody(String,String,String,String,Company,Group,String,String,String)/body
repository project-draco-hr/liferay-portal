{
  return StringUtil.replace(body,new String[]{"[$BLOGS_ENTRY_USER_ADDRESS$]","[$BLOGS_ENTRY_USER_NAME$]","[$BLOGS_ENTRY_URL$]","[$COMPANY_ID$]","[$COMPANY_MX$]","[$COMPANY_NAME$]","[$COMMUNITY_NAME$]","[$FROM_ADDRESS$]","[$FROM_NAME$]","[$PORTAL_URL$]","[$PORTLET_NAME$]"},new String[]{emailAddress,fullName,entryURL,String.valueOf(company.getCompanyId()),company.getMx(),company.getName(),group.getName(),fromAddress,fromName,company.getVirtualHostname(),portletName});
}
