{
  if (queryDefinition.isExcludeStatus()) {
    return blogsEntryPersistence.findByC_LtD_NeS(companyId,displayDate,queryDefinition.getStatus(),queryDefinition.getStart(),queryDefinition.getEnd(),queryDefinition.getOrderByComparator());
  }
 else {
    return blogsEntryPersistence.findByC_LtD_S(companyId,displayDate,queryDefinition.getStatus(),queryDefinition.getStart(),queryDefinition.getEnd(),queryDefinition.getOrderByComparator());
  }
}
