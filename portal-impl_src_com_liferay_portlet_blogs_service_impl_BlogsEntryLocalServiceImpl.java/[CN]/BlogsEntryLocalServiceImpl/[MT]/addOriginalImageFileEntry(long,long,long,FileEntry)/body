{
  if (!fileEntry.isRepositoryCapabilityProvided(TemporaryFileEntriesCapability.class)) {
    return fileEntry.getFileEntryId();
  }
  BlogsEntryAttachmentFileEntryHelper blogsEntryAttachmentFileEntryHelper=new BlogsEntryAttachmentFileEntryHelper();
  Folder folder=addAttachmentsFolder(userId,groupId);
  FileEntry originalFileEntry=blogsEntryAttachmentFileEntryHelper.addBlogsEntryAttachmentFileEntry(groupId,userId,entryId,folder.getFolderId(),fileEntry.getTitle(),fileEntry.getMimeType(),fileEntry.getContentStream());
  return originalFileEntry.getFileEntryId();
}
