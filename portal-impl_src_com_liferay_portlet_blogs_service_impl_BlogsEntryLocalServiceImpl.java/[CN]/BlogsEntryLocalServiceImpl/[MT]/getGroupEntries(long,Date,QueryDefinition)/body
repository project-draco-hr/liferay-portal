{
  if (queryDefinition.isExcludeStatus()) {
    return blogsEntryPersistence.findByG_LtD_NeS(groupId,displayDate,queryDefinition.getStatus(),queryDefinition.getStart(),queryDefinition.getEnd(),queryDefinition.getOrderByComparator());
  }
 else {
    return blogsEntryPersistence.findByG_LtD_S(groupId,displayDate,queryDefinition.getStatus(),queryDefinition.getStart(),queryDefinition.getEnd(),queryDefinition.getOrderByComparator());
  }
}
