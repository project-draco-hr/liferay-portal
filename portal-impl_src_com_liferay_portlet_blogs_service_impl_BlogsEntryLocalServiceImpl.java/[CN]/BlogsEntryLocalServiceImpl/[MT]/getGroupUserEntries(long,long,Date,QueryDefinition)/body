{
  if (queryDefinition.isExcludeStatus()) {
    return blogsEntryPersistence.findByG_U_NeS(groupId,userId,queryDefinition.getStatus(),queryDefinition.getStart(),queryDefinition.getEnd(),queryDefinition.getOrderByComparator());
  }
 else {
    return blogsEntryPersistence.findByG_U_S(groupId,userId,queryDefinition.getStatus(),queryDefinition.getStart(),queryDefinition.getEnd(),queryDefinition.getOrderByComparator());
  }
}
