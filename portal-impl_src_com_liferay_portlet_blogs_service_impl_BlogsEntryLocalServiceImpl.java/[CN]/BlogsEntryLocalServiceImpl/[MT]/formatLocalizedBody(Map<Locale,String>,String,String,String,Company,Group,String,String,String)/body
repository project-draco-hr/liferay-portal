{
  Iterator it=localizedBody.entrySet().iterator();
  while (it.hasNext()) {
    Map.Entry localizedEntry=(Map.Entry)it.next();
    String body=formatBody((String)localizedEntry.getValue(),emailAddress,fullName,entryURL,company,group,fromAddress,fromName,portletName);
    localizedBody.put((Locale)localizedEntry.getKey(),body);
  }
  return localizedBody;
}
