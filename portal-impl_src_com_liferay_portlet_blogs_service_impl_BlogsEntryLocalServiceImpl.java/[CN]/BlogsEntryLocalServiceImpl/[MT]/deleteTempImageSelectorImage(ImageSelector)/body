{
  if ((imageSelector != null) && (imageSelector.getImageId() != 0)) {
    long imageSelectorImageId=imageSelector.getImageId();
    FileEntry imageSelectorImageFileEntry=PortletFileRepositoryUtil.getPortletFileEntry(imageSelectorImageId);
    if (imageSelectorImageFileEntry.isRepositoryCapabilityProvided(TemporaryFileEntriesCapability.class)) {
      PortletFileRepositoryUtil.deletePortletFileEntry(imageSelector.getImageId());
    }
  }
}
