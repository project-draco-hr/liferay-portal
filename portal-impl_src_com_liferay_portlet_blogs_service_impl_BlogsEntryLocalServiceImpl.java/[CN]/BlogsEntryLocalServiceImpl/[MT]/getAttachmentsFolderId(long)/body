{
  Repository repository=PortletFileRepositoryUtil.fetchPortletRepository(groupId,PortletKeys.BLOGS);
  if (repository == null) {
    return DLFolderConstants.DEFAULT_PARENT_FOLDER_ID;
  }
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  long folderId=DLFolderConstants.DEFAULT_PARENT_FOLDER_ID;
  try {
    Folder folder=PortletFileRepositoryUtil.getPortletFolder(repository.getRepositoryId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,PortletKeys.BLOGS);
    folderId=folder.getFolderId();
  }
 catch (  Exception e) {
  }
  return folderId;
}
