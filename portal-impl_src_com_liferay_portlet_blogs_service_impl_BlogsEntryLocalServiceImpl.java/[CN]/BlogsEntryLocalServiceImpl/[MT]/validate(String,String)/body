{
  if (Validator.isNull(title)) {
    throw new EntryTitleException("Blogs entry title must be a non null string");
  }
  int titleMaxLength=ModelHintsUtil.getMaxLength(BlogsEntry.class.getName(),"title");
  if (title.length() > titleMaxLength) {
    throw new EntryTitleException("Blogs entry title must be a non null string with length " + "smaller or equal than " + titleMaxLength);
  }
  if (Validator.isNull(content)) {
    throw new EntryContentException("Blogs entry content must be a non null string");
  }
  int contentMaxLength=ModelHintsUtil.getMaxLength(BlogsEntry.class.getName(),"content");
  if (content.length() > contentMaxLength) {
    throw new EntryContentException("Blogs entry content must be a non null string with length " + "smaller or equal than " + contentMaxLength);
  }
}
