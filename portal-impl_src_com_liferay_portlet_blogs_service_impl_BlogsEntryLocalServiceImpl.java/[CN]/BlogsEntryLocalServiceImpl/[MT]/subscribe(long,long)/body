{
  subscriptionLocalService.addSubscription(userId,BlogsEntry.class.getName(),groupId);
  try {
    assetEntryLocalService.getEntry(BlogsEntry.class.getName(),groupId);
  }
 catch (  Exception e) {
    assetEntryLocalService.updateEntry(userId,groupId,BlogsEntry.class.getName(),groupId,null,null,null,false,null,null,null,null,null,String.valueOf(groupId),null,null,null,0,0,null,false);
  }
  socialEquityLogLocalService.addEquityLogs(userId,BlogsEntry.class.getName(),groupId,ActionKeys.SUBSCRIBE);
}
