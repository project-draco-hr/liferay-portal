{
  Map<String,Serializable> workflowContext=new HashMap<String,Serializable>();
  workflowContext.put(WorkflowConstants.CONTEXT_URL,getEntryURL(entry,serviceContext));
  WorkflowHandlerRegistryUtil.startWorkflowInstance(entry.getCompanyId(),entry.getGroupId(),userId,BlogsEntry.class.getName(),entry.getEntryId(),entry,serviceContext,workflowContext);
}
