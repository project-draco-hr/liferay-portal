{
  byte[] imageBytes=imageSelector.getImageBytes();
  if (imageBytes != null) {
    BlogsEntryAttachmentFileEntryHelper blogsEntryAttachmentFileEntryHelper=new BlogsEntryAttachmentFileEntryHelper();
    Folder folder=addAttachmentsFolder(userId,groupId);
    FileEntry originalFileEntry=blogsEntryAttachmentFileEntryHelper.addBlogsEntryAttachmentFileEntry(groupId,userId,entryId,folder.getFolderId(),imageSelector.getImageTitle(),imageSelector.getImageMimeType(),imageSelector.getImageBytes());
    return originalFileEntry.getFileEntryId();
  }
  return 0;
}
