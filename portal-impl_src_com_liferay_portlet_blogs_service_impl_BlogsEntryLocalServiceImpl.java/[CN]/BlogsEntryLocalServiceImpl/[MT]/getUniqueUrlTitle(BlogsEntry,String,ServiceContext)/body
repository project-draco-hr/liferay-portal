{
  String serviceContextUrlTitle=GetterUtil.getString(serviceContext.getAttribute("urlTitle"));
  String urlTitle=null;
  if (isMatchesServiceContextUrlTitle(serviceContextUrlTitle)) {
    urlTitle=BlogsUtil.getUrlTitle(entry.getEntryId(),serviceContextUrlTitle);
    BlogsEntry urlTitleEntry=blogsEntryPersistence.fetchByG_UT(serviceContext.getScopeGroupId(),urlTitle);
    if ((urlTitleEntry != null) && (urlTitleEntry.getEntryId() != entry.getEntryId())) {
      urlTitle=getUniqueUrlTitle(entry.getEntryId(),serviceContext.getScopeGroupId(),urlTitle);
    }
  }
 else {
    if (!entry.isNew() && isMatchesServiceContextUrlTitle(entry.getUrlTitle())) {
      urlTitle=entry.getUrlTitle();
    }
 else {
      urlTitle=getUniqueUrlTitle(entry.getEntryId(),serviceContext.getScopeGroupId(),title);
    }
  }
  return urlTitle;
}
