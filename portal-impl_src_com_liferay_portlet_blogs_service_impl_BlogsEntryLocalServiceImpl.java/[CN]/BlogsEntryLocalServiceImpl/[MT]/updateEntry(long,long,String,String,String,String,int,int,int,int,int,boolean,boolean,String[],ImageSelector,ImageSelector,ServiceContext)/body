{
  User user=userPersistence.findByPrimaryKey(userId);
  Date displayDate=PortalUtil.getDate(displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute,user.getTimeZone(),EntryDisplayDateException.class);
  return updateEntry(userId,entryId,title,subtitle,description,content,displayDate,allowPingbacks,allowTrackbacks,trackbacks,coverImageImageSelector,smallImageImageSelector,true,serviceContext);
}
