{
  Map<String,Serializable> workflowContext=new HashMap<String,Serializable>();
  workflowContext.put(WorkflowConstants.CONTEXT_URL,getEntryURL(entry,serviceContext));
  WorkflowHandlerRegistryUtil.startWorkflowInstance(user.getCompanyId(),entry.getGroupId(),user.getUserId(),BlogsEntry.class.getName(),entry.getEntryId(),entry,serviceContext,workflowContext);
}
