{
  return new Iterator<E>(){
    Iterator<? extends E> itr=_list.iterator();
    @Override public boolean hasNext(){
      return itr.hasNext();
    }
    @Override public E next(){
      return itr.next();
    }
    @Override public void remove(){
      throw new UnsupportedOperationException(_MESSAGE);
    }
  }
;
}
