{
  EntityCacheUtil.putResult(RoleModelImpl.ENTITY_CACHE_ENABLED,RoleImpl.class,role.getPrimaryKey(),role);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_N,new Object[]{Long.valueOf(role.getCompanyId()),role.getName()},role);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_C_C,new Object[]{Long.valueOf(role.getCompanyId()),Long.valueOf(role.getClassNameId()),Long.valueOf(role.getClassPK())},role);
  role.resetOriginalValues();
}
