{
  String companyId=PortalUtil.getCompanyId(req);
  ServletContext portalCtx=null;
  try {
    portalCtx=ctx.getContext(PrefsPropsUtil.getString(companyId,PropsUtil.PORTAL_CTX));
  }
 catch (  Exception e) {
    _log.error(e);
  }
  if (portalCtx == null) {
    portalCtx=ctx;
  }
  return portalCtx;
}
