{
  if (((couponCodes == null) && (_couponCodes != null)) || ((couponCodes != null) && (_couponCodes == null)) || ((couponCodes != null) && (_couponCodes != null) && !couponCodes.equals(_couponCodes))) {
    if (!XSS_ALLOW_COUPONCODES) {
      couponCodes=XSSUtil.strip(couponCodes);
    }
    _couponCodes=couponCodes;
  }
}
