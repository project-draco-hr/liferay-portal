{
  if (((billingZip == null) && (_billingZip != null)) || ((billingZip != null) && (_billingZip == null)) || ((billingZip != null) && (_billingZip != null) && !billingZip.equals(_billingZip))) {
    if (!XSS_ALLOW_BILLINGZIP) {
      billingZip=XSSUtil.strip(billingZip);
    }
    _billingZip=billingZip;
  }
}
