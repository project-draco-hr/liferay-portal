{
  if (((shippingPhone == null) && (_shippingPhone != null)) || ((shippingPhone != null) && (_shippingPhone == null)) || ((shippingPhone != null) && (_shippingPhone != null) && !shippingPhone.equals(_shippingPhone))) {
    if (!XSS_ALLOW_SHIPPINGPHONE) {
      shippingPhone=XSSUtil.strip(shippingPhone);
    }
    _shippingPhone=shippingPhone;
  }
}
