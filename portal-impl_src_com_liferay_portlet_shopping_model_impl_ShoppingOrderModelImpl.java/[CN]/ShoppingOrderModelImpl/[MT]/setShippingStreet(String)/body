{
  if (((shippingStreet == null) && (_shippingStreet != null)) || ((shippingStreet != null) && (_shippingStreet == null)) || ((shippingStreet != null) && (_shippingStreet != null) && !shippingStreet.equals(_shippingStreet))) {
    if (!XSS_ALLOW_SHIPPINGSTREET) {
      shippingStreet=XSSUtil.strip(shippingStreet);
    }
    _shippingStreet=shippingStreet;
  }
}
