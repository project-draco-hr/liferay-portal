{
  if (((billingStreet == null) && (_billingStreet != null)) || ((billingStreet != null) && (_billingStreet == null)) || ((billingStreet != null) && (_billingStreet != null) && !billingStreet.equals(_billingStreet))) {
    if (!XSS_ALLOW_BILLINGSTREET) {
      billingStreet=XSSUtil.strip(billingStreet);
    }
    _billingStreet=billingStreet;
  }
}
