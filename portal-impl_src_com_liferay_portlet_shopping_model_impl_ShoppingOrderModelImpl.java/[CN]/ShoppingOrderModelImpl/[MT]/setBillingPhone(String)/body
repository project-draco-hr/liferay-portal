{
  if (((billingPhone == null) && (_billingPhone != null)) || ((billingPhone != null) && (_billingPhone == null)) || ((billingPhone != null) && (_billingPhone != null) && !billingPhone.equals(_billingPhone))) {
    if (!XSS_ALLOW_BILLINGPHONE) {
      billingPhone=XSSUtil.strip(billingPhone);
    }
    _billingPhone=billingPhone;
  }
}
