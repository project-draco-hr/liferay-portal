{
  StringBundler sb=new StringBundler(5);
  if (folderId >= 0) {
    sb.append(" AND ");
    sb.append(table);
    sb.append(".");
    if (table.equals("JournalFolder")) {
      sb.append("parentFolderId");
    }
 else {
      sb.append("folderId");
    }
    sb.append(" = ? ");
  }
  return sb.toString();
}
