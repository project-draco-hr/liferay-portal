{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  serviceContext.setAttribute("status",status);
  return DDMTemplateLocalServiceUtil.addTemplate(TestPropsValues.getUserId(),group.getGroupId(),classNameId,classPK,resourceClassNameId,templateKey,getDefaultLocaleMap(name),getDefaultLocaleMap(description),type,mode,language,script,false,false,null,null,serviceContext);
}
