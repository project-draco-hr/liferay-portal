{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Shopping Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//td[1]/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//tr[5]/td[2]/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("The Prodigal Project - J. H."));
  assertTrue(selenium.isTextPresent("For those on a journey, the end of which is faintly out of sight."));
  selenium.clickAt("//input[@value='Add to Shopping Cart']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Shopping Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Cart",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("The Prodigal Project - J. H."));
  assertTrue(selenium.isTextPresent("For those on a journey, the end of which is faintly out of sight."));
  assertTrue(selenium.isTextPresent("Price: $2.99"));
}
