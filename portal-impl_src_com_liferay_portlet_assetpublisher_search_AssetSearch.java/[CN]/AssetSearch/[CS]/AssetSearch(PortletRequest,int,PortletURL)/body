{
  super(portletRequest,new AssetDisplayTerms(portletRequest),new AssetSearchTerms(portletRequest),DEFAULT_CUR_PARAM,delta,iteratorURL,headerNames,EMPTY_RESULTS_MESSAGE);
  AssetDisplayTerms displayTerms=(AssetDisplayTerms)getDisplayTerms();
  iteratorURL.setParameter(AssetDisplayTerms.GROUP_ID,String.valueOf(displayTerms.getGroupId()));
  iteratorURL.setParameter(AssetDisplayTerms.TITLE,displayTerms.getTitle());
  iteratorURL.setParameter(AssetDisplayTerms.DESCRIPTION,displayTerms.getDescription());
  iteratorURL.setParameter(AssetDisplayTerms.USER_NAME,displayTerms.getUserName());
}
