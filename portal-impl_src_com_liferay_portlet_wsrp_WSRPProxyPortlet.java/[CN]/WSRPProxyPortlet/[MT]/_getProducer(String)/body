{
  final String MN="getProducer";
  if (_logger.isLogging(Logger.TRACE_HIGH)) {
    _logger.text(Logger.TRACE_HIGH,MN,"Trying to load producer with ID :" + producerID);
  }
  Producer producer=_consumerEnv.getProducerRegistry().getProducer(producerID);
  if (producer == null) {
    WSRPXHelper.throwX(_logger,Logger.ERROR,MN,ErrorCodes.PRODUCER_DOES_NOT_EXIST);
  }
  return producer;
}
