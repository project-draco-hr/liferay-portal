{
  Lock writeLock=_channelRWLock.writeLock();
  try {
    writeLock.lock();
    for (    String uuid : uuids) {
      _unconfirmedNotificationEvents.remove(uuid);
    }
    _userNotificationEventLocalService.deleteUserNotificationEvents(uuids);
  }
 catch (  Exception e) {
    throw new ChannelException("Failed to confirm delivery for user " + getUserId(),e);
  }
 finally {
    writeLock.unlock();
  }
}
