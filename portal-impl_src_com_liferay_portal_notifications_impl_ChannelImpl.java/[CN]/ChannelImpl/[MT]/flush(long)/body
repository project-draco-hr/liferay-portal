{
  Lock writeLock=_channelRWLock.writeLock();
  try {
    writeLock.lock();
    Iterator<NotificationEvent> eventItr=_notificationEvents.iterator();
    while (eventItr.hasNext()) {
      NotificationEvent event=eventItr.next();
      if (event.getTimestamp() < timestamp) {
        eventItr.remove();
      }
    }
  }
  finally {
    writeLock.unlock();
  }
}
