{
  Session session=null;
  try {
    session=openSession();
    String sql=CustomSQLUtil.get(FIND_BY_VIEW_COUNT);
    StringBuilder sb=new StringBuilder();
    for (int i=0; i < classNameId.length; i++) {
      sb.append("(Asset.classNameId = ?)");
      if ((i + 1) < classNameId.length) {
        sb.append(" OR ");
      }
    }
    sql=StringUtil.replace(sql,"(Asset.classNameId = ?)",sb.toString());
    sql+=" AND (visible = ?)";
    sb=new StringBuilder();
    sb.append(" ORDER BY Asset.viewCount");
    if (asc) {
      sb.append(" ASC");
    }
 else {
      sb.append(" DESC");
    }
    sql+=sb.toString();
    SQLQuery q=session.createSQLQuery(sql);
    q.addEntity("Asset",AssetImpl.class);
    QueryPos qPos=QueryPos.getInstance(q);
    for (int i=0; i < classNameId.length; i++) {
      qPos.add(classNameId[i]);
    }
    qPos.add(true);
    return (List<Asset>)QueryUtil.list(q,getDialect(),start,end);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
}
