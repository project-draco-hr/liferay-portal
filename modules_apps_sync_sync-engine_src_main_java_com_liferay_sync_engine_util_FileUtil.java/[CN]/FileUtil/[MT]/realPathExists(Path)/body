{
  try {
    Path realFilePath=filePath.toRealPath(LinkOption.NOFOLLOW_LINKS);
    String realFilePathString=realFilePath.toString();
    if (!realFilePathString.equals(filePath.toString())) {
      return false;
    }
    return exists(filePath);
  }
 catch (  Exception e) {
    return false;
  }
}
