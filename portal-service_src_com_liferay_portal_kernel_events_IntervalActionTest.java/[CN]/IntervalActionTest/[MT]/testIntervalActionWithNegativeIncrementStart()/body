{
  final IntervalAction intervalAction=new IntervalAction(125,200);
  intervalAction.setPerformIntervalActionMethod(new IntervalAction.PerformIntervalActionMethod(){
    @Override public void performIntervalAction(    int start,    int end) throws PortalException {
      intervalAction.incrementStart(start - end);
    }
  }
);
  intervalAction.performIntervalActions();
}
