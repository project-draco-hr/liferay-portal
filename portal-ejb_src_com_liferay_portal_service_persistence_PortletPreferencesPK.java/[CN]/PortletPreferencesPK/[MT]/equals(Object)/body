{
  if (obj == null) {
    return false;
  }
  PortletPreferencesPK pk=null;
  try {
    pk=(PortletPreferencesPK)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  if ((portletId.equals(pk.portletId)) && (layoutId.equals(pk.layoutId)) && (ownerId.equals(pk.ownerId))) {
    return true;
  }
 else {
    return false;
  }
}
