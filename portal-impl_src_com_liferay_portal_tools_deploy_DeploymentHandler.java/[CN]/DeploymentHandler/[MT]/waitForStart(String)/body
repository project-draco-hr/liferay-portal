{
  while (!_error && !_started) {
    wait();
  }
  if (_error) {
    return;
  }
  TargetModuleID[] targetModuleIDs=_deploymentManager.getAvailableModules(ModuleType.WAR,_deploymentManager.getTargets());
  for (  TargetModuleID targetModuleID : targetModuleIDs) {
    if (!targetModuleID.getModuleID().equals(warContext)) {
      continue;
    }
    _deploymentManager.start(new TargetModuleID[]{targetModuleID});
    break;
  }
}
