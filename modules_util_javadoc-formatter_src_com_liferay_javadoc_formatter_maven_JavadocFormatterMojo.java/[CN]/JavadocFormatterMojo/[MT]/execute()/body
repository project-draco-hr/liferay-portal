{
  try {
    @SuppressWarnings("rawtypes") Map pluginContext=getPluginContext();
    JavadocFormatter javadocFormatter=JavadocFormatterInvoker.invoke(baseDir,_javadocFormatterArgs);
    Set<String> modifiedFileNames=javadocFormatter.getModifiedFileNames();
    pluginContext.put(JavadocFormatterArgs.OUTPUT_KEY_MODIFIED_FILES,modifiedFileNames);
  }
 catch (  Exception e) {
    throw new MojoExecutionException(e.getMessage(),e);
  }
}
