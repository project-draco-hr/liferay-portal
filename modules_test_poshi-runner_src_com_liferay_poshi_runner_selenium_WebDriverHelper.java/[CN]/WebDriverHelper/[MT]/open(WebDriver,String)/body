{
  String targetURL="";
  if (url.startsWith("/")) {
    targetURL=PropsValues.PORTAL_URL + url;
  }
 else {
    targetURL=url;
  }
  for (int second=0; ; second++) {
    if (second >= PropsValues.TIMEOUT_IMPLICIT_WAIT) {
      break;
    }
    try {
      webDriver.get(targetURL);
      if (PropsValues.BROWSER_TYPE.equals("*iehta") || PropsValues.BROWSER_TYPE.equals("*iexplore")) {
        refresh(webDriver);
      }
      if (targetURL.equals(getLocation(webDriver))) {
        break;
      }
      Thread.sleep(1000);
    }
 catch (    Exception e) {
    }
  }
}
