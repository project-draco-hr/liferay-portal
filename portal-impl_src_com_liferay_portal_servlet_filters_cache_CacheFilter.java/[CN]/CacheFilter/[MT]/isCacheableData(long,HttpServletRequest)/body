{
  try {
    if (_pattern == _PATTERN_RESOURCE) {
      return true;
    }
    long plid=getPlid(companyId,request.getPathInfo(),request.getServletPath(),ParamUtil.getLong(request,"p_l_id"));
    if (plid <= 0) {
      return false;
    }
    Layout layout=LayoutLocalServiceUtil.getLayout(plid);
    if (!layout.getType().equals(LayoutConstants.TYPE_PORTLET)) {
      return false;
    }
    UnicodeProperties props=layout.getTypeSettingsProperties();
    for (int i=0; i < 10; i++) {
      String columnId="column-" + i;
      String settings=props.getProperty(columnId,StringPool.BLANK);
      if (!isCacheableColumn(settings,companyId)) {
        return false;
      }
    }
    if (props.containsKey(LayoutTypePortletConstants.NESTED_COLUMN_IDS)) {
      String columnIds[]=StringUtil.split(props.get(LayoutTypePortletConstants.NESTED_COLUMN_IDS));
      for (      String columnId : columnIds) {
        String settings=props.getProperty(columnId,StringPool.BLANK);
        if (!isCacheableColumn(settings,companyId)) {
          return false;
        }
      }
    }
    return true;
  }
 catch (  Exception e) {
    return false;
  }
}
