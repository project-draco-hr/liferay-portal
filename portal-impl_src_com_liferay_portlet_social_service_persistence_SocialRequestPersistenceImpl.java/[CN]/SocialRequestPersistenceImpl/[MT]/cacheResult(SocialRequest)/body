{
  EntityCacheUtil.putResult(SocialRequestModelImpl.ENTITY_CACHE_ENABLED,SocialRequestImpl.class,socialRequest.getPrimaryKey(),socialRequest);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{socialRequest.getUuid(),socialRequest.getGroupId()},socialRequest);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_U_C_C_T_R,new Object[]{socialRequest.getUserId(),socialRequest.getClassNameId(),socialRequest.getClassPK(),socialRequest.getType(),socialRequest.getReceiverUserId()},socialRequest);
  socialRequest.resetOriginalValues();
}
