{
  Session session=null;
  try {
    session=openSession();
    SocialRequest socialRequest=(SocialRequest)session.get(SocialRequestImpl.class,new Long(requestId));
    if (socialRequest == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + requestId);
      }
      throw new NoSuchRequestException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + requestId);
    }
    return socialRequestPersistence.remove(socialRequest);
  }
 catch (  NoSuchRequestException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
