{
  List<SocialRequest> list=findByU_C_C_T_S(userId,classNameId,classPK,type,status,0,1,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(12);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("userId=");
    msg.append(userId);
    msg.append(", classNameId=");
    msg.append(classNameId);
    msg.append(", classPK=");
    msg.append(classPK);
    msg.append(", type=");
    msg.append(type);
    msg.append(", status=");
    msg.append(status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchRequestException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
