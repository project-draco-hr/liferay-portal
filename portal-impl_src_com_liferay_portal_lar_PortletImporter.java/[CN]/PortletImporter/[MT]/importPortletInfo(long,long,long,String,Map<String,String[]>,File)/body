{
  PortletDataContext portletDataContext=null;
  try {
    ExportImportThreadLocal.setPortletImportInProcess(true);
    portletDataContext=getPortletDataContext(userId,plid,groupId,parameterMap,file);
    ExportImportLifecycleManager.fireExportImportLifecycleEvent(ExportImportLifecycleConstants.EVENT_PORTLET_IMPORT_STARTED,PortletDataContextFactoryUtil.clonePortletDataContext(portletDataContext));
    doImportPortletInfo(portletDataContext,userId,portletId);
    ExportImportLifecycleManager.fireExportImportLifecycleEvent(ExportImportLifecycleConstants.EVENT_PORTLET_IMPORT_FINISHED,PortletDataContextFactoryUtil.clonePortletDataContext(portletDataContext));
  }
 catch (  Throwable t) {
    ExportImportLifecycleManager.fireExportImportLifecycleEvent(ExportImportLifecycleConstants.EVENT_PORTLET_IMPORT_FAILED,PortletDataContextFactoryUtil.clonePortletDataContext(portletDataContext),t);
    throw t;
  }
 finally {
    ExportImportThreadLocal.setPortletImportInProcess(false);
    CacheUtil.clearCache();
    JournalContentUtil.clearCache();
    PermissionCacheUtil.clearCache();
  }
}
