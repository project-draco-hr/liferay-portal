{
  try {
    String xml=context.getZipEntryAsString(context.getImportRootPath() + "/categories.xml");
    Document doc=SAXReaderUtil.read(xml);
    Element root=doc.getRootElement();
    List<Element> assets=root.elements("asset");
    for (    Element asset : assets) {
      String className=GetterUtil.getString(asset.attributeValue("class-name"));
      long classPK=GetterUtil.getLong(asset.attributeValue("class-pk"));
      String entries=GetterUtil.getString(asset.attributeValue("entries"));
      context.addTagsCategories(className,new Long(classPK),StringUtil.split(entries,","));
    }
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
