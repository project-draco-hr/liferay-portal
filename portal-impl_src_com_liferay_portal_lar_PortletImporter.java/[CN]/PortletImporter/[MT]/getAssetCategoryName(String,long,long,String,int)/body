{
  AssetCategory assetCategory=null;
  try {
    assetCategory=AssetCategoryUtil.findByG_P_N_First(groupId,parentCategoryId,name,null);
  }
 catch (  NoSuchCategoryException nsce) {
    return name;
  }
  if (Validator.isNotNull(uuid) && uuid.equals(assetCategory.getUuid())) {
    return name;
  }
  name=StringUtil.appendParentheticalSuffix(name,count);
  return getAssetCategoryName(uuid,groupId,parentCategoryId,name,++count);
}
