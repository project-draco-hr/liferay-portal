{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(OrgGroupPermission.class,OrgGroupPermission.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("id.organizationId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("id.organizationId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
