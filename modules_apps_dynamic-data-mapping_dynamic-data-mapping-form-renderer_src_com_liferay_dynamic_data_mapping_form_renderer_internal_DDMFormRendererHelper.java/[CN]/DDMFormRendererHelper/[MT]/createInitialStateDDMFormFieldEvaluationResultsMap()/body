{
  try {
    DDMFormEvaluationResult ddmFormEvaluationResult=_ddmFormEvaluator.evaluate(_ddmForm,_ddmFormValues,_ddmFormRenderingContext.getLocale());
    return ddmFormEvaluationResult.getDDMFormFieldEvaluationResultsMap();
  }
 catch (  DDMFormEvaluationException ddmfee) {
    _log.error("Unable to evaluate the form",ddmfee);
  }
  return new HashMap<>();
}
