{
  ExpressionFactory expressionFactory=getService(ExpressionFactory.class);
  Expression<Boolean> visibilityExpression=expressionFactory.createBooleanExpression(ddmFormField.getVisibilityExpression());
  try {
    return visibilityExpression.evaluate();
  }
 catch (  ExpressionEvaluationException e) {
    return true;
  }
}
