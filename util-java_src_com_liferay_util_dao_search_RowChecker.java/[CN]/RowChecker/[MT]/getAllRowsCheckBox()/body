{
  if (Validator.isNull(_allRowsId)) {
    return StringPool.BLANK;
  }
 else {
    StringBuffer sb=new StringBuffer();
    sb.append("<input name=\"");
    sb.append(_allRowsId);
    sb.append("\" type=\"checkbox\" ");
    sb.append("onClick=\"Liferay.Util.checkAll(");
    sb.append(_formName);
    sb.append(", '");
    sb.append(_rowId);
    sb.append("', this");
    sb.append(");\">");
    return sb.toString();
  }
}
