{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Site Name");
  selenium.clickAt("link=Site Name",RuntimeVariables.replace("Site Name"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("//body[contains(@class,'live-view')]"));
  assertTrue(selenium.isElementNotPresent("//body[contains(@class,'live-staging')]"));
  assertEquals(RuntimeVariables.replace("Staging"),selenium.getText("//div[@class='staging-bar']/ul/li[2]/span/a"));
  selenium.clickAt("//div[@class='staging-bar']/ul/li[2]/span/a",RuntimeVariables.replace("Staging"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("//body[contains(@class,'local-staging')]"));
  assertTrue(selenium.isElementNotPresent("//body[contains(@class,'live-view')]"));
  assertEquals(RuntimeVariables.replace("Draft"),selenium.getText("//span[@class='workflow-status']/strong"));
  assertTrue(selenium.isVisible("//div[@id='column-2']/div/div[contains(@class,'portlet-search')]"));
  assertTrue(selenium.isVisible("//div[@id='column-2']/div/div[contains(@class,'polls-display')]"));
  assertTrue(selenium.isVisible("//div[@id='column-2']/div/div[contains(@class,'document-library')]"));
  assertTrue(selenium.isVisible("//div[@id='column-1']/div/div[contains(@class,'message-boards')]"));
  assertTrue(selenium.isVisible("//div[@id='column-1']/div/div[contains(@class,'user-statistics')]"));
  assertTrue(selenium.isElementNotPresent("//div[@id='column-1']/div/div[contains(@class,'portlet-search')]"));
  assertTrue(selenium.isElementNotPresent("//div[@id='column-1']/div/div[contains(@class,'polls-display')]"));
  assertTrue(selenium.isElementNotPresent("//div[@id='column-1']/div/div[contains(@class,'document-library')]"));
  assertTrue(selenium.isElementNotPresent("//div[@id='column-2']/div/div[contains(@class,'message-boards')]"));
  assertTrue(selenium.isElementNotPresent("//div[@id='column-2']/div/div[contains(@class,'user-statistics')]"));
}
