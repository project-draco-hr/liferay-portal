{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Settings")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Settings",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("additionalEmailAddressesLink",RuntimeVariables.replace(""));
  selenium.clickAt("//div[@id='additionalEmailAddresses']/fieldset/div[2]/div/span/a[1]",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("_130_emailAddressAddress2")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.typeKeys("_130_emailAddressAddress2",RuntimeVariables.replace("Admin@Lifera.com"));
  selenium.type("_130_emailAddressAddress2",RuntimeVariables.replace("Admin@Liferay.com"));
  selenium.select("_130_emailAddressTypeId2",RuntimeVariables.replace("label=E-mail 2"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
}
