{
  if (servletContextName == null) {
    return 0;
  }
  int count=0;
  Iterator<JSONWebServiceActionConfig> itr=_jsonWebServiceActionConfigs.iterator();
  while (itr.hasNext()) {
    JSONWebServiceActionConfig jsonWebServiceActionConfig=itr.next();
    if (servletContextName.equals(jsonWebServiceActionConfig.getServletContextName())) {
      itr.remove();
      count++;
    }
  }
  return count;
}
