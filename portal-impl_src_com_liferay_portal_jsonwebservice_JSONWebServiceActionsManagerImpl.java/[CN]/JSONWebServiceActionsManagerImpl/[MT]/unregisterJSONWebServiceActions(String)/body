{
  int count=0;
  Iterator<JSONWebServiceActionConfig> iterator=_jsonWebServiceActionConfigs.iterator();
  while (iterator.hasNext()) {
    JSONWebServiceActionConfig jsonWebServiceActionConfig=iterator.next();
    if (contextPath.equals(jsonWebServiceActionConfig.getContextPath())) {
      iterator.remove();
      count++;
    }
  }
  return count;
}
