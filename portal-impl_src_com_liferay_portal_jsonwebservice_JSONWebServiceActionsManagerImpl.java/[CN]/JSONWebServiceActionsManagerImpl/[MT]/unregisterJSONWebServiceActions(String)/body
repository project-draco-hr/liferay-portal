{
  int count=0;
  Iterator<JSONWebServiceActionConfig> itr=_jsonWebServiceActionConfigs.iterator();
  while (itr.hasNext()) {
    JSONWebServiceActionConfig jsonWebServiceActionConfig=itr.next();
    if (servletContextPath.equals(jsonWebServiceActionConfig.getServletContextPath())) {
      itr.remove();
      count++;
    }
  }
  return count;
}
