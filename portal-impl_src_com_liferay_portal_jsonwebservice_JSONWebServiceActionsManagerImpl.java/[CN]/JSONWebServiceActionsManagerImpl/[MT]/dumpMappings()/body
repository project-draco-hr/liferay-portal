{
  List<String[]> mappings=new ArrayList<String[]>();
  for (  JSONWebServiceActionConfig jsonWebServiceActionConfig : _jsonWebServiceActionConfig) {
    String[] parameterNames=jsonWebServiceActionConfig.getParameterNames();
    Class<?> actionClass=jsonWebServiceActionConfig.getActionClass();
    Method actionMethod=jsonWebServiceActionConfig.getActionMethod();
    String methodName=actionMethod.getName();
    methodName+="(";
    for (int i=0; i < parameterNames.length; i++) {
      if (i != 0) {
        methodName+=", ";
      }
      methodName+=parameterNames[i];
    }
    methodName+=")";
    String[] mapping=new String[]{jsonWebServiceActionConfig.getMethod(),jsonWebServiceActionConfig.getPath(),actionClass.getName() + '#' + methodName};
    mappings.add(mapping);
  }
  return mappings;
}
