{
  int firstIndex=_pathBinarySearch.findFirst(path);
  if (firstIndex < 0) {
    return -1;
  }
  int lastIndex=_pathBinarySearch.findLast(path,firstIndex);
  if (lastIndex < 0) {
    lastIndex=firstIndex;
  }
  int index=-1;
  int max=-1;
  for (int i=firstIndex; i <= lastIndex; i++) {
    JSONWebServiceActionConfig jsonWebServiceActionConfig=_jsonWebServiceActionConfig.get(i);
    String actionConfigMethod=jsonWebServiceActionConfig.getMethod();
    if (method != null) {
      if ((actionConfigMethod != null) && !actionConfigMethod.equals(method)) {
        continue;
      }
    }
    String[] actionConfigParameterNames=jsonWebServiceActionConfig.getParameterNames();
    int count=_countMatchedElements(parameterNames,actionConfigParameterNames);
    if (count > max) {
      max=count;
      index=i;
    }
  }
  return index;
}
