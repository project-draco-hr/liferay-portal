{
  JSONWebServiceActionParameters jsonWebServiceActionParameters=new JSONWebServiceActionParameters();
  jsonWebServiceActionParameters.collectAll(request,null,null,parameterMap);
  JSONWebServiceActionConfig jsonWebServiceActionConfig=_findJSONWebServiceAction(request,path,method,jsonWebServiceActionParameters);
  return new JSONWebServiceActionImpl(jsonWebServiceActionConfig,jsonWebServiceActionParameters,_jsonWebServiceNaming);
}
