{
  String from=(String)serviceReference.getProperty(UpgradeStepConstants.FROM);
  String to=(String)serviceReference.getProperty(UpgradeStepConstants.TO);
  UpgradeStep upgradeStepProcess=_bundleContext.getService(serviceReference);
  if (upgradeStepProcess == null) {
    _log.log(Logger.LOG_WARNING,"Service " + serviceReference + " is registered as "+ "an upgrade but it is not implementing Upgrade "+ "interface. Not tracking.");
    return null;
  }
  return new UpgradeInfo(from,to,upgradeStepProcess);
}
