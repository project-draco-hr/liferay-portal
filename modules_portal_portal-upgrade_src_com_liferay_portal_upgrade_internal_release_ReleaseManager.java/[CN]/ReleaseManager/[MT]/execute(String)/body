{
  ReleaseGraphManager releaseGraphManager=new ReleaseGraphManager(_serviceTrackerMap.getService(bundleSymbolicName));
  String schemaVersionString=getSchemaVersionString(bundleSymbolicName);
  List<List<UpgradeInfo>> upgradePaths=releaseGraphManager.getUpgradeInfos(schemaVersionString);
  int size=upgradePaths.size();
  if (size > 1) {
    throw new IllegalStateException("There are " + size + " possible end nodes to go from "+ schemaVersionString);
  }
  if (size == 0) {
    return;
  }
  executeUpgradeInfos(bundleSymbolicName,upgradePaths.get(0));
}
