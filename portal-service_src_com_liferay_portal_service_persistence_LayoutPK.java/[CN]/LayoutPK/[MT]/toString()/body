{
  StringBuffer sb=new StringBuffer();
  sb.append(StringPool.OPEN_CURLY_BRACE);
  sb.append("layoutId");
  sb.append(StringPool.EQUAL);
  sb.append(layoutId);
  sb.append(StringPool.COMMA);
  sb.append(StringPool.SPACE);
  sb.append("ownerId");
  sb.append(StringPool.EQUAL);
  sb.append(ownerId);
  sb.append(StringPool.CLOSE_CURLY_BRACE);
  return sb.toString();
}
