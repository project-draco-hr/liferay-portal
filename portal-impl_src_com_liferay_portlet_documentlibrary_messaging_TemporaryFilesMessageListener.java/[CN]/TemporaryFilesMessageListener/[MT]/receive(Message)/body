{
  List<Repository> repositories=RepositoryLocalServiceUtil.getRepositories(QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  for (  Repository repository : repositories) {
    try {
      deleteExpiredTemporaryFileEntries(repository);
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to try to cleanup temporary files in " + "repository " + repository.getRepositoryId(),e);
      }
    }
  }
}
