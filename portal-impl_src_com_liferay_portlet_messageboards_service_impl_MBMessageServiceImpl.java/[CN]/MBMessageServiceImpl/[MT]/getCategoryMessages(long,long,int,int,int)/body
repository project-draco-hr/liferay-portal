{
  List<MBMessage> messages=new ArrayList<MBMessage>();
  List<MBMessage> categoryMessages=mbMessageLocalService.getCategoryMessages(groupId,categoryId,status,start,end);
  for (  MBMessage message : categoryMessages) {
    if (MBMessagePermission.contains(getPermissionChecker(),message,ActionKeys.VIEW)) {
      messages.add(message);
    }
  }
  return messages;
}
