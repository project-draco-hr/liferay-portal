{
  List<MBMessage> messages=new ArrayList<MBMessage>();
  Iterator<MBMessage> itr=mbMessageLocalService.getCategoryMessages(categoryId,status,start,end).iterator();
  while (itr.hasNext()) {
    MBMessage message=itr.next();
    if (MBMessagePermission.contains(getPermissionChecker(),message,ActionKeys.VIEW)) {
      messages.add(message);
    }
  }
  return messages;
}
