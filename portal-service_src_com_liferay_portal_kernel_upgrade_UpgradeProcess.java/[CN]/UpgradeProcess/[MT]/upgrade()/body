{
  long start=System.currentTimeMillis();
  try {
    if (_log.isInfoEnabled()) {
      _log.info("Upgrading " + ClassUtil.getClassName(this));
    }
    doUpgrade();
  }
 catch (  Exception e) {
    throw new UpgradeException(e);
  }
 finally {
    if (_log.isInfoEnabled()) {
      _log.info("Completed upgrade process " + ClassUtil.getClassName(this) + " in "+ (System.currentTimeMillis() - start)+ "ms");
    }
  }
}
