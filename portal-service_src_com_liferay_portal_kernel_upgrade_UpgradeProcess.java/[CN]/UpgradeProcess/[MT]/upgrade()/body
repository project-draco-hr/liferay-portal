{
  long start=System.currentTimeMillis();
  try {
    if (_log.isInfoEnabled()) {
      _log.info("Upgrading " + getClass().getName());
    }
    doUpgrade();
  }
 catch (  Exception e) {
    throw new UpgradeException(e);
  }
 finally {
    if (_log.isInfoEnabled()) {
      _log.info("Completed upgrade process: " + getClass().getName() + " in "+ (System.currentTimeMillis() - start)+ "ms");
    }
  }
}
