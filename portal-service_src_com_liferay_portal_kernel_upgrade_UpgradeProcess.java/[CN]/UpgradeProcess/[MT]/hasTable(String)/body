{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    DatabaseMetaData metadata=con.getMetaData();
    rs=metadata.getTables(null,null,tableName,null);
    while (rs.next()) {
      return true;
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return false;
}
