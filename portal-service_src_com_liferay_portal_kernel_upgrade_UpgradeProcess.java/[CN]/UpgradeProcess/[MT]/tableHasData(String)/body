{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select count(*) from " + tableName);
    rs=ps.executeQuery();
    while (rs.next()) {
      int count=rs.getInt(1);
      if (count > 0) {
        return true;
      }
    }
  }
 catch (  Exception e) {
  }
 finally {
    DataAccess.cleanUp(null,ps,rs);
  }
  return false;
}
