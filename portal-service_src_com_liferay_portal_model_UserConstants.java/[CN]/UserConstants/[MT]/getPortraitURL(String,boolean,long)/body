{
  StringBundler sb=new StringBundler(7);
  sb.append(imagePath);
  sb.append("/user_");
  if (male) {
    sb.append("male");
  }
 else {
    sb.append("female");
  }
  sb.append("_portrait?img_id=");
  sb.append(portraitId);
  sb.append("&t=");
  sb.append(WebServerServletTokenUtil.getToken(portraitId));
  return sb.toString();
}
