{
  String[] parameterNames=_jsonWebServiceActionConfig.getParameterNames();
  Class<?>[] parameterTypes=_jsonWebServiceActionConfig.getParameterTypes();
  for (int i=0; i < parameterNames.length; i++) {
    if (parameters[i] != null) {
      continue;
    }
    String parameterName=parameterNames[i];
    Class<?> parameterType=parameterTypes[i];
    if (parameterName.equals("serviceContext") && parameterType.equals(ServiceContext.class)) {
      parameters[i]=new ServiceContext();
    }
  }
}
