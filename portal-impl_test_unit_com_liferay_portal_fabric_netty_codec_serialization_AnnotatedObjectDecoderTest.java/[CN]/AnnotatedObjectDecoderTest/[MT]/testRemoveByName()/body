{
  _annotatedObjectDecoder.addFirst(DateChannelHandler.class.getName(),_dateChannelHandler);
  _annotatedObjectDecoder.addFirst(UUIDChannelHandler.class.getName(),_uuidChannelHandler);
  _assertChannelPipeline(_uuidChannelHandler,_dateChannelHandler);
  Assert.assertSame(_dateChannelHandler,_annotatedObjectDecoder.remove(DateChannelHandler.class.getName()));
  _assertChannelPipeline(_uuidChannelHandler);
  Assert.assertSame(_uuidChannelHandler,_annotatedObjectDecoder.remove(UUIDChannelHandler.class.getName()));
  _assertChannelPipeline();
}
