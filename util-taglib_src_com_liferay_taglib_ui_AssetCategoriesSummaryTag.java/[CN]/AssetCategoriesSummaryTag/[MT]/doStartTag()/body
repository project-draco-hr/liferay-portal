{
  HttpServletRequest request=(HttpServletRequest)pageContext.getRequest();
  request.setAttribute("liferay-ui:asset_categories_summary:className",_className);
  request.setAttribute("liferay-ui:asset_categories_summary:classPK",String.valueOf(_classPK));
  request.setAttribute("liferay-ui:asset_categories_summary:message",_message);
  request.setAttribute("liferay-ui:asset_categories_summary:portletURL",_portletURL);
  return EVAL_BODY_BUFFERED;
}
