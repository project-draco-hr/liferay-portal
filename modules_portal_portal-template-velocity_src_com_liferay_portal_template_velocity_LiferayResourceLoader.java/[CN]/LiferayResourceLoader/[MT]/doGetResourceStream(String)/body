{
  if (_log.isDebugEnabled()) {
    _log.debug("Get resource for " + source);
  }
  try {
    TemplateResource templateResource=TemplateResourceLoaderUtil.getTemplateResource(TemplateConstants.LANG_TYPE_VM,source);
    return new ReaderInputStream(templateResource.getReader());
  }
 catch (  Exception e) {
    return null;
  }
}
