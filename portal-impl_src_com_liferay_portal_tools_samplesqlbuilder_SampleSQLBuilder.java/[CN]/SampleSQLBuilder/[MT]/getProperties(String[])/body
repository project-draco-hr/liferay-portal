{
  Reader reader=null;
  try {
    Properties properties=new SortedProperties();
    reader=new FileReader(args[0]);
    properties.load(reader);
    return properties;
  }
  finally {
    if (reader != null) {
      try {
        reader.close();
      }
 catch (      IOException ioe) {
        ioe.printStackTrace();
      }
    }
  }
}
