{
  InitUtil.initWithSpring();
  String outputDir=System.getProperty("sample.sql.output.dir");
  int maxMBCategoryCount=GetterUtil.getInteger(System.getProperty("sample.sql.max.mb.category.count"));
  int maxMBMessageCount=GetterUtil.getInteger(System.getProperty("sample.sql.max.mb.message.count"));
  int maxMBThreadCount=GetterUtil.getInteger(System.getProperty("sample.sql.max.mb.thread.count"));
  int maxUserCount=GetterUtil.getInteger(System.getProperty("sample.sql.max.user.count"));
  new SampleSQLBuilder(outputDir,maxMBCategoryCount,maxMBMessageCount,maxMBThreadCount,maxUserCount);
}
