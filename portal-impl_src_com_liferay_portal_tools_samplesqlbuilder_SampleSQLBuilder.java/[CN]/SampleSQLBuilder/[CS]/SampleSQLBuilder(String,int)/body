{
  try {
    _outputDir=outputDir;
    _maxUserCount=maxUserCount;
    _mysqlWriter=new FileWriter(new File(_outputDir + "/sample-mysql.sql"));
    _mysqlDBUtil=DBUtil.getInstance(DBUtil.TYPE_MYSQL);
    createClassNames();
    createResourceCodes();
    createCompany();
    createRoles();
    createGroups();
    createOrganizations();
    createUsers();
    createCounters();
    _mysqlWriter.write(_mysqlDBUtil.buildSQL("COMMIT_TRANSACTION"));
    _mysqlWriter.flush();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
