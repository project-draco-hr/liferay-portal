{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/organization-name/");
  assertEquals(RuntimeVariables.replace("Organization Name"),selenium.getText("//div[@id='heading']/h1/span"));
  assertEquals(RuntimeVariables.replace("Public Page"),selenium.getText("//nav[contains(.,'Public Page')]/ul/li/a"));
  assertEquals(RuntimeVariables.replace("Organization Name"),selenium.getText("//nav[contains(.,'Organization Name')]/ul/li/span/a"));
  assertEquals(RuntimeVariables.replace("Public Page"),selenium.getText("//nav[contains(.,'Public Page')]/ul/li[2]/span/a"));
}
