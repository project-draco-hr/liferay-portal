{
  if (parameters.length != 2) {
    throw new IllegalArgumentException(String.format("Expected 2 parameters, received %d",parameters.length));
  }
  if (Validator.isNull(parameters[0]) || Validator.isNull(parameters[1])) {
    return false;
  }
  String value1=parameters[0].toString();
  String value2=parameters[1].toString();
  return value1.contains(value2);
}
