{
  List<LayoutFriendlyURL> dependentLayoutFriendlyURLs=_dependentLayoutFriendlyURLsMap.get(clazz.getSimpleName());
  if (dependentLayoutFriendlyURLs == null) {
    dependentLayoutFriendlyURLs=new ArrayList<LayoutFriendlyURL>();
    _dependentLayoutFriendlyURLsMap.put(clazz.getSimpleName(),dependentLayoutFriendlyURLs);
  }
  List<LayoutFriendlyURL> layoutFriendlyURLs=LayoutFriendlyURLLocalServiceUtil.getLayoutFriendlyURLs(plid);
  Assert.assertEquals(1,layoutFriendlyURLs.size());
  dependentLayoutFriendlyURLs.add(layoutFriendlyURLs.get(0));
}
