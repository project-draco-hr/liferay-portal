{
  String currentExecutorPath=matchingExecutorPath;
  if (!currentExecutorPath.equals("/")) {
    currentExecutorPath=currentExecutorPath + "/";
  }
  String nextExecutorPath=path.replaceFirst(currentExecutorPath,StringPool.BLANK);
  if (!nextExecutorPath.isEmpty() && !nextExecutorPath.contains(StringPool.SLASH)) {
    return true;
  }
  return false;
}
