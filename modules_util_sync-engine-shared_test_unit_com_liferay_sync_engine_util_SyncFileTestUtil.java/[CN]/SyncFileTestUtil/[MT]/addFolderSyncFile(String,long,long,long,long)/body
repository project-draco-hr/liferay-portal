{
  SyncFile syncFile=SyncFileService.addSyncFile(null,null,null,filePathName,null,null,parentFolderId,repositoryId,syncAccountId,SyncFile.TYPE_FOLDER);
  if (typePK == 0) {
    typePK=syncFile.getSyncFileId();
  }
  syncFile.setTypePK(typePK);
  SyncFileService.update(syncFile);
  return syncFile;
}
