{
  WikiPage newWikiPage=addWikiPage();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(WikiPage.class,WikiPage.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("pageId",newWikiPage.getPageId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  WikiPage existingWikiPage=(WikiPage)result.get(0);
  assertEquals(existingWikiPage,newWikiPage);
}
