{
  ServletContext value=null;
  try {
    Object returnObj=PortalClassInvoker.invoke(_CLASS,_METHOD_GET_SERVLET_CONTEXT,new Object[]{portlet,servletContext},false);
    if (returnObj != null) {
      value=(ServletContext)returnObj;
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return value;
}
