{
  if (Validator.isNotNull(_cssClass) && Validator.isNotNull(_colorSchemeImagesPath)) {
    int pos=_cssClass.indexOf(CharPool.SPACE);
    if (pos > 0) {
      if (_colorSchemeImagesPath.endsWith(_cssClass.substring(0,pos))) {
        String subclassPath=StringUtil.replace(_cssClass,CharPool.SPACE,CharPool.SLASH);
        return _colorSchemeImagesPath + subclassPath.substring(pos);
      }
    }
  }
  return _colorSchemeImagesPath;
}
