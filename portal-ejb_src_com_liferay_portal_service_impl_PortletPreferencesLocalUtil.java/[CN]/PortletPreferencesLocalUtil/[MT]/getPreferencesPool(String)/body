{
  key=_encodeKey(key);
  Map prefsPool=null;
  try {
    prefsPool=(Map)_cache.getFromCache(key);
  }
 catch (  NeedsRefreshException nfe) {
    prefsPool=CollectionFactory.getSyncHashMap();
    _cache.putInCache(key,prefsPool,GROUP_NAME_ARRAY);
  }
 finally {
    if (prefsPool == null) {
      _cache.cancelUpdate(key);
    }
  }
  return prefsPool;
}
