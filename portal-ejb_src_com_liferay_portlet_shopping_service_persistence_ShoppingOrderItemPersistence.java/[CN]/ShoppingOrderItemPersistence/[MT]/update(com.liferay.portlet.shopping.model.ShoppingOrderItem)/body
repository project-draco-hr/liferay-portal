{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(shoppingOrderItem);
    session.flush();
    shoppingOrderItem.setNew(false);
    return shoppingOrderItem;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
