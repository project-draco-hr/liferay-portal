{
  Map<String,Serializable> additionalContextInformation=(Map<String,Serializable>)serviceContext.removeAttribute(WorkflowConstants.CONTEXT_ADDITIONAL_INFO);
  if (additionalContextInformation == null) {
    additionalContextInformation=Collections.EMPTY_MAP;
  }
  startWorkflowInstance(companyId,groupId,userId,className,classPK,model,serviceContext,additionalContextInformation);
}
