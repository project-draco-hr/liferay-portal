{
  com.liferay.portlet.calendar.model.CalEvent returnValue=CalEventServiceUtil.updateEvent(eventId,title,description,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,durationHour,durationMinute,allDay,timeZoneSensitive,type,repeating,recurrence,remindBy,firstReminder,secondReminder);
