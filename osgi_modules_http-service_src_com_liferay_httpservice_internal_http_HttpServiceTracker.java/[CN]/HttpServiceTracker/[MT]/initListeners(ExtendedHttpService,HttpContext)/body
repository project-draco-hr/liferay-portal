{
  List<ListenerDefinition> listeners=_webXML.getListeners();
  for (  ListenerDefinition listenerDefinition : listeners) {
    try {
      httpService.registerListener(listenerDefinition.getListener(),listenerDefinition.getContextParams(),httpContext);
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
}
