{
  Map<String,ServletDefinition> servletDefinitions=_webXML.getServletDefinitions();
  for (  Map.Entry<String,ServletDefinition> entry : servletDefinitions.entrySet()) {
    ServletDefinition servletDefinition=entry.getValue();
    List<String> urlPatterns=servletDefinition.getURLPatterns();
    for (    String urlPattern : urlPatterns) {
      try {
        httpService.registerServlet(urlPattern,servletDefinition.getServlet(),servletDefinition.getInitParameters(),httpContext);
      }
 catch (      Exception e) {
        _log.error(e,e);
      }
    }
  }
}
