{
  if (obj == null) {
    return false;
  }
  JournalArticlePK pk=null;
  try {
    pk=(JournalArticlePK)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  if ((companyId.equals(pk.companyId)) && (groupId.equals(pk.groupId)) && (articleId.equals(pk.articleId))&& (version == pk.version)) {
    return true;
  }
 else {
    return false;
  }
}
