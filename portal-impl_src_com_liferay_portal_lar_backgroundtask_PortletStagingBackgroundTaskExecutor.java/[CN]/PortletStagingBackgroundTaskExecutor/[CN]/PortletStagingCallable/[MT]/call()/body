{
  ExportImportLocalServiceUtil.importPortletDataDeletions(_exportImportConfiguration,_file);
  MissingReferences missingReferences=ExportImportLocalServiceUtil.validateImportPortletInfo(_exportImportConfiguration,_file);
  markBackgroundTask(_backgroundTaskId,"validated");
  ExportImportLocalServiceUtil.importPortletInfo(_exportImportConfiguration,_file);
  return missingReferences;
}
