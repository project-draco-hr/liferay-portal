{
  if (_apple != null) {
    return _apple.booleanValue();
  }
  String osName=System.getProperty("os.name");
  osName=StringUtil.toLowerCase(osName);
  if (osName.contains("darwin") || osName.contains("mac")) {
    _apple=Boolean.TRUE;
  }
 else {
    _apple=Boolean.FALSE;
  }
  return _apple.booleanValue();
}
