{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SocialRelation.class,SocialRelation.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("relationId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("relationId",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
