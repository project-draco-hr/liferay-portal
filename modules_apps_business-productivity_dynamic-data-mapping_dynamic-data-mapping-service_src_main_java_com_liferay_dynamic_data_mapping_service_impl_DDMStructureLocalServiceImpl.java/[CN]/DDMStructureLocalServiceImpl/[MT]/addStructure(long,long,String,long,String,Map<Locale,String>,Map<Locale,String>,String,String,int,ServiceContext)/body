{
  DDMXMLUtil.validateXML(definition);
  DDMForm ddmForm=ddmFormXSDDeserializer.deserialize(definition);
  DDMFormLayout ddmFormLayout=DDMUtil.getDefaultDDMFormLayout(ddmForm);
  return addStructure(userId,groupId,parentStructureKey,classNameId,structureKey,nameMap,descriptionMap,ddmForm,ddmFormLayout,storageType,type,serviceContext);
}
