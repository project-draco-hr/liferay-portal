{
  Bundle bundle=mock(Bundle.class);
  mockBundle(bundle,bsn,version,url,capability);
  MockServiceReference mockServiceReference=new MockServiceReference(bundle,new String[]{ServletContext.class.getName()},_counter.incrementAndGet(),ranking);
  mockServiceReference.setProperties(Collections.<String,Object>singletonMap("osgi.web.contextpath","/" + bsn + "-"+ version.toString()));
  return mockServiceReference;
}
