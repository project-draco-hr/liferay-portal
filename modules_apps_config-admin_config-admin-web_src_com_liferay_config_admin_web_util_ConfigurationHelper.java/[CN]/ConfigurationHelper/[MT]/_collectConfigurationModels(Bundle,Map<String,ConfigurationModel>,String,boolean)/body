{
  MetaTypeInformation metaTypeInformation=_metaTypeService.getMetaTypeInformation(bundle);
  if (metaTypeInformation == null) {
    return;
  }
  String[] pids=null;
  if (factory) {
    pids=metaTypeInformation.getFactoryPids();
  }
 else {
    pids=metaTypeInformation.getPids();
  }
  for (  String pid : pids) {
    ConfigurationModel configurationModel=_getConfigurationModel(bundle,pid,factory,locale);
    if (configurationModel == null) {
      continue;
    }
    modelMap.put(pid,configurationModel);
  }
}
