{
  if (obj == null) {
    return false;
  }
  if (this == obj) {
    return true;
  }
  if (!(obj instanceof DayAndPosition)) {
    return false;
  }
  DayAndPosition that=(DayAndPosition)obj;
  if ((getDayOfWeek() == that.getDayOfWeek()) && (getDayPosition() == that.getDayPosition())) {
    return true;
  }
  return false;
}
