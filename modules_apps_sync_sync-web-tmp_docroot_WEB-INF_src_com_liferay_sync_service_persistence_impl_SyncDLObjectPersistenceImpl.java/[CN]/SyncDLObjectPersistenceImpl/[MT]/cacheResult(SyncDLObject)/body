{
  entityCache.putResult(SyncDLObjectModelImpl.ENTITY_CACHE_ENABLED,SyncDLObjectImpl.class,syncDLObject.getPrimaryKey(),syncDLObject);
  finderCache.putResult(FINDER_PATH_FETCH_BY_T_T,new Object[]{syncDLObject.getType(),syncDLObject.getTypePK()},syncDLObject);
  syncDLObject.resetOriginalValues();
}
