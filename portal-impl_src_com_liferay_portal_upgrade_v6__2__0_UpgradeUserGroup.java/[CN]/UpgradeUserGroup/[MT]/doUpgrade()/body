{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select userGroupId from usergroup where uuid_ is null");
    rs=ps.executeQuery();
    while (rs.next()) {
      long userGroupId=rs.getLong("userGroupId");
      runSQL("update UserGroup set uuid_ = '" + PortalUUIDUtil.generate() + "' where userGroupId = "+ userGroupId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
