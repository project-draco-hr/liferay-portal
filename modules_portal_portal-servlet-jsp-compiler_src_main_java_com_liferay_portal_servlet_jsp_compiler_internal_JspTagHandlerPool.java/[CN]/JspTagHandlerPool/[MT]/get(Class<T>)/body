{
  JspTag jspTag=_jspTags.poll();
  if (jspTag == null) {
    try {
      if (_resourceInjector == null) {
        jspTag=jspTagClass.newInstance();
      }
 else {
        jspTag=_resourceInjector.createTagHandlerInstance(jspTagClass);
      }
    }
 catch (    Exception e) {
      throw new JspException(e);
    }
  }
 else {
    _counter.getAndDecrement();
  }
  return jspTag;
}
