{
  try {
    SyncWatchEvent syncWatchEvent=_syncWatchEventPersistence.fetchBySyncAccountId_First(syncAccountId);
    if (syncWatchEvent == null) {
      return false;
    }
    return true;
  }
 catch (  SQLException sqle) {
    if (_logger.isDebugEnabled()) {
      _logger.debug(sqle.getMessage(),sqle);
    }
    return false;
  }
}
