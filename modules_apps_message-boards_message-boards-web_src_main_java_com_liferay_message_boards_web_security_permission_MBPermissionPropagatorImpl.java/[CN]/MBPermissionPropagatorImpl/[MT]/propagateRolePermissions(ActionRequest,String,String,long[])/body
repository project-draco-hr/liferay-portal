{
  if (className.equals(MBCategory.class.getName())) {
    propagateCategoryRolePermissions(actionRequest,className,primKey,roleIds);
  }
 else   if (className.equals(MBMessage.class.getName())) {
    long messageId=GetterUtil.getLong(primKey);
    MBMessage message=_mbMessageLocalService.getMessage(messageId);
    if (message.isRoot()) {
      propagateThreadRolePermissions(actionRequest,className,messageId,message.getThreadId(),roleIds);
    }
  }
 else   if (className.equals("com.liferay.message.boards")) {
    propagateMBRolePermissions(actionRequest,className,primKey,roleIds);
  }
}
