{
  Map<String,List<StagedModel>> dependentStagedModelsMap=new HashMap<String,List<StagedModel>>();
  PollsQuestion question=PollsTestUtil.addQuestion(group.getGroupId());
  addDependentStagedModel(dependentStagedModelsMap,PollsQuestion.class,question);
  PollsChoice choice=PollsTestUtil.addChoice(group.getGroupId(),question.getQuestionId());
  PollsChoiceUtil.update(choice);
  addDependentStagedModel(dependentStagedModelsMap,PollsChoice.class,choice);
  return dependentStagedModelsMap;
}
