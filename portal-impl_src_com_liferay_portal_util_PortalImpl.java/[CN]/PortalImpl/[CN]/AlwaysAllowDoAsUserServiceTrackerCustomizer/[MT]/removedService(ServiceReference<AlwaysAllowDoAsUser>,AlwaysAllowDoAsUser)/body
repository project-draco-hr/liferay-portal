{
  Registry registry=RegistryUtil.getRegistry();
  registry.ungetService(serviceReference);
  if (_log.isDebugEnabled()) {
    _log.debug("Delete AlwaysAllowDoAsUser " + ClassUtil.getClassName(alwaysAllowDoAsUser));
  }
  _alwaysAllowDoAsUsers.remove(alwaysAllowDoAsUser);
  if (_log.isDebugEnabled()) {
    _log.debug("There are " + _alwaysAllowDoAsUsers.size() + " AlwaysAllowDoAsUser instances");
  }
}
