{
  if (PropsValues.WEB_SERVER_FORWARDED_PROTO_ENABLED) {
    String forwardedProto=request.getHeader(PropsValues.WEB_SERVER_FORWARDED_PROTO_HEADER);
    if (Validator.isNotNull(forwardedProto) && Validator.equals(Http.HTTPS,forwardedProto)) {
      return true;
    }
  }
  return request.isSecure();
}
