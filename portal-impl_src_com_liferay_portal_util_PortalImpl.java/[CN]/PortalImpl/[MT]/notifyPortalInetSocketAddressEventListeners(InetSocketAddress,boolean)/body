{
  for (  PortalInetSocketAddressEventListener portalInetSocketAddressEventListener : _portalInetSocketAddressEventListeners) {
    if (local) {
      portalInetSocketAddressEventListener.portalLocalAddressConfigured(inetSocketAddress);
    }
 else {
      portalInetSocketAddressEventListener.portalServerAddressConfigured(inetSocketAddress);
    }
  }
}
