{
  String[] guestPermissions=request.getParameterValues("guestPermissions");
  boolean inputPermissionsPublic=ParamUtil.getBoolean(request,"inputPermissionsPublic");
  boolean inputPermissionsShowConfigure=ParamUtil.getBoolean(request,"inputPermissionsShowConfigure");
  if (!inputPermissionsShowConfigure) {
    if (!inputPermissionsPublic) {
      guestPermissions=new String[0];
    }
 else     if ((guestPermissions == null) || (guestPermissions.length == 0)) {
      guestPermissions=new String[]{ActionKeys.VIEW};
    }
  }
  return guestPermissions;
}
