{
  portletId=PortletConstants.getRootPortletId(portletId);
  String portletTitle=LanguageUtil.get(resourceBundle,JavaConstants.JAVAX_PORTLET_TITLE.concat(StringPool.PERIOD).concat(portletId));
  if (Validator.isNull(portletTitle)) {
    portletTitle=ResourceBundleUtil.getString(resourceBundle,JavaConstants.JAVAX_PORTLET_TITLE);
  }
  return portletTitle;
}
