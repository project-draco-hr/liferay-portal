{
  String authLoginURL=null;
  if (Validator.isNull(PropsValues.AUTH_LOGIN_COMMUNITY_URL)) {
    return null;
  }
  boolean pageFound=false;
  for (  Layout layout : themeDisplay.getLayouts()) {
    if (layout.getFriendlyURL().equals(PropsValues.AUTH_LOGIN_COMMUNITY_URL)) {
      pageFound=true;
      break;
    }
  }
  if (pageFound && (themeDisplay.getLayout() != null)) {
    String layoutSetFriendlyURL=PortalUtil.getLayoutSetFriendlyURL(themeDisplay.getLayout().getLayoutSet(),themeDisplay);
    authLoginURL=layoutSetFriendlyURL + PropsValues.AUTH_LOGIN_COMMUNITY_URL;
  }
  return authLoginURL;
}
