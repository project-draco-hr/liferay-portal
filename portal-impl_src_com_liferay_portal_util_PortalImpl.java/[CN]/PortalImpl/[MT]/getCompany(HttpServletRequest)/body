{
  long companyId=getCompanyId(request);
  if (companyId <= 0) {
    return null;
  }
  Company company=(Company)request.getAttribute(WebKeys.COMPANY);
  if (company == null) {
    company=CompanyLocalServiceUtil.getCompanyById(companyId);
    request.setAttribute(WebKeys.COMPANY,company);
  }
  return company;
}
