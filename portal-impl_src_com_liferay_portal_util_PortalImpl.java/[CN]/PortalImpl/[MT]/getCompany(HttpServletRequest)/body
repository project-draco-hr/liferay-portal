{
  long companyId=getCompanyId(req);
  if (companyId <= 0) {
    return null;
  }
  Company company=(Company)req.getAttribute(WebKeys.COMPANY);
  if (company == null) {
    company=CompanyLocalServiceUtil.getCompanyById(companyId);
    req.setAttribute(WebKeys.COMPANY,company);
  }
  return company;
}
