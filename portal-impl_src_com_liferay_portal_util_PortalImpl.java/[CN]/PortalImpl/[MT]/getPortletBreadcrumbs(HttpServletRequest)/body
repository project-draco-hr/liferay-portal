{
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  String name=WebKeys.PORTLET_BREADCRUMBS;
  String portletName=portletDisplay.getPortletName();
  if (Validator.isNotNull(portletDisplay.getId()) && !portletName.equals(PortletKeys.BREADCRUMB) && !portletDisplay.isFocused()) {
    name=name.concat(StringPool.UNDERLINE.concat(portletDisplay.getId()));
  }
  return (List<BreadcrumbEntry>)request.getAttribute(name);
}
