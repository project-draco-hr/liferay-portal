{
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  boolean portletBreadcrumbEntry=Validator.isNotNull(portletDisplay.getId()) && !portletDisplay.isFocused();
  addPortletBreadcrumbEntry(request,title,url,null,portletBreadcrumbEntry);
}
