{
  List<BreadcrumbEntry> portletBreadcrumbs=(List<BreadcrumbEntry>)request.getAttribute(WebKeys.PORTLET_BREADCRUMBS);
  if (portletBreadcrumbs == null) {
    portletBreadcrumbs=new ArrayList<BreadcrumbEntry>();
    request.setAttribute(WebKeys.PORTLET_BREADCRUMBS,portletBreadcrumbs);
  }
  portletBreadcrumbs.add(new BreadcrumbEntryImpl(title,url,data));
}
