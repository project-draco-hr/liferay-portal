{
  LayoutType layoutType=layout.getLayoutType();
  if (!layoutType.isURLFriendliable()) {
    return null;
  }
  String groupFriendlyURL=getGroupFriendlyURL(layout.getGroup(),layout.isPrivateLayout(),themeDisplay);
  return groupFriendlyURL.concat(layout.getFriendlyURL(themeDisplay.getLocale()));
}
