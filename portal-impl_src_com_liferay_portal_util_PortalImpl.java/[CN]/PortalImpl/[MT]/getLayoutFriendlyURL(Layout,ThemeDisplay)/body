{
  LayoutTypeController layoutTypeController=LayoutTypeControllerTracker.getLayoutTypeController(layout.getType());
  if (!layoutTypeController.isURLFriendliable()) {
    return null;
  }
  String groupFriendlyURL=getGroupFriendlyURL(layout.getLayoutSet(),themeDisplay,false);
  return groupFriendlyURL.concat(layout.getFriendlyURL(themeDisplay.getLocale()));
}
