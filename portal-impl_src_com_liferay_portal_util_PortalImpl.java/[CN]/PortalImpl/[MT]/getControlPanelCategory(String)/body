{
  String category=null;
  if (ArrayUtil.contains(PropsValues.CONTROL_PANEL_CATEGORY_CONTENT_PORTLETS,portletId)) {
    category="content";
  }
 else   if (ArrayUtil.contains(PropsValues.CONTROL_PANEL_CATEGORY_PORTAL_PORTLETS,portletId)) {
    category="portal";
  }
 else   if (ArrayUtil.contains(PropsValues.CONTROL_PANEL_CATEGORY_SERVER_PORTLETS,portletId)) {
    category="server";
  }
  return category;
}
