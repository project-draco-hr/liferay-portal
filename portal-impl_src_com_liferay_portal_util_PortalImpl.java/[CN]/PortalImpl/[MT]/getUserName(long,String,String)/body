{
  String userName=defaultUserName;
  try {
    User user=UserLocalServiceUtil.getUserById(userId);
    if (userAttribute.equals(UserAttributes.USER_NAME_FULL)) {
      userName=user.getFullName();
    }
 else {
      userName=user.getScreenName();
    }
  }
 catch (  Exception e) {
  }
  return userName;
}
