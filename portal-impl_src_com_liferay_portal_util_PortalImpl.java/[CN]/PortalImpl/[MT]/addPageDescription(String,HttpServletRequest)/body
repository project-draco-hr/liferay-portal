{
  ListMergeable<String> descriptionListMergeable=(ListMergeable<String>)request.getAttribute(WebKeys.PAGE_DESCRIPTION);
  if (descriptionListMergeable == null) {
    descriptionListMergeable=new ListMergeable<String>();
    request.setAttribute(WebKeys.PAGE_DESCRIPTION,descriptionListMergeable);
  }
  descriptionListMergeable.add(description);
}
