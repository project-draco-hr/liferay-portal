{
  UnsyncByteArrayOutputStream unsyncByteArrayOutputStream=new UnsyncByteArrayOutputStream();
  try (ObjectOutputStream objectOutputStream=new ObjectOutputStream(unsyncByteArrayOutputStream)){
    objectOutputStream.writeObject(welder);
  }
   ByteBuffer byteBuffer=unsyncByteArrayOutputStream.unsafeGetByteBuffer();
  UnsyncByteArrayInputStream unsyncByteArrayInputStream=new UnsyncByteArrayInputStream(byteBuffer.array(),0,byteBuffer.remaining());
  ObjectInputStream objectInputStream=new ObjectInputStream(unsyncByteArrayInputStream);
  return (T)objectInputStream.readObject();
}
