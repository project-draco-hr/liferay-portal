{
  try {
    KaleoInstance kaleoInstance=kaleoInstanceLocalService.getKaleoInstance(workflowInstanceId);
    KaleoInstanceToken rootKaleoInstanceToken=kaleoInstance.getRootKaleoInstanceToken(null,serviceContext);
    List<String> transitionNames=new ArrayList<>();
    getNextTransitionNames(rootKaleoInstanceToken,transitionNames);
    return transitionNames;
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
