{
  try {
    KaleoInstance kaleoInstance=kaleoInstanceLocalService.getKaleoInstance(workflowInstanceId);
    KaleoInstanceToken rootKaleoInstanceToken=kaleoInstance.getRootKaleoInstanceToken(serviceContext);
    return WorkflowModelUtil.toWorkflowInstance(kaleoInstance,rootKaleoInstanceToken);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
