{
  if (parameters.length != 2) {
    throw new IllegalArgumentException("Two parameters are expected");
  }
  String fieldName=parameters[0].toString();
  List<DDMFormFieldEvaluationResult> ddmFormFieldEvaluationResults=getDDMFormFieldEvaluationResults(fieldName);
  boolean propertyValue=(Boolean)parameters[1];
  for (  DDMFormFieldEvaluationResult ddmFormFieldEvaluationResult : ddmFormFieldEvaluationResults) {
    ddmFormFieldEvaluationResult.setReadOnly(!propertyValue);
  }
  return true;
}
