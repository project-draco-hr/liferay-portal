        listener.onBeforeRemoveAssociation(assetTag.getPrimaryKey(),AssetEntry.class.getName(),entryId);
      }
    }
  }
  _sqlUpdate.update(new Object[]{new Long(entryId)});
  if ((listeners.length > 0) || (assetTagListeners.length > 0)) {
