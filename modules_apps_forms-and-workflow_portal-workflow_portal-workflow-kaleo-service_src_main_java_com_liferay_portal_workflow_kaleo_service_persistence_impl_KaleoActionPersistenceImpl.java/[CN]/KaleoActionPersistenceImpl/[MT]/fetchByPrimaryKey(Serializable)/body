{
  Serializable serializable=entityCache.getResult(KaleoActionModelImpl.ENTITY_CACHE_ENABLED,KaleoActionImpl.class,primaryKey);
  if (serializable == nullModel) {
    return null;
  }
  KaleoAction kaleoAction=(KaleoAction)serializable;
  if (kaleoAction == null) {
    Session session=null;
    try {
      session=openSession();
      kaleoAction=(KaleoAction)session.get(KaleoActionImpl.class,primaryKey);
      if (kaleoAction != null) {
        cacheResult(kaleoAction);
      }
 else {
        entityCache.putResult(KaleoActionModelImpl.ENTITY_CACHE_ENABLED,KaleoActionImpl.class,primaryKey,nullModel);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(KaleoActionModelImpl.ENTITY_CACHE_ENABLED,KaleoActionImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return kaleoAction;
}
