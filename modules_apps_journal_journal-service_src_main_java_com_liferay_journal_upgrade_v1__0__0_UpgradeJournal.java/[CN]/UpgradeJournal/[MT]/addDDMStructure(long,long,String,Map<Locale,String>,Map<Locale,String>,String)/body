{
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  long userId=_userLocalService.getDefaultUserId(companyId);
  DDMStructure ddmStructure=_ddmStructureLocalService.addStructure(userId,groupId,DDMStructureManager.STRUCTURE_DEFAULT_PARENT_STRUCTURE_ID,PortalUtil.getClassNameId(JournalArticle.class.getName()),ddmStructureKey,nameMap,descriptionMap,definition,StorageEngineManager.STORAGE_TYPE_DEFAULT,DDMStructureManager.STRUCTURE_TYPE_DEFAULT,serviceContext);
  return ddmStructure.getStructureId();
}
