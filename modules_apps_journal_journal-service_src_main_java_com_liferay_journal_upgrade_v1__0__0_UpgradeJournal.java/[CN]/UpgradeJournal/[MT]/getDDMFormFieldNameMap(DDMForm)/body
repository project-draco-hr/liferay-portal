{
  Map<String,String> ddmFormFieldNameMap=new HashMap<>();
  Map<String,DDMFormField> ddmFormFieldsMap=ddmForm.getDDMFormFieldsMap(true);
  for (  DDMFormField ddmFormField : ddmFormFieldsMap.values()) {
    Object oldName=ddmFormField.getProperties().get("oldName");
    if (oldName != null) {
      ddmFormFieldNameMap.put(oldName.toString(),ddmFormField.getName());
    }
 else {
      ddmFormFieldNameMap.put(ddmFormField.getName(),ddmFormField.getName());
    }
  }
  return ddmFormFieldNameMap;
}
