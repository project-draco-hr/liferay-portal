{
  if ((callerClass == PACLAdvice.class) || (callerClass == PACLDataSource.class) || (callerClass == PACLBeanHandler.class)|| (callerClass == PortalSecurityManager.class)) {
    return true;
  }
  if (debug) {
    _log.debug("Frame " + i + " has caller class "+ callerClass.getName());
  }
  return false;
}
