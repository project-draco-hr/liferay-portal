{
  MBCategory category=mbCategoryLocalService.getCategory(categoryId);
  MBCategoryPermission.check(getPermissionChecker(),category,ActionKeys.UPDATE);
  return mbCategoryLocalService.updateCategory(categoryId,parentCategoryId,name,description,displayStyle,emailAddress,inProtocol,inServerName,inServerPort,inUseSSL,inUserName,inPassword,inReadInterval,outEmailAddress,outCustom,outServerName,outServerPort,outUseSSL,outUserName,outPassword,mailingListActive,allowAnonymousEmail,mergeWithParentCategory,serviceContext);
}
