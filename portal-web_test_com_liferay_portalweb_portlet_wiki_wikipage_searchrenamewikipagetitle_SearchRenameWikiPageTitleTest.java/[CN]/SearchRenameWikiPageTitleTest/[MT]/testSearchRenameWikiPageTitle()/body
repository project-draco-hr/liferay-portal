{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Wiki Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace("Wiki Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.type("//input[@id='_36_keywords']",RuntimeVariables.replace("Wiki Page Title"));
  selenium.saveScreenShotAndSource();
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace("Search"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Wiki2 Page2 Title2"),selenium.getText("//td[3]/a"));
  assertFalse(selenium.isTextPresent("Wiki Page Title"));
}
