{
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  long recordSetId=ParamUtil.getLong(request,portletDisplay.getNamespace() + "recordSetId");
  if (recordSetId == 0) {
    return false;
  }
  User user=themeDisplay.getUser();
  if (user.isDefaultUser()) {
    return false;
  }
  try {
    return DDLRecordSetPermission.contains(themeDisplay.getPermissionChecker(),recordSetId,ActionKeys.VIEW);
  }
 catch (  PortalException pe) {
    return false;
  }
}
