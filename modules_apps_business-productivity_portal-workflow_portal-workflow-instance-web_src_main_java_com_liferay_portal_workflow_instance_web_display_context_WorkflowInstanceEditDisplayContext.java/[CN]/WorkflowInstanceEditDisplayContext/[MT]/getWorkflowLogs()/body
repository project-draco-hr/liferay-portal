{
  if (_workflowLogs == null) {
    OrderByComparator<WorkflowLog> orderByComparator=WorkflowComparatorFactoryUtil.getLogCreateDateComparator(false);
    _workflowLogs=WorkflowLogManagerUtil.getWorkflowLogsByWorkflowInstance(workflowInstanceRequestHelper.getCompanyId(),getWorkflowInstanceId(),_LOG_TYPES,QueryUtil.ALL_POS,QueryUtil.ALL_POS,orderByComparator);
  }
  return _workflowLogs;
}
