{
  if (primary) {
    List<Phone> phones=phonePersistence.findByC_C_C_P(companyId,classNameId,classPK,primary);
    for (    Phone phone : phones) {
      if ((phoneId <= 0) || (phone.getPhoneId() != phoneId)) {
        phone.setPrimary(false);
        phonePersistence.update(phone);
      }
    }
  }
}
