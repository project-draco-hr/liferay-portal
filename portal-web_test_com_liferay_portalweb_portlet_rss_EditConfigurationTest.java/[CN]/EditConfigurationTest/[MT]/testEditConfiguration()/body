{
  selenium.click("//img[@title='Configuration']");
  selenium.waitForPageToLoad("30000");
  selenium.click("//a/img");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("document._86_fm._86_url[3]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.type("document._86_fm._86_url[3]","http://sitening.com/digg/rss/");
  selenium.select("_86_entriesPerFeed","label=4");
  selenium.click("//input[@value='Save']");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Return to Full Page");
  selenium.waitForPageToLoad("30000");
  verifyTrue(selenium.isTextPresent("Sitening Digg Feed "));
}
