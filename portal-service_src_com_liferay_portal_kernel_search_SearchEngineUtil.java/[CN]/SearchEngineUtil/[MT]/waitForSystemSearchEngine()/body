{
  try {
    int count=1000;
    while (!_searchEngines.containsKey(SYSTEM_ENGINE_ID) && (--count > 0)) {
      if (_log.isDebugEnabled()) {
        _log.debug("Waiting for a " + SYSTEM_ENGINE_ID);
      }
      Thread.sleep(500);
    }
  }
 catch (  InterruptedException e) {
    _log.error(e,e);
  }
}
