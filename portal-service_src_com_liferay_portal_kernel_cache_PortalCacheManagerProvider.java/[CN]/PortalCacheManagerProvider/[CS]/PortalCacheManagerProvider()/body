{
  _portalCacheManagers=new ConcurrentHashMap<>();
  Registry registry=RegistryUtil.getRegistry();
  _serviceTracker=registry.trackServices((Class<PortalCacheManager<? extends Serializable,?>>)(Class<?>)PortalCacheManager.class,new PortalCacheProviderServiceTrackerCustomizer());
  _serviceTracker.open();
}
