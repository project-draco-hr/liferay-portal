{
  ConfigurationCapability configurationCapability=_documentRepository.getCapability(ConfigurationCapability.class);
  String temporaryFileEntriesTimeout=configurationCapability.getProperty(getClass(),_PROPERTY_TEMPORARY_FILE_ENTRIES_TIMEOUT);
  if (temporaryFileEntriesTimeout == null) {
    return _TEMPORARY_FILE_ENTRIES_TIMEOUT_DEFAULT;
  }
  return GetterUtil.getLong(temporaryFileEntriesTimeout);
}
