{
  Folder folder=addTempFolder(temporaryFileEntriesScope);
  File file=null;
  try {
    file=FileUtil.createTempFile(inputStream);
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setAddGroupPermissions(true);
    serviceContext.setAddGuestPermissions(true);
    return _documentRepository.addFileEntry(temporaryFileEntriesScope.getUserId(),folder.getFolderId(),fileName,mimeType,fileName,StringPool.BLANK,StringPool.BLANK,file,serviceContext);
  }
 catch (  IOException ioe) {
    throw new SystemException("Unable to write temporary file",ioe);
  }
 finally {
    FileUtil.delete(file);
  }
}
