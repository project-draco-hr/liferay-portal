{
  AjaxXmlBuilder builder=new AjaxXmlBuilder();
  boolean nullable=ParamUtil.getBoolean(req,"nullable");
  String countryId=ParamUtil.getString(req,"countryId");
  List regions=RegionServiceUtil.getRegions(countryId,true);
  if (nullable) {
    builder.addItem("null","null");
  }
  builder.addItems(regions,"name","regionId");
  return builder.toString();
}
