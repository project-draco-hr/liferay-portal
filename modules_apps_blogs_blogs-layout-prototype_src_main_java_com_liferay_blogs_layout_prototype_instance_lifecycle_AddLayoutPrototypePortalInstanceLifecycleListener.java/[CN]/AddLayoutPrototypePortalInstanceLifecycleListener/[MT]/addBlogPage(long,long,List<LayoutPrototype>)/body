{
  Map<Locale,String> nameMap=ResourceBundleUtil.getLocalizationMap("content.Language",getClass(),"layout-prototype-blog-title");
  Map<Locale,String> descriptionMap=ResourceBundleUtil.getLocalizationMap("content.Language",getClass(),"layout-prototype-blog-description");
  Layout layout=DefaultLayoutPrototypesUtil.addLayoutPrototype(companyId,defaultUserId,nameMap,descriptionMap,"2_columns_iii",layoutPrototypes);
  if (layout == null) {
    return;
  }
  DefaultLayoutPrototypesUtil.addPortletId(layout,BlogsPortletKeys.BLOGS,"column-1");
  DefaultLayoutPrototypesUtil.addPortletId(layout,AssetTagsNavigationPortletKeys.ASSET_TAGS_CLOUD,"column-2");
  DefaultLayoutPrototypesUtil.addPortletId(layout,RecentBloggersPortletKeys.RECENT_BLOGGERS,"column-2");
}
