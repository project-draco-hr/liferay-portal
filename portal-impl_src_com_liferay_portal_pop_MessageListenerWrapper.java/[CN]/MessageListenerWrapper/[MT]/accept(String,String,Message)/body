{
  if (_log.isDebugEnabled()) {
    _log.debug("Listener " + _listener.getClass().getName());
    _log.debug("From " + from);
    _log.debug("Recipient " + recipient);
  }
  boolean value=_listener.accept(from,recipient,message);
  if (_log.isDebugEnabled()) {
    _log.debug("Accept " + value);
  }
  return value;
}
