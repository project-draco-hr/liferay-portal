{
  try {
    new OutputBlob(null,10);
    Assert.fail();
  }
 catch (  IllegalArgumentException iae) {
  }
  try {
    new OutputBlob(new ByteArrayInputStream(new byte[10]),-1);
    Assert.fail();
  }
 catch (  IllegalArgumentException iae) {
  }
  new OutputBlob(new ByteArrayInputStream(new byte[10]),10);
}
