{
  if (!isEnabled()) {
    return Collections.EMPTY_LIST;
  }
  _readLock.lock();
  try {
    Collection<ClusterNode> nodes=_addressMap.values();
    List<ClusterNode> clusterNodes=new ArrayList<ClusterNode>(nodes.size());
    clusterNodes.addAll(nodes);
    return clusterNodes;
  }
  finally {
    _readLock.unlock();
  }
}
