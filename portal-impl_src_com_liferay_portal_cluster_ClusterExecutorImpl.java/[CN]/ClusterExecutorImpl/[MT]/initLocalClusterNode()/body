{
  InetAddress bindInetAddress=getBindInetAddress(_controlJChannel);
  ClusterNode localClusterNode=new ClusterNode(PortalUUIDUtil.generate(),bindInetAddress);
  InetSocketAddress portalInetSocketAddress=getConfiguredPortalInetSockAddress(_secure);
  if (portalInetSocketAddress != null) {
    localClusterNode.setPortalInetSocketAddress(portalInetSocketAddress);
  }
  _localClusterNode=localClusterNode;
}
