{
  Properties controlProperties=PropsUtil.getProperties(PropsKeys.CLUSTER_LINK_CHANNEL_PROPERTIES_CONTROL,false);
  String controlProperty=controlProperties.getProperty(PropsKeys.CLUSTER_LINK_CHANNEL_PROPERTIES_CONTROL);
  _clusterReceiver=new ClusterRequestReceiver(this);
  _controlChannel=new JGroupsClusterChannel(controlProperty,_DEFAULT_CLUSTER_NAME,_clusterReceiver);
}
