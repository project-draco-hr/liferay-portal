{
  if (!isEnabled() || _localClusterNode.getPort() == PropsValues.PORTAL_INSTANCE_HTTP_PORT) {
    return;
  }
  try {
    _localClusterNode.setPort(port);
    memberJoined(_localAddress,_localClusterNode);
    ClusterRequest clusterRequest=ClusterRequest.createClusterRequest(ClusterMessageType.UPDATE,_localClusterNode);
    _controlJChannel.send(null,null,clusterRequest);
  }
 catch (  Exception e) {
    _log.error("Unable to determine configure node port",e);
  }
}
