{
  _addressMap.put(joinAddress,clusterNode);
  Address previousAddress=_clusterNodeIdMap.put(clusterNode.getClusterNodeId(),joinAddress);
  if ((previousAddress == null) && !getLocalControlAddress().equals(joinAddress)) {
    ClusterEvent clusterEvent=ClusterEvent.join(clusterNode);
    fireClusterEvent(clusterEvent);
  }
}
