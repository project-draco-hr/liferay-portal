{
  MethodHandler methodHandler=clusterRequest.getMethodHandler();
  if (methodHandler == null) {
    return ClusterNodeResponse.createExceptionClusterNodeResponse(_localClusterNode,clusterRequest.getUuid(),new ClusterException("Payload is not of type " + MethodHandler.class.getName()));
  }
  try {
    return ClusterNodeResponse.createResultClusterNodeResponse(_localClusterNode,clusterRequest.getUuid(),methodHandler.invoke());
  }
 catch (  Exception e) {
    return ClusterNodeResponse.createExceptionClusterNodeResponse(_localClusterNode,clusterRequest.getUuid(),e);
  }
}
