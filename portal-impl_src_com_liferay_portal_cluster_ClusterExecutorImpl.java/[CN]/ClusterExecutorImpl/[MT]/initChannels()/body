{
  String controlProperty=PropsUtil.get(PropsKeys.CLUSTER_LINK_CHANNEL_PROPERTIES_CONTROL);
  ClusterRequestReceiver clusterInvokeReceiver=new ClusterRequestReceiver(this);
  try {
    _controlChannel=createChannel(controlProperty,clusterInvokeReceiver,_DEFAULT_CLUSTER_NAME);
  }
 catch (  ChannelException ex) {
    _log.error(ex,ex);
  }
catch (  Exception ex) {
    _log.error(ex,ex);
  }
}
