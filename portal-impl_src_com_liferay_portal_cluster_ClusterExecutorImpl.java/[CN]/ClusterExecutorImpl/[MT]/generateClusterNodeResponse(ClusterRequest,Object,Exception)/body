{
  if (exception != null) {
    return ClusterNodeResponse.createExceptionClusterNodeResponse(getLocalClusterNode(),clusterRequest.getClusterMessageType(),clusterRequest.getUuid(),clusterRequest.isMulticast(),exception);
  }
  return ClusterNodeResponse.createResultClusterNodeResponse(getLocalClusterNode(),clusterRequest.getClusterMessageType(),clusterRequest.getUuid(),clusterRequest.isMulticast(),returnValue);
}
