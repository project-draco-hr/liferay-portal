{
  try {
    java.util.List<com.liferay.portlet.social.model.SocialActivityDefinition> returnValue=SocialActivitySettingServiceUtil.getActivityDefinitions(groupId,className);
    return returnValue.toArray(new com.liferay.portlet.social.model.SocialActivityDefinition[returnValue.size()]);
