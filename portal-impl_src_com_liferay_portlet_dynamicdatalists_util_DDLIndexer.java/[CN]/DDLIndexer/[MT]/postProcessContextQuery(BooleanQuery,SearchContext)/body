{
  int status=GetterUtil.getInteger(searchContext.getAttribute(Field.STATUS),WorkflowConstants.STATUS_APPROVED);
  if (status != WorkflowConstants.STATUS_ANY) {
    contextQuery.addRequiredTerm(Field.STATUS,status);
  }
  long recordSetId=GetterUtil.getLong(searchContext.getAttribute("recordSetId"));
  if (recordSetId > 0) {
    contextQuery.addRequiredTerm("recordSetId",recordSetId);
  }
}
