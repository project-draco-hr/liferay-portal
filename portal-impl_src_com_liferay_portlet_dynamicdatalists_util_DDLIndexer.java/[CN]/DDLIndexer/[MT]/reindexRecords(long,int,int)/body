{
  List<DDLRecord> records=DDLRecordLocalServiceUtil.getCompanyRecords(companyId,WorkflowConstants.STATUS_APPROVED,DDLRecordSetConstants.SCOPE_DYNAMIC_DATA_LISTS,start,end,null);
  Collection<Document> documents=new ArrayList<Document>(records.size());
  for (  DDLRecord record : records) {
    Document document=getDocument(record);
    documents.add(document);
  }
  SearchEngineUtil.updateDocuments(getSearchEngineId(),companyId,documents);
}
