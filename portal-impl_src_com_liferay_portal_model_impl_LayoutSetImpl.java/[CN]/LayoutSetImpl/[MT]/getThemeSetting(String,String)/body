{
  UnicodeProperties settingsProperties=getSettingsProperties();
  String value=settingsProperties.getProperty(ThemeSettingImpl.namespaceProperty(device,key));
  if (value != null) {
    return value;
  }
  Theme theme=null;
  if (device.equals("regular")) {
    theme=getTheme();
  }
 else {
    theme=getWapTheme();
  }
  value=theme.getSetting(key);
  return value;
}
