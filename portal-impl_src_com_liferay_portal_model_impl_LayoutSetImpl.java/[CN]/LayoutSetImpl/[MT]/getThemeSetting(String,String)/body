{
  UnicodeProperties properties=getSettingsProperties();
  String value=properties.getThemeProperty(key,device);
  if (value != null) {
    return value;
  }
  Theme theme=null;
  if (device.equals("regular")) {
    theme=getTheme();
  }
 else {
    theme=getWapTheme();
  }
  value=theme.getSetting(key);
  return value;
}
