{
  try {
    VirtualHost virtualHost=VirtualHostLocalServiceUtil.fetchVirtualHost(getCompanyId(),getLayoutSetId());
    if (virtualHost == null) {
      return StringPool.BLANK;
    }
 else {
      return virtualHost.getHostname();
    }
  }
 catch (  Exception e) {
    return StringPool.BLANK;
  }
}
