{
  Collections.sort(uris);
  StringBuilder sb=new StringBuilder();
  for (  URI uri : uris) {
    sb.append(Utilities.toIntegrityKey(uri));
    sb.append('=');
    sb.append(Utilities.toChecksum(uri));
    sb.append('\n');
  }
  sb.setLength(sb.length() - 1);
  Files.createDirectories(integrityPropertiesPath.getParent());
  Files.write(integrityPropertiesPath,Collections.singleton(sb.toString()),StandardCharsets.UTF_8);
  System.out.println("== Saved integrity.properties at " + integrityPropertiesPath);
}
