{
  Path indexFilePath=Paths.get(indexesFileName);
  if (Files.exists(indexFilePath)) {
    return _loadIndexes(indexFilePath);
  }
  List<URI> uris=new ArrayList<>();
  uris.add(_indexTargetPlatform(moduleFrameworkStaticDir,moduleFrameworkModulesDir,moduleFrameworkPortalDir));
  uris.addAll(_indexLPKGFiles(Utilities.listFiles(moduleFrameworkMarketplaceDir,"*.lpkg")));
  _saveIndexes(indexesFileName,uris);
  return uris;
}
