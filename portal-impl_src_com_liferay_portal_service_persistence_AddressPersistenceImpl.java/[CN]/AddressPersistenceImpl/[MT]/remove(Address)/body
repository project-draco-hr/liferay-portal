{
  Session session=null;
  try {
    session=openSession();
    address=(Address)session.merge(address);
    session.delete(address);
    session.flush();
    return address;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Address.class.getName());
  }
}
