{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(address);
    }
  }
  address=removeImpl(address);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(address);
    }
  }
  return address;
}
