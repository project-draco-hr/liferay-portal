{
  Session session=null;
  try {
    session=openSession();
    Address address=(Address)session.get(AddressImpl.class,new Long(addressId));
    if (address == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No Address exists with the primary key " + addressId);
      }
      throw new NoSuchAddressException("No Address exists with the primary key " + addressId);
    }
    return remove(address);
  }
 catch (  NoSuchAddressException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
