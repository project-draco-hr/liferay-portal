{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,address,merge);
    address.setNew(false);
    return address;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Address.class.getName());
  }
}
