{
  UpgradePortletId upgradePortletId=new UpgradePortletId(){
    @Override protected String[][] getRenamePortletIdsArray(){
      return new String[][]{new String[]{"86",PortletConfigurationPortletKeys.PORTLET_CONFIGURATION}};
    }
  }
;
  _releaseLocalService.updateRelease("com.liferay.portlet.configuration.web",Collections.<UpgradeProcess>singletonList(upgradePortletId),1,1,false);
