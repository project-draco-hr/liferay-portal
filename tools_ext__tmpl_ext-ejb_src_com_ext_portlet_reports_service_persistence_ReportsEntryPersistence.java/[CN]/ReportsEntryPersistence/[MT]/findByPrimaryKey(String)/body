{
  Session session=null;
  try {
    session=openSession();
    ReportsEntry reportsEntry=(ReportsEntry)session.get(ReportsEntry.class,entryId);
    if (reportsEntry == null) {
      _log.warn("No ReportsEntry exists with the primary key " + entryId.toString());
      throw new NoSuchEntryException("No ReportsEntry exists with the primary key " + entryId.toString());
    }
    return reportsEntry;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
