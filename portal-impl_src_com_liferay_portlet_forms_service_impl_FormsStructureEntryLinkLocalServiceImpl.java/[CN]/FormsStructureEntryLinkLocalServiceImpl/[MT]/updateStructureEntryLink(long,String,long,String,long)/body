{
  FormsStructureEntryLink structureEntryLink=formsStructureEntryLinkPersistence.findByPrimaryKey(structureEntryLinkId);
  structureEntryLink.setStructureId(structureId);
  structureEntryLink.setClassName(className);
  structureEntryLink.setClassPK(classPK);
  return formsStructureEntryLinkPersistence.update(structureEntryLink,false);
}
