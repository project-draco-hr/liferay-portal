{
  FormStructure structure=formStructurePersistence.findByPrimaryKey(formStructureLink.getFormStructureId());
  resourceLocalService.addResources(structure.getCompanyId(),structure.getGroupId(),structure.getUserId(),FormStructureLink.class.getName(),formStructureLink.getFormStructureLinkId(),false,addCommunityPermissions,addGuestPermissions);
}
