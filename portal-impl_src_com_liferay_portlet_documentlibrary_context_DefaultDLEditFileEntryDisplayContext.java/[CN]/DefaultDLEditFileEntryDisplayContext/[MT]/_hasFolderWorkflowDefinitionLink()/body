{
  try {
    ThemeDisplay themeDisplay=(ThemeDisplay)_request.getAttribute(WebKeys.THEME_DISPLAY);
    long folderId=BeanParamUtil.getLong(_fileEntry,_request,"folderId");
    return DLUtil.hasWorkflowDefinitionLink(themeDisplay.getCompanyId(),themeDisplay.getScopeGroupId(),folderId,_dlFileEntryType.getFileEntryTypeId());
  }
 catch (  Exception e) {
    throw new SystemException("Unable to check if folder has workflow definition link",e);
  }
}
