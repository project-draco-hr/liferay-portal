{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection(Constants.DATA_SOURCE);
    ps=con.prepareStatement(_UPGRADE_PAGE);
    ps.setString(1,nodeId);
    ps.setBoolean(2,true);
    rs=ps.executeQuery();
    while (rs.next()) {
      String title=rs.getString("title");
      double version=rs.getDouble("version");
      boolean addCommunityPermissions=true;
      boolean addGuestPermissions=true;
      _log.debug("Upgrading page " + new WikiPagePK(nodeId,title,version));
      WikiPageLocalServiceUtil.addPageResources(nodeId,title,addCommunityPermissions,addGuestPermissions);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
