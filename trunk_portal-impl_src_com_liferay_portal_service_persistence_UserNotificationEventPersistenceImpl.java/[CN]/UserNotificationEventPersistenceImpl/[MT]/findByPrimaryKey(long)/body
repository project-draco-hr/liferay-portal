{
  UserNotificationEvent userNotificationEvent=fetchByPrimaryKey(userNotificationEventId);
  if (userNotificationEvent == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + userNotificationEventId);
    }
    throw new NoSuchUserNotificationEventException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + userNotificationEventId);
  }
  return userNotificationEvent;
}
