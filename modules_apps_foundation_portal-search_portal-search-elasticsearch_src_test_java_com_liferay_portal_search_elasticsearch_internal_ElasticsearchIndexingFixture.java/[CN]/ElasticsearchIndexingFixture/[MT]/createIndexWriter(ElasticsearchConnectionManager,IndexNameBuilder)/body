{
  final ElasticsearchUpdateDocumentCommand updateDocumentCommand=new ElasticsearchUpdateDocumentCommandImpl(){
{
      elasticsearchConnectionManager=elasticsearchConnectionManager1;
      elasticsearchDocumentFactory=new DefaultElasticsearchDocumentFactory();
      indexNameBuilder=indexNameBuilder1;
      activate(_properties);
    }
  }
;
  return new ElasticsearchIndexWriter(){
{
      elasticsearchConnectionManager=elasticsearchConnectionManager1;
      elasticsearchUpdateDocumentCommand=updateDocumentCommand;
      indexNameBuilder=indexNameBuilder1;
    }
  }
;
}
