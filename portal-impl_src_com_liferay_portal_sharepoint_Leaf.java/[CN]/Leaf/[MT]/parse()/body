{
  StringBundler sb=new StringBundler(7);
  if (_useEqualSymbol) {
    sb.append(OPEN_LI);
    sb.append(_key);
    sb.append(StringPool.EQUAL);
    sb.append(_value);
  }
 else {
    sb.append(OPEN_LI);
    sb.append(_key);
    sb.append(StringPool.NEW_LINE);
    sb.append(OPEN_LI);
    sb.append(_value);
  }
  if (_newLine) {
    sb.append(StringPool.NEW_LINE);
  }
  return sb.toString();
}
