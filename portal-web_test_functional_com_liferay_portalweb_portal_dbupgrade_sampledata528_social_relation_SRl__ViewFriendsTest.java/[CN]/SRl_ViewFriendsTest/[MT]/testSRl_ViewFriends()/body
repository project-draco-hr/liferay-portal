{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/socialrelationsn1/home/");
  selenium.clickAt("link=Friends Test Page",RuntimeVariables.replace("Friends Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("You have 1 friends."),selenium.getText("//div[@class='portlet-content-container']/div/div"));
  assertTrue(selenium.isPartialText("//a[@class='user-name']","socialrelationfn2 socialrelationmn2 socialrelationln2"));
}
