{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Blogs Test Page",RuntimeVariables.replace("Blogs Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Permissions Blogs Test Entry"),selenium.getText("//div[@class='entry-title']/h2/a"));
  selenium.clickAt("//div[@class='entry-title']/h2/a",RuntimeVariables.replace("Permissions Blogs Test Entry"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("This is a permissions blogs test entry"),selenium.getText("//div[@class='entry-body']"));
}
