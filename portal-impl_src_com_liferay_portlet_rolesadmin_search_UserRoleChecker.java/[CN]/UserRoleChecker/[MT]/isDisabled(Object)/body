{
  User user=(User)obj;
  try {
    if (isChecked(user)) {
      if (RoleMembershipPolicyUtil.isRoleRequired(user.getUserId(),_role.getRoleId())) {
        return true;
      }
    }
 else {
      if (!RoleMembershipPolicyUtil.isRoleAllowed(user.getUserId(),_role.getRoleId())) {
        return true;
      }
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return super.isDisabled(obj);
}
