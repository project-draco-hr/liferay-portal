{
  User user=(User)obj;
  try {
    if (isChecked(user) || RoleMembershipPolicyUtil.isRoleRequired(user.getUserId(),_role.getRoleId()) || !RoleMembershipPolicyUtil.isRoleAllowed(user.getUserId(),_role.getRoleId())) {
      return true;
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return super.isDisabled(obj);
}
