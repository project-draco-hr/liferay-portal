{
  User user=(User)obj;
  try {
    if ((isChecked(user) && RoleMembershipPolicyUtil.isRoleRequired(user.getUserId(),_role.getRoleId())) || (!isChecked(user) && !RoleMembershipPolicyUtil.isRoleAllowed(user.getUserId(),_role.getRoleId()))) {
      return true;
    }
  }
  finally {
    return super.isDisabled(obj);
  }
}
