{
  try {
    ActionUtil.getProductVersion(req);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchProductVersionException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.software_catalog.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward("portlet.software_catalog.view_product_version");
}
