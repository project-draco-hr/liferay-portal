{
  _resourceActions.read(null,UpgradeJournal.class.getClassLoader(),"/META-INF/resource-actions/journal_ddm_composite_models.xml");
  List<String> modelNames=_resourceActions.getPortletModelResources(JournalPortletKeys.JOURNAL);
  for (  String modelName : modelNames) {
    List<String> modelActions=_resourceActions.getModelResourceActions(modelName);
    _resourceActionLocalService.checkResourceActions(modelName,modelActions);
  }
}
