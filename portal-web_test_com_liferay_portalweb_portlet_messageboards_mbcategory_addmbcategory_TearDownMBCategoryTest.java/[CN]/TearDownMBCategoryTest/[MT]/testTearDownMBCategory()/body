case 1:
      selenium.selectWindow("null");
    selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
selenium.clickAt("link=Message Boards Test Page",RuntimeVariables.replace("Message Boards Test Page"));
selenium.waitForPageToLoad("30000");
boolean category1Present=selenium.isElementPresent("//span[@title='Actions']/ul/li/strong/a");
if (!category1Present) {
label=2;
continue;
}
Thread.sleep(5000);
assertEquals(RuntimeVariables.replace("Actions"),selenium.getText("//span[@title='Actions']/ul/li/strong/a/span"));
selenium.clickAt("//span[@title='Actions']/ul/li/strong/a/span",RuntimeVariables.replace("Actions"));
selenium.waitForVisible("//div[@class='lfr-component lfr-menu-list']/ul/li[contains(.,'Delete')]/a");
assertEquals(RuntimeVariables.replace("Delete"),selenium.getText("//div[@class='lfr-component lfr-menu-list']/ul/li[contains(.,'Delete')]/a"));
selenium.click(RuntimeVariables.replace("//div[@class='lfr-component lfr-menu-list']/ul/li[contains(.,'Delete')]/a"));
selenium.waitForPageToLoad("30000");
assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this[\\s\\S] It will be deleted immediately.$"));
