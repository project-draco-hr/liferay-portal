{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  String name=ParamUtil.getString(actionRequest,"name");
  PortletPreferences setup=PortletPreferencesFactoryUtil.getPortletSetup(actionRequest,portlet.getPortletId());
  PortletPreferencesServiceUtil.updateArchivePreferences(themeDisplay.getUserId(),themeDisplay.getScopeGroupId(),name,portlet.getRootPortletId(),setup);
}
