{
  ThemeDisplay themeDisplay=(ThemeDisplay)req.getAttribute(WebKeys.THEME_DISPLAY);
  String name=ParamUtil.getString(req,"name");
  PortletPreferences setup=PortletPreferencesFactoryUtil.getPortletSetup(req,portlet.getPortletId());
  PortletPreferencesServiceUtil.updateArchivePreferences(themeDisplay.getUserId(),themeDisplay.getLayout().getGroupId(),name,portlet.getRootPortletId(),setup);
}
