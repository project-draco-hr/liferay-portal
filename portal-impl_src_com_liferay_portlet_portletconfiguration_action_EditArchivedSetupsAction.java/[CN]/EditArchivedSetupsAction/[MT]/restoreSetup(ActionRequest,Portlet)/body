{
  Layout layout=(Layout)actionRequest.getAttribute(WebKeys.LAYOUT);
  String name=ParamUtil.getString(actionRequest,"name");
  PortletPreferences setup=PortletPreferencesFactoryUtil.getPortletSetup(actionRequest,portlet.getPortletId());
  PortletPreferencesServiceUtil.restoreArchivedPreferences(layout.getGroupId(),name,portlet.getRootPortletId(),setup);
}
