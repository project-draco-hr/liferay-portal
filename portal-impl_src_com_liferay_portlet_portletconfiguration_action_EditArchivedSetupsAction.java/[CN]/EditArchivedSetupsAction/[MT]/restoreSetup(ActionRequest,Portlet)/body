{
  Layout layout=(Layout)req.getAttribute(WebKeys.LAYOUT);
  String name=ParamUtil.getString(req,"name");
  PortletPreferences setup=PortletPreferencesFactoryUtil.getPortletSetup(req,portlet.getPortletId());
  PortletPreferencesServiceUtil.restoreArchivedPreferences(layout.getGroupId(),name,portlet.getRootPortletId(),setup);
}
