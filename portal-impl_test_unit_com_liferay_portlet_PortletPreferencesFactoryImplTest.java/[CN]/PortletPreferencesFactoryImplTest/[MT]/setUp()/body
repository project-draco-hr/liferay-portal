  singleVMPoolImpl.setPortalCacheManager(MemoryPortalCacheManager.createMemoryPortalCacheManager(PortletPreferencesFactoryImplTest.class.getName()));
  singleVMPoolUtil.setSingleVMPool(singleVMPoolImpl);
}
