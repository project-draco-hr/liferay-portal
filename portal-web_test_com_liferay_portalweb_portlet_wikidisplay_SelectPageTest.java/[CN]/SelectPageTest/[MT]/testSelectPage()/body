{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Wiki Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.select("_86_title",RuntimeVariables.replace("label=Link Me 1"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have successfully updated the setup."));
  selenium.clickAt("link=Return to Full Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//li[6]/span/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertTrue(selenium.isElementPresent("Link=Link Me 2"));
  assertNotEquals(RuntimeVariables.replace("Link Me 1"),selenium.getText("//p[2]/a"));
  selenium.clickAt("link=Wiki Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (!selenium.isElementPresent("//li[6]/span/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.select("_86_title",RuntimeVariables.replace("label=Link Me 2"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have successfully updated the setup."));
  selenium.clickAt("link=Return to Full Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//li[6]/span/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertTrue(selenium.isElementPresent("Link=Link Me 1"));
  assertNotEquals(RuntimeVariables.replace("Link Me 2"),selenium.getText("//p[3]/a"));
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (!selenium.isElementPresent("//li[6]/span/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.select("_86_title",RuntimeVariables.replace("label=FrontPage"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have successfully updated the setup."));
  selenium.clickAt("link=Return to Full Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
}
