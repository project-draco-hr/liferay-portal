{
  if (_persistence == null) {
    _persistence=(KaleoInstancePersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoInstancePersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoInstanceUtil.class,"_persistence");
  }
  return _persistence;
}
