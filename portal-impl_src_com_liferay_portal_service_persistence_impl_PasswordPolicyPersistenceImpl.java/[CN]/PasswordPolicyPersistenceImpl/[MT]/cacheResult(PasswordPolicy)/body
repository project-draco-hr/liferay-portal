{
  entityCache.putResult(PasswordPolicyModelImpl.ENTITY_CACHE_ENABLED,PasswordPolicyImpl.class,passwordPolicy.getPrimaryKey(),passwordPolicy);
  finderCache.putResult(FINDER_PATH_FETCH_BY_C_DP,new Object[]{passwordPolicy.getCompanyId(),passwordPolicy.getDefaultPolicy()},passwordPolicy);
  finderCache.putResult(FINDER_PATH_FETCH_BY_C_N,new Object[]{passwordPolicy.getCompanyId(),passwordPolicy.getName()},passwordPolicy);
  passwordPolicy.resetOriginalValues();
}
