{
  for (  PasswordPolicy passwordPolicy : passwordPolicies) {
    if (EntityCacheUtil.getResult(PasswordPolicyModelImpl.ENTITY_CACHE_ENABLED,PasswordPolicyImpl.class,passwordPolicy.getPrimaryKey()) == null) {
      cacheResult(passwordPolicy);
    }
 else {
      passwordPolicy.resetOriginalValues();
    }
  }
}
