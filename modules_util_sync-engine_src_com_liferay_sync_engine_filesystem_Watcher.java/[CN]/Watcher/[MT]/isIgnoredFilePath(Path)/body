{
  if (Files.notExists(filePath)) {
    return true;
  }
  String fileName=String.valueOf(filePath.getFileName());
  if (FileUtil.isIgnoredFilePath(filePath) || ((Files.isDirectory(filePath) && (fileName.length() > 100)) || (!Files.isDirectory(filePath) && (fileName.length() > 255)))) {
    if (_logger.isDebugEnabled()) {
      _logger.debug("Ignored file path {}",filePath);
    }
    return true;
  }
  if (!OSDetector.isWindows()) {
    String sanitizedFileName=FileUtil.getSanitizedFileName(fileName,FilenameUtils.getExtension(fileName));
    if (!sanitizedFileName.equals(fileName)) {
      String sanitizedFilePathName=FileUtil.getFilePathName(String.valueOf(filePath.getParent()),sanitizedFileName);
      sanitizedFilePathName=FileUtil.getNextFilePathName(sanitizedFilePathName);
      FileUtil.moveFile(filePath,java.nio.file.Paths.get(sanitizedFilePathName));
      return true;
    }
  }
  return false;
}
