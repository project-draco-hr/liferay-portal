{
  if (Files.notExists(filePath)) {
    return true;
  }
  String fileName=String.valueOf(filePath.getFileName());
  if (FileUtil.isIgnoredFilePath(filePath) || ((Files.isDirectory(filePath) && (fileName.length() > 100)) || (!Files.isDirectory(filePath) && (fileName.length() > 255)))) {
    if (_logger.isDebugEnabled()) {
      _logger.debug("Ignored file path {}",filePath);
    }
    return true;
  }
  if (!OSDetector.isWindows()) {
    String trimmedFilePathName=StringUtils.stripEnd(filePath.toString()," ");
    if (!trimmedFilePathName.equals(filePath.toString())) {
      Path trimmedFilePath=java.nio.file.Paths.get(trimmedFilePathName);
      FileUtil.moveFile(filePath,trimmedFilePath);
      return true;
    }
  }
  return false;
}
