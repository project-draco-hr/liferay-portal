{
  if (index < watchEvents.size()) {
    WatchEvent<Path> nextWatchEvent=(WatchEvent<Path>)watchEvents.get(index);
    if ((nextWatchEvent != null) && ((WatchEvent.Kind<?>)nextWatchEvent.kind() == StandardWatchEventKind.ENTRY_MODIFY)) {
      PathImpl nextPathImpl=(PathImpl)nextWatchEvent.context();
      if (nextPathImpl != null) {
        Path nextFilePath=parentFilePath.resolve(nextPathImpl.toString());
        if (childFilePath.equals(nextFilePath)) {
          return true;
        }
      }
    }
  }
  return false;
}
