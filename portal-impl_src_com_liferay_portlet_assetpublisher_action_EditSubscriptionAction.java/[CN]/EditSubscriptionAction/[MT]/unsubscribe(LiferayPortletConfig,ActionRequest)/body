{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletPreferences portletPreferences=PortletPreferencesLocalServiceUtil.getPortletPreferences(PortletKeys.PREFS_OWNER_ID_DEFAULT,PortletKeys.PREFS_OWNER_TYPE_LAYOUT,themeDisplay.getPlid(),liferayPortletConfig.getPortletId());
  AssetPublisherUtil.unsubscribe(themeDisplay.getUserId(),themeDisplay.getScopeGroupId(),portletPreferences.getPortletPreferencesId(),themeDisplay.getPermissionChecker());
}
