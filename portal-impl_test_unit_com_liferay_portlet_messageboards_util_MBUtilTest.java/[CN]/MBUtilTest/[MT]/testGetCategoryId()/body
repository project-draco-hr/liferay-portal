{
  ReflectionTestUtil.setFieldValue(PropsValues.class,"POP_SERVER_SUBDOMAIN",StringPool.BLANK);
  Assert.assertEquals(10640,MBUtil.getCategoryId("<mb_message.10640.20646.1425017183884@gmail.com>"));
  ReflectionTestUtil.setFieldValue(PropsValues.class,"POP_SERVER_SUBDOMAIN","events");
  Assert.assertEquals(10640,MBUtil.getCategoryId("mb_message.10640.20646.1425017183884@event.gmail.com"));
}
