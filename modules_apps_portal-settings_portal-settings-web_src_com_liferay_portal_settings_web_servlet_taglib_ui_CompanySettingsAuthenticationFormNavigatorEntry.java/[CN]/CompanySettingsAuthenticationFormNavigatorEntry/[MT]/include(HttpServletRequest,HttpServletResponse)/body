{
  request.setAttribute(PortalSettingsWebKeys.AUTHENTICATION_DYNAMIC_INCLUDES,_dynamicIncludes.values());
  Set<String> tabsNamesSet=_dynamicIncludes.keySet();
  String[] tabsNamesArray=tabsNamesSet.toArray(new String[tabsNamesSet.size()]);
  String tabsNames=StringUtil.merge(tabsNamesArray);
  request.setAttribute(PortalSettingsWebKeys.AUTHENTICATION_TABS_NAMES,tabsNames);
  super.include(request,response);
}
