{
  request.setAttribute(PortalSettingsWebKeys.AUTHENTICATION_DYNAMIC_INCLUDES,_tabs.values());
  String[] tabNamesArray=_tabs.keySet().toArray(new String[_tabs.size()]);
  String tabNames=StringUtil.merge(tabNamesArray);
  request.setAttribute(PortalSettingsWebKeys.AUTHENTICATION_TAB_NAMES,tabNames);
  super.include(request,response);
}
