{
  List<Method> emptyMethods=new ArrayList<Method>();
  for (  Method method : ReflectionUtil.getVisibleMethods(clazz)) {
    if (Modifier.isAbstract(method.getModifiers()) && (method.getAnnotation(Id.class) == null) && (method.getAnnotation(Proxy.class) == null)) {
      emptyMethods.add(method);
    }
  }
  return emptyMethods;
}
