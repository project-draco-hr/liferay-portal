{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ExpandoColumn.class,ExpandoColumn.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("columnId",ServiceTestUtil.nextLong()));
  List<ExpandoColumn> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
