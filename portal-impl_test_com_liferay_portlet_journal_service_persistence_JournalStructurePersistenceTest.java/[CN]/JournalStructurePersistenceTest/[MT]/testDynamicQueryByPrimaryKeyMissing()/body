{
  JournalStructure newJournalStructure=addJournalStructure();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalStructure.class,JournalStructure.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id",nextLong()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
