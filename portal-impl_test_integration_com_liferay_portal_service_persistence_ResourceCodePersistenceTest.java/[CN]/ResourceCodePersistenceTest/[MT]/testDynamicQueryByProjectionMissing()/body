{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ResourceCode.class,ResourceCode.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("codeId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("codeId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
