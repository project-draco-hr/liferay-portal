{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ResourceCode.class,ResourceCode.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("codeId",ServiceTestUtil.nextLong()));
  List<ResourceCode> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
