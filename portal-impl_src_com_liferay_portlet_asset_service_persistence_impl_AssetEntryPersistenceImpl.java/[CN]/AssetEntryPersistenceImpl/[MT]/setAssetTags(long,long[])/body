{
  Set<Long> newAssetTagPKsSet=SetUtil.fromArray(assetTagPKs);
  Set<Long> oldAssetTagPKsSet=SetUtil.fromArray(assetEntryToAssetTagTableMapper.getRightPrimaryKeys(companyProvider.getCompanyId(),pk));
  Set<Long> removeAssetTagPKsSet=new HashSet<Long>(oldAssetTagPKsSet);
  removeAssetTagPKsSet.removeAll(newAssetTagPKsSet);
  for (  long removeAssetTagPK : removeAssetTagPKsSet) {
    assetEntryToAssetTagTableMapper.deleteTableMapping(companyProvider.getCompanyId(),pk,removeAssetTagPK);
  }
  newAssetTagPKsSet.removeAll(oldAssetTagPKsSet);
  for (  long newAssetTagPK : newAssetTagPKsSet) {
    assetEntryToAssetTagTableMapper.addTableMapping(companyProvider.getCompanyId(),pk,newAssetTagPK);
  }
}
