{
  List<ExpandoColumn> columns=expandoColumnLocalService.getColumns(companyId,className,tableName,columnNames);
  Map<String,Serializable> attributeValues=new HashMap<String,Serializable>((int)(columnNames.size() * 1.4));
  ExpandoValue value=new ExpandoValueImpl();
  for (  ExpandoColumn column : columns) {
    value.setColumn(column);
    value.setData(column.getDefaultData());
    Serializable attributeValue=doGetData(companyId,className,tableName,column.getName(),classPK,value,column.getType());
    attributeValues.put(column.getName(),attributeValue);
  }
  return attributeValues;
}
