{
  String namespace=PortalUtil.getPortletNamespace(PortletKeys.PORTLET_CONFIGURATION);
  String portletResource=ParamUtil.getString(this,namespace + "portletResource");
  if (Validator.isNull(portletResource)) {
    return false;
  }
  ThemeDisplay themeDisplay=(ThemeDisplay)this.getAttribute(WebKeys.THEME_DISPLAY);
  Portlet portlet=null;
  try {
    portlet=PortletLocalServiceUtil.getPortletById(themeDisplay.getCompanyId(),portletResource);
  }
 catch (  Exception e) {
  }
  if (portlet == null) {
    return false;
  }
  PortletApp portletApp=portlet.getPortletApp();
  if (portletApp.isWARFile()) {
    return true;
  }
  return false;
}
