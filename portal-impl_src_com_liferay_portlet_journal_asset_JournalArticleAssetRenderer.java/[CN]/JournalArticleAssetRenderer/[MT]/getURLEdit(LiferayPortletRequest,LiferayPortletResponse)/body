{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),PortletKeys.JOURNAL,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/html/portlet/journal/edit_article.jsp");
  portletURL.setParameter("groupId",String.valueOf(_article.getGroupId()));
  portletURL.setParameter("articleId",_article.getArticleId());
  portletURL.setParameter("version",String.valueOf(_article.getVersion()));
  return portletURL;
}
