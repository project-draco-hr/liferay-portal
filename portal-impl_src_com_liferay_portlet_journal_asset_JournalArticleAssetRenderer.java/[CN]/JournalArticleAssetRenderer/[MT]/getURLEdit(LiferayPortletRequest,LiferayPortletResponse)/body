{
  ThemeDisplay themeDisplay=(ThemeDisplay)liferayPortletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletURL editPortletURL=liferayPortletResponse.createRenderURL(PortletKeys.JOURNAL);
  editPortletURL.setParameter("struts_action","/journal/edit_article");
  editPortletURL.setParameter("groupId",String.valueOf(_article.getGroupId()));
  editPortletURL.setParameter("articleId",_article.getArticleId());
  editPortletURL.setParameter("version",String.valueOf(_article.getVersion()));
  return editPortletURL;
}
