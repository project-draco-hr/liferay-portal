{
synchronized (_sessions) {
    Set portletSessions=(Set)_sessions.get(sessionId);
    if (portletSessions != null) {
      Iterator itr=portletSessions.iterator();
      while (itr.hasNext()) {
        HttpSession session=(HttpSession)itr.next();
        try {
          session.invalidate();
        }
 catch (        Exception e) {
        }
      }
    }
    _sessions.remove(sessionId);
  }
}
