{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(liferayPortletRequest,BookmarksPortletKeys.BOOKMARKS,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcRenderCommandName","/bookmarks/edit_entry");
  portletURL.setParameter("folderId",String.valueOf(BookmarksFolderConstants.DEFAULT_PARENT_FOLDER_ID));
  portletURL.setParameter("showFolderSelector",Boolean.TRUE.toString());
  return portletURL;
}
