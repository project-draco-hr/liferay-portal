{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/socialoffice01/so/profile");
  selenium.waitForVisible("//div[@class='lfr-contact-name']/a");
  assertEquals(RuntimeVariables.replace("Social01 Office01 User01"),selenium.getText("//div[@class='lfr-contact-name']/a"));
  assertEquals(RuntimeVariables.replace("To complete your profile, please add:"),selenium.getText("//p[@class='portlet-msg portlet-msg-info']"));
  assertEquals(RuntimeVariables.replace("Introduction"),selenium.getText("//li[@data-title='Introduction']"));
  selenium.clickAt("//li[@data-title='Introduction']",RuntimeVariables.replace("Introduction"));
  selenium.waitForVisible("//textarea[contains(@id,'comments')]");
  selenium.type("//textarea[contains(@id,'comments')]",RuntimeVariables.replace("Introduction Content"));
  Thread.sleep(5000);
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForVisible("//div[@data-title='Introduction']/h3");
  assertEquals(RuntimeVariables.replace("Introduction:"),selenium.getText("//div[@data-title='Introduction']/h3"));
  assertEquals(RuntimeVariables.replace("Introduction Content"),selenium.getText("//div[@data-title='Introduction']/ul/li/span"));
}
