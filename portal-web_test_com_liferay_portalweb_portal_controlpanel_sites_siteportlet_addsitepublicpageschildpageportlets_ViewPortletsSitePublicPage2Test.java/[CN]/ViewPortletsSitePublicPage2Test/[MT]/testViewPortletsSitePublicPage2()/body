{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/site-name/public-page2");
  selenium.waitForVisible("xPath=(//span[@class='portlet-title-text'])[1]");
  assertEquals(RuntimeVariables.replace("Asset Publisher"),selenium.getText("xPath=(//span[@class='portlet-title-text'])[1]"));
  selenium.waitForVisible("xPath=(//span[@class='portlet-title-text'])[2]");
  selenium.waitForVisible("xPath=(//span[@class='portlet-title-text'])[3]");
}
