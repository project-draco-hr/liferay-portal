{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select count(*) from JournalArticle where type_ != 'general'");
    rs=ps.executeQuery();
    while (rs.next()) {
      int count=rs.getInt(1);
      if (count > 0) {
        return true;
      }
    }
    return false;
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
