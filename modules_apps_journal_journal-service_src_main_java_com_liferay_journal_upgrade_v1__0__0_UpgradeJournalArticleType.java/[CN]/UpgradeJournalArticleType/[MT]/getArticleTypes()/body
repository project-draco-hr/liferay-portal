{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select distinct type_ from JournalArticle");
    rs=ps.executeQuery();
    List<String> types=new ArrayList<>();
    while (rs.next()) {
      types.add(rs.getString("type_"));
    }
    return types;
  }
  finally {
    DataAccess.cleanUp(null,ps,rs);
  }
}
