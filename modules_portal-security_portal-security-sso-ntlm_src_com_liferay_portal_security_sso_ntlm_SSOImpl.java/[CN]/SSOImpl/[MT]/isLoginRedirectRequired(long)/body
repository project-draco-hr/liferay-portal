{
  try {
    NtlmConfiguration ntlmConfiguration=_moduleConfigurationFactory.getConfiguration(NtlmConfiguration.class,new CompanyServiceSettingsLocator(companyId,NtlmConstants.SERVICE_NAME));
    return ntlmConfiguration.enabled();
  }
 catch (  SettingsException se) {
    _log.error("Unable to get NTLM configuration",se);
  }
  return false;
}
