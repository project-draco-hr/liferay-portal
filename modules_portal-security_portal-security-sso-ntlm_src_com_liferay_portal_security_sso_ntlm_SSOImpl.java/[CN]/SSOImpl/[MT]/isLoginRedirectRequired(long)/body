{
  try {
    NtlmConfiguration ntlmConfiguration=_configurationFactory.getConfiguration(NtlmConfiguration.class,new CompanyServiceSettingsLocator(companyId,NtlmConstants.SERVICE_NAME));
    return ntlmConfiguration.enabled();
  }
 catch (  ConfigurationException mce) {
    _log.error("Unable to get NTLM configuration",mce);
  }
  return false;
}
