{
  if (((languageId == null) && (_languageId != null)) || ((languageId != null) && (_languageId == null)) || ((languageId != null) && (_languageId != null) && !languageId.equals(_languageId))) {
    if (!XSS_ALLOW_LANGUAGEID) {
      languageId=XSSUtil.strip(languageId);
    }
    _languageId=languageId;
    setModified(true);
  }
}
