{
  try {
    com.liferay.portlet.messageboards.model.MBMessage returnValue=MBMessageServiceUtil.getMessage(topicId,messageId);
    return returnValue;
  }
 catch (  Exception e) {
    String stackTrace=StackTraceUtil.getStackTrace(e);
