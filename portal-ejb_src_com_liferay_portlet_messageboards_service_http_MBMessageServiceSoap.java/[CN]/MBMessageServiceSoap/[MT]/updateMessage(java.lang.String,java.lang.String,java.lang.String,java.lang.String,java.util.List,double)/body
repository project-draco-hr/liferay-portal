{
  try {
    com.liferay.portlet.messageboards.model.MBMessage returnValue=MBMessageServiceUtil.updateMessage(messageId,categoryId,subject,body,files,priority);
    return com.liferay.portlet.messageboards.model.MBMessageSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
