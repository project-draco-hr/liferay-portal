{
  try {
    com.liferay.portlet.messageboards.model.MBMessage returnValue=MBMessageServiceUtil.addMessage(categoryId,threadId,parentMessageId,subject,body,files,anonymous,priority,communityPermissions,guestPermissions);
    return com.liferay.portlet.messageboards.model.MBMessageSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    String stackTrace=StackTraceUtil.getStackTrace(e);
    _log.error(stackTrace);
    throw new RemoteException(stackTrace);
  }
}
