{
  String portletCSS=portletPreferences.getValue("portlet-setup-css",null);
  if (Validator.isNotNull(portletCSS)) {
    portletPreferences.reset("portlet-setup-css");
  }
  if (portletId.startsWith("62_INSTANCE_") || portletId.startsWith("56_INSTANCE_")) {
    String groupId=portletPreferences.getValue("group-id",null);
    if (Validator.isNotNull(groupId)) {
      groupId=String.valueOf(_groupIdMapper.getNewValue(new Long(GetterUtil.getLong(groupId))));
      portletPreferences.setValue("group-id",groupId);
    }
  }
 else   if (portletId.startsWith("59_INSTANCE_")) {
    String questionId=portletPreferences.getValue("question-id",null);
    if (Validator.isNotNull(questionId)) {
      questionId=String.valueOf(_pollsQuestionIdMapper.getNewValue(new Long(GetterUtil.getLong(questionId))));
      portletPreferences.setValue("question-id",questionId);
    }
  }
 else   if (portletId.startsWith("54_INSTANCE_")) {
    String nodeId=portletPreferences.getValue("node-id",null);
    if (Validator.isNotNull(nodeId)) {
      nodeId=String.valueOf(_wikiNodeIdMapper.getNewValue(new Long(GetterUtil.getLong(nodeId))));
      portletPreferences.setValue("node-id",nodeId);
    }
  }
}
