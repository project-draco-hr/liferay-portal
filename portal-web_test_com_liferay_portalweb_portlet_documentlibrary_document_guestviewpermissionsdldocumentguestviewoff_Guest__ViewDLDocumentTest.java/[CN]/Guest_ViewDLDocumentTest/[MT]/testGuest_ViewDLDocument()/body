{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Document Library Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Document Library Test Page",RuntimeVariables.replace("Document Library Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("DL Document Title"),selenium.getText("//span[@class='document-title']"));
  selenium.clickAt("//span[@class='document-title']",RuntimeVariables.replace("DL Document Title"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("DL Document Title"),selenium.getText("//div[@class='document-info']/h2"));
  assertEquals(RuntimeVariables.replace("DL Document Description"),selenium.getText("//span[@class='document-description']"));
  assertEquals(RuntimeVariables.replace("Download (0.5k)"),selenium.getText("//span[@class='download-document']/span/a/span"));
}
