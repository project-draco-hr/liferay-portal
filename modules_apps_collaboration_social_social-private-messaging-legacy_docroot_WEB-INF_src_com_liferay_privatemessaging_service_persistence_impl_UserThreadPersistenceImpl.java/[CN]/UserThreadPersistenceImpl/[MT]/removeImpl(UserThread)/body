{
  userThread=toUnwrappedModel(userThread);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(userThread)) {
      userThread=(UserThread)session.get(UserThreadImpl.class,userThread.getPrimaryKeyObj());
    }
    if (userThread != null) {
      session.delete(userThread);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (userThread != null) {
    clearCache(userThread);
  }
  return userThread;
}
