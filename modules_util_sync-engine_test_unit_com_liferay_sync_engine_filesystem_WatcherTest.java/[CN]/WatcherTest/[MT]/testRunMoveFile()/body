{
  setResponse("dependencies/watcher_test_move_file.json");
  Path sourceFilePath=Paths.get(FileUtil.getFilePathName(_syncSite.getFilePathName(),"test.txt"));
  Files.createFile(sourceFilePath);
  Path targetFilePath=Paths.get(FileUtil.getFilePathName(_syncSite.getFilePathName(),"test"));
  Files.createDirectory(targetFilePath);
  sleep();
  Files.move(sourceFilePath,targetFilePath.resolve(sourceFilePath.getFileName()));
  sleep();
  _syncFiles=SyncFileService.findSyncFiles(syncAccount.getSyncAccountId());
  Assert.assertEquals(4,_syncFiles.size());
  Assert.assertNotNull(SyncFileService.fetchSyncFile(targetFilePath.toString()));
}
