{
  Folder folder=null;
  if (getFolderId() > 0) {
    try {
      folder=DLAppLocalServiceUtil.getFolder(getFolderId());
    }
 catch (    Exception e) {
      folder=new LiferayFolder(new DLFolderImpl());
      _log.error(e);
    }
  }
 else {
    folder=new LiferayFolder(new DLFolderImpl());
  }
  return folder;
}
