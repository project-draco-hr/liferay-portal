{
  Map<Locale,String> nameMap=new HashMap<Locale,String>();
  nameMap.put(defaultLocale,name);
  return DDLRecordSetLocalServiceUtil.addRecordSet(TestPropsValues.getUserId(),groupId,structureId,null,nameMap,null,DDLRecordSetConstants.MIN_DISPLAY_ROWS_DEFAULT,DDLRecordSetConstants.SCOPE_DYNAMIC_DATA_LISTS,serviceContext);
}
