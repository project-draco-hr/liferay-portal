{
  EntityCacheUtil.putResult(DDMTemplateVersionModelImpl.ENTITY_CACHE_ENABLED,DDMTemplateVersionImpl.class,ddmTemplateVersion.getPrimaryKey(),ddmTemplateVersion);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_T_V,new Object[]{ddmTemplateVersion.getTemplateId(),ddmTemplateVersion.getVersion()},ddmTemplateVersion);
  ddmTemplateVersion.resetOriginalValues();
}
