{
  int firstIndex=_pathBinarySearch.findFirst(path);
  if (firstIndex < 0) {
    return -1;
  }
  int lastIndex=_pathBinarySearch.findLast(path,firstIndex);
  if (lastIndex < 0) {
    lastIndex=firstIndex;
  }
  int index=-1;
  int max=-1;
  for (int i=firstIndex; i <= lastIndex; i++) {
    RESTActionConfig restActionConfig=_restActionConfig.get(i);
    String restActionConfigMethod=restActionConfig.getMethod();
    if (method != null) {
      if ((restActionConfigMethod != null) && !restActionConfigMethod.equals(method)) {
        continue;
      }
    }
    String[] restActionConfigParameterNames=restActionConfig.getParameterNames();
    int count=_countMatchedElements(parameterNames,restActionConfigParameterNames);
    if (count > max) {
      max=count;
      index=i;
    }
  }
  return index;
}
