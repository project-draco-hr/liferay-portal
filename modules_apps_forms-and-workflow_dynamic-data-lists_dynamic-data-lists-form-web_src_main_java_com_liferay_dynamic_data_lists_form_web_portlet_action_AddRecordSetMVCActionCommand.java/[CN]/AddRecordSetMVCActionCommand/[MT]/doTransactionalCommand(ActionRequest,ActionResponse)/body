{
  DDMFormValues settingsDDMFormValues=getSettingsDDMFormValues(actionRequest);
  DDMStructure ddmStructure=addDDMStructure(actionRequest,settingsDDMFormValues);
  DDLRecordSet recordSet=addRecordSet(actionRequest,ddmStructure.getStructureId());
  updateRecordSetSettings(actionRequest,recordSet,settingsDDMFormValues);
}
