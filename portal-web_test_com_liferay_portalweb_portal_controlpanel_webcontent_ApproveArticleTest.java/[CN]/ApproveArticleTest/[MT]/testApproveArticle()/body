{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Web Content",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=1.1",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(5000);
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//input[@value='Publish']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//input[@value='Publish']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isElementPresent("link=Draft"));
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
}
