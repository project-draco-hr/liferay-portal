{
  if (_byKey) {
    String key1=kvp1.getKey();
    String key2=kvp2.getKey();
    if (_ascending) {
      return key1.compareTo(key2);
    }
 else {
      return -(key1.compareTo(key2));
    }
  }
 else {
    String value1=kvp1.getValue();
    String value2=kvp2.getValue();
    if (_ascending) {
      return value1.compareTo(value2);
    }
 else {
      return -(value1.compareTo(value2));
    }
  }
}
