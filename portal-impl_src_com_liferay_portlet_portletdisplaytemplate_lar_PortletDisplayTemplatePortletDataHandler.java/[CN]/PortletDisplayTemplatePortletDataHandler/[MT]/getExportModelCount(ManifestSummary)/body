{
  long totalModelCount=-1;
  for (  StagedModelType modelType : modelTypes) {
    long modelCount=manifestSummary.getModelAdditionCount(modelType.getClassName(),modelType.getReferrerClassName());
    if (modelCount == -1) {
      continue;
    }
    if (totalModelCount == -1) {
      totalModelCount=modelCount;
    }
 else {
      totalModelCount+=modelCount;
    }
  }
  return totalModelCount;
}
