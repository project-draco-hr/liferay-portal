{
  for (  long classNameId : TemplateHandlerRegistryUtil.getClassNameIds()) {
    ActionableDynamicQuery ddmTemplateActionableDynamicQuery=getDDMTemplateActionableDynamicQuery(portletDataContext,new Long[]{classNameId},PortalUtil.getClassName(classNameId));
    ddmTemplateActionableDynamicQuery.performCount();
  }
}
