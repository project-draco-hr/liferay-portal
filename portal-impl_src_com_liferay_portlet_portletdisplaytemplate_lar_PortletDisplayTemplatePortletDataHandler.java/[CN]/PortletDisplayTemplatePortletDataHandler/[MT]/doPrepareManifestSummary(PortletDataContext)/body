{
  for (  long classNameId : TemplateHandlerRegistryUtil.getClassNameIds()) {
    ActionableDynamicQuery actionableDynamicQuery=getDDMTemplateActionableDynamicQuery(portletDataContext,new Long[]{classNameId},ManifestSummary.getManifestSummaryKey(DDMTemplate.class.getName(),PortalUtil.getClassName(classNameId)));
    actionableDynamicQuery.performCount();
  }
}
