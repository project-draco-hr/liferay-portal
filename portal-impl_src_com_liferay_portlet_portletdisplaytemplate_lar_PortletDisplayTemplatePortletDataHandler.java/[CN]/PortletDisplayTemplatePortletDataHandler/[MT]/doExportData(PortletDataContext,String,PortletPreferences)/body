{
  Element rootElement=addExportDataRootElement(portletDataContext);
  long[] classNameIds=TemplateHandlerRegistryUtil.getClassNameIds();
  for (  long classNameId : classNameIds) {
    List<DDMTemplate> ddmTemplates=DDMTemplateLocalServiceUtil.getTemplates(portletDataContext.getScopeGroupId(),classNameId);
    for (    DDMTemplate ddmTemplate : ddmTemplates) {
      StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,ddmTemplate);
    }
  }
  return getExportDataRootElementString(rootElement);
}
