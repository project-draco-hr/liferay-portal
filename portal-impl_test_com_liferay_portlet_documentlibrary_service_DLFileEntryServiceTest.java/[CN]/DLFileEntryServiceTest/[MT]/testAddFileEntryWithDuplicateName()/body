{
  String fileName="helloworld.txt";
  String description=StringPool.BLANK;
  String extraSettings=StringPool.BLANK;
  String content="Hello World!";
  byte[] bytes=content.getBytes();
  Group group=GroupLocalServiceUtil.getGroup(TestPropsValues.COMPANY_ID,GroupConstants.GUEST);
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddCommunityPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  serviceContext.setScopeGroupId(group.getGroupId());
  DLFileEntryServiceUtil.addFileEntry(_folder.getFolderId(),fileName,fileName,description,extraSettings,bytes,serviceContext);
  try {
    DLFileEntryServiceUtil.addFileEntry(_folder.getFolderId(),fileName,fileName,description,extraSettings,bytes,serviceContext);
    fail("Able to add two files of the name " + fileName);
  }
 catch (  DuplicateFileException dfe) {
