{
  long groupId=_folder.getGroupId();
  long folderId=DLFolderConstants.DEFAULT_PARENT_FOLDER_ID;
  if (!rootFolder) {
    folderId=_folder.getFolderId();
  }
  String fileName="Title.txt";
  String description=StringPool.BLANK;
  String versionDescription=StringPool.BLANK;
  String extraSettings=StringPool.BLANK;
  String content="Content: Enterprise. Open Source. For Life.";
  byte[] bytes=content.getBytes();
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddCommunityPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  _fileEntry=DLFileEntryServiceUtil.addFileEntry(groupId,folderId,fileName,fileName,description,versionDescription,extraSettings,bytes,serviceContext);
}
