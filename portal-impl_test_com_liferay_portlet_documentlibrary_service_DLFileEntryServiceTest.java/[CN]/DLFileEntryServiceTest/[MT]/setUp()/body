{
  super.setUp();
  long groupId=PortalUtil.getScopeGroupId(TestPropsValues.LAYOUT_PLID);
  String name="Test Folder";
  String description="This is a test folder.";
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddCommunityPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  try {
    DLFolderServiceUtil.deleteFolder(groupId,DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,name);
  }
 catch (  NoSuchFolderException nsfe) {
  }
  _folder=DLFolderServiceUtil.addFolder(groupId,DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,name,description,serviceContext);
}
