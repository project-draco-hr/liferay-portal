  }
 catch (  NoSuchFolderException nsfe) {
  }
  _folder=DLFolderServiceUtil.addFolder(TestPropsValues.LAYOUT_PLID,DLFolderImpl.DEFAULT_PARENT_FOLDER_ID,name,description,addCommunityPermissions,addGuestPermissions);
}
