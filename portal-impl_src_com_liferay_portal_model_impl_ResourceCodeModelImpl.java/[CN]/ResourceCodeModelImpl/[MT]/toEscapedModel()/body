{
  if (isEscapedModel()) {
    return (ResourceCode)this;
  }
 else {
    ResourceCode model=new ResourceCodeImpl();
    model.setNew(isNew());
    model.setEscapedModel(true);
    model.setCodeId(getCodeId());
    model.setCompanyId(getCompanyId());
    model.setName(HtmlUtil.escape(getName()));
    model.setScope(getScope());
    model=(ResourceCode)Proxy.newProxyInstance(ResourceCode.class.getClassLoader(),new Class[]{ResourceCode.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
