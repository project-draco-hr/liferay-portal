{
  value=RuntimeVariables.replace(value);
  for (int second=0; ; second++) {
    if (second >= TestPropsValues.TIMEOUT_EXPLICIT_WAIT) {
      BaseTestCase.fail("Timeout");
    }
    try {
      if (!value.equals(liferaySelenium.getText(locator))) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
