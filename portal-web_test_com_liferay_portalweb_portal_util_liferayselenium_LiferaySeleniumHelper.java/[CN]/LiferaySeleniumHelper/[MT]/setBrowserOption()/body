{
  if (!_BROWSER_TYPE.equals("*chrome") && !_BROWSER_TYPE.equals("*firefox")) {
    return;
  }
  try {
    String[] commands={RuntimeVariables.replace(_SELENIUM_EXECUTABLE_AUTOIT_DIR + TestPropsValues.SELENIUM_SET_BROWSER_OPTION),TestPropsValues.OUTPUT_DIR};
    Runtime runtime=Runtime.getRuntime();
    runtime.exec(commands);
    Thread.sleep(10000);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
