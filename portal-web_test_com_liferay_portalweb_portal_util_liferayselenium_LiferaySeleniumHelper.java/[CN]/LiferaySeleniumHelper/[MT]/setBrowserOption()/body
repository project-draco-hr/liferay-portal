{
  if (!_BROWSER_TYPE.equals("*chrome") && !_BROWSER_TYPE.equals("*firefox")) {
    return;
  }
  try {
    String command=RuntimeVariables.replace(_SELENIUM_EXECUTABLE_DIR + TestPropsValues.SELENIUM_SET_BROWSER_OPTION);
    Runtime runtime=Runtime.getRuntime();
    runtime.exec(command);
    Thread.sleep(10000);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
