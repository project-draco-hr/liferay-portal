{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLSync.class,DLSync.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("syncId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("syncId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
