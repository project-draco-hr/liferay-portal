{
  if (s == null) {
    return null;
  }
  if (oldSubs == null) {
    return s;
  }
  StringBuilder sb=new StringBuilder(s.length());
  iterate:   for (int i=0; i < s.length(); i++) {
    char c=s.charAt(i);
    for (int j=0; j < oldSubs.length; j++) {
      if (c == oldSubs[j]) {
        continue iterate;
      }
    }
    sb.append(c);
  }
  if (s.length() == sb.length()) {
    return s;
  }
  return sb.toString();
}
