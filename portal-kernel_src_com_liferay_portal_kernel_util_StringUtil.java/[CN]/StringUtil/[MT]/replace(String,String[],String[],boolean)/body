{
  if ((s == null) || (oldSubs == null) || (newSubs == null)) {
    return null;
  }
  if (oldSubs.length != newSubs.length) {
    return s;
  }
  if (!exactMatch) {
    replace(s,oldSubs,newSubs);
  }
 else {
    for (int i=0; i < oldSubs.length; i++) {
      s=s.replaceAll("\\b" + oldSubs[i] + "\\b",newSubs[i]);
    }
  }
  return s;
}
