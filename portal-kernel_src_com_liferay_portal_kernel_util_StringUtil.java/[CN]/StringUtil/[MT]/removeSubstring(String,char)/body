{
  if (s == null) {
    return null;
  }
  int y=s.indexOf(oldSub);
  if (y >= 0) {
    StringBundler sb=new StringBundler();
    int x=0;
    while (x <= y) {
      sb.append(s.substring(x,y));
      x=y + 1;
      y=s.indexOf(oldSub,x);
    }
    sb.append(s.substring(x));
    return sb.toString();
  }
 else {
    return s;
  }
}
