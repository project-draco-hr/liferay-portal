{
  if ((s == null) || (suffix == null)) {
    return null;
  }
  if (s.length() > length) {
    for (int j=length; j >= 0; j--) {
      if (Character.isWhitespace(s.charAt(j))) {
        length=j;
        break;
      }
    }
    StringBuilder sb=new StringBuilder();
    sb.append(s.substring(0,length));
    sb.append(suffix);
    s=sb.toString();
  }
  return s;
}
