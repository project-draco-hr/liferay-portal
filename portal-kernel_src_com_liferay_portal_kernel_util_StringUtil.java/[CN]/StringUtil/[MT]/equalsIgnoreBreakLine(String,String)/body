{
  if (s1 == s2) {
    return true;
  }
  if ((s1 == null) || (s2 == null)) {
    return false;
  }
  s1=replace(s1,new String[]{StringPool.RETURN_NEW_LINE,StringPool.NEW_LINE},new String[]{StringPool.BLANK,StringPool.BLANK});
  s2=replace(s2,new String[]{StringPool.RETURN_NEW_LINE,StringPool.NEW_LINE},new String[]{StringPool.BLANK,StringPool.BLANK});
  if (s1.length() != s2.length()) {
    return false;
  }
  return s1.equals(s2);
}
