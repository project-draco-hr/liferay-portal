{
  if (s1 == s2) {
    return true;
  }
  if ((s1 == null) || (s2 == null)) {
    return false;
  }
  if (s1.length() != s2.length()) {
    return false;
  }
  for (int i=0; i < s1.length(); i++) {
    if (!equalsIgnoreCase(s1.charAt(i),s2.charAt(i))) {
      return false;
    }
  }
  return true;
}
