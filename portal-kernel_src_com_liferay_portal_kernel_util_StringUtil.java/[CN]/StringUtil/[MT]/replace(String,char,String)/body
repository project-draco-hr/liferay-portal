{
  if ((s == null) || (newSub == null)) {
    return null;
  }
  StringBuilder sb=new StringBuilder(s.length() + 5 * newSub.length());
  char[] charArray=s.toCharArray();
  for (  char c : charArray) {
    if (c == oldSub) {
      sb.append(newSub);
    }
 else {
      sb.append(c);
    }
  }
  return sb.toString();
}
