{
  if ((s == null) || (newSub == null)) {
    return null;
  }
  StringBuilder sb=new StringBuilder(s.length() + 5 * newSub.length());
  char[] c=s.toCharArray();
  int length=c.length;
  for (int i=0; i < length; i++) {
    if (c[i] == oldSub) {
      sb.append(newSub);
    }
 else {
      sb.append(c[i]);
    }
  }
  return sb.toString();
}
