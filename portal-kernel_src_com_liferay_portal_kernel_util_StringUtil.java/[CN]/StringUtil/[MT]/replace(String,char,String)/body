{
  if ((s == null) || (newSub == null)) {
    return null;
  }
  StringBundler sb=new StringBundler(s.length() + 5 * newSub.length());
  for (int i=0; i < s.length(); i++) {
    char c=s.charAt(i);
    if (c == oldSub) {
      sb.append(newSub);
    }
 else {
      sb.append(c);
    }
  }
  return sb.toString();
}
