{
  if ((s == null) || (text == null) || (delimiter == null)) {
    return false;
  }
  StringMaker sm=null;
  if (!s.endsWith(delimiter)) {
    sm=new StringMaker();
    sm.append(s);
    sm.append(delimiter);
    s=sm.toString();
  }
  sm=new StringMaker();
  sm.append(delimiter);
  sm.append(text);
  sm.append(delimiter);
  String dtd=sm.toString();
  int pos=s.indexOf(dtd);
  if (pos == -1) {
    sm=new StringMaker();
    sm.append(text);
    sm.append(delimiter);
    String td=sm.toString();
    if (s.startsWith(td)) {
      return true;
    }
    return false;
  }
  return true;
}
