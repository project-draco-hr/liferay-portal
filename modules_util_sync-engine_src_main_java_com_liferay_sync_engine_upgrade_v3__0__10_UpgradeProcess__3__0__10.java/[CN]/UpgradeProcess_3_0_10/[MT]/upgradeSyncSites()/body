{
  List<SyncAccount> syncAccounts=SyncAccountService.findAll();
  for (  SyncAccount syncAccount : syncAccounts) {
    List<SyncSite> syncSites=SyncSiteService.findSyncSites(syncAccount.getSyncAccountId());
    for (    SyncSite syncSite : syncSites) {
      if (syncSite.isActive() && !Files.exists(Paths.get(syncSite.getFilePathName()))) {
        Files.createDirectories(Paths.get(syncSite.getFilePathName()));
      }
      SyncFile syncFile=SyncFileService.fetchSyncFile(syncSite.getFilePathName());
      if (syncFile != null) {
        continue;
      }
      SyncFileService.addSyncFile(null,null,false,null,syncSite.getFilePathName(),null,syncSite.getName(),0,syncSite.getGroupId(),0,SyncFile.STATE_SYNCED,syncSite.getSyncAccountId(),SyncFile.TYPE_SYSTEM);
    }
  }
}
