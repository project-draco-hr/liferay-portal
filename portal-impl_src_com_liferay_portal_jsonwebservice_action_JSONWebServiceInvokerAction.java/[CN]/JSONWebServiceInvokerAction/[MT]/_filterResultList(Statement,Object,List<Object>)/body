{
  List<Object> list=_convertObjectToList(result);
  for (  Object object : list) {
    if (object instanceof List) {
      Object value=_filterResultList(statement,object,results);
      results.add(value);
    }
 else {
      Object value=_filterResult(statement,object);
      results.add(value);
    }
  }
  return results;
}
