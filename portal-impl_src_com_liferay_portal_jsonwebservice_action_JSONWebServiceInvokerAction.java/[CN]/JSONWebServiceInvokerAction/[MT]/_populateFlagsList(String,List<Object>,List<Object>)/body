{
  for (  Object object : list) {
    List<Object> listObject=_convertObjectToList(object);
    if (listObject != null) {
      Object value=_populateFlagsList(name,listObject,results);
      results.add(value);
    }
 else {
      _populateFlagsObject(name,object);
      results.add(object);
    }
  }
  return results;
}
