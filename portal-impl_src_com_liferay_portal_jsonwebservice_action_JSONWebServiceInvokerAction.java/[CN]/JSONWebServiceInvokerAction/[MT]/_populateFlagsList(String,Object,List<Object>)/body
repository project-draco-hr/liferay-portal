{
  List<Object> list=_convertObjectToList(result);
  for (  Object object : list) {
    if (object instanceof List) {
      Object value=_populateFlagsList(name,object,results);
      results.add(value);
    }
 else {
      _populateFlagsObject(name,object);
      results.add(object);
    }
  }
  return results;
}
