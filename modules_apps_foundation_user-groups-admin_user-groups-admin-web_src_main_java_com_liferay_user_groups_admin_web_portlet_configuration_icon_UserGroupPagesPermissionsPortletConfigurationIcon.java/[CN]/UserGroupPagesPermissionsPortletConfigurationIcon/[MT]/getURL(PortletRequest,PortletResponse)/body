{
  String url=StringPool.BLANK;
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    UserGroup userGroup=ActionUtil.getUserGroup(portletRequest);
    url=PermissionsURLTag.doTag(StringPool.BLANK,Group.class.getName(),themeDisplay.getScopeGroupName(),null,String.valueOf(userGroup.getGroup().getGroupId()),LiferayWindowState.POP_UP.toString(),null,themeDisplay.getRequest());
  }
 catch (  Exception e) {
  }
  return url;
}
