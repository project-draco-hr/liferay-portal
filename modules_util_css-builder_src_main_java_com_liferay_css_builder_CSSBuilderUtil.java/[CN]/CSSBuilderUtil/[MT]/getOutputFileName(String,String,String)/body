{
  String cacheFileName=StringUtil.replace(fileName,CharPool.BACK_SLASH,CharPool.SLASH);
  int x=cacheFileName.lastIndexOf(CharPool.SLASH);
  int y=cacheFileName.lastIndexOf(CharPool.PERIOD);
  if (cacheFileName.endsWith(".scss")) {
    cacheFileName=cacheFileName.substring(0,y + 1) + "css";
  }
  return cacheFileName.substring(0,x + 1) + outputDirName + cacheFileName.substring(x + 1,y)+ suffix+ cacheFileName.substring(y);
}
