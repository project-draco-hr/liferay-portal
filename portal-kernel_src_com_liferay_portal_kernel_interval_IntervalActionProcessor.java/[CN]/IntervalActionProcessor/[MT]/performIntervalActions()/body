{
  if (_total == 0) {
    return null;
  }
  int pages=_total / _interval;
  for (int i=0; i <= pages; i++) {
    int end=_start + _interval;
    if (end > _total) {
      end=_total;
    }
    T result=performIntervalActions(_start,end);
    if (result != null) {
      return result;
    }
  }
  return null;
}
