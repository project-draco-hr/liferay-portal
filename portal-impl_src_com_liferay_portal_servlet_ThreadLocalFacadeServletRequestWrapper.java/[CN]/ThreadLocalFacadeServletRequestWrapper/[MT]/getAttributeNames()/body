{
  ServletRequest servletRequest=getRequest();
  Lock lock=(Lock)servletRequest.getAttribute(WebKeys.PARALLEL_RENDERING_MERGE_LOCK);
  if (lock != null) {
    lock.lock();
  }
  try {
    return servletRequest.getAttributeNames();
  }
  finally {
    if (lock != null) {
      lock.unlock();
    }
  }
}
