{
  String displayName=srcFile.getName();
  if (StringUtil.endsWith(displayName,".war") || StringUtil.endsWith(displayName,".xml")) {
    displayName=displayName.substring(0,displayName.length() - 4);
  }
  if ((appServerType.equals(ServerDetector.JBOSS_ID) && Validator.isNotNull(jbossPrefix) && displayName.startsWith(jbossPrefix)) || (appServerType.equals(ServerDetector.WILDFLY_ID) && Validator.isNotNull(wildflyPrefix) && displayName.startsWith(wildflyPrefix))) {
    displayName=displayName.substring(1);
  }
  return displayName;
}
