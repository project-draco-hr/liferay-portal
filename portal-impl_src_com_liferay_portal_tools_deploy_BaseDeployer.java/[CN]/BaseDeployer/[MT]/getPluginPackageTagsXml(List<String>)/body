{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < tags.size(); i++) {
    String tag=tags.get(i);
    if (i == 0) {
      sb.append("\r\n");
    }
    sb.append("\t\t<tag>");
    sb.append(tag);
    sb.append("</tag>\r\n");
    if ((i + 1) == tags.size()) {
      sb.append("\t");
    }
  }
  return sb.toString();
}
