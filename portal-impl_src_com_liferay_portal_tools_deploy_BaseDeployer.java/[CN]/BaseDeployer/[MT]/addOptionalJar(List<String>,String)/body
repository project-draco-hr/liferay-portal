{
  String path=DeployUtil.getResourcePath(resource);
  if (_log.isDebugEnabled()) {
    if (path == null) {
      _log.debug("Resource " + resource + " is not available");
    }
 else {
      _log.debug("Resource " + resource + " is available at "+ path);
    }
  }
  if (path != null) {
    jars.add(path);
  }
}
