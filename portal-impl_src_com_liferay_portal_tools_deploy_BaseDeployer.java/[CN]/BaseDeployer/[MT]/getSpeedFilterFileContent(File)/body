{
  Properties properties=getPluginPackageProperties(srcFile);
  boolean enableSpeedFilter=false;
  if (properties != null) {
    enableSpeedFilter=Boolean.parseBoolean(properties.getProperty("enable.speed.filters"));
  }
  if (enableSpeedFilter) {
    String speedFiltersContent=FileUtil.read(DeployUtil.getResourcePath("speed_filters.xml"));
    return speedFiltersContent;
  }
  return StringPool.BLANK;
}
