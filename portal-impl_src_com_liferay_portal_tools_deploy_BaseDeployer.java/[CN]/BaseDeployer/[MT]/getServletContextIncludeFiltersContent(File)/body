{
  boolean servletContextIncludeFiltersEnabled=true;
  Properties properties=getPluginPackageProperties(srcFile);
  if (properties != null) {
    servletContextIncludeFiltersEnabled=GetterUtil.getBoolean(properties.getProperty("servlet-context-include-filters-enabled"),true);
  }
  if (servletContextIncludeFiltersEnabled) {
    String servletContextIncludeFiltersContent=FileUtil.read(DeployUtil.getResourcePath("servlet-context-include-filters-web.xml"));
    return servletContextIncludeFiltersContent;
  }
 else {
    return StringPool.BLANK;
  }
}
