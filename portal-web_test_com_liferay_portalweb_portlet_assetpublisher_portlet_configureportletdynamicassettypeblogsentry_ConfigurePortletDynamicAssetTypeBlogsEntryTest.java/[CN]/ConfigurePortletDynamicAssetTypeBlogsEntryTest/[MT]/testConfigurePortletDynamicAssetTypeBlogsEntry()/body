{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.select("_86_anyAssetType",RuntimeVariables.replace("label=Filter..."));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("_86_currentClassNameIds")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.addSelection("_86_currentClassNameIds",RuntimeVariables.replace("label=Blogs Entry"));
  selenium.clickAt("//fieldset[2]/div/div/table/tbody/tr/td[2]/a[1]/img",RuntimeVariables.replace(""));
  Thread.sleep(5000);
  assertEquals(RuntimeVariables.replace("Blogs Entry"),selenium.getText("_86_availableClassNameIds"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have successfully updated the setup."));
}
