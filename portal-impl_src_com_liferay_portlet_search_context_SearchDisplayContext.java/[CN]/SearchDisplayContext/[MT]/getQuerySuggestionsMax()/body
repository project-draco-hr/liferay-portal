{
  if (_querySuggestionsMax != null) {
    return _querySuggestionsMax;
  }
  _querySuggestionsMax=GetterUtil.getInteger(_portletPreferences.getValue("querySuggestionsMax",null),PropsValues.INDEX_SEARCH_QUERY_SUGGESTION_MAX);
  if (_querySuggestionsMax <= 0) {
    _querySuggestionsMax=PropsValues.INDEX_SEARCH_QUERY_SUGGESTION_MAX;
  }
  return _querySuggestionsMax;
}
