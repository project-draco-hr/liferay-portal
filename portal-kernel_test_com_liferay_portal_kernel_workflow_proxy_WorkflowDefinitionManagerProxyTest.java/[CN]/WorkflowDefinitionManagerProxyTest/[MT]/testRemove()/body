{
  final String definitionName="definitionName";
  MessageListener removeListener=new MessageListener(){
    public void receive(    Message message){
      WorkflowDefinitionRequest request=(WorkflowDefinitionRequest)message.getPayload();
      assertEquals(WorkflowDefinitionRequestType.REMOVE,request.getType());
      assertEquals(definitionName,request.getWorkflowDefinitionInfo().getWorkflowDefinitionName());
    }
  }
;
  messageBus.registerMessageListener(DestinationNames.WORKFLOW_DEFINITION,removeListener);
  _proxy.removeWorkflowDefinition(definitionName);
}
