      if (selenium.isElementPresent("//textarea[@id='_33_editor' and @style='display: none;']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//span[.='Source']")) {
        break;
      }
    }
 catch (    Exception e) {
