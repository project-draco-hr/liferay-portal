{
  ThreadPoolExecutor threadPoolExecutor=new ThreadPoolExecutor(1,2,TestUtil.KEEPALIVE_TIME,TimeUnit.MILLISECONDS,true,3);
  long startTime=System.currentTimeMillis();
  assertFalse(threadPoolExecutor.awaitTermination(TestUtil.KEEPALIVE_TIME,TimeUnit.MILLISECONDS));
  long waitTime=System.currentTimeMillis() - startTime;
  assertTrue(waitTime >= TestUtil.KEEPALIVE_TIME);
}
