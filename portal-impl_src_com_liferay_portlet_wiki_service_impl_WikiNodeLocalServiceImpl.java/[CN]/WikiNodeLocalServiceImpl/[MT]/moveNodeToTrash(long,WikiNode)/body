{
  WikiNode updatedNode=updateStatus(userId,node,WorkflowConstants.STATUS_IN_TRASH,new ServiceContext());
  TrashEntry trashEntry=trashEntryLocalService.getEntry(WikiNode.class.getName(),updatedNode.getNodeId());
  String trashTitle=TrashUtil.getTrashTitle(trashEntry.getEntryId());
  updatedNode.setName(trashTitle);
  return wikiNodePersistence.update(updatedNode);
}
