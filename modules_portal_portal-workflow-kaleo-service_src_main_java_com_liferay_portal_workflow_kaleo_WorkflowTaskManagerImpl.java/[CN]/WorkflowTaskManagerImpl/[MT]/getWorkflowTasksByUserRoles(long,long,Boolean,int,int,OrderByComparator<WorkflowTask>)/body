{
  try {
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setCompanyId(companyId);
    serviceContext.setUserId(userId);
    List<KaleoTaskInstanceToken> kaleoTaskInstanceTokens=KaleoTaskInstanceTokenLocalServiceUtil.search(null,completed,Boolean.TRUE,start,end,KaleoTaskInstanceTokenOrderByComparator.getOrderByComparator(orderByComparator),serviceContext);
    return toWorkflowTasks(kaleoTaskInstanceTokens);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
