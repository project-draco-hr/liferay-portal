{
  try {
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setCompanyId(companyId);
    if (userId != null) {
      serviceContext.setUserId(userId);
    }
    return KaleoTaskInstanceTokenLocalServiceUtil.getKaleoTaskInstanceTokensCount(workflowInstanceId,completed,serviceContext);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
