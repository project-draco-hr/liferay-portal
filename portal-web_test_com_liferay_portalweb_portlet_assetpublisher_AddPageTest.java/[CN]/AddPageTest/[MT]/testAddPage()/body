{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//div[@id='_145_myPlacesContainer']/ul/li[2]/a/span[1]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("//div[@id='_145_myPlacesContainer']/ul/li[2]/a/span[1]"));
