{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),UserThreadService.class.getName());
    if (invokableService instanceof UserThreadService) {
      _service=(UserThreadService)invokableService;
    }
 else {
      _service=new UserThreadServiceClp(invokableService);
    }
    ReferenceRegistry.registerReference(UserThreadServiceUtil.class,"_service");
  }
  return _service;
}
