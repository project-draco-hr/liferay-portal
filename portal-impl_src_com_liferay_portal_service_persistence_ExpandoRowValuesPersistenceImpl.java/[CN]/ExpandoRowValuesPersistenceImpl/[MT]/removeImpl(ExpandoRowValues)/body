{
  Session session=null;
  try {
    session=openSession();
    session.delete(expandoRowValues);
    session.flush();
    return expandoRowValues;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(ExpandoRowValues.class.getName());
  }
}
