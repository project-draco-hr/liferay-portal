{
  ExpandoRowValues expandoRowValues=fetchByPrimaryKey(expandoRowValuesPK);
  if (expandoRowValues == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ExpandoRowValues exists with the primary key " + expandoRowValuesPK);
    }
    throw new NoSuchExpandoRowValuesException("No ExpandoRowValues exists with the primary key " + expandoRowValuesPK);
  }
  return expandoRowValues;
}
