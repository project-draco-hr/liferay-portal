  UpgradeColumn upgradeClassPKColumn=new ClassPKUpgradeColumnImpl(classNameIdColumn,Organization.class.getName(),organizationIdMapper,true);
  upgradeTable=new DefaultUpgradeTableImpl(AddressImpl.TABLE_NAME,AddressImpl.TABLE_COLUMNS,classNameIdColumn,upgradeClassPKColumn);
  upgradeTable.updateTable();
