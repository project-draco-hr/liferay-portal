{
  List<String> values=new ArrayList<>();
  for (int i=0; ; i++) {
    String value=ParamUtil.getString(actionRequest,"preferences--" + key + i+ "--");
    if (Validator.isNull(value)) {
      break;
    }
    values.add(value);
  }
  setPreference(actionRequest,key,values.toArray(new String[values.size()]));
}
