{
  Registry registry=RegistryUtil.getRegistry();
  DLDisplayContextFactory dlDisplayContextFactory=registry.getService(serviceReference);
  DLDisplayContextFactoryReference dlDisplayContextFactoryReference=new DLDisplayContextFactoryReference(dlDisplayContextFactory,serviceReference);
  _dlDisplayContextFactoryReferences.add(dlDisplayContextFactoryReference);
  _dlDisplayContextFactoryReferencesMap.put(dlDisplayContextFactory,dlDisplayContextFactoryReference);
  return dlDisplayContextFactory;
}
