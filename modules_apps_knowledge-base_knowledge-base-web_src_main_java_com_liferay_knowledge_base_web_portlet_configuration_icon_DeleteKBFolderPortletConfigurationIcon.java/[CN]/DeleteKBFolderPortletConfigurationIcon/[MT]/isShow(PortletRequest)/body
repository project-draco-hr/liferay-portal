{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  KBFolder kbFolder=(KBFolder)portletRequest.getAttribute(KBWebKeys.KNOWLEDGE_BASE_KB_FOLDER);
  PermissionChecker permissionChecker=themeDisplay.getPermissionChecker();
  if (KBFolderPermission.contains(permissionChecker,kbFolder,KBActionKeys.DELETE)) {
    return true;
  }
  return false;
}
