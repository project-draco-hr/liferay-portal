{
  List<DLFileEntryMetadata> dlFileEntryMetadataWithNoStructures=DLFileEntryMetadataLocalServiceUtil.getNoStructureFileEntryMetadatas();
  if (_log.isDebugEnabled()) {
    _log.debug("Deleting " + dlFileEntryMetadataWithNoStructures.size() + " file entry metadatas with no structures");
  }
  for (  DLFileEntryMetadata dlFileEntryMetadata : dlFileEntryMetadataWithNoStructures) {
    deleteUnusedDLFileEntryMetadata(dlFileEntryMetadata);
  }
  List<DLFileEntryMetadata> dlFileEntryMetadataMismatchedCompanyIds=DLFileEntryMetadataLocalServiceUtil.getMismatchedCompanyIdFileEntryMetadatas();
  if (_log.isDebugEnabled()) {
    _log.debug("Deleting " + dlFileEntryMetadataWithNoStructures.size() + " file entry metadatas with mismatched company IDs");
  }
  for (  DLFileEntryMetadata dlFileEntryMetadata : dlFileEntryMetadataMismatchedCompanyIds) {
    deleteUnusedDLFileEntryMetadata(dlFileEntryMetadata);
  }
}
