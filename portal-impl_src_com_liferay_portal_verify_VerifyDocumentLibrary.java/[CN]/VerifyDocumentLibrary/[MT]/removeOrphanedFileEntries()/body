{
  List<DLFileEntry> fileEntries=DLFileEntryLocalServiceUtil.getOrphanedFileEntries();
  if (_log.isDebugEnabled()) {
    _log.debug("Processing " + fileEntries.size() + " file entries with no group");
  }
  for (  DLFileEntry fileEntry : fileEntries) {
    try {
      DLFileEntryLocalServiceUtil.deleteFileEntry(fileEntry);
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to remove file entry " + fileEntry.getFileEntryId() + " with group "+ fileEntry.getGroupId()+ ": "+ e.getMessage());
      }
    }
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Removed orphaned file entries");
  }
}
