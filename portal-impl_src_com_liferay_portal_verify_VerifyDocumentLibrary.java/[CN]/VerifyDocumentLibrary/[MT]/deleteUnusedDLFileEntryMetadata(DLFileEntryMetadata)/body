{
  DLFileEntryMetadataLocalServiceUtil.deleteDLFileEntryMetadata(dlFileEntryMetadata);
  StorageAdapter storageAdapter=StorageAdapterRegistryUtil.getStorageAdapter("xml");
  storageAdapter.deleteByClass(dlFileEntryMetadata.getDDMStorageId());
  DDMStructureLinkLocalServiceUtil.deleteClassStructureLink(dlFileEntryMetadata.getFileEntryMetadataId());
}
