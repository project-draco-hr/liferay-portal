{
  DLFileEntryMetadataLocalServiceUtil.deleteDLFileEntryMetadata(dlFileEntryMetadata);
  StorageAdapter storageAdapter=StorageAdapterRegistryUtil.getStorageAdapter(StorageType.JSON.toString());
  storageAdapter.deleteByClass(dlFileEntryMetadata.getDDMStorageId());
  DDMStructureLinkLocalServiceUtil.deleteStructureLinks(PortalUtil.getClassNameId(DLFileEntryMetadata.class),dlFileEntryMetadata.getFileEntryMetadataId());
}
