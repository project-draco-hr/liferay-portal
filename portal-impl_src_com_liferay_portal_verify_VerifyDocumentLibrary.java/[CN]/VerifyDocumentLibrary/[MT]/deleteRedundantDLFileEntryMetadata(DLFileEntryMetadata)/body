{
  DLFileEntryMetadataLocalServiceUtil.deleteDLFileEntryMetadata(fileEntryMetadata);
  DDMContentLocalServiceUtil.deleteDDMContent(fileEntryMetadata.getDDMStorageId());
  DDMStorageLinkLocalServiceUtil.deleteClassStorageLink(fileEntryMetadata.getDDMStorageId());
  DDMStructureLinkLocalServiceUtil.deleteClassStructureLink(fileEntryMetadata.getFileEntryMetadataId());
}
