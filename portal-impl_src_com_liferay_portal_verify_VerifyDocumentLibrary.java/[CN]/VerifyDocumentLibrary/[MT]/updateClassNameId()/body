{
  try {
    runSQL("update DLFileEntry set classNameId = 0 where classNameId " + "is null");
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to update classNameId column for file entries " + "where classNameId is null",e);
    }
    return;
  }
  if (_log.isDebugEnabled()) {
    _log.debug("ClassNameId column verified for file entries");
  }
}
