{
  trashEntry=toUnwrappedModel(trashEntry);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(trashEntry)) {
      trashEntry=(TrashEntry)session.get(TrashEntryImpl.class,trashEntry.getPrimaryKeyObj());
    }
    if (trashEntry != null) {
      session.delete(trashEntry);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (trashEntry != null) {
    clearCache(trashEntry);
  }
  return trashEntry;
}
