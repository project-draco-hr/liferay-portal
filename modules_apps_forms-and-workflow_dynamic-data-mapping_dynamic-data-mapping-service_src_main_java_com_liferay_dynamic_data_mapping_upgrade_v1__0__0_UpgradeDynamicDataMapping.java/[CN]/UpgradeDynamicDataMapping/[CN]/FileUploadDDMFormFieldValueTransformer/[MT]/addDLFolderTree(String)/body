{
  long ddmFolderId=addDDMDLFolder();
  long entryIdFolderId=addEntryIdDLFolder(ddmFolderId);
  long entryVersionFolderId=addEntryVersionDLFolder(entryIdFolderId);
  long fieldNameFolderId=increment();
  addDLFolder(PortalUUIDUtil.generate(),fieldNameFolderId,_groupId,_companyId,_userId,_userName,_now,_now,_groupId,entryVersionFolderId,ddmFormFieldName,StringPool.BLANK,_now);
  return fieldNameFolderId;
}
