{
  List<String> ddmDateFieldNames=getDDMDateFieldNames(ddmForm);
  if (ddmDateFieldNames.isEmpty()) {
    return;
  }
  Map<Long,String> ddmTemplateScriptMap=getDDMTemplateScriptMap(structureId);
  for (  Long ddmTemplateId : ddmTemplateScriptMap.keySet()) {
    String script=ddmTemplateScriptMap.get(ddmTemplateId);
    for (    String ddmDateFieldName : ddmDateFieldNames) {
      script=updateTemplateScriptDateAssignStatement(ddmDateFieldName,script);
      script=updateTemplateScriptDateIfStatement(ddmDateFieldName,script);
      script=updateTemplateScriptDateParseStatement(ddmDateFieldName,script);
    }
    updateTemplateScript(ddmTemplateId,script);
  }
}
