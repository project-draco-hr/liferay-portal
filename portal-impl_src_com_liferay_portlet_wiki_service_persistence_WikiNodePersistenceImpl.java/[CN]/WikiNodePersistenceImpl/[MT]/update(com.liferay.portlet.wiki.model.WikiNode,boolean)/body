{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(wikiNode);
    }
 else {
      if (wikiNode.isNew()) {
        session.save(wikiNode);
      }
 else {
        wikiNode=(WikiNode)session.merge(wikiNode);
      }
    }
    session.flush();
    wikiNode.setNew(false);
    return wikiNode;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(WikiNode.class.getName());
  }
}
