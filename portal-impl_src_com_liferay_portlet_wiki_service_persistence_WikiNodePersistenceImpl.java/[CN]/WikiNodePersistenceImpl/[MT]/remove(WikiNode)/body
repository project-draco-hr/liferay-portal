{
  Session session=null;
  try {
    session=openSession();
    wikiNode=(WikiNode)session.merge(wikiNode);
    session.delete(wikiNode);
    session.flush();
    return wikiNode;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(WikiNode.class.getName());
  }
}
