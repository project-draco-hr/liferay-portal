{
  Session session=null;
  try {
    session=openSession();
    WikiNode wikiNode=(WikiNode)session.get(WikiNodeImpl.class,new Long(nodeId));
    if (wikiNode == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + nodeId);
      }
      throw new NoSuchNodeException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + nodeId);
    }
    return remove(wikiNode);
  }
 catch (  NoSuchNodeException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
