{
  String id=StringUtil.replace(_panelCategory.getKey(),StringPool.PERIOD,StringPool.UNDERLINE);
  id="panel-manage-" + id;
  request.setAttribute("liferay-application-list:panel-category-body:id",id);
  List<PanelApp> panelApps=_panelApps;
  if (panelApps == null) {
    panelApps=getPanelApps();
  }
  request.setAttribute("liferay-application-list:panel-category-body:panelApps",panelApps);
  request.setAttribute("liferay-application-list:panel-category-body:panelCategory",_panelCategory);
}
