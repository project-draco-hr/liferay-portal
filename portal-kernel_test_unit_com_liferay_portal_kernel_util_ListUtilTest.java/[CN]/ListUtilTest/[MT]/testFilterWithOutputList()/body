{
  List<String> expectedOutputList=new ArrayList<>();
  expectedOutputList.add("0");
  expectedOutputList.add("b");
  List<String> inputList=new ArrayList<>();
  inputList.add("a");
  inputList.add("b");
  inputList.add("c");
  List<String> outputList=new ArrayList<>();
  outputList.add("0");
  PredicateFilter<String> predicateFilter=new PredicateFilter<String>(){
    @Override public boolean filter(    String string){
      return string.equals("b");
    }
  }
;
  Collection<String> actualOutputList=ListUtil.filter(inputList,outputList,predicateFilter);
  Assert.assertSame(outputList,actualOutputList);
  Assert.assertEquals(expectedOutputList,actualOutputList);
}
