{
  try {
    long companyId=PortalInstances.getCompanyId(request);
    if (BrowserSnifferUtil.isIe(request) && PrefsPropsUtil.getBoolean(companyId,PropsKeys.NTLM_AUTH_ENABLED,_ntlmConfiguration.enabled())) {
      return true;
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return false;
}
