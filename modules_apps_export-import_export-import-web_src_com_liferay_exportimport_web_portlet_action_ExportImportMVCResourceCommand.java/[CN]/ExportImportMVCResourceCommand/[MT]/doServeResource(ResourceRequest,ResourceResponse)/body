{
  String cmd=ParamUtil.getString(resourceRequest,Constants.CMD);
  PortletContext portletContext=getPortletConfig(resourceRequest).getPortletContext();
  PortletRequestDispatcher portletRequestDispatcher=null;
  if (cmd.equals(Constants.EXPORT)) {
    portletRequestDispatcher=portletContext.getRequestDispatcher("/export_portlet_processes.jsp");
  }
 else   if (cmd.equals(Constants.IMPORT)) {
    portletRequestDispatcher=portletContext.getRequestDispatcher("/import_portlet_processes.jsp");
  }
 else   if (cmd.equals(Constants.PUBLISH)) {
    portletRequestDispatcher=portletContext.getRequestDispatcher("/publish_portlet_processes.jsp");
  }
 else {
    portletRequestDispatcher=portletContext.getRequestDispatcher("/import_portlet_resources.jsp");
  }
  resourceRequest=ActionUtil.getWrappedResourceRequest(resourceRequest,null);
  portletRequestDispatcher.include(resourceRequest,resourceResponse);
}
