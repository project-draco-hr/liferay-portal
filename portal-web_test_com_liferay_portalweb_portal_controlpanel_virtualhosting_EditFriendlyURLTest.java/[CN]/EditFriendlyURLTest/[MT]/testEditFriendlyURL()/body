    Thread.sleep(1000);
  }
  selenium.clickAt("//strong/span",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Manage Pages")) {
        break;
      }
    }
 catch (    Exception e) {
