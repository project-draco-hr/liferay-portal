{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Return to Full Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("link=Return to Full Page");
  selenium.waitForPageToLoad("30000");
  selenium.typeKeys("_19_keywords1","T?st");
  selenium.click("//input[@value='Search Messages']");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=T?st M?ssag?")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("link=T?st M?ssag?");
  selenium.waitForPageToLoad("30000");
  verifyTrue(selenium.isTextPresent("This is a t?st r?ply m?ssag?!"));
}
