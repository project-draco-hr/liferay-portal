{
  FileEntry fileEntry=null;
  Map<Long,Long> groupIds=(Map<Long,Long>)portletDataContext.getNewPrimaryKeysMap(Group.class);
  long groupId=jsonObject.getLong("groupId");
  String uuid=jsonObject.getString("uuid");
  groupId=MapUtil.getLong(groupIds,groupId,groupId);
  if (Validator.isNotNull(groupId) && Validator.isNotNull(uuid)) {
    try {
      fileEntry=_dlAppService.getFileEntryByUuidAndGroupId(uuid,groupId);
    }
 catch (    NoSuchFileEntryException nsfee) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to find file entry with uuid " + uuid + " and groupId "+ groupId);
      }
    }
  }
  return fileEntry;
}
