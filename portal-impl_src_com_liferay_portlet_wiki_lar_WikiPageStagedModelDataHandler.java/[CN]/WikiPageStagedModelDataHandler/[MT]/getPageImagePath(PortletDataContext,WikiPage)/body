{
  StringBundler sb=new StringBundler(6);
  sb.append(ExportImportPathUtil.getPortletPath(portletDataContext,PortletKeys.WIKI));
  sb.append("/page/");
  sb.append(page.getUuid());
  sb.append(StringPool.SLASH);
  sb.append(page.getVersion());
  sb.append(StringPool.SLASH);
  return sb.toString();
}
