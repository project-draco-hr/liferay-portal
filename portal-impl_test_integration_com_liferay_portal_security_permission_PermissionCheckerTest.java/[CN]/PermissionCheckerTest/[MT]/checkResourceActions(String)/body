{
  List<String> portletActions=ResourceActionsUtil.getPortletResourceActions(portletName);
  ResourceActionLocalServiceUtil.checkResourceActions(portletName,portletActions);
  List<String> modelNames=ResourceActionsUtil.getPortletModelResources(portletName);
  for (  String modelName : modelNames) {
    List<String> modelActions=ResourceActionsUtil.getModelResourceActions(modelName);
    ResourceActionLocalServiceUtil.checkResourceActions(modelName,modelActions);
  }
}
