{
  Role portalReviewerRole=RoleLocalServiceUtil.fetchRole(_group.getCompanyId(),RoleConstants.PORTAL_CONTENT_REVIEWER);
  if (portalReviewerRole == null) {
    portalReviewerRole=RoleServiceUtil.addRole(null,0,RoleConstants.PORTAL_CONTENT_REVIEWER,ServiceTestUtil.randomLocaleStringMap(),ServiceTestUtil.randomLocaleStringMap(),RoleConstants.TYPE_REGULAR,ServiceTestUtil.randomString(),ServiceTestUtil.getServiceContext());
  }
  return portalReviewerRole;
}
