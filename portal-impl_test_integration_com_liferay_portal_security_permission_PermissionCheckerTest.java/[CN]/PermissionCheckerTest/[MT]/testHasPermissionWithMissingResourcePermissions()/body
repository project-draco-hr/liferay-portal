{
  PermissionChecker permissionChecker=_getPermissionChecker(TestPropsValues.getUser());
  try {
    permissionChecker.hasPermission(0,_MODEL_RESOURCE_NAME,12345,ActionKeys.VIEW);
    Assert.fail();
  }
 catch (  Throwable t) {
    boolean found=false;
    Throwable cause=t;
    while (!found && (cause != null)) {
      if (cause instanceof NoSuchResourcePermissionException) {
        found=true;
      }
      cause=cause.getCause();
    }
    if (!found) {
      Assert.fail(t.getMessage());
      throw t;
    }
  }
}
