{
  Session session=null;
  try {
    session=openSession();
    session.delete(announcementsFlag);
    session.flush();
    return announcementsFlag;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(AnnouncementsFlag.class.getName());
  }
}
