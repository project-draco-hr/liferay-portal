{
  Session session=null;
  try {
    session=openSession();
    AnnouncementsFlag announcementsFlag=(AnnouncementsFlag)session.get(AnnouncementsFlagImpl.class,new Long(flagId));
    if (announcementsFlag == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No AnnouncementsFlag exists with the primary key " + flagId);
      }
      throw new NoSuchFlagException("No AnnouncementsFlag exists with the primary key " + flagId);
    }
    return remove(announcementsFlag);
  }
 catch (  NoSuchFlagException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
