{
  AnnouncementsFlag result=(AnnouncementsFlag)EntityCacheUtil.getResult(AnnouncementsFlagModelImpl.ENTITY_CACHE_ENABLED,AnnouncementsFlag.class,flagId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      AnnouncementsFlag announcementsFlag=(AnnouncementsFlag)session.get(AnnouncementsFlagImpl.class,new Long(flagId));
      if (announcementsFlag != null) {
        cacheResult(announcementsFlag);
      }
      return announcementsFlag;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (AnnouncementsFlag)result;
  }
}
