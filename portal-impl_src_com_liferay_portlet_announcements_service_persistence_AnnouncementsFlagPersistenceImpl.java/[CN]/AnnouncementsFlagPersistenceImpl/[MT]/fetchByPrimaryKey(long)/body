{
  AnnouncementsFlag announcementsFlag=(AnnouncementsFlag)EntityCacheUtil.getResult(AnnouncementsFlagModelImpl.ENTITY_CACHE_ENABLED,AnnouncementsFlagImpl.class,flagId,this);
  if (announcementsFlag == null) {
    Session session=null;
    try {
      session=openSession();
      announcementsFlag=(AnnouncementsFlag)session.get(AnnouncementsFlagImpl.class,Long.valueOf(flagId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (announcementsFlag != null) {
        cacheResult(announcementsFlag);
      }
      closeSession(session);
    }
  }
  return announcementsFlag;
}
