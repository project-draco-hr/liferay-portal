{
  Session session=null;
  try {
    Object returnObj=PortalClassInvoker.invoke(false,_createSession,parameters);
    if (returnObj != null) {
      session=(Session)returnObj;
    }
  }
 catch (  RepositoryException re) {
    throw re;
  }
catch (  Exception e) {
    _log.error(e,e);
  }
  return session;
}
