{
  String targetURL="";
  if (url.startsWith("/")) {
    targetURL=TestPropsValues.PORTAL_URL + url;
  }
 else {
    targetURL=url;
  }
  for (int second=0; ; second++) {
    if (second >= TestPropsValues.TIMEOUT_IMPLICIT_WAIT) {
      break;
    }
    try {
      webDriver.get(targetURL);
      if (TestPropsValues.BROWSER_TYPE.equals("*iehta") || TestPropsValues.BROWSER_TYPE.equals("*iexplore")) {
        refresh(webDriver);
      }
      if (targetURL.equals(getLocation(webDriver))) {
        break;
      }
      Thread.sleep(1000);
    }
 catch (    Exception e) {
    }
  }
}
