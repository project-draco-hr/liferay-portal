{
  WebElement webElement=getWebElement(locator);
  if (!webElement.isEnabled()) {
    return;
  }
  if (TestPropsValues.MOBILE_DEVICE_ENABLED) {
    webElement.clear();
    webElement.click();
    try {
      Thread.sleep(1000);
    }
 catch (    Exception e) {
    }
    Screen screen=new Screen();
    screen.type(value);
  }
 else {
    webElement.clear();
    webElement.sendKeys(value);
  }
}
