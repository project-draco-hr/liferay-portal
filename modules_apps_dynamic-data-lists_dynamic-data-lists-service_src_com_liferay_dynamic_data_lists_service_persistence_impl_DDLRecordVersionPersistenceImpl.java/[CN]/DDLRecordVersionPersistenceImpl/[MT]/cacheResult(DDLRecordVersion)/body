{
  entityCache.putResult(DDLRecordVersionModelImpl.ENTITY_CACHE_ENABLED,DDLRecordVersionImpl.class,ddlRecordVersion.getPrimaryKey(),ddlRecordVersion);
  finderCache.putResult(FINDER_PATH_FETCH_BY_R_V,new Object[]{ddlRecordVersion.getRecordId(),ddlRecordVersion.getVersion()},ddlRecordVersion);
  ddlRecordVersion.resetOriginalValues();
}
