{
  String fileName=getFileName(image.getImageId(),image.getType());
  InputStream is=DLLocalServiceUtil.getFileAsStream(_COMPANY_ID,_REPOSITORY_ID,fileName);
  byte[] bytes=null;
  try {
    bytes=FileUtil.getBytes(is);
  }
 catch (  IOException e) {
    throw new SystemException(e);
  }
  return bytes;
}
