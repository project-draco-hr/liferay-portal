{
  super.init(portletConfig);
  PortletContext portletContext=portletConfig.getPortletContext();
  _portletContextName=portletContext.getPortletContextName();
  _actionTemplate=getVelocityTemplateId(getInitParameter("action-template"));
  _editTemplate=getVelocityTemplateId(getInitParameter("edit-template"));
  _helpTemplate=getVelocityTemplateId(getInitParameter("help-template"));
  _resourceTemplate=getVelocityTemplateId(getInitParameter("resource-template"));
  _viewTemplate=getVelocityTemplateId(getInitParameter("view-template"));
}
