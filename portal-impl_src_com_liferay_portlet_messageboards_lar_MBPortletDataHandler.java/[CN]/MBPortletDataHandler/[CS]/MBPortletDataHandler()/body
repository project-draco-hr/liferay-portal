{
  setDeletionSystemEventStagedModelTypes(new StagedModelType(MBBan.class),new StagedModelType(MBCategory.class),new StagedModelType(MBMessage.class),new StagedModelType(MBThreadFlag.class));
  setExportControls(new PortletDataHandlerBoolean(NAMESPACE,"messages",true,false,null,MBMessage.class.getName()),new PortletDataHandlerBoolean(NAMESPACE,"thread-flags",true,false,null,MBThreadFlag.class.getName()),new PortletDataHandlerBoolean(NAMESPACE,"user-bans",true,false,null,MBBan.class.getName()));
  setImportControls(getExportControls());
  setPublishToLiveByDefault(PropsValues.MESSAGE_BOARDS_PUBLISH_TO_LIVE_BY_DEFAULT);
}
