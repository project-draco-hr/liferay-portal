{
  Project project=getDownloadProject(sampleDir.getName());
  int hostNameCount=LoadBalancerUtil.getHostNameCount(project,sampleDir.getName());
  for (int i=1; i <= hostNameCount; i++) {
    downloadSampleURL(new File(sampleDir,sampleDir.getName() + "-" + i),createURL(project.getProperty("jenkins.local.url[" + sampleDir.getName() + "-"+ i+ "]")),"/computer/api/json?pretty&tree=computer" + "[displayName,idle,offline]");
    downloadSampleURL(new File(sampleDir,sampleDir.getName() + "-" + i),createURL(project.getProperty("jenkins.local.url[" + sampleDir.getName() + "-"+ i+ "]")),"/queue/api/json?pretty");
  }
}
