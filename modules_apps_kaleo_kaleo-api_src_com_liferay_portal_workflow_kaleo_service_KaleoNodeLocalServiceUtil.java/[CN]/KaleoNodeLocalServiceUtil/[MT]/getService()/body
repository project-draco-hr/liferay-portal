{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),KaleoNodeLocalService.class.getName());
    if (invokableLocalService instanceof KaleoNodeLocalService) {
      _service=(KaleoNodeLocalService)invokableLocalService;
    }
 else {
      _service=new KaleoNodeLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(KaleoNodeLocalServiceUtil.class,"_service");
  }
  return _service;
}
