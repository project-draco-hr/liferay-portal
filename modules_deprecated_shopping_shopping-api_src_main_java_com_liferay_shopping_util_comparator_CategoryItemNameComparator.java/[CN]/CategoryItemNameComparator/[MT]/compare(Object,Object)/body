{
  int value=0;
  if ((object1 instanceof ShoppingCategory) && (object2 instanceof ShoppingCategory)) {
    value=getName(object1).compareTo(getName(object2));
  }
 else   if (object1 instanceof ShoppingCategory) {
    value=-1;
  }
 else   if (object2 instanceof ShoppingCategory) {
    value=1;
  }
 else {
    value=getName(object1).compareTo(getName(object2));
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
