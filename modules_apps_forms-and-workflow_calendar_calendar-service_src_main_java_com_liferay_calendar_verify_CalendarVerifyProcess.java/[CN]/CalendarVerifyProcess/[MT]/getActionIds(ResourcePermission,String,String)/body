{
  long actionIds=0;
  List<ResourceAction> oldResourceActions=_resourceActionLocalService.getResourceActions(oldClassName);
  for (  ResourceAction oldResourceAction : oldResourceActions) {
    boolean hasActionId=_resourcePermissionLocalService.hasActionId(resourcePermission,oldResourceAction);
    if (!hasActionId) {
      continue;
    }
    actionIds=actionIds | getActionId(oldResourceAction,newClassName);
  }
  return actionIds;
}
