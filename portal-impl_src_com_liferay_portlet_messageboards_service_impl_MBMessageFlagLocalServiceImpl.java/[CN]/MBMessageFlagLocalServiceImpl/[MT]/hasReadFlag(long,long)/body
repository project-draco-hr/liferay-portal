{
  User user=userPersistence.findByPrimaryKey(userId);
  if (user.isDefaultUser()) {
    return true;
  }
  MBMessageFlag messageFlag=mbMessageFlagPersistence.fetchByU_M_F(userId,messageId,MBMessageFlagImpl.READ_FLAG);
  if (messageFlag != null) {
    return true;
  }
 else {
    return false;
  }
}
