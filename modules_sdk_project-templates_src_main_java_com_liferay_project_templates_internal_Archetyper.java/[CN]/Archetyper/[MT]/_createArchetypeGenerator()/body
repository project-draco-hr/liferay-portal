{
  ArchetypeGenerator archetypeGenerator=new DefaultArchetypeGenerator();
  ArchetypeArtifactManager archetypeArtifactManager=_createArchetypeArtifactManager();
  ReflectionUtil.setFieldValue(DefaultArchetypeGenerator.class,"archetypeArtifactManager",archetypeGenerator,archetypeArtifactManager);
  ReflectionUtil.setFieldValue(DefaultArchetypeGenerator.class,"filesetGenerator",archetypeGenerator,_createFilesetArchetypeGenerator(archetypeArtifactManager));
  return archetypeGenerator;
}
