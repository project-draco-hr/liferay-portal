{
  Lock newLock=addLock();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Lock.class,Lock.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("lockId",newLock.getLockId()));
  List<Lock> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Lock existingLock=result.get(0);
  assertEquals(existingLock,newLock);
}
