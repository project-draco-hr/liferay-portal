{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Lock.class,Lock.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("lockId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("lockId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
