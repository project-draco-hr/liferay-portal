{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  WikiNode newWikiNode=addWikiNode();
  _persistence.clearCache();
  WikiNode existingWikiNode=_persistence.findByPrimaryKey(newWikiNode.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingWikiNode.getUuid(),ReflectionTestUtil.invoke(existingWikiNode,"getOriginalUuid",new Class<?>[0])));
  Assert.assertEquals(existingWikiNode.getGroupId(),ReflectionTestUtil.invoke(existingWikiNode,"getOriginalGroupId",new Class<?>[0]));
  Assert.assertEquals(existingWikiNode.getGroupId(),ReflectionTestUtil.invoke(existingWikiNode,"getOriginalGroupId",new Class<?>[0]));
  Assert.assertTrue(Validator.equals(existingWikiNode.getName(),ReflectionTestUtil.invoke(existingWikiNode,"getOriginalName",new Class<?>[0])));
}
