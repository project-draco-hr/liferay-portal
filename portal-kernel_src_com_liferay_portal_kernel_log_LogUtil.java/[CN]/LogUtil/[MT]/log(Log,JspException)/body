{
  Throwable cause=jspe.getRootCause();
  if (cause == null) {
    cause=jspe;
  }
  if ((cause != jspe) && (cause instanceof JspException)) {
    log(log,(JspException)cause);
  }
 else   if (cause instanceof ServletException) {
    log(log,(ServletException)cause);
  }
 else {
    _log(log,cause);
  }
}
