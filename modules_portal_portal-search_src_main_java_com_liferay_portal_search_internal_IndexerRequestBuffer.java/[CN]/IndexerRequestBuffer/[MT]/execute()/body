{
  for (  IndexerRequest indexerRequest : _indexerRequests.values()) {
    try {
      indexerRequest.execute();
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to execute index request: " + indexerRequest);
      }
    }
  }
  _indexerRequests.clear();
}
