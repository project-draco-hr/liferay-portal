{
  if (_recordSet != null) {
    return _recordSet;
  }
  _recordSet=(DDLRecordSet)_renderRequest.getAttribute(DDLFormWebKeys.DYNAMIC_DATA_LISTS_RECORD_SET);
  if (_recordSet != null) {
    return _recordSet;
  }
  _recordSet=DDLRecordSetLocalServiceUtil.fetchDDLRecordSet(getRecordSetId());
  return _recordSet;
}
