{
  String key=companyId;
  Boolean value=(Boolean)_isCompanyAdmin.get(key);
  if (value == null) {
    Role adminRole=RoleServiceUtil.getRole(companyId,RoleImpl.ADMINISTRATOR);
    value=new Boolean(UserServiceUtil.hasRoleUser(adminRole.getRoleId(),_userId));
    _isCompanyAdmin.put(key,value);
  }
  return value.booleanValue();
}
