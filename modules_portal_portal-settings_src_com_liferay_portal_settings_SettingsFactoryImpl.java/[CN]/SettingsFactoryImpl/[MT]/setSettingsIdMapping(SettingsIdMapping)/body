{
  String settingsId=settingsIdMapping.getSettingsId();
  Class<?> configurationBeanClass=settingsIdMapping.getConfigurationBeanClass();
  ConfigurationBeanClassSettingsDescriptor configurationBeanClassSettingsDescriptor=new ConfigurationBeanClassSettingsDescriptor(configurationBeanClass);
  _register(settingsId,configurationBeanClassSettingsDescriptor,null);
}
