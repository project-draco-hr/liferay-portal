{
  List<ArchivedSettings> archivedSettingsList=new ArrayList<>();
  List<PortletItem> portletItems=PortletItemLocalServiceUtil.getPortletItems(groupId,portletId,com.liferay.portal.model.PortletPreferences.class.getName());
  for (  PortletItem portletItem : portletItems) {
    archivedSettingsList.add(new ArchivedSettingsImpl(portletItem));
  }
  return archivedSettingsList;
}
