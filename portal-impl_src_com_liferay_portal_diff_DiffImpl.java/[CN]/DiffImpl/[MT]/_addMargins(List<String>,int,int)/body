{
  List<String> changedLines=new ArrayList<>();
  if ((margin == 0) || (startPos == 0)) {
    return changedLines;
  }
  int i=startPos - margin;
  for (; i < 0; i++) {
    changedLines.add(CONTEXT_LINE);
  }
  for (; i < startPos; i++) {
    if (i < stringList.size()) {
      changedLines.add(stringList.get(i));
    }
  }
  return changedLines;
}
