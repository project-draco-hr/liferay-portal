{
  if (sassCompilerImpl == null || sassCompilerImpl.equals("jni")) {
    try {
      _sassCompiler=new JniSassCompiler();
    }
 catch (    Throwable t) {
      System.out.println("Unable to load native compiler, falling back to ruby");
      _sassCompiler=new RubySassCompiler(PropsValues.SCRIPTING_JRUBY_COMPILE_MODE,PropsValues.SCRIPTING_JRUBY_COMPILE_THRESHOLD,_TMP_DIR);
    }
  }
 else {
    try {
      _sassCompiler=new RubySassCompiler(PropsValues.SCRIPTING_JRUBY_COMPILE_MODE,PropsValues.SCRIPTING_JRUBY_COMPILE_THRESHOLD,_TMP_DIR);
    }
 catch (    Exception e) {
      System.out.println("Unable to load ruby compiler, falling back to native");
      _sassCompiler=new JniSassCompiler();
    }
  }
}
