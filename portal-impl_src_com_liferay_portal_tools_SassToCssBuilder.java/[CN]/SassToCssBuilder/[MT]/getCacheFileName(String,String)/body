{
  String cacheFileName=StringUtil.replace(fileName,StringPool.BACK_SLASH,StringPool.SLASH);
  int pos1=cacheFileName.lastIndexOf(StringPool.SLASH);
  int pos2=cacheFileName.lastIndexOf(StringPool.PERIOD);
  return cacheFileName.substring(0,pos1 + 1) + ".sass-cache/" + cacheFileName.substring(pos1 + 1,pos2)+ suffix+ cacheFileName.substring(pos2);
}
