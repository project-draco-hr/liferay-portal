{
  fileName=StringUtil.replace(fileName,StringPool.BACK_SLASH,StringPool.SLASH);
  int pos1=fileName.lastIndexOf(StringPool.SLASH);
  int pos2=fileName.lastIndexOf(StringPool.PERIOD);
  String cacheFileName=fileName.substring(0,pos1 + 1) + ".sass-cache/" + fileName.substring(pos1 + 1,pos2)+ suffix+ fileName.substring(pos2);
  return new File(cacheFileName);
}
