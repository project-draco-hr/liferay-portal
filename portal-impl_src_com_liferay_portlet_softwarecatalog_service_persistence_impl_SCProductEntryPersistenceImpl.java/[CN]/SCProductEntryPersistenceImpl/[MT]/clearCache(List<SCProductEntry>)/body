{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  SCProductEntry scProductEntry : scProductEntries) {
    EntityCacheUtil.removeResult(SCProductEntryModelImpl.ENTITY_CACHE_ENABLED,SCProductEntryImpl.class,scProductEntry.getPrimaryKey());
    clearUniqueFindersCache((SCProductEntryModelImpl)scProductEntry);
  }
}
