{
  Document document=null;
  try {
    document=SAXReaderUtil.read(content);
    Element rootElement=document.getRootElement();
    format(user,groupId,article,rootElement);
    content=XMLUtil.formatXML(document);
  }
 catch (  DocumentException de) {
    _log.error(de,de);
  }
  return content;
}
