{
  Map<Locale,String> journalArticleLocalizationTitleMap=new HashMap<>();
  List<JournalArticleLocalization> journalArticleLocalizationList=journalArticleLocalizationPersistence.findByArticlePK(articleId);
  for (  JournalArticleLocalization journalArticleLocalization : journalArticleLocalizationList) {
    journalArticleLocalizationTitleMap.put(LocaleUtil.fromLanguageId(journalArticleLocalization.getLanguageId()),journalArticleLocalization.getTitle());
  }
  return journalArticleLocalizationTitleMap;
}
