{
  if ((_type == TYPE_CLASS_PRIVATE) || (_type == TYPE_CLASS_PRIVATE_STATIC) || (_type == TYPE_CONSTRUCTOR_PRIVATE)|| (_type == TYPE_METHOD_PRIVATE)|| (_type == TYPE_METHOD_PRIVATE_STATIC)|| (_type == TYPE_VARIABLE_PRIVATE)|| (_type == TYPE_VARIABLE_PRIVATE_STATIC)) {
    return true;
  }
 else {
    return false;
  }
}
