{
  UnicodeProperties prototypeTypeSettingsProperties=layoutPrototypeLayout.getTypeSettingsProperties();
  if (newMergeFailCount == 0) {
    prototypeTypeSettingsProperties.remove(MERGE_FAIL_COUNT);
  }
 else {
    prototypeTypeSettingsProperties.setProperty(MERGE_FAIL_COUNT,String.valueOf(newMergeFailCount));
  }
}
