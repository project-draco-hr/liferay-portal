{
  LayoutTypePortlet sourceLayoutType=(LayoutTypePortlet)sourceLayout.getLayoutType();
  List<String> removePortletIds=new ArrayList<>(targetLayoutPortletIds);
  removePortletIds.removeAll(sourceLayoutType.getPortletIds());
  PortletLocalServiceUtil.deletePortlets(targetLayout.getCompanyId(),removePortletIds.toArray(new String[removePortletIds.size()]),targetLayout.getPlid());
}
