{
  List<Long> userGroupIds=UserGroupLocalServiceUtil.getGroupUserGroupIds(group.getGroupId());
  userGroupIds.retainAll(UserGroupLocalServiceUtil.getUserUserGroupIds(user.getUserId()));
  for (  Long userGroupId : userGroupIds) {
    UserGroup userGroup=UserGroupLocalServiceUtil.fetchUserGroup(userGroupId);
    userGroupNames.add(userGroup.getName());
  }
  return !userGroupIds.isEmpty();
}
