{
  LayoutLocalServiceUtil.updateLookAndFeel(targetLayout.getGroupId(),targetLayout.isPrivateLayout(),targetLayout.getLayoutId(),sourceLayout.getThemeId(),sourceLayout.getColorSchemeId(),sourceLayout.getCss(),false);
  LayoutLocalServiceUtil.updateLookAndFeel(targetLayout.getGroupId(),targetLayout.isPrivateLayout(),targetLayout.getLayoutId(),sourceLayout.getWapThemeId(),sourceLayout.getWapColorSchemeId(),sourceLayout.getCss(),true);
}
