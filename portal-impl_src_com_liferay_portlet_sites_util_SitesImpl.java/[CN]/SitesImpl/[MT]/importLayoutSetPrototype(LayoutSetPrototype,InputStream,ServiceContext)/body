{
  LayoutSet layoutSet=layoutSetPrototype.getLayoutSet();
  Map<String,String[]> parameterMap=getLayoutSetPrototypeParameters(serviceContext);
  setLayoutSetPrototypeLinkEnabledParameter(parameterMap,layoutSet,serviceContext);
  User user=UserLocalServiceUtil.fetchUser(serviceContext.getUserId());
  if (user == null) {
    user=UserLocalServiceUtil.getUser(GetterUtil.getLong(PrincipalThreadLocal.getName()));
  }
  Map<String,Serializable> settingsMap=ExportImportConfigurationSettingsMapFactory.buildImportSettingsMap(user.getUserId(),layoutSet.getGroupId(),layoutSet.isPrivateLayout(),null,parameterMap,Constants.IMPORT,user.getLocale(),user.getTimeZone(),StringPool.BLANK);
  ExportImportConfiguration exportImportConfiguration=ExportImportConfigurationLocalServiceUtil.addExportImportConfiguration(user.getUserId(),layoutSet.getGroupId(),StringPool.BLANK,StringPool.BLANK,ExportImportConfigurationConstants.TYPE_IMPORT_LAYOUT,settingsMap,WorkflowConstants.STATUS_DRAFT,serviceContext);
  LayoutServiceUtil.importLayouts(exportImportConfiguration,inputStream);
}
