{
  try {
    if (layout instanceof VirtualLayout) {
      return false;
    }
    if (Validator.isNull(layout.getLayoutPrototypeUuid()) && Validator.isNull(layout.getSourcePrototypeLayoutUuid())) {
      return true;
    }
    LayoutSet layoutSet=layout.getLayoutSet();
    if (layoutSet.isLayoutSetPrototypeLinkActive()) {
      boolean layoutSetPrototypeUpdateable=isLayoutSetPrototypeUpdateable(layoutSet);
      if (!layoutSetPrototypeUpdateable) {
        return false;
      }
      Layout layoutSetPrototypeLayout=getLayoutSetPrototypeLayout(layout);
      String layoutUpdateable=layoutSetPrototypeLayout.getTypeSettingsProperty(LAYOUT_UPDATEABLE);
      if (Validator.isNull(layoutUpdateable)) {
        return true;
      }
      return GetterUtil.getBoolean(layoutUpdateable);
    }
  }
 catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      _log.debug(e,e);
    }
  }
  return true;
}
