{
  LayoutSet layoutSetPrototypeLayoutSet=layoutSetPrototype.getLayoutSet();
  UnicodeProperties layoutSetPrototypeSettingsProperties=layoutSetPrototypeLayoutSet.getSettingsProperties();
  if (newMergeFailCount == 0) {
    layoutSetPrototypeSettingsProperties.remove(MERGE_FAIL_COUNT);
  }
 else {
    layoutSetPrototypeSettingsProperties.setProperty(MERGE_FAIL_COUNT,String.valueOf(newMergeFailCount));
  }
  LayoutSetServiceUtil.updateSettings(layoutSetPrototypeLayoutSet.getGroupId(),layoutSetPrototypeLayoutSet.getPrivateLayout(),layoutSetPrototypeLayoutSet.getSettings());
}
