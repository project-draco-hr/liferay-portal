{
  if (layoutPrototype == null) {
    throw new IllegalArgumentException("LayoutPrototype has to be not null");
  }
  if (layoutPrototype.getLayoutPrototypeId() == 0) {
    throw new IllegalArgumentException("layoutSetPrototype has to have non-zero layoutPrototypeId: " + layoutPrototype.getLayoutPrototypeId());
  }
  Layout layoutPrototypeLayout=layoutPrototype.getLayout();
  UnicodeProperties prototypeTypeSettingsProperties=layoutPrototypeLayout.getTypeSettingsProperties();
  int mergeFailCount=GetterUtil.getInteger(prototypeTypeSettingsProperties.getProperty(MERGE_FAIL_COUNT));
  return mergeFailCount;
}
