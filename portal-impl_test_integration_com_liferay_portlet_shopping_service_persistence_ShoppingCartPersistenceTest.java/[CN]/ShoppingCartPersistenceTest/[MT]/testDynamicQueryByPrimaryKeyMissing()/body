{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ShoppingCart.class,ShoppingCart.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("cartId",ServiceTestUtil.nextLong()));
  List<ShoppingCart> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
