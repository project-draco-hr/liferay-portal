{
  MirageLoggerUtil.enter(_log,_CLASS_NAME,"getBinaryContents");
  process(criteria);
  ContentSearchInvoker contentSearchInvoker=(ContentSearchInvoker)criteria;
  List<JournalContentSearch> contentSearches=(List<JournalContentSearch>)contentSearchInvoker.getReturnValue();
  List<BinaryContent> binaryContents=new ArrayList<BinaryContent>(contentSearches.size());
  for (  JournalContentSearch contentSearch : contentSearches) {
    binaryContents.add(new MirageJournalContentSearch(contentSearch));
  }
  MirageLoggerUtil.exit(_log,_CLASS_NAME,"getBinaryContents");
  return binaryContents;
}
