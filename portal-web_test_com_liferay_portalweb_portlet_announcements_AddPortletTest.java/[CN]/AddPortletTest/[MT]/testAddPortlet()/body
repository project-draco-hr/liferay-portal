{
  selenium.click("link=Add Application");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("document.getElementById('News-Announcements').getElementsByTagName('p')[0].getElementsByTagName('a')[0]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  Thread.sleep(3000);
  selenium.mouseDown("document.getElementById('News-Announcements').getElementsByTagName('p')[0].getElementsByTagName('a')[0]");
  selenium.mouseUp("document.getElementById('News-Announcements').getElementsByTagName('p')[0].getElementsByTagName('a')[0]");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//img[@title='Configuration']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
