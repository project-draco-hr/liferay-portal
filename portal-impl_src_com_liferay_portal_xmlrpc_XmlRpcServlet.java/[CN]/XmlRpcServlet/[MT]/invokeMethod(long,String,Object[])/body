{
  Method method=_methodRegistry.get(methodName);
  if (method == null) {
    return new Fault(XmlRpcConstants.REQUESTED_METHOD_NOT_FOUND,"Requested method not found");
  }
  if (!method.setArguments(args)) {
    return new Fault(XmlRpcConstants.INVALID_METHOD_PARAMETERS,"Method arguments are invalid");
  }
  return method.execute(companyId);
}
