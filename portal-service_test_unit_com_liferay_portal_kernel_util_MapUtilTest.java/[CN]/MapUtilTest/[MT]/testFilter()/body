{
  Map<String,String> inputMap=new HashMap<String,String>();
  inputMap.put("1","one");
  inputMap.put("2","two");
  inputMap.put("3","three");
  inputMap.put("4","four");
  inputMap.put("5","five");
  Map<String,String> outputMap=MapUtil.filter(inputMap,new PredicateFilter<String>(){
    @Override public boolean filter(    String key){
      return (Integer.valueOf(key).intValue() % 2) == 0;
    }
  }
);
  Assert.assertEquals(2,outputMap.size());
  Assert.assertEquals("two",outputMap.get("2"));
  Assert.assertEquals("four",outputMap.get("4"));
}
