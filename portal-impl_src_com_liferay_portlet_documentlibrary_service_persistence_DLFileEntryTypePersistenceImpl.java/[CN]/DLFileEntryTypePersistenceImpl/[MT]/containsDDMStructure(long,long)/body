{
  Object[] finderArgs=new Object[]{pk,ddmStructurePK};
  Boolean value=(Boolean)FinderCacheUtil.getResult(FINDER_PATH_CONTAINS_DDMSTRUCTURE,finderArgs,this);
  if (value == null) {
    try {
      value=Boolean.valueOf(containsDDMStructure.contains(pk,ddmStructurePK));
      FinderCacheUtil.putResult(FINDER_PATH_CONTAINS_DDMSTRUCTURE,finderArgs,value);
    }
 catch (    Exception e) {
      FinderCacheUtil.removeResult(FINDER_PATH_CONTAINS_DDMSTRUCTURE,finderArgs);
      throw processException(e);
    }
  }
  return value.booleanValue();
}
