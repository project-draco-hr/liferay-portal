{
  Object[] finderArgs=new Object[]{pk,dlFolderPK};
  Boolean value=(Boolean)FinderCacheUtil.getResult(FINDER_PATH_CONTAINS_DLFOLDER,finderArgs,this);
  if (value == null) {
    try {
      value=Boolean.valueOf(containsDLFolder.contains(pk,dlFolderPK));
      FinderCacheUtil.putResult(FINDER_PATH_CONTAINS_DLFOLDER,finderArgs,value);
    }
 catch (    Exception e) {
      FinderCacheUtil.removeResult(FINDER_PATH_CONTAINS_DLFOLDER,finderArgs);
      throw processException(e);
    }
  }
  return value.booleanValue();
}
