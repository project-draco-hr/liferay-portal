{
  Session session=null;
  try {
    session=openSession();
    DLFileEntryType dlFileEntryType=(DLFileEntryType)session.get(DLFileEntryTypeImpl.class,primaryKey);
    if (dlFileEntryType == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchFileEntryTypeException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(dlFileEntryType);
  }
 catch (  NoSuchFileEntryTypeException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
