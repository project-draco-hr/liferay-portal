{
  addStructure(_CLASS_NAME_ID,"Contact");
  addStructure(_CLASS_NAME_ID,"Event");
  List<DDMStructure> structures=DDMStructureLocalServiceUtil.search(TestPropsValues.getCompanyId(),new long[]{group.getGroupId()},_CLASS_NAME_ID,"Contact",null,null,DDMStructureConstants.TYPE_DEFAULT,WorkflowConstants.STATUS_APPROVED,true,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  Assert.assertEquals("Contact",getStructureName(structures.get(0)));
}
