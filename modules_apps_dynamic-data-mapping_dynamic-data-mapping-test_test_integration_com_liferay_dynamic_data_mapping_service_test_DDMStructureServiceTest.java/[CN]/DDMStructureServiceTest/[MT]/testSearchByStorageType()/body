{
  addStructure(_CLASS_NAME_ID,StringUtil.randomString());
  List<DDMStructure> structures=DDMStructureLocalServiceUtil.search(TestPropsValues.getCompanyId(),new long[]{group.getGroupId()},_CLASS_NAME_ID,null,null,StorageType.JSON.toString(),DDMStructureConstants.TYPE_DEFAULT,WorkflowConstants.STATUS_APPROVED,true,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  Assert.assertEquals(1,structures.size());
}
