{
  addStructure(_CLASS_NAME_ID,"Contact","Contact");
  addStructure(_CLASS_NAME_ID,"Event","Event");
  List<DDMStructure> structures=DDMStructureLocalServiceUtil.search(TestPropsValues.getCompanyId(),new long[]{group.getGroupId()},_CLASS_NAME_ID,"Contact","Event",null,DDMStructureConstants.TYPE_DEFAULT,WorkflowConstants.STATUS_APPROVED,true,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
