{
  addStructure(_CLASS_NAME_ID,"Contact","Contact");
  addStructure(_CLASS_NAME_ID,"Event","Event");
  List<DDMStructure> structures=DDMStructureLocalServiceUtil.search(TestPropsValues.getCompanyId(),new long[]{group.getGroupId()},_CLASS_NAME_ID,"Contact","Event",null,DDMStructureConstants.TYPE_DEFAULT,true,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null,WorkflowConstants.STATUS_APPROVED);
