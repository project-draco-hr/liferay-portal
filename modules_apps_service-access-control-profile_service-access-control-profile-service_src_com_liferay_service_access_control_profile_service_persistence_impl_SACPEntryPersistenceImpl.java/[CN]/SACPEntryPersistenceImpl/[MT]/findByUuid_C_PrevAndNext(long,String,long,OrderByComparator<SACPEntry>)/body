{
  SACPEntry sacpEntry=findByPrimaryKey(sacpEntryId);
  Session session=null;
  try {
    session=openSession();
    SACPEntry[] array=new SACPEntryImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,sacpEntry,uuid,companyId,orderByComparator,true);
    array[1]=sacpEntry;
    array[2]=getByUuid_C_PrevAndNext(session,sacpEntry,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
