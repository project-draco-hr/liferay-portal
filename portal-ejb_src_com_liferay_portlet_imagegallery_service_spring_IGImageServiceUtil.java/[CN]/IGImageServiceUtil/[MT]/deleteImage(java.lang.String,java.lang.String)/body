{
  try {
    IGImageService igImageService=IGImageServiceFactory.getService();
    igImageService.deleteImage(companyId,imageId);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
