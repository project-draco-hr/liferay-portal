{
  ShoppingCategory shoppingCategory=(ShoppingCategory)EntityCacheUtil.getResult(ShoppingCategoryModelImpl.ENTITY_CACHE_ENABLED,ShoppingCategoryImpl.class,categoryId,this);
  if (shoppingCategory == null) {
    Session session=null;
    try {
      session=openSession();
      shoppingCategory=(ShoppingCategory)session.get(ShoppingCategoryImpl.class,Long.valueOf(categoryId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (shoppingCategory != null) {
        cacheResult(shoppingCategory);
      }
      closeSession(session);
    }
  }
  return shoppingCategory;
}
