{
  LayoutBranch layoutBranch=findByPrimaryKey(branchId);
  Session session=null;
  try {
    session=openSession();
    LayoutBranch[] array=new LayoutBranchImpl[3];
    array[0]=getByG_PrevAndNext(session,layoutBranch,groupId,orderByComparator,true);
    array[1]=layoutBranch;
    array[2]=getByG_PrevAndNext(session,layoutBranch,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
