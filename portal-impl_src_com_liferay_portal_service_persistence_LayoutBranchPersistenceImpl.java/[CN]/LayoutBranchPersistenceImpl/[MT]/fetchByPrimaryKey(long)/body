{
  LayoutBranch layoutBranch=(LayoutBranch)EntityCacheUtil.getResult(LayoutBranchModelImpl.ENTITY_CACHE_ENABLED,LayoutBranchImpl.class,branchId,this);
  if (layoutBranch == null) {
    Session session=null;
    try {
      session=openSession();
      layoutBranch=(LayoutBranch)session.get(LayoutBranchImpl.class,new Long(branchId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (layoutBranch != null) {
        cacheResult(layoutBranch);
      }
      closeSession(session);
    }
  }
  return layoutBranch;
}
