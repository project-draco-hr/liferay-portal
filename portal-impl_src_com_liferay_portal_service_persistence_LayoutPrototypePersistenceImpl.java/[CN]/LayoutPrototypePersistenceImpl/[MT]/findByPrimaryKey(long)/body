{
  LayoutPrototype layoutPrototype=fetchByPrimaryKey(layoutPrototypeId);
  if (layoutPrototype == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No LayoutPrototype exists with the primary key " + layoutPrototypeId);
    }
    throw new NoSuchLayoutPrototypeException("No LayoutPrototype exists with the primary key " + layoutPrototypeId);
  }
  return layoutPrototype;
}
