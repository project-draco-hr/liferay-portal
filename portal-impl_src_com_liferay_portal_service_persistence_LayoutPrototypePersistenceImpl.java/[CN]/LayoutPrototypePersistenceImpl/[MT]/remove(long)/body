{
  Session session=null;
  try {
    session=openSession();
    LayoutPrototype layoutPrototype=(LayoutPrototype)session.get(LayoutPrototypeImpl.class,new Long(layoutPrototypeId));
    if (layoutPrototype == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + layoutPrototypeId);
      }
      throw new NoSuchLayoutPrototypeException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + layoutPrototypeId);
    }
    return remove(layoutPrototype);
  }
 catch (  NoSuchLayoutPrototypeException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
