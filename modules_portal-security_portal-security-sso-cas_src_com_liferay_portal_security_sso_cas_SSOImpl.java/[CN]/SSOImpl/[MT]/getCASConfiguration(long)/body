{
  try {
    CASConfiguration casCompanyServiceSettings=_moduleConfigurationFactory.getConfiguration(CASConfiguration.class,new CompanyServiceSettingsLocator(companyId,CASConstants.SERVICE_NAME));
    return casCompanyServiceSettings;
  }
 catch (  SettingsException se) {
    _log.error("Unable to get CAS configuration",se);
  }
  return null;
}
