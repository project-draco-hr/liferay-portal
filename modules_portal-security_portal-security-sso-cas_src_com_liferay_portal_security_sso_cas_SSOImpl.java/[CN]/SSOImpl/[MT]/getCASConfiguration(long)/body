{
  try {
    CASConfiguration casCompanyServiceSettings=_moduleConfigurationFactory.getModuleConfiguration(CASConfiguration.class,new CompanyServiceSettingsLocator(companyId,CASConstants.SERVICE_NAME));
    return casCompanyServiceSettings;
  }
 catch (  ModuleConfigurationException mce) {
    _log.error("Unable to get CAS configuration",mce);
  }
  return null;
}
