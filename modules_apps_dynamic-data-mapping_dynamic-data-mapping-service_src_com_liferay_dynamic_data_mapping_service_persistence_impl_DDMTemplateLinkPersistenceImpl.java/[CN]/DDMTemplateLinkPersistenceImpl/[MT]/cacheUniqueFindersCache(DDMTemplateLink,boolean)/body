{
  if (isNew) {
    Object[] args=new Object[]{ddmTemplateLink.getClassNameId(),ddmTemplateLink.getClassPK()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_C_C,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_C,args,ddmTemplateLink);
  }
 else {
    DDMTemplateLinkModelImpl ddmTemplateLinkModelImpl=(DDMTemplateLinkModelImpl)ddmTemplateLink;
    if ((ddmTemplateLinkModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_C_C.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{ddmTemplateLink.getClassNameId(),ddmTemplateLink.getClassPK()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_C_C,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_C,args,ddmTemplateLink);
    }
  }
}
