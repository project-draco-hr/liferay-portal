{
  for (  DDMTemplateLink ddmTemplateLink : ddmTemplateLinks) {
    if (EntityCacheUtil.getResult(DDMTemplateLinkModelImpl.ENTITY_CACHE_ENABLED,DDMTemplateLinkImpl.class,ddmTemplateLink.getPrimaryKey()) == null) {
      cacheResult(ddmTemplateLink);
    }
 else {
      ddmTemplateLink.resetOriginalValues();
    }
  }
}
