{
  StringBundler sb=new StringBundler(3);
  sb.append(companyName);
  sb.append(StringPool.PERIOD);
  sb.append(ServiceTestUtil.randomString(3));
  String virtualHostname=sb.toString();
  return CompanyLocalServiceUtil.addCompany(companyName,virtualHostname,virtualHostname,PropsValues.SHARD_DEFAULT_NAME,false,0,true);
}
