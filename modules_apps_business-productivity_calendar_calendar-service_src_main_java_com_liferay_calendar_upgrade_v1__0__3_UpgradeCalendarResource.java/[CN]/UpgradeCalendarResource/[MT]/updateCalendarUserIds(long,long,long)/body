{
  try (PreparedStatement ps=connection.prepareStatement("select Calendar.calendarId from Calendar, CalendarResource " + "where CalendarResource.classNameId = ? " + "and CalendarResource.userId = ?")){
    ps.setLong(1,groupClassNameId);
    ps.setLong(2,defaultUserId);
    try (ResultSet rs=ps.executeQuery()){
      while (rs.next()) {
        long calendarId=rs.getLong(1);
        updateCalendarUserId(calendarId,adminUserId);
      }
    }
   }
 }
