{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < folderIds.size(); i++) {
    sb.append(table);
    sb.append(".");
    if (table.equals("DLFolder")) {
      sb.append("parentFolderId");
    }
 else {
      sb.append("folderId");
    }
    sb.append("= ? ");
    if ((i + 1) != folderIds.size()) {
      sb.append("OR ");
    }
  }
  return sb.toString();
}
