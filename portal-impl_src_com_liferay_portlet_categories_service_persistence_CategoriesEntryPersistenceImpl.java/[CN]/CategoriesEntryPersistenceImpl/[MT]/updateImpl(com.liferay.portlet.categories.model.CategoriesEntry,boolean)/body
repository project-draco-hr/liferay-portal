{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,categoriesEntry,merge);
    categoriesEntry.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.putResult(CategoriesEntryModelImpl.ENTITY_CACHE_ENABLED,CategoriesEntryImpl.class,categoriesEntry.getPrimaryKey(),categoriesEntry);
  return categoriesEntry;
}
