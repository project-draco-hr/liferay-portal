{
  String host=PortalUtil.getHost(request);
  String portalURL=PortalUtil.getPortalURL(host,request.getServerPort(),request.isSecure());
  String pathImage=PortalUtil.getPathImage();
  if (pathImage.startsWith(Http.HTTP_WITH_SLASH) || pathImage.startsWith(Http.HTTPS_WITH_SLASH)) {
    return pathImage + "/software_catalog";
  }
 else {
    return portalURL + pathImage + "/software_catalog";
  }
}
