{
  try {
    if (_job == null) {
      JobDetail jobDetail=context.getJobDetail();
      Class<IntervalJob> intervalJobClass=JobClassUtil.get(jobDetail.getName());
      _classLoader=intervalJobClass.getClassLoader();
      _job=(IntervalJob)_classLoader.loadClass(intervalJobClass.getName()).newInstance();
    }
    Thread currentThread=Thread.currentThread();
    ClassLoader contextClassLoader=currentThread.getContextClassLoader();
    try {
      currentThread.setContextClassLoader(_classLoader);
      for (      String shardName : PropsValues.SHARD_AVAILABLE_NAMES) {
        ShardUtil.pushCompanyService(shardName);
        try {
          _job.execute(new JobExecutionContextImpl(context));
        }
  finally {
          ShardUtil.popCompanyService();
        }
      }
    }
  finally {
      currentThread.setContextClassLoader(contextClassLoader);
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
