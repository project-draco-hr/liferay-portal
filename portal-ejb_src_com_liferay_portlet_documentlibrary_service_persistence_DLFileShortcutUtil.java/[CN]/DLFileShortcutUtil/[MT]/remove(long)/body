{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(fileShortcutId));
  }
  com.liferay.portlet.documentlibrary.model.DLFileShortcut dlFileShortcut=getPersistence().remove(fileShortcutId);
  if (listener != null) {
    listener.onAfterRemove(dlFileShortcut);
  }
  return dlFileShortcut;
}
