{
  Matcher matcher=_validatorEqualsPattern.matcher(content);
  if (!matcher.find()) {
    return content;
  }
  content=StringUtil.replaceFirst(content,"Validator.equals(","Objects.equals(");
  if (content.contains("import java.util.Objects;")) {
    return content;
  }
  int pos=content.indexOf("\npackage ");
  pos=content.indexOf("\n",pos + 1);
  return StringUtil.insert(content,"import java.util.Objects;\n",pos + 1);
}
