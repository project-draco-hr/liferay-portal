{
  if (fileName.endsWith("Test.java")) {
    return;
  }
  int x=fileName.lastIndexOf(CharPool.SLASH);
  if ((fileName.indexOf("Upgrade",x) == -1) && (fileName.indexOf("Verify",x) == -1)) {
    return;
  }
  if (isExcludedPath(_upgradeDataAccessConnectionExcludes,fileName)) {
    return;
  }
  if (content.contains("ThrowableAwareRunnable")) {
    return;
  }
  x=content.indexOf("DataAccess.getUpgradeOptimizedConnection");
  while (x != -1) {
    int lineCount=getLineCount(content,x);
    processErrorMessage(fileName,"Use connection field " + fileName + " "+ lineCount);
    x=content.indexOf("DataAccess.getUpgradeOptimizedConnection",x + 40);
  }
}
