{
  Tuple bndFileLocationAndContentTuple=getBNDFileLocationAndContentTuple(fileName);
  String bndFileLocation=(String)bndFileLocationAndContentTuple.getObject(0);
  Tuple bndInheritTuple=_bndInheritRequiredTupleMap.get(bndFileLocation);
  if ((bndInheritTuple == null) || bndInheritRequired) {
    String bndContent=(String)bndFileLocationAndContentTuple.getObject(1);
    _bndInheritRequiredTupleMap.put(bndFileLocation,new Tuple(bndContent,bndInheritRequired));
  }
}
