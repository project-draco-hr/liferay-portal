{
  if ((_type == TYPE_CLASS_PRIVATE_STATIC) || (_type == TYPE_CLASS_PROTECTED_STATIC) || (_type == TYPE_CLASS_PUBLIC_STATIC)|| (_type == TYPE_METHOD_PRIVATE_STATIC)|| (_type == TYPE_METHOD_PROTECTED_STATIC)|| (_type == TYPE_METHOD_PUBLIC_STATIC)|| (_type == TYPE_VARIABLE_PRIVATE_STATIC)|| (_type == TYPE_VARIABLE_PROTECTED_STATIC)|| (_type == TYPE_VARIABLE_PUBLIC_STATIC)) {
    return true;
  }
 else {
    return false;
  }
}
