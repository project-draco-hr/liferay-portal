{
  String portletId=PortletProviderUtil.getPortletId(DLFileEntryType.class.getName(),PortletProvider.Action.EDIT);
  Boolean hasPermission=StagingPermissionUtil.hasPermission(permissionChecker,fileEntryType.getGroupId(),DLFileEntryType.class.getName(),fileEntryType.getFileEntryTypeId(),portletId,actionId);
  if (hasPermission != null) {
    return hasPermission.booleanValue();
  }
  if (permissionChecker.hasOwnerPermission(fileEntryType.getCompanyId(),DLFileEntryType.class.getName(),fileEntryType.getFileEntryTypeId(),fileEntryType.getUserId(),actionId)) {
    return true;
  }
  return permissionChecker.hasPermission(fileEntryType.getGroupId(),DLFileEntryType.class.getName(),fileEntryType.getFileEntryTypeId(),actionId);
}
