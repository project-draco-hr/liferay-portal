{
  JournalSyndicatedFeedPermission.check(getPermissionChecker(),groupId,feedId,ActionKeys.UPDATE);
  return journalSyndicatedFeedLocalService.updateSyndicatedFeed(groupId,feedId,name,description,type,structureId,templateId,rendererTemplateId,delta,orderByCol,orderByType,targetLayoutFriendlyUrl,targetPortletId,contentField,feedType,feedVersion);
}
