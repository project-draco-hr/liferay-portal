{
  ResourceRequestImpl resourceRequestImpl=null;
  if (PropsValues.COMMONS_POOL_ENABLED) {
    resourceRequestImpl=(ResourceRequestImpl)_instance._pool.borrowObject();
  }
 else {
    resourceRequestImpl=new ResourceRequestImpl();
  }
  resourceRequestImpl.init(req,portlet,invokerPortlet,portletCtx,windowState,portletMode,prefs,plid);
  return resourceRequestImpl;
}
