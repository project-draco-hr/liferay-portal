{
  ResourceRequestImpl resourceReqImpl=null;
  if (PropsValues.COMMONS_POOL_ENABLED) {
    resourceReqImpl=(ResourceRequestImpl)_instance._pool.borrowObject();
  }
 else {
    resourceReqImpl=new ResourceRequestImpl();
  }
  resourceReqImpl.init(req,portlet,invokerPortlet,portletCtx,windowState,portletMode,prefs,plid);
  return resourceReqImpl;
}
