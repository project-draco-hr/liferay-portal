{
  String themeParent=getThemeParent();
  if (Validator.isNull(themeParent) || ArrayUtil.contains(_PORTAL_THEMES,themeParent)) {
    return null;
  }
  if (_themeParentProject == null) {
    File themeParentDir=_project.file(themeParent);
    _themeParentProject=GradleUtil.getProject(_project.getRootProject(),themeParentDir);
  }
  return _themeParentProject;
}
