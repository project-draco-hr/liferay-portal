{
  ByteArrayInputStream inputStream=new ByteArrayInputStream(bytes);
  FlashMagicBytesUtil.Result result=FlashMagicBytesUtil.check(inputStream);
  Assert.assertEquals(expectFlash,result.isFlash());
  InputStream returnedInputStream=result.getInputStream();
  byte[] buffer=new byte[4096];
  int size=returnedInputStream.read(buffer);
  if (size == _EOF) {
    Assert.assertEquals(0,bytes.length);
    return;
  }
  Assert.assertEquals(bytes.length,size);
}
