{
  int start=0;
  int end=SEARCH_INTERVAL;
  int total=RoleLocalServiceUtil.getRolesByTypeCount(RoleConstants.TYPE_REGULAR);
  while (start <= total) {
    List<Role> roles=RoleLocalServiceUtil.getRolesByType(RoleConstants.TYPE_REGULAR,start,end);
    for (    Role role : roles) {
      verifyPolicy(role);
    }
    start=end;
    end+=end;
  }
}
