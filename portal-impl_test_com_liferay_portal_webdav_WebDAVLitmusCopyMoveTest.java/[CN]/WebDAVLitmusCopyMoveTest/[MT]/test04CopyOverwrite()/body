{
  Tuple tuple=serviceCopyOrMove(Method.COPY,"copysrc",null,"copydest",0,false);
  assertEquals(HttpServletResponse.SC_PRECONDITION_FAILED,getStatusCode(tuple));
  tuple=serviceCopyOrMove(Method.COPY,"copysrc",null,"copydest",0,true);
  assertEquals(HttpServletResponse.SC_NO_CONTENT,getStatusCode(tuple));
  tuple=serviceCopyOrMove(Method.COPY,"copysrc",null,"copycoll",0,true);
  assertEquals(HttpServletResponse.SC_NO_CONTENT,getStatusCode(tuple));
}
