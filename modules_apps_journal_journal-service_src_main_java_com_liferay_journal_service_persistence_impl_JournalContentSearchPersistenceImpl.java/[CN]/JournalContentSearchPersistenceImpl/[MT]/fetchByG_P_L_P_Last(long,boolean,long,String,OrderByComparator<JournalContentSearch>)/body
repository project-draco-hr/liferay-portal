{
  int count=countByG_P_L_P(groupId,privateLayout,layoutId,portletId);
  if (count == 0) {
    return null;
  }
  List<JournalContentSearch> list=findByG_P_L_P(groupId,privateLayout,layoutId,portletId,count - 1,count,orderByComparator);
  if (!list.isEmpty()) {
    return list.get(0);
  }
  return null;
}
