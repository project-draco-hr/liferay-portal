{
  if (userId <= 0) {
    return null;
  }
  LinkedHashMap<String,String> userInfo=new LinkedHashMap<String,String>();
  try {
    User user=UserLocalServiceUtil.getUserById(userId);
    userInfo=getUserInfo(user,userInfo,portlet);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return userInfo;
}
