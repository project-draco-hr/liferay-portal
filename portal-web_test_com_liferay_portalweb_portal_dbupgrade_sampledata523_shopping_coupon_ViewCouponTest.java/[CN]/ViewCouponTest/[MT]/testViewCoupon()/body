{
  selenium.open("/user/joebloggs/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Communities I Own")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Communities I Own",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_29_name",RuntimeVariables.replace("Shopping Coupon Community"));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//td[2]/a",RuntimeVariables.replace("Open"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Shopping Coupon Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Coupons",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//td[2]/a",RuntimeVariables.replace("Coupon Code"));
  selenium.waitForPageToLoad("30000");
  assertEquals("Coupon Test",selenium.getValue("_34_name"));
  assertEquals("This is a coupon test.",selenium.getValue("_34_description"));
  assertEquals("$0.00",selenium.getValue("_34_minOrder"));
  assertEquals("0.50",selenium.getValue("_34_discount"));
  assertEquals("Percentage",selenium.getSelectedLabel("_34_discountType"));
}
