{
  Session session=null;
  try {
    session=openSession();
    session.delete(tasksReview);
    session.flush();
    return tasksReview;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(TasksReview.class.getName());
  }
}
