{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,tasksReview,merge);
    tasksReview.setNew(false);
    return tasksReview;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(TasksReview.class.getName());
  }
}
