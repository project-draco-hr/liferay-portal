{
  Session session=null;
  try {
    session=openSession();
    TasksReview tasksReview=(TasksReview)session.get(TasksReviewImpl.class,new Long(reviewId));
    if (tasksReview == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No TasksReview exists with the primary key " + reviewId);
      }
      throw new NoSuchReviewException("No TasksReview exists with the primary key " + reviewId);
    }
    return remove(tasksReview);
  }
 catch (  NoSuchReviewException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
