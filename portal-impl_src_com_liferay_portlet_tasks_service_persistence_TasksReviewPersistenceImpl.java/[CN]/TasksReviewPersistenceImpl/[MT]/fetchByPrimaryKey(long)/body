{
  TasksReview tasksReview=(TasksReview)EntityCacheUtil.getResult(TasksReviewModelImpl.ENTITY_CACHE_ENABLED,TasksReviewImpl.class,reviewId,this);
  if (tasksReview == null) {
    Session session=null;
    try {
      session=openSession();
      tasksReview=(TasksReview)session.get(TasksReviewImpl.class,new Long(reviewId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (tasksReview != null) {
        cacheResult(tasksReview);
      }
      closeSession(session);
    }
  }
  return tasksReview;
}
