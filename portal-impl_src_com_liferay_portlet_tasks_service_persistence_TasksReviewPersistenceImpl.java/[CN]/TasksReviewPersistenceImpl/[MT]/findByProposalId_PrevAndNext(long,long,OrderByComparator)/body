{
  TasksReview tasksReview=findByPrimaryKey(reviewId);
  Session session=null;
  try {
    session=openSession();
    TasksReview[] array=new TasksReviewImpl[3];
    array[0]=getByProposalId_PrevAndNext(session,tasksReview,proposalId,orderByComparator,true);
    array[1]=tasksReview;
    array[2]=getByProposalId_PrevAndNext(session,tasksReview,proposalId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
