{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Polls",RuntimeVariables.replace("Polls"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Test Poll Question",RuntimeVariables.replace("Test Poll Question"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Area",RuntimeVariables.replace("Area"));
  selenium.waitForPopUp("viewChart",RuntimeVariables.replace("30000"));
  selenium.selectWindow("viewChart");
  selenium.close();
  selenium.selectWindow("null");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Horizontal Bar")) {
        break;
      }
