{
  PortletPermissionUtil.check(getPermissionChecker(),plid,PortletKeys.SHOPPING,ActionKeys.MANAGE_COUPONS);
  return ShoppingCouponLocalServiceUtil.updateCoupon(getUserId(),couponId,name,description,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,endDateHour,endDateMinute,neverExpire,active,limitCategories,limitSkus,minOrder,discount,discountType);
}
