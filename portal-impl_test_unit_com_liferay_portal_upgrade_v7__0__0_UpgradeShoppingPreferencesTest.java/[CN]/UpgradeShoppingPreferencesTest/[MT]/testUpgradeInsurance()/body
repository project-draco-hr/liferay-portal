{
  _portletPreferences.setValue("insurance","1,2,3,4,5");
  _upgradeShoppingPreferences.upgradeInsurance(_portletPreferences);
  String[] insurance=_portletPreferences.getValues("insurance",StringPool.EMPTY_ARRAY);
  Assert.assertArrayEquals(new String[]{"1","2","3","4","5"},insurance);
  Assert.assertEquals("1",_portletPreferences.getValue("insurance",StringPool.BLANK));
}
