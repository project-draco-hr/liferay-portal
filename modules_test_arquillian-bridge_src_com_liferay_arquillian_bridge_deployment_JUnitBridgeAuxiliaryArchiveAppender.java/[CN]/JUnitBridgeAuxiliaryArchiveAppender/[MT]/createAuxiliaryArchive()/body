{
  JavaArchive archive=ShrinkWrap.create(JavaArchive.class,"arquillian-junit-bridge.jar");
  archive.add(EmptyAsset.INSTANCE,"/arquillian.remote.marker");
  archive.addAsServiceProviderAndClasses(RemoteLoadableExtension.class,LiferayArquillianExtension.class);
  archive.addClass(JUnitBridgeObserver.class);
  return archive;
}
