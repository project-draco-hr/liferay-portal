{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    WikiNode node=ActionUtil.getNode(portletRequest);
    if (WikiNodePermissionChecker.contains(themeDisplay.getPermissionChecker(),node,ActionKeys.DELETE) && (_wikiNodeService.getNodesCount(themeDisplay.getScopeGroupId()) > 1)) {
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
