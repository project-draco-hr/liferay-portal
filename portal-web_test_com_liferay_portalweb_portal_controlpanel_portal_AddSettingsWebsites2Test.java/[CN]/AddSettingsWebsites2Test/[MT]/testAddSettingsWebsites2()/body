{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Settings")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Settings"));
  selenium.waitForPageToLoad("30000");
  selenium.click("websitesLink");
  selenium.click("//div[@id='websites']/fieldset/div[2]/div/span/a[1]");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("_130_websiteUrl2")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.typeKeys("_130_websiteUrl2",RuntimeVariables.replace("http://www.digg.com"));
  selenium.type("_130_websiteUrl2",RuntimeVariables.replace("http://www.digg.com"));
  selenium.select("_130_websiteTypeId2",RuntimeVariables.replace("label=Intranet"));
  selenium.click("_130_websitePrimary2");
  selenium.click(RuntimeVariables.replace("//input[@value='Save']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
}
