{
  List<SCProductEntry> productEntries=SCProductEntryLocalServiceUtil.getCompanyProductEntries(companyId,start,end);
  if (productEntries.isEmpty()) {
    return;
  }
  Collection<Document> documents=new ArrayList<Document>();
  for (  SCProductEntry productEntry : productEntries) {
    Document document=getDocument(productEntry);
    documents.add(document);
  }
  SearchEngineUtil.updateDocuments(getSearchEngineId(),companyId,documents);
}
