{
  StringBundler sb=new StringBundler(6);
  sb.append(portletDataContext.getPortletPath(PortletKeys.JOURNAL));
  sb.append("/articles/");
  sb.append(article.getArticleResourceUuid());
  sb.append(StringPool.SLASH);
  sb.append(article.getVersion());
  sb.append(StringPool.SLASH);
  return sb.toString();
}
