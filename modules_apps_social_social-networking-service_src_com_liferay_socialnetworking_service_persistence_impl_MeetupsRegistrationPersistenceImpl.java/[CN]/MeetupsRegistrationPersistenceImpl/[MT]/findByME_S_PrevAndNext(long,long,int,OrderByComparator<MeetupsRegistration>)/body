{
  MeetupsRegistration meetupsRegistration=findByPrimaryKey(meetupsRegistrationId);
  Session session=null;
  try {
    session=openSession();
    MeetupsRegistration[] array=new MeetupsRegistrationImpl[3];
    array[0]=getByME_S_PrevAndNext(session,meetupsRegistration,meetupsEntryId,status,orderByComparator,true);
    array[1]=meetupsRegistration;
    array[2]=getByME_S_PrevAndNext(session,meetupsRegistration,meetupsEntryId,status,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
