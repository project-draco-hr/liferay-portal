{
  _thread=null;
  try {
    for (    Map.Entry<Thread,Exception> entry : _concurrentFailures.entrySet()) {
      Thread thread=entry.getKey();
      Exception exception=entry.getValue();
      Assert.fail("Thread " + thread + " caught concurrent failure: "+ exception);
      ReflectionUtil.throwException(exception);
    }
  }
  finally {
    _concurrentFailures.clear();
  }
}
