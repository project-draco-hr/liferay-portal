{
  mdrAction=toUnwrappedModel(mdrAction);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,mdrAction);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  MDRActionModelImpl mdrActionModelImpl=(MDRActionModelImpl)mdrAction;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{mdrActionModelImpl.getUuid(),Long.valueOf(mdrActionModelImpl.getGroupId())});
  EntityCacheUtil.removeResult(MDRActionModelImpl.ENTITY_CACHE_ENABLED,MDRActionImpl.class,mdrAction.getPrimaryKey());
  return mdrAction;
}
