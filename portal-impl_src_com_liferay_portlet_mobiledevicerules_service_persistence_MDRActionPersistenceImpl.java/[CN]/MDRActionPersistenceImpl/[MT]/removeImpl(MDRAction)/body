{
  mdrAction=toUnwrappedModel(mdrAction);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,mdrAction);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(mdrAction);
  return mdrAction;
}
