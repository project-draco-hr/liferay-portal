{
  EntityCacheUtil.removeResult(MDRActionModelImpl.ENTITY_CACHE_ENABLED,MDRActionImpl.class,mdrAction.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{mdrAction.getUuid(),Long.valueOf(mdrAction.getGroupId())});
}
