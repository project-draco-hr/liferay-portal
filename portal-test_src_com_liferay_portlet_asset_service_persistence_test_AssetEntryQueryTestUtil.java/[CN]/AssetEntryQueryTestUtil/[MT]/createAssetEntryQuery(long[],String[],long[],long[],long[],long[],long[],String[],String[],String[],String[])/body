{
  long[] classNameIds=new long[classNames.length];
  for (int i=0; i < classNames.length; i++) {
    classNameIds[i]=PortalUtil.getClassNameId(classNames[i]);
  }
  return createAssetEntryQuery(groupIds,classNameIds,classTypeIds,notAllCategories,notAnyCategories,allCategories,anyCategories,notAllTags,notAnyTags,allTags,anyTags);
}
