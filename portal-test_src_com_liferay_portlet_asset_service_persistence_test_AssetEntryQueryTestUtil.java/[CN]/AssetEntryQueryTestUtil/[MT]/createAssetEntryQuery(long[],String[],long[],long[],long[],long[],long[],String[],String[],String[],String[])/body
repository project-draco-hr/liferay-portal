{
  long[] classNameIds=new long[classNames.length];
  for (int i=0; i < classNames.length; i++) {
    classNameIds[i]=PortalUtil.getClassNameId(classNames[i]);
  }
  return createAssetEntryQuery(groupIds,classNameIds,classTypeIds,notAllCategoryIds,notAnyCategoryIds,allCategoryIds,anyCategoryIds,notAllTagNames,notAnyTagNames,allTagNames,anyTagNames);
}
