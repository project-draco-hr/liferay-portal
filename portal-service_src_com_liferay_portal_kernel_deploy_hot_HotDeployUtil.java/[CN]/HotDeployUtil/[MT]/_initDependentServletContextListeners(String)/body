{
  Set<DependentServletContext> dependentServletContexts=_dependentServletContexts.get(servletContextName);
  if ((dependentServletContexts == null) || dependentServletContexts.isEmpty()) {
    return;
  }
  Thread currentThread=Thread.currentThread();
  ClassLoader contextClassLoader=currentThread.getContextClassLoader();
  try {
    for (    DependentServletContext dependentServletContext : dependentServletContexts) {
      _initDependentServletContextListener(currentThread,dependentServletContext);
    }
  }
  finally {
    currentThread.setContextClassLoader(contextClassLoader);
  }
}
