{
  Iterator<String> dependentContextKeysIter=_dependentServletContexts.keySet().iterator();
  while (dependentContextKeysIter.hasNext()) {
    Set<DependentServletContext> dependentServletContexts=_dependentServletContexts.get(dependentContextKeysIter.next());
    Iterator<DependentServletContext> dependentServletContextsIter=dependentServletContexts.iterator();
    while (dependentServletContextsIter.hasNext()) {
      DependentServletContext dependentServletContext=dependentServletContextsIter.next();
      HotDeployEvent hotDeployEvent=dependentServletContext.getHotDeployEvent();
      if (isMissingDependentServletContext(hotDeployEvent)) {
        continue;
      }
      Thread currentThread=Thread.currentThread();
      ClassLoader contextClassLoader=currentThread.getContextClassLoader();
      try {
        _initDependentServletContextListener(currentThread,dependentServletContext);
      }
  finally {
        currentThread.setContextClassLoader(contextClassLoader);
      }
      dependentServletContextsIter.remove();
    }
    if (dependentServletContexts.isEmpty()) {
      dependentContextKeysIter.remove();
    }
  }
}
