{
  String resourceName=Layout.class.getName();
  importGroupRoles(companyId,groupId,resourceName,"community",rolesEl);
  importUserRoles(companyId,groupId,resourceName,rolesEl);
  importInheritedRoles(companyId,groupId,resourceName,"organization",rolesEl);
  importInheritedRoles(companyId,groupId,resourceName,"user-group",rolesEl);
}
