{
  Element permissionsElement=layoutElement.element("permissions");
  if (permissionsElement != null) {
    String resourceName=Layout.class.getName();
    String resourcePrimKey=String.valueOf(layout.getPlid());
    boolean isPrivate=layout.isPrivateLayout();
    importPermissions(layoutCache,companyId,groupId,userId,resourceName,resourcePrimKey,permissionsElement,isPrivate,false);
  }
}
