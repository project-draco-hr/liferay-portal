{
  Element entityRolesEl=parentEl.element(entityName + "-roles");
  if (entityRolesEl == null) {
    return;
  }
  List<Element> entityEls=entityRolesEl.elements(entityName);
  for (int i=0; i < entityEls.size(); i++) {
    Element entityEl=entityEls.get(i);
    String name=entityEl.attributeValue("name");
    long entityGroupId=_layoutCache.getEntityGroupId(companyId,entityName,name);
    if (entityGroupId == 0) {
      _log.warn("Ignore inherited roles for entity " + entityName + " with name "+ name);
    }
 else {
      importRolePermissions(companyId,resourceName,ResourceConstants.SCOPE_GROUP,String.valueOf(groupId),entityEl,false);
    }
  }
}
