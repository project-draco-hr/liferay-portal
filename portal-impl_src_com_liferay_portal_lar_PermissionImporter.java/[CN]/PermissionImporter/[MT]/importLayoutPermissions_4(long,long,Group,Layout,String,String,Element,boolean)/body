{
  importGroupPermissions(companyId,groupId,resourceName,resourcePrimKey,permissionsEl,"community-actions",false);
  if (groupId != guestGroup.getGroupId()) {
    importGroupPermissions(companyId,guestGroup.getGroupId(),resourceName,resourcePrimKey,permissionsEl,"guest-actions",false);
  }
  if (importUserPermissions) {
    importUserPermissions(companyId,groupId,resourceName,resourcePrimKey,permissionsEl,false);
  }
  importInheritedPermissions(companyId,resourceName,resourcePrimKey,permissionsEl,"organization",false);
  importInheritedPermissions(companyId,resourceName,resourcePrimKey,permissionsEl,"user-group",false);
}
