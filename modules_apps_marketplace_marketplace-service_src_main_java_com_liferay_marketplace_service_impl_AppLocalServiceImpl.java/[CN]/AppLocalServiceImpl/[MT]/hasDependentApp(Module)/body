{
  List<Module> modules=modulePersistence.findByContextName(module.getContextName());
  for (  Module curModule : modules) {
    App app=appPersistence.findByPrimaryKey(curModule.getAppId());
    if (curModule.getAppId() == module.getAppId()) {
      continue;
    }
    if (app.isInstalled()) {
      return true;
    }
  }
  return false;
}
