{
  Account account=(Account)EntityCacheUtil.getResult(AccountModelImpl.ENTITY_CACHE_ENABLED,AccountImpl.class,accountId,this);
  if (account == null) {
    Session session=null;
    try {
      session=openSession();
      account=(Account)session.get(AccountImpl.class,new Long(accountId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (account != null) {
        cacheResult(account);
      }
      closeSession(session);
    }
  }
  return account;
}
