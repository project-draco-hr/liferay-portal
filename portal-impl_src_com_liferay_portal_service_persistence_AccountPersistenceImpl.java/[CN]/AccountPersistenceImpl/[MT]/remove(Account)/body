{
  Session session=null;
  try {
    session=openSession();
    account=(Account)session.merge(account);
    session.delete(account);
    session.flush();
    return account;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Account.class.getName());
  }
}
