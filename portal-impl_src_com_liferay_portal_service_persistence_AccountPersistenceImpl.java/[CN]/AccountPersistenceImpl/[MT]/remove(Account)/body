{
  FinderCache.clearCache(Account.class.getName());
  Session session=null;
  try {
    session=openSession();
    session.delete(account);
    session.flush();
    return account;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
