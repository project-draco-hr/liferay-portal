{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,account,merge);
    account.setNew(false);
    return account;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Account.class.getName());
  }
}
