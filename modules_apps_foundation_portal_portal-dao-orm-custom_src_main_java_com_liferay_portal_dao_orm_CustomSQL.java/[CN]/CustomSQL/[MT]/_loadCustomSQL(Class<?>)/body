{
  try {
    ClassLoader classLoader=clazz.getClassLoader();
    BundleContext bundleContext=getBundleContext(clazz);
    String[] configs=getConfigs();
    for (    String config : configs) {
      read(bundleContext,classLoader,config);
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
