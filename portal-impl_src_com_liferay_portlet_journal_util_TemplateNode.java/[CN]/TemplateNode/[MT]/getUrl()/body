{
  if (getType().equals("link_to_layout")) {
    StringBuilder sb=new StringBuilder();
    sb.append("@friendly_url_current@");
    sb.append(StringPool.SLASH);
    sb.append("@group_id@");
    sb.append(StringPool.SLASH);
    sb.append(getData());
    return sb.toString();
  }
 else {
    return StringPool.BLANK;
  }
}
