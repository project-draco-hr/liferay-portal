{
  if (getType().equals("link_to_layout")) {
    StringMaker sm=new StringMaker();
    sm.append(PropsValues.LAYOUT_FRIENDLY_URL_PRIVATE_GROUP_SERVLET_MAPPING);
    sm.append(StringPool.SLASH);
    sm.append("@group_id@");
    sm.append(StringPool.SLASH);
    sm.append(getData());
    return sm.toString();
  }
 else {
    return StringPool.BLANK;
  }
}
