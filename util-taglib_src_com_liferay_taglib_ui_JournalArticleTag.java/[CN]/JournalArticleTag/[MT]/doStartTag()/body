{
  ServletRequest req=pageContext.getRequest();
  req.setAttribute("liferay-ui:journal-article:classPK",String.valueOf(_classPK));
  req.setAttribute("liferay-ui:journal-article:languageId",_languageId);
  req.setAttribute("liferay-ui:journal-article:articlePage",String.valueOf(_articlePage));
  req.setAttribute("liferay-ui:journal-article:xmlRequest",_xmlRequest);
  req.setAttribute("liferay-ui:journal-article:showTitle",String.valueOf(_showTitle));
  req.setAttribute("liferay-ui:journal-article:showAvailableLocales",String.valueOf(_showAvailableLocales));
  return EVAL_BODY_BUFFERED;
}
