  request.setAttribute("liferay-ui:journal-article:languageId",_languageId);
  request.setAttribute("liferay-ui:journal-article:portletRequestModel",_portletRequestModel);
  request.setAttribute("liferay-ui:journal-article:showAvailableLocales",String.valueOf(_showAvailableLocales));
  request.setAttribute("liferay-ui:journal-article:showTitle",String.valueOf(_showTitle));
}
