{
  if (folder.getParentFolderId() != JournalFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,folder.getParentFolder());
  }
  Element folderElement=portletDataContext.getExportDataElement(folder);
  portletDataContext.addClassedModel(folderElement,ExportImportPathUtil.getModelPath(folder),folder,JournalPortletDataHandler.NAMESPACE);
}
