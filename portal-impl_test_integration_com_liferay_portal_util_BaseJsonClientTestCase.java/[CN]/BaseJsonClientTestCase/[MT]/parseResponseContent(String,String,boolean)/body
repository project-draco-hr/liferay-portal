{
  variable="\"" + variable + "\"";
  int beginIndex=responseContent.indexOf(variable);
  if (beginIndex == -1) {
    return null;
  }
  beginIndex+=variable.length() + 1;
  if (string) {
    beginIndex++;
    int endIndex=responseContent.indexOf("\",",beginIndex);
    if (endIndex == -1) {
      endIndex=responseContent.length() - 2;
    }
    return responseContent.substring(beginIndex,endIndex);
  }
 else {
    return responseContent.substring(beginIndex,responseContent.indexOf(",",beginIndex));
  }
}
