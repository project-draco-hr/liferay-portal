{
  _folderId=folderId;
  if (_folderRemoteModel != null) {
    try {
      Class<?> clazz=_folderRemoteModel.getClass();
      Method method=clazz.getMethod("setFolderId",long.class);
      method.invoke(_folderRemoteModel,folderId);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
