{
  _userId=userId;
  if (_folderRemoteModel != null) {
    try {
      Class<?> clazz=_folderRemoteModel.getClass();
      Method method=clazz.getMethod("setUserId",long.class);
      method.invoke(_folderRemoteModel,userId);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
