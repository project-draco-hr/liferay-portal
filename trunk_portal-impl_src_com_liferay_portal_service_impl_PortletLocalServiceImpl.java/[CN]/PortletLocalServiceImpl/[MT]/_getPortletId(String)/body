{
  if (_portletIdsByStrutsPath.size() == 0) {
    for (    Portlet portlet : _getPortletsPool().values()) {
      _portletIdsByStrutsPath.put(portlet.getStrutsPath(),portlet.getPortletId());
    }
  }
  String portletId=_portletIdsByStrutsPath.get(securityPath);
  if (Validator.isNull(portletId)) {
    _log.error("Struts path " + securityPath + " is not mapped to a portlet "+ "in liferay-portlet.xml");
  }
  return portletId;
}
