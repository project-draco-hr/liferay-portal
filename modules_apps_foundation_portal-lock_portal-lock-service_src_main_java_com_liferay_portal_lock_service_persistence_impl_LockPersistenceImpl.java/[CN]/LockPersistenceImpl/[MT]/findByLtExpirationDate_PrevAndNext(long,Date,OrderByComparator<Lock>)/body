{
  Lock lock=findByPrimaryKey(lockId);
  Session session=null;
  try {
    session=openSession();
    Lock[] array=new LockImpl[3];
    array[0]=getByLtExpirationDate_PrevAndNext(session,lock,expirationDate,orderByComparator,true);
    array[1]=lock;
    array[2]=getByLtExpirationDate_PrevAndNext(session,lock,expirationDate,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
