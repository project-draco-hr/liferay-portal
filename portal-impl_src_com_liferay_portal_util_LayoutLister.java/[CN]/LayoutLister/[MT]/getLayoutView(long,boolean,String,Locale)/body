{
  StopWatch stopWatch=null;
  if (_log.isDebugEnabled()) {
    stopWatch=new StopWatch();
    stopWatch.start();
  }
  LayoutView layoutView=doGetLayoutView(groupId,privateLayout,rootNodeName,locale);
  if (_log.isDebugEnabled()) {
    stopWatch.stop();
    _log.debug("Runtime: " + stopWatch.getTime());
  }
  return layoutView;
}
