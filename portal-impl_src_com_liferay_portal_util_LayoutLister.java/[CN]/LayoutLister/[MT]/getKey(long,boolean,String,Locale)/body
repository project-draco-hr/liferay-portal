{
  StringBundler sb=new StringBundler(5);
  sb.append(StringUtil.toHexString(groupId));
  sb.append(StringPool.POUND);
  sb.append(privateLayout);
  sb.append(StringPool.POUND);
  sb.append(rootNodeName);
  sb.append(StringPool.POUND);
  if (locale != null) {
    sb.append(locale.toString());
  }
 else {
    sb.append(StringPool.NULL);
  }
  return sb.toString();
}
