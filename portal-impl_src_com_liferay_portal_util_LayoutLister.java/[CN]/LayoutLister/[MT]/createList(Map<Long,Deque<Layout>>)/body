{
  Deque<LayoutNode> layoutNodeStack=new LinkedList<LayoutNode>();
  pushLayouts(layoutNodeStack,layoutsBag,LayoutConstants.DEFAULT_PARENT_LAYOUT_ID,1,_nodeId);
  while (!layoutNodeStack.isEmpty()) {
    LayoutNode layoutNode=layoutNodeStack.pop();
    visitLayout(layoutNode);
    pushLayouts(layoutNodeStack,layoutsBag,layoutNode.layout.getLayoutId(),layoutNode.depth + 1,_nodeId);
  }
}
