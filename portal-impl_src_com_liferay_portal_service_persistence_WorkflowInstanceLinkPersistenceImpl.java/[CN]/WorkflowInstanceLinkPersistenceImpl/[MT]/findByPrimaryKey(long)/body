{
  WorkflowInstanceLink workflowInstanceLink=fetchByPrimaryKey(workflowInstanceLinkId);
  if (workflowInstanceLink == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No WorkflowInstanceLink exists with the primary key " + workflowInstanceLinkId);
    }
    throw new NoSuchWorkflowInstanceLinkException("No WorkflowInstanceLink exists with the primary key " + workflowInstanceLinkId);
  }
  return workflowInstanceLink;
}
