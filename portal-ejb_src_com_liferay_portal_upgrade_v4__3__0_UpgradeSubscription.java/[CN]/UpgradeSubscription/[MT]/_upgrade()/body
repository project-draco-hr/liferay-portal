{
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(SubscriptionImpl.TABLE_NAME,SubscriptionImpl.TABLE_COLUMNS,new PKUpgradeColumnImpl(),new ClassNameUpgradeColumnImpl());
  upgradeTable.updateTable();
  CounterLocalServiceUtil.reset(Subscription.class.getName());
  DBUtil.getInstance().executeSQL(_UPGRADE_SCHEMA);
}
