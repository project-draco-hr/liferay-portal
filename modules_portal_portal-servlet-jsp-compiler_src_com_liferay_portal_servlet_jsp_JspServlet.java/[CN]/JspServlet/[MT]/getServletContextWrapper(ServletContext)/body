{
  if (_jspServletContext == null) {
synchronized (this) {
      if (_jspServletContext == null) {
        _jspServletContext=(ServletContext)Proxy.newProxyInstance(_jspBundleClassloader,_INTERFACES,new JspServletContextInvocationHandler(servletContext));
      }
    }
  }
  return _jspServletContext;
}
