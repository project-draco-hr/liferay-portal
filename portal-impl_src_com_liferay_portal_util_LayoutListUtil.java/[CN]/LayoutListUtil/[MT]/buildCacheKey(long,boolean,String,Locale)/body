{
  StringBundler sb=new StringBundler(7);
  sb.append(StringUtil.toHexString(groupId));
  sb.append(StringPool.POUND);
  sb.append(privateLayout);
  sb.append(StringPool.POUND);
  sb.append(rootNodeName);
  sb.append(StringPool.POUND);
  sb.append(LocaleUtil.toLanguageId(locale));
  return sb.toString();
}
