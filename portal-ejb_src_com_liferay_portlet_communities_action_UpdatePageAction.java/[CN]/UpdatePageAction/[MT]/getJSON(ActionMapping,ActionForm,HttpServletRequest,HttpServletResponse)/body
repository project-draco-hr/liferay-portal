{
  String cmd=ParamUtil.getString(req,Constants.CMD);
  JSONObject jo=new JSONObject();
  if ("add".equals(cmd)) {
    String url=addPage(req);
    jo.put("url",url);
  }
 else   if ("delete".equals(cmd)) {
    deletePage(req);
  }
 else   if ("reorder".equals(cmd)) {
    updateDisplayOrder(req);
  }
 else   if ("title".equals(cmd)) {
    updateTitle(req);
  }
  return jo.toString();
}
