{
  Object sessionId=cometEvent.getHttpServletRequest().getAttribute(COMET_CONNECTION_ID);
  if (sessionId == null || !(sessionId instanceof String)) {
    sessionId=PortalUUIDUtil.generate();
    cometEvent.getHttpServletRequest().setAttribute(COMET_CONNECTION_ID,sessionId);
  }
  return (String)sessionId;
}
