{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(expandoValue);
    }
  }
  expandoValue=removeImpl(expandoValue);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(expandoValue);
    }
  }
  return expandoValue;
}
