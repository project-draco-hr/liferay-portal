{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,expandoValue,merge);
    expandoValue.setNew(false);
    return expandoValue;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ExpandoValue.class.getName());
  }
}
