{
  Session session=null;
  try {
    session=openSession();
    ExpandoValue expandoValue=(ExpandoValue)session.get(ExpandoValueImpl.class,new Long(valueId));
    if (expandoValue == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + valueId);
      }
      throw new NoSuchValueException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + valueId);
    }
    return remove(expandoValue);
  }
 catch (  NoSuchValueException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
