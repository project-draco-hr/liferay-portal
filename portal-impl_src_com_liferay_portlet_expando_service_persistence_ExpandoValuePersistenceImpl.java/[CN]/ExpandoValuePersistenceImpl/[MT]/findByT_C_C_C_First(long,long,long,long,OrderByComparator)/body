{
  List<ExpandoValue> list=findByT_C_C_C(tableId,columnId,classNameId,classPK,0,1,obc);
  if (list.isEmpty()) {
    StringBuilder msg=new StringBuilder();
    msg.append("No ExpandoValue exists with the key {");
    msg.append("tableId=" + tableId);
    msg.append(", ");
    msg.append("columnId=" + columnId);
    msg.append(", ");
    msg.append("classNameId=" + classNameId);
    msg.append(", ");
    msg.append("classPK=" + classPK);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchValueException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
