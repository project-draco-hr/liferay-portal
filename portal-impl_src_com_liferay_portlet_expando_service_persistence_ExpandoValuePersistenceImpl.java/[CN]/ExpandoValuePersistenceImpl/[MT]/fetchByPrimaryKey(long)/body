{
  ExpandoValue expandoValue=(ExpandoValue)EntityCacheUtil.getResult(ExpandoValueModelImpl.ENTITY_CACHE_ENABLED,ExpandoValueImpl.class,valueId);
  if (expandoValue == _nullExpandoValue) {
    return null;
  }
  if (expandoValue == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      expandoValue=(ExpandoValue)session.get(ExpandoValueImpl.class,Long.valueOf(valueId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (expandoValue != null) {
        cacheResult(expandoValue);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(ExpandoValueModelImpl.ENTITY_CACHE_ENABLED,ExpandoValueImpl.class,valueId,_nullExpandoValue);
      }
      closeSession(session);
    }
  }
  return expandoValue;
}
