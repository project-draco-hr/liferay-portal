{
  expandoValue=toUnwrappedModel(expandoValue);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,expandoValue);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(expandoValue);
  return expandoValue;
}
