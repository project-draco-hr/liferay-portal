{
  Session session=null;
  try {
    session=openSession();
    session.delete(expandoValue);
    session.flush();
    return expandoValue;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(ExpandoValue.class.getName());
  }
}
