{
  EntityCacheUtil.putResult(ExpandoValueModelImpl.ENTITY_CACHE_ENABLED,ExpandoValueImpl.class,expandoValue.getPrimaryKey(),expandoValue);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_R,new Object[]{new Long(expandoValue.getColumnId()),new Long(expandoValue.getRowId())},expandoValue);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_T_C_R,new Object[]{new Long(expandoValue.getTableId()),new Long(expandoValue.getColumnId()),new Long(expandoValue.getRowId())},expandoValue);
}
