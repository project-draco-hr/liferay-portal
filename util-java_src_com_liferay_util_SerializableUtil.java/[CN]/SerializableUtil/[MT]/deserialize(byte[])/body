{
  ObjectInputStream ois=null;
  try {
    ois=new ObjectInputStream(new UnsyncByteArrayInputStream(bytes));
    return ois.readObject();
  }
 catch (  ClassNotFoundException e) {
    throw new RuntimeException(e);
  }
catch (  IOException e) {
    throw new RuntimeException(e);
  }
 finally {
    StreamUtil.cleanUp(ois);
  }
}
