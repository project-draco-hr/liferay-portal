{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps=connection.prepareStatement("select groupId, liveGroupId from Group_ where liveGroupId " + "!= 0");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long groupId=rs.getLong("groupId");
      long liveGroupId=rs.getLong("liveGroupId");
      updateUUID(groupId,liveGroupId);
    }
  }
 }
