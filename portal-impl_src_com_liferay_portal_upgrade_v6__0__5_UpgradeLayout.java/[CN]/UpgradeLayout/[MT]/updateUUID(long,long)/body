{
  try (PreparedStatement ps=connection.prepareStatement("select plid, privateLayout, layoutId, friendlyURL from " + "Layout where groupId = ?")){
    ps.setLong(1,groupId);
    try (ResultSet rs=ps.executeQuery()){
      while (rs.next()) {
        long plid=rs.getLong("plid");
        boolean privateLayout=rs.getBoolean("privateLayout");
        long layoutId=rs.getLong("layoutId");
        String friendlyURL=rs.getString("friendlyURL");
        updateUUID(plid,liveGroupId,privateLayout,layoutId,friendlyURL);
      }
    }
   }
 }
