{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals("Dynamic",selenium.getSelectedLabel("_86_selectionStyle"));
  assertEquals(RuntimeVariables.replace("Source"),selenium.getText("//form/div[2]/div[1]/div[1]"));
  assertEquals(RuntimeVariables.replace("Filter"),selenium.getText("//div[2]/div[2]/div[1]"));
  assertEquals(RuntimeVariables.replace("Ordering and Grouping"),selenium.getText("//div[3]/div[1]/div/span"));
  assertEquals(RuntimeVariables.replace("Display Settings"),selenium.getText("//div[2]/div[4]/div[1]"));
}
