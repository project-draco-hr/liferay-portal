{
  StringBundler sb=new StringBundler(11);
  sb.append("{uuid=");
  sb.append(getUuid());
  sb.append(", choiceId=");
  sb.append(getChoiceId());
  sb.append(", questionId=");
  sb.append(getQuestionId());
  sb.append(", name=");
  sb.append(getName());
  sb.append(", description=");
  sb.append(getDescription());
  sb.append("}");
  return sb.toString();
}
