{
  String httpMethod=_jsonWebServiceMappingResolver.resolveHttpMethod(method);
  if (!_jsonWebServiceNaming.acceptHttpMethod(httpMethod)) {
    return;
  }
  if (_wireViaUtil == true) {
    Class<?> utilClass=loadUtilClass(serviceBeanClass);
    try {
      method=utilClass.getMethod(method.getName(),method.getParameterTypes());
    }
 catch (    NoSuchMethodException nsme) {
      return;
    }
    String path=_jsonWebServiceMappingResolver.resolvePath(serviceBeanClass,method);
    JSONWebServiceActionsManagerUtil.registerJSONWebServiceAction(contextPath,method.getDeclaringClass(),method,path,httpMethod);
  }
 else {
    String path=_jsonWebServiceMappingResolver.resolvePath(serviceBeanClass,method);
    JSONWebServiceActionsManagerUtil.registerJSONWebServiceAction(contextPath,serviceBean,serviceBeanClass,method,path,httpMethod);
  }
}
