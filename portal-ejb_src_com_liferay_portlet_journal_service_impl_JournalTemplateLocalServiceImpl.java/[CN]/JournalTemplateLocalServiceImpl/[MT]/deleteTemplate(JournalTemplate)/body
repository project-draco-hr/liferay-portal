{
  if (JournalArticleUtil.countByC_T(template.getCompanyId(),template.getTemplateId()) > 0) {
    throw new RequiredTemplateException();
  }
  ImageLocalUtil.remove(template.getSmallImageId());
  ResourceLocalServiceUtil.deleteResource(template.getCompanyId(),JournalTemplate.class.getName(),Resource.TYPE_CLASS,Resource.SCOPE_INDIVIDUAL,template.getPrimaryKey().toString());
  JournalTemplateUtil.remove(template.getPrimaryKey());
}
