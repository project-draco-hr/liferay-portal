{
  try {
    CompanyService companyService=CompanyServiceFactory.getService();
    return companyService.updateCompany(companyId,portalURL,homeURL,mx,name,legalName,legalId,legalType,sicCode,tickerSymbol,industry,type,size);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
