{
  User user=(User)obj;
  try {
    if ((isChecked(user) && UserGroupMembershipPolicyUtil.isMembershipRequired(user.getUserId(),_userGroup.getUserGroupId())) || (!isChecked(user) && !UserGroupMembershipPolicyUtil.isMembershipAllowed(user.getUserId(),_userGroup.getUserGroupId()))) {
      return true;
    }
  }
  finally {
    return super.isDisabled(obj);
  }
}
