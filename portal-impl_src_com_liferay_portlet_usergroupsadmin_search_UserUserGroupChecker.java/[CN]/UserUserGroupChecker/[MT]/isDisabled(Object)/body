{
  User user=(User)obj;
  try {
    if (isChecked(user)) {
      if (UserGroupMembershipPolicyUtil.isMembershipRequired(user.getUserId(),_userGroup.getUserGroupId())) {
        return true;
      }
    }
 else {
      if (!UserGroupMembershipPolicyUtil.isMembershipAllowed(user.getUserId(),_userGroup.getUserGroupId())) {
        return true;
      }
    }
  }
  finally {
    return super.isDisabled(obj);
  }
}
