{
  larFile=LayoutLocalServiceUtil.exportLayoutsAsFile(layout.getGroupId(),layout.isPrivateLayout(),null,getExportParameterMap(),null,null);
  importedGroup=GroupTestUtil.addGroup();
  LayoutLocalServiceUtil.importLayouts(TestPropsValues.getUserId(),importedGroup.getGroupId(),layout.isPrivateLayout(),getImportParameterMap(),larFile);
  importedLayout=LayoutLocalServiceUtil.fetchLayoutByUuidAndGroupId(layout.getUuid(),importedGroup.getGroupId(),layout.isPrivateLayout());
  Assert.assertNotNull(importedLayout);
}
