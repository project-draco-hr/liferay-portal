{
  Group scopeGroup=layout.getScopeGroup();
  if (scopeGroup != null) {
    return scopeGroup;
  }
  return GroupLocalServiceUtil.addGroup(userId,GroupConstants.DEFAULT_PARENT_GROUP_ID,Layout.class.getName(),layout.getPlid(),GroupConstants.DEFAULT_LIVE_GROUP_ID,String.valueOf(layout.getPlid()),null,0,null,false,true,null);
}
