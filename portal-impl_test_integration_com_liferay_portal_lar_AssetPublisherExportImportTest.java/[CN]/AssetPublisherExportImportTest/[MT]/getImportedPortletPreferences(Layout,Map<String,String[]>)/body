{
  String assetPublisherPortletId=addAssetPublisherPortletToLayout(TestPropsValues.getUserId(),_layout,"column-1",preferenceMap);
  Map<String,String[]> parameterMap=new HashMap<String,String[]>();
  parameterMap.put(PortletDataHandlerKeys.PORTLET_SETUP,new String[]{Boolean.TRUE.toString()});
  File file=LayoutLocalServiceUtil.exportLayoutsAsFile(layout.getGroupId(),layout.isPrivateLayout(),null,parameterMap,null,null);
  _importedGroup=GroupTestUtil.addGroup();
  LayoutLocalServiceUtil.importLayouts(TestPropsValues.getUserId(),_importedGroup.getGroupId(),layout.isPrivateLayout(),parameterMap,file);
  _importedLayout=LayoutLocalServiceUtil.fetchLayoutByUuidAndGroupId(layout.getUuid(),_importedGroup.getGroupId(),layout.isPrivateLayout());
  Assert.assertNotNull(_importedLayout);
  return getPortletPreferences(_importedLayout.getCompanyId(),_importedLayout.getPlid(),assetPublisherPortletId);
}
