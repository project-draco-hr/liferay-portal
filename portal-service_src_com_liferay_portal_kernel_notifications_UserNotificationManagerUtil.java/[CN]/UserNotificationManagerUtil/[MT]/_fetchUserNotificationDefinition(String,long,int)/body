{
  List<UserNotificationDefinition> userNotificationDefinitions=_userNotificationDefinitions.getService(portletId);
  if (userNotificationDefinitions == null) {
    return null;
  }
  for (  UserNotificationDefinition userNotificationDefinition : userNotificationDefinitions) {
    if ((userNotificationDefinition.getClassNameId() == classNameId) && (userNotificationDefinition.getNotificationType() == notificationType)) {
      return userNotificationDefinition;
    }
  }
  return null;
}
