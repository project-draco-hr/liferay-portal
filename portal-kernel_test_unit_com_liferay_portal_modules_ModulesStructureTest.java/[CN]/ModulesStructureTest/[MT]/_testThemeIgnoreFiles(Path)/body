{
  Path resourcesImporterDirPath=dirPath.resolve("resources-importer");
  if (Files.exists(resourcesImporterDirPath)) {
    Path resourcesImporterIgnorePath=resourcesImporterDirPath.resolve(_SOURCE_FORMATTER_IGNORE_FILE_NAME);
    Assert.assertTrue("Missing " + resourcesImporterIgnorePath,Files.exists(resourcesImporterIgnorePath));
  }
}
