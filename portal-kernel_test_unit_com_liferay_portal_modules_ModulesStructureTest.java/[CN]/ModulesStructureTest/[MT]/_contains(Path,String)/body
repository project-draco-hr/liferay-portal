{
  try (FileReader fileReader=new FileReader(path.toFile());UnsyncBufferedReader unsyncBufferedReader=new UnsyncBufferedReader(fileReader)){
    String line=null;
    while ((line=unsyncBufferedReader.readLine()) != null) {
      if (line.contains(s)) {
        return true;
      }
    }
  }
   return false;
}
