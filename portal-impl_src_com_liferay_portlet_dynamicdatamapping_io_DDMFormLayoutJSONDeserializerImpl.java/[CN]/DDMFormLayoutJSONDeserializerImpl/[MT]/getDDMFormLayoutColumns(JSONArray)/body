{
  List<DDMFormLayoutColumn> ddmFormLayoutColumns=new ArrayList<DDMFormLayoutColumn>();
  for (int i=0; i < jsonArray.length(); i++) {
    DDMFormLayoutColumn ddmFormLayoutColumn=getDDMFormLayoutColumn(jsonArray.getJSONObject(i));
    ddmFormLayoutColumns.add(ddmFormLayoutColumn);
  }
  return ddmFormLayoutColumns;
}
