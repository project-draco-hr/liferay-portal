{
  if (jsonObject == null) {
    return null;
  }
  LocalizedValue title=new LocalizedValue();
  Iterator<String> itr=jsonObject.keys();
  while (itr.hasNext()) {
    String languageId=itr.next();
    title.addString(LocaleUtil.fromLanguageId(languageId),jsonObject.getString(languageId));
  }
  return title;
}
