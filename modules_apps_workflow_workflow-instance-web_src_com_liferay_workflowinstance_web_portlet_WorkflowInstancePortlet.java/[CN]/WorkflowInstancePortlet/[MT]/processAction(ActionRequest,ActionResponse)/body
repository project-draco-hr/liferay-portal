{
  String actionName=ParamUtil.getString(actionRequest,ActionRequest.ACTION_NAME);
  if (Validator.isNotNull(actionName) && StringUtil.equalsIgnoreCase(actionName,_DISCUSSION_ACTION)) {
    SessionMessages.add(actionRequest,getPortletConfig().getPortletName() + SessionMessages.KEY_SUFFIX_HIDE_DEFAULT_SUCCESS_MESSAGE);
  }
  super.processAction(actionRequest,actionResponse);
}
