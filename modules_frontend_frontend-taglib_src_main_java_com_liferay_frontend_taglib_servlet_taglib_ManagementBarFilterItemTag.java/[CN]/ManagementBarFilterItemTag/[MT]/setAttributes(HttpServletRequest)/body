{
  ManagementBarNavigationTag managementBarNavigationTag=(ManagementBarNavigationTag)findAncestorWithClass(this,ManagementBarNavigationTag.class);
  List<FilterNavigationItem> filterNavigationItems=managementBarNavigationTag.getFilterNavigationItems();
  if (filterNavigationItems != null) {
    FilterNavigationItem filterNavigationItem=new FilterNavigationItem(_active,_id,_label,_url);
    filterNavigationItems.add(filterNavigationItem);
  }
}
