{
  MethodKey methodKey=new MethodKey(serviceClassName,"get" + modelName,long.class);
  Method method=MethodCache.get(methodKey);
  Object obj=new MethodHandler(methodKey,pk).invoke(true);
  methodKey=new MethodKey(serviceClassName,"update" + modelName,method.getReturnType());
  new MethodHandler(methodKey,obj).invoke(true);
}
