{
  StringBuilder sb=new StringBuilder();
  sb.append(_getLineItemText("misc","Running "));
  sb.append(_getLineItemText("command-name",element.attributeValue("method")));
  if (!arguments.isEmpty()) {
    sb.append(_getLineItemText("misc"," with parameters"));
    for (    String argument : arguments) {
      argument="Arg: " + argument;
      sb.append(_getLineItemText("param-value",HtmlUtil.escape(argument)));
    }
  }
  if (returnValue != null) {
    returnValue="Return: " + returnValue;
    sb.append(_getLineItemText("param-value",HtmlUtil.escape(returnValue)));
  }
  return sb.toString();
}
