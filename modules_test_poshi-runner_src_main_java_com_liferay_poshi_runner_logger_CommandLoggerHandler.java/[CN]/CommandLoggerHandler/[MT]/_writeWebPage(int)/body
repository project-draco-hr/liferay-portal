{
  WebDriver webDriver=WebDriverUtil.getWebDriver();
  String testClassCommandName=PoshiRunnerContext.getTestCaseCommandName();
  testClassCommandName=StringUtil.replace(testClassCommandName,"#","_");
  WebDriverHelper.saveWebPage(webDriver,PoshiRunnerGetterUtil.getCanonicalPath(".") + "/test-results/" + testClassCommandName+ "/web-pages/index"+ errorLinkId+ ".html",_htmlSource);
}
