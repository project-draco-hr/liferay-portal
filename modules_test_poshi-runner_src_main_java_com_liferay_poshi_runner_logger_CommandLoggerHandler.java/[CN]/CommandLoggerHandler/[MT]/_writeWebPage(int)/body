{
  String testClassCommandName=PoshiRunnerContext.getTestCaseCommandName();
  testClassCommandName=StringUtil.replace(testClassCommandName,"#","_");
  WebDriverHelper.saveWebPage(PoshiRunnerGetterUtil.getCanonicalPath(".") + "/test-results/" + testClassCommandName+ "/web-pages/index"+ errorLinkId+ ".html",_htmlSource);
}
