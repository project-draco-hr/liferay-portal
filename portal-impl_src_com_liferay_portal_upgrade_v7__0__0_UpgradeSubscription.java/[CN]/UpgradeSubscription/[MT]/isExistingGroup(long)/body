{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select count(*) from Group_ where groupId = ?");
    ps.setLong(1,groupId);
    rs=ps.executeQuery();
    while (rs.next()) {
      int count=rs.getInt(1);
      if (count > 0) {
        return true;
      }
    }
    return false;
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
