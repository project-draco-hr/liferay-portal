{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select count(*) from Group_ where groupId = ?");
    ps.setLong(1,groupId);
    rs=ps.executeQuery();
    if (rs.next()) {
      int count=rs.getInt(1);
      if (count > 0) {
        return true;
      }
    }
    return false;
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
