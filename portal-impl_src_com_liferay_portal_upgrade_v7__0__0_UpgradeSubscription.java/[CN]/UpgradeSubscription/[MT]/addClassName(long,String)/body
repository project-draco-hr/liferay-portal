{
  PreparedStatement ps=null;
  try {
    ps=connection.prepareStatement("insert into ClassName_ (mvccVersion, classNameId, value) " + "values (?, ?, ?)");
    ps.setLong(1,0);
    ps.setLong(2,classNameId);
    ps.setString(3,className);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(ps);
  }
}
