{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement(sql);
    rs=ps.executeQuery();
    while (rs.next()) {
      visitor.visit(rs);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
