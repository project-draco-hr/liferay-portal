{
  try {
    User oldUser=UserLocalServiceUtil.getUser(newUser.getUserId());
    Collection<ProfileAttribute> attributes=getChangedAttributes(newUser,oldUser);
    if (attributes.size() > 0) {
      AuditMessageSenderUtil.send(UserProfileModificationAuditMessageBuilder.build(newUser.getUserId(),attributes));
    }
  }
 catch (  Exception e) {
    throw new ModelListenerException(e);
  }
}
