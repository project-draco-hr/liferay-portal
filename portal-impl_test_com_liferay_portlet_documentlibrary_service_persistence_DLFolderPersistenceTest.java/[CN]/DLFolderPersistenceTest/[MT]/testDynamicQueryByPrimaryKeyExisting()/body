{
  DLFolder newDLFolder=addDLFolder();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLFolder.class,DLFolder.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("folderId",newDLFolder.getFolderId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  DLFolder existingDLFolder=(DLFolder)result.get(0);
  assertEquals(existingDLFolder,newDLFolder);
}
