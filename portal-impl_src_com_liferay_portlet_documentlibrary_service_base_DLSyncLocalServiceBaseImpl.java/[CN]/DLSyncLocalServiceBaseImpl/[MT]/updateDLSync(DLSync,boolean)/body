{
  dlSync.setNew(false);
  dlSync=dlSyncPersistence.update(dlSync,merge);
  Indexer indexer=IndexerRegistryUtil.getIndexer(getModelClassName());
  if (indexer != null) {
    try {
      indexer.reindex(dlSync);
    }
 catch (    SearchException se) {
      if (_log.isWarnEnabled()) {
        _log.warn(se,se);
      }
    }
  }
  return dlSync;
}
