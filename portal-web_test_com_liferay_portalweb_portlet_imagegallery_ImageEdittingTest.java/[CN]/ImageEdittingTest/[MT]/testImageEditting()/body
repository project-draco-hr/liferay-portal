{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Image Gallery Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Image Gallery Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("//tr[4]/td[1]/a[1]/b"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("//b"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//img[@alt='Test Image. This is the Liferay logo!']");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Close")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  Thread.sleep(5000);
  assertTrue(selenium.isElementPresent("link=Edit"));
  assertTrue(selenium.isElementPresent("link=Permissions"));
  assertTrue(selenium.isElementPresent("link=Delete"));
  selenium.click("link=Close");
  Thread.sleep(5000);
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//img[@alt='Test Image. This is the Liferay logo!']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("//img[@alt='Test Image. This is the Liferay logo!']");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//div[4]/div[1]/div[1]/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  Thread.sleep(5000);
  assertTrue(selenium.isElementPresent("link=Edit"));
  assertTrue(selenium.isElementPresent("link=Permissions"));
  assertTrue(selenium.isElementPresent("link=Delete"));
  selenium.click("//div[4]/div[1]/div[1]/a");
  Thread.sleep(5000);
}
