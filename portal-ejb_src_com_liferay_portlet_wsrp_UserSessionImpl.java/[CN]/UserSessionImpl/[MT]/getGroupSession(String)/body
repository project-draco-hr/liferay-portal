{
  if (groupID != null) {
    GroupSessionMgr groupSession=(GroupSessionMgr)this.groupSessions.get(groupID);
    if (groupSession == null) {
      groupSession=new GroupSessionImpl(groupID,this.getMarkupInterfaceURL());
      addGroupSession(groupSession);
    }
    return groupSession;
  }
  return null;
}
