{
  selenium.click("//input[@value='Add Article']");
  Thread.sleep(5000);
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("_15_title")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.typeKeys("_15_title","Test Journal Article");
  Thread.sleep(5000);
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("_15_editor")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("FCKeditor1___Frame")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//textarea")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.selectFrame("//iframe[@id=\"_15_editor\"]");
  selenium.selectFrame("//iframe[@id=\"FCKeditor1___Frame\"]");
  selenium.selectFrame("//iframe");
  selenium.typeKeys("//body","This is a test Journal Article!");
  selenium.selectFrame("relative=top");
  selenium.select("_15_type","label=Announcements");
  selenium.click("//input[@value='Save and Approve']");
  selenium.waitForPageToLoad("30000");
}
