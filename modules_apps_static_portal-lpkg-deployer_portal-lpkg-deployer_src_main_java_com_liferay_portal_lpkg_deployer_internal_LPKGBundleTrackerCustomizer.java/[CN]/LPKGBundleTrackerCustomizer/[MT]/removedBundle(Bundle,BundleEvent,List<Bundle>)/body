{
  if (bundle.getState() != Bundle.UNINSTALLED) {
    return;
  }
  for (  Bundle newBundle : bundles) {
    try {
      newBundle.uninstall();
    }
 catch (    BundleException be) {
      _log.error("Unable to uninstall " + newBundle + " in response to uninstallation of "+ bundle,be);
    }
  }
}
