{
  try {
    ResourceBlockIdsBag resourceBlockIdsBag=getResourceBlockIdsBag(companyId,groupId,userId,name);
    return ResourceBlockLocalServiceUtil.getResourceBlockIds(resourceBlockIdsBag,name,actionId);
  }
 catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      _log.debug(e,e);
    }
  }
  return Collections.emptyList();
}
