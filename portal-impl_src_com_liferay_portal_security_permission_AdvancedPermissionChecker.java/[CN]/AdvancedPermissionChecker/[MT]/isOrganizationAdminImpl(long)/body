{
  if (!signedIn) {
    return false;
  }
  if (isOmniadmin()) {
    return true;
  }
  if (organizationId <= 0) {
    return false;
  }
  Organization organization=OrganizationLocalServiceUtil.fetchOrganization(organizationId);
  if (organization == null) {
    return false;
  }
  if (isCompanyAdmin(organization.getCompanyId())) {
    return true;
  }
  PermissionCheckerBag bag=getUserBag(user.getUserId(),organization.getGroupId());
  if (bag == null) {
    _log.error("Bag should never be null");
  }
  if (bag.isOrganizationAdmin(this,organization)) {
    return true;
  }
 else {
    return false;
  }
}
