{
  ResourceBlockIdsBag resourceBlockIdsBag=PermissionCacheUtil.getResourceBlockIdsBag(companyId,groupId,defaultUserId,name,true);
  if (resourceBlockIdsBag != null) {
    return resourceBlockIdsBag;
  }
  try {
    PermissionCheckerBag bag=getGuestUserBag();
    long[] roleIds=bag.getRoleIds();
    resourceBlockIdsBag=ResourceBlockLocalServiceUtil.getResourceBlockIdsBag(getCompanyId(),groupId,name,roleIds);
    PermissionCacheUtil.putResourceBlockIdsBag(companyId,groupId,defaultUserId,name,true,resourceBlockIdsBag);
    return resourceBlockIdsBag;
  }
  finally {
    if (resourceBlockIdsBag == null) {
      resourceBlockIdsBag=new ResourceBlockIdsBag();
    }
    PermissionCacheUtil.putResourceBlockIdsBag(companyId,defaultUserId,groupId,name,true,resourceBlockIdsBag);
  }
}
