{
  try {
    if (!signedIn) {
      return hasGuestPermission(groupId,name,primKey,actionId);
    }
    if (ResourceBlockLocalServiceUtil.isSupported(name)) {
      return hasUserPermission(groupId,name,primKey,actionId,true);
    }
    boolean value=false;
    if (checkGuest) {
      value=hasGuestPermission(groupId,name,primKey,actionId);
    }
    if (!value) {
      value=hasUserPermission(groupId,name,primKey,actionId,true);
    }
    return value;
  }
 catch (  Exception e) {
    _log.error(e,e);
    return false;
  }
}
