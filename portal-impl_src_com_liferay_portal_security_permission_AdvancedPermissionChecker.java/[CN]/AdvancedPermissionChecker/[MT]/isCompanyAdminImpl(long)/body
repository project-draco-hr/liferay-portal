{
  if (!signedIn) {
    return false;
  }
  if (isOmniadmin()) {
    return true;
  }
  Boolean value=companyAdmins.get(companyId);
  if (value == null) {
    boolean hasAdminRole=RoleLocalServiceUtil.hasUserRole(user.getUserId(),companyId,RoleConstants.ADMINISTRATOR,true);
    value=Boolean.valueOf(hasAdminRole);
    companyAdmins.put(companyId,value);
  }
  return value.booleanValue();
}
