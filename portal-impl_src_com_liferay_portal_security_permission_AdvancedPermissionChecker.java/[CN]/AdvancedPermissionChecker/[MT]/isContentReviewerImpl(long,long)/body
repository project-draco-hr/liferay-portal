{
  if (!signedIn) {
    return false;
  }
  if (isOmniadmin()) {
    return true;
  }
  if (isCompanyAdmin(companyId)) {
    return true;
  }
  if (groupId <= 0) {
    return false;
  }
  if (isGroupAdmin(groupId)) {
    return true;
  }
  Group group=GroupLocalServiceUtil.getGroup(groupId);
  Boolean value=PermissionCacheUtil.getUserPrimaryKeyRole(getUserId(),group.getGroupId(),RoleConstants.SITE_CONTENT_REVIEWER);
  try {
    if (value == null) {
      value=isContentReviewerImpl(group);
      PermissionCacheUtil.putUserPrimaryKeyRole(getUserId(),group.getGroupId(),RoleConstants.SITE_CONTENT_REVIEWER,value);
    }
  }
 catch (  Exception e) {
    PermissionCacheUtil.removeUserPrimaryKeyRole(getUserId(),group.getGroupId(),RoleConstants.SITE_CONTENT_REVIEWER);
    throw e;
  }
  return value;
}
