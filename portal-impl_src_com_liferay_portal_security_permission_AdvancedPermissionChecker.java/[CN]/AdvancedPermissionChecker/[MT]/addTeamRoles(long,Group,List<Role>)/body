{
  if (((PropsValues.PERMISSIONS_USER_CHECK_ALGORITHM == 5) || (PropsValues.PERMISSIONS_USER_CHECK_ALGORITHM == 6)) && (group.isCommunity() || group.isOrganization())) {
    List<Team> teams=TeamLocalServiceUtil.getUserTeams(userId,group.getGroupId());
    for (    Team team : teams) {
      Role role=RoleLocalServiceUtil.getTeamRole(team.getCompanyId(),team.getTeamId());
      roles.add(role);
    }
  }
}
