{
  try {
    if (!signedIn) {
      return hasGuestPermission(groupId,name,primKey,actionId);
    }
    if (ResourceBlockLocalServiceUtil.isSupported(name)) {
      return hasUserPermissionImpl(groupId,name,primKey,roleIds,actionId);
    }
    return hasUserPermissionImpl(groupId,name,primKey,roleIds,actionId);
  }
 catch (  Exception e) {
    _log.error(e,e);
    return false;
  }
}
