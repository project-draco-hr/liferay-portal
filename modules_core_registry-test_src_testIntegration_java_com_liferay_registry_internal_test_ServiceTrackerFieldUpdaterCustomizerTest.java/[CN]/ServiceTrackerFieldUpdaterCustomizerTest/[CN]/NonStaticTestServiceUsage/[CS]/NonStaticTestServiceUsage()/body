{
  Registry registry=RegistryUtil.getRegistry();
  Field field=NonStaticTestServiceUsage.class.getDeclaredField("_testService");
  field.setAccessible(true);
  _serviceTracker=registry.trackServices(TestService.class,new ServiceTrackerFieldUpdaterCustomizer<TestService,TestService>(field,this,null));
  _serviceTracker.open();
}
