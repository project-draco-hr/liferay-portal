{
  sikuliClick(liferaySelenium,image);
  Keyboard keyboard=new DesktopKeyboard();
  keyboard.keyDown(Key.CTRL);
  keyboard.type("a");
  keyboard.keyUp(Key.CTRL);
  String relativePathName=FileUtil.getSeparator() + _TEST_DEPENDENCIES_DIR_NAME + FileUtil.getSeparator()+ value;
  String fileName=getFullPathNameFromRelativePathName(_TEST_SEARCH_DIR_NAMES,relativePathName);
  if (OSDetector.isWindows()) {
    fileName=StringUtil.replace(fileName,"/","\\");
  }
  sikuliType(liferaySelenium,image,fileName);
  keyboard.type(Key.ENTER);
}
