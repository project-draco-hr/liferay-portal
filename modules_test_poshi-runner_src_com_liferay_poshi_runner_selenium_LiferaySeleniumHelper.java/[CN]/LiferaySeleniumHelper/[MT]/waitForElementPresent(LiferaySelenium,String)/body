{
  for (int second=0; ; second++) {
    if (second >= PropsValues.TIMEOUT_EXPLICIT_WAIT) {
      liferaySelenium.assertElementPresent(locator);
    }
    try {
      if (liferaySelenium.isElementPresent(locator)) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
