{
  String uuid=displayStyle.substring(DISPLAY_STYLE_PREFIX_6_2.length());
  try (PreparedStatement ps=connection.prepareStatement("select templateKey from DDMTemplate where groupId = ?" + " and uuid_ = ?")){
    ps.setLong(1,displayStyleGroupId);
    ps.setString(2,uuid);
    try (ResultSet rs=ps.executeQuery()){
      while (rs.next()) {
        return rs.getString("templateKey");
      }
      return null;
    }
   }
 }
