{
  String uuid=displayStyle.substring(DISPLAY_STYLE_PREFIX_6_2.length());
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select templateKey from DDMTemplate where groupId = ?" + " and uuid_ = ?");
    ps.setLong(1,displayStyleGroupId);
    ps.setString(2,uuid);
    rs=ps.executeQuery();
    while (rs.next()) {
      return rs.getString("templateKey");
    }
    return null;
  }
  finally {
    DataAccess.cleanUp(null,ps,rs);
  }
}
