{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  DDMTemplateLink newDDMTemplateLink=addDDMTemplateLink();
  _persistence.clearCache();
  DDMTemplateLink existingDDMTemplateLink=_persistence.findByPrimaryKey(newDDMTemplateLink.getPrimaryKey());
  Assert.assertEquals(existingDDMTemplateLink.getClassNameId(),ReflectionTestUtil.invoke(existingDDMTemplateLink,"getOriginalClassNameId",new Class<?>[0]));
  Assert.assertEquals(existingDDMTemplateLink.getClassPK(),ReflectionTestUtil.invoke(existingDDMTemplateLink,"getOriginalClassPK",new Class<?>[0]));
}
