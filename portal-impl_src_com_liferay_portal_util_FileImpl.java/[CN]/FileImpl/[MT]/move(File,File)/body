{
  if (!source.exists()) {
    return false;
  }
  destination.delete();
  if (source.isDirectory()) {
    try {
      FileUtils.moveDirectory(source,destination);
      return true;
    }
 catch (    IOException ioe) {
    }
  }
 else {
    try {
      FileUtils.moveFile(source,destination);
      return true;
    }
 catch (    IOException ioe) {
    }
  }
  return false;
}
