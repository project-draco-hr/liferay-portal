{
  try {
    ActionUtil.getOrder(req);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchOrderException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.shopping.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward(getForward(req,"portlet.shopping.edit_order"));
}
