{
  super.populateRequiredContext(template,ddmFormField,ddmFormFieldRenderingContext);
  template.put("multiple",ddmFormField.isMultiple() ? "multiple" : StringPool.BLANK);
  template.put("options",getOptions(ddmFormField,ddmFormFieldRenderingContext));
  Map<String,String> stringsMap=new HashMap<>();
  stringsMap.put("chooseAnOption",LanguageUtil.get(ddmFormFieldRenderingContext.getLocale(),"choose-an-option"));
  template.put("strings",stringsMap);
}
