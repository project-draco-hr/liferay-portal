{
  List<DDMFormField> ddmFormFields=_ddmForm.getDDMFormFields();
  String[] privateFieldNames=PropsValues.DYNAMIC_DATA_MAPPING_STRUCTURE_PRIVATE_FIELD_NAMES;
  for (  String privateFieldName : privateFieldNames) {
    DDMFormField privateDDMFormField=createPrivateDDMFormField(privateFieldName);
    ddmFormFields.add(privateDDMFormField);
  }
}
