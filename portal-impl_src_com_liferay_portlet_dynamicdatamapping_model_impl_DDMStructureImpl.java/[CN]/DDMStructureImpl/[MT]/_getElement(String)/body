{
  try {
    Document document=_getDocument();
    XPath xPathSelector=SAXReaderUtil.createXPath("//dynamic-element[@name='" + name + "']");
    List<Node> nodes=xPathSelector.selectNodes(document);
    if (nodes.size() == 1) {
      return (Element)nodes.get(0);
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return null;
}
