{
  for (  String beanName : _applicationContext.getBeanDefinitionNames()) {
    Object bean=null;
    try {
      bean=_applicationContext.getBean(beanName);
    }
 catch (    BeanIsAbstractException biae) {
    }
catch (    Exception e) {
      _log.log(Logger.LOG_ERROR,"Unable to register service " + beanName,e);
    }
    if (bean != null) {
      registerService(_bundleContext,bean);
    }
  }
  Bundle bundle=_bundleContext.getBundle();
  registerApplicationContext(_applicationContext,bundle.getSymbolicName());
}
