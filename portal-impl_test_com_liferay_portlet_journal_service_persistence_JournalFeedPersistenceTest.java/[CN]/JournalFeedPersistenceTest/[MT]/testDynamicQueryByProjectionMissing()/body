{
  JournalFeed newJournalFeed=addJournalFeed();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalFeed.class,JournalFeed.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("id"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("id",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
