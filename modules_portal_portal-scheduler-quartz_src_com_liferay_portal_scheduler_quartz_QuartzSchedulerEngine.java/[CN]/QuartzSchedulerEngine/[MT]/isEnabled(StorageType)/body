{
  if (!isEnabled()) {
    return false;
  }
  Scheduler scheduler=getScheduler(storageType);
  try {
    if (scheduler.isShutdown() || scheduler.isInStandbyMode()) {
      return false;
    }
  }
 catch (  org.quartz.SchedulerException se) {
    throw new SchedulerException(se);
  }
  return true;
}
