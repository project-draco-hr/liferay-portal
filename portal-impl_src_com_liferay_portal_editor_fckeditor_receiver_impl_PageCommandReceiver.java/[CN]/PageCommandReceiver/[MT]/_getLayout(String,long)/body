{
  Layout layout=null;
  try {
    layout=LayoutLocalServiceUtil.getFriendlyURLLayout(groupId,false,layoutName);
    return layout;
  }
 catch (  NoSuchLayoutException nsle) {
  }
  try {
    layout=LayoutLocalServiceUtil.getFriendlyURLLayout(groupId,true,layoutName);
  }
 catch (  NoSuchLayoutException nsle) {
  }
  return layout;
}
