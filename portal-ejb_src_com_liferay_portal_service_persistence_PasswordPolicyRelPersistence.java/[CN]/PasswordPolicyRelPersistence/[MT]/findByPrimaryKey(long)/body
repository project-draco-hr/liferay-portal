{
  PasswordPolicyRel passwordPolicyRel=fetchByPrimaryKey(passwordPolicyRelId);
  if (passwordPolicyRel == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No PasswordPolicyRel exists with the primary key " + passwordPolicyRelId);
    }
    throw new NoSuchPasswordPolicyRelException("No PasswordPolicyRel exists with the primary key " + passwordPolicyRelId);
  }
  return passwordPolicyRel;
}
