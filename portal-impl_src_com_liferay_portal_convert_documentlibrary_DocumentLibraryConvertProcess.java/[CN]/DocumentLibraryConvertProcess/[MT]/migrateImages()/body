{
  int count=ImageLocalServiceUtil.getImagesCount();
  MaintenanceUtil.appendStatus("Migrating " + count + " images");
  ActionableDynamicQuery actionableDynamicQuery=ImageLocalServiceUtil.getActionableDynamicQuery();
  actionableDynamicQuery.setPerformActionMethod(new ActionableDynamicQuery.PerformActionMethod<Image>(){
    @Override public void performAction(    Image image){
      String fileName=image.getImageId() + StringPool.PERIOD + image.getType();
      migrateFile(0,0,fileName,Store.VERSION_DEFAULT);
    }
  }
);
  actionableDynamicQuery.performActions();
}
