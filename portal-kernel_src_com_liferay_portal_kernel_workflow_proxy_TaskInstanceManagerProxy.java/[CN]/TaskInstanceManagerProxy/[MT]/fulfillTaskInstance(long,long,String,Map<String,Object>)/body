{
  try {
    return (TaskInstanceInfo)_synchronousMessageSender.send(TaskInstanceRequest.createFulfillRequest(taskInstanceId,actorId,description,contextInfo));
  }
 catch (  MessageBusException ex) {
    throw new WorkflowException("Unable to fulfil " + "taskInstanceId:" + taskInstanceId + " by "+ "userId:"+ actorId,ex);
  }
}
