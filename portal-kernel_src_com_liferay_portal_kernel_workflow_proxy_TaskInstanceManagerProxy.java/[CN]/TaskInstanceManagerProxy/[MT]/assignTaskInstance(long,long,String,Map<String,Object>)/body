{
  try {
    return (TaskInstanceInfo)_synchronousMessageSender.send(TaskInstanceRequest.createAssignRequest(taskInstanceId,actorId,description,contextInfo));
  }
 catch (  MessageBusException ex) {
    throw new WorkflowException("Unable to assign " + "taskInstanceId:" + taskInstanceId + " to "+ "userId:"+ actorId,ex);
  }
}
