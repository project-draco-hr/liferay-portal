{
  try {
    return (List<TaskInstanceInfo>)_synchronousMessageSender.send(TaskInstanceRequest.createGetByUserIdRequest(actorId,finished));
  }
 catch (  MessageBusException ex) {
    throw new WorkflowException("Unable to get taskInstances by " + "actorId:" + actorId + " finished:"+ finished,ex);
  }
}
