{
  _spiRegistryImpl=new SPIRegistryImpl();
  _spiRegistryImpl.setErrorSPI(new ErrorSPI());
  SPIRegistryUtil spiRegistryUtil=new SPIRegistryUtil();
  spiRegistryUtil.setSPIRegistry(_spiRegistryImpl);
  _excludedPortletIds=(Set<String>)ReflectionTestUtil.getFieldValue(_spiRegistryImpl,"_excludedPortletIds");
  _portletIds=(Map<SPI,String[]>)ReflectionTestUtil.getFieldValue(_spiRegistryImpl,"_portletIds");
  _portletSPIs=(Map<String,SPI>)ReflectionTestUtil.getFieldValue(_spiRegistryImpl,"_portletSPIs");
}
