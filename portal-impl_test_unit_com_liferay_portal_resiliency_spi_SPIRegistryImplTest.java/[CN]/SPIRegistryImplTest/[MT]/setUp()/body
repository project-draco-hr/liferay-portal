{
  _spiRegistryImpl=new SPIRegistryImpl();
  _spiRegistryImpl.setErrorSPI(new ErrorSPI());
  SPIRegistryUtil spiRegistryUtil=new SPIRegistryUtil();
  spiRegistryUtil.setSPIRegistry(_spiRegistryImpl);
  _excludedPortletIds=ReflectionTestUtil.getFieldValue(_spiRegistryImpl,"_excludedPortletIds");
  _portletIds=ReflectionTestUtil.getFieldValue(_spiRegistryImpl,"_portletIds");
  _portletSPIs=ReflectionTestUtil.getFieldValue(_spiRegistryImpl,"_portletSPIs");
}
