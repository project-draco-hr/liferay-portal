{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.USERS_FULL_NAME_VALIDATOR);
  }
  ClassLoader classLoader=PACLClassLoaderUtil.getPortalClassLoader();
  _originalFullNameValidator=(FullNameValidator)InstanceFactory.newInstance(classLoader,PropsValues.USERS_FULL_NAME_VALIDATOR);
  _fullNameValidator=_originalFullNameValidator;
}
