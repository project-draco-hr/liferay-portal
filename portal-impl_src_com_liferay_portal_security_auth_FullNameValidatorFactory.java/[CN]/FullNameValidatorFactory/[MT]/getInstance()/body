{
  if (_fullNameValidator == null) {
    if (_log.isDebugEnabled()) {
      _log.debug("Instantiate " + PropsValues.USERS_FULL_NAME_VALIDATOR);
    }
    ClassLoader classLoader=PACLClassLoaderUtil.getPortalClassLoader();
    try {
      _fullNameValidator=(FullNameValidator)classLoader.loadClass(PropsValues.USERS_FULL_NAME_VALIDATOR).newInstance();
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Return " + _fullNameValidator.getClass().getName());
  }
  return _fullNameValidator;
}
