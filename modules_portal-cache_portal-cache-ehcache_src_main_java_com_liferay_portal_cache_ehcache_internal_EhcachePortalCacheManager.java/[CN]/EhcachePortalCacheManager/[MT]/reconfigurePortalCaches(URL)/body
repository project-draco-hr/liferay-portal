{
  ObjectValuePair<Configuration,PortalCacheManagerConfiguration> configurationObjectValuePair=baseEhcachePortalCacheManagerConfigurator.getConfigurationObjectValuePair(getPortalCacheManagerName(),configurationURL,_usingDefault);
  reconfigEhcache(configurationObjectValuePair.getKey());
  reconfigPortalCache(configurationObjectValuePair.getValue());
}
