{
  ObjectValuePair<Configuration,PortalCacheManagerConfiguration> configurationObjectValuePair=ehcacheConfigurationHelperUtil.getConfigurationObjectValuePair(getPortalCacheManagerName(),configurationURL,isClusterAware(),_usingDefault);
  reconfigEhcache(configurationObjectValuePair.getKey());
  reconfigPortalCache(configurationObjectValuePair.getValue());
}
