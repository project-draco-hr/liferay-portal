{
  ObjectValuePair<Configuration,PortalCacheManagerConfiguration> configurationObjectValuePair=ehcachePortalCacheManagerConfigurator.getConfigurationObjectValuePair(getPortalCacheManagerName(),configurationURL,isClusterAware(),_usingDefault);
  reconfigEhcache(configurationObjectValuePair.getKey());
  reconfigPortalCache(configurationObjectValuePair.getValue());
}
