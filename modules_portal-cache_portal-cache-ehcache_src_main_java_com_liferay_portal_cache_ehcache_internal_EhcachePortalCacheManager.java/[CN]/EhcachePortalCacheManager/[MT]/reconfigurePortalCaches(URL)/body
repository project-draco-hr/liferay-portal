{
  ObjectValuePair<Configuration,PortalCacheManagerConfiguration> configurationObjectValuePair=abstractEhcachePortalCacheManagerConfigurator.getConfigurationObjectValuePair(getPortalCacheManagerName(),configurationURL,_usingDefault);
  reconfigEhcache(configurationObjectValuePair.getKey());
  reconfigPortalCache(configurationObjectValuePair.getValue());
}
