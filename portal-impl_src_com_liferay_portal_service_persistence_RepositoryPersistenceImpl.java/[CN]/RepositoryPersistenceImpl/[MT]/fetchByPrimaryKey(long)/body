{
  Repository repository=(Repository)EntityCacheUtil.getResult(RepositoryModelImpl.ENTITY_CACHE_ENABLED,RepositoryImpl.class,repositoryId,this);
  if (repository == null) {
    Session session=null;
    try {
      session=openSession();
      repository=(Repository)session.get(RepositoryImpl.class,Long.valueOf(repositoryId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (repository != null) {
        cacheResult(repository);
      }
      closeSession(session);
    }
  }
  return repository;
}
