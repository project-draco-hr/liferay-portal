{
  try {
    MethodKey methodKey=new MethodKey(ShoppingCategoryServiceUtil.class,"updateCategory",_updateCategoryParameterTypes7);
    MethodHandler methodHandler=new MethodHandler(methodKey,categoryId,parentCategoryId,name,description,mergeWithParentCategory,serviceContext);
    Object returnObj=null;
    try {
      returnObj=TunnelUtil.invoke(httpPrincipal,methodHandler);
    }
 catch (    Exception e) {
      if (e instanceof com.liferay.portal.kernel.exception.PortalException) {
        throw (com.liferay.portal.kernel.exception.PortalException)e;
      }
      throw new com.liferay.portal.kernel.exception.SystemException(e);
    }
    return (com.liferay.shopping.model.ShoppingCategory)returnObj;
  }
 catch (  com.liferay.portal.kernel.exception.SystemException se) {
    _log.error(se,se);
    throw se;
  }
}
