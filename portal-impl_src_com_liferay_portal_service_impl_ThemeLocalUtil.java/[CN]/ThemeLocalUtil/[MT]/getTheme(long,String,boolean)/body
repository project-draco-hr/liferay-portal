{
  themeId=GetterUtil.getString(themeId);
  Theme theme=(Theme)_getThemes(companyId).get(themeId);
  if (theme == null) {
    if (wapTheme) {
      themeId=ThemeImpl.getDefaultWapThemeId();
    }
 else {
      themeId=ThemeImpl.getDefaultRegularThemeId();
    }
    theme=(Theme)_themes.get(themeId);
  }
  return theme;
}
