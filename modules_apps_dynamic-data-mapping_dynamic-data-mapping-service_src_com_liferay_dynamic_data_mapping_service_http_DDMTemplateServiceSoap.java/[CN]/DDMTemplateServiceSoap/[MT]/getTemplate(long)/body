{
  try {
    com.liferay.portlet.dynamicdatamapping.model.DDMTemplate returnValue=DDMTemplateServiceUtil.getTemplate(templateId);
    return com.liferay.portlet.dynamicdatamapping.model.DDMTemplateSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
