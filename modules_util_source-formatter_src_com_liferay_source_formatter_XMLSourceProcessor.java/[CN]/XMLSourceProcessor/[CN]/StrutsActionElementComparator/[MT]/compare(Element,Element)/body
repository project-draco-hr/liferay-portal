{
  String path1=actionElement1.attributeValue("path");
  String path2=actionElement2.attributeValue("path");
  if (!path1.startsWith("/portal/") && path2.startsWith("/portal/")) {
    return 1;
  }
  if (path1.startsWith("/portal/") && !path2.startsWith("/portal/")) {
    return -1;
  }
  return path1.compareTo(path2);
}
