{
  NaturalOrderStringComparator naturalOrderStringComparator=new NaturalOrderStringComparator(true,false);
  int value=naturalOrderStringComparator.compare(getElementName(solrElement1),getElementName(solrElement2));
  if (value <= 0) {
    return value;
  }
  String solrElementContent1=solrElement1.asXML();
  String solrElementContent2=solrElement2.asXML();
  int x=_solrElementsContent.indexOf(solrElementContent1) + solrElementContent1.length();
  int y=_solrElementsContent.indexOf(solrElementContent2);
  if (x > y) {
    return -1;
  }
  String betweenElementsContent=_solrElementsContent.substring(x,y);
  if (betweenElementsContent.contains("<!--")) {
    return -1;
  }
  return 1;
}
