{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(OrgGroupRole.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("id.organizationId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("id.organizationId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
