{
  if (name == null) {
    throw new IllegalArgumentException();
  }
  if (_invalid) {
    throw new IllegalStateException();
  }
  if (scope == PortletSession.PORTLET_SCOPE) {
    getHttpSession().removeAttribute(_getPortletScopeName(name));
  }
 else {
    getHttpSession().removeAttribute(name);
  }
}
