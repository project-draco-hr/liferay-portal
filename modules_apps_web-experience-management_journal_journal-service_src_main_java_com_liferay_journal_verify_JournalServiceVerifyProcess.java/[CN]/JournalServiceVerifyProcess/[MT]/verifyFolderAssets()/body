{
  List<JournalFolder> folders=_journalFolderLocalService.getNoAssetFolders();
  if (_log.isDebugEnabled()) {
    _log.debug("Processing " + folders.size() + " folders with no asset");
  }
  for (  JournalFolder folder : folders) {
    try {
      _journalFolderLocalService.updateAsset(folder.getUserId(),folder,null,null,null,null);
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to update asset for folder " + folder.getFolderId() + ": "+ e.getMessage());
      }
    }
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Assets verified for folders");
  }
}
