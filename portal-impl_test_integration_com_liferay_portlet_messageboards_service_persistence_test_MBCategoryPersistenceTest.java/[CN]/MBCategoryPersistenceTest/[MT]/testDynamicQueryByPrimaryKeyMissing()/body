{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBCategory.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("categoryId",RandomTestUtil.nextLong()));
  List<MBCategory> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
