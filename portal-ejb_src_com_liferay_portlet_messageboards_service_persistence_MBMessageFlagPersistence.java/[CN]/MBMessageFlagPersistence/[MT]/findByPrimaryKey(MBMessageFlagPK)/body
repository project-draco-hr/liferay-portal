{
  MBMessageFlag mbMessageFlag=fetchByPrimaryKey(mbMessageFlagPK);
  if (mbMessageFlag == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No MBMessageFlag exists with the primary key " + mbMessageFlagPK);
    }
    throw new NoSuchMessageFlagException("No MBMessageFlag exists with the primary key " + mbMessageFlagPK);
  }
  return mbMessageFlag;
}
