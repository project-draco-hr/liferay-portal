{
  MBMessageFlag mbMessageFlag=fetchByPrimaryKey(messageFlagId);
  if (mbMessageFlag == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No MBMessageFlag exists with the primary key " + messageFlagId);
    }
    throw new NoSuchMessageFlagException("No MBMessageFlag exists with the primary key " + messageFlagId);
  }
  return mbMessageFlag;
}
