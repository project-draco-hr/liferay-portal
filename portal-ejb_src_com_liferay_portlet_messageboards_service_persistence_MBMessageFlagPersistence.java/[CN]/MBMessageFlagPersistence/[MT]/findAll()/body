{
  Session session=null;
  try {
    session=openSession();
    StringBuffer query=new StringBuffer();
    query.append("FROM MBMessageFlag IN CLASS com.liferay.portlet.messageboards.service.persistence.MBMessageFlagHBM ");
    Query q=session.createQuery(query.toString());
    Iterator itr=q.iterate();
    List list=new ArrayList();
    while (itr.hasNext()) {
      MBMessageFlagHBM mbMessageFlagHBM=(MBMessageFlagHBM)itr.next();
      list.add(MBMessageFlagHBMUtil.model(mbMessageFlagHBM));
    }
    return list;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
