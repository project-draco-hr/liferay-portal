{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(mbMessageFlag);
    session.flush();
    mbMessageFlag.setNew(false);
    return mbMessageFlag;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
