{
  if (!GroupPermissionUtil.contains(getPermissionChecker(),groupId,ActionKeys.MANAGE_STAGING) && !GroupPermissionUtil.contains(getPermissionChecker(),groupId,ActionKeys.PUBLISH_STAGING) && !GroupPermissionUtil.contains(getPermissionChecker(),groupId,ActionKeys.ASSIGN_REVIEWER)&& !TasksProposalPermission.contains(getPermissionChecker(),proposalId,ActionKeys.DELETE)) {
    throw new PrincipalException();
  }
  tasksProposalLocalService.deleteProposal(proposalId);
}
