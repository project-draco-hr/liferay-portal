{
  AssetTagProperty assetTagProperty=findByPrimaryKey(tagPropertyId);
  Session session=null;
  try {
    session=openSession();
    AssetTagProperty[] array=new AssetTagPropertyImpl[3];
    array[0]=getByC_K_PrevAndNext(session,assetTagProperty,companyId,key,orderByComparator,true);
    array[1]=assetTagProperty;
    array[2]=getByC_K_PrevAndNext(session,assetTagProperty,companyId,key,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
