{
  try {
    long previousPrimaryKey=-1;
    while (true) {
      long lastPrimaryKey=doPerformActions(previousPrimaryKey);
      if (lastPrimaryKey < 0) {
        return;
      }
      intervalCompleted(previousPrimaryKey,lastPrimaryKey);
      previousPrimaryKey=lastPrimaryKey;
    }
  }
  finally {
    actionsCompleted();
  }
}
