{
  UserTrackerPath userTrackerPath=findByPrimaryKey(userTrackerPathId);
  Session session=null;
  try {
    session=openSession();
    UserTrackerPath[] array=new UserTrackerPathImpl[3];
    array[0]=getByUserTrackerId_PrevAndNext(session,userTrackerPath,userTrackerId,orderByComparator,true);
    array[1]=userTrackerPath;
    array[2]=getByUserTrackerId_PrevAndNext(session,userTrackerPath,userTrackerId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
