{
  StringMaker sm=new StringMaker();
  sm.append("(Announcement.classNameId = ?) AND (");
  for (int i=0; i < classPKs.length; i++) {
    sm.append("(Announcement.classPK = ?)");
    if (i + 1 != classPKs.length) {
      sm.append(" OR ");
    }
 else {
      sm.append(")");
    }
  }
  return sm.toString();
}
