{
  long[] classNameIds=new long[_templateHandlers.size()];
  int i=0;
  for (  Map.Entry<String,TemplateHandler> entry : _templateHandlers.entrySet()) {
    TemplateHandler templateHandler=entry.getValue();
    classNameIds[i++]=_portal.getClassNameId(templateHandler.getClassName());
  }
  return classNameIds;
}
