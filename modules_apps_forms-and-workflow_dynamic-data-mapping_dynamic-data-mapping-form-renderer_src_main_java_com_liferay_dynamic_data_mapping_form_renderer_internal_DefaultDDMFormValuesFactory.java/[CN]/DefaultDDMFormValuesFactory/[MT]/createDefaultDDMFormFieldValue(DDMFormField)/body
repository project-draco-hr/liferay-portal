{
  DDMFormFieldValue ddmFormFieldValue=new DDMFormFieldValue();
  ddmFormFieldValue.setName(ddmFormField.getName());
  Value value=createDefaultValue(ddmFormField);
  ddmFormFieldValue.setValue(value);
  for (  DDMFormField nestedDDMFormField : ddmFormField.getNestedDDMFormFields()) {
    ddmFormFieldValue.addNestedDDMFormFieldValue(createDefaultDDMFormFieldValue(nestedDDMFormField));
  }
  return ddmFormFieldValue;
}
