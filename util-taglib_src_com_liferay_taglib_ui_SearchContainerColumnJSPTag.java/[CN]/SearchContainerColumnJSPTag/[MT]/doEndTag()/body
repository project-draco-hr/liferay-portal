{
  try {
    SearchContainerRowTag parentTag=(SearchContainerRowTag)findAncestorWithClass(this,SearchContainerRowTag.class);
    ResultRow row=parentTag.getRow();
    if (index <= -1) {
      index=row.getEntries().size();
    }
    row.addJSP(index,getAlign(),getValign(),getColspan(),getPath(),pageContext.getServletContext(),getServletRequest(),getServletResponse());
    return EVAL_PAGE;
  }
  finally {
    if (!ServerDetector.isResin()) {
      align=SearchEntry.DEFAULT_ALIGN;
      colspan=SearchEntry.DEFAULT_COLSPAN;
      index=-1;
      name=StringPool.BLANK;
      _path=null;
      valign=SearchEntry.DEFAULT_VALIGN;
    }
  }
}
