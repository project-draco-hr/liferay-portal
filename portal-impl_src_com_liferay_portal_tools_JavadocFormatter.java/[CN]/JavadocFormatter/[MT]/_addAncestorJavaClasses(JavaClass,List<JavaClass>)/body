{
  JavaClass superJavaClass=javaClass.getSuperJavaClass();
  if (superJavaClass != null) {
    ancestorJavaClasses.add(superJavaClass);
    ancestorJavaClasses=_addAncestorJavaClasses(superJavaClass,ancestorJavaClasses);
  }
  JavaClass[] implementedInterfaces=javaClass.getImplementedInterfaces();
  for (  JavaClass implementedInterface : implementedInterfaces) {
    ancestorJavaClasses.add(implementedInterface);
    ancestorJavaClasses=_addAncestorJavaClasses(implementedInterface,ancestorJavaClasses);
  }
  return ancestorJavaClasses;
}
