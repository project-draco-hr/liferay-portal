{
  List<ServiceRegistration<?>> serviceRegistrations=_springContextServices.remove(applicationContext);
  if (serviceRegistrations == null) {
    return;
  }
  for (  ServiceRegistration<?> serviceRegistration : serviceRegistrations) {
    try {
      serviceRegistration.unregister();
    }
 catch (    IllegalStateException ise) {
      if (_log.isDebugEnabled()) {
        _log.debug("Service registration " + serviceRegistration + " is already unregistered");
      }
    }
  }
}
