{
  if (_log.isDebugEnabled()) {
    _log.debug("Starting the OSGi framework");
  }
  _framework.start();
  _setUpPrerequisiteFrameworkServices(_framework.getBundleContext());
  _setUpInitialBundles();
  FrameworkStartLevel frameworkStartLevel=_framework.adapt(FrameworkStartLevel.class);
  frameworkStartLevel.setStartLevel(PropsValues.MODULE_FRAMEWORK_DYNAMIC_INSTALL_START_LEVEL);
  if (_log.isDebugEnabled()) {
    _log.debug("Started the OSGi framework");
  }
}
