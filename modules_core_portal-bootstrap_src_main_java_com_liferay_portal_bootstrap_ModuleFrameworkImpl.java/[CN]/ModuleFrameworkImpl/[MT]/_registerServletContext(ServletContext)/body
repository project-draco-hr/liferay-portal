{
  BundleContext bundleContext=_framework.getBundleContext();
  if (_log.isDebugEnabled()) {
    _log.debug("Register servlet context");
  }
  ServiceRegistration<?> serviceRegistration=bundleContext.registerService(new String[]{ServletContext.class.getName()},servletContext,_getProperties(servletContext,"liferayServletContext"));
  if (_log.isDebugEnabled()) {
    _log.debug("Registered servlet context as " + serviceRegistration.getReference());
  }
}
