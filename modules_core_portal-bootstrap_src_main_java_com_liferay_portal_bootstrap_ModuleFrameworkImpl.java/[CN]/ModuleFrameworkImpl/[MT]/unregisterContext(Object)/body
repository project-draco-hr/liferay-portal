{
  if (context == null) {
    return;
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Unregistering context " + context);
  }
  if (!(context instanceof ApplicationContext)) {
    return;
  }
  _unregisterApplicationContext((ApplicationContext)context);
  if (_log.isDebugEnabled()) {
    _log.debug("Registered context " + context);
  }
}
