{
  if (_framework == null) {
    return;
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Starting the OSGi runtime");
  }
  FrameworkStartLevel frameworkStartLevel=_framework.adapt(FrameworkStartLevel.class);
  frameworkStartLevel.setStartLevel(PropsValues.MODULE_FRAMEWORK_RUNTIME_START_LEVEL);
  if (_log.isDebugEnabled()) {
    _log.debug("Started the OSGi runtime");
  }
}
