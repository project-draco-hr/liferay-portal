{
  try {
    if (_initialized) {
      return;
    }
    try {
      _sassCompiler=new JniSassCompiler();
    }
 catch (    Throwable t) {
      File sassTempDir=_getSassTempDir(servletContext);
      _sassCompiler=new RubySassCompiler(PropsValues.SCRIPTING_JRUBY_COMPILE_MODE,PropsValues.SCRIPTING_JRUBY_COMPILE_THRESHOLD,sassTempDir.getCanonicalPath());
    }
    _initialized=true;
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
