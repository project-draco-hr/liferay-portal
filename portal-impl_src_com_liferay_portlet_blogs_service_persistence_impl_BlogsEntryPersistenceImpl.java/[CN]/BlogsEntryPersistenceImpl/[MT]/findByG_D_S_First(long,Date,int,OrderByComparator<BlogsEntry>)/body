{
  BlogsEntry blogsEntry=fetchByG_D_S_First(groupId,displayDate,status,orderByComparator);
  if (blogsEntry != null) {
    return blogsEntry;
  }
  StringBundler msg=new StringBundler(8);
  msg.append(_NO_SUCH_ENTITY_WITH_KEY);
  msg.append("groupId=");
  msg.append(groupId);
  msg.append(", displayDate=");
  msg.append(displayDate);
  msg.append(", status=");
  msg.append(status);
  msg.append(StringPool.CLOSE_CURLY_BRACE);
  throw new NoSuchEntryException(msg.toString());
}
