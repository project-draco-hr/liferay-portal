{
  BlogsEntry blogsEntry=fetchByG_NotS_First(groupId,status,orderByComparator);
  if (blogsEntry != null) {
    return blogsEntry;
  }
  StringBundler msg=new StringBundler(6);
  msg.append(_NO_SUCH_ENTITY_WITH_KEY);
  msg.append("groupId=");
  msg.append(groupId);
  msg.append(", status=");
  msg.append(status);
  msg.append(StringPool.CLOSE_CURLY_BRACE);
  throw new NoSuchEntryException(msg.toString());
}
