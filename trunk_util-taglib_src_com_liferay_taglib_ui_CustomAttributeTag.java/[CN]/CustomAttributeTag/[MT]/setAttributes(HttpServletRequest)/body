{
  request.setAttribute("liferay-ui:custom-attribute:className",_className);
  request.setAttribute("liferay-ui:custom-attribute:classPK",String.valueOf(_classPK));
  request.setAttribute("liferay-ui:custom-attribute:editable",String.valueOf(_editable));
  request.setAttribute("liferay-ui:custom-attribute:label",String.valueOf(_label));
  request.setAttribute("liferay-ui:custom-attribute:name",_name);
}
