{
  DataSource dataSource=getDataSource();
  String sql="select * from " + tableName;
  if (companyId > 0) {
    sql+=" where companyId = ?";
  }
  try (Connection con=dataSource.getConnection();PreparedStatement ps=buildPreparedStatement(con,sql,companyId);ResultSet rs=ps.executeQuery()){
    ResultSetMetaData resultSetMetaData=rs.getMetaData();
    int columnCount=resultSetMetaData.getColumnCount();
    while (rs.next()) {
      String[] fields=new String[columnCount];
      for (int i=0; i < columnCount; i++) {
        fields[i]=serializeTableField(rs.getObject(i + 1));
      }
      generateInsertSQL(outputStream,tableName,fields);
    }
  }
 catch (  IOException|SQLException e) {
    _logger.error("Unable to export " + tableName,e);
  }
}
