{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    if (WikiPagePermissionChecker.contains(themeDisplay.getPermissionChecker(),_page,ActionKeys.UPDATE) && WikiNodePermissionChecker.contains(themeDisplay.getPermissionChecker(),_page.getNodeId(),ActionKeys.ADD_PAGE)) {
      return true;
    }
  }
 catch (  PortalException pe) {
  }
  return false;
}
