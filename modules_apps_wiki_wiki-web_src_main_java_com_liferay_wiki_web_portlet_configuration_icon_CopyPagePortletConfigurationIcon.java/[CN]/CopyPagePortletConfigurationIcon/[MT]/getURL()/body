{
  PortletURL copyURL=PortalUtil.getControlPanelPortletURL(portletRequest,WikiPortletKeys.WIKI_ADMIN,PortletRequest.RENDER_PHASE);
  copyURL.setParameter("mvcRenderCommandName","/wiki/edit_page");
  copyURL.setParameter("redirect",themeDisplay.getURLCurrent());
  copyURL.setParameter("nodeId",String.valueOf(_node.getNodeId()));
  copyURL.setParameter("title",StringPool.BLANK);
  copyURL.setParameter("editTitle","1");
  copyURL.setParameter("templateNodeId",String.valueOf(_page.getNodeId()));
  copyURL.setParameter("templateTitle",HtmlUtil.unescape(_page.getTitle()));
  return copyURL.toString();
}
