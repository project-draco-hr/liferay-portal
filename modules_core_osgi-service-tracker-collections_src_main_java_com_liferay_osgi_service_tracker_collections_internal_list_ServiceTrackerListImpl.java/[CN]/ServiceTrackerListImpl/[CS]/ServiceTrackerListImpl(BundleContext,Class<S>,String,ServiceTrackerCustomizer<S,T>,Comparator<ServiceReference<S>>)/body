{
  _bundleContext=bundleContext;
  _serviceTrackerCustomizer=serviceTrackerCustomizer;
  if (comparator == null) {
    _comparator=Collections.reverseOrder();
  }
 else {
    _comparator=new ServiceReferenceServiceTupleComparator<>(comparator);
  }
  _serviceTracker=ServiceTrackerUtil.createServiceTracker(_bundleContext,clazz,filterString,new ServiceReferenceServiceTrackerCustomizer());
}
