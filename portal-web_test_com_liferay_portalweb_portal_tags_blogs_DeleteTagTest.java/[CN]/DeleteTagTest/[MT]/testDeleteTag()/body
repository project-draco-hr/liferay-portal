{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Tags")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Tags"));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(500);
  selenium.clickAt("link=selenium2 liferay2",RuntimeVariables.replace(""));
  Thread.sleep(500);
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//input[@value='Delete']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("//input[@value='Delete']");
  assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this tag[\\s\\S]$"));
  Thread.sleep(500);
  assertFalse(selenium.isTextPresent("link=selenium2 liferay2"));
}
