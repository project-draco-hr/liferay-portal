{
  String useCustomTitle=GetterUtil.getString(portletSetup.getValue("portletSetupUseCustomTitle",StringPool.BLANK));
  if (!useCustomTitle.equals("true")) {
    return null;
  }
  Map<Locale,String> map=new HashMap<>();
  boolean empty=true;
  for (  Locale locale : LanguageUtil.getAvailableLocales()) {
    String portletTitle=getPortletTitle(portletSetup,LocaleUtil.toLanguageId(locale));
    if (Validator.isNotNull(portletTitle)) {
      empty=false;
    }
 else {
      portletTitle=StringPool.BLANK;
    }
    map.put(locale,portletTitle);
  }
  if (!empty) {
    return map;
  }
  return null;
}
