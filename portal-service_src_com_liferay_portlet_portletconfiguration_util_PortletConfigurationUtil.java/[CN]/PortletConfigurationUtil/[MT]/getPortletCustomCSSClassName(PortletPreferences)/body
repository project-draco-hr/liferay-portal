{
  String customCSSClassName=StringPool.BLANK;
  String css=portletSetup.getValue("portletSetupCss",StringPool.BLANK);
  if (Validator.isNotNull(css)) {
    JSONObject cssJSONObject=PortletSetupUtil.cssToJSONObject(portletSetup,css);
    JSONObject advancedDataJSONObject=cssJSONObject.getJSONObject("advancedData");
    if (advancedDataJSONObject != null) {
      customCSSClassName=advancedDataJSONObject.getString("customCSSClassName");
    }
  }
  return customCSSClassName;
}
