{
  if (this == obj) {
    return true;
  }
  if (!(obj instanceof ShoppingCartItem)) {
    return false;
  }
  ShoppingCartItem cartItem=(ShoppingCartItem)obj;
  if (getItem().equals(cartItem.getItem()) && getFields().equals(cartItem.getFields())) {
    return true;
  }
 else {
    return false;
  }
}
