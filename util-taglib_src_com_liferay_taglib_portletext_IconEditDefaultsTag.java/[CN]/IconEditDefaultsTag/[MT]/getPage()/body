{
  PortletDisplay portletDisplay=(PortletDisplay)pageContext.getAttribute("portletDisplay");
  if (!portletDisplay.isShowEditDefaultsIcon()) {
    return null;
  }
  setImage("../portlet/edit_defaults");
  setMessage("default-preferences");
  setToolTip(false);
  setUrl(portletDisplay.getURLEditDefaults());
  return super.getPage();
}
