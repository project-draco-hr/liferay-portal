{
  ExpandoColumn column=ExpandoTestUtil.addColumn(_expandoTable,"Test Column",ExpandoColumnConstants.STRING_ARRAY);
  ExpandoValue value=ExpandoTestUtil.addValue(_expandoTable,column,new String[]{"one","two, three"});
  value=ExpandoValueLocalServiceUtil.getExpandoValue(value.getValueId());
  String[] data=value.getStringArray();
  Assert.assertEquals(2,data.length);
  Assert.assertEquals("one",data[0]);
  Assert.assertEquals("two, three",data[1]);
}
