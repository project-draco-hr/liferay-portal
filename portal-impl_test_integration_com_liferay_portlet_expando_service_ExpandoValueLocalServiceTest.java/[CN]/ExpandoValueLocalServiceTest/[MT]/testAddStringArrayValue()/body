{
  ExpandoTable table=ExpandoTestUtil.addTable(_classNameId,"testAddStringArrayValue");
  ExpandoColumn column=ExpandoTestUtil.addColumn(table,"Test Column",ExpandoColumnConstants.STRING_ARRAY);
  ExpandoValue value=ExpandoTestUtil.addValue(table,column,new String[]{"one","two, three"});
  value=ExpandoValueLocalServiceUtil.getExpandoValue(value.getValueId());
  String[] data=value.getStringArray();
  Assert.assertEquals(2,data.length);
  Assert.assertEquals("one",data[0]);
  Assert.assertEquals("two, three",data[1]);
}
