{
  ResourcePermission resourcePermission=(ResourcePermission)EntityCacheUtil.getResult(ResourcePermissionModelImpl.ENTITY_CACHE_ENABLED,ResourcePermissionImpl.class,resourcePermissionId,this);
  if (resourcePermission == null) {
    Session session=null;
    try {
      session=openSession();
      resourcePermission=(ResourcePermission)session.get(ResourcePermissionImpl.class,Long.valueOf(resourcePermissionId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (resourcePermission != null) {
        cacheResult(resourcePermission);
      }
      closeSession(session);
    }
  }
  return resourcePermission;
}
