{
  long parentLayoutId=layout.getParentLayoutId();
  List missingParents=new ArrayList();
  while (parentLayoutId > 0) {
    try {
      LayoutLocalServiceUtil.getLayout(targetGroupId,layout.isPrivateLayout(),parentLayoutId);
      break;
    }
 catch (    NoSuchLayoutException e) {
      Layout parent=LayoutLocalServiceUtil.getLayout(layout.getGroupId(),layout.isPrivateLayout(),parentLayoutId);
      missingParents.add(parent);
      parentLayoutId=parent.getParentLayoutId();
    }
  }
  return missingParents;
}
