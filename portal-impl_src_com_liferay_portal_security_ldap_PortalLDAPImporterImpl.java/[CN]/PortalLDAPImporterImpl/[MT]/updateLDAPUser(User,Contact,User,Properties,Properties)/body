{
  Contact contact=user.getContact();
  for (  String propertyName : _CONTACT_PROPERTY_NAMES) {
    if (!contactMappings.containsKey(propertyName) || _ldapUserIgnoreAttributes.contains(propertyName)) {
      setProperty(ldapContact,contact,propertyName);
    }
  }
  for (  String propertyName : _USER_PROPERTY_NAMES) {
    if (!userMappings.containsKey(propertyName) || _ldapUserIgnoreAttributes.contains(propertyName)) {
      setProperty(ldapUser,user,propertyName);
    }
  }
}
