{
  Contact contact=user.getContact();
  Set<String> ldapUserIgnoreAttributes=SetUtil.fromArray(PropsValues.LDAP_USER_IGNORE_ATTRIBUTES);
  for (  String propertyName : _CONTACT_PROPERTY_NAMES) {
    if (!contactMappings.containsKey(propertyName) || ldapUserIgnoreAttributes.contains(propertyName)) {
      setProperty(ldapContact,contact,propertyName);
    }
  }
  for (  String propertyName : _USER_PROPERTY_NAMES) {
    if (!userMappings.containsKey(propertyName) || ldapUserIgnoreAttributes.contains(propertyName)) {
      setProperty(ldapUser,user,propertyName);
    }
  }
}
