{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Test Transaction Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Test Transaction Page",RuntimeVariables.replace("Test Transaction Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//p[1]","BarLocalServiceUtil.addBar_Success=PASSED"));
  assertTrue(selenium.isPartialText("//p[1]","BarLocalServiceUtil.addBarAndClassName_PortalRollback=PASSED"));
  assertTrue(selenium.isPartialText("//p[1]","BarLocalServiceUtil.addBarAndClassName_PortletRollback=PASSED"));
  assertTrue(selenium.isPartialText("//p[2]","PortalServiceUtil.testAddClassNameAndTestTransactionPortletBar_Success=PASSED"));
  assertTrue(selenium.isPartialText("//p[2]","PortalServiceUtil.testAddClassNameAndTestTransactionPortletBar_PortalRollback=PASSED"));
  assertTrue(selenium.isPartialText("//p[2]","PortletServiceUtil.testAddClassNameAndTestTransactionPortletBar_PortletRollback=PASSED"));
}
