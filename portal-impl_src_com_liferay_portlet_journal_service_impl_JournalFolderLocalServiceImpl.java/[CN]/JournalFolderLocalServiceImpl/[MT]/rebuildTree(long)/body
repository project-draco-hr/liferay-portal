{
  TreePathUtil.rebuildTree(companyId,JournalFolderConstants.DEFAULT_PARENT_FOLDER_ID,new TreeModelFinder<JournalFolder>(){
    @Override public List<JournalFolder> findTreeModels(    long previousId,    long companyId,    long parentId,    int size) throws SystemException {
      return journalFolderPersistence.findByF_C_P_NotS(previousId,companyId,parentId,WorkflowConstants.STATUS_IN_TRASH,QueryUtil.ALL_POS,size,new FolderIdComparator());
    }
  }
);
}
