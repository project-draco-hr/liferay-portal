{
  Folder folder=PortletFileRepositoryUtil.getPortletFolder(stagingRequestId);
  PortletFileRepositoryUtil.addPortletFileEntry(folder.getGroupId(),userId,Group.class.getName(),folder.getGroupId(),PortletKeys.SITES_ADMIN,folder.getFolderId(),new UnsyncByteArrayInputStream(bytes),fileName,ContentTypes.APPLICATION_ZIP,false);
}
