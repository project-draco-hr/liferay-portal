{
  ServiceContext serviceContext=new ServiceContext();
  Repository repository=PortletFileRepositoryUtil.addPortletRepository(groupId,_PORTLET_REPOSITORY_ID,serviceContext);
  Folder folder=PortletFileRepositoryUtil.addPortletFolder(userId,repository.getRepositoryId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,checksum,serviceContext);
  return folder.getFolderId();
}
