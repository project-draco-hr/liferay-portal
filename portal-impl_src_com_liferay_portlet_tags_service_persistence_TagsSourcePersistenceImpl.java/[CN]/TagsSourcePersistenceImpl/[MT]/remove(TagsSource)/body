{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(tagsSource);
    }
  }
  tagsSource=removeImpl(tagsSource);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(tagsSource);
    }
  }
  return tagsSource;
}
