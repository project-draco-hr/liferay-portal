{
  Session session=null;
  try {
    session=openSession();
    tagsSource=(TagsSource)session.merge(tagsSource);
    session.delete(tagsSource);
    session.flush();
    return tagsSource;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(TagsSource.class.getName());
  }
}
