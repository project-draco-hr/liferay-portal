{
  Session session=null;
  try {
    session=openSession();
    if (BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(TagsSourceImpl.class,tagsSource.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(tagsSource);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  TagsSourceModelImpl tagsSourceModelImpl=(TagsSourceModelImpl)tagsSource;
  EntityCacheUtil.removeResult(TagsSourceModelImpl.ENTITY_CACHE_ENABLED,TagsSource.class,tagsSource.getPrimaryKey());
  return tagsSource;
}
