{
  Session session=null;
  try {
    session=openSession();
    session.delete(tagsSource);
    session.flush();
    return tagsSource;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(TagsSource.class.getName());
  }
}
