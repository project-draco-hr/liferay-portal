{
  DDMFormFieldValue ddmFormFieldValue=new DDMFormFieldValue();
  ddmFormFieldValue.setName(dynamicElement.attributeValue("name"));
  List<Element> dynamicContentElements=dynamicElement.elements("dynamic-content");
  ddmFormFieldValue.setValue(getValue(dynamicContentElements));
  ddmFormFieldValue.setNestedDDMFormFields(getDDMFormFieldValues(dynamicElement.elements("dynamic-element")));
  return ddmFormFieldValue;
}
