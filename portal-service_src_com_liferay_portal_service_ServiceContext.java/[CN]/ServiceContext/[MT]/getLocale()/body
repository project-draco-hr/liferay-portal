{
  if (_locale != null) {
    return _locale;
  }
  Locale locale=Locale.getDefault();
  if ((_languageId != null) && (_languageId.indexOf(StringPool.UNDERLINE) != -1)) {
    String parts[]=_languageId.split(StringPool.UNDERLINE);
    locale=new Locale(parts[0],parts[1]);
  }
  _locale=locale;
  return _locale;
}
