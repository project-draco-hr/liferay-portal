{
  StringMaker sm=new StringMaker();
  sm.append(getClass().getName());
  sm.append("[weeks=");
  sm.append(weeks);
  sm.append(",days=");
  sm.append(days);
  sm.append(",hours=");
  sm.append(hours);
  sm.append(",minutes=");
  sm.append(minutes);
  sm.append(",seconds=");
  sm.append(seconds);
  sm.append("]");
  return sm.toString();
}
