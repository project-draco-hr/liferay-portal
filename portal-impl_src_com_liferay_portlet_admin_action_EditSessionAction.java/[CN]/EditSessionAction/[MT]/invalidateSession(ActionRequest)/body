{
  String sessionId=ParamUtil.getString(req,"sessionId");
  HttpSession userSession=PortalSessionContext.get(sessionId);
  if (userSession != null) {
    try {
      if (!req.getPortletSession().getId().equals(sessionId)) {
        userSession.invalidate();
      }
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
}
