{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Web Content Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Web Content Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("This is a test wcd web content. Edit."),selenium.getText("//div[@class='journal-content-article']/p"));
  selenium.clickAt("//img[@title='Edit Web Content']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Version: 1.1"),selenium.getText("//span[@class='workflow-version']"));
  selenium.click(RuntimeVariables.replace("//input[@value='Delete this Version']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this article version[\\s\\S]$"));
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("This is a test wcd web content."),selenium.getText("//div[@class='journal-content-article']/p"));
}
