{
  if (checkedFiles.contains(fileName)) {
    return false;
  }
  checkedFiles.add(fileName);
  if (!_jspFilesMap.containsKey(fileName)) {
    return false;
  }
  String content=_jspFilesMap.get(fileName);
  if (Validator.isNull(content)) {
    return false;
  }
  Pattern pattern=Pattern.compile("[^A-Za-z0-9_]" + importedClassName + "[^A-Za-z0-9_"+ StringPool.QUOTE+ "]");
  Matcher matcher=pattern.matcher(content);
  if (matcher.find()) {
    return true;
  }
  includedFiles=_getJSPIncludedFiles(fileName,includedFiles);
  fileName=fileName.replace(CharPool.BACK_SLASH,CharPool.FORWARD_SLASH);
  fileName=fileName.replaceFirst("portal-web" + StringPool.FORWARD_SLASH + "docroot",StringPool.BLANK);
  if (fileName.endsWith("init.jsp")) {
    includedFiles=_getJSPReferenceFiles(fileName,includedFiles);
  }
  for (  String includedFile : includedFiles) {
    if (!checkedFiles.contains(includedFile) && _checkJSPImportedClass(includedFile,importedClassName,includedFiles,checkedFiles)) {
      return true;
    }
  }
  return false;
}
