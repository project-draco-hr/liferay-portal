{
  if (true) {
    return line;
  }
  String newLine=StringUtil.replace(line,new String[]{"new Boolean(","new Byte(","new Character(","new Integer(","new Long(","new Short("},new String[]{"Boolean.valueOf(","Byte.valueOf(","Character.valueOf(","Integer.valueOf(","Long.valueOf(","Short.valueOf("});
  if (!line.equals(newLine)) {
    _sourceFormatterHelper.printError(fileName,"> new Primitive(: " + fileName + " "+ lineCount);
  }
  return newLine;
}
