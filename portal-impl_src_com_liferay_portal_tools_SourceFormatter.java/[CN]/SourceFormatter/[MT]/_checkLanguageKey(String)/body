{
  StringBundler sb=new StringBundler();
  int length=s.length();
  int count=0;
  for (int i=0; i < length; i++) {
    char c=s.charAt(i);
switch (c) {
case CharPool.CLOSE_PARENTHESIS:
      if (count <= 1) {
        return null;
      }
    count--;
  break;
case CharPool.OPEN_PARENTHESIS:
count++;
break;
case CharPool.QUOTE:
while ((count <= 1) && (i < length)) {
i++;
if (s.charAt(i) == CharPool.QUOTE) {
return sb.toString();
}
sb.append(s.charAt(i));
}
}
}
return null;
}
