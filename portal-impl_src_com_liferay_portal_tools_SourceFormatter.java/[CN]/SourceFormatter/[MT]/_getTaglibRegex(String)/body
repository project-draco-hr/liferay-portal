{
  StringBundler sb=new StringBundler(_TAG_LIBRARIES.length * 2 + 7);
  sb.append("<(");
  for (int i=0; i < _TAG_LIBRARIES.length; i++) {
    sb.append(_TAG_LIBRARIES[i]);
    if ((i + 1) != _TAG_LIBRARIES.length) {
      sb.append(StringPool.PIPE);
    }
  }
  sb.append("):([^>]|%>)*");
  sb.append(quoteType);
  sb.append("<%=.*");
  sb.append(quoteType);
  sb.append(".*%>");
  sb.append(quoteType);
  sb.append("([^>]|%>)*>");
  return sb.toString();
}
