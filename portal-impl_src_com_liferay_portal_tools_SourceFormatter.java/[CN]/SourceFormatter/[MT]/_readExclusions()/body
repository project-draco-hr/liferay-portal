{
  _exclusions=new Properties();
  ClassLoader classLoader=SourceFormatter.class.getClassLoader();
  String sourceFormatterExclusions=System.getProperty("source-formatter-exclusions","com/liferay/portal/tools/dependencies/" + "source_formatter_exclusions.properties");
  URL url=classLoader.getResource(sourceFormatterExclusions);
  if (url == null) {
    return;
  }
  InputStream is=url.openStream();
  _exclusions.load(is);
  is.close();
}
