{
  _exclusions=new Properties();
  ClassLoader classLoader=SourceFormatter.class.getClassLoader();
  URL url=classLoader.getResource("com/liferay/portal/tools/dependencies/" + "source_formatter_exclusions.properties");
  if (url == null) {
    return;
  }
  InputStream is=url.openStream();
  _exclusions.load(is);
  is.close();
}
