{
  try {
    return _fileUtil.read("old-copyright.txt");
  }
 catch (  Exception e1) {
    try {
      return _fileUtil.read("../old-copyright.txt");
    }
 catch (    Exception e2) {
      try {
        return _fileUtil.read("../../old-copyright.txt");
      }
 catch (      Exception e3) {
        return null;
      }
    }
  }
}
