{
  List<String> importClassNames=_getJSPUnusedImportClassNames(fileName,content);
  for (  String importClassName : importClassNames) {
    content=StringUtil.replace(content,new String[]{"\n\n<%@ page import=\"" + importClassName + "\" %>\n\n","<%@ page import=\"" + importClassName + "\" %>\n"},new String[]{"\n\n",StringPool.BLANK});
  }
  return content;
}
