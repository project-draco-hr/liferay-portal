{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(dlFileEntryPK));
  }
  com.liferay.portlet.documentlibrary.model.DLFileEntry dlFileEntry=getPersistence().remove(dlFileEntryPK);
  if (listener != null) {
    listener.onAfterRemove(dlFileEntry);
  }
  return dlFileEntry;
}
