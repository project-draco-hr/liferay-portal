{
  ModelListener listener=null;
  if (Validator.isNotNull(_LISTENER)) {
    try {
      listener=(ModelListener)Class.forName(_LISTENER).newInstance();
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(dlFileEntryPK));
  }
  com.liferay.portlet.documentlibrary.model.DLFileEntry dlFileEntry=getPersistence().remove(dlFileEntryPK);
  if (listener != null) {
    listener.onAfterRemove(dlFileEntry);
  }
  return dlFileEntry;
}
