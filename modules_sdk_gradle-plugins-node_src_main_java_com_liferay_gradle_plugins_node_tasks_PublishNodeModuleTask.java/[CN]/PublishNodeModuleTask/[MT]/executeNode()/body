{
  try {
    setArgs(getCompleteArgs());
    createNpmrcFile();
    createPackageJsonFile();
    super.executeNode();
  }
 catch (  IOException ioe) {
    throw new GradleException(ioe.getMessage(),ioe);
  }
 finally {
    Project project=getProject();
    project.delete(getNpmrcFile(),getPackageJsonFile());
  }
}
