{
  if (file.getName().endsWith(".xml")) {
    if (_log.isDebugEnabled()) {
      _log.debug(file.getPath() + " has a matching extension");
    }
    return true;
  }
 else {
    if (_log.isDebugEnabled()) {
      _log.debug(file.getPath() + " does not have a matching extension");
    }
    return false;
  }
}
