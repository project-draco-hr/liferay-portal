{
  try {
    return JournalArticleLocalServiceUtil.getArticleDisplay(groupId,articleId,templateId,languageId,themeDisplay);
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to get display for " + groupId + " "+ articleId+ " "+ languageId);
    }
    return null;
  }
}
