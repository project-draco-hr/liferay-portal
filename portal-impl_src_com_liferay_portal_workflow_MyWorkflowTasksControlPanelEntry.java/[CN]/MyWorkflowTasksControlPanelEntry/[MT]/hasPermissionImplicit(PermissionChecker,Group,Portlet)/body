{
  if (WorkflowTaskManagerUtil.getWorkflowTaskCountByUser(permissionChecker.getCompanyId(),permissionChecker.getUserId(),null) > 0) {
    return true;
  }
  if (WorkflowTaskManagerUtil.getWorkflowTaskCountByUserRoles(permissionChecker.getCompanyId(),permissionChecker.getUserId(),null) > 0) {
    return true;
  }
  return false;
}
