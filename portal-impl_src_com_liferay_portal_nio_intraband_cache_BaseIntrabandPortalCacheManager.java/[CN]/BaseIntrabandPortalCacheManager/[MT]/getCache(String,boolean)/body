{
  PortalCache<K,V> portalCache=_portalCaches.get(portalCacheName);
  if (portalCache == null) {
    portalCache=(PortalCache<K,V>)IntrabandProxyUtil.newStubInstance(_STUB_CLASS,portalCacheName,_registrationReference,WarnLogExceptionHandler.INSTANCE);
    _portalCaches.put(portalCacheName,portalCache);
  }
  return portalCache;
}
