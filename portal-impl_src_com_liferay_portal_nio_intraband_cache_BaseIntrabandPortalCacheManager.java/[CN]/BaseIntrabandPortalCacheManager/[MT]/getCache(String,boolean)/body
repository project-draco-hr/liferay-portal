{
  PortalCache<K,V> portalCache=_portalCaches.get(name);
  if (portalCache == null) {
    portalCache=(PortalCache<K,V>)IntrabandProxyUtil.newStubInstance(_STUB_CLASS,name,_registrationReference,WarnLogExceptionHandler.INSTANCE);
    _portalCaches.put(name,portalCache);
  }
  return portalCache;
}
