{
  Session session=null;
  try {
    session=openSession();
    MBCategory mbCategory=(MBCategory)session.get(MBCategoryImpl.class,categoryId);
    if (mbCategory == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No MBCategory exists with the primary key " + categoryId);
      }
      throw new NoSuchCategoryException("No MBCategory exists with the primary key " + categoryId);
    }
    return remove(mbCategory);
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
