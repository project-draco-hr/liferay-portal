{
  Session session=null;
  try {
    session=openSession();
    if (mbCategory.isNew()) {
      session.save(mbCategory);
    }
    session.flush();
    mbCategory.setNew(false);
    return mbCategory;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
