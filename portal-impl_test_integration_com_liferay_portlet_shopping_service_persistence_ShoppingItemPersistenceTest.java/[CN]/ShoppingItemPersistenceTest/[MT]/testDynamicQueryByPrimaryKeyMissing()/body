{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ShoppingItem.class,ShoppingItem.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("itemId",ServiceTestUtil.nextLong()));
  List<ShoppingItem> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
