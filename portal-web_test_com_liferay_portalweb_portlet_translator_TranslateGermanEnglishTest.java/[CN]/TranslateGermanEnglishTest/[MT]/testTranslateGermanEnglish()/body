{
  selenium.select("_26_id","label=German to English");
  selenium.type("_26_text","Mein Name ist \u00dcbersetzer von Liferay, flie\u00dfender in mehr als 6 Millionen Kommunikationsformen.");
  selenium.click("//input[@value='Translate']");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isTextPresent("My name is a translator of Liferay, more flowing into more than 6 million communication forms.")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
