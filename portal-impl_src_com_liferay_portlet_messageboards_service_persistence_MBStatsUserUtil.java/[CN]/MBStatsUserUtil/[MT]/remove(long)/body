{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(statsUserId));
  }
  com.liferay.portlet.messageboards.model.MBStatsUser mbStatsUser=getPersistence().remove(statsUserId);
  if (listener != null) {
    listener.onAfterRemove(mbStatsUser);
  }
  return mbStatsUser;
}
