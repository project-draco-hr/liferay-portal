{
  Session session=null;
  try {
    session=openSession();
    PollsQuestion pollsQuestion=(PollsQuestion)session.get(PollsQuestionImpl.class,Long.valueOf(questionId));
    if (pollsQuestion == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + questionId);
      }
      throw new NoSuchQuestionException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + questionId);
    }
    return pollsQuestionPersistence.remove(pollsQuestion);
  }
 catch (  NoSuchQuestionException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
