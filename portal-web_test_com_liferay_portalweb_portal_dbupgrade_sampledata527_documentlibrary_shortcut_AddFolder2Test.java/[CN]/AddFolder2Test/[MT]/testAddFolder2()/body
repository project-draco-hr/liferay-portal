{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/document-library-shortcut-community/");
  selenium.clickAt("link=Document Library Page",RuntimeVariables.replace("Document Library Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Folder']",RuntimeVariables.replace("Add Folder"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_20_name']",RuntimeVariables.replace("Test2 Folder2"));
  selenium.type("//textarea[@id='_20_description']",RuntimeVariables.replace("This is test2 folder2."));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request processed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertTrue(selenium.isElementPresent("//tr[4]/td[1]/a[2]/b"));
}
