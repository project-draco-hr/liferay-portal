{
  name=StringUtil.lowerCase(name);
  description=StringUtil.lowerCase(description);
  String[] names=CustomSQLUtil.keywords(name);
  String[] realNames=CustomSQLUtil.keywords(realName);
  String[] descriptions=CustomSQLUtil.keywords(description);
  return countByC_PG_C_N_D(companyId,GroupConstants.ANY_PARENT_GROUP_ID,StringPool.NOT_EQUAL,_getGroupOrganizationClassNameIds(),names,realNames,descriptions,params,andOperator);
}
