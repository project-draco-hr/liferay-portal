{
  try {
    if (!signedIn) {
      return hasGuestPermission(name,primKey,actionId);
    }
 else {
      boolean value=false;
      if (checkGuest) {
        signedIn=false;
        value=hasGuestPermission(name,primKey,actionId);
        signedIn=true;
      }
      if (!value) {
        value=hasUserPermission(groupId,name,primKey,actionId);
      }
      return value;
    }
  }
 catch (  Exception e) {
    _log.error(StackTraceUtil.getStackTrace(e));
    return false;
  }
}
