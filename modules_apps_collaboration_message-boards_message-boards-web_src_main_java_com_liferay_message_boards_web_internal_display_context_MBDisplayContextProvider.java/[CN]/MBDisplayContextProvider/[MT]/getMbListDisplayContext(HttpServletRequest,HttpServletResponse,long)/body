{
  Collection<MBDisplayContextFactory> mbDisplayContextFactories=_mbDisplayContextFactories.values();
  MBListDisplayContext mbListDisplayContext=new DefaultMBListDisplayContext(request,response,categoryId);
  for (  MBDisplayContextFactory mbDisplayContextFactory : mbDisplayContextFactories) {
    mbListDisplayContext=mbDisplayContextFactory.getMBListDisplayContext(mbListDisplayContext,request,response,categoryId);
  }
  return mbListDisplayContext;
}
