{
  selenium.click(RuntimeVariables.replace("link=Roles"));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//strong/span")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("//strong/span");
  selenium.click("//div[2]/ul/li[4]/nobr/a");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Available")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Available"));
  selenium.waitForPageToLoad("30000");
  selenium.typeKeys("toggle_id_enterprise_admin_user_searchkeywords",RuntimeVariables.replace("Selenium"));
