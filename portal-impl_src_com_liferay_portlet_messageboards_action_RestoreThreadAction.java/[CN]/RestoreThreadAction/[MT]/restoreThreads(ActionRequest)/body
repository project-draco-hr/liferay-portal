{
  ThemeDisplay themeDislay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long[] restoreThreadIds=StringUtil.split(ParamUtil.getString(actionRequest,"restoreThreadIds"),0L);
  for (  long restoreThreadId : restoreThreadIds) {
    MBThreadLocalServiceUtil.restoreThreadFromTrash(themeDislay.getUserId(),restoreThreadId);
  }
}
