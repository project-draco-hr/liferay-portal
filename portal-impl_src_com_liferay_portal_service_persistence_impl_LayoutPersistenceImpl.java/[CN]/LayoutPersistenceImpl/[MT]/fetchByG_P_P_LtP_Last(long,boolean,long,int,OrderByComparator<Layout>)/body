{
  int count=countByG_P_P_LtP(groupId,privateLayout,parentLayoutId,priority);
  if (count == 0) {
    return null;
  }
  List<Layout> list=findByG_P_P_LtP(groupId,privateLayout,parentLayoutId,priority,count - 1,count,orderByComparator);
  if (!list.isEmpty()) {
    return list.get(0);
  }
  return null;
}
