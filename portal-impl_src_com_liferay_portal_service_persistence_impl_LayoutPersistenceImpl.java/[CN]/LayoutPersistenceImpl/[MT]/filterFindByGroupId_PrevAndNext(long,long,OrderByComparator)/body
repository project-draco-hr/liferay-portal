{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByGroupId_PrevAndNext(plid,groupId,orderByComparator);
  }
  Layout layout=findByPrimaryKey(plid);
  Session session=null;
  try {
    session=openSession();
    Layout[] array=new LayoutImpl[3];
    array[0]=filterGetByGroupId_PrevAndNext(session,layout,groupId,orderByComparator,true);
    array[1]=layout;
    array[2]=filterGetByGroupId_PrevAndNext(session,layout,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
