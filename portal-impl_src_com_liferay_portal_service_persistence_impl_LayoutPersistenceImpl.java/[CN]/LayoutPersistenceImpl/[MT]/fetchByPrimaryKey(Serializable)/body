{
  Serializable serializable=entityCache.getResult(LayoutModelImpl.ENTITY_CACHE_ENABLED,LayoutImpl.class,primaryKey);
  if (serializable == nullModel) {
    return null;
  }
  Layout layout=(Layout)serializable;
  if (layout == null) {
    Session session=null;
    try {
      session=openSession();
      layout=(Layout)session.get(LayoutImpl.class,primaryKey);
      if (layout != null) {
        cacheResult(layout);
      }
 else {
        entityCache.putResult(LayoutModelImpl.ENTITY_CACHE_ENABLED,LayoutImpl.class,primaryKey,nullModel);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(LayoutModelImpl.ENTITY_CACHE_ENABLED,LayoutImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return layout;
}
