{
  String fileName=RandomTestUtil.randomString();
  store.addFile(companyId,repositoryId,fileName,_DATA_VERSION_1);
  addVersions(fileName,1);
  store.deleteFile(companyId,repositoryId,fileName,Store.VERSION_DEFAULT);
  Assert.assertFalse(store.hasFile(companyId,repositoryId,fileName,Store.VERSION_DEFAULT));
  Assert.assertTrue(store.hasFile(companyId,repositoryId,fileName,"1.1"));
}
