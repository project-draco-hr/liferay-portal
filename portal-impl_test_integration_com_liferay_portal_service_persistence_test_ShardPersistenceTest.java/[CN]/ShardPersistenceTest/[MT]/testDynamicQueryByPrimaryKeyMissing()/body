{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Shard.class,Shard.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("shardId",RandomTestUtil.nextLong()));
  List<Shard> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
