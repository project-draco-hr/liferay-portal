{
  String xml=StringPool.BLANK;
  if (!ENABLED) {
    if (_log.isDebugEnabled()) {
      _log.debug("Search is disabled");
    }
    return xml;
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Search with " + url);
  }
  HttpClient client=Http.getNonProxyClient();
  HttpState state=new HttpState();
  state.setCredentials(new AuthScope(HOST,PORT,REALM),new UsernamePasswordCredentials(USERNAME,PASSWORD));
  GetMethod get=new GetMethod(url);
  get.setDoAuthentication(true);
  try {
    int status=client.executeMethod(client.getHostConfiguration(),get,state);
    xml=get.getResponseBodyAsString();
  }
 catch (  IOException ioe) {
    _log.error("Unable to search with " + url,ioe);
  }
 finally {
    get.releaseConnection();
  }
  return xml;
}
