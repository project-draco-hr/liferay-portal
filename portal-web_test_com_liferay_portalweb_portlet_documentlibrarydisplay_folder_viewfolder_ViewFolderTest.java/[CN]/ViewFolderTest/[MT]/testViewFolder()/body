{
  selenium.open("/web/guest/home/");
  selenium.getEval("window.Liferay.fire('initDockbar');");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Documents and Media Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Documents and Media Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.getEval("window.Liferay.fire('initDockbar');");
  assertEquals(RuntimeVariables.replace("DL Folder Name\nDL Folder Description"),selenium.getText("//td[1]/a[2]"));
  selenium.clickAt("//td[1]/a[2]",RuntimeVariables.replace("DL Folder Name\nDL Folder Description"));
  selenium.waitForPageToLoad("30000");
  selenium.getEval("window.Liferay.fire('initDockbar');");
  assertEquals(RuntimeVariables.replace("DL Folder Name"),selenium.getText("//h1[@class='header-title']/span"));
  assertEquals(RuntimeVariables.replace("DL Folder Description"),selenium.getText("//div[@class='lfr-asset-description']"));
}
