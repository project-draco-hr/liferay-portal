{
  if (merge) {
    session.merge(model);
  }
 else {
    boolean contains=false;
    if (isEnabled()) {
      Object obj=session.get(model.getClass(),model.getPrimaryKeyObj());
      contains=(obj != null) && obj.equals(model);
    }
    if (model.isNew()) {
      session.save(model);
    }
 else     if (!session.contains(model) && !contains) {
      session.saveOrUpdate(model);
    }
  }
  if (!isEnabled()) {
    session.flush();
    return;
  }
  if (_counter.get() < PropsValues.HIBERNATE_JDBC_BATCH_SIZE) {
    _counter.set(_counter.get() + 1);
  }
 else {
    _counter.set(BatchSessionCounter.INITIAL_VALUE);
    session.flush();
    session.clear();
  }
}
