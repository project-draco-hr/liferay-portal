{
  tdi.breakpoints.clear();
  for (Iterator iter=tdi.templates.iterator(); iter.hasNext(); ) {
    TemplateReference ref=(TemplateReference)iter.next();
    Template t=ref.getTemplate();
    if (t == null) {
      iter.remove();
    }
 else {
      removeDebugBreaks(t.getRootTreeNode());
    }
  }
}
