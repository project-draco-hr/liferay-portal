{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace("Shopping Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Coupons",RuntimeVariables.replace("Coupons"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Coupon']",RuntimeVariables.replace("Add Coupon"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isChecked("//input[@id='_34_autoCodeCheckbox']"));
  selenium.clickAt("//input[@id='_34_autoCodeCheckbox']",RuntimeVariables.replace("Autogenerate Code"));
  assertTrue(selenium.isChecked("//input[@id='_34_autoCodeCheckbox']"));
  selenium.type("//input[@id='_34_name']",RuntimeVariables.replace("Shopping Coupon Name"));
  selenium.type("//textarea[@id='_34_description']",RuntimeVariables.replace("Shopping Coupon Description"));
  selenium.type("//input[@id='_34_discount']",RuntimeVariables.replace("0.50"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertTrue(selenium.isPartialText("//tr[@class='portlet-section-body results-row last']/td[3]/a","Shopping Coupon Name"));
  assertTrue(selenium.isPartialText("//tr[@class='portlet-section-body results-row last']/td[3]/a","Shopping Coupon Description"));
}
