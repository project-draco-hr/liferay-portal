{
  Lock readLock=_readWriteLock.readLock();
  try {
    readLock.lock();
    Channel channel=_channels.get(userId);
    if (channel == null) {
      throw new UnknownChannelException("No channel exists with user id " + userId);
    }
    return channel;
  }
  finally {
    readLock.unlock();
  }
}
