{
  super(String.format("User %s was locked on %s by password policy %s and will " + "be automatically unlocked on %s",user.getUserId(),user.getLockoutDate(),passwordPolicy.getName(),DateUtil.newDate(user.getLockoutDate().getTime() + passwordPolicy.getLockoutDuration() * 1000)));
  this.user=user;
  this.passwordPolicy=passwordPolicy;
}
