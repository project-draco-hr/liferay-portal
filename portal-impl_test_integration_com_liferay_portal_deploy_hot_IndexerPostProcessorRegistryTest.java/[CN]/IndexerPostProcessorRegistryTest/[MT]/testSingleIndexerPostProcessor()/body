{
  Indexer<BlogsEntry> blogsEntryIndexer=IndexerRegistryUtil.getIndexer(BlogsIndexer.class.getName());
  IndexerPostProcessor[] blogIndexerPostProcessors=blogsEntryIndexer.getIndexerPostProcessors();
  assertEquals(1,blogIndexerPostProcessors.length);
  IndexerPostProcessor blogsIndexerPostProcessor=blogIndexerPostProcessors[0];
  assertNotNull(blogsIndexerPostProcessor);
}
