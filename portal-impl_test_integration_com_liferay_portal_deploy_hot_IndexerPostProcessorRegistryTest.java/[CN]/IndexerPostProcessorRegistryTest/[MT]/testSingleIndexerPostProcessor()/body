{
  Indexer blogsIndexer=IndexerRegistryUtil.getIndexer("com.liferay.portlet.blogs.util.BlogsIndexer");
  IndexerPostProcessor[] blogIndexerPostProcessors=blogsIndexer.getIndexerPostProcessors();
  assertEquals(1,blogIndexerPostProcessors.length);
  IndexerPostProcessor blogsIndexerPostProcessor=blogIndexerPostProcessors[0];
  assertNotNull(blogsIndexerPostProcessor);
}
