{
  Indexer contactIndexer=IndexerRegistryUtil.getIndexer("com.liferay.portal.model.Contact");
  IndexerPostProcessor[] contactIndexerPostProcessors=contactIndexer.getIndexerPostProcessors();
  assertEquals(1,contactIndexerPostProcessors.length);
  IndexerPostProcessor contactIndexerPostProcessor=contactIndexerPostProcessors[0];
  assertNotNull(contactIndexerPostProcessor);
}
