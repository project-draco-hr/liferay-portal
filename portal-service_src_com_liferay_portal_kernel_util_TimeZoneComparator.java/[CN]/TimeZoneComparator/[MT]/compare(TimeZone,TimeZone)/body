{
  Integer totalOffset1=timeZone1.getRawOffset() + timeZone1.getDSTSavings();
  Integer totalOffset2=timeZone2.getRawOffset() + timeZone2.getDSTSavings();
  int value=totalOffset1.compareTo(totalOffset2);
  if (value == 0) {
    String timeZoneId1=timeZone1.getID();
    String timeZoneId2=timeZone2.getID();
    value=timeZoneId1.compareTo(timeZoneId2);
  }
  return value;
}
