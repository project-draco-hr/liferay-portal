{
  super.init(servletConfig);
  ServletContext servletContext=getServletContext();
  ModuleConfig moduleConfig=(ModuleConfig)servletContext.getAttribute(Globals.MODULE_KEY);
  PortletRequestProcessor portletRequestProcessor=PortletRequestProcessor.getInstance(this,moduleConfig);
  servletContext.setAttribute(WebKeys.PORTLET_STRUTS_PROCESSOR,portletRequestProcessor);
}
