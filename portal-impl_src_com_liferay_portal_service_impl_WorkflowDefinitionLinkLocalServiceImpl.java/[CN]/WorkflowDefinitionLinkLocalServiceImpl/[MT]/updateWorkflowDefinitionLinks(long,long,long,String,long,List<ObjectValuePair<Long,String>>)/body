{
  for (  ObjectValuePair<Long,String> workflowDefinition : workflowDefinitions) {
    long typePK=workflowDefinition.getKey();
    String workflowDefinitionName=workflowDefinition.getValue();
    if (Validator.isNull(workflowDefinitionName)) {
      deleteWorkflowDefinitionLink(companyId,groupId,className,classPK,typePK);
    }
 else {
      updateWorkflowDefinitionLink(userId,companyId,groupId,className,classPK,typePK,workflowDefinitionName);
    }
  }
}
