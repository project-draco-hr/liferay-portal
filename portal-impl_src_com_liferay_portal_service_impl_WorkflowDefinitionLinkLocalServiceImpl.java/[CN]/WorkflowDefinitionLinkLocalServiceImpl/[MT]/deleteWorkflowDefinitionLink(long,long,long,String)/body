{
  try {
    WorkflowDefinitionLink workflowDefinitionLink=getWorkflowDefinitionLink(companyId,groupId,className);
    deleteWorkflowDefinitionLink(workflowDefinitionLink);
    WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(companyId,userId,workflowDefinitionLink.getWorkflowDefinitionName(),workflowDefinitionLink.getWorkflowDefinitionVersion());
  }
 catch (  NoSuchWorkflowDefinitionLinkException nswdle) {
  }
}
