{
  if (_emailAddress == null) {
    try {
      if (_user == null) {
        _user=UserLocalServiceUtil.getUserById(getUserId());
      }
      _emailAddress=_user.getEmailAddress();
    }
 catch (    Exception e) {
    }
  }
  if (_emailAddress == null) {
    _emailAddress=StringPool.BLANK;
  }
  return _emailAddress;
}
