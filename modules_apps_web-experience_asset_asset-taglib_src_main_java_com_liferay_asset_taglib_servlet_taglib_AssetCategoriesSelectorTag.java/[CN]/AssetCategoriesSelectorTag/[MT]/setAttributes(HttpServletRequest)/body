{
  request.setAttribute("liferay-asset:asset-categories-selector:className",_className);
  request.setAttribute("liferay-asset:asset-categories-selector:classPK",String.valueOf(_classPK));
  request.setAttribute("liferay-asset:asset-categories-selector:classTypePK",String.valueOf(_classTypePK));
