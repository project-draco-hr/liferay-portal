{
  List<String> list=new ArrayList<String>();
  UnsyncBufferedReader br=null;
  try {
    br=new UnsyncBufferedReader(new InputStreamReader(is));
    String line=br.readLine();
    while (line != null) {
      line=line.trim();
      if (Validator.isNotNull(line)) {
        list.add(line);
      }
      line=br.readLine();
    }
  }
  finally {
    if (br != null) {
      try {
        br.close();
      }
 catch (      Exception e) {
      }
    }
  }
  return list.toArray(new String[]{});
}
