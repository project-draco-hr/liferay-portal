{
  try {
    SearchContainerRowTag<R> searchContainerRowTag=(SearchContainerRowTag<R>)findAncestorWithClass(this,SearchContainerRowTag.class);
    ResultRow row=searchContainerRowTag.getRow();
    if (index <= -1) {
      index=row.getEntries().size();
    }
    ScoreSearchEntry scoreSearchEntry=new ScoreSearchEntry();
    scoreSearchEntry.setScore(getScore());
    row.addSearchEntry(index,scoreSearchEntry);
    return EVAL_PAGE;
  }
  finally {
    index=-1;
    if (!ServerDetector.isResin()) {
      _name=DEFAULT_NAME;
      _score=0;
    }
  }
}
