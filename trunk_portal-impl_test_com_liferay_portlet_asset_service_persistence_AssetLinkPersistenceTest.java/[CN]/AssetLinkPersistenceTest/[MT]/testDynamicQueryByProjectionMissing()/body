{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AssetLink.class,AssetLink.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("linkId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("linkId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
