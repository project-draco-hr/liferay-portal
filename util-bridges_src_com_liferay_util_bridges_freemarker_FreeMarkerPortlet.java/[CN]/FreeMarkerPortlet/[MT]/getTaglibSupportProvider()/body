{
  if (_templateTaglibSupportProvider != null) {
    return _templateTaglibSupportProvider;
  }
  List<TemplateTaglibSupportProvider> templateTaglibSupportProviders=ServiceLoader.load(TemplateTaglibSupportProvider.class);
  if (!templateTaglibSupportProviders.isEmpty()) {
    _templateTaglibSupportProvider=templateTaglibSupportProviders.get(0);
  }
  return _templateTaglibSupportProvider;
}
