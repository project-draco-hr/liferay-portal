{
  try {
    LayoutLocalServiceUtil.importPortletDataDeletions(_userId,_targetPlid,_targetGroupId,_portletId,_parameterMap,_file);
    LayoutLocalServiceUtil.importPortletInfo(_userId,_targetPlid,_targetGroupId,_portletId,_parameterMap,_file);
  }
  finally {
    FileUtil.delete(_file);
  }
  return null;
}
