{
  User user=(User)EntityCacheUtil.getResult(UserModelImpl.ENTITY_CACHE_ENABLED,UserImpl.class,userId,this);
  if (user == null) {
    Session session=null;
    try {
      session=openSession();
      user=(User)session.get(UserImpl.class,Long.valueOf(userId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (user != null) {
        cacheResult(user);
      }
      closeSession(session);
    }
  }
  return user;
}
