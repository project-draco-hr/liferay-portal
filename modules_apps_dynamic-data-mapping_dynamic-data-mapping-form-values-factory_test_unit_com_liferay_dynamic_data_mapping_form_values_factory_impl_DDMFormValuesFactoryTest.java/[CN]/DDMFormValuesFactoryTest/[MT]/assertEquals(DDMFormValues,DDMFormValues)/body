{
  String serializedExpectedDDMFormValues=DDMFormValuesJSONSerializerUtil.serialize(expectedDDMFormValues);
  String serializedActualDDMFormValues=DDMFormValuesJSONSerializerUtil.serialize(actualDDMFormValues);
  JSONAssert.assertEquals(serializedExpectedDDMFormValues,serializedActualDDMFormValues,false);
}
