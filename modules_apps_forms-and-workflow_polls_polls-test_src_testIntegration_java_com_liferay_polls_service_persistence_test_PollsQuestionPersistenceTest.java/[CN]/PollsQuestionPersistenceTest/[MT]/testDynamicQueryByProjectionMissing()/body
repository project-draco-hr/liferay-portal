{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PollsQuestion.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("questionId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("questionId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
