  selenium.type("_86_fieldLabel3","Additional Comments");
  selenium.click("//input[@value='Save']");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Return to Full Page");
  selenium.waitForPageToLoad("30000");
}
