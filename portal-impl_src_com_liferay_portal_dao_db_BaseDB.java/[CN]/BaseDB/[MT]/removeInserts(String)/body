{
  BufferedReader br=new BufferedReader(new UnsyncStringReader(data));
  StringBuilder sb=new StringBuilder();
  String line=null;
  while ((line=br.readLine()) != null) {
    if (!line.startsWith("insert into ") && !line.startsWith("update ")) {
      sb.append(line);
      sb.append("\n");
    }
  }
  br.close();
  return sb.toString();
}
