{
  ExpandoValue value1=ExpandoValueServiceUtil.addValue(ExpandoServiceTest.class.getName(),_TABLENAME,_COLUMNNAME1,_CLASSPK1,_VALUE1_COLUMN1);
  assertNotNull(value1);
  assertEquals(value1.getClassPK(),_CLASSPK1);
  assertEquals(value1.getString(),_VALUE1_COLUMN1);
  ExpandoValue value2=ExpandoValueServiceUtil.addValue(ExpandoServiceTest.class.getName(),_TABLENAME,_COLUMNNAME2,_CLASSPK1,_VALUE1_COLUMN2);
  assertNotNull(value2);
  assertEquals(value2.getClassPK(),_CLASSPK1);
  assertEquals(value2.getDouble(),_VALUE1_COLUMN2);
}
