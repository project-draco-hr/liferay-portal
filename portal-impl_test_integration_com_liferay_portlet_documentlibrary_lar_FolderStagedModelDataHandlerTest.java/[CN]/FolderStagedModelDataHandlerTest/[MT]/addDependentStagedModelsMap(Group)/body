{
  Map<String,List<StagedModel>> dependentStagedModelsMap=new HashMap<String,List<StagedModel>>();
  DDMStructure ddmStructure=DDMStructureTestUtil.addStructure(group.getGroupId(),DLFileEntryType.class.getName());
  addDependentStagedModel(dependentStagedModelsMap,DDMStructure.class,ddmStructure);
  DLFileEntryType dlFileEntryType=DLAppTestUtil.addDLFileEntryType(group.getGroupId(),ddmStructure.getStructureId());
  addDependentStagedModel(dependentStagedModelsMap,DLFileEntryType.class,dlFileEntryType);
  Folder folder=DLAppTestUtil.addFolder(group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,ServiceTestUtil.randomString());
  addDependentStagedModel(dependentStagedModelsMap,DLFolder.class,folder);
  return dependentStagedModelsMap;
}
