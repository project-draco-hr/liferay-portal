{
  initExport();
  Map<String,List<StagedModel>> dependentStagedModelMap=new HashMap<String,List<StagedModel>>();
  Repository repository=DLAppTestUtil.addRepository(stagingGroup.getGroupId());
  addDependentStagedModel(dependentStagedModelMap,Repository.class,repository);
  Folder folder=DLAppLocalServiceUtil.getMountFolder(repository.getRepositoryId());
  StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,folder);
  validateExport(portletDataContext,folder,dependentStagedModelMap);
  initImport();
  Folder exportedFolder=(Folder)readExportedStagedModel(folder);
  StagedModelDataHandlerUtil.importStagedModel(portletDataContext,exportedFolder);
  Repository importedRepository=RepositoryLocalServiceUtil.getRepositoryByUuidAndGroupId(repository.getUuid(),liveGroup.getGroupId());
  DLAppLocalServiceUtil.getMountFolder(importedRepository.getRepositoryId());
}
