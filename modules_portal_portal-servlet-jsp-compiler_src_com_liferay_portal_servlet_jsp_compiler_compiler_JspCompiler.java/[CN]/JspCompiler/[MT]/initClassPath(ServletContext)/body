{
  _lock.lock();
  try {
    _classPath=(ArrayList<File>)servletContext.getAttribute(_JSP_COMPILER_CLASS_PATH);
    if (_classPath != null) {
      return;
    }
    _classPath=new ArrayList<File>();
    addDependenciesToClassPath();
    servletContext.setAttribute(_JSP_COMPILER_CLASS_PATH,_classPath);
  }
  finally {
    _lock.unlock();
  }
}
