{
  Map<String,DDMFormFieldValue> nestedDDMFormFieldValuesMap=new HashMap<String,DDMFormFieldValue>();
  for (  DDMFormFieldValue nestedDDMFormFieldValue : _nestedDDMFormFieldValues) {
    nestedDDMFormFieldValuesMap.put(nestedDDMFormFieldValue.getName(),nestedDDMFormFieldValue);
  }
  return nestedDDMFormFieldValuesMap;
}
