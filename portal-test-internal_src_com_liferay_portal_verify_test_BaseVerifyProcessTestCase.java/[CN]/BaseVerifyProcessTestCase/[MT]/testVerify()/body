{
  Exception exception=null;
  try {
    doVerify();
  }
 catch (  VerifyException ve) {
    exception=ve;
  }
 finally {
    for (    ObjectValuePair<Connection,Exception> objectValuePair : _objectValuePairs) {
      Connection connection=objectValuePair.getKey();
      if (!connection.isClosed()) {
        if (exception == null) {
          exception=objectValuePair.getValue();
        }
 else {
          exception.addSuppressed(objectValuePair.getValue());
        }
      }
    }
    if (exception != null) {
      throw exception;
    }
  }
}
