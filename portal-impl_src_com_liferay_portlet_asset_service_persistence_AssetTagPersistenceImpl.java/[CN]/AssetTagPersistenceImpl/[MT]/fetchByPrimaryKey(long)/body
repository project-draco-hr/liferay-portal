{
  AssetTag assetTag=(AssetTag)EntityCacheUtil.getResult(AssetTagModelImpl.ENTITY_CACHE_ENABLED,AssetTagImpl.class,tagId,this);
  if (assetTag == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (assetTag == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      assetTag=(AssetTag)session.get(AssetTagImpl.class,Long.valueOf(tagId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (assetTag == null)) {
        EntityCacheUtil.putResult(AssetTagModelImpl.ENTITY_CACHE_ENABLED,AssetTagImpl.class,tagId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(assetTag);
      }
      closeSession(session);
    }
  }
  return assetTag;
}
