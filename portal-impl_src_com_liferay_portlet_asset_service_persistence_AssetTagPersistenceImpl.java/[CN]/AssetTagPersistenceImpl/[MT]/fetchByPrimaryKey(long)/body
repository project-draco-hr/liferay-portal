{
  AssetTag assetTag=(AssetTag)EntityCacheUtil.getResult(AssetTagModelImpl.ENTITY_CACHE_ENABLED,AssetTagImpl.class,tagId);
  if (assetTag == _nullAssetTag) {
    return null;
  }
  if (assetTag == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      assetTag=(AssetTag)session.get(AssetTagImpl.class,Long.valueOf(tagId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (assetTag != null) {
        cacheResult(assetTag);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(AssetTagModelImpl.ENTITY_CACHE_ENABLED,AssetTagImpl.class,tagId,_nullAssetTag);
      }
      closeSession(session);
    }
  }
  return assetTag;
}
