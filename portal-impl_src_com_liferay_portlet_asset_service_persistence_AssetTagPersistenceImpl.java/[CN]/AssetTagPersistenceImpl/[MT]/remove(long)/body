{
  Session session=null;
  try {
    session=openSession();
    AssetTag assetTag=(AssetTag)session.get(AssetTagImpl.class,new Long(tagId));
    if (assetTag == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No AssetTag exists with the primary key " + tagId);
      }
      throw new NoSuchTagException("No AssetTag exists with the primary key " + tagId);
    }
    return remove(assetTag);
  }
 catch (  NoSuchTagException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
