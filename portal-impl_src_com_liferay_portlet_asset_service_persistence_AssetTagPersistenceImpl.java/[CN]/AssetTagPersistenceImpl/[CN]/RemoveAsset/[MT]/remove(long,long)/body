{
  if (_persistenceImpl.containsAsset.contains(tagId,assetId)) {
    ModelListener<com.liferay.portlet.asset.model.Asset>[] assetListeners=assetPersistence.getListeners();
    for (    ModelListener<AssetTag> listener : listeners) {
      listener.onBeforeRemoveAssociation(tagId,com.liferay.portlet.asset.model.Asset.class.getName(),assetId);
    }
    for (    ModelListener<com.liferay.portlet.asset.model.Asset> listener : assetListeners) {
      listener.onBeforeRemoveAssociation(assetId,AssetTag.class.getName(),tagId);
    }
    _sqlUpdate.update(new Object[]{new Long(tagId),new Long(assetId)});
    for (    ModelListener<AssetTag> listener : listeners) {
      listener.onAfterRemoveAssociation(tagId,com.liferay.portlet.asset.model.Asset.class.getName(),assetId);
    }
    for (    ModelListener<com.liferay.portlet.asset.model.Asset> listener : assetListeners) {
      listener.onAfterRemoveAssociation(assetId,AssetTag.class.getName(),tagId);
    }
  }
}
