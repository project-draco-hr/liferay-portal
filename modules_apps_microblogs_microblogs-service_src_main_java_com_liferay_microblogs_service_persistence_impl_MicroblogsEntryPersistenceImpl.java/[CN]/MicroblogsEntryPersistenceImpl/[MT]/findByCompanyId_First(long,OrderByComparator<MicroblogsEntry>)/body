{
  MicroblogsEntry microblogsEntry=fetchByCompanyId_First(companyId,orderByComparator);
  if (microblogsEntry != null) {
    return microblogsEntry;
  }
  StringBundler msg=new StringBundler(4);
  msg.append(_NO_SUCH_ENTITY_WITH_KEY);
