{
  try {
    String args=ParamUtil.getString(req,"args");
    GoogleSearch googleSearch=GoogleUtil.getGoogleSearch();
    byte[] cachedBytes=googleSearch.doGetCachedPage(args);
    String cachedString=new String(cachedBytes);
    req.setAttribute(WebKeys.GOOGLE_CACHED_PAGE,cachedString);
    return mapping.findForward("portlet.google.cached");
  }
 catch (  GoogleSearchFault gse) {
    GoogleUtil.useNewKey();
    return mapping.findForward("portlet.google.error");
  }
}
