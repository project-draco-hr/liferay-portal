{
  try {
    SocialRelationLocalServiceUtil.addRelation(request.getUserId(),request.getReceiverUserId(),SocialRelationConstants.TYPE_BI_FRIEND);
    SocialActivityLocalServiceUtil.addActivity(request.getUserId(),0,User.class.getName(),request.getUserId(),FriendsActivityKeys.ADD_FRIEND,StringPool.BLANK,request.getReceiverUserId());
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return true;
}
