{
  if (PropsValues.SESSION_DISABLED) {
    return;
  }
  HttpSession ses=event.getSession();
  PortalSessionContext.put(ses.getId(),ses);
  try {
    EventsProcessor.process(PropsUtil.SERVLET_SESSION_CREATE_EVENTS,PropsValues.SERVLET_SESSION_CREATE_EVENTS,ses);
  }
 catch (  ActionException ae) {
    _log.error(ae,ae);
  }
}
