{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select fileVersionId from DLFileVersion where folderId = ? " + "and name = ? order by version desc");
    ps.setLong(1,folderId);
    ps.setString(2,name);
    rs=ps.executeQuery();
    List<Long> fileVersionIds=new ArrayList<Long>();
    while (rs.next()) {
      long fileVersionId=rs.getLong("fileVersionId");
      fileVersionIds.add(fileVersionId);
    }
    return fileVersionIds;
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
