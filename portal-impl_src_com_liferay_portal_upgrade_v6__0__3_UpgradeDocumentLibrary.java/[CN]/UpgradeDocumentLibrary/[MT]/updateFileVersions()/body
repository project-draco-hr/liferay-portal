{
  try (PreparedStatement ps=connection.prepareStatement("select folderId, name, extension, title, description, " + "extraSettings from DLFileEntry");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long folderId=rs.getLong("folderId");
      String name=rs.getString("name");
      String extension=rs.getString("extension");
      String title=rs.getString("title");
      String description=rs.getString("description");
      String extraSettings=rs.getString("extraSettings");
      List<Long> fileVersionIds=getFileVersionIds(folderId,name);
      for (      long fileVersionId : fileVersionIds) {
        updateFileVersion(fileVersionId,extension,title,description,extraSettings);
      }
    }
  }
 }
