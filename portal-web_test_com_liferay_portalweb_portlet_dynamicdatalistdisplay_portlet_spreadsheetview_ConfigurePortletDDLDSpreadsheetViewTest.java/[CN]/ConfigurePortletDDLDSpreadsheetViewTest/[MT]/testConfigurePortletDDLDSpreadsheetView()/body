{
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Dynamic Data List Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Dynamic Data List Display Test Page",RuntimeVariables.replace("Dynamic Data List Display Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isVisible("//input[@value='Add Record']"));
  Thread.sleep(5000);
  selenium.clickAt("//strong/a",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Configuration"),selenium.getText("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a"));
  selenium.click("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//iframe")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.selectFrame("//iframe");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//input[@id='_86_spreadsheetCheckbox']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertFalse(selenium.isChecked("//input[@id='_86_spreadsheetCheckbox']"));
  selenium.clickAt("//input[@id='_86_spreadsheetCheckbox']",RuntimeVariables.replace("Spreadsheet View"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("You have successfully updated the setup."),selenium.getText("//div[@class='portlet-msg-success']"));
}
