{
  try {
    SchedulerRequest schedulerRequest=SchedulerRequest.createRetrieveRequest(new CronTrigger(groupName,groupName,null));
    List<SchedulerRequest> schedulerRequests=(List<SchedulerRequest>)MessageBusUtil.sendSynchronousMessage(DestinationNames.SCHEDULER,schedulerRequest,DestinationNames.SCHEDULER_RESPONSE);
    return schedulerRequests;
  }
 catch (  Exception e) {
    throw new SchedulerException(e);
  }
}
