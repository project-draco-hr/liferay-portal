{
  try {
    SchedulerRequest schedulerRequest=new SchedulerRequest(SchedulerRequest.COMMAND_RETRIEVE,null,groupName,null,null,null,null,null,null);
    List<SchedulerRequest> schedulerRequests=(List<SchedulerRequest>)MessageBusUtil.sendSynchronousMessage(DestinationNames.SCHEDULER,schedulerRequest);
    return schedulerRequests;
  }
 catch (  Exception e) {
    throw new SchedulerException(e);
  }
}
