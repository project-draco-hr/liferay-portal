{
  try {
    SchedulerRequest schedulerRequest=SchedulerRequest.createRetrieveRequest(groupName);
    List<SchedulerRequest> schedulerRequests=(List<SchedulerRequest>)MessageBusUtil.sendSynchronousMessage(DestinationNames.SCHEDULER_ENGINE,schedulerRequest,DestinationNames.SCHEDULER_ENGINE_RESPONSE);
    return schedulerRequests;
  }
 catch (  Exception e) {
    throw new SchedulerException(e);
  }
}
