{
  Registry registry=RegistryUtil.getRegistry();
  Map<String,Object> properties=new HashMap<String,Object>();
  properties.put("javax.portlet.name",portletId);
  ServiceRegistration<PollerProcessor> serviceRegistration=registry.registerService(PollerProcessor.class,pollerProcessor,properties);
  _serviceRegistrations.put(portletId,serviceRegistration);
}
