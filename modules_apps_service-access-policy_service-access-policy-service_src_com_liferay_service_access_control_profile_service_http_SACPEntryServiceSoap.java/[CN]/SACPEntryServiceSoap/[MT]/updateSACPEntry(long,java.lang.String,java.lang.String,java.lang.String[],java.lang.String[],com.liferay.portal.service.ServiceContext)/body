{
  try {
    Map<Locale,String> titleMap=LocalizationUtil.getLocalizationMap(titleMapLanguageIds,titleMapValues);
    com.liferay.service.access.policy.model.SACPEntry returnValue=SACPEntryServiceUtil.updateSACPEntry(sacpEntryId,allowedServiceSignatures,name,titleMap,serviceContext);
    return com.liferay.service.access.policy.model.SACPEntrySoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
