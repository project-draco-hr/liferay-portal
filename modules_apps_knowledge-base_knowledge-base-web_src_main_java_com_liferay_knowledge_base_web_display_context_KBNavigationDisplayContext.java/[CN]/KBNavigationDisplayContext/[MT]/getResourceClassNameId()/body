{
  if (_resourceClassNameId == null) {
    if (_kbDisplayPortletInstanceConfiguration.resourceClassNameId() != 0) {
      _resourceClassNameId=_kbDisplayPortletInstanceConfiguration.resourceClassNameId();
    }
    _resourceClassNameId=PortalUtil.getClassNameId(KBFolderConstants.getClassName());
  }
  return _resourceClassNameId;
}
