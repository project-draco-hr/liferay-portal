{
  try {
    ActionUtil.getRole(req);
  }
 catch (  Exception e) {
    if (e != null && e instanceof NoSuchRoleException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.enterprise_admin.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward(getForward(req,"portlet.enterprise_admin.edit_role_permissions"));
}
