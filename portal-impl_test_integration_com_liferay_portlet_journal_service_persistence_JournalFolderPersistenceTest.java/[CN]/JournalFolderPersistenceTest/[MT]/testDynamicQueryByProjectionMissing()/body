{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalFolder.class,JournalFolder.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("folderId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("folderId",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
