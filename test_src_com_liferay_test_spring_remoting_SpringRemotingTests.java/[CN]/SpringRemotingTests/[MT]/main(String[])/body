{
  if (args.length > 0) {
    _protocol=args[0];
    if (Validator.isNotNull(_protocol)) {
      _protocol=_protocol.substring(0,1).toUpperCase() + _protocol.substring(1,_protocol.length()).toLowerCase();
    }
  }
  TestRunner runner=new TestRunner(new ResultPrinter(System.out));
  runner.doRun(suite());
}
