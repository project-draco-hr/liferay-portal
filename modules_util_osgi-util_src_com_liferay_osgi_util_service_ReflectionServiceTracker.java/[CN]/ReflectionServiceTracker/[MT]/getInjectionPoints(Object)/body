{
  Class<?> targetObjectClass=target.getClass();
  ArrayList<Class<?>> interfaces=new ArrayList<Class<?>>();
  List<Method> injectionPointMethods=getInjectionPointMethods(target);
  for (  Method injectionPointMethod : injectionPointMethods) {
    Class<?> parameterType=injectionPointMethod.getParameterTypes()[0];
    if (parameterType.isInterface()) {
      interfaces.add(parameterType);
    }
  }
  ClassLoader classLoader=targetObjectClass.getClassLoader();
  _unavailableServiceProxy=Proxy.newProxyInstance(classLoader,interfaces.toArray(new Class[0]),_INVOCATION_HANDLER);
  ArrayList<InjectionPoint> injectionPoints=new ArrayList<InjectionPoint>();
  for (  Method injectionPointMethod : injectionPointMethods) {
    InjectionPoint injectionPoint=createInjectionPoint(target,injectionPointMethod);
    if (injectionPoint != null) {
      injectionPoints.add(injectionPoint);
    }
  }
  return injectionPoints;
}
