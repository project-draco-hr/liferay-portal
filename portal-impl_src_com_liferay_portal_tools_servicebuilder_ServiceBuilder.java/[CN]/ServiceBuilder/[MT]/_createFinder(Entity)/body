{
  if (!entity.hasFinderClass()) {
    return;
  }
  JavaClass javaClass=_getJavaClass(_outputPath + "/service/persistence/" + entity.getName()+ "FinderImpl.java");
  Map<String,Object> context=_getContext();
  context.put("entity",entity);
  context.put("methods",_getMethods(javaClass));
  String content=_processTemplate(_tplFinder,context);
  File ejbFile=new File(_serviceOutputPath + "/service/persistence/" + entity.getName()+ "Finder.java");
  writeFile(ejbFile,content,_author);
  _writeJavadoc(ejbFile);
  if (!_serviceOutputPath.equals(_outputPath)) {
    ejbFile=new File(_outputPath + "/service/persistence/" + entity.getName()+ "Finder.java");
    if (ejbFile.exists()) {
      System.out.println("Relocating " + ejbFile);
      ejbFile.delete();
    }
  }
}
