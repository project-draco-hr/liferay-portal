{
  Map<String,Object> context=_getContext();
  context.put("entity",entity);
  context.put("referenceList",_mergeReferenceList(entity.getReferenceList()));
  String content=_processTemplate(_tplPersistenceImpl,context);
  File ejbFile=new File(_outputPath + "/service/persistence/" + entity.getName()+ "PersistenceImpl.java");
  writeFile(ejbFile,content,_author);
}
