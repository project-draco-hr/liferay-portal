{
  File ejbFile=new File(_serviceOutputPath + "/service/persistence/" + entity.getName()+ "FinderUtil.java");
  if (ejbFile.exists()) {
    ejbFile.delete();
  }
  if (!_serviceOutputPath.equals(_outputPath)) {
    ejbFile=new File(_outputPath + "/service/persistence/" + entity.getName()+ "FinderUtil.java");
    if (ejbFile.exists()) {
      ejbFile.delete();
    }
  }
}
