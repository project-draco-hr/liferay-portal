{
  if (type.matches("[Bb]oolean")) {
    return "Boolean";
  }
 else   if (type.matches("[Dd]ouble")) {
    return "Double";
  }
 else   if (type.matches("[Ff]loat")) {
    return "Float";
  }
 else   if (type.matches("int|Integer")) {
    return "Integer";
  }
 else   if (type.matches("[Ll]ong")) {
    return "Long";
  }
 else   if (type.matches("[Ss]hort")) {
    return "Short";
  }
 else {
    return type;
  }
}
