{
  if (Validator.isNull(_osgiModule)) {
    return;
  }
  File file=new File(_implDir + "/" + StringUtil.replace(_propsUtil,".","/")+ ".java");
  if (file.exists()) {
    return;
  }
  Map<String,Object> context=_getContext();
  int indexOf=_propsUtil.lastIndexOf(".");
  context.put("servicePropsUtilClass",_propsUtil.substring(indexOf + 1));
  context.put("servicePropsUtilPackage",_propsUtil.substring(0,indexOf));
  String content=_processTemplate(_tplServicePropsUtil,context);
  writeFile(file,content);
}
