{
  Map<String,Object> context=_getContext();
  context.put("entity",entity);
  String content=_processTemplate(_tplModel,context);
  File modelFile=new File(_serviceOutputPath + "/model/" + entity.getName()+ "Model.java");
  writeFile(modelFile,content,_author);
  FileUtil.write(modelFile.toString() + "doc",_processTemplate(_tplModelJavadoc,context));
  if (!_serviceOutputPath.equals(_outputPath)) {
    modelFile=new File(_outputPath + "/model/" + entity.getName()+ "Model.java");
    if (modelFile.exists()) {
      System.out.println("Relocating " + modelFile);
      modelFile.delete();
    }
  }
}
