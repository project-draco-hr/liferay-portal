{
  if (Validator.isNull(sql)) {
    return sql;
  }
  for (  EntityColumn column : columns) {
    String name=column.getName();
    String dbName=column.getDBName();
    String regex="(?<!\\.)\\b" + name + "\\b";
    if (sql.indexOf(name) != -1) {
      sql=sql.replaceAll(regex,alias + "." + dbName);
    }
  }
  return sql;
}
