{
  StringBundler sb=new StringBundler(_springNamespaces.length * 4);
  for (  String namespace : _springNamespaces) {
    sb.append("\txmlns");
    if (!_SPRING_BEANS_NAMESPACE.equals(namespace)) {
      sb.append(":");
      sb.append(namespace);
    }
    sb.append("=\"http://www.springframework.org/schema/");
    sb.append(namespace);
    sb.append("\"\n");
  }
  return sb.toString();
}
