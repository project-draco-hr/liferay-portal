{
  File ejbFile=new File(_serviceOutputPath + "/service/" + entity.getName()+ _getSessionTypeName(sessionType)+ "ServiceUtil.java");
  ejbFile.delete();
  if (!_serviceOutputPath.equals(_outputPath)) {
    ejbFile=new File(_outputPath + "/service/" + entity.getName()+ _getSessionTypeName(sessionType)+ "ServiceUtil.java");
    ejbFile.delete();
  }
}
