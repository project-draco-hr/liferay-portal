{
  if (entity.isResolved()) {
    return;
  }
  for (  String reference : entity.getUnresolvedReferences()) {
    Entity entityReference=getEntity(reference);
    if (entityReference == null) {
      throw new RuntimeException("Cannot find forward defined " + reference + " in "+ ListUtil.toString(_ejbList,Entity.NAME_ACCESSOR));
    }
    entity.addReference(entity);
  }
  entity.setResolved();
}
