{
  int pos=fileName.indexOf(_implDir + "/");
  if (pos != -1) {
    pos+=_implDir.length();
  }
 else {
    pos=fileName.indexOf(_apiDir + "/") + _apiDir.length();
  }
  String srcFile=fileName.substring(pos + 1,fileName.length());
  String className=StringUtil.replace(srcFile.substring(0,srcFile.length() - 5),"/",".");
  JavaClass javaClass=_javaClassCache.get(className);
  if (javaClass == null) {
    JavaDocBuilder builder=new JavaDocBuilder();
    File file=new File(fileName);
    if (!file.exists()) {
      return null;
    }
    builder.addSource(file);
    javaClass=builder.getClassByName(className);
    _javaClassCache.put(className,javaClass);
  }
  return javaClass;
}
