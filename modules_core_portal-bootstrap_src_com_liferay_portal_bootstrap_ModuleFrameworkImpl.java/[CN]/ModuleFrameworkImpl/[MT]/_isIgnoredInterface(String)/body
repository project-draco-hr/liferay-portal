{
  for (  String ignoredClass : PropsValues.MODULE_FRAMEWORK_SERVICES_IGNORED_INTERFACES) {
    boolean isNegated=ignoredClass.startsWith(StringPool.EXCLAMATION);
    if (!isNegated && (ignoredClass.equals(interfaceClassName) || (ignoredClass.endsWith(StringPool.STAR) && interfaceClassName.startsWith(ignoredClass.substring(0,ignoredClass.length() - 1))))) {
      return true;
    }
  }
  return false;
}
