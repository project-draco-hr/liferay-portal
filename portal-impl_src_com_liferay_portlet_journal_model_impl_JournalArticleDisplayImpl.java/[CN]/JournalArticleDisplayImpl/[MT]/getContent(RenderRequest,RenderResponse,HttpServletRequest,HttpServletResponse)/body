{
  RuntimeLogic portletLogic=new PortletLogic(request,response);
  RuntimeLogic actionURLLogic=new ActionURLLogic(renderResponse);
  RuntimeLogic renderURLLogic=new RenderURLLogic(renderResponse);
  String processedContent=_content;
  processedContent=RuntimePageUtil.processXML(request,processedContent,portletLogic);
  processedContent=RuntimePageUtil.processXML(request,processedContent,actionURLLogic);
  processedContent=RuntimePageUtil.processXML(request,processedContent,renderURLLogic);
  return processedContent;
}
