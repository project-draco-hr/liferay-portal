{
  RoleTestUtil.addResourcePermission(getRoleName(),getResourceName(),ResourceConstants.SCOPE_GROUP,getPrimKey(),ActionKeys.VIEW);
  Role role=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),getRoleName());
  ResourcePermissionLocalServiceUtil.setResourcePermissions(group.getCompanyId(),getResourceName(),ResourceConstants.SCOPE_INDIVIDUAL,getPrimKey(),role.getRoleId(),new String[]{ActionKeys.VIEW});
