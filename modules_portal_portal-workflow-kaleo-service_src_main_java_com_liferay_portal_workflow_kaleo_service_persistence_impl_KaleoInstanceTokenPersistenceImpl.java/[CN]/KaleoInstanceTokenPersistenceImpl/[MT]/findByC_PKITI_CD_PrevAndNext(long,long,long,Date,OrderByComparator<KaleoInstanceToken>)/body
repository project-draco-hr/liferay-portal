{
  KaleoInstanceToken kaleoInstanceToken=findByPrimaryKey(kaleoInstanceTokenId);
  Session session=null;
  try {
    session=openSession();
    KaleoInstanceToken[] array=new KaleoInstanceTokenImpl[3];
    array[0]=getByC_PKITI_CD_PrevAndNext(session,kaleoInstanceToken,companyId,parentKaleoInstanceTokenId,completionDate,orderByComparator,true);
    array[1]=kaleoInstanceToken;
    array[2]=getByC_PKITI_CD_PrevAndNext(session,kaleoInstanceToken,companyId,parentKaleoInstanceTokenId,completionDate,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
