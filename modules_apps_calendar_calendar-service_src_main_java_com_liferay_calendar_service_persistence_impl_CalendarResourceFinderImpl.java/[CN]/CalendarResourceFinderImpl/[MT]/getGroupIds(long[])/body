{
  if (groupIds.length == 0) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(groupIds.length * 2);
  sb.append("(");
  for (int i=0; i < groupIds.length; i++) {
    sb.append("groupId = ?");
    if ((i + 1) < groupIds.length) {
      sb.append(" OR ");
    }
  }
  sb.append(") AND");
  return sb.toString();
}
