{
  JournalPermission.check(getPermissionChecker(),groupId,ActionKeys.ADD_TEMPLATE);
  return journalTemplateLocalService.addTemplate(getUserId(),templateId,autoTemplateId,groupId,structureId,name,description,xsl,formatXsl,langType,cacheable,smallImage,smallImageURL,smallFile,addCommunityPermissions,addGuestPermissions);
}
