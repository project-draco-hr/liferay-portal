{
  PortletPermissionUtil.check(getPermissionChecker(),plid,PortletKeys.JOURNAL,ActionKeys.ADD_TEMPLATE);
  return journalTemplateLocalService.addTemplate(getUserId(),templateId,autoTemplateId,plid,structureId,name,description,xsl,formatXsl,langType,cacheable,smallImage,smallImageURL,smallFile,communityPermissions,guestPermissions);
}
