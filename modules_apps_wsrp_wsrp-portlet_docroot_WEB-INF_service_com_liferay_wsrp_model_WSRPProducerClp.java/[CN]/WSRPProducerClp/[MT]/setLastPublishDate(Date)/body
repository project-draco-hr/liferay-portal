{
  _lastPublishDate=lastPublishDate;
  if (_wsrpProducerRemoteModel != null) {
    try {
      Class<?> clazz=_wsrpProducerRemoteModel.getClass();
      Method method=clazz.getMethod("setLastPublishDate",Date.class);
