{
  _uuid=uuid;
  if (_wsrpProducerRemoteModel != null) {
    try {
      Class<?> clazz=_wsrpProducerRemoteModel.getClass();
      Method method=clazz.getMethod("setUuid",String.class);
      method.invoke(_wsrpProducerRemoteModel,uuid);
