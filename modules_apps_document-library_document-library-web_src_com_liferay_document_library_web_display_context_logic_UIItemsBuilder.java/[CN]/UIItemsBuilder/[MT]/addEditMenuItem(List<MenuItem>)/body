{
  if (!_fileEntryDisplayContextHelper.isEditActionAvailable()) {
    return;
  }
  PortletURL portletURL=_getRenderURL("/document_library/edit_file_entry");
  if (_fileShortcut != null) {
    portletURL=_getRenderURL("/document_library/edit_file_shortcut");
  }
  portletURL.setParameter("backURL",_getCurrentURL());
  _addURLUIItem(new URLMenuItem(),menuItems,"icon-edit",DLUIItemKeys.EDIT,"edit",portletURL.toString());
}
