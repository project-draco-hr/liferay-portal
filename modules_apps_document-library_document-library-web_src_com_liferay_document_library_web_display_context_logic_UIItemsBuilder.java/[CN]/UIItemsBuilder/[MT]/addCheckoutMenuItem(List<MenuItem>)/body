{
  if (!_fileEntryDisplayContextHelper.isCheckoutDocumentActionAvailable()) {
    return;
  }
  PortletURL portletURL=_getActionURL("/document_library/edit_file_entry",Constants.CHECKOUT);
  _addURLUIItem(new URLMenuItem(),menuItems,"icon-unlock",DLUIItemKeys.CHECKOUT,"checkout[document]",portletURL.toString());
}
