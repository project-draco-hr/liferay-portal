{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select distinct buildNumber from Release_");
    rs=ps.executeQuery();
    while (rs.next()) {
      updateSchemaVersion(rs.getString("buildNumber"));
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
