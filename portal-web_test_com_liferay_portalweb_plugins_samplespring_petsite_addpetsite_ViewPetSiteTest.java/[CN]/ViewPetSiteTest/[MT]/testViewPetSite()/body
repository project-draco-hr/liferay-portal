{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Pet Sites Test Page",RuntimeVariables.replace("Pet Sites Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Pet Sites"),selenium.getText("//div[@class='portlet-body']/h1"));
  assertTrue(selenium.isVisible("//select[@name='url']"));
  selenium.select("//select[@name='url']",RuntimeVariables.replace("Liferay [http://www.liferay.com]"));
  selenium.clickAt("//button[@type='submit']",RuntimeVariables.replace("View Selected"));
  selenium.waitForVisible("//img[@alt='Open Source, Enterprise, For Life']");
  assertTrue(selenium.isVisible("//img[@alt='Open Source, Enterprise, For Life']"));
}
