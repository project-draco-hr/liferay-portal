{
  PredicateFilter<String> predicateFilter=new PredicateFilter<String>(){
    @Override public boolean filter(    String param){
      if (!param.startsWith(prefix)) {
        return false;
      }
      if ((excludeParams != null) && excludeParams.contains(param)) {
        return false;
      }
      return true;
    }
  }
;
  return ListUtil.filter(params,predicateFilter);
}
