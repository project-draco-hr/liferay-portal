{
  FileVersion fileVersion=fileEntry.getFileVersion();
  String binPath=fileEntryElement.attributeValue(binPathName);
  InputStream is=portletDataContext.getZipEntryAsInputStream(binPath);
  String thumbnailFilePath=getThumbnailFilePath(fileVersion,getThumbnailType(fileVersion),thumbnailIndex);
  addFileToStore(portletDataContext.getCompanyId(),THUMBNAIL_PATH,thumbnailFilePath,is);
}
