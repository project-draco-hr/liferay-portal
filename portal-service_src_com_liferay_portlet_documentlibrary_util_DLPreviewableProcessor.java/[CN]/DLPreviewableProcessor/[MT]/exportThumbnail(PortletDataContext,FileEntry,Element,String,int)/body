{
  FileVersion fileVersion=fileEntry.getFileVersion();
  File file=doGetThumbnail(fileVersion,thumbnailIndex);
  String binPath=getBinPath(portletDataContext,fileEntry,thumbnailIndex);
  fileEntryElement.addAttribute(binPathName,binPath);
  exportBinary(portletDataContext,fileEntryElement,fileVersion,file,binPath,binPathName);
}
