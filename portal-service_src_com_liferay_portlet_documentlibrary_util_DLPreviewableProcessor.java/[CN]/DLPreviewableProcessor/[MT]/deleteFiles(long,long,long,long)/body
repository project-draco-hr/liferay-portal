{
  try {
    DLStoreUtil.deleteDirectory(companyId,REPOSITORY_ID,getPathSegment(groupId,fileEntryId,fileVersionId,true));
  }
 catch (  Exception e) {
  }
  try {
    DLStoreUtil.deleteDirectory(companyId,REPOSITORY_ID,getPathSegment(groupId,fileEntryId,fileVersionId,false));
  }
 catch (  Exception e) {
