{
  StringBundler sb=null;
  if (index > 0) {
    sb=new StringBundler(5);
  }
 else {
    sb=new StringBundler(3);
  }
  sb.append(getPathSegment(groupId,fileEntryId,fileVersionId,true));
  if (index > 0) {
    sb.append(StringPool.SLASH);
    sb.append(index - 1);
  }
  sb.append(StringPool.PERIOD);
  sb.append(type);
  return sb.toString();
}
