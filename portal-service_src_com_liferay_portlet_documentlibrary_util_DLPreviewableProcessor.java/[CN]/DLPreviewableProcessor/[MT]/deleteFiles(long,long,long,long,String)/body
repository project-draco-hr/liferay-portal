{
  try {
    DLStoreUtil.deleteDirectory(companyId,REPOSITORY_ID,getPathSegment(groupId,fileEntryId,fileVersionId,true));
  }
 catch (  Exception e) {
  }
  try {
    String directoryPath=getPathSegment(groupId,fileEntryId,fileVersionId,false);
    if (fileVersionId > 0) {
      directoryPath=directoryPath.concat(StringPool.PERIOD);
      directoryPath=directoryPath.concat(thumbnailType);
    }
    DLStoreUtil.deleteDirectory(companyId,REPOSITORY_ID,directoryPath);
  }
 catch (  Exception e) {
  }
}
