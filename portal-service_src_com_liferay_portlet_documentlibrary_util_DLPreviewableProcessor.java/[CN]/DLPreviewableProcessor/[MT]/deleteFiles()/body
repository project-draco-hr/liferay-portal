{
  long[] companyIds=PortalUtil.getCompanyIds();
  for (  long companyId : companyIds) {
    try {
      DLStoreUtil.deleteDirectory(companyId,REPOSITORY_ID,PREVIEW_PATH);
    }
 catch (    Exception e) {
    }
    try {
      DLStoreUtil.deleteDirectory(companyId,REPOSITORY_ID,THUMBNAIL_PATH);
    }
 catch (    Exception e) {
    }
  }
}
