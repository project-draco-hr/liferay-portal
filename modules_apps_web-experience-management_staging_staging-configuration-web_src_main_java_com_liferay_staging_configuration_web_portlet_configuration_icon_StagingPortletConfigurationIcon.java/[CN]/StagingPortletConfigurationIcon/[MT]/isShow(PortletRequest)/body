{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  String rootPortletId=portletDisplay.getRootPortletId();
  if (rootPortletId.equals(ExportImportPortletKeys.EXPORT) || rootPortletId.equals(ExportImportPortletKeys.EXPORT_IMPORT) || rootPortletId.equals(ExportImportPortletKeys.IMPORT)|| rootPortletId.equals(StagingProcessesPortletKeys.STAGING_PROCESSES)|| rootPortletId.equals(StagingConfigurationPortletKeys.STAGING_CONFIGURATION)) {
    return false;
  }
  return portletDisplay.isShowStagingIcon();
}
