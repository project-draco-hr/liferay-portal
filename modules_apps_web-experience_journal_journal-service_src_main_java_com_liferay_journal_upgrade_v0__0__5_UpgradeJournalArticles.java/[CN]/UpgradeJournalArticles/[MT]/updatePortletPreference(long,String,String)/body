{
  try (PreparedStatement ps=connection.prepareStatement("update PortletPreferences set preferences = ?, " + "portletId = ? where portletPreferencesId = " + portletPreferencesId)){
    ps.setString(1,newPreferences);
    ps.setString(2,newPortletId);
    ps.executeUpdate();
  }
 catch (  SQLException sqle) {
    if (_log.isWarnEnabled()) {
      _log.warn(sqle,sqle);
    }
  }
}
