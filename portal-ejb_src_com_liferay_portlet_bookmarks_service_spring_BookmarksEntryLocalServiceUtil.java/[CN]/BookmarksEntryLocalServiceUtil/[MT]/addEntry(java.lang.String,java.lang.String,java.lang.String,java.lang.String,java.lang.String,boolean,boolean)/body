{
  try {
    BookmarksEntryLocalService bookmarksEntryLocalService=BookmarksEntryLocalServiceFactory.getService();
    return bookmarksEntryLocalService.addEntry(userId,folderId,name,url,comments,addCommunityPermissions,addGuestPermissions);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
