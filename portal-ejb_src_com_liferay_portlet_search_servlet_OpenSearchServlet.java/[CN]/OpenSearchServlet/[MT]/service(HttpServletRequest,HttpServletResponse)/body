{
  if (!PortalInstances.matches()) {
    return;
  }
  res.setContentType("text/xml; charset=UTF-8");
  OutputStreamWriter out=new OutputStreamWriter(res.getOutputStream());
  try {
    String rss="";
    if (!res.isCommitted()) {
      out.write(rss);
    }
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn(e,e);
    }
  }
 finally {
    out.flush();
    out.close();
  }
}
