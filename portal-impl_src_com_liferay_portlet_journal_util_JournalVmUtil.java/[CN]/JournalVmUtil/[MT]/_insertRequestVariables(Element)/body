{
  Map map=new HashMap();
  if (parent == null) {
    return map;
  }
  Iterator itr=parent.elements().iterator();
  while (itr.hasNext()) {
    Element el=(Element)itr.next();
    String name=el.getName();
    if (el.elements().size() > 0) {
      map.put(name,_insertRequestVariables(el));
    }
 else {
      map.put(name,el.getText());
    }
  }
  return map;
}
