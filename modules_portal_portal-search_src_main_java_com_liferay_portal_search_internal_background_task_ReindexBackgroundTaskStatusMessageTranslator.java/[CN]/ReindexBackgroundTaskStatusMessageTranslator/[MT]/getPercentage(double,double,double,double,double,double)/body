{
  if (totalCompanies <= 0) {
    return 100;
  }
  if (totalIndexers <= 0) {
    return 100;
  }
  double indexerPercentage=1;
  if (totalDocuments != 0) {
    indexerPercentage=(completedDocuments / totalDocuments);
  }
  double companyPercentage=(completedIndexers + (indexerPercentage)) / totalIndexers;
  double totalPercentage=(completedCompanies + companyPercentage) / totalCompanies;
  return (int)Math.min(Math.ceil(totalPercentage * 100),100);
}
