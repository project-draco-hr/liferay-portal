{
  String itemId=(String)oldValue;
  int pos=itemId.indexOf("|");
  if (pos == -1) {
    return String.valueOf(_shoppingItemIdMapper.getNewValue(new Long(GetterUtil.getLong(itemId))));
  }
 else {
    Long oldItemIdObj=new Long(GetterUtil.getLong(itemId.substring(0,pos)));
    return _shoppingItemIdMapper.getNewValue(oldItemIdObj) + itemId.substring(pos,itemId.length());
  }
}
