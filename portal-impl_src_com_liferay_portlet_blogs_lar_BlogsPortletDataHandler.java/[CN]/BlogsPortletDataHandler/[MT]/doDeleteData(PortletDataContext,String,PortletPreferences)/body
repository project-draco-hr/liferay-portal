{
  if (portletDataContext.addPrimaryKey(BlogsPortletDataHandler.class,"deleteData")) {
    return portletPreferences;
  }
  BlogsEntryLocalServiceUtil.deleteEntries(portletDataContext.getScopeGroupId());
  BlogsStatsUserLocalServiceUtil.deleteStatsUserByGroupId(portletDataContext.getScopeGroupId());
  return portletPreferences;
}
