{
  StringBundler sb=new StringBundler(6);
  sb.append(ExportImportPathUtil.getPortletPath(portletDataContext,PortletKeys.BLOGS));
  sb.append("/entries/");
  sb.append(entry.getUuid());
  sb.append("/thumbnail");
  sb.append(StringPool.PERIOD);
  sb.append(entry.getSmallImageType());
  return sb.toString();
}
