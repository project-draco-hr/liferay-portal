{
  selenium.clickAt("//input[@value='Add Blog Entry']",RuntimeVariables.replace("Add Blog Entry"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  selenium.type("//input[@id='_33_title']",RuntimeVariables.replace("Entry2"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//td[@id='cke_contents__33_editor']/iframe")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.selectFrame("//td[@id='cke_contents__33_editor']/iframe");
  selenium.type("//body",RuntimeVariables.replace("Content"));
  selenium.selectFrame("relative=top");
  assertTrue(selenium.isElementPresent("//body"));
  assertTrue(selenium.isElementPresent("//body"));
  selenium.clickAt("//input[@value='Publish']",RuntimeVariables.replace("Publish"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@class='portlet-msg-success']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals(RuntimeVariables.replace("Entry2"),selenium.getText("//div[@class='entry-title']/h2/a"));
  assertEquals(RuntimeVariables.replace("Content"),selenium.getText("//div[@class='entry-body']/p"));
}
