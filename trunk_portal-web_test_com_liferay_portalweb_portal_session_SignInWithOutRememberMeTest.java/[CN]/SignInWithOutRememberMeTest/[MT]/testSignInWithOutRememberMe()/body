{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Sign Out")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.click(RuntimeVariables.replace("link=Sign Out"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Sign In")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.click(RuntimeVariables.replace("link=Sign In"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.type("_58_login",RuntimeVariables.replace("test@liferay.com"));
  selenium.saveScreenShotAndSource();
  selenium.type("_58_password",RuntimeVariables.replace("test"));
  selenium.saveScreenShotAndSource();
  selenium.click(RuntimeVariables.replace("//input[@value='Sign In']"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Session Expiration Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.click(RuntimeVariables.replace("link=Session Expiration Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
}
