{
  try {
    ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
    UserGroup userGroup=ActionUtil.getUserGroup(portletRequest);
    if (UserGroupPermissionUtil.contains(themeDisplay.getPermissionChecker(),userGroup.getUserGroupId(),ActionKeys.UPDATE) && UserGroupPermissionUtil.contains(themeDisplay.getPermissionChecker(),userGroup.getUserGroupId(),ActionKeys.VIEW)) {
      return true;
    }
    return false;
  }
 catch (  Exception e) {
  }
  return false;
}
