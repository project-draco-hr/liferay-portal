{
  _description=description;
  if (_kaleoTaskRemoteModel != null) {
    try {
      Class<?> clazz=_kaleoTaskRemoteModel.getClass();
      Method method=clazz.getMethod("setDescription",String.class);
      method.invoke(_kaleoTaskRemoteModel,description);
    }
