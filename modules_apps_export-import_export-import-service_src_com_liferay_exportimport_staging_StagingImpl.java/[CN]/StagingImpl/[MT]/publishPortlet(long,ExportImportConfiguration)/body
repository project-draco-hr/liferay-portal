{
  Map<String,Serializable> taskContextMap=new HashMap<>();
  taskContextMap.put("exportImportConfigurationId",exportImportConfiguration.getExportImportConfigurationId());
  BackgroundTaskManagerUtil.addBackgroundTask(userId,exportImportConfiguration.getGroupId(),exportImportConfiguration.getName(),null,PortletStagingBackgroundTaskExecutor.class,taskContextMap,new ServiceContext());
}
