{
  Map<String,Serializable> taskContextMap=new HashMap<>();
  taskContextMap.put("exportImportConfigurationId",exportImportConfiguration.getExportImportConfigurationId());
  BackgroundTaskManagerUtil.addBackgroundTask(userId,exportImportConfiguration.getGroupId(),exportImportConfiguration.getName(),PortletStagingBackgroundTaskExecutor.class.getName(),taskContextMap,new ServiceContext());
}
