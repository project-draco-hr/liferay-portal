{
  PortalPreferences portalPreferences=PortletPreferencesFactoryUtil.getPortalPreferences(request);
  try {
    return getRecentLayoutAttribute(portalPreferences,getRecentLayoutSetBranchIdKey(layoutSetId));
  }
 catch (  JSONException jsone) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to get recent layout set branch ID with layout " + "set " + layoutSetId,jsone);
    }
  }
  return 0;
}
