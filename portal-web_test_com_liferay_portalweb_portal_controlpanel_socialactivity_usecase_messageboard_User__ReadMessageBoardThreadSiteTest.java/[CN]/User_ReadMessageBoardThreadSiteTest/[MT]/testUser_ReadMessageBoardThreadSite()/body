{
  selenium.open("/web/site-name/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Message Boards Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Message Boards Test Page",RuntimeVariables.replace("Message Boards Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=MB Thread Message Subject",RuntimeVariables.replace("MB Thread Message Subject"));
  selenium.waitForPageToLoad("30000");
}
