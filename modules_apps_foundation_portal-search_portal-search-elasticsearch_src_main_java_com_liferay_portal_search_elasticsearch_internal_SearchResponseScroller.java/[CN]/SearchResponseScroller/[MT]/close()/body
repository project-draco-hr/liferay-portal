{
  try {
    ClearScrollRequestBuilder clearScrollRequestBuilder=_client.prepareClearScroll();
    clearScrollRequestBuilder.setScrollIds(_previousScrollIds);
    ClearScrollResponse clearScrollResponse=clearScrollRequestBuilder.get();
    LogUtil.logActionResponse(_log,clearScrollResponse);
    return clearScrollResponse.isSucceeded();
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn(e,e);
    }
    return false;
  }
}
