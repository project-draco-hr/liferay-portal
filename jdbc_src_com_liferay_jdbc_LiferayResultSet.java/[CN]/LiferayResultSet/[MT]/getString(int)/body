{
  InputStream is=getAsciiStream(1);
  StringBuffer sb=new StringBuffer();
  try {
    int c;
    while ((c=is.read()) != -1) {
      sb.append((char)c);
    }
    is.close();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
    throw new SQLException(ioe.toString());
  }
  return sb.toString();
}
