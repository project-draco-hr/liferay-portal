{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Search Permissions Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Web Content Search Permissions Page",RuntimeVariables.replace("Web Content Search Permissions Page"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("You do not have the roles required to access this portlet."),selenium.getText("//div[@class='portlet-msg-error']"));
}
