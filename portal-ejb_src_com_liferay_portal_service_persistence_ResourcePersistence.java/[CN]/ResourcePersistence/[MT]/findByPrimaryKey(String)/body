{
  Session session=null;
  try {
    session=openSession();
    ResourceHBM resourceHBM=(ResourceHBM)session.get(ResourceHBM.class,resourceId);
    if (resourceHBM == null) {
      _log.warn("No Resource exists with the primary key " + resourceId.toString());
      throw new NoSuchResourceException("No Resource exists with the primary key " + resourceId.toString());
    }
    return ResourceHBMUtil.model(resourceHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
