{
  PermissionCheckerImpl permissionCheckerImpl=(PermissionCheckerImpl)permissionChecker;
  if (permissionChecker.hasPermission(0,Organization.class.getName(),organizationId,actionId)) {
    return true;
  }
 else   if (actionId.equals(ActionKeys.VIEW)) {
    User user=permissionCheckerImpl.getUser();
    Organization organization=user.getOrganization();
    if (organizationId == organization.getOrganizationId()) {
      return true;
    }
 else {
      return false;
    }
  }
 else {
    return false;
  }
}
