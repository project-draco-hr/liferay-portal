{
  if ((ss == null) || (ss.length == 0)) {
    return this;
  }
  if ((_array.length - _arrayIndex) < ss.length) {
    expandCapacity((_array.length + ss.length) * 2);
  }
  for (int i=0; i < ss.length; i++) {
    if (ss[i] != null && ss[i].length() > 0) {
      _array[_arrayIndex++]=ss[i];
    }
  }
  return this;
}
