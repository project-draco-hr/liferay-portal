{
  char[] charArray=new char[length];
  int offset=0;
  for (int i=0; i < index; i++) {
    String s=array[i];
    s.getChars(0,s.length(),charArray,offset);
    offset+=s.length();
  }
  try {
    return _unsafeStringConstructor.newInstance(0,length,charArray);
  }
 catch (  Exception e) {
    throw new IllegalStateException(e);
  }
}
