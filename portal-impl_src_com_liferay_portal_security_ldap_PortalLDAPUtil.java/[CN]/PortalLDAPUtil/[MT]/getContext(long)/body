{
  String baseProviderURL=PrefsPropsUtil.getString(companyId,PropsKeys.LDAP_BASE_PROVIDER_URL);
  String pricipal=PrefsPropsUtil.getString(companyId,PropsKeys.LDAP_SECURITY_PRINCIPAL);
  String credentials=PrefsPropsUtil.getString(companyId,PropsKeys.LDAP_SECURITY_CREDENTIALS);
  return getContext(companyId,baseProviderURL,pricipal,credentials);
}
