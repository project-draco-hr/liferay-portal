{
  Properties userMappings=LDAPSettingsUtil.getAllUserContactMappings(ldapServerId,companyId);
  List<String> mappedUserAttributeIds=new ArrayList<String>(userMappings.size());
  for (  Object mapping : userMappings.keySet()) {
    mappedUserAttributeIds.add(userMappings.getProperty((String)mapping));
  }
  return _getAttributes(ldapContext,fullDistinguishedName,mappedUserAttributeIds.toArray(new String[mappedUserAttributeIds.size()]));
}
