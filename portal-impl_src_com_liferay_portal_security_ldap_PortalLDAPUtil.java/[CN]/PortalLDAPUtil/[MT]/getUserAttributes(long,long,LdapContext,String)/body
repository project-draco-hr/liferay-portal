{
  Properties userMappings=LDAPSettingsUtil.getUserMappings(ldapServerId,companyId);
  String[] mappedUserAttributeIds={userMappings.getProperty("screenName"),userMappings.getProperty("emailAddress"),userMappings.getProperty("fullName"),userMappings.getProperty("firstName"),userMappings.getProperty("middleName"),userMappings.getProperty("lastName"),userMappings.getProperty("jobTitle"),userMappings.getProperty("group")};
  return _getAttributes(ctx,fullDistinguishedName,mappedUserAttributeIds);
}
