{
  String userDN=PrefsPropsUtil.getString(companyId,PropsUtil.LDAP_USERS_DN);
  String userFilter=PrefsPropsUtil.getString(companyId,PropsUtil.LDAP_IMPORT_USER_SEARCH_FILTER);
  SearchControls cons=new SearchControls(SearchControls.SUBTREE_SCOPE,maxResults,0,null,false,false);
  return ctx.search(userDN,userFilter,cons);
}
