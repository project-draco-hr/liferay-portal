{
  LocalContextProvider localContextProvider=scriptingContainer.getProvider();
  try {
    ThreadLocal<LocalContext> contextHolder=(ThreadLocal<LocalContext>)_contextHolderField.get(localContextProvider);
    LocalContext localContext=contextHolder.get();
    _runtimeField.set(localContext,null);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
