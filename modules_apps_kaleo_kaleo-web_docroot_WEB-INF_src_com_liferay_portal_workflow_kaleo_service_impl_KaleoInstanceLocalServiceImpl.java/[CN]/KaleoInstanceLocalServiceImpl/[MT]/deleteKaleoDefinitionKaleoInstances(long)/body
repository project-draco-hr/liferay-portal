{
  kaleoInstancePersistence.removeByKaleoDefinitionId(kaleoDefinitionId);
  kaleoInstanceTokenLocalService.deleteKaleoDefinitionKaleoInstanceTokens(kaleoDefinitionId);
  kaleoLogLocalService.deleteKaleoDefinitionKaleoLogs(kaleoDefinitionId);
  kaleoTaskInstanceTokenLocalService.deleteKaleoDefinitionKaleoTaskInstanceTokens(kaleoDefinitionId);
}
