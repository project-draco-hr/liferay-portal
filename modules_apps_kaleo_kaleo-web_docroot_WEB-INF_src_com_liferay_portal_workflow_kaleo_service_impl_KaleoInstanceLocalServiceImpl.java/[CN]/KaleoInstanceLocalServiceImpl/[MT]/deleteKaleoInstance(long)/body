{
  KaleoInstance kaleoInstance=null;
  try {
    kaleoInstance=kaleoInstancePersistence.remove(kaleoInstanceId);
  }
 catch (  NoSuchInstanceException nsie) {
    return null;
  }
  kaleoInstanceTokenLocalService.deleteKaleoInstanceKaleoInstanceTokens(kaleoInstanceId);
  kaleoLogLocalService.deleteKaleoInstanceKaleoLogs(kaleoInstanceId);
  kaleoTaskInstanceTokenLocalService.deleteKaleoInstanceKaleoTaskInstanceTokens(kaleoInstanceId);
  kaleoTimerInstanceTokenLocalService.deleteKaleoTimerInstanceTokens(kaleoInstanceId);
  return kaleoInstance;
}
