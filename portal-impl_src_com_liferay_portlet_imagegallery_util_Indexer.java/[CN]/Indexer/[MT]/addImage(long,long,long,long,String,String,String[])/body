{
  Document doc=getAddImageDocument(companyId,groupId,folderId,imageId,name,description,tagsEntries);
  IndexWriter writer=null;
  try {
    writer=LuceneUtil.getWriter(companyId);
    writer.addDocument(doc);
  }
  finally {
    if (writer != null) {
      LuceneUtil.write(companyId);
    }
  }
}
