{
  if (!ShoppingCategoryServiceUtil.hasAdmin() || !getUser().getCompanyId().equals(companyId)) {
    throw new PrincipalException();
  }
  List list=null;
  if (Validator.isNull(ppPaymentStatus)) {
    list=ShoppingOrderUtil.findByCompanyId(companyId);
  }
 else {
    list=ShoppingOrderUtil.findByC_PPPS(companyId,ShoppingUtil.getPpPaymentStatus(ppPaymentStatus));
  }
  return list;
}
