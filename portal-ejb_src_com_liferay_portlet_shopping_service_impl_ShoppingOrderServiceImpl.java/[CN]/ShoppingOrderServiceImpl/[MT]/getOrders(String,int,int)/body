{
  List list=null;
  if (Validator.isNull(ppPaymentStatus)) {
    list=ShoppingOrderUtil.findByUserId(getUserId(),begin,end,new OrderDateComparator(false));
  }
 else {
    list=ShoppingOrderUtil.findByU_PPPS(getUserId(),ShoppingUtil.getPpPaymentStatus(ppPaymentStatus),begin,end,new OrderDateComparator(false));
  }
  return list;
}
