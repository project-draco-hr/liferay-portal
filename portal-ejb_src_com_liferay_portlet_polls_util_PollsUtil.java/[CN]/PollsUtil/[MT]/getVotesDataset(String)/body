{
  DefaultCategoryDataset dataset=new DefaultCategoryDataset();
  String seriesName=StringPool.BLANK;
  Iterator itr=PollsChoiceLocalServiceUtil.getChoices(questionId).iterator();
  while (itr.hasNext()) {
    PollsChoice choice=(PollsChoice)itr.next();
    String category=choice.getChoiceId();
    Integer number=new Integer(PollsVoteLocalServiceUtil.getVotesCount(questionId,choice.getChoiceId()));
    dataset.addValue(number,seriesName,choice.getChoiceId());
  }
  return dataset;
}
