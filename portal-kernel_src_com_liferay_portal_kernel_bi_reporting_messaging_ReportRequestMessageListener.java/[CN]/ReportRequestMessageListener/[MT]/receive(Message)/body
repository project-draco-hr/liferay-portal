{
  ReportRequest request=(ReportRequest)message.getPayload();
  ReportResultContainer reportResultContainer=_reportResultContainer.clone(request.getReportName());
  Message replyMessage=MessageBusUtil.createResponseMessage(message);
  try {
    _reportEngine.execute(request,reportResultContainer);
  }
 catch (  ReportGenerationException rge) {
    _log.error("Unable to generate report",rge);
    reportResultContainer.setReportGenerationException(rge);
  }
 finally {
    replyMessage.setPayload(reportResultContainer);
    MessageBusUtil.sendMessage(replyMessage.getDestination(),replyMessage);
  }
}
