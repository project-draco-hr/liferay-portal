{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Announcements Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Announcements Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Manage Entries",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.select("_84_distributionScope",RuntimeVariables.replace("label=General"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Entry']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_84_title",RuntimeVariables.replace("Announcements Entry Title Null"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("You have entered invalid data. Please try again."),selenium.getText("//section/div/div/div/div"));
  assertEquals(RuntimeVariables.replace("Please enter valid content."),selenium.getText("//form/div[1]"));
}
