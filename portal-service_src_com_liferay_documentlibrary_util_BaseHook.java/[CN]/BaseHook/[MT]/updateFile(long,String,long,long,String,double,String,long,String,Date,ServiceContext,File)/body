{
  InputStream is=null;
  try {
    is=new BufferedInputStream(new FileInputStream(file));
    updateFile(companyId,portletId,groupId,repositoryId,fileName,versionNumber,sourceFileName,fileEntryId,properties,modifiedDate,serviceContext,is);
  }
 catch (  FileNotFoundException fnfe) {
    throw new NoSuchFileException(fileName);
  }
 finally {
    try {
      if (is != null) {
        is.close();
      }
    }
 catch (    IOException ioe) {
      _log.error(ioe);
    }
  }
}
