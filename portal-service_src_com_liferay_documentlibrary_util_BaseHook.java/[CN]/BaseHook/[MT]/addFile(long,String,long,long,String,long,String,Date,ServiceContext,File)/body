{
  InputStream is=null;
  try {
    is=new UnsyncBufferedInputStream(new FileInputStream(file));
    addFile(companyId,portletId,groupId,repositoryId,fileName,fileEntryId,properties,modifiedDate,serviceContext,is);
  }
 catch (  FileNotFoundException fnfe) {
    throw new NoSuchFileException(fileName);
  }
 finally {
    try {
      if (is != null) {
        is.close();
      }
    }
 catch (    IOException ioe) {
      _log.error(ioe);
    }
  }
}
