{
  if (_assetVocabularyIds != null) {
    return _assetVocabularyIds;
  }
  _assetVocabularyIds=getAvailableAssetVocabularyIds();
  if (!isAllAssetVocabularies() && (_portletPreferences.getValues("assetVocabularyIds",null) != null)) {
    _assetVocabularyIds=StringUtil.split(_portletPreferences.getValue("assetVocabularyIds",null),0L);
  }
  return _assetVocabularyIds;
}
