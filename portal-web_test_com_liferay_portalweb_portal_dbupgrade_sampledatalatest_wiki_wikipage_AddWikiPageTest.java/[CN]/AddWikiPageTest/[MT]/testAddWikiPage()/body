{
  selenium.open("/web/wiki-wiki-page-community/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Wiki Page Test")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Wiki Page Test",RuntimeVariables.replace("Wiki Page Test"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.clickAt("//div[5]/div/div/a",RuntimeVariables.replace("This page is empty. Edit it to add some text."));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.type("//input[@id='_36_content']",RuntimeVariables.replace("Front Page Text"));
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace(""),selenium.getText("//input[@id='_36_content']"));
  selenium.clickAt("//input[@value='Publish']",RuntimeVariables.replace("Publish"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@class='portlet-msg-success']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals(RuntimeVariables.replace("FrontPage"),selenium.getText("//h1[@class='header-title']/span"));
  assertEquals(RuntimeVariables.replace("Front Page Text"),selenium.getText("//div[@class='wiki-body']"));
}
