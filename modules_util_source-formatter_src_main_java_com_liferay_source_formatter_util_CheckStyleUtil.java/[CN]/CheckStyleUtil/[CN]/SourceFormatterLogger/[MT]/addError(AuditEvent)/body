{
  String fileName=StringUtil.replace(auditEvent.getFileName(),StringPool.BACK_SLASH,StringPool.SLASH);
  if (fileName.startsWith(_baseDirAbsolutePath + "/")) {
    fileName=StringUtil.replaceFirst(fileName,_baseDirAbsolutePath + "/",StringPool.BLANK);
  }
  _sourceFormatterMessages.add(new SourceFormatterMessage(fileName,auditEvent.getMessage(),auditEvent.getLine()));
  super.addError(auditEvent);
}
