{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Weather Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Weather Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//strong/a");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@class='lfr-component lfr-menu-list']/ul/li[3]/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("//div[@class='lfr-component lfr-menu-list']/ul/li[3]/a"));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(5000);
  selenium.type("_1_WAR_weatherportlet_zips",RuntimeVariables.replace("Diamond Bar, CA"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Weather Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Weather Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=Diamond Bar, CA"));
}
