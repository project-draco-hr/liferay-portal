{
  try {
    new FileRequest(null,System.currentTimeMillis(),true);
    Assert.fail();
  }
 catch (  NullPointerException npe) {
  }
  Assert.assertEquals(_path,_fileRequest.getPath());
  Assert.assertEquals(_LAST_MODIFIED_TIME,_fileRequest.getLastModifiedTime());
  Assert.assertEquals(_DELETE_AFTER_FETCH,_fileRequest.isDeleteAfterFetch());
}
