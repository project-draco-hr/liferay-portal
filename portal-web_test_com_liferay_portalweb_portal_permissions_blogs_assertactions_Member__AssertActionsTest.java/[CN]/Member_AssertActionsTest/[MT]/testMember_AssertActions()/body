{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Blogs Permissions Page");
  selenium.clickAt("link=Blogs Permissions Page",RuntimeVariables.replace("Blogs Permissions Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("RSS"),selenium.getText("//div[4]/span/a/span[1]"));
  assertTrue(selenium.isElementNotPresent("//input[@value='Add Blog Entry']"));
  assertTrue(selenium.isElementNotPresent("link=Edit"));
  assertTrue(selenium.isElementNotPresent("link=Permissions"));
  assertTrue(selenium.isElementNotPresent("link=Delete"));
  selenium.clickAt("link=Permissions Blogs Test Entry",RuntimeVariables.replace("Permissions Blogs Test Entry"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementNotPresent("link=Edit"));
  assertTrue(selenium.isElementNotPresent("link=Permissions"));
  assertTrue(selenium.isElementNotPresent("link=Delete"));
}
