{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Address.class,Address.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("addressId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("addressId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
