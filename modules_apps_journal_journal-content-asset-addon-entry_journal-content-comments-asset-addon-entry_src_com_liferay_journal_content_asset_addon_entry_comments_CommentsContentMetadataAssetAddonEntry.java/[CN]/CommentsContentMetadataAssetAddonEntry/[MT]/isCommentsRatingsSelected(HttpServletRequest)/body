{
  if (_commentRatingsContentMetadataAssetAddonEntry != null) {
    List<AssetAddonEntry> selectedEntries=(List<AssetAddonEntry>)request.getAttribute(WebKeys.ASSET_ADDON_ENTRIES);
    if ((selectedEntries != null) && selectedEntries.contains(_commentRatingsContentMetadataAssetAddonEntry)) {
