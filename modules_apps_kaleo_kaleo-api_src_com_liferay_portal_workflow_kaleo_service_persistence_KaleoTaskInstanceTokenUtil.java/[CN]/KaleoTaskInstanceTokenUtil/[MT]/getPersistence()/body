{
  if (_persistence == null) {
    _persistence=(KaleoTaskInstanceTokenPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoTaskInstanceTokenPersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoTaskInstanceTokenUtil.class,"_persistence");
  }
  return _persistence;
}
