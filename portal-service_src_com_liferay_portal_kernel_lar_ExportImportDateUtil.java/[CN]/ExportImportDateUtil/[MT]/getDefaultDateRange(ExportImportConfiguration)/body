{
  if (exportImportConfiguration.getType() == TYPE_EXPORT_LAYOUT) {
    return RANGE_ALL;
  }
 else   if (exportImportConfiguration.getType() == TYPE_EXPORT_PORTLET) {
    return RANGE_ALL;
  }
 else   if (exportImportConfiguration.getType() == TYPE_IMPORT_LAYOUT) {
    return RANGE_ALL;
  }
 else   if (exportImportConfiguration.getType() == TYPE_IMPORT_PORTLET) {
    return RANGE_ALL;
  }
 else   if (exportImportConfiguration.getType() == TYPE_PUBLISH_LAYOUT_LOCAL) {
    return RANGE_FROM_LAST_PUBLISH_DATE;
  }
 else   if (exportImportConfiguration.getType() == TYPE_PUBLISH_LAYOUT_REMOTE) {
    return RANGE_FROM_LAST_PUBLISH_DATE;
  }
 else   if (exportImportConfiguration.getType() == TYPE_PUBLISH_PORTLET) {
    return RANGE_FROM_LAST_PUBLISH_DATE;
  }
 else   if (exportImportConfiguration.getType() == TYPE_SCHEDULED_PUBLISH_LAYOUT_LOCAL) {
    return RANGE_FROM_LAST_PUBLISH_DATE;
  }
 else   if (exportImportConfiguration.getType() == TYPE_SCHEDULED_PUBLISH_LAYOUT_REMOTE) {
    return RANGE_FROM_LAST_PUBLISH_DATE;
  }
  return RANGE_ALL;
}
