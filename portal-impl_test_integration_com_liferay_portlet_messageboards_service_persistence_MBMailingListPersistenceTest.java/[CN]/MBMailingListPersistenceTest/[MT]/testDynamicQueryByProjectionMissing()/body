{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBMailingList.class,MBMailingList.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("mailingListId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("mailingListId",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
