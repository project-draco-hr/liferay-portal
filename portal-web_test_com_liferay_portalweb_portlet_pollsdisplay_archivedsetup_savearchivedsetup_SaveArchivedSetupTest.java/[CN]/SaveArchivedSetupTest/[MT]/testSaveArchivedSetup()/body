{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Polls Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Polls Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Archived",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_86_name",RuntimeVariables.replace("Archived Setup"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request processed successfully."),selenium.getText("//div[@id='p_p_id_86_']/div/div"));
  assertEquals(RuntimeVariables.replace("Archived Setup"),selenium.getText("//tr[3]/td[1]"));
  assertEquals(RuntimeVariables.replace("Joe Bloggs"),selenium.getText("//tr[3]/td[2]"));
}
