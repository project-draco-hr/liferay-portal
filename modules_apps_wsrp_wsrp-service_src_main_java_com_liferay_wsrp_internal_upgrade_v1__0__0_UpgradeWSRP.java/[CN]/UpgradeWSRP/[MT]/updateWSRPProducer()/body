{
  runSQL("update WSRP_WSRPProducer set version = '2.0' where version is " + "null or version = ''");
  List<WSRPProducer> wsrpProducers=WSRPProducerLocalServiceUtil.getWSRPProducers(QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  for (  WSRPProducer wsrpProducer : wsrpProducers) {
    WSRPProducerLocalServiceUtil.updateWSRPProducer(wsrpProducer);
  }
}
