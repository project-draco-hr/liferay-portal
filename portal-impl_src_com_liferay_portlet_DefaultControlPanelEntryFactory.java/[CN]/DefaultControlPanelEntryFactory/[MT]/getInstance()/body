{
  if (_controlPanelEntry == null) {
    if (_log.isDebugEnabled()) {
      _log.debug("Instantiate " + PropsValues.CONTROL_PANEL_DEFAULT_ENTRY_CLASS);
    }
    ClassLoader classLoader=PACLClassLoaderUtil.getPortalClassLoader();
    try {
      _controlPanelEntry=(ControlPanelEntry)classLoader.loadClass(PropsValues.CONTROL_PANEL_DEFAULT_ENTRY_CLASS).newInstance();
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Return " + ClassUtil.getClassName(_controlPanelEntry));
  }
  return _controlPanelEntry;
}
