{
  Set<String> cachelessMappingTableNames=TableMapperFactory.cachelessMappingTableNames;
  ReflectionTestUtil.setFieldValue(TableMapperFactory.class,"cachelessMappingTableNames",new HashSet<String>(){
    @Override public boolean contains(    Object o){
      return true;
    }
  }
);
  try {
    testTableMapperFactory();
  }
  finally {
    ReflectionTestUtil.setFieldValue(TableMapperFactory.class,"cachelessMappingTableNames",cachelessMappingTableNames);
  }
}
