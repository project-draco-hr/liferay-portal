{
  PortalCache<?,?> portalCache=_portalCaches.get(name);
  if (portalCache == null) {
    portalCache=new MemoryPortalCache<>(new MockPortalCacheManager<Long,long[]>(name),name,16);
    _portalCaches.put(name,portalCache);
  }
  return (PortalCache<? extends Serializable,? extends Serializable>)portalCache;
}
