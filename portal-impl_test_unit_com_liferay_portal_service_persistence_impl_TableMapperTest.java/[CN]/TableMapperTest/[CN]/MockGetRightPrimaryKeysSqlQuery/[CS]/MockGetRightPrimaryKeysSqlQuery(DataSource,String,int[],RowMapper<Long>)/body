{
  Assert.assertSame(_dataSource,dataSource);
  Assert.assertEquals("SELECT " + _RIGHT_COLUMN_NAME + " FROM "+ _TABLE_NAME+ " WHERE "+ _COMPANY_COLUMN_NAME+ " = ? AND "+ _LEFT_COLUMN_NAME+ " = ?",sql);
  Assert.assertArrayEquals(new int[]{Types.BIGINT,Types.BIGINT},types);
  Assert.assertSame(RowMapper.PRIMARY_KEY,rowMapper);
}
