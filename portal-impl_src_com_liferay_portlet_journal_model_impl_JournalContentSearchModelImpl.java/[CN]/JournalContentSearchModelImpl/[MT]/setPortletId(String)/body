{
  if (((portletId == null) && (_portletId != null)) || ((portletId != null) && (_portletId == null)) || ((portletId != null) && (_portletId != null) && !portletId.equals(_portletId))) {
    _portletId=portletId;
    if (_originalPortletId == null) {
      _originalPortletId=portletId;
