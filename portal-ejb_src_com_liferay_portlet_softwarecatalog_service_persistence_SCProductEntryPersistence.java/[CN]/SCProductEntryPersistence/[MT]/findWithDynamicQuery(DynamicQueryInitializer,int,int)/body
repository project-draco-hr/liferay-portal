{
  Session session=null;
  try {
    session=openSession();
    DynamicQuery query=queryInitializer.initialize(session);
    query.setLimit(begin,end);
    return query.list();
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
