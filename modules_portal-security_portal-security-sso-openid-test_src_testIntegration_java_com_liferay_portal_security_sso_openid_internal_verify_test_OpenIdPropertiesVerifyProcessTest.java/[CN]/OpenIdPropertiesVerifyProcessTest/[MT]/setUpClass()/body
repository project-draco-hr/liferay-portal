{
  UnicodeProperties properties=new UnicodeProperties();
  properties.put(LegacyOpenIdPropsKeys.OPENID_AUTH_ENABLED,"false");
  List<Company> companies=CompanyLocalServiceUtil.getCompanies(false);
  for (  Company company : companies) {
    CompanyLocalServiceUtil.updatePreferences(company.getCompanyId(),properties);
  }
  Bundle bundle=FrameworkUtil.getBundle(OpenIdPropertiesVerifyProcessTest.class);
  _bundleContext=bundle.getBundleContext();
}
