{
  try {
    if (FileUtil.isIgnoredFilePath(filePath) || !FileUtil.isValidFileName(filePath)) {
      if (_logger.isDebugEnabled()) {
        _logger.debug("Ignored file path {}",filePath);
      }
      return true;
    }
    return false;
  }
 catch (  Exception e) {
    if (_logger.isDebugEnabled()) {
      _logger.debug(e.getMessage(),e);
    }
    return false;
  }
}
