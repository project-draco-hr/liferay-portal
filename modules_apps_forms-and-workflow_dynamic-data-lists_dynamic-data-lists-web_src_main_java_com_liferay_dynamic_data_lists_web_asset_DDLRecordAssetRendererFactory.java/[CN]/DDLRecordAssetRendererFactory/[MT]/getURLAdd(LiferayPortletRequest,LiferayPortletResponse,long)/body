{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(liferayPortletRequest,(Group)liferayPortletRequest.getAttribute(WebKeys.GROUP),DDLPortletKeys.DYNAMIC_DATA_LISTS,0,0,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/edit_record.jsp");
  if (classTypeId > 0) {
    portletURL.setParameter("recordSetId",String.valueOf(classTypeId));
  }
  return portletURL;
}
