{
  Indexer<?> indexer=IndexerRegistryUtil.nullSafeGetIndexer(UserGroup.class);
  if (indexer.isIndexerEnabled() && PropsValues.USER_GROUPS_SEARCH_WITH_INDEX && MapUtil.isEmpty(params)) {
    return false;
  }
  for (  String key : params.keySet()) {
    if (ArrayUtil.contains(UserGroupFinderConstants.PARAM_KEYS_USER_GROUP_FINDER,key)) {
      return true;
    }
  }
  return false;
}
