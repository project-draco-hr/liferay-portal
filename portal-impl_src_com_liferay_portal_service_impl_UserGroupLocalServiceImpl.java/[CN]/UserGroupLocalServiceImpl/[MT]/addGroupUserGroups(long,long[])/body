{
  groupPersistence.addUserGroups(groupId,userGroupIds);
  Group group=groupPersistence.findByPrimaryKey(groupId);
  Role role=rolePersistence.findByC_N(group.getCompanyId(),RoleConstants.COMMUNITY_MEMBER);
  for (  long userGroupId : userGroupIds) {
    userGroupGroupRoleLocalService.addUserGroupGroupRoles(userGroupId,groupId,new long[]{role.getRoleId()});
  }
  PermissionCacheUtil.clearCache();
}
