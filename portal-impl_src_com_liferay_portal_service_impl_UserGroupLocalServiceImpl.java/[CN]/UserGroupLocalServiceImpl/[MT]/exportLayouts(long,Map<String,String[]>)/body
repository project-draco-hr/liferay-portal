{
  File[] exportFiles=new File[2];
  UserGroup userGroup=userGroupLocalService.getUserGroup(userGroupId);
  long groupId=userGroup.getGroup().getGroupId();
  if (userGroup.hasPrivateLayouts()) {
    exportFiles[0]=layoutLocalService.exportLayoutsAsFile(groupId,true,null,parameterMap,null,null);
  }
  if (userGroup.hasPublicLayouts()) {
    exportFiles[1]=layoutLocalService.exportLayoutsAsFile(groupId,false,null,parameterMap,null,null);
  }
  return exportFiles;
}
