{
  User user=userPersistence.findByPrimaryKey(userId);
  long groupId=user.getGroup().getGroupId();
  if (privateLayoutsIS != null) {
    layoutLocalService.importLayouts(userId,groupId,true,parameterMap,privateLayoutsIS);
  }
  if (publicLayoutsIS != null) {
    layoutLocalService.importLayouts(userId,groupId,false,parameterMap,publicLayoutsIS);
  }
}
