{
  User user=userPersistence.findByPrimaryKey(userId);
  if (layoutHolder.hasPrivateLayouts()) {
    long targetGroupId=user.getGroup().getGroupId();
    layoutLocalService.importLayouts(userId,targetGroupId,true,parameterMap,layoutHolder.getPrivateLayouts());
  }
  if (layoutHolder.hasPublicLayouts()) {
    long targetGroupId=user.getGroup().getGroupId();
    layoutLocalService.importLayouts(userId,targetGroupId,false,parameterMap,layoutHolder.getPublicLayouts());
  }
}
