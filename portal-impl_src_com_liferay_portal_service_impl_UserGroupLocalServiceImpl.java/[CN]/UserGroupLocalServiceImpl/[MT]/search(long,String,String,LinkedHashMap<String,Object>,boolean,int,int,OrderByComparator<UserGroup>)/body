{
  if (isUseCustomSQL(params)) {
    return userGroupFinder.filterFindByC_N_D(companyId,name,description,params,andOperator,start,end,obc);
  }
  String orderByType=StringPool.BLANK;
  if (obc.isAscending()) {
    orderByType="asc";
  }
  Sort sort=SortFactoryUtil.getSort(UserGroup.class,obc.getOrderBy(),orderByType);
  try {
    return UsersAdminUtil.getUserGroups(search(companyId,name,description,params,andOperator,start,end,sort));
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
