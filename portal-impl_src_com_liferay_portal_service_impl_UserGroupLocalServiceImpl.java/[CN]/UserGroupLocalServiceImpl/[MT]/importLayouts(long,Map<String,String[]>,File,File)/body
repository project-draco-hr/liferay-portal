{
  User user=userPersistence.findByPrimaryKey(userId);
  long groupId=user.getGroup().getGroupId();
  if (privateExportFile != null) {
    layoutLocalService.importLayouts(userId,groupId,true,parameterMap,privateExportFile);
  }
  if (publicExportFile != null) {
    layoutLocalService.importLayouts(userId,groupId,false,parameterMap,publicExportFile);
  }
}
