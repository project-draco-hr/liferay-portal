{
  for (  long classPK : classPKs) {
    JournalArticle article=JournalArticleLocalServiceUtil.getLatestArticle(classPK);
    if (checkPermission) {
      JournalArticleServiceUtil.deleteArticle(article.getGroupId(),article.getArticleId(),null);
    }
 else {
      JournalArticleLocalServiceUtil.deleteArticle(article.getGroupId(),article.getArticleId(),null);
    }
  }
}
