{
  if (SessionErrors.contains(renderRequest,WorkflowException.class.getName())) {
    hideDefaultErrorMessage(renderRequest);
    include("/error.jsp",renderRequest,renderResponse);
  }
 else   if (SessionErrors.contains(renderRequest,RequiredWorkflowDefinitionException.class.getName())) {
    hideDefaultErrorMessage(renderRequest);
    include("/view.jsp",renderRequest,renderResponse);
  }
 else {
    if (SessionErrors.contains(renderRequest,WorkflowDefinitionFileException.class.getName())) {
      hideDefaultErrorMessage(renderRequest);
    }
    super.doDispatch(renderRequest,renderResponse);
  }
}
