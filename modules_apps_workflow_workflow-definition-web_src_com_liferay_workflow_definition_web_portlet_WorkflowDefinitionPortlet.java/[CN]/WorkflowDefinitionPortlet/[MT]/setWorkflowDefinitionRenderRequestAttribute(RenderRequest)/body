{
  ThemeDisplay themeDisplay=(ThemeDisplay)renderRequest.getAttribute(WebKeys.THEME_DISPLAY);
  String name=ParamUtil.getString(renderRequest,"name");
  int version=ParamUtil.getInteger(renderRequest,"version");
  if (Validator.isNull(name)) {
    return;
  }
  WorkflowDefinition workflowDefinition=WorkflowDefinitionManagerUtil.getWorkflowDefinition(themeDisplay.getCompanyId(),name,version);
  renderRequest.setAttribute(WebKeys.WORKFLOW_DEFINITION,workflowDefinition);
}
