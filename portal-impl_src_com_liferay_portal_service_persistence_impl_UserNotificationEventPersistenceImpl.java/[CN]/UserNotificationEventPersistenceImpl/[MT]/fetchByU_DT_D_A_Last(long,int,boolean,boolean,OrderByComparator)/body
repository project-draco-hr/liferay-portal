{
  int count=countByU_DT_D_A(userId,deliveryType,delivered,actionRequired);
  if (count == 0) {
    return null;
  }
  List<UserNotificationEvent> list=findByU_DT_D_A(userId,deliveryType,delivered,actionRequired,count - 1,count,orderByComparator);
  if (!list.isEmpty()) {
    return list.get(0);
  }
  return null;
}
