{
  UserNotificationEvent userNotificationEvent=fetchByU_D_A_First(userId,delivered,actionRequired,orderByComparator);
  if (userNotificationEvent != null) {
    return userNotificationEvent;
  }
  StringBundler msg=new StringBundler(8);
  msg.append(_NO_SUCH_ENTITY_WITH_KEY);
  msg.append("userId=");
  msg.append(userId);
  msg.append(", delivered=");
  msg.append(delivered);
  msg.append(", actionRequired=");
  msg.append(actionRequired);
  msg.append(StringPool.CLOSE_CURLY_BRACE);
  throw new NoSuchUserNotificationEventException(msg.toString());
}
