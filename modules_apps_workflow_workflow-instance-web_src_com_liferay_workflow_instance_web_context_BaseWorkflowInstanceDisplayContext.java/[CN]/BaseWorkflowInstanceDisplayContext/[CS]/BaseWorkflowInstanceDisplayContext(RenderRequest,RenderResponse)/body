{
  ThemeDisplay themeDisplay=(ThemeDisplay)renderRequest.getAttribute(WebKeys.THEME_DISPLAY);
  this.dateFormatDateTime=FastDateFormatFactoryUtil.getDateTime(themeDisplay.getLocale(),themeDisplay.getTimeZone());
  this.renderRequest=renderRequest;
  this.renderResponse=renderResponse;
  this.workflowInstanceRequestHelper=new WorkflowInstanceRequestHelper(renderRequest);
}
