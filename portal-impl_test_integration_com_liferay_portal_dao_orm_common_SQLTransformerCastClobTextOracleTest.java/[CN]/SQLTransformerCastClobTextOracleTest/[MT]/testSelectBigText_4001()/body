{
  List<String> values=select(_BIG_TEXT_A_4000,_A);
  Assert.assertTrue(values.toString(),values.isEmpty());
  values=select(_BIG_TEXT_A_3999_B_1,_B);
  Assert.assertTrue(values.toString(),values.isEmpty());
  values=select(_BIG_TEXT_A_4000,_B);
  Assert.assertTrue(values.toString(),values.isEmpty());
}
