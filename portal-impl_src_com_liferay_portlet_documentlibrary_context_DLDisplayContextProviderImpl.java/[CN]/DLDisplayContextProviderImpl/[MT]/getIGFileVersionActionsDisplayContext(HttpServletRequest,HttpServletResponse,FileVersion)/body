{
  DLViewFileVersionDisplayContext dlViewFileVersionDisplayContext=new DefaultIGViewFileVersionDisplayContext(request,response,fileVersion);
  if (fileVersion == null) {
    return dlViewFileVersionDisplayContext;
  }
  for (  DLDisplayContextFactory dlDisplayContextFactory : getDisplayContextFactories()) {
    dlViewFileVersionDisplayContext=dlDisplayContextFactory.getIGFileVersionActionsDisplayContext(dlViewFileVersionDisplayContext,request,response,fileVersion);
  }
  return dlViewFileVersionDisplayContext;
}
