{
  for (  String className : PropsValues.SANITIZER_IMPL) {
    if (Validator.isNull(className)) {
      continue;
    }
    try {
      Sanitizer sanitizer=(Sanitizer)InstanceFactory.newInstance(className);
      registerSanitizer(sanitizer);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
}
