{
  DDMExpressionImpl<Boolean> expression=new DDMExpressionImpl<>("not false",Boolean.class);
  Expression model=expression.getModel();
  Assert.assertEquals(NotExpression.class,model.getClass());
  NotExpression notExpression=(NotExpression)model;
  Expression operand=notExpression.getOperand();
  Assert.assertEquals(Term.class,operand.getClass());
  Term term=(Term)operand;
  Assert.assertEquals("false",term.getValue());
}
