{
  if (!PropsValues.SOCIAL_EQUITY_EQUITY_LOG_ENABLED) {
    return;
  }
  AssetEntry assetEntry=assetEntryLocalService.getEntry(className,classPK);
  List<SocialEquityLog> equityLogs=socialEquityLogPersistence.findByU_AEI_AID_A_E(userId,assetEntry.getEntryId(),actionId,true,extraData);
  deactivateEquityLogs(equityLogs);
}
