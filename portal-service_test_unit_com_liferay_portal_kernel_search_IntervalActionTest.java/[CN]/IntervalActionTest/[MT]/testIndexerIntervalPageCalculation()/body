{
  IntervalAction intervalActionInstance=new IntervalAction();
  intervalActionInstance.setPerformActionMethod(new IntervalAction.PerformIntervalActionMethod(){
    @Override public void performAction(    int start,    int end) throws PortalException {
      incrementCounter();
    }
  }
);
  intervalActionInstance.setTotal(125);
  intervalActionInstance.performActions();
  Assert.assertEquals(2,_count);
}
