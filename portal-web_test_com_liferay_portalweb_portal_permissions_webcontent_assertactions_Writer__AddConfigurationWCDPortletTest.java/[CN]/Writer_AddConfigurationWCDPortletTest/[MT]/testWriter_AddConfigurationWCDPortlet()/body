{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Display Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Web Content Display Permissions Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//img[@alt='Select Web Content']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("_86_showAvailableLocalesCheckbox",RuntimeVariables.replace(""));
  Thread.sleep(500);
  selenium.clickAt("_86_enablePrintCheckbox",RuntimeVariables.replace(""));
  Thread.sleep(500);
  selenium.clickAt("_86_enableRatingsCheckbox",RuntimeVariables.replace(""));
  Thread.sleep(500);
  selenium.clickAt("_86_enableCommentsCheckbox",RuntimeVariables.replace(""));
  Thread.sleep(500);
  selenium.clickAt("_86_enableCommentRatingsCheckbox",RuntimeVariables.replace(""));
}
