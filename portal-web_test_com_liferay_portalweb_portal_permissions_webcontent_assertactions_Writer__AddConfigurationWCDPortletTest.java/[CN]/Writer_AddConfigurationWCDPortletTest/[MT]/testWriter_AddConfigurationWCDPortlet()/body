{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Display Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Web Content Display Permissions Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("//img[@alt='Select Web Content']"));
  selenium.waitForPageToLoad("30000");
  selenium.click("_86_showAvailableLocalesCheckbox");
  Thread.sleep(500);
  selenium.click("_86_enablePrintCheckbox");
  Thread.sleep(500);
  selenium.click("_86_enableRatingsCheckbox");
  Thread.sleep(500);
  selenium.click("_86_enableCommentsCheckbox");
  Thread.sleep(500);
  selenium.click("_86_enableCommentRatingsCheckbox");
}
