{
  JournalArticle article=_journalArticleLocalService.getLatestArticle(activity.getClassPK());
  Layout layout=article.getLayout();
  if (layout != null) {
    String groupFriendlyURL=PortalUtil.getGroupFriendlyURL(layout.getLayoutSet(),serviceContext.getThemeDisplay());
    return groupFriendlyURL.concat(JournalArticleConstants.CANONICAL_URL_SEPARATOR).concat(article.getUrlTitle());
  }
  return null;
}
