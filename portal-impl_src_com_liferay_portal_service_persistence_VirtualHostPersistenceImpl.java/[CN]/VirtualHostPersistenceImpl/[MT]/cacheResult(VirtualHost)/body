{
  EntityCacheUtil.putResult(VirtualHostModelImpl.ENTITY_CACHE_ENABLED,VirtualHostImpl.class,virtualHost.getPrimaryKey(),virtualHost);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_HOSTNAME,new Object[]{virtualHost.getHostname()},virtualHost);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_L,new Object[]{new Long(virtualHost.getCompanyId()),new Long(virtualHost.getLayoutSetId())},virtualHost);
}
