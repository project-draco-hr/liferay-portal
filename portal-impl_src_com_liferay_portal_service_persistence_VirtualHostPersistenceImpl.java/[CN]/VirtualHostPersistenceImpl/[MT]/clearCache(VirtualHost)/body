{
  EntityCacheUtil.removeResult(VirtualHostModelImpl.ENTITY_CACHE_ENABLED,VirtualHostImpl.class,virtualHost.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_HOSTNAME,new Object[]{virtualHost.getHostname()});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_L,new Object[]{Long.valueOf(virtualHost.getCompanyId()),Long.valueOf(virtualHost.getLayoutSetId())});
}
