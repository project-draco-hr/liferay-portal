{
  VirtualHost virtualHost=(VirtualHost)EntityCacheUtil.getResult(VirtualHostModelImpl.ENTITY_CACHE_ENABLED,VirtualHostImpl.class,virtualHostId,this);
  if (virtualHost == null) {
    Session session=null;
    try {
      session=openSession();
      virtualHost=(VirtualHost)session.get(VirtualHostImpl.class,new Long(virtualHostId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (virtualHost != null) {
        cacheResult(virtualHost);
      }
      closeSession(session);
    }
  }
  return virtualHost;
}
