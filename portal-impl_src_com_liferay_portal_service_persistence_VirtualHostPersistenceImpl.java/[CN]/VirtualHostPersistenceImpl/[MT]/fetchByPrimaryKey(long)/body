{
  VirtualHost virtualHost=(VirtualHost)EntityCacheUtil.getResult(VirtualHostModelImpl.ENTITY_CACHE_ENABLED,VirtualHostImpl.class,virtualHostId,this);
  if (virtualHost == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (virtualHost == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      virtualHost=(VirtualHost)session.get(VirtualHostImpl.class,Long.valueOf(virtualHostId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (virtualHost == null)) {
        EntityCacheUtil.putResult(VirtualHostModelImpl.ENTITY_CACHE_ENABLED,VirtualHostImpl.class,virtualHostId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(virtualHost);
      }
      closeSession(session);
    }
  }
  return virtualHost;
}
