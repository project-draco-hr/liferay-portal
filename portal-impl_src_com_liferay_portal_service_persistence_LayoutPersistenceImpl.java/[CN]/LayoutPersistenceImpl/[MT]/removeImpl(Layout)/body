{
  layout=toUnwrappedModel(layout);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,layout);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  LayoutModelImpl layoutModelImpl=(LayoutModelImpl)layout;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{layoutModelImpl.getUuid(),new Long(layoutModelImpl.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_ICONIMAGEID,new Object[]{new Long(layoutModelImpl.getIconImageId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_P_L,new Object[]{new Long(layoutModelImpl.getGroupId()),Boolean.valueOf(layoutModelImpl.getPrivateLayout()),new Long(layoutModelImpl.getLayoutId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_P_F,new Object[]{new Long(layoutModelImpl.getGroupId()),Boolean.valueOf(layoutModelImpl.getPrivateLayout()),layoutModelImpl.getFriendlyURL()});
  EntityCacheUtil.removeResult(LayoutModelImpl.ENTITY_CACHE_ENABLED,LayoutImpl.class,layout.getPrimaryKey());
  return layout;
}
