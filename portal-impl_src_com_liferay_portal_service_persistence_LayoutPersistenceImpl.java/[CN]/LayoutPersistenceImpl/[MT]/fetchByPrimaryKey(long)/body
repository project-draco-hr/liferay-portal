{
  Layout layout=(Layout)EntityCacheUtil.getResult(LayoutModelImpl.ENTITY_CACHE_ENABLED,LayoutImpl.class,plid,this);
  if (layout == null) {
    Session session=null;
    try {
      session=openSession();
      layout=(Layout)session.get(LayoutImpl.class,new Long(plid));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (layout != null) {
        cacheResult(layout);
      }
      closeSession(session);
    }
  }
  return layout;
}
