{
  Layout layout=(Layout)EntityCacheUtil.getResult(LayoutModelImpl.ENTITY_CACHE_ENABLED,LayoutImpl.class,plid,this);
  if (layout == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (layout == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      layout=(Layout)session.get(LayoutImpl.class,Long.valueOf(plid));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (layout == null)) {
        EntityCacheUtil.putResult(LayoutModelImpl.ENTITY_CACHE_ENABLED,LayoutImpl.class,plid,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(layout);
      }
      closeSession(session);
    }
  }
  return layout;
}
