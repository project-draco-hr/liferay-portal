{
  Layout result=(Layout)EntityCacheUtil.getResult(LayoutModelImpl.ENTITY_CACHE_ENABLED,Layout.class,plid,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      Layout layout=(Layout)session.get(LayoutImpl.class,new Long(plid));
      cacheResult(layout);
      return layout;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (Layout)result;
  }
}
