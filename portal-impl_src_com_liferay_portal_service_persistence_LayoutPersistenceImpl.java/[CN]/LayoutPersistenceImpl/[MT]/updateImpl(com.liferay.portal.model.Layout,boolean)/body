{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,layout,merge);
    layout.setNew(false);
    return layout;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Layout.class.getName());
  }
}
