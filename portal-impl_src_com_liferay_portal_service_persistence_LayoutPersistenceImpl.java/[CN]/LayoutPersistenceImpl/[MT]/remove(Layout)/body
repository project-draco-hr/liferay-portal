{
  Session session=null;
  try {
    session=openSession();
    layout=(Layout)session.merge(layout);
    session.delete(layout);
    session.flush();
    return layout;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Layout.class.getName());
  }
}
