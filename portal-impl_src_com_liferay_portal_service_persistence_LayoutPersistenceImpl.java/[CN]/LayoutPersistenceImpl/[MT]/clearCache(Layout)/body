{
  EntityCacheUtil.removeResult(LayoutModelImpl.ENTITY_CACHE_ENABLED,LayoutImpl.class,layout.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{layout.getUuid(),Long.valueOf(layout.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_ICONIMAGEID,new Object[]{Long.valueOf(layout.getIconImageId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_P_L,new Object[]{Long.valueOf(layout.getGroupId()),Boolean.valueOf(layout.getPrivateLayout()),Long.valueOf(layout.getLayoutId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_P_F,new Object[]{Long.valueOf(layout.getGroupId()),Boolean.valueOf(layout.getPrivateLayout()),layout.getFriendlyURL()});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_P_TLU,new Object[]{Long.valueOf(layout.getGroupId()),Boolean.valueOf(layout.getPrivateLayout()),layout.getTemplateLayoutUuid()});
}
