{
  try {
    List<AssetCategory> categories=AssetCategoryServiceUtil.getCategories(_className,_classPK);
    if (!categories.isEmpty()) {
      return EVAL_BODY_INCLUDE;
    }
    return SKIP_BODY;
  }
 catch (  Exception e) {
    throw new JspException(e);
  }
 finally {
    if (!ServerDetector.isResin()) {
      _className=null;
      _classPK=0;
    }
  }
}
