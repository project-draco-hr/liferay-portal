{
  Map<String,String[]> parameterMap=new HashMap<String,String[]>();
  String portletId=req.getParameter("p_p_id");
  String namespace=PortalUtil.getPortletNamespace(portletId);
  Set<Map.Entry<String,String[]>> entries=req.getParameterMap().entrySet();
  for (  Map.Entry<String,String[]> mapEntry : entries) {
    String key=mapEntry.getKey();
    if (PortalUtil.isReservedParameter(key)) {
      continue;
    }
    if (key.startsWith(namespace)) {
      parameterMap.put(key.substring(namespace.length()),mapEntry.getValue());
    }
 else     if (key.startsWith(QNameUtil.PUBLIC_RENDER_PARAMETER_NAMESPACE)) {
      String identifier=QNameUtil.getPublicRenderParameterIdentifier(key);
      parameterMap.put(identifier,mapEntry.getValue());
    }
 else {
      parameterMap.put(key,mapEntry.getValue());
    }
  }
  return parameterMap;
}
