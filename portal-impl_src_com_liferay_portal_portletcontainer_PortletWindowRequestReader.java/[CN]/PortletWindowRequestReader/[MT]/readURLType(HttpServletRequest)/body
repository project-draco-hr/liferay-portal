{
  String urlType=ParamUtil.getString(req,"p_p_url_type");
  if (urlType.equals("0")) {
    return ChannelURLType.RENDER;
  }
 else   if (urlType.equals("1")) {
    return ChannelURLType.ACTION;
  }
 else   if (urlType.equals("2")) {
    return ChannelURLType.RESOURCE;
  }
 else {
    return ChannelURLType.RENDER;
  }
}
