{
  WorkflowDefinitionLinkSearchTerms searchTerms=(WorkflowDefinitionLinkSearchTerms)searchContainer.getSearchTerms();
  List<WorkflowDefinitionLinkSearchEntry> workflowDefinitionLinkSearchEntries=createWorkflowDefinitionLinkSearchEntryList();
  if (searchTerms.isAdvancedSearch()) {
    filter(workflowDefinitionLinkSearchEntries,searchTerms);
  }
 else {
    filter(workflowDefinitionLinkSearchEntries,StringUtil.toLowerCase(searchTerms.getKeywords()));
  }
  searchContainer.setTotal(workflowDefinitionLinkSearchEntries.size());
  sort(workflowDefinitionLinkSearchEntries);
  return workflowDefinitionLinkSearchEntries;
}
