{
  DDMFormLayoutPage ddmFormLayoutPage=new DDMFormLayoutPage();
  LocalizedValue ddmFormLayoutPageTitle=new LocalizedValue(LocaleUtil.US);
  ddmFormLayoutPageTitle.addString(LocaleUtil.US,"Page1");
