{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBBan.class,MBBan.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("banId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("banId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
