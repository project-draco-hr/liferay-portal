{
  Destination destination=_destinations.get(destinationName);
  if (destination != null) {
    return destination.register(messageListener);
  }
  List<MessageListener> queuedMessageListeners=_queuedMessageListeners.get(destinationName);
  if (queuedMessageListeners == null) {
    queuedMessageListeners=new ArrayList<>();
    _queuedMessageListeners.put(destinationName,queuedMessageListeners);
  }
  queuedMessageListeners.add(messageListener);
  if (_log.isWarnEnabled()) {
    _log.warn("Queuing message listener until destination " + destinationName + " is added");
  }
  return false;
}
