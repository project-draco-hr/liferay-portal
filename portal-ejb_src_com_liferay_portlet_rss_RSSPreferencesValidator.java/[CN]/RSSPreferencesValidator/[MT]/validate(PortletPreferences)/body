{
  List badURLs=new ArrayList();
  String[] urls=prefs.getValues("urls",new String[0]);
  for (int i=0; i < urls.length; i++) {
    try {
      new URL(urls[i]);
    }
 catch (    MalformedURLException murle) {
      badURLs.add(urls[i]);
    }
  }
  if (badURLs.size() > 0) {
    throw new ValidatorException("Failed to retrieve URLs",badURLs);
  }
}
