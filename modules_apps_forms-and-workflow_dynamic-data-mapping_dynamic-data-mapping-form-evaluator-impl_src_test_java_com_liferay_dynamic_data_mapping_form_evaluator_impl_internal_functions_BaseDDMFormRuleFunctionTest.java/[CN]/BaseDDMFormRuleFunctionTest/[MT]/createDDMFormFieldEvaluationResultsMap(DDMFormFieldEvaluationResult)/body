{
  Map<String,List<DDMFormFieldEvaluationResult>> ddmFormFieldEvaluationResultsMap=new HashMap<>();
  for (  DDMFormFieldEvaluationResult ddmFormFieldEvaluationResult : ddmFormFieldEvaluationResultArray) {
    List<DDMFormFieldEvaluationResult> ddmFormFieldEvaluationResults=ddmFormFieldEvaluationResultsMap.get(ddmFormFieldEvaluationResult.getName());
    if (ddmFormFieldEvaluationResults == null) {
      ddmFormFieldEvaluationResults=new ArrayList<>();
      ddmFormFieldEvaluationResultsMap.put(ddmFormFieldEvaluationResult.getName(),ddmFormFieldEvaluationResults);
    }
    ddmFormFieldEvaluationResults.add(ddmFormFieldEvaluationResult);
  }
  return ddmFormFieldEvaluationResultsMap;
}
