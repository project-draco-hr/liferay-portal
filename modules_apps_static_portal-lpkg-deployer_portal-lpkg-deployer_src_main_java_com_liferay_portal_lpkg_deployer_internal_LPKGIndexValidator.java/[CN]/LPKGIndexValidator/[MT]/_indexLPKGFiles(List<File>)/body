{
  List<URI> uris=new ArrayList<>();
  UnsyncByteArrayOutputStream unsyncByteArrayOutputStream=new UnsyncByteArrayOutputStream();
  try {
    for (    File lpkgFile : lpkgFiles) {
      Indexer indexer=_indexerFactory.createLPKGIndexer(lpkgFile,_jarFileNames);
      indexer.index(unsyncByteArrayOutputStream);
      String name=lpkgFile.getName();
      URL url=_bytesURLProtocolSupport.putBytes(name.substring(0,name.length() - 5),unsyncByteArrayOutputStream.toByteArray());
      unsyncByteArrayOutputStream.reset();
      uris.add(url.toURI());
    }
  }
 catch (  Exception e) {
    _cleanUp(uris);
    throw e;
  }
  return uris;
}
