{
  Builder builder=new Builder();
  builder.setArguments(Arrays.asList("-Djava.awt.headless=true"));
  String classpath=ClassPathUtil.getGlobalClassPath();
  builder.setBootstrapClassPath(classpath);
  builder.setReactClassLoader(PortalClassLoaderUtil.getClassLoader());
  String runtimeClasspath=ClassPathUtil.buildClassPath(IndexerFactory.class,Bundle.class,TargetPlatformIndexerProcessCallable.class);
  builder.setRuntimeClassPath(runtimeClasspath.concat(File.pathSeparator).concat(classpath));
  _processConfig=builder.build();
}
