{
  Builder builder=new Builder();
  builder.setArguments(Arrays.asList("-Djava.awt.headless=true"));
  String classpath=ClassPathUtil.buildClassPath(IndexerFactory.class,Bundle.class,TargetPlatformIndexerProcessCallable.class);
  classpath=classpath.concat(File.pathSeparator).concat(ClassPathUtil.getGlobalClassPath());
  builder.setBootstrapClassPath(classpath);
  builder.setReactClassLoader(PortalClassLoaderUtil.getClassLoader());
  builder.setRuntimeClassPath(classpath);
  _processConfig=builder.build();
}
