{
  String url=StringPool.BLANK;
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    FileEntry fileEntry=ActionUtil.getFileEntry(portletRequest);
    url=PermissionsURLTag.doTag(null,DLFileEntryConstants.getClassName(),HtmlUtil.unescape(fileEntry.getTitle()),null,String.valueOf(fileEntry.getFileEntryId()),LiferayWindowState.POP_UP.toString(),null,themeDisplay.getRequest());
  }
 catch (  Exception e) {
  }
  return url;
}
