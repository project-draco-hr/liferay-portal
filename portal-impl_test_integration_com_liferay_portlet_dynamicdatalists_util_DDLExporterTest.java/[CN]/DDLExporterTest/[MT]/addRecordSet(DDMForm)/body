{
  String definition=DDMFormJSONSerializerUtil.serialize(ddmForm);
  DDMStructureTestHelper ddmStructureTestHelper=new DDMStructureTestHelper(_group);
  DDMStructure ddmStructure=ddmStructureTestHelper.addStructure(definition,StorageType.JSON.toString());
  DDLRecordSetTestHelper recordSetTestHelper=new DDLRecordSetTestHelper(_group);
  return recordSetTestHelper.addRecordSet(ddmStructure);
}
