{
  if (_content != null) {
    return _content;
  }
  String xmlUrl=XSLContentUtil.replaceUrlTokens(_themeDisplay,_xslContentPortletInstanceConfiguration.xmlUrl());
  String xslUrl=XSLContentUtil.replaceUrlTokens(_themeDisplay,_xslContentPortletInstanceConfiguration.xslUrl());
  _content=XSLContentUtil.transform(_xslContentConfiguration,new URL(xmlUrl),new URL(xslUrl));
  return _content;
}
