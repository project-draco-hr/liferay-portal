{
  if (_hasViewPermission != null) {
    return _hasViewPermission;
  }
  _hasViewPermission=true;
  if (_recordSet != null) {
    ThemeDisplay themeDisplay=getThemeDisplay();
    _hasViewPermission=DDLRecordSetPermission.contains(themeDisplay.getPermissionChecker(),_recordSet,ActionKeys.VIEW);
  }
  return _hasViewPermission;
}
