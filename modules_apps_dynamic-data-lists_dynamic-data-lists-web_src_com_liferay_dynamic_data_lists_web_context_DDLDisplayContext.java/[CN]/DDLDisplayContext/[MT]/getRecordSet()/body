{
  if (_recordSet != null) {
    return _recordSet;
  }
  RenderRequest renderRequest=_ddlRequestHelper.getRenderRequest();
  _recordSet=(DDLRecordSet)renderRequest.getAttribute(DDLWebKeys.DYNAMIC_DATA_LISTS_RECORD_SET);
  if (_recordSet != null) {
    return _recordSet;
  }
  _recordSet=DDLRecordSetLocalServiceUtil.fetchDDLRecordSet(getRecordSetId());
  return _recordSet;
}
