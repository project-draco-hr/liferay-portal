{
  long productVersionId=ParamUtil.getLong(req,"productVersionId",-1);
  SRProductVersion productVersion=null;
  SRProductEntry productEntry=null;
  if (productVersionId > 0) {
    productVersion=SRProductVersionServiceUtil.getProductVersion(productVersionId);
    productEntry=SRProductEntryServiceUtil.getProductEntry(productVersion.getProductEntryId());
  }
  req.setAttribute(WebKeys.SOFTWARE_REPOSITORY_PRODUCT_VERSION,productVersion);
  if (productEntry != null) {
    req.setAttribute(WebKeys.SOFTWARE_REPOSITORY_PRODUCT_ENTRY,productEntry);
  }
}
