{
  long frameworkVersionId=ParamUtil.getLong(req,"frameworkVersionId");
  SRFrameworkVersion frameworkVersion=null;
  if (frameworkVersionId > 0) {
    frameworkVersion=SRFrameworkVersionServiceUtil.getFrameworkVersion(frameworkVersionId);
  }
  req.setAttribute(WebKeys.SOFTWARE_REPOSITORY_FRAMEWORK_VERSION,frameworkVersion);
}
