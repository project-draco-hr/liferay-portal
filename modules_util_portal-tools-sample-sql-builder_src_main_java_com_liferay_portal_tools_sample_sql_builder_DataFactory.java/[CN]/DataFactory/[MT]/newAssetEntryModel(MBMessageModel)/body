{
  long classNameId=0;
  boolean visible=false;
  if (mbMessageModel.getCategoryId() == MBCategoryConstants.DISCUSSION_CATEGORY_ID) {
    classNameId=getClassNameId(MBDiscussion.class);
  }
 else {
    classNameId=getClassNameId(MBMessage.class);
    visible=true;
  }
  return newAssetEntryModel(mbMessageModel.getGroupId(),mbMessageModel.getCreateDate(),mbMessageModel.getModifiedDate(),classNameId,mbMessageModel.getMessageId(),mbMessageModel.getUuid(),0,true,visible,ContentTypes.TEXT_HTML,mbMessageModel.getSubject());
}
