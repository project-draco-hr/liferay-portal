{
  JournalContentSearchModel journalContentSearchModel=new JournalContentSearchModelImpl();
  journalContentSearchModel.setContentSearchId(_counter.get());
  journalContentSearchModel.setGroupId(journalArticleModel.getGroupId());
  journalContentSearchModel.setCompanyId(_companyId);
  journalContentSearchModel.setLayoutId(layoutId);
  journalContentSearchModel.setPortletId("com_liferay_journal_content_web_portlet_JournalContentPortlet");
  journalContentSearchModel.setArticleId(journalArticleModel.getArticleId());
  return journalContentSearchModel;
}
