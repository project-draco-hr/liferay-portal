{
  int type=JournalActivityKeys.UPDATE_ARTICLE;
  if (journalArticleModel.getVersion() == JournalArticleConstants.VERSION_DEFAULT) {
    type=JournalActivityKeys.ADD_ARTICLE;
  }
  return newSocialActivityModel(journalArticleModel.getGroupId(),getClassNameId(JournalArticle.class),journalArticleModel.getResourcePrimKey(),type,"{\"title\":\"" + journalArticleModel.getUrlTitle() + "\"}");
}
