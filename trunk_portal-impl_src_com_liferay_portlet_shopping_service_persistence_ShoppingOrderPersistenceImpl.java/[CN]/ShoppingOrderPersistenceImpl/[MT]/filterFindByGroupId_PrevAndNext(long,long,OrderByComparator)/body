{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByGroupId_PrevAndNext(orderId,groupId,orderByComparator);
  }
  ShoppingOrder shoppingOrder=findByPrimaryKey(orderId);
  Session session=null;
  try {
    session=openSession();
    ShoppingOrder[] array=new ShoppingOrderImpl[3];
    array[0]=filterGetByGroupId_PrevAndNext(session,shoppingOrder,groupId,orderByComparator,true);
    array[1]=shoppingOrder;
    array[2]=filterGetByGroupId_PrevAndNext(session,shoppingOrder,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
