{
  if (Validator.isNull(screenName) || Validator.isNumber(screenName) || Validator.isEmailAddress(screenName)|| (screenName.equalsIgnoreCase("cyrus"))|| (screenName.equalsIgnoreCase("postfix"))|| (screenName.indexOf(UserImpl.DEFAULT) != -1)|| (screenName.indexOf(companyId) != -1)|| (screenName.indexOf("_") != -1)|| (screenName.indexOf("/") != -1)) {
    return false;
  }
 else {
    return true;
  }
}
