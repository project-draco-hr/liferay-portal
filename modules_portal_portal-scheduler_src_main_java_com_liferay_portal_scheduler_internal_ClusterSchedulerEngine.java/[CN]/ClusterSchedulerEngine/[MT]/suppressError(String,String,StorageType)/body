{
  boolean memoryClusteredSlaveJob=isMemoryClusteredSlaveJob(storageType);
  if (!memoryClusteredSlaveJob) {
    _readLock.lock();
    try {
      _schedulerEngine.suppressError(jobName,groupName,storageType);
    }
  finally {
      _readLock.unlock();
    }
  }
  setClusterableThreadLocal(storageType);
}
