{
  Map<String,String[]> map=new HashMap<String,String[]>();
  String queryString=PortletParameterUtil.addNamespace("15","param1=value1&param2=value2&param3=value3");
  DynamicServletRequest.addQueryStringToParameterMap(map,queryString);
  Map<String,String[]> expected=createMap("p_p_id=15&_15_param1=value1&_15_param2=value2&_15_param3=value3");
  AssertUtils.assertEquals(expected,map);
}
