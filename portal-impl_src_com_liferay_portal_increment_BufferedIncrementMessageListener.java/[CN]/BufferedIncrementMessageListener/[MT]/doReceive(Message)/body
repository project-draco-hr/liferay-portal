{
  boolean isLazy=PropsValues.SOCIAL_EQUITY_EQUITY_VALUE_BUFFERED_INCREMENT_LAZILY;
  BatchablePipe<String,BufferedIncreasableEntry> pipe=(BatchablePipe<String,BufferedIncreasableEntry>)message.getPayload();
  BufferedIncreasableEntry entry=null;
  while ((entry=(BufferedIncreasableEntry)pipe.take()) != null) {
    try {
      entry.proceed();
    }
 catch (    Throwable t) {
      _log.error("Cannot write buffered increment value to the " + "database",t);
    }
    if (isLazy) {
      return;
    }
  }
}
