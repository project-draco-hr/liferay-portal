{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AssetTag.class,AssetTag.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("tagId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("tagId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
