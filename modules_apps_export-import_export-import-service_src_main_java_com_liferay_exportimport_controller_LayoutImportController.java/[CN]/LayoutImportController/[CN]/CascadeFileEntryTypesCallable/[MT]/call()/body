{
  _processedFolders=new HashSet<>();
  for (  Long newFolderId : _folderIdPairs.values()) {
    DLFolder newFolder=_dlFolderLocalService.fetchDLFolder(newFolderId);
    DLFolder rootFolder=getProcessableRootFolder(newFolder);
    if (Validator.isNotNull(rootFolder)) {
      _dlFileEntryTypeLocalService.cascadeFileEntryTypes(rootFolder.getUserId(),rootFolder);
    }
  }
  return null;
}
