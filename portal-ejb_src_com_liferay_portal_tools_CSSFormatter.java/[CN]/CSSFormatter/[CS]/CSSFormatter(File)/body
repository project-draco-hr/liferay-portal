{
  try {
    StringBuffer sb=new StringBuffer();
    String content=FileUtil.read(file);
    content=StringUtil.replace(content,new String[]{"*/\n","*/ /*","*/    /*"},new String[]{"*/\n\n","*/\n\n/*","*/\n\n/*"});
    FileUtil.write(file,content,true);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
