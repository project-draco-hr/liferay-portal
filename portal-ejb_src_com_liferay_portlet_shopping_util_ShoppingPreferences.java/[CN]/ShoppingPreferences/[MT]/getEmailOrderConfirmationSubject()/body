{
  String emailOrderConfirmationSubject=_prefs.getValue("email-order-confirmation-subject",StringPool.BLANK);
  if (Validator.isNotNull(emailOrderConfirmationSubject)) {
    return emailOrderConfirmationSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsUtil.SHOPPING_EMAIL_ORDER_CONFIRMATION_SUBJECT));
  }
}
