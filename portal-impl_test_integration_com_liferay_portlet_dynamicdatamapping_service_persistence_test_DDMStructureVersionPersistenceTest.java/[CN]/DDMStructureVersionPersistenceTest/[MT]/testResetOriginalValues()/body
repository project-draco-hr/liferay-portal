{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  DDMStructureVersion newDDMStructureVersion=addDDMStructureVersion();
  _persistence.clearCache();
  DDMStructureVersion existingDDMStructureVersion=_persistence.findByPrimaryKey(newDDMStructureVersion.getPrimaryKey());
  Assert.assertEquals(existingDDMStructureVersion.getStructureId(),ReflectionTestUtil.invoke(existingDDMStructureVersion,"getOriginalStructureId",new Class<?>[0]));
  Assert.assertTrue(Validator.equals(existingDDMStructureVersion.getVersion(),ReflectionTestUtil.invoke(existingDDMStructureVersion,"getOriginalVersion",new Class<?>[0])));
}
