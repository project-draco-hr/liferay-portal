{
  long companyId=WSRPUtil.getCompanyId();
  Portlet portlet=null;
  try {
    portlet=PortletLocalServiceUtil.getPortletById(companyId,portletHandle);
  }
 catch (  Exception e) {
    throw new WSRPException(ErrorCodes.LOAD_SERVICEDESCRIPTION_FAILED,e);
  }
  return _getPortletDescription(portlet,regContext,desiredLocales);
}
