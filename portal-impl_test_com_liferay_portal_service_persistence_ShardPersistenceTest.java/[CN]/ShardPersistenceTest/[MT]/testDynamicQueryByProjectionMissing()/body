{
  Shard newShard=addShard();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Shard.class,Shard.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("shardId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("shardId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
