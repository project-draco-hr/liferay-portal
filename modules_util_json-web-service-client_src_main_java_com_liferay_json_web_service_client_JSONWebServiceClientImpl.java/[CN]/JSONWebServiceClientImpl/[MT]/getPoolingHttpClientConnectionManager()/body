{
  PoolingHttpClientConnectionManager poolingHttpClientConnectionManager=null;
  if (_keyStore != null) {
    poolingHttpClientConnectionManager=new PoolingHttpClientConnectionManager(getSocketFactoryRegistry(),null,null,null,60000,TimeUnit.MILLISECONDS);
  }
 else {
    poolingHttpClientConnectionManager=new PoolingHttpClientConnectionManager(60000,TimeUnit.MILLISECONDS);
  }
  poolingHttpClientConnectionManager.setMaxTotal(20);
  return poolingHttpClientConnectionManager;
}
