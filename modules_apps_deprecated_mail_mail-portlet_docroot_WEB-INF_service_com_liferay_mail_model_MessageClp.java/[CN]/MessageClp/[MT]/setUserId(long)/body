{
  _userId=userId;
  if (_messageRemoteModel != null) {
    try {
      Class<?> clazz=_messageRemoteModel.getClass();
      Method method=clazz.getMethod("setUserId",long.class);
      method.invoke(_messageRemoteModel,userId);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
