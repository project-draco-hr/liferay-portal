{
  _size=size;
  if (_messageRemoteModel != null) {
    try {
      Class<?> clazz=_messageRemoteModel.getClass();
      Method method=clazz.getMethod("setSize",long.class);
      method.invoke(_messageRemoteModel,size);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
