{
  _cc=cc;
  if (_messageRemoteModel != null) {
    try {
      Class<?> clazz=_messageRemoteModel.getClass();
      Method method=clazz.getMethod("setCc",String.class);
      method.invoke(_messageRemoteModel,cc);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
