{
  if (!_started) {
    _started=true;
  }
  if (_deferredEhcaches.isEmpty()) {
    return;
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Loading deferred caches");
  }
  try {
    for (    String cacheManagerName : _deferredEhcaches.keySet()) {
      List<String> cacheNames=_deferredEhcaches.get(cacheManagerName);
      if (cacheNames.isEmpty()) {
        continue;
      }
      loadCachesFromCluster(cacheManagerName,cacheNames.toArray(new String[cacheNames.size()]));
    }
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to load cache data from the cluster",e);
    }
  }
 finally {
    _deferredEhcaches.clear();
  }
}
