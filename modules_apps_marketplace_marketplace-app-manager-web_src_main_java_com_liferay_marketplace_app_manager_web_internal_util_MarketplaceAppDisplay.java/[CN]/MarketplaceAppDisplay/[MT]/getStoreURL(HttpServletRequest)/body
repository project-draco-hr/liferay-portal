{
  try {
    PortletURL portletURL=PortletURLFactoryUtil.create(request,MarketplaceStorePortletKeys.MARKETPLACE_STORE,PortletRequest.RENDER_PHASE);
    portletURL.setParameter("appEntryId",String.valueOf(_app.getRemoteAppId()));
    portletURL.setWindowState(LiferayWindowState.MAXIMIZED);
    return portletURL.toString();
  }
 catch (  Exception e) {
  }
  return StringPool.BLANK;
}
