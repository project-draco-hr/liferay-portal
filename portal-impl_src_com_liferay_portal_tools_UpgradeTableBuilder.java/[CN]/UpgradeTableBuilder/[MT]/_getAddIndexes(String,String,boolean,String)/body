{
  List<String> addIndexes=new ArrayList<String>();
  File indexesFile=new File(indexesFileName);
  if (!indexesFile.exists() && !module) {
    indexesFile=new File("../sql/indexes.sql");
  }
 else {
    indexesFile=new File(_findModuleIndexFileName(moduleName));
  }
  try (UnsyncBufferedReader unsyncBufferedReader=new UnsyncBufferedReader(new InputStreamReader(new FileInputStream(indexesFile)))){
    String line=null;
    while ((line=unsyncBufferedReader.readLine()) != null) {
      if (line.contains(" on " + tableName + " (") || line.contains(" on " + tableName + "_ (")) {
        String sql=line.trim();
        if (sql.endsWith(";")) {
          sql=sql.substring(0,sql.length() - 1);
        }
        addIndexes.add(sql);
      }
    }
  }
   return addIndexes.toArray(new String[addIndexes.size()]);
}
