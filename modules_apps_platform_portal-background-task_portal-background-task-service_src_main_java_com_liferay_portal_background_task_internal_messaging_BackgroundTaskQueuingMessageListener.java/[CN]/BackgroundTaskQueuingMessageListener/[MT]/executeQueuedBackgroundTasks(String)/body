{
  if (_log.isDebugEnabled()) {
    _log.debug("Acquiring next queued background task for " + taskExecutorClassName);
  }
  BackgroundTask backgroundTask=_backgroundTaskManager.fetchFirstBackgroundTask(taskExecutorClassName,BackgroundTaskConstants.STATUS_QUEUED);
  if (backgroundTask == null) {
    if (_log.isDebugEnabled()) {
      _log.debug("No additional queued background tasks for " + taskExecutorClassName);
    }
    return;
  }
  _backgroundTaskManager.resumeBackgroundTask(backgroundTask.getBackgroundTaskId());
}
