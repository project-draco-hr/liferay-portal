{
  int value=DateUtil.compareTo(article1.getReviewDate(),article2.getReviewDate());
  if (value == 0) {
    if (article1.getVersion() < article2.getVersion()) {
      value=-1;
    }
 else     if (article1.getVersion() > article2.getVersion()) {
      value=1;
    }
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
