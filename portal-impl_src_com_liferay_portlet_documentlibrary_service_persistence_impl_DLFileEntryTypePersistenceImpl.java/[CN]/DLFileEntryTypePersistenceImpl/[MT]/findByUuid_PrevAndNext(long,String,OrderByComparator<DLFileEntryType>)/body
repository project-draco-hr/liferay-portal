{
  DLFileEntryType dlFileEntryType=findByPrimaryKey(fileEntryTypeId);
  Session session=null;
  try {
    session=openSession();
    DLFileEntryType[] array=new DLFileEntryTypeImpl[3];
    array[0]=getByUuid_PrevAndNext(session,dlFileEntryType,uuid,orderByComparator,true);
    array[1]=dlFileEntryType;
    array[2]=getByUuid_PrevAndNext(session,dlFileEntryType,uuid,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
