{
  DLFileEntryType dlFileEntryType=fetchByPrimaryKey(pk);
  if (dlFileEntryType == null) {
    dlFileEntryTypeToDLFolderTableMapper.addTableMapping(companyProvider.getCompanyId(),pk,dlFolder.getPrimaryKey());
  }
 else {
    dlFileEntryTypeToDLFolderTableMapper.addTableMapping(dlFileEntryType.getCompanyId(),pk,dlFolder.getPrimaryKey());
  }
}
