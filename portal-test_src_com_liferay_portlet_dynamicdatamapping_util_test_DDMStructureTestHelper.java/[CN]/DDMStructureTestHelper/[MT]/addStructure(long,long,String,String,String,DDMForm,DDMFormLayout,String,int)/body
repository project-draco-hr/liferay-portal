{
  return DDMStructureLocalServiceUtil.addStructure(TestPropsValues.getUserId(),_group.getGroupId(),parentStructureId,classNameId,structureKey,LocaleTestUtil.getDefaultLocaleMap(name),LocaleTestUtil.getDefaultLocaleMap(description),ddmForm,ddmFormLayout,storageType,type,ServiceContextTestUtil.getServiceContext(_group.getGroupId()));
}
