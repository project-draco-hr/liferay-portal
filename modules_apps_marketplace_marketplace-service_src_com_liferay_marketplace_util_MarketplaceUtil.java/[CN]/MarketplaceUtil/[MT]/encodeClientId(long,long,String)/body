{
  if (Validator.isNull(token)) {
    return StringPool.BLANK;
  }
  String clientId=ExpandoValueLocalServiceUtil.getData(companyId,User.class.getName(),"MP","clientId",userId,"default-client-id");
  byte[] encodedClientIdBytes=xor(clientId.getBytes(),token.getBytes());
  return UnicodeFormatter.bytesToHex(encodedClientIdBytes);
}
