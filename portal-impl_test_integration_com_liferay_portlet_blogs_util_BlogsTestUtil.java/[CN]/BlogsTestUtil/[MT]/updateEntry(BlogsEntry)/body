{
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext(blogsEntry.getGroupId());
  serviceContext.setCommand(Constants.UPDATE);
  serviceContext.setLayoutFullURL("http://localhost");
  return BlogsEntryLocalServiceUtil.updateEntry(blogsEntry.getUserId(),blogsEntry.getEntryId(),ServiceTestUtil.randomString(),blogsEntry.getDescription(),blogsEntry.getContent(),1,1,2012,12,00,true,true,new String[0],blogsEntry.getSmallImage(),blogsEntry.getSmallImageURL(),StringPool.BLANK,null,serviceContext);
}
