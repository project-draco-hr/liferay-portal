{
  if (_queryConfig != null) {
    return _queryConfig;
  }
  _queryConfig=new QueryConfig();
  _queryConfig.setCollatedSpellCheckResultEnabled(isCollatedSpellCheckResultEnabled());
  _queryConfig.setCollatedSpellCheckResultScoresThreshold(getCollatedSpellCheckResultDisplayThreshold());
  _queryConfig.setQueryIndexingEnabled(isQueryIndexingEnabled());
  _queryConfig.setQueryIndexingThreshold(getQueryIndexingThreshold());
  _queryConfig.setQuerySuggestionEnabled(isQuerySuggestionsEnabled());
  _queryConfig.setQuerySuggestionScoresThreshold(getQuerySuggestionsDisplayThreshold());
  _queryConfig.setQuerySuggestionsMax(getQuerySuggestionsMax());
  return _queryConfig;
}
