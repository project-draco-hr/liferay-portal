{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Control Panel");
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Documents and Media",RuntimeVariables.replace("Documents and Media"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isChecked("//input[@id='_20_allRowIdsCheckbox']"));
  selenium.clickAt("//input[@id='_20_allRowIdsCheckbox']",RuntimeVariables.replace("All Rows"));
  assertTrue(selenium.isChecked("//input[@id='_20_allRowIdsCheckbox']"));
  assertFalse(selenium.isVisible("link=Actions"));
  assertFalse(selenium.isVisible("link=Delete"));
}
