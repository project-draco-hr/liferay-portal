{
  if (session == null) {
    return null;
  }
  try {
    Map<String,Object> map=(Map<String,Object>)session.getAttribute(key);
    if ((map == null) && createIfAbsent) {
      map=new SessionMessagesMap();
      session.setAttribute(key,map);
    }
    return map;
  }
 catch (  IllegalStateException ise) {
    return null;
  }
}
