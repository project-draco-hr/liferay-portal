{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      selenium.selectWindow("null");
    selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
selenium.clickAt("link=Directory Test Page",RuntimeVariables.replace("Directory Test Page"));
selenium.waitForPageToLoad("30000");
selenium.clickAt("link=Organizations",RuntimeVariables.replace("Organizations"));
selenium.waitForPageToLoad("30000");
boolean basicVisible=selenium.isVisible("//a[.='\u00ab Basic']");
if (!basicVisible) {
label=2;
continue;
}
selenium.clickAt("//a[.='\u00ab Basic']",RuntimeVariables.replace("\u00ab Basic"));
case 2:
selenium.type("//input[@name='_11_keywords']",RuntimeVariables.replace("Test Organization"));
selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace("Search"));
selenium.waitForPageToLoad("30000");
selenium.type("//input[@name='_11_keywords']",RuntimeVariables.replace(""));
assertEquals(RuntimeVariables.replace("Test Organization"),selenium.getText("//tr[3]/td[1]/a"));
selenium.type("//input[@name='_11_keywords']",RuntimeVariables.replace("Test1 Organization1"));
selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace("Search"));
selenium.waitForPageToLoad("30000");
selenium.type("//input[@name='_11_keywords']",RuntimeVariables.replace(""));
assertFalse(selenium.isTextPresent("Test Organization"));
case 100:
label=-1;
}
}
}
