{
  OutputStreamContainerFactory outputStreamContainerFactory=_outputStreamContainerFactories.getService(outputStreamContainerName);
  if (outputStreamContainerFactory == null) {
    runWithSwappedLog(runnable,outputStreamHint);
    return;
  }
  OutputStreamContainer outputStreamContainer=outputStreamContainerFactory.create(outputStreamHint);
  runWithSwappedLog(runnable,outputStreamContainer.getDescription(),outputStreamContainer.getOutputStream());
}
