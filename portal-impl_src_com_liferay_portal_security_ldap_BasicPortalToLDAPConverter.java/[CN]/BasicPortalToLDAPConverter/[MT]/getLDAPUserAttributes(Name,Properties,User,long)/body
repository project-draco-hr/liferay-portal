{
  Attributes attrs=new BasicAttributes(true);
  Attribute objectClass=new BasicAttribute(_USER_OBJECT_CLASS);
  String postfix=LDAPSettingsUtil.getPropertyPostfix(ldapServerId);
  String[] defaultObjectClasses=PrefsPropsUtil.getStringArray(user.getCompanyId(),PropsKeys.LDAP_USER_DEFAULT_OBJECT_CLASSES + postfix,StringPool.COMMA);
  for (int i=0; i < defaultObjectClasses.length; i++) {
    objectClass.add(defaultObjectClasses[i]);
  }
  attrs.put(objectClass);
  addAttributeMapping(userMappings.getProperty(LDAPConverterKeys.USER_FIRST_NAME),user.getFirstName(),attrs);
  addAttributeMapping(userMappings.getProperty(LDAPConverterKeys.USER_LAST_NAME),user.getLastName(),attrs);
  addAttributeMapping(userMappings.getProperty(LDAPConverterKeys.USER_PASSWORD),user.getPasswordUnencrypted(),attrs);
  addAttributeMapping(userMappings.getProperty(LDAPConverterKeys.USER_EMAIL_ADDRESS),user.getEmailAddress(),attrs);
  return attrs;
}
