{
  Modifications modifications=getModifications(user,userMappings,_reservedUserFields);
  if (user.isPasswordModified() && Validator.isNotNull(user.getPasswordUnencrypted())) {
    addModificationItem(userMappings.getProperty(LDAPConverterKeys.USER_PASSWORD),user.getPasswordUnencrypted(),modifications);
  }
  return modifications;
}
