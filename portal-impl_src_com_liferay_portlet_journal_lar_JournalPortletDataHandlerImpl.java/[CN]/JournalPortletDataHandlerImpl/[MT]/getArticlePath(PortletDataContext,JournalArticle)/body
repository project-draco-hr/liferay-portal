{
  StringMaker sm=new StringMaker();
  sm.append(context.getPortletPath(PortletKeys.JOURNAL));
  sm.append("/articles/");
  sm.append(article.getArticleId());
  sm.append(StringPool.SLASH);
  sm.append(article.getVersion());
  sm.append(StringPool.SLASH);
  sm.append(article.getArticleId());
  sm.append(".xml");
  return sm.toString();
}
