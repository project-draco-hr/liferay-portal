{
  StringBundler sb=new StringBundler(8);
  sb.append(context.getPortletPath(PortletKeys.JOURNAL));
  sb.append("/articles/");
  sb.append(article.getArticleId());
  sb.append(StringPool.SLASH);
  sb.append(article.getVersion());
  sb.append(StringPool.SLASH);
  sb.append(article.getArticleId());
  sb.append(".xml");
  return sb.toString();
}
