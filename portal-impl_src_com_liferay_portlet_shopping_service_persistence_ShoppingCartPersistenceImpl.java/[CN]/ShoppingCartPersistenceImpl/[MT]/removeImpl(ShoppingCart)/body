{
  shoppingCart=toUnwrappedModel(shoppingCart);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,shoppingCart);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(shoppingCart);
  return shoppingCart;
}
