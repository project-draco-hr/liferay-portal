{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,shoppingCart,merge);
    shoppingCart.setNew(false);
    return shoppingCart;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ShoppingCart.class.getName());
  }
}
