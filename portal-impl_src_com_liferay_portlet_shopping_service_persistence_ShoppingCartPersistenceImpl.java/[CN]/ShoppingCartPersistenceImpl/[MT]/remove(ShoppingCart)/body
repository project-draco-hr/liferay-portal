{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(shoppingCart);
    }
  }
  shoppingCart=removeImpl(shoppingCart);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(shoppingCart);
    }
  }
  return shoppingCart;
}
