{
  ShoppingCart shoppingCart=(ShoppingCart)EntityCacheUtil.getResult(ShoppingCartModelImpl.ENTITY_CACHE_ENABLED,ShoppingCartImpl.class,cartId);
  if (shoppingCart == _nullShoppingCart) {
    return null;
  }
  if (shoppingCart == null) {
    Session session=null;
    try {
      session=openSession();
      shoppingCart=(ShoppingCart)session.get(ShoppingCartImpl.class,Long.valueOf(cartId));
      if (shoppingCart != null) {
        cacheResult(shoppingCart);
      }
 else {
        EntityCacheUtil.putResult(ShoppingCartModelImpl.ENTITY_CACHE_ENABLED,ShoppingCartImpl.class,cartId,_nullShoppingCart);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(ShoppingCartModelImpl.ENTITY_CACHE_ENABLED,ShoppingCartImpl.class,cartId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return shoppingCart;
}
