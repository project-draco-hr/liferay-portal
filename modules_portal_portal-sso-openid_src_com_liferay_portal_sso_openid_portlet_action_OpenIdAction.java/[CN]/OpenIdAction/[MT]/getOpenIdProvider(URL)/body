{
  String hostName=endpointURL.getHost();
  String[] openIdProviders=StringUtil.split(_openIdConfiguration.openIdProviders());
  for (  String openIdProvider : openIdProviders) {
    String openIdURLString=PropsUtil.get(PropsKeys.OPEN_ID_URL,new Filter(openIdProvider));
    if (hostName.equals(openIdURLString)) {
      return openIdProvider;
    }
  }
  return _OPEN_ID_PROVIDER_DEFAULT;
}
