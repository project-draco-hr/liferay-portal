{
  JSONWebServiceConfigurator jsonWebServiceConfigurator=new JSONWebServiceConfigurator();
  jsonWebServiceConfigurator.setJSONWebServiceActionsManager(JSONWebServiceActionsManagerUtil.getJSONWebServiceActionsManager());
  try {
    jsonWebServiceConfigurator.configure(classLoader);
  }
 catch (  Exception e) {
