{
  try {
    JSONWebServiceAction jsonWebServiceAction=JSONWebServiceActionsManagerUtil.getJSONWebServiceAction(request);
    JSONWebServiceActionMapping jsonWebServiceActionMapping=jsonWebServiceAction.getJSONWebServiceActionMapping();
    Method actionMethod=jsonWebServiceActionMapping.getActionMethod();
    checkMethodPublicAccess(request,actionMethod.getName(),PropsValues.JSONWS_WEB_SERVICE_PUBLIC_METHODS);
    Object returnObj=jsonWebServiceAction.invoke();
    if (returnObj != null) {
      return getReturnValue(returnObj);
    }
 else {
      return JSONFactoryUtil.getNullJSON();
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
    return JSONFactoryUtil.serializeException(e);
  }
}
