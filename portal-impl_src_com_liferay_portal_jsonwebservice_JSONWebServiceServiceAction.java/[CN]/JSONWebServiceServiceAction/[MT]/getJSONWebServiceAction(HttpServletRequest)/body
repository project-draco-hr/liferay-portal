{
  String path=GetterUtil.getString(request.getPathInfo());
  if (path.equals("/invoke")) {
    return new JSONWebServiceInvokerAction(request);
  }
  if (PropsValues.JSONWS_WEB_SERVICE_API_INFO_ENABLED && (request.getParameter("discover") != null)) {
    return new JSONWebServiceDiscoverAction(request);
  }
  return JSONWebServiceActionsManagerUtil.getJSONWebServiceAction(request);
}
