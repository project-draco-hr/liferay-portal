{
  JSONWebServiceConfigurator configurator=null;
  try {
    List<JSONWebServiceConfigurator> configurators=ServiceLoader.load(classLoader,JSONWebServiceConfigurator.class);
    if (!configurators.isEmpty()) {
      configurator=configurators.get(0);
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  if (configurator == null) {
    configurator=new JSONWebServiceConfiguratorImpl();
  }
  return configurator;
}
