{
  long folderId=ParamUtil.getLong(req,"folderId");
  String name=ParamUtil.getString(req,"name");
  double version=ParamUtil.getDouble(req,"version");
  long fileShortcutId=ParamUtil.getLong(req,"fileShortcutId");
  ThemeDisplay themeDisplay=(ThemeDisplay)req.getAttribute(WebKeys.THEME_DISPLAY);
  HttpServletRequest httpReq=((ActionRequestImpl)req).getHttpServletRequest();
  HttpServletResponse httpRes=((ActionResponseImpl)res).getHttpServletResponse();
  getFile(folderId,name,version,fileShortcutId,themeDisplay,httpReq,httpRes);
  setForward(req,ActionConstants.COMMON_NULL);
}
