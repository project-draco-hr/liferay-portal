{
  StringBundler sb=new StringBundler();
  sb.append("test1");
  sb.append("test2");
  sb.append("test3");
  sb.append("test4");
  sb.append("test5");
  StringWriter stringWriter=new StringWriter();
  sb.writeTo(stringWriter);
  Assert.assertEquals("test1test2test3test4test5",stringWriter.toString());
}
