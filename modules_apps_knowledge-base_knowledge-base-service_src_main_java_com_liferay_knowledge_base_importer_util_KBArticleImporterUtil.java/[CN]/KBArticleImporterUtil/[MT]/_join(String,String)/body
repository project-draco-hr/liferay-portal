{
  if ((strings == null) || (strings.length == 0)) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(strings.length * 2 - 1);
  sb.append(strings[0]);
  for (int i=1; i < strings.length; i++) {
    String previous=strings[i - 1];
    if (!previous.endsWith(separator)) {
      sb.append(separator);
    }
    sb.append(strings[i]);
  }
  return sb.toString();
}
