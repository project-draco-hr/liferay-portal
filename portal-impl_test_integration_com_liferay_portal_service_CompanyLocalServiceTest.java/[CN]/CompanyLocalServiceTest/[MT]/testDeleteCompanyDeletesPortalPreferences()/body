{
  String webId="webId";
  final Company company=CompanyLocalServiceUtil.addCompany(webId,webId,webId + ".com",PropsValues.SHARD_DEFAULT_NAME,false,0,true);
  PortalInstances.initCompany(_mockServletContext,webId);
