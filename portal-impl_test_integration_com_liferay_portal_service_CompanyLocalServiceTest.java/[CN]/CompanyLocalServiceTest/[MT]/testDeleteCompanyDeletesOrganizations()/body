{
  String webId="webId";
  Company company=CompanyLocalServiceUtil.addCompany(webId,webId,webId + ".com",PropsValues.SHARD_DEFAULT_NAME,false,0,true);
  PortalInstances.initCompany(_mockServletContext,webId);
  CompanyLocalServiceUtil.deleteCompany(company);
  int organizationsCount=OrganizationLocalServiceUtil.getOrganizationsCount(company.getCompanyId(),OrganizationConstants.DEFAULT_PARENT_ORGANIZATION_ID);
  Assert.assertEquals(0,organizationsCount);
}
