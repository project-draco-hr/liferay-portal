{
  Company company=addCompany();
  CompanyLocalServiceUtil.deleteCompany(company);
  int searchCount=LayoutPrototypeLocalServiceUtil.searchCount(company.getCompanyId(),true);
  Assert.assertEquals(0,searchCount);
  searchCount=LayoutPrototypeLocalServiceUtil.searchCount(company.getCompanyId(),false);
  Assert.assertEquals(0,searchCount);
}
