{
  Session session=null;
  try {
    session=openSession();
    ABContactHBM abContactHBM=(ABContactHBM)session.get(ABContactHBM.class,contactId);
    if (abContactHBM == null) {
      _log.warn("No ABContact exists with the primary key " + contactId.toString());
      throw new NoSuchContactException("No ABContact exists with the primary key " + contactId.toString());
    }
    com.liferay.portlet.addressbook.model.ABContact abContact=ABContactHBMUtil.model(abContactHBM);
    session.delete(abContactHBM);
    session.flush();
    ABContactPool.remove(contactId);
    return abContact;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
