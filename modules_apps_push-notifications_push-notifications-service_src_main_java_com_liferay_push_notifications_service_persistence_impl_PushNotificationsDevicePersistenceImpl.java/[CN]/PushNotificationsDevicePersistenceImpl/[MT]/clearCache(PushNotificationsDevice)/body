{
  entityCache.removeResult(PushNotificationsDeviceModelImpl.ENTITY_CACHE_ENABLED,PushNotificationsDeviceImpl.class,pushNotificationsDevice.getPrimaryKey());
  finderCache.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  finderCache.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  clearUniqueFindersCache((PushNotificationsDeviceModelImpl)pushNotificationsDevice);
}
