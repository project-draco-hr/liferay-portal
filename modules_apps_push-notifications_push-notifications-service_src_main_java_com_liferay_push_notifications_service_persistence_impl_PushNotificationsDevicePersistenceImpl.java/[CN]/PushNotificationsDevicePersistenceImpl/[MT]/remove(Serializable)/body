{
  Session session=null;
  try {
    session=openSession();
    PushNotificationsDevice pushNotificationsDevice=(PushNotificationsDevice)session.get(PushNotificationsDeviceImpl.class,primaryKey);
    if (pushNotificationsDevice == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchDeviceException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(pushNotificationsDevice);
  }
 catch (  NoSuchDeviceException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
