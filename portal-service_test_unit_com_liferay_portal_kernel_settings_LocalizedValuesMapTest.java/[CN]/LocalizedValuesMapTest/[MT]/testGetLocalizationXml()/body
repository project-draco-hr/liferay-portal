{
  String xml=_localizedValuesMap.getLocalizationXml(_KEY,_DEFAULT_LOCALE,_locales);
  Assert.assertEquals("<?xml version='1.0' encoding='UTF-8'?>" + "<root available-locales=\"es_ES,en_GB,en_US,pt_BR\" " + "default-locale=\"es_ES\">"+ "<key language-id=\"es_ES\">value es_ES</key>"+ "<key language-id=\"en_GB\">value en_GB</key>"+ "<key language-id=\"en_US\">value en_US</key>"+ "<key language-id=\"pt_BR\">defaultValue</key>"+ "</root>",xml);
}
