{
  LocalizedValuesMap localizedValuesMap=new LocalizedValuesMap(_KEY,_DEFAULT_LOCALE,_AVAILABLE_LOCALES);
  for (  Locale locale : _AVAILABLE_LOCALES) {
    localizedValuesMap.put(locale,"value " + locale.toString());
  }
  String xml=localizedValuesMap.getLocalizationXml();
  Assert.assertEquals("<?xml version='1.0' encoding='UTF-8'?>" + "<root available-locales=\"es_ES,en_GB,en_US\" " + "default-locale=\"es_ES\">"+ "<key language-id=\"es_ES\">value es_ES</key>"+ "<key language-id=\"en_GB\">value en_GB</key>"+ "<key language-id=\"en_US\">value en_US</key>"+ "</root>",xml);
}
