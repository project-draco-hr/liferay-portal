{
  SyncFile syncFile=SyncFileService.fetchSyncFile(filePathName);
  if (syncFile == null) {
    return addSyncFile(null,null,null,filePathName,null,null,0,0,SyncFile.STATE_UNSYNCED,0,null);
  }
  syncFile.setState(SyncFile.STATE_UNSYNCED);
  update(syncFile);
  unsyncChildSyncFiles(syncFile);
  return syncFile;
}
