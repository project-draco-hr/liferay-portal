{
  if (OSDetector.isWindows()) {
    Path filePath=Paths.get(syncFile.getFilePathName());
    FileUtil.writeFileKey(filePath,String.valueOf(syncFile.getSyncFileId()));
    syncFile.setFileKey(String.valueOf(syncFile.getSyncFileId()));
  }
 else {
    syncFile.setFileKey(FileUtil.getFileKey(syncFile.getFilePathName()));
  }
  return update(syncFile);
}
