{
  DDMContent newDDMContent=addDDMContent();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMContent.class,DDMContent.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("contentId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("contentId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
