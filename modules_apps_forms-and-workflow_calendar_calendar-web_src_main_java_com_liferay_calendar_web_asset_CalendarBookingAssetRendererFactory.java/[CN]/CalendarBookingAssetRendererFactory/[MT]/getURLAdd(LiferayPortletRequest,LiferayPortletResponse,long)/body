{
  ThemeDisplay themeDisplay=(ThemeDisplay)liferayPortletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  CalendarResource calendarResource=CalendarResourceUtil.getScopeGroupCalendarResource(liferayPortletRequest,themeDisplay.getScopeGroupId());
  if (calendarResource == null) {
    return null;
  }
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(liferayPortletRequest,(Group)liferayPortletRequest.getAttribute(WebKeys.GROUP),CalendarPortletKeys.CALENDAR,0,0,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/edit_calendar_booking.jsp");
  Calendar calendar=calendarResource.getDefaultCalendar();
  portletURL.setParameter("calendarId",String.valueOf(calendar.getCalendarId()));
  return portletURL;
}
