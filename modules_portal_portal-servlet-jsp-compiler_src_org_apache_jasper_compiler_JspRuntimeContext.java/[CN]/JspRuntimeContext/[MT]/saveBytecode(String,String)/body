{
  byte[] bytecode=getBytecode(className);
  if (bytecode != null) {
    try {
      FileOutputStream fos=new FileOutputStream(classFileName);
      fos.write(bytecode);
      fos.close();
    }
 catch (    IOException ex) {
      context.log("Error in saving bytecode for " + className + " to "+ classFileName,ex);
    }
  }
}
