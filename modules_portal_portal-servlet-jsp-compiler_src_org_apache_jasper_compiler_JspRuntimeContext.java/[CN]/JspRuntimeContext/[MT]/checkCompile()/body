{
  for (  JspServletWrapper jsw : jsps.values()) {
    if (jsw.isTagFile()) {
      continue;
    }
    JspCompilationContext ctxt=jsw.getJspEngineContext();
synchronized (jsw) {
      try {
        ctxt.compile();
      }
 catch (      FileNotFoundException ex) {
        ctxt.incrementRemoved();
      }
catch (      Throwable t) {
        jsw.getServletContext().log("Background compile failed",t);
      }
    }
  }
}
