{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMDataProviderInstance.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("dataProviderInstanceId",RandomTestUtil.nextLong()));
  List<DDMDataProviderInstance> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
