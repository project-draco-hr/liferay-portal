{
  SoftReference<T> softReference=_softReferenceThreadLocal.get();
  if (softReference == _nullPlaceHolder) {
    return null;
  }
  T value=null;
  if (softReference != null) {
    value=softReference.get();
  }
  if (value == null) {
    value=initialValue();
    set(value);
  }
  return value;
}
