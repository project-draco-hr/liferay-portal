{
  if (_workflowTaskAssignees == null) {
    return false;
  }
  if (_workflowTaskAssignees.size() != 1) {
    return false;
  }
  WorkflowTaskAssignee workflowTaskAssignee=_workflowTaskAssignees.get(0);
  if (User.class.getName().equals(workflowTaskAssignee.getAssigneeClassName())) {
    return true;
  }
 else {
    return false;
  }
}
