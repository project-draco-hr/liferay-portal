{
  try {
    Class clazz=Class.forName("net.sf.ehcache.hibernate.EhCacheProvider");
    Field filed=clazz.getDeclaredField("manager");
    filed.setAccessible(true);
    CacheManager cacheManager=(CacheManager)filed.get(_CACHE_PRIVIDER);
    if (cacheManager == null) {
      throw new SystemException("Underline CacheManger has been initialized yet, " + "make sure you are not calling this method too early.");
    }
    return cacheManager;
  }
 catch (  Exception ex) {
    throw new SystemException(ex);
  }
}
