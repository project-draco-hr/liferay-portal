{
  try {
    MBMessageDisplay messageDisplay=ActionUtil.getMessageDisplay(portletRequest);
    MBCategory category=messageDisplay.getCategory();
    ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
    if (MBCategoryPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),getCategoryId(category),ActionKeys.MOVE_THREAD)) {
      return true;
    }
  }
 catch (  PortalException pe) {
  }
  return false;
}
