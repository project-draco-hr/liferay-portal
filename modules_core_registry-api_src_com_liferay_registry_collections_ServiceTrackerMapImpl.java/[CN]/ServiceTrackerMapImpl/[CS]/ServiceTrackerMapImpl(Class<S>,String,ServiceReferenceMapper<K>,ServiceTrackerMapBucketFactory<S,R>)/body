{
  _serviceReferenceMapper=serviceReferenceMapper;
  _bucketFactory=serviceTrackerMapBucketFactory;
  Registry registry=RegistryUtil.getRegistry();
  filterString="(&(objectClass=" + clazz.getName() + ")"+ filterString+ ")";
  _serviceTracker=registry.trackServices(registry.getFilter(filterString),new MapServiceTrackerCustomizer());
