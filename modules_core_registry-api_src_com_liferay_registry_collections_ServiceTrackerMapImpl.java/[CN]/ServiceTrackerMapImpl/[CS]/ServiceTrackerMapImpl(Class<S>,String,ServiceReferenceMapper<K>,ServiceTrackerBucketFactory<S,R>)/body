{
  _serviceReferenceMapper=serviceReferenceMapper;
  _serviceTrackerMapBucketFactory=serviceTrackerMapBucketFactory;
  Registry registry=RegistryUtil.getRegistry();
  _serviceTracker=registry.trackServices(registry.getFilter("(&(objectClass=" + clazz.getName() + ")"+ filterString+ ")"),new MapServiceTrackerCustomizer());
