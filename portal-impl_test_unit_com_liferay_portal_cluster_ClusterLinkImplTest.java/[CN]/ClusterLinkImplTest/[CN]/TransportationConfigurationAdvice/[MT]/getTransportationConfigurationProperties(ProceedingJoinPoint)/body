{
  Object[] arguments=proceedingJoinPoint.getArgs();
  if (PropsKeys.CLUSTER_LINK_CHANNEL_PROPERTIES_TRANSPORT.equals(arguments[0]) && Boolean.TRUE.equals(arguments[1])) {
    Properties properties=new Properties();
    for (int i=0; i < _CHANNEL_COUNT; i++) {
      properties.put(PropsKeys.CLUSTER_LINK_CHANNEL_PROPERTIES_TRANSPORT + CharPool.POUND + i,"udp.xml");
    }
    return properties;
  }
  return proceedingJoinPoint.proceed();
}
