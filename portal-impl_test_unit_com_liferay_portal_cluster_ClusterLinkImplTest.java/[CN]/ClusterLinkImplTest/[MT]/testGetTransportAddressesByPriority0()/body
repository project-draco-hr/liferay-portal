{
  Callable<Void> callable=new Callable<Void>(){
    public Void call() throws Exception {
      ObjectValuePair<ClusterLinkImpl,JChannel[]> clusterPlatform=_getClusterLinkPlatform(1,false);
      List<Address> addresses=null;
      try {
        addresses=clusterPlatform.getKey().getTransportAddresses(Priority.LEVEL1);
      }
 catch (      Exception e) {
        fail();
      }
      assertEquals(true,addresses.isEmpty());
      return null;
    }
  }
;
  runInNewClassLoader((Class<? extends Callable<Void>>)callable.getClass());
}
