{
  Callable<Void> callable=new Callable<Void>(){
    public Void call() throws Exception {
      ClusterLinkImpl clusterLinkImpl=_getClusterLinkImpl(1,false);
      List<Address> addresses=clusterLinkImpl.getTransportAddresses(Priority.LEVEL1);
      assertTrue(addresses.isEmpty());
      clusterLinkImpl.destroy();
      return null;
    }
  }
;
  runInNewClassLoader((Class<? extends Callable<Void>>)callable.getClass());
}
