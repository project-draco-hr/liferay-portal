{
  for (  String bundleClassPathFile : StringUtil.split(PropsUtil.get("bundle.import.files"),CharPool.SEMICOLON)) {
    String[] coordinates=StringUtil.split(bundleClassPathFile,CharPool.COMMA);
    if (coordinates.length != 3) {
      throw new IllegalArgumentException("Malformed maven coordinates " + bundleClassPathFile);
    }
    manifestAssetBuilder.addImports(listPackagesInJarFile(coordinates[0],coordinates[1],coordinates[2]));
  }
}
