{
  String[] bundleImportFileNames=StringUtil.split(PropsUtil.get("bundle.import.files"),CharPool.SEMICOLON);
  for (  String bundleImportFileName : bundleImportFileNames) {
    String[] coordinates=StringUtil.split(bundleImportFileName,CharPool.COMMA);
    if (coordinates.length != 3) {
      throw new IllegalArgumentException("Malformed Maven coordinates " + bundleImportFileName);
    }
    manifestAssetBuilder.addImports(getJarPackageNames(coordinates[0],coordinates[1],coordinates[2]));
  }
}
