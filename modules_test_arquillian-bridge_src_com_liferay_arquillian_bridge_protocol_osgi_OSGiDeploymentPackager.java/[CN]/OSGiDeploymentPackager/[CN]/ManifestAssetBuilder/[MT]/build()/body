{
  _importPackages.removeAll(Arrays.asList(StringUtil.split(PropsUtil.get("bundle.import.exclude.packages"),CharPool.SEMICOLON)));
  _osgiManifestBuilder.addBundleActivator(ArquillianBundleActivator.class);
  _osgiManifestBuilder.addBundleClasspath(StringUtil.merge(_classPathFileNames));
  _osgiManifestBuilder.addExportPackages(_exportPackages.toArray(new String[_exportPackages.size()]));
  _osgiManifestBuilder.addImportPackages(_importPackages.toArray(new String[_importPackages.size()]));
  return new ByteArrayAsset(_osgiManifestBuilder.openStream());
}
