{
  Attributes mainAttributes=manifest.getMainAttributes();
  mainAttributes.remove(Name.MANIFEST_VERSION);
  String bundleActivator=(String)mainAttributes.remove(new Name("Bundle-Activator"));
  if (bundleActivator != null) {
    _bundleActivators.add(bundleActivator);
  }
  String importPackage=(String)mainAttributes.remove(new Name("Import-Package"));
  if (importPackage != null) {
    _imports.addAll(Arrays.asList(StringUtil.split(importPackage)));
  }
  String exportPackage=(String)mainAttributes.remove(new Name("Export-Package"));
  if (exportPackage != null) {
    _exports.addAll(Arrays.asList(StringUtil.split(exportPackage)));
  }
  for (  Map.Entry<Object,Object> entry : mainAttributes.entrySet()) {
    _osgiManifestBuilder.addManifestHeader(String.valueOf(entry.getKey()),String.valueOf(entry.getValue()));
  }
}
