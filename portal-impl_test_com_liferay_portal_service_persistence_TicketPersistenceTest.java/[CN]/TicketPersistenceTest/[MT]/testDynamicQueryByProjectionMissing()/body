{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Ticket.class,Ticket.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("ticketId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("ticketId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
