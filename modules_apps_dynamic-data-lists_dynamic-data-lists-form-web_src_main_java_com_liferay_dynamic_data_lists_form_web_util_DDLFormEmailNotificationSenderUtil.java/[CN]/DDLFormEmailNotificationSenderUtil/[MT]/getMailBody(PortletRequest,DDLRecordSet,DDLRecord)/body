{
  Class<?> clazz=DDLFormEmailNotificationSenderUtil.class;
  String notificationTemplate=ContentUtil.get(clazz.getClassLoader(),DDLFormWebConfigurationValues.DDL_FORM_WEB_EMAIL_FORM_ENTRY_NOTIFICATION_BODY);
  Map<String,Serializable> context=getContext(portletRequest,recordSet,record);
  return replaceTokens(notificationTemplate,context);
}
