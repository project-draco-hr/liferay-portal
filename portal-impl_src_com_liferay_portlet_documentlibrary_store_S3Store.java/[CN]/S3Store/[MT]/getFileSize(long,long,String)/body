{
  try {
    String versionLabel=getHeadVersionLabel(companyId,repositoryId,fileName);
    StorageObject s3ObjectDetails=_s3Service.getObjectDetails(_s3Bucket.getName(),getKey(companyId,repositoryId,fileName,versionLabel));
    return s3ObjectDetails.getContentLength();
  }
 catch (  ServiceException se) {
    throw new SystemException(se);
  }
}
