{
  if (file.isDirectory()) {
    String[] subDirs=FileUtil.listDirs(file);
    if (subDirs.length == 0) {
      if (file.lastModified() < lastDateToKeep) {
        FileUtil.deltree(file);
        return;
      }
    }
 else {
      for (      String subDir : subDirs) {
        deleteTempFiles(new File(file,subDir),lastDateToKeep);
      }
      if (file.list().length == 0) {
        FileUtil.deltree(file);
        return;
      }
    }
  }
}
