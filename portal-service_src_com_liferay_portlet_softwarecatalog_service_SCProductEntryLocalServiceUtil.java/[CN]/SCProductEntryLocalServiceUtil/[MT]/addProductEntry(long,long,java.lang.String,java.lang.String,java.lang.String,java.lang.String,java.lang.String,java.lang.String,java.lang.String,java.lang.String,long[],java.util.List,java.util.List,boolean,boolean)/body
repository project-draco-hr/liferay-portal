{
  SCProductEntryLocalService scProductEntryLocalService=SCProductEntryLocalServiceFactory.getService();
  return scProductEntryLocalService.addProductEntry(userId,plid,name,type,shortDescription,longDescription,pageURL,author,repoGroupId,repoArtifactId,licenseIds,thumbnails,fullImages,addCommunityPermissions,addGuestPermissions);
}
