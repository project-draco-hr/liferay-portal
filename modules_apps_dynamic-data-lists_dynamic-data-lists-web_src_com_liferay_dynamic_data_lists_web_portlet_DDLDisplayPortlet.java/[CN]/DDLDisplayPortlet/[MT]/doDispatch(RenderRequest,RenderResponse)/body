{
  if (SessionErrors.contains(renderRequest,NoSuchRecordSetException.class.getName()) || SessionErrors.contains(renderRequest,PortletPreferencesException.MustBeStrictPortlet.class.getName()) || SessionErrors.contains(renderRequest,PrincipalException.getNestedClasses())) {
    include(templatePath + "error.jsp",renderRequest,renderResponse);
  }
 else {
    super.doDispatch(renderRequest,renderResponse);
  }
}
