{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(classNameId));
  }
  com.liferay.portal.model.ClassName className=getPersistence().remove(classNameId);
  if (listener != null) {
    listener.onAfterRemove(className);
  }
  return className;
}
