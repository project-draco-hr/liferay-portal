{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Control Panel");
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Web Content",RuntimeVariables.replace("Web Content"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Web Content Name"),selenium.getText("//tr[3]/td[3]/a"));
  assertEquals(RuntimeVariables.replace("Approved"),selenium.getText("//tr[3]/td[4]/a"));
  assertNotEquals(RuntimeVariables.replace("Expired"),selenium.getText("//tr[3]/td[4]/a"));
  selenium.clickAt("//tr[3]/td/input",RuntimeVariables.replace("Web Content Name"));
  selenium.clickAt("//input[@value='Expire']",RuntimeVariables.replace("Expire"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertTrue(selenium.getConfirmation().matches("^Are you sure you want to expire the selected web content[\\s\\S]$"));
  assertEquals(RuntimeVariables.replace("Web Content Name"),selenium.getText("//tr[3]/td[3]/a"));
  assertEquals(RuntimeVariables.replace("Expired"),selenium.getText("//tr[3]/td[4]/a"));
  selenium.clickAt("//tr[3]/td[3]/a",RuntimeVariables.replace("Web Content Name"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Status: Expired"),selenium.getText("//div[@class='taglib-workflow-status']/span[3]"));
}
