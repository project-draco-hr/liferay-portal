{
  request.setAttribute("liferay-ui:my_sites:classNames",_classNames);
  request.setAttribute("liferay-ui:my_sites:cssClass",String.valueOf(_cssClass));
  request.setAttribute("liferay-ui:my_sites:includeControlPanel",String.valueOf(_includeControlPanel));
  request.setAttribute("liferay-ui:my_sites:max",String.valueOf(_max));
}
