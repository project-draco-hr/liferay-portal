{
  if (s == null) {
    return null;
  }
  byte byteArray[]=decode(s);
  ByteArrayInputStream bais=new ByteArrayInputStream(byteArray);
  try {
    ObjectInputStream is=new ObjectInputStream(new BufferedInputStream(bais));
    return is.readObject();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return null;
}
