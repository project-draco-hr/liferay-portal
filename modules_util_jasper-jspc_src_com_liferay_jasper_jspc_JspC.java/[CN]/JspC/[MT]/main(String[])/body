{
  Logger logger=Logger.getLogger(WebXml.class.getName());
  logger.setLevel(Level.SEVERE);
  JspC jspc=new JspC();
  try {
    jspc.setArgs(args);
    jspc.execute();
  }
 catch (  Exception e) {
    System.err.println(e);
    if (jspc.dieLevel != NO_DIE_LEVEL) {
      System.exit(jspc.dieLevel);
    }
  }
}
