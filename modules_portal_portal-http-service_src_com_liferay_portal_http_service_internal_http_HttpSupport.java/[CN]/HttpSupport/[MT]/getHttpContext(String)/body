{
  if (Validator.isNull(contextId)) {
    return null;
  }
  Filter filter=getFilter(contextId);
  Collection<ServiceReference<HttpContext>> serviceReferences=_bundleContext.getServiceReferences(HttpContext.class,filter.toString());
  Iterator<ServiceReference<HttpContext>> iterator=serviceReferences.iterator();
  if (iterator.hasNext()) {
    ServiceReference<HttpContext> httpContextReference=iterator.next();
    return _bundleContext.getService(httpContextReference);
  }
  return null;
}
