{
  try {
    Project project=getProject();
    ServiceBuilder serviceBuilder=ServiceBuilderInvoker.invoke(project.getBaseDir(),_serviceBuilderArgs);
    Set<String> modifiedFileNames=serviceBuilder.getModifiedFileNames();
    project.addIdReference(ServiceBuilder.OUTPUT_KEY_MODIFIED_FILES,modifiedFileNames);
  }
 catch (  Exception e) {
    throw new BuildException(e);
  }
}
