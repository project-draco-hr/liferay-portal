{
  try {
    ServiceBuilder serviceBuilder=ServiceBuilderInvoker.invoke(_serviceBuilderBean);
    Set<String> modifiedFiles=serviceBuilder.getModifiedFiles();
    Project project=getProject();
    project.addIdReference(ServiceBuilder.MODIFIED_FILES_ATTRIBUTE,modifiedFiles);
  }
 catch (  Exception e) {
    throw new BuildException(e);
  }
}
