{
  selenium.click("//div[@id='portlet-wrapper-19']/div[2]/div/div/form[1]/div[4]/table/tbody/tr[2]/td[1]/a/b");
  selenium.waitForPageToLoad("30000");
  selenium.click("//input[@value='Post New Thread']");
  selenium.waitForPageToLoad("30000");
  selenium.typeKeys("_19_subject",RuntimeVariables.replace("T\u00e9st M\u00e9ssag\u00e9"));
  selenium.type("_19_subject",RuntimeVariables.replace("T\u00e9st M\u00e9ssag\u00e9"));
  selenium.typeKeys("_19_textArea",RuntimeVariables.replace("This is a t\u00e9st m\u00e9ssag\u00e9!"));
  selenium.type("_19_textArea",RuntimeVariables.replace("This is a t\u00e9st m\u00e9ssag\u00e9!"));
  selenium.click("//input[@value='Save']");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isTextPresent("This is a t\u00e9st m\u00e9ssag\u00e9!")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
