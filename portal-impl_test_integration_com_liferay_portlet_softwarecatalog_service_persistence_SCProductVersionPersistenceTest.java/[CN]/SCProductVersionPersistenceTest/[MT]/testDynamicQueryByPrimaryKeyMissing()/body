{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SCProductVersion.class,SCProductVersion.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("productVersionId",ServiceTestUtil.nextLong()));
  List<SCProductVersion> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
