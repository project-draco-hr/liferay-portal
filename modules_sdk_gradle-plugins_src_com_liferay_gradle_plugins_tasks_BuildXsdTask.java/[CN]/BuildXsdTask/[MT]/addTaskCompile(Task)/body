{
  JavaCompile javaCompile=GradleUtil.addTask(_project,LiferayJavaPlugin.BUILD_XSD_TASK_NAME + "Compile",JavaCompile.class);
  javaCompile.dependsOn(BasePlugin.CLEAN_TASK_NAME + StringUtil.capitalize(javaCompile.getName()));
  javaCompile.setClasspath(getConfiguration());
  File tmpBinDir=new File(getTmpDir(),"bin");
  javaCompile.setDestinationDir(tmpBinDir);
  javaCompile.setSource(generateTask.getOutputs());
  return javaCompile;
}
