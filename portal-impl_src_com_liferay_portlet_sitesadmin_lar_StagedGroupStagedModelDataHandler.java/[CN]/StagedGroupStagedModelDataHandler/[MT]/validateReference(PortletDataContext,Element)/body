{
  String groupIdAttribute=referenceElement.attributeValue("group-id");
  if (Validator.isNull(groupIdAttribute)) {
    return true;
  }
  Group existingGroup=fetchExistingGroup(portletDataContext,referenceElement);
  if (existingGroup == null) {
    return false;
  }
  Map<Long,Long> groupIds=(Map<Long,Long>)portletDataContext.getNewPrimaryKeysMap(Group.class);
  groupIds.put(GetterUtil.getLong(groupIdAttribute),existingGroup.getGroupId());
  return true;
}
