{
  Session session=null;
  try {
    session=HibernateUtil.openSession();
    session.beginTransaction();
    session.save(foodItem);
    session.flush();
    session.getTransaction().commit();
  }
 catch (  Exception e) {
    throw new Exception(e);
  }
 finally {
    HibernateUtil.closeSession(session);
  }
}
