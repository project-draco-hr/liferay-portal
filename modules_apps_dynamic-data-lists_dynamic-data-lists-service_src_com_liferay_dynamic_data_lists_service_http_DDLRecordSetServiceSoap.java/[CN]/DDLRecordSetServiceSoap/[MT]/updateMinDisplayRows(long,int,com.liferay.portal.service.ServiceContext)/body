{
  try {
    com.liferay.portlet.dynamicdatalists.model.DDLRecordSet returnValue=DDLRecordSetServiceUtil.updateMinDisplayRows(recordSetId,minDisplayRows,serviceContext);
    return com.liferay.portlet.dynamicdatalists.model.DDLRecordSetSoap.toSoapModel(returnValue);
