{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  BookmarksFolder bookmarksFolder : bookmarksFolders) {
    EntityCacheUtil.removeResult(BookmarksFolderModelImpl.ENTITY_CACHE_ENABLED,BookmarksFolderImpl.class,bookmarksFolder.getPrimaryKey());
    clearUniqueFindersCache(bookmarksFolder);
  }
}
