{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletURLImpl portletURL=new PortletURLImpl(actionRequest,themeDisplay.getPpid(),themeDisplay.getPlid(),PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/copy_template");
  portletURL.setParameter("templateId",String.valueOf(template.getTemplateId()),false);
  portletURL.setWindowState(actionRequest.getWindowState());
  return portletURL.toString();
}
