{
  Set<String> availableLanguageIds=SetUtil.fromArray(super.getAvailableLanguageIds());
  String[] contentAvailableLanguageIds=LocalizationUtil.getAvailableLanguageIds(getContent());
  for (  String availableLanguageId : contentAvailableLanguageIds) {
    availableLanguageIds.add(availableLanguageId);
  }
  return availableLanguageIds.toArray(new String[availableLanguageIds.size()]);
}
