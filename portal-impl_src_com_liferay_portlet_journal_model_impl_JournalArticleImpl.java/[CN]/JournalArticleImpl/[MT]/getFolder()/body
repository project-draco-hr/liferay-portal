{
  JournalFolder folder=null;
  if (getFolderId() > 0) {
    try {
      folder=JournalFolderLocalServiceUtil.getFolder(getFolderId());
    }
 catch (    Exception e) {
      folder=new JournalFolderImpl();
      _log.error(e);
    }
  }
 else {
    folder=new JournalFolderImpl();
  }
  return folder;
}
