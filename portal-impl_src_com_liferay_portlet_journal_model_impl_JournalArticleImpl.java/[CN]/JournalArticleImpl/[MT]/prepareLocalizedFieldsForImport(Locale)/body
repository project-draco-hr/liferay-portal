{
  super.prepareLocalizedFieldsForImport(defaultImportLocale);
  String defaultImportLanguageId=LocaleUtil.toLanguageId(defaultImportLocale);
  String content=LocalizationUtil.updateLocalization(getContent(),"static-content",LocalizationUtil.getLocalization(getContent(),defaultImportLanguageId),defaultImportLanguageId,defaultImportLanguageId,true,true);
  setContent(content);
}
