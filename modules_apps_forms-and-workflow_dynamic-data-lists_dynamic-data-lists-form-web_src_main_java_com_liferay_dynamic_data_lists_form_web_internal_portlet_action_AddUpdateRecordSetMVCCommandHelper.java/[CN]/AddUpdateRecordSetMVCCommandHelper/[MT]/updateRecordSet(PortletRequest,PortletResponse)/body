{
  DDMStructure ddmStructure=updateDDMStructure(portletRequest);
  DDLRecordSet recordSet=updateRecordSet(portletRequest,ddmStructure.getStructureId());
  DDMFormValues settingsDDMFormValues=getSettingsDDMFormValues(portletRequest);
  updateRecordSetSettings(portletRequest,recordSet,settingsDDMFormValues);
  return recordSet;
}
