{
  if (!_replicateRemovals) {
    return;
  }
  PortalCacheManager<K,V> portalCacheManager=portalCache.getPortalCacheManager();
  PortalCacheClusterEvent portalCacheClusterEvent=new PortalCacheClusterEvent(portalCacheManager.getName(),portalCache.getName(),key,PortalCacheClusterEventType.REMOVE);
  PortalCacheClusterLinkUtil.sendEvent(portalCacheClusterEvent);
}
