{
  Connection con=null;
  PreparedStatement ps=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("update ExpandoValue set data_ = ? where valueId = ?");
    ps.setString(1,data);
    ps.setLong(2,valueId);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
}
