    MethodWrapper methodWrapper=new MethodWrapper(AnnouncementsFlagServiceUtil.class.getName(),"addFlag",new Object[]{paramObj0,paramObj1});
    try {
      TunnelUtil.invoke(httpPrincipal,methodWrapper);
    }
 catch (    Exception e) {
      if (e instanceof com.liferay.portal.PortalException) {
