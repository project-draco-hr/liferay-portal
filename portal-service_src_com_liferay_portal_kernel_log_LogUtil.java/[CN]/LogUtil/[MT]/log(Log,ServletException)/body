{
  Throwable cause=se.getRootCause();
  if (cause == null) {
    cause=se;
  }
  if (cause instanceof JspException) {
    log(log,(JspException)cause);
  }
 else   if ((cause != se) && (cause instanceof ServletException)) {
    log(log,(ServletException)cause);
  }
 else {
    _log(log,cause);
  }
}
