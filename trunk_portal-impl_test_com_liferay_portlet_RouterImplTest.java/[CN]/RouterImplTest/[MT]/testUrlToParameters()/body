{
  assertUrlGeneratesParameters("GET/boxes/16","id=16&action=view&method=GET&format=html&controller=boxes");
  assertUrlGeneratesParameters("GET/boxes/25.xml","id=25&action=view&method=GET&controller=boxes&format=xml");
  assertUrlGeneratesParameters("POST/boxes/8","id=8&action=update&method=POST&format=html&controller=boxes");
  assertUrlGeneratesParameters("POST/boxes/34.xml","id=34&action=update&method=POST&controller=boxes&format=xml");
  assertUrlGeneratesParameters("GET/boxes/new","action=new&method=GET&format=html&controller=boxes");
  assertUrlGeneratesParameters("GET/boxes/8/export","id=8&action=export&method=GET&format=html&controller=boxes");
  assertUrlGeneratesParameters("GET/boxes","action=index&method=GET&format=html&controller=boxes");
  assertUrlGeneratesParameters("GET/boxes.xml","action=index&method=GET&controller=boxes&format=xml");
  assertUrlGeneratesParameters("POST/boxes","action=create&method=POST&format=html&controller=boxes");
  assertUrlGeneratesParameters("POST/boxes.xml","action=create&method=POST&controller=boxes&format=xml");
}
