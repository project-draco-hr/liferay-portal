{
  if (ddmFormValues == null) {
    return null;
  }
  DDMFormValues copyDDMFormValues=new DDMFormValues(translate(ddmFormValues.getDDMForm()));
  copyDDMFormValues.setAvailableLocales(ddmFormValues.getAvailableLocales());
  copyDDMFormValues.setDefaultLocale(ddmFormValues.getDefaultLocale());
  for (  com.liferay.portlet.dynamicdatamapping.storage.DDMFormFieldValue ddmFormFieldValue : ddmFormValues.getDDMFormFieldValues()) {
    copyDDMFormValues.addDDMFormFieldValue(translate(ddmFormFieldValue));
  }
  return copyDDMFormValues;
}
