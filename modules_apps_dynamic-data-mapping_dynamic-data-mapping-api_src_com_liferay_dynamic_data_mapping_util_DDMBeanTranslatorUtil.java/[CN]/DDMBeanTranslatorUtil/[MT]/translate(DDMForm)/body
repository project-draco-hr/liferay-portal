{
  if (ddmForm == null) {
    return null;
  }
  com.liferay.portlet.dynamicdatamapping.model.DDMForm copyDDMForm=new com.liferay.portlet.dynamicdatamapping.model.DDMForm();
  copyDDMForm.setAvailableLocales(ddmForm.getAvailableLocales());
  copyDDMForm.setDefaultLocale(ddmForm.getDefaultLocale());
  for (  DDMFormField ddmFormField : ddmForm.getDDMFormFields()) {
    com.liferay.portlet.dynamicdatamapping.model.DDMFormField copyDDMFormField=translate(ddmFormField);
    copyDDMFormField.setDDMForm(copyDDMForm);
    copyDDMForm.addDDMFormField(copyDDMFormField);
  }
  return copyDDMForm;
}
