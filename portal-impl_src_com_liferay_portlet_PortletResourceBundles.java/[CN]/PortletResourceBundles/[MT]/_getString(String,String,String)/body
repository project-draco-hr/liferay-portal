{
  if (servletContextName != null) {
    ResourceBundle resourceBundle=_getResourceBundle(servletContextName,languageId);
    return resourceBundle.getString(key);
  }
  for (  Map.Entry<String,Map<String,ResourceBundle>> entry : _resourceBundles.entrySet()) {
    Map<String,ResourceBundle> resourceBundles=entry.getValue();
    ResourceBundle resourceBundle=_getResourceBundle(resourceBundles,languageId);
    return ResourceBundleUtil.getString(resourceBundle,key);
  }
  return null;
}
