{
  ResourceBundle resourceBundle=resourceBundles.get(languageId);
  if (resourceBundle == null) {
    try {
      resourceBundle=new NullSafeResourceBundle(new PropertyResourceBundle(new UnsyncByteArrayInputStream(new byte[0])));
      resourceBundles.put(languageId,resourceBundle);
    }
 catch (    IOException ioe) {
      _log.error(ioe);
    }
  }
  return resourceBundle;
}
