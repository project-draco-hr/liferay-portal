{
  String cmd=ParamUtil.getString(req,Constants.CMD);
  try {
    if (cmd.equals("community_orgs")) {
      updateCommunityOrgs(req);
    }
 else     if (cmd.equals("community_users")) {
      updateCommunityUsers(req);
    }
    if (Validator.isNotNull(cmd)) {
      sendRedirect(req,res);
    }
  }
 catch (  Exception e) {
    if (e != null && e instanceof NoSuchGroupException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      setForward(req,"portlet.communities.error");
    }
 else {
      throw e;
    }
  }
}
