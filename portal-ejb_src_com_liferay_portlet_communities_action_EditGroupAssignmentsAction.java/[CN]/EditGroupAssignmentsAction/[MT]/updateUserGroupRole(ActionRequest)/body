{
  String userId=ParamUtil.getString(req,"userId");
  long groupId=ParamUtil.getLong(req,"groupId");
  String[] addRoleIds=StringUtil.split(ParamUtil.getString(req,"addRoleIds"));
  String[] removeRoleIds=StringUtil.split(ParamUtil.getString(req,"removeRoleIds"));
  UserGroupRoleLocalServiceUtil.addGroupRoles(userId,groupId,addRoleIds);
  UserGroupRoleLocalServiceUtil.unsetGroupRoles(userId,groupId,removeRoleIds);
}
