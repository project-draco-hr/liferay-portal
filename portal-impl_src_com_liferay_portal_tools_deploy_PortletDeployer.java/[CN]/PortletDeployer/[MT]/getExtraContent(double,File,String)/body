{
  String extraContent=super.getExtraContent(webXmlVersion,srcFile,displayName);
  extraContent+="<listener>" + "<listener-class>" + "com.liferay.portal.kernel.servlet.PortletContextListener"+ "</listener-class>"+ "</listener>";
  File facesXML=new File(srcFile + "/WEB-INF/faces-config.xml");
  File portletXML=new File(srcFile + "/WEB-INF/" + Portal.PORTLET_XML_FILE_NAME_STANDARD);
  File webXML=new File(srcFile + "/WEB-INF/web.xml");
  extraContent+=getServletContent(portletXML,webXML);
  setupJSF(facesXML,portletXML);
  if (_sunFacesPortlet) {
    extraContent+="<listener>" + "<listener-class>" + "com.liferay.util.bridges.jsf.sun.LiferayConfigureListener"+ "</listener-class>"+ "</listener>";
  }
  return extraContent;
}
