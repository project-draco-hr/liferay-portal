{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace("Asset Publisher Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Options"),selenium.getText("//strong/a"));
  selenium.clickAt("//strong/a",RuntimeVariables.replace("Options"));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Configuration"),selenium.getText("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a"));
  selenium.click("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//select[@id='_86_selectionStyle']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  assertEquals("Dynamic",selenium.getSelectedLabel("//select[@id='_86_selectionStyle']"));
  assertEquals(RuntimeVariables.replace("Source"),selenium.getText("//form[@id='_86_fm']/div[1]/div[1]/div[1]/div/span"));
  assertEquals(RuntimeVariables.replace("Filter"),selenium.getText("//form[@id='_86_fm']/div[1]/div[2]/div[1]/div/span"));
  assertEquals(RuntimeVariables.replace("Ordering and Grouping"),selenium.getText("//form[@id='_86_fm']/div[1]/div[3]/div[1]/div/span"));
  assertEquals(RuntimeVariables.replace("Display Settings"),selenium.getText("//form[@id='_86_fm']/div[1]/div[4]/div[1]/div/span"));
}
