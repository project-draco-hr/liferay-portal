{
  SimpleKeywordTokenizer simpleKeywordTokenizer=new SimpleKeywordTokenizer();
  Assert.assertTrue(simpleKeywordTokenizer.requiresTokenization("This is a simple test"));
  Assert.assertTrue(simpleKeywordTokenizer.requiresTokenization("This \"is a simple\" test"));
  Assert.assertFalse(simpleKeywordTokenizer.requiresTokenization("\"is a simple\""));
}
