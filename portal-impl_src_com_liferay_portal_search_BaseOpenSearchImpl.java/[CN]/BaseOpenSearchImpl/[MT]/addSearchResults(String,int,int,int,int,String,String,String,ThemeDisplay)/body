{
  int totalPages=0;
  if (total % itemsPerPage == 0) {
    totalPages=total / itemsPerPage;
  }
 else {
    totalPages=(total / itemsPerPage) + 1;
  }
  int previousPage=startPage - 1;
  int nextPage=startPage + 1;
  Document doc=SAXReaderUtil.createDocument();
  if (format.equals("rss")) {
    return addSearchResultsRSS(keywords,startPage,itemsPerPage,total,start,totalPages,previousPage,nextPage,title,searchPath,themeDisplay);
  }
 else {
    return addSearchResultsAtom(keywords,startPage,itemsPerPage,total,start,totalPages,previousPage,nextPage,title,searchPath,themeDisplay);
  }
}
