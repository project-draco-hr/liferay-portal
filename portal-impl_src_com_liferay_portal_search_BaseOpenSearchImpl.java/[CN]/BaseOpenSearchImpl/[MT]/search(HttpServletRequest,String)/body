{
  String keywords=GetterUtil.getString(Http.getParameter(url,"keywords",false));
  int startPage=GetterUtil.getInteger(Http.getParameter(url,"p",false),1);
  int itemsPerPage=GetterUtil.getInteger(Http.getParameter(url,"c",false),SearchContainer.DEFAULT_DELTA);
  return search(req,keywords,startPage,itemsPerPage);
}
