{
  StringBuffer sb=new StringBuffer();
  for (int i=0; i < entryIds.length; i++) {
    sb.append("TagsEntry.entryId = ? ");
    if ((i + 1) != entryIds.length) {
      if (andOperator) {
        sb.append("AND ");
      }
 else {
        sb.append("OR ");
      }
    }
  }
  return sb.toString();
}
