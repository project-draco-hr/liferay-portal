{
  ExecutionRequestMessage executionRequestMessage=(ExecutionRequestMessage)requestMessage.getPayload();
  if (!_executionRequestMessageAcceptor.accept(executionRequestMessage)) {
    return;
  }
  try {
    doReceive(requestMessage,executionRequestMessage);
  }
 catch (  Exception e) {
    _log.error("Unable to process rule engine execution request",e);
  }
}
