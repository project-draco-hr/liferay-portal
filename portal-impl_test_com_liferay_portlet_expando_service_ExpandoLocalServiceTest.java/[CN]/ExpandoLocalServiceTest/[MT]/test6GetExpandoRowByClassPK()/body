{
  ExpandoRow row=ExpandoRowLocalServiceUtil.getRow(ExpandoLocalServiceTest.class.getName(),_TABLENAME,_CLASSPK1);
  assertNotNull(row);
  List<ExpandoValue> values=ExpandoValueLocalServiceUtil.getRowValues(row.getRowId());
  assertEquals(values.size(),2);
  assertEquals(values.get(0).getClassPK(),_CLASSPK1);
  assertEquals(values.get(0).getString(),_VALUE1_COLUMN1);
  assertEquals(values.get(1).getClassPK(),_CLASSPK1);
  assertEquals(values.get(1).getDouble(),_VALUE1_COLUMN2);
}
