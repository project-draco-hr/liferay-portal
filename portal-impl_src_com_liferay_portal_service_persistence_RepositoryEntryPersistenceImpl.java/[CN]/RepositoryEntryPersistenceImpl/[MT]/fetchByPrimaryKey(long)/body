{
  RepositoryEntry repositoryEntry=(RepositoryEntry)EntityCacheUtil.getResult(RepositoryEntryModelImpl.ENTITY_CACHE_ENABLED,RepositoryEntryImpl.class,repositoryEntryId,this);
  if (repositoryEntry == null) {
    Session session=null;
    try {
      session=openSession();
      repositoryEntry=(RepositoryEntry)session.get(RepositoryEntryImpl.class,new Long(repositoryEntryId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (repositoryEntry != null) {
        cacheResult(repositoryEntry);
      }
      closeSession(session);
    }
  }
  return repositoryEntry;
}
