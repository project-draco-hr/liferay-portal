{
  RepositoryEntry repositoryEntry=(RepositoryEntry)EntityCacheUtil.getResult(RepositoryEntryModelImpl.ENTITY_CACHE_ENABLED,RepositoryEntryImpl.class,repositoryEntryId,this);
  if (repositoryEntry == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (repositoryEntry == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      repositoryEntry=(RepositoryEntry)session.get(RepositoryEntryImpl.class,Long.valueOf(repositoryEntryId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (repositoryEntry == null)) {
        EntityCacheUtil.putResult(RepositoryEntryModelImpl.ENTITY_CACHE_ENABLED,RepositoryEntryImpl.class,repositoryEntryId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(repositoryEntry);
      }
      closeSession(session);
    }
  }
  return repositoryEntry;
}
