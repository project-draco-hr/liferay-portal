{
  EntityCacheUtil.removeResult(RepositoryEntryModelImpl.ENTITY_CACHE_ENABLED,RepositoryEntryImpl.class,repositoryEntry.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{repositoryEntry.getUuid(),Long.valueOf(repositoryEntry.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_R_M,new Object[]{Long.valueOf(repositoryEntry.getRepositoryId()),repositoryEntry.getMappedId()});
}
