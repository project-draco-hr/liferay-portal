{
  Session session=null;
  try {
    session=openSession();
    BlogsStatsUser blogsStatsUser=(BlogsStatsUser)session.get(BlogsStatsUserImpl.class,primaryKey);
    if (blogsStatsUser == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchStatsUserException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(blogsStatsUser);
  }
 catch (  NoSuchStatsUserException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
