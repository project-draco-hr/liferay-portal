{
  BlogsStatsUser blogsStatsUser=findByPrimaryKey(statsUserId);
  Session session=null;
  try {
    session=openSession();
    BlogsStatsUser[] array=new BlogsStatsUserImpl[3];
    array[0]=getByGroupId_PrevAndNext(session,blogsStatsUser,groupId,orderByComparator,true);
    array[1]=blogsStatsUser;
    array[2]=getByGroupId_PrevAndNext(session,blogsStatsUser,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
