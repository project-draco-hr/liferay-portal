{
  if (isNew) {
    Object[] args=new Object[]{blogsStatsUser.getGroupId(),blogsStatsUser.getUserId()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_G_U,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_U,args,blogsStatsUser);
  }
 else {
    BlogsStatsUserModelImpl blogsStatsUserModelImpl=(BlogsStatsUserModelImpl)blogsStatsUser;
    if ((blogsStatsUserModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_G_U.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{blogsStatsUser.getGroupId(),blogsStatsUser.getUserId()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_G_U,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_U,args,blogsStatsUser);
    }
  }
}
