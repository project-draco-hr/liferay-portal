{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId(),TestPropsValues.getUserId());
  _folder=DLAppServiceUtil.addFolder(group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,RandomTestUtil.randomString(),RandomTestUtil.randomString(),serviceContext);
  DLAppServiceUtil.moveFolderToTrash(_folder.getFolderId());
  DLAppServiceUtil.restoreFolderFromTrash(_folder.getFolderId());
}
