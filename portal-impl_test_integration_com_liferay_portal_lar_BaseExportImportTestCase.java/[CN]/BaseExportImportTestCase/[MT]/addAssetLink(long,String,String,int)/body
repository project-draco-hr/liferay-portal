{
  AssetEntry originAssetEntry=AssetEntryLocalServiceUtil.getEntry(groupId,sourceStagedModelUuid);
  AssetEntry targetAssetEntry=AssetEntryLocalServiceUtil.getEntry(groupId,targetStagedModelUuid);
  return AssetLinkLocalServiceUtil.addLink(TestPropsValues.getUserId(),originAssetEntry.getEntryId(),targetAssetEntry.getEntryId(),0,weight);
}
