{
  String expectedException="\"This is Site 1\" does not contain \"Blah Blah Blah\" " + "at \"//html/body/h1\"";
  if (TestPropsValues.SELENIUM_LOGGER_ENABLED) {
    expectedException="Command failure \"assertPartialText\" with parameters " + "\"//html/body/h1\" \"Blah Blah Blah\" : " + "\"This is Site 1\" does not contain "+ "\"Blah Blah Blah\" at \"//html/body/h1\"";
  }
  try {
    selenium.assertPartialText("//html/body/h1","Blah Blah Blah");
  }
 catch (  Throwable e) {
    assertEquals(e.getMessage(),expectedException);
  }
}
