{
  Compiler compiler=new Compiler(new LogErrorManager(_log));
  SourceFile sourceFile=SourceFile.fromCode(resourceName,content);
  CompilerOptions compilerOptions=new CompilerOptions();
  compilerOptions.setWarningLevel(DiagnosticGroups.NON_STANDARD_JSDOC,CheckLevel.OFF);
  setDefaultCompileOptions(compilerOptions);
  compiler.compile(SourceFile.fromCode("extern",StringPool.BLANK),sourceFile,compilerOptions);
  return compiler.toSource();
}
