{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(tagsAsset);
    }
 else {
      if (tagsAsset.isNew()) {
        session.save(tagsAsset);
      }
    }
    session.flush();
    tagsAsset.setNew(false);
    return tagsAsset;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
