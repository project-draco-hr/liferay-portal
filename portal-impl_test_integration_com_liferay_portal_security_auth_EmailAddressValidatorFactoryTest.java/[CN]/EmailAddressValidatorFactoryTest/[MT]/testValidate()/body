{
  EmailAddressValidator emailAddressValidator=EmailAddressValidatorFactory.getInstance();
  Assert.assertTrue(emailAddressValidator.validate(1,"TestEmailAddressValidator@liferay-test.com"));
  Assert.assertFalse(emailAddressValidator.validate(2,"TestEmailAddressValidator@liferay-test.com"));
  Assert.assertFalse(emailAddressValidator.validate(1,"not@liferay-test.com"));
}
