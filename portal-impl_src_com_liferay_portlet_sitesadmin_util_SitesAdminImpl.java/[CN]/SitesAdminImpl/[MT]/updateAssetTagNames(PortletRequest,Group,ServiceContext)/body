{
  if (serviceContext.getAssetTagNames().length > 0) {
    return;
  }
  List<AssetTag> tags=AssetTagServiceUtil.getTags(liveGroup.getClassName(),liveGroup.getClassPK());
  String defaultAssetTagNames=ListUtil.toString(tags,AssetTag.NAME_ACCESSOR);
  serviceContext.setAssetTagNames(StringUtil.split(defaultAssetTagNames));
}
