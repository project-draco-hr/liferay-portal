{
  if (serviceContext.getAssetCategoryIds().length > 0) {
    return;
  }
  List<AssetCategory> categories=AssetCategoryServiceUtil.getCategories(liveGroup.getClassName(),liveGroup.getClassPK());
  List<Long> assetCategoryIdsList=new ArrayList<Long>();
  for (  AssetCategory category : categories) {
    assetCategoryIdsList.add(category.getCategoryId());
  }
  serviceContext.setAssetCategoryIds(ArrayUtil.toArray(assetCategoryIdsList.toArray(new Long[assetCategoryIdsList.size()])));
}
