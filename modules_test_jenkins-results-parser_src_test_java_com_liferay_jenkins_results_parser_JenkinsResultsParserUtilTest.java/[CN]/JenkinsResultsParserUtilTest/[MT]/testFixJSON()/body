{
  String[] expectedResultArray=new String[]{"&#09;","&#34; ","&#39;","&#40;","&#41;","&#60;","&#62;","&#91;","&#92;","&#93;","&#123;","&#125;","<br />"};
  String[] testArray=new String[]{"\t","\"","'","(",")","<",">","[","\\","]","{","}","\n"};
  if (expectedResultArray.length != testArray.length) {
    throw new RuntimeException("Expected results array length does not equal test array" + " length.");
  }
  for (int i=0; i < expectedResultArray.length; i++) {
    String expectedResult="ABC" + expectedResultArray[i] + "123";
    String test="ABC" + testArray[i] + "123";
    assertEquals(expectedResult,JenkinsResultsParserUtil.fixJSON(test));
  }
}
