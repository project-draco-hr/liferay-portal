{
  if (obj == null) {
    return false;
  }
  MBMessageFlagPK pk=null;
  try {
    pk=(MBMessageFlagPK)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  if ((topicId.equals(pk.topicId)) && (messageId.equals(pk.messageId)) && (userId == pk.userId)) {
    return true;
  }
 else {
    return false;
  }
}
