{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(igImagePK));
  }
  com.liferay.portlet.imagegallery.model.IGImage igImage=getPersistence().remove(igImagePK);
  if (listener != null) {
    listener.onAfterRemove(igImage);
  }
  return igImage;
}
