{
  ModelListener listener=null;
  if (Validator.isNotNull(_LISTENER)) {
    try {
      listener=(ModelListener)Class.forName(_LISTENER).newInstance();
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(igImagePK));
  }
  com.liferay.portlet.imagegallery.model.IGImage igImage=getPersistence().remove(igImagePK);
  if (listener != null) {
    listener.onAfterRemove(igImage);
  }
  return igImage;
}
