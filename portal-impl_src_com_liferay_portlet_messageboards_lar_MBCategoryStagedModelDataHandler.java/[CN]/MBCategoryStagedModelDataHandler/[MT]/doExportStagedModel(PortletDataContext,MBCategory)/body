{
  if ((category.getCategoryId() == MBCategoryConstants.DEFAULT_PARENT_CATEGORY_ID) || (category.getCategoryId() == MBCategoryConstants.DISCUSSION_CATEGORY_ID)) {
    return;
  }
  if (category.getParentCategory() != null) {
    StagedModelDataHandlerUtil.exportReferenceStagedModel(portletDataContext,category,category.getParentCategory(),PortletDataContext.REFERENCE_TYPE_PARENT);
  }
  Element categoryElement=portletDataContext.getExportDataElement(category);
  portletDataContext.addClassedModel(categoryElement,ExportImportPathUtil.getModelPath(category),category);
}
