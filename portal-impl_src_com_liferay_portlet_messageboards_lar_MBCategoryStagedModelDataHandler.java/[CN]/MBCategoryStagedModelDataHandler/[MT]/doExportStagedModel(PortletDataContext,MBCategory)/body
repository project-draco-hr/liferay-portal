{
  if ((category.getCategoryId() == MBCategoryConstants.DEFAULT_PARENT_CATEGORY_ID) || (category.getCategoryId() == MBCategoryConstants.DISCUSSION_CATEGORY_ID)) {
    return;
  }
  if (category.getParentCategory() != null) {
    StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,category.getParentCategory());
  }
  Element categoryElement=portletDataContext.getExportDataElement(category);
  portletDataContext.addClassedModel(categoryElement,ExportImportPathUtil.getModelPath(category),category,MBPortletDataHandler.NAMESPACE);
}
