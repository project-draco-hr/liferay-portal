{
  if (_article != null) {
    return _article;
  }
  _article=(JournalArticle)_request.getAttribute(WebKeys.JOURNAL_ARTICLE);
  if (_article != null) {
    return _article;
  }
  _article=JournalArticleLocalServiceUtil.fetchLatestArticle(getArticleGroupId(),getArticleId(),WorkflowConstants.STATUS_ANY);
  return _article;
}
