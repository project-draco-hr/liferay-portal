{
  try {
    long companyId=PortalUtil.getCompanyId(request);
    CASConfiguration casConfiguration=_settingsFactory.getSettings(CASConfiguration.class,new CompanyServiceSettingsLocator(companyId,CASConstants.SERVICE_NAME));
    if (casConfiguration.enabled()) {
      return true;
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return false;
}
