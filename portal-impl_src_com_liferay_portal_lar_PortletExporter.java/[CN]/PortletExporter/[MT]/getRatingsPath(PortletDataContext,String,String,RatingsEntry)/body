{
  StringBundler sb=new StringBundler(8);
  sb.append(context.getRootPath());
  sb.append("/ratings/");
  sb.append(PortalUtil.getClassNameId(className));
  sb.append(CharPool.FORWARD_SLASH);
  sb.append(classPK);
  sb.append(CharPool.FORWARD_SLASH);
  sb.append(rating.getEntryId());
  sb.append(".xml");
  return sb.toString();
}
