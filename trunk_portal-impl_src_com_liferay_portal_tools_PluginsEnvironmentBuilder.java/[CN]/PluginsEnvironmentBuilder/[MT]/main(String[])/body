{
  InitUtil.initWithSpring();
  File dir=new File(System.getProperty("plugins.env.dir"));
  boolean svn=GetterUtil.getBoolean(System.getProperty("plugins.env.svn"));
  boolean eclipse=GetterUtil.getBoolean(System.getProperty("plugins.env.eclipse"));
  new PluginsEnvironmentBuilder(dir,svn,eclipse);
}
