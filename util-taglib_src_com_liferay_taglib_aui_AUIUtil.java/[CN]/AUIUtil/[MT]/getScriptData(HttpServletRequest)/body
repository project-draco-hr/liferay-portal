{
  ScriptData scriptData=(ScriptData)request.getAttribute(ScriptTag.class.getName());
  if (scriptData == null) {
    scriptData=(ScriptData)request.getAttribute(WebKeys.AUI_SCRIPT_DATA);
    if (scriptData != null) {
      request.removeAttribute(WebKeys.AUI_SCRIPT_DATA);
    }
  }
  return scriptData;
}
