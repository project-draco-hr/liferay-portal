{
  try {
    FlexjsonBeanAnalyzerTransformer flexjsonBeanAnalyzerTransformer=new FlexjsonBeanAnalyzerTransformer(type){
      @Override protected String getTypeName(      Class<?> type){
        return _formatType(type,null,false);
      }
    }
;
    return flexjsonBeanAnalyzerTransformer.collect();
  }
 catch (  Exception e) {
    return null;
  }
}
