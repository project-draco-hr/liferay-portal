{
  Session session=null;
  try {
    session=openSession();
    session.delete(release);
    session.flush();
    return release;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Release.class.getName());
  }
}
