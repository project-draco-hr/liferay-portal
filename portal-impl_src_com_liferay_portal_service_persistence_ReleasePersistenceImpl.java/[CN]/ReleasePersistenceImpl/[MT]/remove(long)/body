{
  Session session=null;
  try {
    session=openSession();
    Release release=(Release)session.get(ReleaseImpl.class,new Long(releaseId));
    if (release == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + releaseId);
      }
      throw new NoSuchReleaseException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + releaseId);
    }
    return releasePersistence.remove(release);
  }
 catch (  NoSuchReleaseException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
