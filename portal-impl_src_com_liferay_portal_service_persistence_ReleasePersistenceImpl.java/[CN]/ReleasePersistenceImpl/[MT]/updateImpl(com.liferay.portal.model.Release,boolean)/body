{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,release,merge);
    release.setNew(false);
    return release;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Release.class.getName());
  }
}
