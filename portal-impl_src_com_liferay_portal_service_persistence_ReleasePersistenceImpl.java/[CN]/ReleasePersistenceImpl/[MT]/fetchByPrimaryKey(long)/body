{
  Release release=(Release)EntityCacheUtil.getResult(ReleaseModelImpl.ENTITY_CACHE_ENABLED,ReleaseImpl.class,releaseId);
  if (release == _nullRelease) {
    return null;
  }
  if (release == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      release=(Release)session.get(ReleaseImpl.class,Long.valueOf(releaseId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (release != null) {
        cacheResult(release);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(ReleaseModelImpl.ENTITY_CACHE_ENABLED,ReleaseImpl.class,releaseId,_nullRelease);
      }
      closeSession(session);
    }
  }
  return release;
}
