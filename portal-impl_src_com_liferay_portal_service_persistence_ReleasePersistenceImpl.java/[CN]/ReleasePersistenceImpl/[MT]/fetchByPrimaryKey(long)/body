{
  Release result=(Release)EntityCacheUtil.getResult(ReleaseModelImpl.ENTITY_CACHE_ENABLED,Release.class,releaseId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      Release release=(Release)session.get(ReleaseImpl.class,new Long(releaseId));
      cacheResult(release);
      return release;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (Release)result;
  }
}
