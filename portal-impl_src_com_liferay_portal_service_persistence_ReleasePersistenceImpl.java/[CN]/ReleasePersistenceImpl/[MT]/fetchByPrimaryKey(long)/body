{
  Release release=(Release)EntityCacheUtil.getResult(ReleaseModelImpl.ENTITY_CACHE_ENABLED,ReleaseImpl.class,releaseId,this);
  if (release == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (release == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      release=(Release)session.get(ReleaseImpl.class,Long.valueOf(releaseId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (release == null)) {
        EntityCacheUtil.putResult(ReleaseModelImpl.ENTITY_CACHE_ENABLED,ReleaseImpl.class,releaseId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(release);
      }
      closeSession(session);
    }
  }
  return release;
}
