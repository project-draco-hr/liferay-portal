{
  Release release=(Release)EntityCacheUtil.getResult(ReleaseModelImpl.ENTITY_CACHE_ENABLED,ReleaseImpl.class,releaseId,this);
  if (release == null) {
    Session session=null;
    try {
      session=openSession();
      release=(Release)session.get(ReleaseImpl.class,Long.valueOf(releaseId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (release != null) {
        cacheResult(release);
      }
      closeSession(session);
    }
  }
  return release;
}
