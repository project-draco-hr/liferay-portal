{
  _companyIndexFactory.addIndexSettingsContributor(new BaseIndexSettingsContributor(1){
    @Override public void populate(    Builder builder){
      builder.put("index.number_of_replicas","2");
      builder.put("index.number_of_shards","3");
    }
  }
);
  _companyIndexFactory.setAdditionalIndexConfigurations(new String[]{"index.number_of_replicas: 0","index.number_of_shards: 0"});
  Settings settings=createIndex();
  Assert.assertEquals("2",settings.get("index.number_of_replicas"));
  Assert.assertEquals("3",settings.get("index.number_of_shards"));
}
