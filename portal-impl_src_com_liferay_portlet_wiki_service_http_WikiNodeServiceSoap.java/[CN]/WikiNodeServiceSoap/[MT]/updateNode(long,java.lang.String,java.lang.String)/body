{
  try {
    com.liferay.portlet.wiki.model.WikiNode returnValue=WikiNodeServiceUtil.updateNode(nodeId,name,description);
    return com.liferay.portlet.wiki.model.WikiNodeSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
