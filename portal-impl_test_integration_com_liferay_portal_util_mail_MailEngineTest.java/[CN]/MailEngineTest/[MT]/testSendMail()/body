{
  MailMessage mailMessage=new MailMessage(new InternetAddress("from@test.com"),new InternetAddress("to@test.com"),"Hello","My name is Inigo Montoya.",true);
  MailEngine.send(mailMessage);
  Assert.assertEquals(1,MailServiceTestUtil.getInboxSize());
  List<SmtpMessage> smtMessages=MailServiceTestUtil.getMessages("Body","My name is Inigo Montoya.");
  Assert.assertEquals(1,smtMessages.size());
  smtMessages=MailServiceTestUtil.getMessages("Subject","Hello");
  Assert.assertEquals(1,smtMessages.size());
  smtMessages=MailServiceTestUtil.getMessages("To","to@test.com");
  Assert.assertEquals(1,smtMessages.size());
}
