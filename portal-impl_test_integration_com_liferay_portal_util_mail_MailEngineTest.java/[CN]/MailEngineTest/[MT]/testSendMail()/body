{
  MailEngine.send(MailServiceTestUtil.createMailMessage("sender@here.com","receiver@here.com","Test","Test Body",true));
  Assert.assertEquals(1,MailServiceTestUtil.getInboxSize());
  List<SmtpMessage> messages=MailServiceTestUtil.findMessagesBySubject("Test");
  Assert.assertEquals(1,messages.size());
  messages=MailServiceTestUtil.findMessagesByDestination("receiver@here.com");
  Assert.assertEquals(1,messages.size());
  messages=MailServiceTestUtil.findMessagesByBody("Test Body");
  Assert.assertEquals(1,messages.size());
}
