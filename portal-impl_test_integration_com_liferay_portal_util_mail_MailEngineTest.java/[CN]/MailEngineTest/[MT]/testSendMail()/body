{
  MailMessage mailMessage=new MailMessage(new InternetAddress("from@test.com"),new InternetAddress("to@test.com"),"Hello","My name is Inigo Montoya.",true);
  MailEngine.send(mailMessage);
  Assert.assertEquals(1,MailServiceTestUtil.getInboxSize());
  List<com.dumbster.smtp.MailMessage> mailMessages=MailServiceTestUtil.getMailMessages("Body","My name is Inigo Montoya.");
  Assert.assertEquals(1,mailMessages.size());
  mailMessages=MailServiceTestUtil.getMailMessages("Subject","Hello");
  Assert.assertEquals(1,mailMessages.size());
  mailMessages=MailServiceTestUtil.getMailMessages("To","to@test.com");
  Assert.assertEquals(1,mailMessages.size());
}
