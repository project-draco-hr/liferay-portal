  if (folderId != 0) {
    initByFolderId(folderId);
  }
 else   if (fileEntryId != 0) {
    initByFileEntryId(fileEntryId);
  }
 else   if (fileVersionId != 0) {
    initByFileVersionId(fileVersionId);
  }
}
