{
  try {
    DLFolder dlFolder=dlRepositoryLocalService.getFolder(folderId);
    initByRepositoryId(dlFolder.getRepositoryId());
  }
 catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      if (e instanceof NoSuchFolderException) {
        _log.debug(e.getMessage());
      }
 else {
        _log.debug(e,e);
      }
    }
  }
}
