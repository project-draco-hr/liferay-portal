{
  _bundleContext=bundleContext;
  for (  Map.Entry<ServiceReference<IGDisplayContextFactory>,IGDisplayContextFactory> entry : _igDisplayContextFactories.entrySet()) {
    if (entry.getValue() != null) {
      continue;
    }
    ServiceReference<IGDisplayContextFactory> serviceReference=entry.getKey();
    IGDisplayContextFactory igDisplayContextFactory=_bundleContext.getService(serviceReference);
    _igDisplayContextFactories.put(serviceReference,igDisplayContextFactory);
  }
}
