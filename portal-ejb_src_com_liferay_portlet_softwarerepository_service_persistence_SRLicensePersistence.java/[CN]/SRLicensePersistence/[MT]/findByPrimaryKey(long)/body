{
  SRLicense srLicense=fetchByPrimaryKey(licenseId);
  if (srLicense == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No SRLicense exists with the primary key " + licenseId);
    }
    throw new NoSuchLicenseException("No SRLicense exists with the primary key " + licenseId);
  }
  return srLicense;
}
