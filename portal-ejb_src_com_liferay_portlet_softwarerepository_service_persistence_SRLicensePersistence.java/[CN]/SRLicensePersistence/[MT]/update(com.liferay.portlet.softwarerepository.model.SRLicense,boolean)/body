{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(srLicense);
    }
 else {
      if (srLicense.isNew()) {
        session.save(srLicense);
      }
    }
    session.flush();
    srLicense.setNew(false);
    return srLicense;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
