{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("_58_login")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertTrue(selenium.isElementPresent("link=Sign In"));
  assertTrue(selenium.isTextPresent("Welcome!"));
}
