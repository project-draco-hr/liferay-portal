{
  SocialEquityGroupSetting socialEquityGroupSetting=(SocialEquityGroupSetting)EntityCacheUtil.getResult(SocialEquityGroupSettingModelImpl.ENTITY_CACHE_ENABLED,SocialEquityGroupSettingImpl.class,equityGroupSettingId,this);
  if (socialEquityGroupSetting == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (socialEquityGroupSetting == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      socialEquityGroupSetting=(SocialEquityGroupSetting)session.get(SocialEquityGroupSettingImpl.class,Long.valueOf(equityGroupSettingId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (socialEquityGroupSetting == null)) {
        EntityCacheUtil.putResult(SocialEquityGroupSettingModelImpl.ENTITY_CACHE_ENABLED,SocialEquityGroupSettingImpl.class,equityGroupSettingId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(socialEquityGroupSetting);
      }
      closeSession(session);
    }
  }
  return socialEquityGroupSetting;
}
