{
  SocialEquityGroupSetting socialEquityGroupSetting=(SocialEquityGroupSetting)EntityCacheUtil.getResult(SocialEquityGroupSettingModelImpl.ENTITY_CACHE_ENABLED,SocialEquityGroupSettingImpl.class,equityGroupSettingId);
  if (socialEquityGroupSetting == _nullSocialEquityGroupSetting) {
    return null;
  }
  if (socialEquityGroupSetting == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      socialEquityGroupSetting=(SocialEquityGroupSetting)session.get(SocialEquityGroupSettingImpl.class,Long.valueOf(equityGroupSettingId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (socialEquityGroupSetting != null) {
        cacheResult(socialEquityGroupSetting);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(SocialEquityGroupSettingModelImpl.ENTITY_CACHE_ENABLED,SocialEquityGroupSettingImpl.class,equityGroupSettingId,_nullSocialEquityGroupSetting);
      }
      closeSession(session);
    }
  }
  return socialEquityGroupSetting;
}
