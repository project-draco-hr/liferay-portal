{
  SocialEquityGroupSetting socialEquityGroupSetting=fetchByPrimaryKey(equityGroupSettingId);
  if (socialEquityGroupSetting == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + equityGroupSettingId);
    }
    throw new NoSuchEquityGroupSettingException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + equityGroupSettingId);
  }
  return socialEquityGroupSetting;
}
