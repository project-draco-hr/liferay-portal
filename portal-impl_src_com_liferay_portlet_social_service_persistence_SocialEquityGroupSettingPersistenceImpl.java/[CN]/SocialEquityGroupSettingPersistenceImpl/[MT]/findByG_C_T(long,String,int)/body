{
  SocialEquityGroupSetting socialEquityGroupSetting=fetchByG_C_T(groupId,className,type);
  if (socialEquityGroupSetting == null) {
    StringBundler msg=new StringBundler(8);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("groupId=");
    msg.append(groupId);
    msg.append(", className=");
    msg.append(className);
    msg.append(", type=");
    msg.append(type);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    if (_log.isWarnEnabled()) {
      _log.warn(msg.toString());
    }
    throw new NoSuchEquityGroupSettingException(msg.toString());
  }
  return socialEquityGroupSetting;
}
