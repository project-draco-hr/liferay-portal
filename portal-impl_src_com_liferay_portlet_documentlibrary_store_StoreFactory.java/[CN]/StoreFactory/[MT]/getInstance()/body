{
  if (_store == null) {
    checkProperties();
    if (_log.isDebugEnabled()) {
      _log.debug("Instantiate " + PropsValues.DL_STORE_IMPL);
    }
    try {
      _store=_getInstance();
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
  if ((_store != null) && _log.isDebugEnabled()) {
    Class<?> clazz=_store.getClass();
    _log.debug("Return " + clazz.getName());
  }
  return _store;
}
