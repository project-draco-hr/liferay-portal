{
  if (_store == null) {
    Set<String> keySet=_serviceTrackerMap.keySet();
    for (    String key : keySet) {
      if (key.endsWith("FileSystemStore")) {
        _store=getStoreInstance(key);
        break;
      }
    }
    if (_store == null) {
      return _NULL_STORE;
    }
  }
  return _store;
}
