{
  setParams(params);
  params.put("p_p_id",_PORTLET_ID);
  params.put("p_p_action","0");
  int x=friendlyURLPath.indexOf(StringPool.SLASH,1);
  int y=friendlyURLPath.length();
  String[] entries=StringUtil.split(friendlyURLPath.substring(x + 1,y),StringPool.SLASH);
  if (entries.length > 0) {
    StringBuffer sb=new StringBuffer();
    for (int i=0; i < entries.length; i++) {
      String entry=StringUtil.replace(entries[i],StringPool.PLUS,StringPool.SPACE);
      if (i != 0) {
        sb.append(StringPool.COMMA);
      }
      sb.append(entry);
    }
    addParam("entries",sb.toString());
  }
}
