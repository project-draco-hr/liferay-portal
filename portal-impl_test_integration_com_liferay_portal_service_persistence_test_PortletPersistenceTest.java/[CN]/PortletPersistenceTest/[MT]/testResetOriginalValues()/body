{
  Portlet newPortlet=addPortlet();
  _persistence.clearCache();
  Portlet existingPortlet=_persistence.findByPrimaryKey(newPortlet.getPrimaryKey());
  Assert.assertEquals(Long.valueOf(existingPortlet.getCompanyId()),ReflectionTestUtil.<Long>invoke(existingPortlet,"getOriginalCompanyId",new Class<?>[0]));
  Assert.assertTrue(Validator.equals(existingPortlet.getPortletId(),ReflectionTestUtil.invoke(existingPortlet,"getOriginalPortletId",new Class<?>[0])));
}
