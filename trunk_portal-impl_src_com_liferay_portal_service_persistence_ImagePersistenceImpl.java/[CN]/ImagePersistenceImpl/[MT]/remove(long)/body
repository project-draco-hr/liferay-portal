{
  Session session=null;
  try {
    session=openSession();
    Image image=(Image)session.get(ImageImpl.class,Long.valueOf(imageId));
    if (image == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + imageId);
      }
      throw new NoSuchImageException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + imageId);
    }
    return imagePersistence.remove(image);
  }
 catch (  NoSuchImageException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
