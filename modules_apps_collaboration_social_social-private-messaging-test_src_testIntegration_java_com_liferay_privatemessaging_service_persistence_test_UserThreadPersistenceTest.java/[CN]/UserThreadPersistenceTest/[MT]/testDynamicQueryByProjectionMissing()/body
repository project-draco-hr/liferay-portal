{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(UserThread.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("userThreadId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("userThreadId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
