{
  UserThread newUserThread=addUserThread();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(UserThread.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("userThreadId"));
  Object newUserThreadId=newUserThread.getUserThreadId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("userThreadId",new Object[]{newUserThreadId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Object existingUserThreadId=result.get(0);
  Assert.assertEquals(existingUserThreadId,newUserThreadId);
}
