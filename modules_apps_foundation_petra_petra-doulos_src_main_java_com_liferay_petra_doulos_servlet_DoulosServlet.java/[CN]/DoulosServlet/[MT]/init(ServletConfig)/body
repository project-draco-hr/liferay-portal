{
  try {
    registerDoulosRequestProcessors();
  }
 catch (  Exception e) {
    throw new ServletException(e);
  }
  String validIpsString=servletConfig.getInitParameter("validIps");
  if (validIpsString != null) {
    _validIps=validIpsString.split(",");
  }
 else {
    _validIps=new String[0];
  }
}
