{
  for (int i=0; i < _databaseTypes.length; i++) {
    String databaseType=_databaseTypes[i];
    if (databaseType.equals(DB.TYPE_HYPERSONIC) || databaseType.equals(DB.TYPE_INTERBASE) || databaseType.equals(DB.TYPE_JDATASTORE)|| databaseType.equals(DB.TYPE_SAP)) {
      continue;
    }
    DB db=DBFactoryUtil.getDB(_databaseTypes[i]);
    if (db != null) {
      db.buildCreateFile(_databaseName,sqlDir);
    }
  }
}
