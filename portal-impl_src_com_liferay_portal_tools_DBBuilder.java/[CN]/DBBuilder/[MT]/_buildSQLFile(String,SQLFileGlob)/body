{
  try (DirectoryStream<Path> sqlFiles=Files.newDirectoryStream(Paths.get(sqlDir),sqlFileGlob.getPattern())){
    for (    Path sqlFile : sqlFiles) {
      Path path=sqlFile.getFileName();
      String fileName=path.toString();
      _generateSQLFile(sqlDir,fileName.replace(".sql",StringPool.BLANK));
    }
  }
 }
