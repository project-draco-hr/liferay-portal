{
  Properties properties=new Properties();
  downloadSampleJobMessages(url.toString() + "/logText/progressiveText",properties,sampleDir);
  JSONObject jsonObject=JenkinsResultsParserUtil.toJSONObject(JenkinsResultsParserUtil.getLocalURL(url.toString() + "/api/json"));
  properties.setProperty("env.BUILD_URL",toURLString(sampleDir));
  properties.setProperty("top.level.result",jsonObject.getString("result"));
  saveProperties(new File(sampleDir,"sample.properties"),properties);
}
