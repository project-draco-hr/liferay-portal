{
  remoteURL=remoteURL.replace("${user.dir}",System.getProperty("user.dir"));
  if (remoteURL.startsWith("file")) {
    remoteURL=remoteURL.replace("?","%3F");
  }
  Matcher matcher=_localURLPattern1.matcher(remoteURL);
  if (matcher.find()) {
    StringBuilder sb=new StringBuilder();
    sb.append("http://test-");
    sb.append(matcher.group(1));
    sb.append("/");
    sb.append(matcher.group(1));
    sb.append("/");
    return remoteURL.replaceAll(matcher.group(0),sb.toString());
  }
  matcher=_localURLPattern2.matcher(remoteURL);
  if (matcher.find()) {
    StringBuilder sb=new StringBuilder();
    sb.append("http://");
    sb.append(matcher.group(1));
    sb.append("/");
    return remoteURL.replaceAll(matcher.group(0),sb.toString());
  }
  return remoteURL;
}
