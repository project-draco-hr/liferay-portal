{
  JSONObject jsonObject=new JSONObject(jsonString);
  if (jsonObject.isNull("duration") || jsonObject.isNull("result") || jsonObject.isNull("url")) {
    return jsonObject;
  }
  String url=jsonObject.getString("url");
  if (!url.contains("AXIS_VARIABLE")) {
    return jsonObject;
  }
  Object result=jsonObject.get("result");
  if (result instanceof JSONObject) {
    return jsonObject;
  }
  if ((jsonObject.getInt("duration") == 0) && result.equals("FAILURE")) {
    String actualResult=getActualResult(url);
    jsonObject.put("result",actualResult == null ? JSONObject.NULL : actualResult);
  }
  return jsonObject;
}
