{
  File file=new File("/tmp/notification_email_body.txt");
  if (file.exists()) {
    file.delete();
  }
  write(file,body);
  StringBuffer sb=new StringBuffer();
  sb.append("cat /tmp/notification_email_body.txt | mail -v -s ");
  sb.append("\"");
  sb.append(subject);
  sb.append("\" -r \"");
  sb.append(from);
  sb.append("\" \"");
  sb.append(to);
  sb.append("\"\n");
  executeBashCommands(sb.toString());
}
