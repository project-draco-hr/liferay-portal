{
  Boolean hasPermission=StagingPermissionUtil.hasPermission(permissionChecker,groupId,RESOURCE_NAME,groupId,PortletKeys.MOBILE_DEVICE_RULES,actionId);
  if (hasPermission != null) {
    return hasPermission.booleanValue();
  }
  return permissionChecker.hasPermission(groupId,RESOURCE_NAME,groupId,actionId);
}
