{
  if (Validator.isNotNull(entryId)) {
    BookmarksEntry entry=BookmarksEntryLocalServiceUtil.getEntry(entryId);
    return getBreadcrumbs(entry.getFolder(),entry,pageContext,res,popUp);
  }
 else {
    BookmarksFolder folder=null;
    try {
      folder=BookmarksFolderLocalServiceUtil.getFolder(folderId);
    }
 catch (    Exception e) {
    }
    return getBreadcrumbs(folder,null,pageContext,res,popUp);
  }
}
