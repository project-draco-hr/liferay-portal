{
  _compare(PQLEntity.fixPQL("(((test)))"),"test");
  _compare(PQLEntity.fixPQL(" (((test)) )"),"test");
  _compare(PQLEntity.fixPQL(" ((( test test ))) "),"test test");
  _compare(PQLEntity.fixPQL(")test("),")test(");
  _compare(PQLEntity.fixPQL(" )test( "),")test(");
  _compare(PQLEntity.fixPQL(" ) test"),") test");
  _compare(PQLEntity.fixPQL(" ( test"),"( test");
  _compare(PQLEntity.fixPQL("test ) "),"test )");
  _compare(PQLEntity.fixPQL("test ( "),"test (");
  _compare(PQLEntity.fixPQL("( (test) OR (test))"),"(test) OR (test)");
  _compare(PQLEntity.fixPQL(" ( (( test ( test ))) "),"( (( test ( test )))");
}
