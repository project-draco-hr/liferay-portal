{
  DistributedRegistry.registerDistributed("name1",Direction.REQUEST,MatchType.EXACT);
  Assert.assertEquals(1,_exactDirections.size());
  Assert.assertTrue(_postfixDirections.isEmpty());
  Assert.assertTrue(_prefixDirections.isEmpty());
  Assert.assertEquals(Direction.REQUEST,_exactDirections.remove("name1"));
  DistributedRegistry.registerDistributed("name2",Direction.RESPONSE,MatchType.POSTFIX);
  Assert.assertEquals(1,_postfixDirections.size());
  Assert.assertTrue(_exactDirections.isEmpty());
  Assert.assertTrue(_prefixDirections.isEmpty());
  Assert.assertEquals(Direction.RESPONSE,_postfixDirections.remove("name2"));
  DistributedRegistry.registerDistributed("name3",Direction.DUPLEX,MatchType.PREFIX);
  Assert.assertEquals(1,_prefixDirections.size());
  Assert.assertTrue(_exactDirections.isEmpty());
  Assert.assertTrue(_postfixDirections.isEmpty());
  Assert.assertEquals(Direction.DUPLEX,_prefixDirections.remove("name3"));
}
