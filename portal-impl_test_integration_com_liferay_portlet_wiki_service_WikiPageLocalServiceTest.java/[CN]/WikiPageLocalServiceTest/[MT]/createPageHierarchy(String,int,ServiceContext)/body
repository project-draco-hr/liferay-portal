{
  List<WikiPage> pages=new ArrayList<WikiPage>();
  for (int i=0; i < level; i++) {
    WikiPage page=WikiTestUtil.addPage(TestPropsValues.getUserId(),_node.getNodeId(),RandomTestUtil.randomString(),RandomTestUtil.randomString(),parentTitle,true,serviceContext);
    parentTitle=page.getTitle();
    pages.add(page);
  }
  return pages;
}
