{
  WikiPage wikiPage1=WikiTestUtil.addPage(_group.getGroupId(),_node.getNodeId(),true);
  WikiPage wikiPage2=WikiTestUtil.addPage(_group.getGroupId(),_node.getNodeId(),true);
  AssetEntry assetEntry=AssetEntryLocalServiceUtil.fetchEntry(WikiPage.class.getName(),wikiPage2.getResourcePrimKey());
  Assert.assertNotNull(assetEntry);
  AssetEntryLocalServiceUtil.deleteAssetEntry(assetEntry);
  List<WikiPage> wikiPages=WikiPageLocalServiceUtil.getNoAssetPages();
  Assert.assertEquals(1,wikiPages.size());
  Assert.assertEquals(wikiPage2.getResourcePrimKey(),wikiPages.get(0).getResourcePrimKey());
}
