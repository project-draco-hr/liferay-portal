{
  List<String> fieldsDisplayValues=new ArrayList<String>();
  for (  String fieldName : fieldNames) {
    if (ddmStructure.isFieldPrivate(fieldName)) {
      continue;
    }
    fieldsDisplayValues.add(fieldName + DDMImpl.INSTANCE_SEPARATOR + StringUtil.randomString());
  }
  Field fieldsDisplayField=new Field(ddmStructure.getStructureId(),DDMImpl.FIELDS_DISPLAY_NAME,StringUtil.merge(fieldsDisplayValues));
  fieldsDisplayField.setDefaultLocale(LocaleUtil.US);
  return fieldsDisplayField;
}
