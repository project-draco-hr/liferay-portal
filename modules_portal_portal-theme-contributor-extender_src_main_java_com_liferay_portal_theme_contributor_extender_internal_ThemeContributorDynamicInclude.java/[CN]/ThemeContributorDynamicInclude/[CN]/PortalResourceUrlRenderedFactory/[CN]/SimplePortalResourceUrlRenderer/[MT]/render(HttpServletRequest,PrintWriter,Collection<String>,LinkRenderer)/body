{
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  String basePath=themeDisplay.getPortalURL() + themeDisplay.getPathContext();
  for (  String resourcePath : resourceUrls) {
    String resourceUrl=PortalUtil.getStaticResourceURL(request,basePath + resourcePath,_themeLastModified);
    linkRenderer.render(printWriter,resourceUrl);
  }
}
