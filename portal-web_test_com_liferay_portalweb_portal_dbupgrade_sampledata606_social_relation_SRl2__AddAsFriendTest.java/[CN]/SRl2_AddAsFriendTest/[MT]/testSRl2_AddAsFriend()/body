{
  selenium.open("/web/socialrelationsn1/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Summary Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Summary Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Add as Friend"),selenium.getText("//p[@class='add-as-friend']/span/a/span"));
  selenium.clickAt("//p[@class='add-as-friend']/span/a/span",RuntimeVariables.replace("Add as Friend"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request processed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
}
