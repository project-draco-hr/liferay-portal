{
  ServiceContext serviceContext=portletDataContext.createServiceContext(ddlRecordSet);
  return _ddlRecordSetLocalService.updateRecordSet(ddlRecordSet.getRecordSetId(),ddlRecordSet.getDDMStructureId(),ddlRecordSet.getNameMap(),ddlRecordSet.getDescriptionMap(),ddlRecordSet.getMinDisplayRows(),serviceContext);
}
