{
  Organization newOrganization=addOrganization();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Organization.class,Organization.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("organizationId",newOrganization.getOrganizationId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Organization existingOrganization=(Organization)result.get(0);
  assertEquals(existingOrganization,newOrganization);
}
