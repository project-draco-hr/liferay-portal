{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Web Content Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_86_keywords",RuntimeVariables.replace("WCD Web Content Test"));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=WCD Web Content Test"));
  selenium.type("_86_keywords",RuntimeVariables.replace("WCD1 Web1 Content1 Test1"));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isElementPresent("link=WCD Web Content Test"));
}
