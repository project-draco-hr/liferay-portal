{
  Long companyIdObj=(Long)req.getAttribute(WebKeys.COMPANY_ID);
  if (companyIdObj != null) {
    return companyIdObj.longValue();
  }
  long companyId=GetterUtil.getLong(CookieUtil.get(req.getCookies(),CookieKeys.COMPANY_ID));
  if (companyId > 0) {
    return companyId;
  }
 else {
    return 0;
  }
}
