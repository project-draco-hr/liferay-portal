  selenium.clickAt("//div[@class='view-all-tasks']/a",RuntimeVariables.replace("View All Tasks"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertEquals(RuntimeVariables.replace("Task1 Description"),selenium.getText("link=Task1 Description"));
  assertEquals(RuntimeVariables.replace("Task2 Description"),selenium.getText("link=Task2 Description"));
  assertEquals(RuntimeVariables.replace("Task3 Description"),selenium.getText("link=Task3 Description"));
