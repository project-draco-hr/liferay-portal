{
  RatingsEntry ratingsEntry=findByPrimaryKey(entryId);
  Session session=null;
  try {
    session=openSession();
    RatingsEntry[] array=new RatingsEntryImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,ratingsEntry,uuid,companyId,orderByComparator,true);
    array[1]=ratingsEntry;
    array[2]=getByUuid_C_PrevAndNext(session,ratingsEntry,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
