{
  long[] folderIds=getFolderIds(groupId,rootFolderId);
  if (folderIds.length == 0) {
    return Collections.EMPTY_LIST;
  }
 else   if (userId <= 0) {
    return dlFileEntryPersistence.filterFindByG_F(groupId,folderIds,start,end,obc);
  }
 else {
    return dlFileEntryPersistence.filterFindByG_U_F(groupId,userId,folderIds,start,end,obc);
  }
}
