{
  DLFileEntry dlFileEntry=getFileEntry(fileEntryId);
  InputStream inputStream=dlLocalService.getFileAsStream(dlFileEntry.getCompanyId(),dlFileEntry.getFolderId(),dlFileEntry.getName());
  addFileEntry(groupId,repositoryId,destFolderId,dlFileEntry.getTitle(),dlFileEntry.getDescription(),null,inputStream,dlFileEntry.getSize(),serviceContext);
}
