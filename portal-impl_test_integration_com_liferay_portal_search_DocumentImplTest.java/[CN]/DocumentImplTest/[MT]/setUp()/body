{
  _indexer=IndexerRegistryUtil.getIndexer(UserIndexer.class);
  _indexer.registerIndexerPostProcessor(new ExtendedUserIndexerPostProcessor());
  populateUsersNumbers();
  for (  String screenName : _userSingleDouble.keySet()) {
    String firstName=screenName.replaceFirst("user",StringPool.BLANK);
    User user=UserTestUtil.addUser(screenName,false,firstName,"Smith",null);
    _indexer.reindex(user);
  }
}
