{
  SearchContext searchContext=buildSearchContext(keywords);
  checkSearchContext(searchContext,SortFactoryUtil.create(field,type,false),ascendingScreenNames);
  String[] descendingScreenNames=Arrays.copyOf(ascendingScreenNames,ascendingScreenNames.length);
  ArrayUtil.reverse(descendingScreenNames);
  checkSearchContext(searchContext,SortFactoryUtil.create(field,type,true),descendingScreenNames);
}
