{
  AccessControlServiceSoapBindingStub accessControlService=WebServiceFactory.getAccessControlService();
  boolean hasPerm=false;
  try {
    AuthenticationUtils.startSession(login,password);
    Reference reference=new Reference(_SPACES_STORE,uuid,null);
    Predicate predicate=new Predicate(new Reference[]{reference},_SPACES_STORE,null);
    HasPermissionsResult[] results=accessControlService.hasPermissions(predicate,new String[]{Constants.WRITE});
    if ((results.length == 1) && results[0].getPermission().equals(Constants.WRITE) && results[0].getAccessStatus().equals(AccessStatus.acepted)) {
      hasPerm=true;
    }
  }
 catch (  Exception e) {
    hasPerm=false;
    _log.warn("Could not start session: \n" + e.getMessage());
  }
 finally {
    AuthenticationUtils.endSession();
  }
  return hasPerm;
}
