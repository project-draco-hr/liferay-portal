{
  try {
    SocialEquityGroupSetting socialEquityGroupSetting=socialEquityGroupSettingPersistence.findByG_C_T(groupId,className,type);
    return socialEquityGroupSetting.getEnabled();
  }
 catch (  NoSuchEquityGroupSettingException e) {
    if (Group.class.getName().equals(className)) {
      return false;
    }
    return true;
  }
}
