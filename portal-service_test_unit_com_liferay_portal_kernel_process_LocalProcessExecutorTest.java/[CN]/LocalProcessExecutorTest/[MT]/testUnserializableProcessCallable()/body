{
  UnserializableProcessCallable unserializableProcessCallable=new UnserializableProcessCallable();
  try {
    _localProcessExecutor.execute(_createJPDAProcessConfig(_JPDA_OPTIONS1),unserializableProcessCallable);
    Assert.fail();
  }
 catch (  ProcessException pe) {
    Throwable throwable=pe.getCause();
    Assert.assertTrue(throwable instanceof NotSerializableException);
  }
}
