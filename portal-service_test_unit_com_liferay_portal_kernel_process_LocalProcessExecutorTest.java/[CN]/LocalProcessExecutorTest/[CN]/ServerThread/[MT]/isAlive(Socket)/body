{
  try {
    InputStream inputStream=socket.getInputStream();
    OutputStream outputStream=socket.getOutputStream();
    outputStream.write(_CODE_ECHO);
    outputStream.flush();
    if (inputStream.read() == _CODE_ECHO) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  Exception e) {
    return false;
  }
}
