{
  UnsyncByteArrayOutputStream unsyncByteArrayOutputStream=new UnsyncByteArrayOutputStream();
  extendedProperties.save(unsyncByteArrayOutputStream,StringPool.BLANK);
  InputStream inputStream=new UnsyncByteArrayInputStream(unsyncByteArrayOutputStream.unsafeGetByteArray(),0,unsyncByteArrayOutputStream.size());
  load(inputStream);
}
