{
  Order order1=webXMLDefinition1.getOrder();
  Order order2=webXMLDefinition2.getOrder();
  if (order1.isOrdered() && !order2.isOrdered()) {
    EnumMap<Path,String[]> routes=order1.getRoutes();
    if (!ArrayUtil.isEmpty(routes.get(Order.Path.AFTER)) && !order1.isBeforeOthers()) {
      return true;
    }
  }
  if (order2.isBefore(webXMLDefinition1.getFragmentName()) || order1.isAfter(webXMLDefinition2.getFragmentName())) {
    return true;
  }
  if (order1.isAfterOthers() && !order1.isBefore(webXMLDefinition2.getFragmentName()) && !(order1.isAfterOthers() && order2.isAfterOthers())) {
    return true;
  }
  if (order2.isBeforeOthers() && !order2.isAfter(webXMLDefinition1.getFragmentName()) && !(order1.isBeforeOthers() && order2.isBeforeOthers())) {
    return true;
  }
  return false;
}
