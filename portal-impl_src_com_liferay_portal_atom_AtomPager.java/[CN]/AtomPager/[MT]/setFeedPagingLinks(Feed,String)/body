{
  FeedPagingHelper.setFirst(feed,AtomUtil.setPageInUrl(url,1));
  FeedPagingHelper.setLast(feed,AtomUtil.setPageInUrl(url,getLastPage()));
  if (getPreviousPage() != 0) {
    FeedPagingHelper.setPrevious(feed,AtomUtil.setPageInUrl(url,getPreviousPage()));
  }
  if (getNextPage() != 0) {
    FeedPagingHelper.setNext(feed,AtomUtil.setPageInUrl(url,getNextPage()));
  }
}
