{
  if (mimeType == null) {
    mimeType=ContentTypes.TEXT_HTML;
  }
  Set<String> mimeTypePortletModes=_portletModes.get(mimeType);
  if (mimeTypePortletModes == null) {
    return false;
  }
  if (mimeTypePortletModes.contains(portletMode.toString())) {
    return true;
  }
 else {
    return false;
  }
}
