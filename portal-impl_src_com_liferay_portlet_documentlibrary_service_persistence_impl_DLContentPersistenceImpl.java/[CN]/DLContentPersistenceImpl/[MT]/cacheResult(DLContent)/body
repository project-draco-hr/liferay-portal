{
  entityCache.putResult(DLContentModelImpl.ENTITY_CACHE_ENABLED,DLContentImpl.class,dlContent.getPrimaryKey(),dlContent);
  finderCache.putResult(FINDER_PATH_FETCH_BY_C_R_P_V,new Object[]{dlContent.getCompanyId(),dlContent.getRepositoryId(),dlContent.getPath(),dlContent.getVersion()},dlContent);
  dlContent.resetOriginalValues();
}
