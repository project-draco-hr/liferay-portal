{
  if (isNew) {
    Object[] args=new Object[]{dlContent.getCompanyId(),dlContent.getRepositoryId(),dlContent.getPath(),dlContent.getVersion()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_C_R_P_V,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_R_P_V,args,dlContent);
  }
 else {
    DLContentModelImpl dlContentModelImpl=(DLContentModelImpl)dlContent;
    if ((dlContentModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_C_R_P_V.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{dlContent.getCompanyId(),dlContent.getRepositoryId(),dlContent.getPath(),dlContent.getVersion()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_C_R_P_V,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_R_P_V,args,dlContent);
    }
  }
}
