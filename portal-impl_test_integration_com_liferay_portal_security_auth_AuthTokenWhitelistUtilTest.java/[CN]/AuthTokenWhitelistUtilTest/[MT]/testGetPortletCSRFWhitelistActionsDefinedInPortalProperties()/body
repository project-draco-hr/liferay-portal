{
  Set<String> portletCSRFWhitelistActions=AuthTokenWhitelistUtil.getPortletCSRFWhitelistActions();
  for (  String authTokenIgnoreAction : PropsValues.AUTH_TOKEN_IGNORE_ACTIONS) {
    Assert.assertTrue("The URL a can't be found in " + portletCSRFWhitelistActions,portletCSRFWhitelistActions.contains(authTokenIgnoreAction));
  }
}
