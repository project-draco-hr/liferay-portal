{
  Portlet portlet=PortletLocalServiceUtil.getPortletById(TestMVCResourceCommand.TEST_PORTLET);
  MockHttpServletRequest request=new MockHttpServletRequest();
  request.setParameter("p_p_id",TestMVCResourceCommand.TEST_PORTLET);
  request.setParameter("p_p_resource_id",TestMVCResourceCommand.TEST_MVC_COMMAND_NAME);
  ThemeDisplay themeDisplay=new ThemeDisplay();
  themeDisplay.setLifecycleResource(true);
  request.setAttribute(WebKeys.THEME_DISPLAY,themeDisplay);
  Assert.assertTrue(AuthTokenWhitelistUtil.isPortletInvocationWhitelisted(request,portlet));
}
