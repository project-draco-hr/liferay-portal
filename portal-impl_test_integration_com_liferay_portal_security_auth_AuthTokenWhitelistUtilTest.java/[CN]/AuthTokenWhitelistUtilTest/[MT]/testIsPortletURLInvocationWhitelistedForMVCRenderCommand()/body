{
  LiferayPortletURL liferayPortletURL=new PortletURLImpl(new MockHttpServletRequest(),TestMVCRenderCommand.TEST_PORTLET_ID,0,PortletRequest.RENDER_PHASE);
  liferayPortletURL.setParameter("mvcRenderCommandName",TestMVCRenderCommand.TEST_MVC_COMMAND_NAME);
  Assert.assertTrue(AuthTokenWhitelistUtil.isPortletURLPortletInvocationWhitelisted(liferayPortletURL));
}
