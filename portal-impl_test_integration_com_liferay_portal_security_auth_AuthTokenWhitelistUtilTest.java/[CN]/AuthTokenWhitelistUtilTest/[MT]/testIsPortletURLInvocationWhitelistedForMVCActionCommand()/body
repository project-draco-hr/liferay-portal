{
  LiferayPortletURL liferayPortletURL=new PortletURLImpl(new MockHttpServletRequest(),TestMVCActionCommand.TEST_PORTLET,0,PortletRequest.ACTION_PHASE);
  liferayPortletURL.setParameter(ActionRequest.ACTION_NAME,TestMVCActionCommand.TEST_MVC_COMMAND_NAME);
  Assert.assertTrue(AuthTokenWhitelistUtil.isPortletURLPortletInvocationWhitelisted(liferayPortletURL));
}
