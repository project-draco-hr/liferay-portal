{
  LiferayPortletURL liferayPortletURL=new PortletURLImpl(new MockHttpServletRequest(),TestMVCResourceCommand.TEST_PORTLET,0,PortletRequest.RESOURCE_PHASE);
  liferayPortletURL.setResourceID(TestMVCResourceCommand.TEST_MVC_COMMAND_NAME);
  Assert.assertTrue(AuthTokenWhitelistUtil.isPortletURLPortletInvocationWhitelisted(liferayPortletURL));
}
