{
  DDMForm ddmStructureDDMForm=DDMFormTestUtil.createDDMForm(_availableLocales,_defaultLocale);
  DDLRecordSet ddlRecordSet=addRecordSet(ddmStructureDDMForm,FailStorageAdapter.STORAGE_TYPE);
  DDMStructure ddmStructure=ddlRecordSet.getDDMStructure();
  Assert.assertEquals(ddmStructure.getStorageType(),FailStorageAdapter.STORAGE_TYPE);
}
