{
  Session session=null;
  try {
    session=openSession();
    SocialActivityLimit socialActivityLimit=(SocialActivityLimit)session.get(SocialActivityLimitImpl.class,Long.valueOf(activityLimitId));
    if (socialActivityLimit == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + activityLimitId);
      }
      throw new NoSuchActivityLimitException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + activityLimitId);
    }
    return socialActivityLimitPersistence.remove(socialActivityLimit);
  }
 catch (  NoSuchActivityLimitException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
