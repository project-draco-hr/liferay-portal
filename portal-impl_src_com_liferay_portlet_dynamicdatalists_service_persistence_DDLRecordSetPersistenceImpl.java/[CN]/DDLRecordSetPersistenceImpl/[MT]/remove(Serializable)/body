{
  Session session=null;
  try {
    session=openSession();
    DDLRecordSet ddlRecordSet=(DDLRecordSet)session.get(DDLRecordSetImpl.class,primaryKey);
    if (ddlRecordSet == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchRecordSetException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(ddlRecordSet);
  }
 catch (  NoSuchRecordSetException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
