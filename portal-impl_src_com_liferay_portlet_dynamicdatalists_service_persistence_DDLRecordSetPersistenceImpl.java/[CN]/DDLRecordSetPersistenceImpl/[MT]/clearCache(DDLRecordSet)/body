{
  EntityCacheUtil.removeResult(DDLRecordSetModelImpl.ENTITY_CACHE_ENABLED,DDLRecordSetImpl.class,ddlRecordSet.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddlRecordSet.getUuid(),Long.valueOf(ddlRecordSet.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_R,new Object[]{Long.valueOf(ddlRecordSet.getGroupId()),ddlRecordSet.getRecordSetKey()});
}
