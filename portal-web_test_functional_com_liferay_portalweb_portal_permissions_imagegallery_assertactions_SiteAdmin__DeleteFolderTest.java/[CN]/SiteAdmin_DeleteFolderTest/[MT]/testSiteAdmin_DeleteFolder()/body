{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Media Gallery Test Page",RuntimeVariables.replace("Media Gallery Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Media Gallery Permissions Test Folder 2 Edited"),selenium.getText("xpath=(//span[@class='image-title'])[2]"));
  selenium.clickAt("xpath=(//span[@class='image-title'])[2]",RuntimeVariables.replace("Media Gallery Permissions Test Folder 2 Edited"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Move to the Recycle Bin"),selenium.getText("//div[@class='lfr-component lfr-menu-list lfr-menu-expanded align-right null']/ul/li[contains(.,'Move to the Recycle Bin')]/a"));
  selenium.click(RuntimeVariables.replace("//div[@class='lfr-component lfr-menu-list lfr-menu-expanded align-right null']/ul/li[contains(.,'Move to the Recycle Bin')]/a"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("The selected item was moved to the Recycle Bin. Undo"),selenium.getText("//div[@class='portlet-msg-success taglib-trash-undo']"));
  assertTrue(selenium.isElementNotPresent("//div[@class='portlet-msg-info']"));
}
