{
  Map<String,Object> map=new LinkedHashMap<String,Object>();
  Map<String,Object> params=new LinkedHashMap<String,Object>();
  map.put("/foo/hello-world",params);
  params.put("userId",Integer.valueOf(173));
  params.put("worldName","Jupiter");
  String command=toJSON(map);
  JSONWebServiceAction jsonWebServiceAction=prepareInvokerAction(command);
  Assert.assertEquals("Welcome 173 to Jupiter",jsonWebServiceAction.invoke());
}
