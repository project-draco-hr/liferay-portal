{
  MockHttpServletRequest mockHttpServletRequest=createHttpRequest("/invoker");
  String command="{\n" + "   \"/foo/hello-world\": {\n" + "       \"userId\": 173,\n"+ "       \"worldName\": \"Jupiter\",\n"+ "   }\n"+ "}";
  mockHttpServletRequest.setParameter("cmd",command);
  JSONWebServiceAction jsonWebServiceAction=new JSONWebServiceInvokerAction(mockHttpServletRequest);
  assertEquals("Welcome 173 to Jupiter",jsonWebServiceAction.invoke());
}
