{
  if (!containsAddTemplatePermission(permissionChecker,groupId,classNameId,resourceClassNameId)) {
    ServiceWrapper<DDMTemplatePermissionSupport> templatePermissionSupportServiceWrapper=_ddmPermissionSupportTracker.getDDMTemplatePermissionSupportServiceWrapper(resourceClassNameId);
    throw new PrincipalException.MustHavePermission(permissionChecker,getResourceName(templatePermissionSupportServiceWrapper,classNameId),groupId,getAddTemplateActionId(templatePermissionSupportServiceWrapper));
  }
}
