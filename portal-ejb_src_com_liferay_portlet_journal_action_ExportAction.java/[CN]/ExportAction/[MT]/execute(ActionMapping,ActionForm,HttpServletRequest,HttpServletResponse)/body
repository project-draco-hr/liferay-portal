{
  try {
    if (OmniadminUtil.isOmniadmin(req.getRemoteUser())) {
      String siteGroupId=ParamUtil.getString(req,"siteGroupId",DEFAULT_SITE_GROUP_ID);
      String cmsGroupId=ParamUtil.getString(req,"cmsGroupId",DEFAULT_CMS_GROUP_ID);
      ZipCollection zipCol=new ZipCollection();
      List journalContentSearches=new ArrayList();
      insertDataCMSLayout(siteGroupId,zipCol,journalContentSearches);
      insertDataCMSContent(cmsGroupId,zipCol,journalContentSearches);
      insertDataImage(zipCol);
      String fileName="journal.zip";
      ServletResponseUtil.sendFile(res,fileName,zipCol.finish());
    }
    return null;
  }
 catch (  Exception e) {
    req.setAttribute(PageContext.EXCEPTION,e);
    return mapping.findForward(Constants.COMMON_ERROR);
  }
}
