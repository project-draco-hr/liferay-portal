{
  String expected="<div class=\"lfr-code\"><table><tbody><tr>" + "<td class=\"line-numbers\" data-line-number=\"1\"></td>" + "<td class=\"lines\"><div class=\"line\">:)</div>"+ "</td></tr></tbody></table></div>";
  String actual=_htmlBBCodeTranslator.parse("[code]:)[/code]");
  Assert.assertEquals(expected,actual);
}
