{
  String insuranceFormula=ParamUtil.get(req,"config_i",ShoppingConfig.DEFAULT_INSURANCE_FORMULA);
  String[] insurance=new String[5];
  for (int i=0; i < insurance.length; i++) {
    insurance[i]=Double.toString(ParamUtil.get(req,"config_i_" + i,0.0));
  }
  shoppingConfig.setInsuranceFormula(insuranceFormula);
  shoppingConfig.setInsurance(insurance);
}
