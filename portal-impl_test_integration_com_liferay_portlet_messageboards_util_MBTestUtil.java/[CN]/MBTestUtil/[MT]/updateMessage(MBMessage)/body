{
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext(message.getGroupId());
  serviceContext.setCommand(Constants.UPDATE);
  serviceContext.setLayoutFullURL("http://localhost");
  return MBMessageLocalServiceUtil.updateMessage(TestPropsValues.getUserId(),message.getMessageId(),ServiceTestUtil.randomString(),ServiceTestUtil.randomString(50),Collections.<ObjectValuePair<String,InputStream>>emptyList(),Collections.<String>emptyList(),message.getPriority(),message.isAllowPingbacks(),serviceContext);
}
