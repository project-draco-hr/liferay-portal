{
  if (isNew) {
    Object[] args=new Object[]{layoutRevision.getLayoutSetBranchId(),layoutRevision.getHead(),layoutRevision.getPlid()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_L_H_P,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_L_H_P,args,layoutRevision);
  }
 else {
    LayoutRevisionModelImpl layoutRevisionModelImpl=(LayoutRevisionModelImpl)layoutRevision;
    if ((layoutRevisionModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_L_H_P.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{layoutRevision.getLayoutSetBranchId(),layoutRevision.getHead(),layoutRevision.getPlid()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_L_H_P,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_L_H_P,args,layoutRevision);
    }
  }
}
