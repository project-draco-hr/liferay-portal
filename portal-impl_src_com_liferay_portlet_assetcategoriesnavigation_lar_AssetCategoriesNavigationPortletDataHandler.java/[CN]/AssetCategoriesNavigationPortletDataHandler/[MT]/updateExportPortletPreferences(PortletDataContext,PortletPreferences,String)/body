{
  Portlet portlet=PortletLocalServiceUtil.getPortletById(portletDataContext.getCompanyId(),portletId);
  Enumeration<String> enu=portletPreferences.getNames();
  while (enu.hasMoreElements()) {
    String name=enu.nextElement();
    if (name.equals("assetVocabularyIds")) {
      ExportImportHelperUtil.updateExportPortletPreferencesClassPKs(portletDataContext,portlet,portletPreferences,name,AssetVocabulary.class.getName(),portletDataContext.getExportDataRootElement());
    }
  }
  return portletPreferences;
}
