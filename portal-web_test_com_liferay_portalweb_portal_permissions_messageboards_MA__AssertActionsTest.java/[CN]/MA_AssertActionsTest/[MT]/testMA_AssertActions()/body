  assertTrue(selenium.isVisible("//input[@value='Save']"));
  selenium.open("/web/site-name/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Message Boards Permissions Page")) {
        break;
      }
    }
 catch (    Exception e) {
