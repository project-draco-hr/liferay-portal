{
  try {
    if (_folder.getFolderId() == BookmarksFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
      return false;
    }
    if (BookmarksFolderPermissionChecker.contains(themeDisplay.getPermissionChecker(),_folder,ActionKeys.UPDATE)) {
      return true;
    }
  }
 catch (  PortalException pe) {
  }
  return false;
}
