{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(portletRequest,BookmarksPortletKeys.BOOKMARKS_ADMIN,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcRenderCommandName","/bookmarks/move_entry");
  portletURL.setParameter("redirect",PortalUtil.getCurrentURL(portletRequest));
  portletURL.setParameter("rowIdsBookmarksFolder",String.valueOf(_folder.getFolderId()));
  return portletURL.toString();
}
