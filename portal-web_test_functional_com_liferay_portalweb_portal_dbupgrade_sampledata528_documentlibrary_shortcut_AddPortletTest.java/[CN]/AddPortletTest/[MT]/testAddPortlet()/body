{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/document-library-shortcut-community/");
  selenium.clickAt("link=Document Library Page",RuntimeVariables.replace("Document Library Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//h2[@class='user-greeting']/span","Welcome"));
  selenium.mouseOver("//h2[@class='user-greeting']/span");
  selenium.clickAt("//h2[@class='user-greeting']/span",RuntimeVariables.replace("Welcome"));
  selenium.waitForVisible("link=Add Application");
  selenium.clickAt("link=Add Application",RuntimeVariables.replace("Add Application"));
  selenium.waitForVisible("//input[@id='layout_configuration_content']");
  selenium.sendKeys("//input[@id='layout_configuration_content']",RuntimeVariables.replace("d"));
  selenium.waitForVisible("//div[@title='Document Library']/p/a");
  selenium.clickAt("//div[@title='Document Library']/p/a",RuntimeVariables.replace("Add"));
  selenium.waitForElementPresent("//td[1]/div/div[1]/div");
  assertTrue(selenium.isElementPresent("//td[1]/div/div[1]/div"));
}
