{
  String cmd=ParamUtil.getString(req,Constants.CMD);
  try {
    if (cmd.equals(Constants.ADD) || cmd.equals(Constants.UPDATE)) {
      updatePage(req);
    }
 else     if (cmd.equals(Constants.DELETE)) {
      deletePage(req);
    }
 else     if (cmd.equals(Constants.REVERT)) {
      revertPage(req);
    }
    if (Validator.isNotNull(cmd)) {
      sendRedirect(req,res);
    }
  }
 catch (  Exception e) {
    if (e != null && e instanceof NoSuchNodeException || e instanceof NoSuchPageException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      setForward(req,"portlet.wiki.error");
    }
 else     if (e != null && e instanceof PageContentException || e instanceof PageTitleException) {
      SessionErrors.add(req,e.getClass().getName());
    }
 else {
      throw e;
    }
  }
}
