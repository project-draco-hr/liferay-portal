{
  if (_servletContext != null) {
    RequestDispatcher requestDispatcher=_servletContext.getRequestDispatcher(_path);
    StringServletResponse stringResponse=new StringServletResponse(_response);
    requestDispatcher.include(_request,stringResponse);
    pageContext.getOut().print(stringResponse.getString());
  }
 else {
    pageContext.include(_path);
  }
}
