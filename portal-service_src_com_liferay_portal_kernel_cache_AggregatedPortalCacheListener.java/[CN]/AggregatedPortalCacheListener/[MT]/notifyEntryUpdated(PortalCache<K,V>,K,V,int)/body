{
  for (  Map.Entry<PortalCacheListener<K,V>,PortalCacheListenerScope> entry : _cacheListeners.entrySet()) {
    PortalCacheListener<K,V> cacheListener=entry.getKey();
    if (_shouldDeliver(cacheListener,entry.getValue())) {
      cacheListener.notifyEntryUpdated(portalCache,key,value,timeToLive);
    }
  }
}
