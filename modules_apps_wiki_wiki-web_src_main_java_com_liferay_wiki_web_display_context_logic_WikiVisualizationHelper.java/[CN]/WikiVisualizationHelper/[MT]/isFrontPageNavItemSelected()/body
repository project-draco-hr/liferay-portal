{
  WikiPage wikiPage=_wikiRequestHelper.getWikiPage();
  String mvcRenderCommandName=_wikiRequestHelper.getMVCrenderCommandName();
  String frontPageName=_wikiGroupServiceConfiguration.frontPageName();
  if (Validator.isNull(mvcRenderCommandName) || (wikiPage != null) && frontPageName.equals(wikiPage.getTitle())) {
    return true;
  }
  return false;
}
