{
  List<DLFolder> dlFolders=_dlFolderLocalService.getDLFoldersByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<DLFolder>());
  List<Folder> folders=new ArrayList<>();
  for (  DLFolder dlFolder : dlFolders) {
    folders.add(new LiferayFolder(dlFolder));
  }
  return folders;
}
