{
  String url=StringPool.BLANK;
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    url=PermissionsURLTag.doTag(null,DLFileEntryConstants.getClassName(),HtmlUtil.unescape(_fileEntry.getTitle()),null,String.valueOf(_fileEntry.getFileEntryId()),LiferayWindowState.POP_UP.toString(),null,themeDisplay.getRequest());
  }
 catch (  Exception e) {
  }
  return url;
}
