{
  WikiPage page=fetchLatestPage(resourcePrimKey,nodeId,status,preferApproved);
  if (page == null) {
    StringBundler sb=new StringBundler(7);
    sb.append("{resourcePrimKey=");
    sb.append(resourcePrimKey);
    sb.append(", nodeId=");
    sb.append(nodeId);
    sb.append(", status=");
    sb.append(status);
    sb.append("}");
    throw new NoSuchPageException(sb.toString());
  }
  return page;
}
