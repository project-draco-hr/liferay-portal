{
  portletURL.setParameter("displayStyle",displayStyle);
  portletURL.setParameter("folderId",String.valueOf(DLFolderConstants.DEFAULT_PARENT_FOLDER_ID));
  PortalUtil.addPortletBreadcrumbEntry(request,LanguageUtil.get(request,"home"),portletURL.toString());
  if (folderId != DLFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    DLUtil.addPortletBreadcrumbEntries(DLAppServiceUtil.getFolder(folderId),request,portletURL);
  }
}
