{
  EntityCacheUtil.removeResult(DDLRecordVersionModelImpl.ENTITY_CACHE_ENABLED,DDLRecordVersionImpl.class,ddlRecordVersion.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_R_V,new Object[]{Long.valueOf(ddlRecordVersion.getRecordId()),ddlRecordVersion.getVersion()});
}
