{
  String serializedSettingsDDMFormValues=ParamUtil.getString(portletRequest,"serializedSettingsDDMFormValues");
  DDMForm ddmForm=DDMFormFactory.create(DDLRecordSetSettings.class);
  DDMFormValues settingsDDMFormValues=ddmFormValuesJSONDeserializer.deserialize(ddmForm,serializedSettingsDDMFormValues);
  return settingsDDMFormValues;
}
