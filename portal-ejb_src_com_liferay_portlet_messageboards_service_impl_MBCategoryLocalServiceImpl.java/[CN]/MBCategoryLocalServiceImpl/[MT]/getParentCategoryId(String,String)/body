{
  if (!parentCategoryId.equals(MBCategory.DEFAULT_PARENT_CATEGORY_ID)) {
    MBCategory parentCategory=MBCategoryUtil.fetchByPrimaryKey(parentCategoryId);
    if ((parentCategory == null) || (!companyId.equals(parentCategory.getCompanyId()))) {
      parentCategoryId=MBCategory.DEFAULT_PARENT_CATEGORY_ID;
    }
  }
  return parentCategoryId;
}
