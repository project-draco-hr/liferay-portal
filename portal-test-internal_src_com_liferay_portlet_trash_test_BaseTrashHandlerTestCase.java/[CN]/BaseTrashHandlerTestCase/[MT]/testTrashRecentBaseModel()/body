{
  Assume.assumeTrue(this instanceof WhenHasRecentBaseModelCount);
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  BaseModel<?> parentBaseModel=getParentBaseModel(group,serviceContext);
  int initialBaseModelsCount=((WhenHasRecentBaseModelCount)this).getRecentBaseModelsCount(group.getGroupId());
  addBaseModel(parentBaseModel,true,serviceContext);
  Assert.assertEquals(initialBaseModelsCount + 1,((WhenHasRecentBaseModelCount)this).getRecentBaseModelsCount(group.getGroupId()));
  moveParentBaseModelToTrash((Long)parentBaseModel.getPrimaryKeyObj());
  Assert.assertEquals(initialBaseModelsCount,((WhenHasRecentBaseModelCount)this).getRecentBaseModelsCount(group.getGroupId()));
}
