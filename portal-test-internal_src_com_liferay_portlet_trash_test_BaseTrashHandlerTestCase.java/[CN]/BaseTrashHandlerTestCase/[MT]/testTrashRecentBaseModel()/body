{
  Assume.assumeTrue(this instanceof WhenHasParent);
  Assume.assumeTrue(this instanceof WhenHasRecentBaseModelCount);
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  BaseModel<?> parentBaseModel=getParentBaseModel(group,serviceContext);
  WhenHasRecentBaseModelCount whenHasRecentBaseModelCount=(WhenHasRecentBaseModelCount)this;
  int initialBaseModelsCount=whenHasRecentBaseModelCount.getRecentBaseModelsCount(group.getGroupId());
  addBaseModel(parentBaseModel,true,serviceContext);
  Assert.assertEquals(initialBaseModelsCount + 1,whenHasRecentBaseModelCount.getRecentBaseModelsCount(group.getGroupId()));
  moveParentBaseModelToTrash((Long)parentBaseModel.getPrimaryKeyObj());
  Assert.assertEquals(initialBaseModelsCount,whenHasRecentBaseModelCount.getRecentBaseModelsCount(group.getGroupId()));
}
