{
  Assume.assumeTrue(this instanceof WhenHasRecentBaseModelCount);
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  BaseModel<?> parentBaseModel=getParentBaseModel(group,serviceContext);
  WhenHasRecentBaseModelCount whenHasRecentBaseModelCountInstance=(WhenHasRecentBaseModelCount)this;
  int initialBaseModelsCount=whenHasRecentBaseModelCountInstance.getRecentBaseModelsCount(group.getGroupId());
  addBaseModel(parentBaseModel,true,serviceContext);
  Assert.assertEquals(initialBaseModelsCount + 1,whenHasRecentBaseModelCountInstance.getRecentBaseModelsCount(group.getGroupId()));
  moveParentBaseModelToTrash((Long)parentBaseModel.getPrimaryKeyObj());
  Assert.assertEquals(initialBaseModelsCount,whenHasRecentBaseModelCountInstance.getRecentBaseModelsCount(group.getGroupId()));
}
