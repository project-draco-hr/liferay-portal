{
  for (int second=0; ; second++) {
    if (second >= TestPropsValues.TIMEOUT_EXPLICIT_WAIT) {
      BaseTestCase.fail("Timeout: Still able to find the Text \"" + value + "\"");
    }
    try {
      if (!isTextPresent(value)) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
