{
  for (int second=0; ; second++) {
    if (second >= TestPropsValues.TIMEOUT_EXPLICIT_WAIT) {
      BaseTestCase.fail("Timeout: Still able to find the Locator \"" + locator + "\"");
    }
    try {
      if (!isVisible(locator)) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
