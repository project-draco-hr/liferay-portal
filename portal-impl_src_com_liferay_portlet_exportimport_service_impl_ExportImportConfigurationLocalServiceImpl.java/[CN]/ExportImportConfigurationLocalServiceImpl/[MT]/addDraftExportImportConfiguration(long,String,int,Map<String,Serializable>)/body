{
  long groupId=GetterUtil.getLong(settingsMap.get("sourceGroupId"));
  if ((type == ExportImportConfigurationConstants.TYPE_IMPORT_LAYOUT) || (type == ExportImportConfigurationConstants.TYPE_IMPORT_PORTLET)) {
    groupId=GetterUtil.getLong(settingsMap.get("targetGroupId"));
  }
  return exportImportConfigurationLocalService.addExportImportConfiguration(userId,groupId,name,StringPool.BLANK,type,settingsMap,WorkflowConstants.STATUS_DRAFT,new ServiceContext());
}
