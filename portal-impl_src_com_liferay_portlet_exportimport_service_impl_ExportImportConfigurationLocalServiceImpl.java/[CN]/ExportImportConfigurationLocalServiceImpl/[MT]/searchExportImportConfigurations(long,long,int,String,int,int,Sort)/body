{
  String description=null;
  String name=null;
  boolean andOperator=false;
  if (Validator.isNotNull(keywords)) {
    description=keywords;
    name=keywords;
  }
 else {
    andOperator=true;
  }
  return searchExportImportConfigurations(companyId,groupId,type,name,description,andOperator,start,end,sort);
}
