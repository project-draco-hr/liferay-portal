{
  try {
    WikiPage returnValue=WikiPageServiceUtil.addPage(nodeId,title,content,summary,minorEdit,format,parentTitle,redirectTitle,serviceContext);
    return WikiPageSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
