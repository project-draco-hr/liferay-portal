{
  JSONObject jsonObj=JSONFactoryUtil.createJSONObject();
  jsonObj.put("uuid",model.getUuid());
  jsonObj.put("mailingListId",model.getMailingListId());
  jsonObj.put("groupId",model.getGroupId());
  jsonObj.put("companyId",model.getCompanyId());
  jsonObj.put("userId",model.getUserId());
  jsonObj.put("userName",model.getUserName());
  Date createDate=model.getCreateDate();
  String createDateJSON=StringPool.BLANK;
  if (createDate != null) {
    createDateJSON=String.valueOf(createDate.getTime());
  }
  jsonObj.put("createDate",createDateJSON);
  Date modifiedDate=model.getModifiedDate();
  String modifiedDateJSON=StringPool.BLANK;
  if (modifiedDate != null) {
    modifiedDateJSON=String.valueOf(modifiedDate.getTime());
  }
  jsonObj.put("modifiedDate",modifiedDateJSON);
  jsonObj.put("categoryId",model.getCategoryId());
  jsonObj.put("emailAddress",model.getEmailAddress());
  jsonObj.put("inProtocol",model.getInProtocol());
  jsonObj.put("inServerName",model.getInServerName());
  jsonObj.put("inServerPort",model.getInServerPort());
  jsonObj.put("inUseSSL",model.getInUseSSL());
  jsonObj.put("inUserName",model.getInUserName());
  jsonObj.put("inPassword",model.getInPassword());
  jsonObj.put("inReadInterval",model.getInReadInterval());
  jsonObj.put("outEmailAddress",model.getOutEmailAddress());
  jsonObj.put("outCustom",model.getOutCustom());
  jsonObj.put("outServerName",model.getOutServerName());
  jsonObj.put("outServerPort",model.getOutServerPort());
  jsonObj.put("outUseSSL",model.getOutUseSSL());
  jsonObj.put("outUserName",model.getOutUserName());
  jsonObj.put("outPassword",model.getOutPassword());
  jsonObj.put("active",model.getActive());
  return jsonObj;
}
