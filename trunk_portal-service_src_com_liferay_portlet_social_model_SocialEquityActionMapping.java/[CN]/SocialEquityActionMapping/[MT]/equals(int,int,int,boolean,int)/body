{
  if (_unique != unique) {
    return false;
  }
  if (type == SocialEquitySettingConstants.TYPE_INFORMATION) {
    if ((_informationDailyLimit != dailyLimit) || (_informationLifespan != lifeSpan) || (_informationValue != value)) {
      return false;
    }
  }
 else {
    if ((_participationDailyLimit != dailyLimit) || (_participationLifespan != lifeSpan) || (_participationValue != value)) {
      return false;
    }
  }
  return true;
}
