{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ShoppingItem.class,ShoppingItem.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("itemId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("itemId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
