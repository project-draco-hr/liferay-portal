{
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  long bundleLastModified=_bundle.getLastModified();
  String basePath=themeDisplay.getPortalURL() + themeDisplay.getPathContext() + _contextPath;
  for (  String resourcePath : _cssResourcePaths) {
    PrintWriter printWriter=response.getWriter();
    StringBundler sb=new StringBundler(3);
    String resourceUrl=PortalUtil.getStaticResourceURL(request,basePath + resourcePath,bundleLastModified);
    sb.append("<link href=\"");
    sb.append(resourceUrl);
    sb.append("\" rel=\"stylesheet\" type = \"text/css\" />");
    printWriter.println(sb.toString());
  }
  for (  String resourcePath : _jsResourcePaths) {
    PrintWriter printWriter=response.getWriter();
    StringBundler sb=new StringBundler(3);
    String resourceUrl=PortalUtil.getStaticResourceURL(request,basePath + resourcePath,bundleLastModified);
    sb.append("<script src=\"");
    sb.append(resourceUrl);
    sb.append("\" type = \"text/javascript\"></script>");
    printWriter.println(sb.toString());
  }
}
