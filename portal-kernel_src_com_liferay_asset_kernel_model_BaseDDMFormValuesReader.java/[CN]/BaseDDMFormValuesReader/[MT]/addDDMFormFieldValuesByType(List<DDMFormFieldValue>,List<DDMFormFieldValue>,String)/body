{
  for (  DDMFormFieldValue ddmFormFieldValue : ddmFormFieldValues) {
    if (ddmFormFieldType.equals(ddmFormFieldValue.getType())) {
      filteredDDMFormFieldValues.add(ddmFormFieldValue);
    }
    addDDMFormFieldValuesByType(ddmFormFieldValue.getNestedDDMFormFieldValues(),filteredDDMFormFieldValues,ddmFormFieldType);
  }
}
