{
  Registry registry=RegistryUtil.getRegistry();
  ExportImportLifecycleListener exportImportLifecycleListener=registry.getService(serviceReference);
  if (exportImportLifecycleListener.isParallel()) {
    _asyncExportImportLifecycleListeners.add(exportImportLifecycleListener);
  }
 else {
    _syncExportImportLifecycleListeners.add(exportImportLifecycleListener);
  }
  return exportImportLifecycleListener;
}
