{
  folderName=DigesterUtil.digestHex(Digester.SHA_256,folderName);
  TemporaryFileEntriesCapability temporaryFileEntriesCapability=_getTemporaryFileEntriesCapability(groupId);
  return temporaryFileEntriesCapability.getTemporaryFileEntry(new TemporaryFileEntriesScope(_UUID,userId,folderName),fileName);
}
