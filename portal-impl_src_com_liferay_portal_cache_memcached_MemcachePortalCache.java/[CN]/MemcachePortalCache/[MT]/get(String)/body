{
  Object cachedObject=null;
  Future<Object> future=_memcachedClient.asyncGet(key);
  try {
    cachedObject=future.get(_timeout,_timeoutTimeUnit);
  }
 catch (  Throwable e) {
    if (_log.isWarnEnabled()) {
      _log.warn("Memcache operation error",e);
    }
    future.cancel(true);
  }
  return cachedObject;
}
