{
  CalendarResource calendarResource=findByPrimaryKey(calendarResourceId);
  Session session=null;
  try {
    session=openSession();
    CalendarResource[] array=new CalendarResourceImpl[3];
    array[0]=getByGroupId_PrevAndNext(session,calendarResource,groupId,orderByComparator,true);
    array[1]=calendarResource;
    array[2]=getByGroupId_PrevAndNext(session,calendarResource,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
