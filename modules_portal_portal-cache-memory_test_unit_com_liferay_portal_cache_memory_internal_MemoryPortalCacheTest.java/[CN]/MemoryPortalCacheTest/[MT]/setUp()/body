{
  PortalCacheManager<String,String> portalCacheManager=TestPortalCacheManager.createTestPortalCacheManager(_CACHE_MANAGER_NAME);
  _memoryPortalCache=new MemoryPortalCache<>(portalCacheManager,_CACHE_NAME,16);
  _memoryPortalCache.put(_KEY_1,_VALUE_1);
  _defaultCacheListener=new TestCacheListener<>();
