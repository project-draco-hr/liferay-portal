{
  objectOutput.writeLong(id);
  objectOutput.writeLong(companyId);
  if (portletId == null) {
    objectOutput.writeUTF(StringPool.BLANK);
  }
 else {
    objectOutput.writeUTF(portletId);
  }
  if (roles == null) {
    objectOutput.writeUTF(StringPool.BLANK);
  }
 else {
    objectOutput.writeUTF(roles);
  }
  objectOutput.writeBoolean(active);
}
