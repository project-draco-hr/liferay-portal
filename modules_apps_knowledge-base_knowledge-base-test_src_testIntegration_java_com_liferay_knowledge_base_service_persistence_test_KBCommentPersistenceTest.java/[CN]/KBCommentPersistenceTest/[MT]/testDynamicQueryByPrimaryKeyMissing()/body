{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(KBComment.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("kbCommentId",RandomTestUtil.nextLong()));
  List<KBComment> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
