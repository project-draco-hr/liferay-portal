{
  KBComment newKBComment=addKBComment();
  _persistence.clearCache();
  KBComment existingKBComment=_persistence.findByPrimaryKey(newKBComment.getPrimaryKey());
  Assert.assertTrue(Objects.equals(existingKBComment.getUuid(),ReflectionTestUtil.invoke(existingKBComment,"getOriginalUuid",new Class<?>[0])));
  Assert.assertEquals(Long.valueOf(existingKBComment.getGroupId()),ReflectionTestUtil.<Long>invoke(existingKBComment,"getOriginalGroupId",new Class<?>[0]));
}
