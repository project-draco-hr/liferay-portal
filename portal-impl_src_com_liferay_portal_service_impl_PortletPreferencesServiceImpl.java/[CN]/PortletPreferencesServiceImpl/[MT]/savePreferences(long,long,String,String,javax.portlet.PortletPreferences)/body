{
  GroupPermissionUtil.check(getPermissionChecker(),groupId,ActionKeys.MANAGE_SAVED_SETUPS);
  PortletItem portletItem=portletItemLocalService.updatePortletItem(userId,groupId,portletId,PortletPreferences.class.getName(),name);
  long ownerId=portletItem.getPortletItemId();
  int ownerType=PortletKeys.PREFS_OWNER_TYPE_SAVED;
  long plid=0;
  javax.portlet.PortletPreferences savedSetup=PortletPreferencesLocalServiceUtil.getPreferences(portletItem.getCompanyId(),ownerId,ownerType,plid,portletId,StringPool.BLANK);
  copyPreferences(setup,savedSetup);
}
