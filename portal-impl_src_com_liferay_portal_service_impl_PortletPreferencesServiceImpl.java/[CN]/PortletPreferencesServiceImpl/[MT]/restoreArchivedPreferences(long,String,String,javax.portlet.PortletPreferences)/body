{
  GroupPermissionUtil.check(getPermissionChecker(),groupId,ActionKeys.MANAGE_ARCHIVED_SETUPS);
  PortletItem portletItem=portletItemLocalService.getPortletItem(groupId,name,portletId,PortletPreferences.class.getName());
  long ownerId=portletItem.getPortletItemId();
  int ownerType=PortletKeys.PREFS_OWNER_TYPE_SAVED;
  long plid=0;
  javax.portlet.PortletPreferences archivedPrefs=portletPreferencesLocalService.getPreferences(portletItem.getCompanyId(),ownerId,ownerType,plid,portletId);
  copyPreferences(archivedPrefs,prefs);
}
