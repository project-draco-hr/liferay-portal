{
  List<String> fileNames=new ArrayList<>();
  String line=null;
  int gitLevel=getGitLevel(baseDirName);
  UnsyncBufferedReader unsyncBufferedReader=getGitCommandReader("git diff --diff-filter=AM --name-only " + commitId);
  while ((line=unsyncBufferedReader.readLine()) != null) {
    if (StringUtil.count(line,CharPool.SLASH) >= gitLevel) {
      fileNames.add(getFileName(line,gitLevel));
    }
  }
  return fileNames;
}
