{
  List<Phone> phones=PhoneLocalServiceUtil.getPhones(user.getCompanyId(),com.liferay.portal.kernel.model.Contact.class.getName(),user.getContactId());
  for (  Phone phone : phones) {
    ListType listType=phone.getType();
    if (!listTypeName.equals(listType.getName())) {
      continue;
    }
    TelephoneNum telephoneNum=new TelephoneNum();
    telephoneNum.setExt(phone.getExtension());
    telephoneNum.setNumber(phone.getNumber());
    return telephoneNum;
  }
  return null;
}
