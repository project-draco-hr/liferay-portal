{
  List<Website> websites=WebsiteLocalServiceUtil.getWebsites(user.getCompanyId(),com.liferay.portal.kernel.model.Contact.class.getName(),user.getContactId());
  for (  Website website : websites) {
    ListType listType=website.getType();
    if (!listTypeName.equals(listType.getName())) {
      continue;
    }
    return website.getUrl();
  }
  return null;
}
