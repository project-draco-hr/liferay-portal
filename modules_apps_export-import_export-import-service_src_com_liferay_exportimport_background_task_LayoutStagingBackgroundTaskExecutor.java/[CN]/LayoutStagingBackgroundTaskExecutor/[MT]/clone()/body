{
  LayoutStagingBackgroundTaskExecutor layoutStagingBackgroundTaskExecutor=new LayoutStagingBackgroundTaskExecutor();
  layoutStagingBackgroundTaskExecutor.setBackgroundTaskStatusMessageTranslator(getBackgroundTaskStatusMessageTranslator());
  layoutStagingBackgroundTaskExecutor.setIsolationLevel(getIsolationLevel());
