{
  long oldCodeId=getCodeId(companyId,"com.liferay.portal.model.Location",scope);
  long newCodeId=getCodeId(companyId,"com.liferay.portal.model.Organization",scope);
  runSQL("update Resource_ set codeId = " + newCodeId + " where codeId = "+ oldCodeId);
  runSQL("delete from ResourceCode where codeId = " + oldCodeId);
}
