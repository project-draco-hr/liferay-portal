{
  Calendar losAngelesJCalendar=CalendarFactoryUtil.getCalendar(randomYear(),randomMonth(),randomDayOfMonth(),randomHour(),randomMinute(),randomSecond(),randomMillisecond(),_losAngelesTimeZone);
  Calendar madridJCalendar=JCalendarUtil.getJCalendar(losAngelesJCalendar,_madridTimeZone);
  Assert.assertEquals(_madridTimeZone,madridJCalendar.getTimeZone());
  Assert.assertEquals(losAngelesJCalendar.getTimeInMillis(),madridJCalendar.getTimeInMillis());
}
