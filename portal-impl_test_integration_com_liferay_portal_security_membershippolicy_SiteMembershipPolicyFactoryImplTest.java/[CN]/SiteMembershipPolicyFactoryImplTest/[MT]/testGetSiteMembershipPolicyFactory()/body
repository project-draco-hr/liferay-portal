{
  SiteMembershipPolicyFactory siteMembershipPolicyFactory=SiteMembershipPolicyFactoryUtil.getSiteMembershipPolicyFactory();
  SiteMembershipPolicy siteMembershipPolicy=siteMembershipPolicyFactory.getSiteMembershipPolicy();
  Class<?> clazz=siteMembershipPolicy.getClass();
  Assert.assertEquals(TestSiteMembershipPolicy.class.getName(),clazz.getName());
}
