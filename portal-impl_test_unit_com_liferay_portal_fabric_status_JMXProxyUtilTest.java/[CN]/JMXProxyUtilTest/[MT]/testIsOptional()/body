{
class TestOptional {
    @Optional public void method1(){
    }
    @SuppressWarnings("unused") public void method2(){
    }
  }
  Assert.assertTrue(JMXProxyUtil.isOptional(TestOptional.class.getMethod("method1")));
  Assert.assertFalse(JMXProxyUtil.isOptional(TestOptional.class.getMethod("method2")));
}
