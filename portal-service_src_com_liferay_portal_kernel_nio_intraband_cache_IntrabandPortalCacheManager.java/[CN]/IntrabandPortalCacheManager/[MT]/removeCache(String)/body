{
  _portalCaches.remove(name);
  Serializer serializer=new Serializer();
  serializer.writeInt(PortalCacheActionType.REMOVE_CACHE.ordinal());
  serializer.writeString(name);
  SystemDataType systemDataType=SystemDataType.PORTAL_CACHE;
  _intraband.sendDatagram(_registrationReference,Datagram.createRequestDatagram(systemDataType.getValue(),serializer.toByteBuffer()));
}
