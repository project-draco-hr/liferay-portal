{
  try {
    BasicAuthHeaderAutoLoginConfiguration basicAuthHeaderAutoLoginConfiguration=_moduleConfigurationFactory.getModuleConfiguration(BasicAuthHeaderAutoLoginConfiguration.class,new CompanyServiceSettingsLocator(companyId,BasicAuthHeaderAutoLoginConstants.SERVICE_NAME));
    return basicAuthHeaderAutoLoginConfiguration;
  }
 catch (  ModuleConfigurationException mce) {
    _log.error("Unable to get basic auth header configuration",mce);
  }
  return null;
}
