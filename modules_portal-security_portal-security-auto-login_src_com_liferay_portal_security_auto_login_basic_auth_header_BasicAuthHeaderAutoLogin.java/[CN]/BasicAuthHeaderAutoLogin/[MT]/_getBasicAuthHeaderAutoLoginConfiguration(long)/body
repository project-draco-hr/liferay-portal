{
  try {
    BasicAuthHeaderAutoLoginConfiguration basicAuthHeaderAutoLoginConfiguration=_configurationFactory.getConfiguration(BasicAuthHeaderAutoLoginConfiguration.class,new CompanyServiceSettingsLocator(companyId,BasicAuthHeaderAutoLoginConstants.SERVICE_NAME));
    return basicAuthHeaderAutoLoginConfiguration;
  }
 catch (  ConfigurationException mce) {
    _log.error("Unable to get basic auth header configuration",mce);
  }
  return null;
}
