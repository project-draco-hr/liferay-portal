{
  AssetRendererFactory<DLFileEntry> assetRendererFactory=getAssetRendererFactory();
  if (assetRendererFactory != null) {
    DLFileVersion dlFileVersion=DLFileVersionLocalServiceUtil.getFileVersion(classPK);
    return assetRendererFactory.getAssetRenderer(dlFileVersion.getFileEntryId(),AssetRendererFactory.TYPE_LATEST);
  }
 else {
    return null;
  }
}
