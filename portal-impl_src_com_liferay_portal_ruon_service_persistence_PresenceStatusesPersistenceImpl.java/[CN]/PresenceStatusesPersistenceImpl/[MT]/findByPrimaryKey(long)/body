{
  PresenceStatuses presenceStatuses=fetchByPrimaryKey(presenceStatusId);
  if (presenceStatuses == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No PresenceStatuses exists with the primary key " + presenceStatusId);
    }
    throw new NoSuchPresenceStatusesException("No PresenceStatuses exists with the primary key " + presenceStatusId);
  }
  return presenceStatuses;
}
