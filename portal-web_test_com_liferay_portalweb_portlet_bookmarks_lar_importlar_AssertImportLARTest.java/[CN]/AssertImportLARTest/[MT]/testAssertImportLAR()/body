{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Bookmarks Test Page",RuntimeVariables.replace("Bookmarks Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Edited Test Folder"),selenium.getText("//a/strong"));
  selenium.clickAt("//a/strong",RuntimeVariables.replace("Edited Test Folder"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Edited Test Bookmark"),selenium.getText("//td[1]/a"));
  assertEquals(RuntimeVariables.replace("http://www.narutofan.com"),selenium.getText("//td[2]/a"));
}
