{
  if (_persistence == null) {
    _persistence=(KaleoTimerInstanceTokenPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoTimerInstanceTokenPersistence.class.getName());
