{
  Group group=GroupTestUtil.addGroup();
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext(group.getGroupId());
  UserGroup userGroup=addUserGroup(serviceContext);
  Group userGroupGroup=userGroup.getGroup();
  Layout layout=addLayout(userGroupGroup.getGroupId(),privateLayout);
  UserGroupLocalServiceUtil.addUserUserGroup(serviceContext.getUserId(),userGroup.getUserGroupId());
  assertLocalizedURLVirtualLayoutFriendlyURL(userGroupGroup.getGroupId(),layout,"/home",_deLocale,"/home");
}
