{
  Group group=GroupTestUtil.addGroup();
  Layout layout=LayoutTestUtil.addLayout(group.getGroupId(),privateLayout,_nameMap,_friendlyURLMap);
  String requestedFriendlyURL="/inicio";
  if (Validator.isNotNull(queryString)) {
    requestedFriendlyURL+=queryString;
    expectedLayoutFriendlyURL+=queryString;
  }
  assertLocalizedSiteLayoutFriendlyURL(group.getGroupId(),layout,requestedFriendlyURL,locale,LocaleUtil.SPAIN,expectedLayoutFriendlyURL,true);
}
