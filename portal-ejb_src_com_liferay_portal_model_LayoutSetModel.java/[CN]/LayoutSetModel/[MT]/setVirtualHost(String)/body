{
  if (((virtualHost == null) && (_virtualHost != null)) || ((virtualHost != null) && (_virtualHost == null)) || ((virtualHost != null) && (_virtualHost != null) && !virtualHost.equals(_virtualHost))) {
    if (!XSS_ALLOW_VIRTUALHOST) {
      virtualHost=XSSUtil.strip(virtualHost);
    }
    _virtualHost=virtualHost;
  }
}
