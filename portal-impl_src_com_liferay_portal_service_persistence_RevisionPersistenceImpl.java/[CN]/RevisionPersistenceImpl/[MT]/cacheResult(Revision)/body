{
  EntityCacheUtil.putResult(RevisionModelImpl.ENTITY_CACHE_ENABLED,RevisionImpl.class,revision.getPrimaryKey(),revision);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_B_P_H,new Object[]{new Long(revision.getBranchId()),new Long(revision.getPlid()),Boolean.valueOf(revision.getHead())},revision);
}
