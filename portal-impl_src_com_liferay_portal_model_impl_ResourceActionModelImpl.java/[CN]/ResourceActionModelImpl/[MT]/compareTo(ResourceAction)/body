{
  int value=0;
  value=getName().compareTo(resourceAction.getName());
  if (value != 0) {
    return value;
  }
  if (getBitwiseValue() < resourceAction.getBitwiseValue()) {
    value=-1;
  }
 else   if (getBitwiseValue() > resourceAction.getBitwiseValue()) {
    value=1;
  }
 else {
    value=0;
  }
  if (value != 0) {
    return value;
  }
  return 0;
}
