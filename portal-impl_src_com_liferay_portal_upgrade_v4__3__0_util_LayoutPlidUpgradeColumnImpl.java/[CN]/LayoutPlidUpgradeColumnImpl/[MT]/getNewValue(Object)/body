{
  Object newValue=super.getNewValue(oldValue);
  Long groupIdObj=_groupIdColumn.getGroupId();
  Boolean privateLayoutObj=_privateLayoutColumn.isPrivateLayout();
  Long layoutId=(Long)_layoutIdColumn.getTemp();
  if ((groupIdObj == null) || (privateLayoutObj == null) || (Validator.isNull(layoutId))) {
    throw new StagnantRowException("{groupId=" + groupIdObj + ",privateLayout="+ privateLayoutObj+ ",layoutId="+ layoutId+ "}");
  }
  _layoutPlidMapper.mapValue(groupIdObj + "_" + privateLayoutObj+ "_"+ layoutId,newValue);
  return newValue;
}
