{
  String[] itemIds=StringUtil.split((String)oldValue);
  for (int i=0; i < itemIds.length; i++) {
    String itemId=itemIds[i];
    int pos=itemId.indexOf("|");
    if (pos == -1) {
      itemIds[i]=String.valueOf(_shoppingItemIdMapper.getNewValue(new Long(GetterUtil.getLong(itemId))));
    }
 else {
      Long oldItemIdObj=new Long(GetterUtil.getLong(itemId.substring(0,pos)));
      itemIds[i]=_shoppingItemIdMapper.getNewValue(oldItemIdObj) + itemId.substring(pos,itemId.length());
    }
  }
  return StringUtil.merge(itemIds);
}
