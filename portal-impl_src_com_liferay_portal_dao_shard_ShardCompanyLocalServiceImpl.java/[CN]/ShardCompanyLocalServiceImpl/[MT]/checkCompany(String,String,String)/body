{
  try {
    Company company=super.getCompanyByWebId(webId);
    shardName=company.getShardName();
  }
 catch (  NoSuchCompanyException nsce) {
  }
  ShardUtil.pushCompanyService(shardName);
  try {
    return super.checkCompany(webId,mx,shardName);
  }
  finally {
    ShardUtil.popCompanyService();
  }
}
