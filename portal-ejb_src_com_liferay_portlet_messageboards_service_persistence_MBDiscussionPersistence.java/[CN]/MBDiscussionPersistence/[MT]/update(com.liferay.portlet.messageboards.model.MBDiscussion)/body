{
  Session session=null;
  try {
    session=openSession();
    if (mbDiscussion.isNew()) {
      session.save(mbDiscussion);
    }
    session.flush();
    mbDiscussion.setNew(false);
    return mbDiscussion;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
