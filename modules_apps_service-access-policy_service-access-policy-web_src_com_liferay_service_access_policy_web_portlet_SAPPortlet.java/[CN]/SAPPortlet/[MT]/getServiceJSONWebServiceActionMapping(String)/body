{
  Map<String,Set<JSONWebServiceActionMapping>> jsonWebServiceActionMappingsMap=new LinkedHashMap<>();
  List<JSONWebServiceActionMapping> jsonWebServiceActionMappings=_jsonWebServiceActionsManager.getJSONWebServiceActionMappings(contextName);
  for (  JSONWebServiceActionMapping jsonWebServiceActionMapping : jsonWebServiceActionMappings) {
    Class<?> serviceClass=jsonWebServiceActionMapping.getActionObject().getClass();
    Class[] serviceInterfaces=serviceClass.getInterfaces();
    for (    Class serviceInterface : serviceInterfaces) {
      Annotation[] declaredAnnotations=serviceInterface.getDeclaredAnnotations();
      for (      Annotation declaredAnnotation : declaredAnnotations) {
        if (declaredAnnotation instanceof AccessControlled) {
          String serviceName=serviceInterface.getName();
          Set<JSONWebServiceActionMapping> jsonWebServiceMappings=jsonWebServiceActionMappingsMap.get(serviceName);
          if (jsonWebServiceMappings == null) {
            jsonWebServiceMappings=new LinkedHashSet<>();
            jsonWebServiceActionMappingsMap.put(serviceName,jsonWebServiceMappings);
          }
          jsonWebServiceMappings.add(jsonWebServiceActionMapping);
        }
      }
    }
  }
  return jsonWebServiceActionMappingsMap;
}
