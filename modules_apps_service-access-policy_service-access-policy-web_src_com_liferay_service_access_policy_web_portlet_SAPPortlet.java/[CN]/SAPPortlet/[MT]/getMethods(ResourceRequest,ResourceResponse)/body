{
  String contextName=ParamUtil.get(resourceRequest,"context",StringPool.BLANK);
  String serviceClass=ParamUtil.get(resourceRequest,"serviceClass",StringPool.BLANK);
  Map<String,Set> jsonWebServiceClasses=getJsonWebServiceClasses(contextName);
  Set<JSONWebServiceActionMapping> jsonWebServiceActionMappings=jsonWebServiceClasses.get(serviceClass);
  JSONArray jsonArray=JSONFactoryUtil.createJSONArray();
  for (  JSONWebServiceActionMapping jsonWebServiceActionMapping : jsonWebServiceActionMappings) {
    Method method=jsonWebServiceActionMapping.getActionMethod();
  }
  PrintWriter writer=resourceResponse.getWriter();
  writer.write(jsonArray.toString());
}
