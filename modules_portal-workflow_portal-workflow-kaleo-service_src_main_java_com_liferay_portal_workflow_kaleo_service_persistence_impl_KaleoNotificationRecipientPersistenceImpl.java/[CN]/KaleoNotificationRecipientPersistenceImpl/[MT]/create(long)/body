{
  KaleoNotificationRecipient kaleoNotificationRecipient=new KaleoNotificationRecipientImpl();
  kaleoNotificationRecipient.setNew(true);
  kaleoNotificationRecipient.setPrimaryKey(kaleoNotificationRecipientId);
