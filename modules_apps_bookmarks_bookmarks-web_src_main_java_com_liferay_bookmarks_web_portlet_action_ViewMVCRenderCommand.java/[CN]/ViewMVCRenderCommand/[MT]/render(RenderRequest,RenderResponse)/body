{
  try {
    ActionUtil.getFolder(renderRequest);
    renderRequest.setAttribute(BookmarksWebKeys.BOOKMARKS_PORTLET_TOOLBAR_CONTRIBUTOR,_bookmarksPortletToolbarContributor);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchFolderException || e instanceof PrincipalException) {
      SessionErrors.add(renderRequest,e.getClass());
      return "/bookmarks/error.jsp";
    }
 else {
      throw new PortletException(e);
    }
  }
  return "/bookmarks/view.jsp";
}
