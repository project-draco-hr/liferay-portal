{
  _group=GroupTestUtil.addGroup();
  _folder=DLAppTestUtil.addFolder(_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,"Test Folder Permissions",true);
  _subfolder=DLAppTestUtil.addFolder(_group.getGroupId(),_folder.getFolderId(),"Test SubFolder Permissions",true);
  RoleTestUtil.addResourcePermission(RoleConstants.POWER_USER,DLPermission.RESOURCE_NAME,ResourceConstants.SCOPE_GROUP,String.valueOf(_group.getGroupId()),ActionKeys.VIEW);
}
