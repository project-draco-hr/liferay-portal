{
  final Project project=deployTask.getProject();
  Closure<String> closure=new Closure<String>(null){
    @SuppressWarnings("unused") public String doCall(    String fileName){
      return fileName.replace("-" + project.getVersion() + "."+ Jar.DEFAULT_EXTENSION,"." + Jar.DEFAULT_EXTENSION);
    }
  }
;
  deployTask.rename(closure);
}
