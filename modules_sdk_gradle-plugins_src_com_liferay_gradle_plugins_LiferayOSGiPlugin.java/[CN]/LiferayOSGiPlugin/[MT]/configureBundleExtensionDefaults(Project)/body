{
  Map<String,String> bundleInstructions=getBundleInstructions(project);
  Map<String,String> bundleDefaultInstructions=getBundleDefaultInstructions(project);
  for (  Map.Entry<String,String> entry : bundleDefaultInstructions.entrySet()) {
    String key=entry.getKey();
    if (!bundleInstructions.containsKey(key)) {
      bundleInstructions.put(key,entry.getValue());
    }
  }
}
