{
  selenium.click(RuntimeVariables.replace("//div[@id='banner']/div/div/ul/li[9]/ul/li[4]/ul/li[1]/a[1]/span"));
  selenium.waitForPageToLoad("30000");
  selenium.click("document.getElementById('add-page').getElementsByTagName('a')[0].getElementsByTagName('span')[0]");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("new_page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.typeKeys("new_page",RuntimeVariables.replace("Enterprise Admin Test Page"));
  selenium.type("new_page",RuntimeVariables.replace("Enterprise Admin Test Page"));
  selenium.click("link=Save");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Enterprise Admin Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Enterprise Admin Test Page"));
  selenium.waitForPageToLoad("30000");
}
