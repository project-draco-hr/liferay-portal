{
  Organization organization=OrganizationTestUtil.addOrganization(true);
  Role role=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),RoleConstants.SITE_ADMINISTRATOR);
  User subjectUser=UserTestUtil.addOrganizationOwnerUser(organization);
  User objectUser=UserTestUtil.addGroupAdminUser(organization.getGroup());
  deleteUserGroupRolesByRole(organization.getGroupId(),role.getRoleId(),subjectUser,objectUser);
  Assert.assertFalse(UserGroupRoleLocalServiceUtil.hasUserGroupRole(objectUser.getUserId(),organization.getGroupId(),role.getRoleId()));
}
