{
  Organization organization=OrganizationTestUtil.addOrganization(true);
  User subjectUser=UserTestUtil.addOrganizationAdminUser(organization);
  User objectUser=UserTestUtil.addGroupAdminUser(organization.getGroup());
  Role role=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),RoleConstants.SITE_ADMINISTRATOR);
  deleteUserGroupRolesByUser(organization.getGroupId(),role.getRoleId(),subjectUser,objectUser);
  Assert.assertTrue(UserGroupRoleLocalServiceUtil.hasUserGroupRole(objectUser.getUserId(),organization.getGroupId(),role.getRoleId()));
}
