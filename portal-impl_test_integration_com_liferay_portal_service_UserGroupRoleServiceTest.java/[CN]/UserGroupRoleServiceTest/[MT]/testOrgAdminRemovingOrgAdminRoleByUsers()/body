{
  Organization organization=OrganizationTestUtil.addOrganization();
  User orgAdminOperator=UserTestUtil.addOrganizationAdminUser(organization);
  User orgAdminUpdated=UserTestUtil.addOrganizationAdminUser(organization);
  Role orgAdminRole=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),RoleConstants.ORGANIZATION_ADMINISTRATOR);
  deleteUserGroupRoleByUsers(organization.getGroupId(),orgAdminRole.getRoleId(),orgAdminOperator,orgAdminUpdated);
  Assert.assertTrue(UserGroupRoleLocalServiceUtil.hasUserGroupRole(orgAdminUpdated.getUserId(),organization.getGroupId(),orgAdminRole.getRoleId()));
}
