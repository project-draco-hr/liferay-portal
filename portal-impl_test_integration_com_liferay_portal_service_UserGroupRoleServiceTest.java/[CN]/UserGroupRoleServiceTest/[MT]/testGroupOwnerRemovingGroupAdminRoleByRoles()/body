{
  Group group=ServiceTestUtil.addGroup();
  User subjectUser=addGroupOwnerUser(group);
  User objectUser=addGroupAdminUser(group);
  Role role=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),RoleConstants.SITE_ADMINISTRATOR);
  deleteUserGroupRolesByRole(group.getGroupId(),role.getRoleId(),subjectUser,objectUser);
  Assert.assertFalse(UserGroupRoleLocalServiceUtil.hasUserGroupRole(objectUser.getUserId(),group.getGroupId(),role.getRoleId()));
}
