{
  Group site=GroupTestUtil.addGroup();
  Role role=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),RoleConstants.SITE_ADMINISTRATOR);
  _subjectUser=UserTestUtil.addGroupOwnerUser(site);
  _objectUser=UserTestUtil.addGroupAdminUser(site);
  deleteUserGroupRolesByUser(site.getGroupId(),role.getRoleId(),_subjectUser,_objectUser);
  Assert.assertFalse(UserGroupRoleLocalServiceUtil.hasUserGroupRole(_objectUser.getUserId(),site.getGroupId(),role.getRoleId()));
}
