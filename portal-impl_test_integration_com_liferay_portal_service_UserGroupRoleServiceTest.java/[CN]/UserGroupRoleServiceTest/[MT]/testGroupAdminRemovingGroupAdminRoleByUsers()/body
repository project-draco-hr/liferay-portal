{
  _group=GroupTestUtil.addGroup();
  Role role=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),RoleConstants.SITE_ADMINISTRATOR);
  _subjectUser=UserTestUtil.addGroupAdminUser(_group);
  _objectUser=UserTestUtil.addGroupAdminUser(_group);
  try {
    deleteUserGroupRolesByUser(_group.getGroupId(),role.getRoleId(),_subjectUser,_objectUser);
    Assert.fail();
  }
 catch (  PrincipalException pe) {
    Assert.assertTrue(UserGroupRoleLocalServiceUtil.hasUserGroupRole(_objectUser.getUserId(),_group.getGroupId(),role.getRoleId()));
  }
}
