{
  Group site=ServiceTestUtil.addGroup(0,"site");
  User siteAdminOperator=addGroupAdminUser(site);
  User siteOwnerUpdated=addGroupOwnerUser(site);
  Role siteOwnerRole=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),RoleConstants.SITE_OWNER);
  deleteUserGroupRoleByUsers(site.getGroupId(),siteOwnerRole.getRoleId(),siteAdminOperator,siteOwnerUpdated);
  Assert.assertTrue(UserGroupRoleLocalServiceUtil.hasUserGroupRole(siteOwnerUpdated.getUserId(),site.getGroupId(),siteOwnerRole.getRoleId()));
}
