{
  Group site=ServiceTestUtil.addGroup(0,"site");
  User siteAdminOperator=addGroupAdminUser(site);
  User siteAdminUpdated=addGroupAdminUser(site);
  Role siteAdminRole=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),RoleConstants.SITE_ADMINISTRATOR);
  deleteUserGroupRoleByRoles(site.getGroupId(),siteAdminRole.getRoleId(),siteAdminOperator,siteAdminUpdated);
  Assert.assertTrue(UserGroupRoleLocalServiceUtil.hasUserGroupRole(siteAdminUpdated.getUserId(),site.getGroupId(),siteAdminRole.getRoleId()));
}
