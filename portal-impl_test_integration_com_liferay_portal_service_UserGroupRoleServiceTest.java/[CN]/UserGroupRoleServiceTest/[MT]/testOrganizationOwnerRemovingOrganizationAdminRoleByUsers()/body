{
  Organization organization=OrganizationTestUtil.addOrganization();
  User subjectUser=UserTestUtil.addOrganizationOwnerUser(organization);
  User objectUser=UserTestUtil.addOrganizationAdminUser(organization);
  Role role=RoleLocalServiceUtil.getRole(TestPropsValues.getCompanyId(),RoleConstants.ORGANIZATION_ADMINISTRATOR);
  deleteUserGroupRolesByUser(organization.getGroupId(),role.getRoleId(),subjectUser,objectUser);
  Assert.assertFalse(UserGroupRoleLocalServiceUtil.hasUserGroupRole(objectUser.getUserId(),organization.getGroupId(),role.getRoleId()));
}
