{
  SocialRequest socialRequest=findByPrimaryKey(requestId);
  Session session=null;
  try {
    session=openSession();
    SocialRequest[] array=new SocialRequestImpl[3];
    array[0]=getByReceiverUserId_PrevAndNext(session,socialRequest,receiverUserId,orderByComparator,true);
    array[1]=socialRequest;
    array[2]=getByReceiverUserId_PrevAndNext(session,socialRequest,receiverUserId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
