{
  selenium.open("/web/guest/home/");
  Thread.sleep(5000);
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("addPage")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("addPage",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//input")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.type("//input",RuntimeVariables.replace("Hello World Test Page"));
  selenium.clickAt("//span[@id='save']/span",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Hello World Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Hello World Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
}
