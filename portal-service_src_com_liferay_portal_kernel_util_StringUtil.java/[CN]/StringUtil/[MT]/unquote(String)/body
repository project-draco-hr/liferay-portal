{
  if (s == null) {
    return s;
  }
  if ((s.charAt(0) == CharPool.APOSTROPHE) && (s.charAt(s.length() - 1) == CharPool.APOSTROPHE)) {
    return s.substring(1,s.length() - 1);
  }
 else   if ((s.charAt(0) == CharPool.QUOTE) && (s.charAt(s.length() - 1) == CharPool.QUOTE)) {
    return s.substring(1,s.length() - 1);
  }
  return s;
}
