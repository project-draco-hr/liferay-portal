{
  if (s == null) {
    return s;
  }
  if (s.startsWith(StringPool.APOSTROPHE) && s.endsWith(StringPool.APOSTROPHE)) {
    s=s.replaceAll("'$",StringPool.BLANK);
    s=s.replaceAll("^'",StringPool.BLANK);
  }
 else   if (s.startsWith(StringPool.QUOTE) && s.endsWith(StringPool.QUOTE)) {
    s=s.replaceAll("\"$",StringPool.BLANK);
    s=s.replaceAll("^\"",StringPool.BLANK);
  }
  return s;
}
