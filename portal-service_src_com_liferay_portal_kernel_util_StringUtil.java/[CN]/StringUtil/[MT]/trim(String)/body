{
  if (s == null) {
    return null;
  }
  if (s.length() == 0) {
    return s;
  }
  int len=s.length();
  int x=len;
  for (int i=0; i < len; i++) {
    char c=s.charAt(i);
    if (!Character.isWhitespace(c)) {
      x=i;
      break;
    }
  }
  if (x == len) {
    return StringPool.BLANK;
  }
  int y=x + 1;
  for (int i=len - 1; i > x; i--) {
    char c=s.charAt(i);
    if (!Character.isWhitespace(c)) {
      y=i + 1;
      break;
    }
  }
  if ((x == 0) && (y == len)) {
    return s;
  }
  return s.substring(x,y);
}
