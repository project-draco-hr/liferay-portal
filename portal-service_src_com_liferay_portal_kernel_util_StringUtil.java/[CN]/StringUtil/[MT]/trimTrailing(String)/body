{
  if (s == null) {
    return null;
  }
  if (s.length() == 0) {
    return s;
  }
  int len=s.length();
  int y=0;
  for (int i=len - 1; i >= 0; i--) {
    char c=s.charAt(i);
    if (!Character.isWhitespace(c)) {
      y=i + 1;
      break;
    }
  }
  if (y == 0) {
    return StringPool.BLANK;
  }
 else   if (y == len) {
    return s;
  }
 else {
    return s.substring(0,y);
  }
}
