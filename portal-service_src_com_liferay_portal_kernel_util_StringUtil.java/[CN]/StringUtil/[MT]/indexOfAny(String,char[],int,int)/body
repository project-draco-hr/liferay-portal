{
  if ((s == null) || (toIndex < fromIndex)) {
    return -1;
  }
  if ((chars == null) || (chars.length == 0)) {
    return -1;
  }
  if (fromIndex >= s.length()) {
    return -1;
  }
  if (fromIndex < 0) {
    fromIndex=0;
  }
  if (toIndex >= s.length()) {
    toIndex=s.length() - 1;
  }
  for (int i=fromIndex; i <= toIndex; i++) {
    char c=s.charAt(i);
    for (int j=0; j < chars.length; j++) {
      if (c == chars[j]) {
        return i;
      }
    }
  }
  return -1;
}
