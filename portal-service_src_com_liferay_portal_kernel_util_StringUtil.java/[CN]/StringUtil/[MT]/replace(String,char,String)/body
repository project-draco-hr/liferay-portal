{
  if ((s == null) || (newSub == null)) {
    return null;
  }
  StringBundler sb=new StringBundler(s.length() + 5 * newSub.length());
  char[] chars=s.toCharArray();
  for (  char c : chars) {
    if (c == oldSub) {
      sb.append(newSub);
    }
 else {
      sb.append(c);
    }
  }
  return sb.toString();
}
