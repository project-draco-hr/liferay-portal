{
  if (s == null) {
    return null;
  }
  int index=s.indexOf(remove);
  if (index < 0) {
    return s;
  }
  int lastIndex=0;
  StringBuilder sb=new StringBuilder(s.length());
  while (index >= 0) {
    sb.append(s.subSequence(lastIndex,index));
    lastIndex=index + 1;
    index=s.indexOf(remove,lastIndex);
  }
  sb.append(s.substring(lastIndex));
  return sb.toString();
}
