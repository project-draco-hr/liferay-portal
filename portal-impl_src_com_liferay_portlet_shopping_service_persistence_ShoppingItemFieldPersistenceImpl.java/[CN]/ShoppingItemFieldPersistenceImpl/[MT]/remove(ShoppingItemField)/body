{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(shoppingItemField);
    }
  }
  shoppingItemField=removeImpl(shoppingItemField);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(shoppingItemField);
    }
  }
  return shoppingItemField;
}
