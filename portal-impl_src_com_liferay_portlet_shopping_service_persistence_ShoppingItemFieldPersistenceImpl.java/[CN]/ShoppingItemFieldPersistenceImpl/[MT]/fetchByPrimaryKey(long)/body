{
  ShoppingItemField shoppingItemField=(ShoppingItemField)EntityCacheUtil.getResult(ShoppingItemFieldModelImpl.ENTITY_CACHE_ENABLED,ShoppingItemFieldImpl.class,itemFieldId,this);
  if (shoppingItemField == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (shoppingItemField == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      shoppingItemField=(ShoppingItemField)session.get(ShoppingItemFieldImpl.class,Long.valueOf(itemFieldId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (shoppingItemField == null)) {
        EntityCacheUtil.putResult(ShoppingItemFieldModelImpl.ENTITY_CACHE_ENABLED,ShoppingItemFieldImpl.class,itemFieldId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(shoppingItemField);
      }
      closeSession(session);
    }
  }
  return shoppingItemField;
}
