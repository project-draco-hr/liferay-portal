{
  Session session=null;
  try {
    session=openSession();
    session.delete(shoppingItemField);
    session.flush();
    return shoppingItemField;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(ShoppingItemField.class.getName());
  }
}
