{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByG_A_PrevAndNext(frameworkVersionId,groupId,active,orderByComparator);
  }
  SCFrameworkVersion scFrameworkVersion=findByPrimaryKey(frameworkVersionId);
  Session session=null;
  try {
    session=openSession();
    SCFrameworkVersion[] array=new SCFrameworkVersionImpl[3];
    array[0]=filterGetByG_A_PrevAndNext(session,scFrameworkVersion,groupId,active,orderByComparator,true);
    array[1]=scFrameworkVersion;
    array[2]=filterGetByG_A_PrevAndNext(session,scFrameworkVersion,groupId,active,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
