{
  List<TrashRenderer> trashRenderers=new ArrayList<TrashRenderer>();
  MBCategory category=MBCategoryLocalServiceUtil.getCategory(classPK);
  List<MBThread> threads=MBThreadLocalServiceUtil.getThreads(category.getGroupId(),classPK,WorkflowConstants.STATUS_IN_TRASH,start,end);
  for (  MBThread thread : threads) {
    TrashHandler trashHandler=TrashHandlerRegistryUtil.getTrashHandler(MBThread.class.getName());
    TrashRenderer trashRenderer=trashHandler.getTrashRenderer(thread.getPrimaryKey());
    trashRenderers.add(trashRenderer);
  }
  return trashRenderers;
}
