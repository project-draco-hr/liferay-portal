{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SocialActivityCounter.class,SocialActivityCounter.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("activityCounterId",ServiceTestUtil.nextLong()));
  List<SocialActivityCounter> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
