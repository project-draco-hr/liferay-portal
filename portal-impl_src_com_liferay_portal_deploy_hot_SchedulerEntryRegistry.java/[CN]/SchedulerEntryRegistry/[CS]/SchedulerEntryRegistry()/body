{
  if (!PropsValues.SCHEDULER_ENABLED) {
    return;
  }
  Registry registry=RegistryUtil.getRegistry();
  Filter schedulerEntryFilter=registry.getFilter("(&(javax.portlet.name=*)(objectClass=" + SchedulerEntry.class.getName() + "))");
  _serviceTracker=registry.trackServices(schedulerEntryFilter,new SchedulerEntryServiceTrackerCustomizer());
  _serviceTracker.open();
}
