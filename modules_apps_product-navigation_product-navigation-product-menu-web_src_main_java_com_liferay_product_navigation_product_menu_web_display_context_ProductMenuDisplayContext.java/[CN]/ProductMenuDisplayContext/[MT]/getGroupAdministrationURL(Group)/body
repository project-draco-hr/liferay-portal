{
  PortletURL groupAdministrationURL=null;
  String portletId=_panelCategoryHelper.getFirstPortletId(PanelCategoryKeys.SITE_ADMINISTRATION,_themeDisplay.getPermissionChecker(),group);
  if (Validator.isNotNull(portletId)) {
    groupAdministrationURL=PortalUtil.getControlPanelPortletURL(_portletRequest,group,portletId,0,0,PortletRequest.RENDER_PHASE);
    if (groupAdministrationURL != null) {
      return groupAdministrationURL.toString();
    }
  }
  return null;
}
