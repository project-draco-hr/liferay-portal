{
  mockStatic(SingleVMPoolUtil.class);
  MemoryPortalCacheManager memoryPortalCacheManager=new MemoryPortalCacheManager();
  memoryPortalCacheManager.afterPropertiesSet();
  String comboServletClass=ComboServlet.class.getName();
  when(SingleVMPoolUtil.getCache(comboServletClass)).thenReturn(memoryPortalCacheManager.getCache(comboServletClass));
  String fileContentBagClass=comboServletClass + CharPool.PERIOD + "FileContentBag";
  when(SingleVMPoolUtil.getCache(fileContentBagClass)).thenReturn(memoryPortalCacheManager.getCache(fileContentBagClass));
  _comboServlet=new ComboServlet();
}
