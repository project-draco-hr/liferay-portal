{
  PreparedStatement ps=null;
  try {
    ps=connection.prepareStatement("update RatingsEntry set score = ? where score = ? and " + "classNameId = ?");
    ps.setDouble(1,0);
    ps.setDouble(2,-1);
    ps.setLong(3,classNameId);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(ps);
  }
}
