{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select distinct classNameId from RatingsEntry");
    rs=ps.executeQuery();
    while (rs.next()) {
      upgradeRatingsEntry(rs.getLong("classNameId"));
    }
  }
  finally {
    DataAccess.cleanUp(null,ps,rs);
  }
}
