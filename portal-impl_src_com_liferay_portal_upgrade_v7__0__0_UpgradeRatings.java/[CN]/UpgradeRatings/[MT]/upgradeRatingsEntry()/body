{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select distinct classNameId from RatingsEntry");
    rs=ps.executeQuery();
    while (rs.next()) {
      upgradeRatingsEntry(rs.getLong("classNameId"));
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
