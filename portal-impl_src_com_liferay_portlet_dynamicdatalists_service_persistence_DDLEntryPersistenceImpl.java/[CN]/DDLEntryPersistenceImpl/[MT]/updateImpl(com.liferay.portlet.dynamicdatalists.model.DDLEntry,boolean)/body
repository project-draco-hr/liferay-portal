{
  ddlEntry=toUnwrappedModel(ddlEntry);
  boolean isNew=ddlEntry.isNew();
  DDLEntryModelImpl ddlEntryModelImpl=(DDLEntryModelImpl)ddlEntry;
  if (Validator.isNull(ddlEntry.getUuid())) {
    String uuid=PortalUUIDUtil.generate();
    ddlEntry.setUuid(uuid);
  }
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,ddlEntry,merge);
    ddlEntry.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.putResult(DDLEntryModelImpl.ENTITY_CACHE_ENABLED,DDLEntryImpl.class,ddlEntry.getPrimaryKey(),ddlEntry);
  if (!isNew && (!Validator.equals(ddlEntry.getUuid(),ddlEntryModelImpl.getOriginalUuid()) || (ddlEntry.getGroupId() != ddlEntryModelImpl.getOriginalGroupId()))) {
    FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddlEntryModelImpl.getOriginalUuid(),Long.valueOf(ddlEntryModelImpl.getOriginalGroupId())});
  }
  if (isNew || (!Validator.equals(ddlEntry.getUuid(),ddlEntryModelImpl.getOriginalUuid()) || (ddlEntry.getGroupId() != ddlEntryModelImpl.getOriginalGroupId()))) {
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddlEntry.getUuid(),Long.valueOf(ddlEntry.getGroupId())},ddlEntry);
  }
  if (!isNew && ((ddlEntry.getGroupId() != ddlEntryModelImpl.getOriginalGroupId()) || !Validator.equals(ddlEntry.getEntryKey(),ddlEntryModelImpl.getOriginalEntryKey()))) {
    FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_E,new Object[]{Long.valueOf(ddlEntryModelImpl.getOriginalGroupId()),ddlEntryModelImpl.getOriginalEntryKey()});
  }
  if (isNew || ((ddlEntry.getGroupId() != ddlEntryModelImpl.getOriginalGroupId()) || !Validator.equals(ddlEntry.getEntryKey(),ddlEntryModelImpl.getOriginalEntryKey()))) {
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_E,new Object[]{Long.valueOf(ddlEntry.getGroupId()),ddlEntry.getEntryKey()},ddlEntry);
  }
  return ddlEntry;
}
