{
  DDLEntry ddlEntry=(DDLEntry)EntityCacheUtil.getResult(DDLEntryModelImpl.ENTITY_CACHE_ENABLED,DDLEntryImpl.class,entryId,this);
  if (ddlEntry == null) {
    Session session=null;
    try {
      session=openSession();
      ddlEntry=(DDLEntry)session.get(DDLEntryImpl.class,Long.valueOf(entryId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (ddlEntry != null) {
        cacheResult(ddlEntry);
      }
      closeSession(session);
    }
  }
  return ddlEntry;
}
