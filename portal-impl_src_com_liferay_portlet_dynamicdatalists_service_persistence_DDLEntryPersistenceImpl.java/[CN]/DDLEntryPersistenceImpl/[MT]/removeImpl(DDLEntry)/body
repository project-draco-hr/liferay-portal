{
  ddlEntry=toUnwrappedModel(ddlEntry);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,ddlEntry);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  DDLEntryModelImpl ddlEntryModelImpl=(DDLEntryModelImpl)ddlEntry;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddlEntryModelImpl.getUuid(),Long.valueOf(ddlEntryModelImpl.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_E,new Object[]{Long.valueOf(ddlEntryModelImpl.getGroupId()),ddlEntryModelImpl.getEntryKey()});
  EntityCacheUtil.removeResult(DDLEntryModelImpl.ENTITY_CACHE_ENABLED,DDLEntryImpl.class,ddlEntry.getPrimaryKey());
  return ddlEntry;
}
