{
  EntityCacheUtil.removeResult(DDLEntryModelImpl.ENTITY_CACHE_ENABLED,DDLEntryImpl.class,ddlEntry.getPrimaryKey());
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddlEntry.getUuid(),Long.valueOf(ddlEntry.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_E,new Object[]{Long.valueOf(ddlEntry.getGroupId()),ddlEntry.getEntryKey()});
}
