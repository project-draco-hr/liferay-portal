{
  Object[] finderArgs=new Object[]{groupId,entryKey};
  Object result=null;
  if (retrieveFromCache) {
    result=FinderCacheUtil.getResult(FINDER_PATH_FETCH_BY_G_E,finderArgs,this);
  }
  if (result == null) {
    StringBundler query=new StringBundler(3);
    query.append(_SQL_SELECT_DDLENTRY_WHERE);
    query.append(_FINDER_COLUMN_G_E_GROUPID_2);
    if (entryKey == null) {
      query.append(_FINDER_COLUMN_G_E_ENTRYKEY_1);
    }
 else {
      if (entryKey.equals(StringPool.BLANK)) {
        query.append(_FINDER_COLUMN_G_E_ENTRYKEY_3);
      }
 else {
        query.append(_FINDER_COLUMN_G_E_ENTRYKEY_2);
      }
    }
    String sql=query.toString();
    Session session=null;
    try {
      session=openSession();
      Query q=session.createQuery(sql);
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      if (entryKey != null) {
        qPos.add(entryKey);
      }
      List<DDLEntry> list=q.list();
      result=list;
      DDLEntry ddlEntry=null;
      if (list.isEmpty()) {
        FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_E,finderArgs,list);
      }
 else {
        ddlEntry=list.get(0);
        cacheResult(ddlEntry);
        if ((ddlEntry.getGroupId() != groupId) || (ddlEntry.getEntryKey() == null) || !ddlEntry.getEntryKey().equals(entryKey)) {
          FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_E,finderArgs,ddlEntry);
        }
      }
      return ddlEntry;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (result == null) {
        FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_E,finderArgs);
      }
      closeSession(session);
    }
  }
 else {
    if (result instanceof List<?>) {
      return null;
    }
 else {
      return (DDLEntry)result;
    }
  }
}
