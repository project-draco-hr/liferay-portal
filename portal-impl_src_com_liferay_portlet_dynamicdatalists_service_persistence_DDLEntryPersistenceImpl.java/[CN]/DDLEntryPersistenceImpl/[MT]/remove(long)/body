{
  Session session=null;
  try {
    session=openSession();
    DDLEntry ddlEntry=(DDLEntry)session.get(DDLEntryImpl.class,Long.valueOf(entryId));
    if (ddlEntry == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + entryId);
      }
      throw new NoSuchEntryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + entryId);
    }
    return ddlEntryPersistence.remove(ddlEntry);
  }
 catch (  NoSuchEntryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
