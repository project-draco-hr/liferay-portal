{
  String namespace=PortalUtil.getPortletNamespace(portletId);
  String[] actionNameParameterValues=ParamUtil.getParameterValues(request,namespace + ActionRequest.ACTION_NAME);
  String actionNameParameterValue=StringUtil.merge(actionNameParameterValues);
  String[] mvcActionCommandNames=StringUtil.split(actionNameParameterValue);
  if (mvcActionCommandNames.length == 0) {
    return false;
  }
  return containsCSRFWhitelistActions(portletId,_portletInvocationActionWhitelistActions,mvcActionCommandNames);
}
