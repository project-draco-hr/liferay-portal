{
  String portletId=liferayPortletURL.getPortletId();
  String lifecycle=liferayPortletURL.getLifecycle();
  if (lifecycle.equals(PortletRequest.ACTION_PHASE)) {
    String[] mvcActionCommandNames=getMVCActionCommandNames(liferayPortletURL);
    return _containsAll(portletId,_portletInvocationWhitelistAction,mvcActionCommandNames);
  }
 else   if (lifecycle.equals(PortletRequest.RENDER_PHASE)) {
    String mvcRenderCommandName=liferayPortletURL.getParameter("mvcRenderCommandName");
    return _contains(portletId,_portletInvocationWhitelistRender,mvcRenderCommandName);
  }
 else   if (lifecycle.equals(PortletRequest.RESOURCE_PHASE)) {
    String mvcResourceCommandName=liferayPortletURL.getResourceID();
    return _contains(portletId,_portletInvocationWhitelistResource,mvcResourceCommandName);
  }
  return false;
}
