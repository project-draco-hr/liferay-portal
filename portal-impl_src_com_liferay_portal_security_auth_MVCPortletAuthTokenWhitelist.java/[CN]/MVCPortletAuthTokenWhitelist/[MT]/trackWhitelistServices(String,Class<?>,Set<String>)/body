{
  Registry registry=RegistryUtil.getRegistry();
  ServiceTracker<Object,Object> serviceTracker=registry.trackServices(registry.getFilter("(&(&(" + whitelistName + "=*)(javax.portlet.name=*))"+ "(objectClass="+ serviceClass.getName()+ "))"),new TokenWhitelistTrackerCustomizer(whiteList));
  serviceTracker.open();
  serviceTrackers.add(serviceTracker);
}
