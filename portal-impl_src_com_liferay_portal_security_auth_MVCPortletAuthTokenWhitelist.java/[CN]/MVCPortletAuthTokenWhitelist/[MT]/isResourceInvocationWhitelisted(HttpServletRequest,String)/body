{
  String ppid=ParamUtil.getString(request,"p_p_id");
  if (!portletId.equals(ppid)) {
    return false;
  }
  String mvcResourceCommandName=ParamUtil.getString(request,"p_p_resource_id");
  String rootPortletId=PortletConstants.getRootPortletId(portletId);
  if (_portletInvocationResourceWhitelistActions.contains(getWhitelistValue(rootPortletId,mvcResourceCommandName))) {
    return true;
  }
  return false;
}
