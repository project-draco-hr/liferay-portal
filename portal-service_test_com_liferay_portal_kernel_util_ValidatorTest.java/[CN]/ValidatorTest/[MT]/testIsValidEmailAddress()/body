{
  String[] validEmailAddresses={"email@domain.com","firstname.lastname@domain.com","firstname+lastname@domain.com","123456789@domain.com","email@domain-one.com","____@domain.com","email@domain.name","firstname-lastname@domain.com"};
  for (  String validEmailAddress : validEmailAddresses) {
    assertTrue(Validator.isEmailAddress(validEmailAddress));
  }
  String[] invalidEmailAddresses={"domain","#$%^&.com","joe smith <email@domain.com","email.domain.com","email@domain@domain.com","email@domain.com (joe smith)","email@domain"};
  for (  String invalidEmailAddress : invalidEmailAddresses) {
    assertFalse(Validator.isEmailAddress(invalidEmailAddress));
  }
}
