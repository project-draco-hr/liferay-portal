{
  try {
    com.liferay.portal.kernel.model.Group returnValue=GroupServiceUtil.addGroup(parentGroupId,liveGroupId,name,description,type,manualMembership,membershipRestriction,friendlyURL,site,active,serviceContext);
    return com.liferay.portal.kernel.model.GroupSoap.toSoapModel(returnValue);
