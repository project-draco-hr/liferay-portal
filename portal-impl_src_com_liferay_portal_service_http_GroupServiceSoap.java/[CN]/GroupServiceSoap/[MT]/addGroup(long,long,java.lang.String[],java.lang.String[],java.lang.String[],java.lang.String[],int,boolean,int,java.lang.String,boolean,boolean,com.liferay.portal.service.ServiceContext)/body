{
  try {
    Map<Locale,String> nameMap=LocalizationUtil.getLocalizationMap(nameMapLanguageIds,nameMapValues);
    Map<Locale,String> descriptionMap=LocalizationUtil.getLocalizationMap(descriptionMapLanguageIds,descriptionMapValues);
    com.liferay.portal.model.Group returnValue=GroupServiceUtil.addGroup(parentGroupId,liveGroupId,nameMap,descriptionMap,type,manualMembership,membershipRestriction,friendlyURL,site,active,serviceContext);
    return com.liferay.portal.model.GroupSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
