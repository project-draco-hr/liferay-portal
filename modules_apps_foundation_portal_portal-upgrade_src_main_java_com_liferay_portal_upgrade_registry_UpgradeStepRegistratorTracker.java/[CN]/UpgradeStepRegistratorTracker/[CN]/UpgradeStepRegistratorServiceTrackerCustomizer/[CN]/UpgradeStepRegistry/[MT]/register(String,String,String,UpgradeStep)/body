{
  List<UpgradeInfo> upgradeInfos=createUpgradeInfos(fromSchemaVersionString,toSchemaVersionString,upgradeSteps);
  for (  UpgradeInfo upgradeInfo : upgradeInfos) {
    ServiceRegistration<UpgradeStep> serviceRegistration=_register(_bundleContext,bundleSymbolicName,upgradeInfo,new Hashtable<String,Object>());
    _serviceRegistrations.add(serviceRegistration);
  }
}
