{
  Organization newOrganization=addOrganization();
  _persistence.clearCache();
  Organization existingOrganization=_persistence.findByPrimaryKey(newOrganization.getPrimaryKey());
  Assert.assertEquals(existingOrganization.getCompanyId(),ReflectionTestUtil.invoke(existingOrganization,"getOriginalCompanyId",new Class<?>[0]));
  Assert.assertTrue(Validator.equals(existingOrganization.getName(),ReflectionTestUtil.invoke(existingOrganization,"getOriginalName",new Class<?>[0])));
}
