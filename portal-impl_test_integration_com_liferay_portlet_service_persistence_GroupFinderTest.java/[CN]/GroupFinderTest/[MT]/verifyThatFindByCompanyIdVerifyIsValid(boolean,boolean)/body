{
  LinkedHashMap<String,Object> groupParams=new LinkedHashMap<String,Object>();
  groupParams.put("inherit",inherit);
  groupParams.put("site",site);
  groupParams.put("usersGroups",TestPropsValues.getUserId());
  List<Group> list=GroupFinderUtil.findByCompanyId(TestPropsValues.getCompanyId(),groupParams,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new GroupNameComparator(true));
  Assert.assertTrue(list.size() > 0);
}
