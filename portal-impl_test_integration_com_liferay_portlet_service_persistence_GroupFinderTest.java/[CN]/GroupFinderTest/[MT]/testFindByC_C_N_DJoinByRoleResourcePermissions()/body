{
  List<Group> groups=findByC_C_N_D(_arbitraryResourceAction.getActionId(),_resourcePermission.getName(),_resourcePermission.getRoleId());
  for (  Group group : groups) {
    if (group.getGroupId() == _group.getGroupId()) {
      return;
    }
  }
  Assert.fail("The method findByC_C_N_D should have returned the group " + _group.getGroupId());
}
