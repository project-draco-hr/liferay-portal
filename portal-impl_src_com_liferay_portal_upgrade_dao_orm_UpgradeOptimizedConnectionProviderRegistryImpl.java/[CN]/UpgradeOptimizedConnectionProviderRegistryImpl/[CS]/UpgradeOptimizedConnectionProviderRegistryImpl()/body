{
  ServiceLoader<UpgradeOptimizedConnectionProvider> serviceLoader=ServiceLoader.load(UpgradeOptimizedConnectionProvider.class,UpgradeOptimizedConnectionProviderRegistryImpl.class.getClassLoader());
  for (  UpgradeOptimizedConnectionProvider upgradeOptimizedConnectionProvider : serviceLoader) {
    _upgradeOptimizedConnectionProviders.put(upgradeOptimizedConnectionProvider.getDBType(),upgradeOptimizedConnectionProvider);
  }
}
