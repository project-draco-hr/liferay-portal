{
  _upgradeOptimizedConnectionProviderMap=new HashMap<>();
  Class<UpgradeOptimizedConnectionProviderRegistryImpl> providerRegisterClass=UpgradeOptimizedConnectionProviderRegistryImpl.class;
  ServiceLoader<UpgradeOptimizedConnectionProvider> serviceLoader=ServiceLoader.load(UpgradeOptimizedConnectionProvider.class,providerRegisterClass.getClassLoader());
  for (  UpgradeOptimizedConnectionProvider upgradeOptimizedConnectionProvider : serviceLoader) {
    _upgradeOptimizedConnectionProviderMap.put(upgradeOptimizedConnectionProvider.getDBType(),upgradeOptimizedConnectionProvider);
  }
}
