{
  selenium.open("/web/guest/home/");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  selenium.clickAt("link=Categories",RuntimeVariables.replace("Categories"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//li/div/div[4]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Category Name"),selenium.getText("//li/div/div[4]"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@class='aui-tree-hitarea']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//div[@class='aui-tree-hitarea']",RuntimeVariables.replace("Drop Down Arrow"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//li/ul/li[3]/div/div[4]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Subcategory3 Name"),selenium.getText("//li/ul/li[3]/div/div[4]"));
  selenium.clickAt("//li/ul/li[3]/div/div[4]",RuntimeVariables.replace("Subcategory3 Name"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (RuntimeVariables.replace("Subcategory3 Name").equals(selenium.getText("//div[@class='view-category']/div/h1/span"))) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Subcategory3 Name"),selenium.getText("//div[@class='view-category']/div/h1/span"));
  assertEquals(RuntimeVariables.replace("Description: Subcategory3 Description"),selenium.getText("//div[@class='view-category']/div[2]"));
}
