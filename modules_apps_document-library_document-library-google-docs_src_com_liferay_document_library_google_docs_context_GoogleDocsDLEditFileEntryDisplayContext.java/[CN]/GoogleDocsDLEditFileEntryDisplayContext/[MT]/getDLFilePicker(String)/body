{
  GoogleDocsMetadataHelper googleDocsMetadataHelper=new GoogleDocsMetadataHelper(dlFileEntryType);
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  return new GoogleDocsDLFilePicker(googleDocsMetadataHelper,themeDisplay,portletDisplay.getNamespace(),onFilePickCallback);
}
