{
  Object[] finderArgs=new Object[]{new Long(groupId),Boolean.valueOf(folksonomy)};
  List<TagsVocabulary> list=(List<TagsVocabulary>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_G_F,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT tagsVocabulary FROM TagsVocabulary tagsVocabulary WHERE ");
      query.append("tagsVocabulary.groupId = ?");
      query.append(" AND ");
      query.append("tagsVocabulary.folksonomy = ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("tagsVocabulary.name ASC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      qPos.add(folksonomy);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<TagsVocabulary>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_G_F,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
