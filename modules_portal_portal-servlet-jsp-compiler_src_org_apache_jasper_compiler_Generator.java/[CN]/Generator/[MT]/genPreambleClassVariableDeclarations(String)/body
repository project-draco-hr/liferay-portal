{
  if (isPoolingEnabled) {
    if (!tagHandlerPoolNames.isEmpty()) {
      for (int i=0; i < tagHandlerPoolNames.size(); i++) {
        out.printil("private org.apache.jasper.runtime.TagHandlerPool " + tagHandlerPoolNames.elementAt(i) + ";");
      }
      out.println();
    }
  }
  out.printil("private org.glassfish.jsp.api.ResourceInjector " + "_jspx_resourceInjector;");
  out.println();
}
