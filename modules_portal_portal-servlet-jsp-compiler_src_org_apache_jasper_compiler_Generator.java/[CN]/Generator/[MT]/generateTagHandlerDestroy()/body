{
  if (!isPoolingEnabled || tagHandlerPoolNames.isEmpty()) {
    return;
  }
  out.printil("public void _jspDestroy() {");
  out.pushIndent();
  for (int i=0; i < tagHandlerPoolNames.size(); i++) {
    out.printin((String)tagHandlerPoolNames.elementAt(i));
    out.println(".release();");
  }
  out.popIndent();
  out.printil("}");
  out.println();
}
