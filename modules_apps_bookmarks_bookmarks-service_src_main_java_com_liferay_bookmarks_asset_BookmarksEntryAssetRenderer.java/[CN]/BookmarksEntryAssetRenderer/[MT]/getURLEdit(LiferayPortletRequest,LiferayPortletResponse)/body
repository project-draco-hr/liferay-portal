{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(liferayPortletRequest,BookmarksPortletKeys.BOOKMARKS_ADMIN,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcRenderCommandName","/bookmarks/edit_entry");
  portletURL.setParameter("folderId",String.valueOf(_entry.getFolderId()));
  portletURL.setParameter("entryId",String.valueOf(_entry.getEntryId()));
  portletURL.setParameter("showFolderSelector",Boolean.TRUE.toString());
  return portletURL;
}
