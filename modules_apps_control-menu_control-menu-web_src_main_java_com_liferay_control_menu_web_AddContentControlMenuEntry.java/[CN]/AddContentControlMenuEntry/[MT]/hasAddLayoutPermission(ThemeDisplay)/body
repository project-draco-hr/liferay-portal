{
  Layout layout=themeDisplay.getLayout();
  if (layout.getParentLayoutId() == LayoutConstants.DEFAULT_PARENT_LAYOUT_ID) {
    return GroupPermissionUtil.contains(themeDisplay.getPermissionChecker(),layout.getGroup(),ActionKeys.ADD_LAYOUT);
  }
  return LayoutPermissionUtil.contains(themeDisplay.getPermissionChecker(),layout,ActionKeys.ADD_LAYOUT);
}
