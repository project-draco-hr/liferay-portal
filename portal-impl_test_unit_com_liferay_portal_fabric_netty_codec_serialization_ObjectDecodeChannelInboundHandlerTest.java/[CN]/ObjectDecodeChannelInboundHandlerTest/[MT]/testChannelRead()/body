{
  DateChannelHandler dateChannelHandler=new DateChannelHandler();
  try {
    dateChannelHandler.channelRead(null,null);
    Assert.fail();
  }
 catch (  UnsupportedOperationException uoe) {
  }
  ReflectionTestUtil.invoke(dateChannelHandler,"channelRead0",new Class<?>[]{ChannelHandlerContext.class,Object.class},null,null);
  UUID uuid=UUID.randomUUID();
  Assert.assertSame(uuid,dateChannelHandler.channelRead(null,uuid,null));
  Assert.assertNull(dateChannelHandler.getDate());
  Date date=new Date();
  Assert.assertSame(date,dateChannelHandler.channelRead(null,date,null));
  Assert.assertSame(date,dateChannelHandler.getDate());
  dateChannelHandler.setFailRead(true);
  Assert.assertSame(date,dateChannelHandler.channelRead(null,date,null));
  Assert.assertSame(DateChannelHandler._exception,dateChannelHandler.getThrowable());
}
