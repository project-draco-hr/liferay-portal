{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Media Gallery Test Page",RuntimeVariables.replace("Media Gallery Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Recent",RuntimeVariables.replace("Recent"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Recent"),selenium.getText("//h1[@class='header-title']/span"));
  selenium.waitForVisible("//img[@alt='DM Folder Image Title - DM Folder Image Description']");
  assertTrue(selenium.isVisible("//img[@alt='DM Folder Image Title - DM Folder Image Description']"));
  assertEquals(RuntimeVariables.replace("DM Folder Image Title"),selenium.getText("//span[@class='image-title']"));
}
