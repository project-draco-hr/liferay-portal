{
  if (Validator.isNull(friendlyURL)) {
    return friendlyURL;
  }
  friendlyURL=friendlyURL.toLowerCase();
  friendlyURL=Normalizer.normalizeToAscii(friendlyURL);
  friendlyURL=friendlyURL.replaceAll(friendlyURLPattern.pattern(),StringPool.DASH);
  friendlyURL=friendlyURL.replaceAll(_friendlyURLHyphenPattern.pattern(),StringPool.DASH);
  return friendlyURL;
}
