{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/community-site-test/home/");
  selenium.waitForVisible("link=Staging");
  selenium.clickAt("link=Staging",RuntimeVariables.replace("Staging"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//a[@id='_145_addApplication']","More"));
  selenium.clickAt("//a[@id='_145_addApplication']",RuntimeVariables.replace("More"));
  selenium.waitForElementPresent("//div[@title='Web Content Display']/p/a");
  selenium.clickAt("//div[@title='Web Content Display']/p/a",RuntimeVariables.replace("Web Content Display"));
  selenium.waitForVisible("//section");
  assertTrue(selenium.isVisible("//section"));
}
