{
  super.init(filterConfig);
  _servletContext=filterConfig.getServletContext();
  File tempDir=(File)_servletContext.getAttribute(JavaConstants.JAVAX_SERVLET_CONTEXT_TEMPDIR);
  _tempDir=new File(tempDir,_TEMP_DIR);
  _tempDir.mkdirs();
  if (PropsValues.MINIFIER_FILES_LIMIT > 0) {
    _limitedFilesCache=new LimitedFilesCache<String>(PropsValues.MINIFIER_FILES_LIMIT);
    if (_log.isDebugEnabled()) {
      _log.debug("Aggregate files limit " + PropsValues.MINIFIER_FILES_LIMIT);
    }
  }
}
