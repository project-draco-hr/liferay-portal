{
  List<Integer> countList=null;
  try {
    countList=getTableMappingSqlQuery.execute(leftPrimaryKey,rightPrimaryKey);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
  if (countList.isEmpty()) {
    return false;
  }
  int count=countList.get(0);
  if (count == 0) {
    return false;
  }
  return true;
}
