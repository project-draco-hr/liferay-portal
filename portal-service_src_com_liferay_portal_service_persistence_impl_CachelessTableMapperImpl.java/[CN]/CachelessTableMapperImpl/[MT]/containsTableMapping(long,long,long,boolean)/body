{
  List<Integer> counts=null;
  try {
    counts=getTableMappingSqlQuery.execute(companyId,leftPrimaryKey,rightPrimaryKey);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
  if (counts.isEmpty()) {
    return false;
  }
  int count=counts.get(0);
  if (count == 0) {
    return false;
  }
  return true;
}
