{
  boolean isNew=role.isNew();
  for (  ModelListener listener : listeners) {
    if (isNew) {
      listener.onBeforeCreate(role);
    }
 else {
      listener.onBeforeUpdate(role);
    }
  }
  role=updateImpl(role,merge);
  for (  ModelListener listener : listeners) {
    if (isNew) {
      listener.onAfterCreate(role);
    }
 else {
      listener.onAfterUpdate(role);
    }
  }
  return role;
}
