{
  Session session=null;
  try {
    session=openSession();
    Role role=(Role)session.get(RoleImpl.class,new Long(roleId));
    if (role == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + roleId);
      }
      throw new NoSuchRoleException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + roleId);
    }
    return remove(role);
  }
 catch (  NoSuchRoleException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
