{
  List<Role> list=findByT_S(type,subtype,0,1,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(6);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("type=");
    msg.append(type);
    msg.append(", subtype=");
    msg.append(subtype);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchRoleException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
