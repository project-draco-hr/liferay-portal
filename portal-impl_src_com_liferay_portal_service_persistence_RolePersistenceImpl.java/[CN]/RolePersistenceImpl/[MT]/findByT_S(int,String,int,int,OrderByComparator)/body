{
  Object[] finderArgs=new Object[]{new Integer(type),subtype,String.valueOf(start),String.valueOf(end),String.valueOf(obc)};
  List<Role> list=(List<Role>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_OBC_T_S,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portal.model.Role WHERE ");
      query.append("type_ = ?");
      query.append(" AND ");
      if (subtype == null) {
        query.append("subtype IS NULL");
      }
 else {
        query.append("subtype = ?");
      }
      query.append(" ");
      if (obc != null) {
        query.append("ORDER BY ");
        query.append(obc.getOrderBy());
      }
 else {
        query.append("ORDER BY ");
        query.append("name ASC");
      }
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(type);
      if (subtype != null) {
        qPos.add(subtype);
      }
      list=(List<Role>)QueryUtil.list(q,getDialect(),start,end);
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<Role>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_OBC_T_S,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
