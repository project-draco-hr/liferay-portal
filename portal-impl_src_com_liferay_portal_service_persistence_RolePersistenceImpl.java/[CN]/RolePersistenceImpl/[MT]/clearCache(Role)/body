{
  EntityCacheUtil.removeResult(RoleModelImpl.ENTITY_CACHE_ENABLED,RoleImpl.class,role.getPrimaryKey());
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_N,new Object[]{new Long(role.getCompanyId()),role.getName()});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_C_C,new Object[]{new Long(role.getCompanyId()),new Long(role.getClassNameId()),new Long(role.getClassPK())});
}
