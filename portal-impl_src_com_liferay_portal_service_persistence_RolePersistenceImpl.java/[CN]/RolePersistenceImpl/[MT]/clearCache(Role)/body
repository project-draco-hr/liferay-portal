{
  EntityCacheUtil.removeResult(RoleModelImpl.ENTITY_CACHE_ENABLED,RoleImpl.class,role.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_N,new Object[]{Long.valueOf(role.getCompanyId()),role.getName()});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_C_C,new Object[]{Long.valueOf(role.getCompanyId()),Long.valueOf(role.getClassNameId()),Long.valueOf(role.getClassPK())});
}
