{
  Role result=(Role)EntityCacheUtil.getResult(RoleModelImpl.ENTITY_CACHE_ENABLED,Role.class,roleId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      Role role=(Role)session.get(RoleImpl.class,new Long(roleId));
      cacheResult(role);
      return role;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (Role)result;
  }
}
