{
  Role role=(Role)EntityCacheUtil.getResult(RoleModelImpl.ENTITY_CACHE_ENABLED,RoleImpl.class,roleId,this);
  if (role == null) {
    Session session=null;
    try {
      session=openSession();
      role=(Role)session.get(RoleImpl.class,new Long(roleId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (role != null) {
        cacheResult(role);
      }
      closeSession(session);
    }
  }
  return role;
}
