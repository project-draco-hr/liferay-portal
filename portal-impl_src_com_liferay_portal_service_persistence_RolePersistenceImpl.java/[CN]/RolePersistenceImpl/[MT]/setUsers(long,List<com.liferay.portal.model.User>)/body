{
  try {
    clearUsers.clear(pk);
    for (int i=0; i < users.size(); i++) {
      com.liferay.portal.model.User user=users.get(i);
      addUser.add(pk,user.getPrimaryKey());
    }
  }
 catch (  DataAccessException dae) {
    throw new SystemException(dae);
  }
 finally {
    FinderCache.clearCache("Users_Roles");
  }
}
