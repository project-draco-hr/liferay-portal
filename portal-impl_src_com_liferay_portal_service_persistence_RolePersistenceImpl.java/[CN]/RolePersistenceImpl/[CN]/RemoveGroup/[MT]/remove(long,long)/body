{
  if (containsGroup.contains(roleId,groupId)) {
    ModelListener<com.liferay.portal.model.Group>[] groupListeners=groupPersistence.getListeners();
    for (    ModelListener<Role> listener : listeners) {
      listener.onBeforeRemoveAssociation(roleId,com.liferay.portal.model.Group.class.getName(),groupId);
    }
    for (    ModelListener<com.liferay.portal.model.Group> listener : groupListeners) {
      listener.onBeforeRemoveAssociation(groupId,Role.class.getName(),roleId);
    }
    _sqlUpdate.update(new Object[]{new Long(roleId),new Long(groupId)});
    for (    ModelListener<Role> listener : listeners) {
      listener.onAfterRemoveAssociation(roleId,com.liferay.portal.model.Group.class.getName(),groupId);
    }
    for (    ModelListener<com.liferay.portal.model.Group> listener : groupListeners) {
      listener.onAfterRemoveAssociation(groupId,Role.class.getName(),roleId);
    }
  }
}
