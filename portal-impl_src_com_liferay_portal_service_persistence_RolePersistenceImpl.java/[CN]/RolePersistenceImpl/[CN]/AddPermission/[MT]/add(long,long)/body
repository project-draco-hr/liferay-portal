{
  if (!_persistenceImpl.containsPermission.contains(roleId,permissionId)) {
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onBeforeAddAssociation(roleId,com.liferay.portal.model.Permission.class.getName(),permissionId);
      }
    }
    _sqlUpdate.update(new Object[]{new Long(roleId),new Long(permissionId)});
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onAfterAddAssociation(roleId,com.liferay.portal.model.Permission.class.getName(),permissionId);
      }
    }
  }
}
