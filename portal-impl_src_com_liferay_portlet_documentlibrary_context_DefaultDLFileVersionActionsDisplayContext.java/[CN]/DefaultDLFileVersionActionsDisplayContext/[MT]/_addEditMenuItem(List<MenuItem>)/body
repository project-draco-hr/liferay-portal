{
  if (_dlActionsDisplayContext.isShowActions()) {
    if (isEditButtonVisible()) {
      PortletURL editURL=_liferayPortletResponse.createRenderURL();
      editURL.setParameter("struts_action","/document_library/edit_file_entry");
      editURL.setParameter("redirect",_currentURL);
      editURL.setParameter("backURL",_currentURL);
      editURL.setParameter("fileEntryId",String.valueOf(_fileEntry.getFileEntryId()));
      menuItems.add(new URLMenuItem(DLMenuItems.MENU_ITEM_ID_EDIT,"icon-edit","edit",editURL.toString()));
    }
  }
}
