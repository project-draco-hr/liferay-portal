{
  try {
    boolean outboundMessage=(boolean)context.get(MessageContext.MESSAGE_OUTBOUND_PROPERTY);
    if (!outboundMessage) {
      return true;
    }
    Source streamSource=new StreamSource(_url.openStream());
    Transformer transformer=_transformerFactory.newTransformer(streamSource);
    LogicalMessage logicalMessage=context.getMessage();
    Source source=logicalMessage.getPayload();
    DOMResult domResult=new DOMResult();
    transformer.transform(source,domResult);
    logicalMessage.setPayload(new DOMSource(domResult.getNode()));
    return true;
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
