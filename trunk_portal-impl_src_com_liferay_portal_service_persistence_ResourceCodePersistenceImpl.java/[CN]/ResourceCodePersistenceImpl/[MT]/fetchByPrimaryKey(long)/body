{
  ResourceCode resourceCode=(ResourceCode)EntityCacheUtil.getResult(ResourceCodeModelImpl.ENTITY_CACHE_ENABLED,ResourceCodeImpl.class,codeId,this);
  if (resourceCode == null) {
    Session session=null;
    try {
      session=openSession();
      resourceCode=(ResourceCode)session.get(ResourceCodeImpl.class,Long.valueOf(codeId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (resourceCode != null) {
        cacheResult(resourceCode);
      }
      closeSession(session);
    }
  }
  return resourceCode;
}
