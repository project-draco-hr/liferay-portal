{
  Session session=null;
  try {
    session=openSession();
    TagsProperty tagsProperty=(TagsProperty)session.get(TagsPropertyImpl.class,new Long(propertyId));
    if (tagsProperty == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No TagsProperty exists with the primary key " + propertyId);
      }
      throw new NoSuchPropertyException("No TagsProperty exists with the primary key " + propertyId);
    }
    return remove(tagsProperty);
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
