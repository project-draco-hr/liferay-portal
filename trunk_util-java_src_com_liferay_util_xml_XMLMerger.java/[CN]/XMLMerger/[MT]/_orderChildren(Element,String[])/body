{
  if (orderedChildrenNames == null) {
    return;
  }
  List<Element> elements=new ArrayList<Element>();
  for (int i=0; i < orderedChildrenNames.length; i++) {
    elements.addAll(parent.elements(orderedChildrenNames[i]));
  }
  for (  Element el : elements) {
    el.detach();
    parent.add(el);
  }
}
