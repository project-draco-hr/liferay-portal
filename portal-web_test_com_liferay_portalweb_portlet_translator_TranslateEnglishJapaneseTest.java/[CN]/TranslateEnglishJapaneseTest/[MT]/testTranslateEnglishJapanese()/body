{
  selenium.select("_26_id",RuntimeVariables.replace("label=English to Japanese"));
  selenium.type("_26_text",RuntimeVariables.replace("My name is JR Skywalker, fluent in over 6 million forms of communication."));
  selenium.click(RuntimeVariables.replace("//input[@value='Translate']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("???????????????6,000,000???????????Skywalker???? "));
}
