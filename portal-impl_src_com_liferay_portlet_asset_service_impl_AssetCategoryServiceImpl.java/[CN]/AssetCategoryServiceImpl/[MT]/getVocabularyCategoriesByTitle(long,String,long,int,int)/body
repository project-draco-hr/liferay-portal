{
  Hits hits=getHits(groupId,title,new long[]{vocabularyId},start,end);
  Document[] documents=hits.getDocs();
  List<AssetCategory> assetCategories=new ArrayList<AssetCategory>(documents.length);
  for (  Document document : documents) {
    long categoryId=GetterUtil.getLong(document.get(Field.CATEGORY_ID));
    AssetCategory assetCategory=getCategory(categoryId);
    assetCategories.add(assetCategory);
  }
  return assetCategories;
}
