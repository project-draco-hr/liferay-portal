{
  JSONArray categories=JSONFactoryUtil.createJSONArray();
  for (  AssetVocabulary vocabulary : AssetVocabularyServiceUtil.getVocabularies(vocabularyIds)) {
    List<AssetCategory> categoriesVocabulary=assetCategoryFinder.findByG_N_V(groupId,name,vocabulary.getVocabularyId(),start,end,null);
    JSONArray convertedCategories=convertToJSON(categoriesVocabulary);
    for (int i=0; i < convertedCategories.length(); ++i) {
      categories.put(convertedCategories.getJSONObject(i));
    }
  }
  return categories;
}
