{
  JSONObject jsonObject=JSONFactoryUtil.createJSONObject();
  int total=0;
  List<AssetCategory> categories=filterCategories(assetCategoryLocalService.getVocabularyCategories(vocabularyId,start,end,obc));
  total=categories.size();
  JSONArray categoriesJSONArray=toJSONArray(categories);
  jsonObject.put("categories",categoriesJSONArray);
  jsonObject.put("total",total);
  return jsonObject;
}
