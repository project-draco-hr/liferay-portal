{
  JSONArray jsonArray=JSONFactoryUtil.createJSONArray();
  for (  AssetVocabulary vocabulary : assetVocabularyService.getVocabularies(vocabularyIds)) {
    for (int i=0; i < groupIds.length; i++) {
      List<AssetCategory> vocabularyCategory=assetCategoryFinder.findByG_N_V(groupIds[i],name,vocabulary.getVocabularyId(),start,end,null);
      JSONArray vocabularyCategoryJSONArray=toJSONArray(vocabularyCategory);
      int length=vocabularyCategoryJSONArray.length();
      for (int j=0; j < length; j++) {
        JSONObject vocabularyCategoryJSONObject=vocabularyCategoryJSONArray.getJSONObject(j);
        jsonArray.put(vocabularyCategoryJSONObject);
      }
    }
  }
  return jsonArray;
}
