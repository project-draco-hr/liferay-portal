{
  try {
    com.liferay.portlet.messageboards.model.MBMessage returnValue=MBMessageServiceUtil.addMessage(categoryId,threadId,parentMessageId,subject,body,files,anonymous,priority,serviceContext);
    return com.liferay.portlet.messageboards.model.MBMessageSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
