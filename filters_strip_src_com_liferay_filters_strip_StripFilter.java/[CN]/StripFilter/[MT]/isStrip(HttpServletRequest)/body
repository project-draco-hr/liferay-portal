{
  if (!ParamUtil.get(req,_STRIP,true)) {
    return false;
  }
 else {
    boolean action=ParamUtil.getBoolean(req,"p_p_action");
    String windowState=ParamUtil.getString(req,"p_p_state");
    if (action && windowState.equals("exclusive")) {
      return false;
    }
 else {
      return true;
    }
  }
}
