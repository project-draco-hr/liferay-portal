{
  try {
    SearchContext searchContext=buildSearchContext(companyId,groupIds,userId,classTypeId,userName,title,description,assetCategoryIds,assetTagNames,showNonindexable,statuses,andSearch,start,end);
    return doSearch(companyId,className,searchContext);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
