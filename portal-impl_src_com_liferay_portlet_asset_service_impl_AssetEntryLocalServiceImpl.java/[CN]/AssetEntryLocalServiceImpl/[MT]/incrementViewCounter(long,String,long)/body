{
  User user=userPersistence.findByPrimaryKey(userId);
  assetEntryLocalService.incrementViewCounter(user.getUserId(),className,classPK,1);
  AssetEntry assetEntry=getEntry(className,classPK);
  if (!user.isDefaultUser()) {
    SocialActivityHandlerUtil.addActivity(user.getUserId(),assetEntry,SocialActivityConstants.TYPE_VIEW,StringPool.BLANK,0);
  }
  return assetEntry;
}
