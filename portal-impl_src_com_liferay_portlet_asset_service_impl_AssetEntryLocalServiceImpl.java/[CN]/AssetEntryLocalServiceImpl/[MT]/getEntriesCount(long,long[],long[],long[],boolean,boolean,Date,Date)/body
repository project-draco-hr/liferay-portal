{
  if ((tagIds.length == 0) && (notTagIds.length == 0)) {
    return assetEntryFinder.countEntries(groupId,classNameIds,excludeZeroViewCount,publishDate,expirationDate);
  }
 else   if (andOperator) {
    return assetEntryFinder.countByAndTagIds(groupId,classNameIds,tagIds,notTagIds,excludeZeroViewCount,publishDate,expirationDate);
  }
 else {
    return assetEntryFinder.countByOrTagIds(groupId,classNameIds,tagIds,notTagIds,excludeZeroViewCount,publishDate,expirationDate);
  }
}
