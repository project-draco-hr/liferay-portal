{
  Lock readLock=_cacheListenersReadWriteLock.readLock();
  try {
    readLock.lock();
    if (updated) {
      for (      CacheListener cacheListener : _cacheListeners) {
        cacheListener.notifyEntryUpdated(this,key,value);
      }
    }
 else {
      for (      CacheListener cacheListener : _cacheListeners) {
        cacheListener.notifyEntryPut(this,key,value);
      }
    }
  }
  finally {
    readLock.unlock();
  }
}
