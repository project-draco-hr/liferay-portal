{
  if (updated) {
    for (    CacheListener<K,V> cacheListener : _cacheListeners) {
      cacheListener.notifyEntryUpdated(this,key,value,timeToLive);
    }
  }
 else {
    for (    CacheListener<K,V> cacheListener : _cacheListeners) {
      cacheListener.notifyEntryPut(this,key,value,timeToLive);
    }
  }
}
