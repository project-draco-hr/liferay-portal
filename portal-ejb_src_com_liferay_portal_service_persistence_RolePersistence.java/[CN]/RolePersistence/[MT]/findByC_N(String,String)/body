{
  Role role=fetchByC_N(companyId,name);
  if (role == null) {
    String msg="No Role exists with the key ";
    msg+=StringPool.OPEN_CURLY_BRACE;
    msg+="companyId=";
    msg+=companyId;
    msg+=", ";
    msg+="name=";
    msg+=name;
    msg+=StringPool.CLOSE_CURLY_BRACE;
    if (_log.isWarnEnabled()) {
      _log.warn(msg);
    }
    throw new NoSuchRoleException(msg);
  }
  return role;
}
