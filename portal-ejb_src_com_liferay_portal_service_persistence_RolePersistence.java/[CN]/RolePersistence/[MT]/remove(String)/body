{
  Session session=null;
  try {
    session=openSession();
    RoleHBM roleHBM=(RoleHBM)session.get(RoleHBM.class,roleId);
    if (roleHBM == null) {
      _log.warn("No Role exists with the primary key " + roleId.toString());
      throw new NoSuchRoleException("No Role exists with the primary key " + roleId.toString());
    }
    session.delete(roleHBM);
    session.flush();
    return RoleHBMUtil.model(roleHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
