{
  Session session=null;
  try {
    session=openSession();
    Role role=(Role)session.get(RoleImpl.class,roleId);
    if (role == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No Role exists with the primary key " + roleId);
      }
      throw new NoSuchRoleException("No Role exists with the primary key " + roleId);
    }
    return remove(role);
  }
 catch (  NoSuchRoleException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
