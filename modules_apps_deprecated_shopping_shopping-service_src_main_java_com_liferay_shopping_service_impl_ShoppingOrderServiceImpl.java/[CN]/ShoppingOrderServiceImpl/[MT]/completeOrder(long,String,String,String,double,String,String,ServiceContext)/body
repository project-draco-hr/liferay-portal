{
  ShoppingOrder order=shoppingOrderPersistence.findByNumber(number);
  ShoppingOrderPermission.check(getPermissionChecker(),groupId,order.getOrderId(),ActionKeys.UPDATE);
  shoppingOrderLocalService.completeOrder(number,ppTxnId,ppPaymentStatus,ppPaymentGross,ppReceiverEmail,ppPayerEmail,false,serviceContext);
}
