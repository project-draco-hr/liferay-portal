{
  Object obj=SingleVMPoolUtil.get(_cache,key);
  if (obj == null) {
    try {
      obj=wci.convert(key);
      int timeToLive=(int)(wci.getRefreshTime() / Time.SECOND);
      _cache.put(key,obj,timeToLive);
    }
 catch (    WebCacheException wce) {
      _log.error(wce.getMessage());
    }
  }
  return obj;
}
