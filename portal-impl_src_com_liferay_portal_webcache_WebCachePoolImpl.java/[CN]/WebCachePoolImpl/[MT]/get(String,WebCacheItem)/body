{
  Object obj=_singleVMPool.get(_cache,key);
  if (obj == null) {
    try {
      obj=wci.convert(key);
      int timeToLive=(int)(wci.getRefreshTime() / Time.SECOND);
      _cache.put(key,obj,timeToLive);
    }
 catch (    WebCacheException wce) {
      Throwable cause=wce.getCause();
      if (cause != null) {
        _log.error(cause,cause);
      }
 else {
        _log.error(wce,wce);
      }
    }
  }
  return obj;
}
