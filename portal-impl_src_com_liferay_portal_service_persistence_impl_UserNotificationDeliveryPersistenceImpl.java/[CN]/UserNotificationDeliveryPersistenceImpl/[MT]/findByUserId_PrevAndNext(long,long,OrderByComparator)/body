{
  UserNotificationDelivery userNotificationDelivery=findByPrimaryKey(userNotificationDeliveryId);
  Session session=null;
  try {
    session=openSession();
    UserNotificationDelivery[] array=new UserNotificationDeliveryImpl[3];
    array[0]=getByUserId_PrevAndNext(session,userNotificationDelivery,userId,orderByComparator,true);
    array[1]=userNotificationDelivery;
    array[2]=getByUserId_PrevAndNext(session,userNotificationDelivery,userId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
