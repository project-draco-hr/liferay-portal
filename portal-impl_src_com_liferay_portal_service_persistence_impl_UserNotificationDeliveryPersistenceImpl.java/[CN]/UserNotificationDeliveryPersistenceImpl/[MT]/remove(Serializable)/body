{
  Session session=null;
  try {
    session=openSession();
    UserNotificationDelivery userNotificationDelivery=(UserNotificationDelivery)session.get(UserNotificationDeliveryImpl.class,primaryKey);
    if (userNotificationDelivery == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchUserNotificationDeliveryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(userNotificationDelivery);
  }
 catch (  NoSuchUserNotificationDeliveryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
