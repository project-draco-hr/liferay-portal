{
  long nodeId=ParamUtil.getLong(actionRequest,"nodeId");
  String title=ParamUtil.getString(actionRequest,"title");
  String attachment=ParamUtil.getString(actionRequest,"fileName");
  JSONObject jsonObject=com.liferay.portlet.trash.action.ActionUtil.checkEntry(actionRequest);
  writeJSON(actionRequest,actionResponse,jsonObject);
  WikiPageServiceUtil.restorePageAttachmentFromTrash(nodeId,title,attachment);
}
