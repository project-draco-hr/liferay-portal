{
  long[] restoreEntryIds=StringUtil.split(ParamUtil.getString(actionRequest,"restoreEntryIds"),0L);
  for (  long restoreEntryId : restoreEntryIds) {
    DLFileEntry dlFileEntry=DLFileEntryLocalServiceUtil.getFileEntry(restoreEntryId);
    DLFolder dlFolder=DLFolderLocalServiceUtil.getDLFolder(dlFileEntry.getFolderId());
    long resourcePrimKey=GetterUtil.getLong(dlFolder.getName());
    WikiPage wikiPage=WikiPageLocalServiceUtil.getPage(resourcePrimKey);
    WikiPageServiceUtil.restorePageAttachmentFromTrash(wikiPage.getNodeId(),wikiPage.getTitle(),dlFileEntry.getTitle());
  }
}
