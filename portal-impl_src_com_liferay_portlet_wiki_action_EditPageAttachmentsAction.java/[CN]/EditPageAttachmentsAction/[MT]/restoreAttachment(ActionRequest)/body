{
  long[] restoreEntryIds=StringUtil.split(ParamUtil.getString(actionRequest,"restoreEntryIds"),0L);
  for (  long restoreEntryId : restoreEntryIds) {
    FileEntry fileEntry=PortletFileRepositoryUtil.getPortletFileEntry(restoreEntryId);
    WikiPage wikiPage=WikiPageAttachmentUtil.getPageByFileEntryId(fileEntry.getFileEntryId());
    WikiPageServiceUtil.restorePageAttachmentFromTrash(wikiPage.getNodeId(),wikiPage.getTitle(),fileEntry.getTitle());
  }
}
