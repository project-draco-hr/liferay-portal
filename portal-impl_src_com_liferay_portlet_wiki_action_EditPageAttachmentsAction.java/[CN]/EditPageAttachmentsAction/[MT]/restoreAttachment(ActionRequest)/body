{
  String[] restoreAttachmentId=StringUtil.split(ParamUtil.getString(actionRequest,"restoreAttachmentId"));
  long nodeId=GetterUtil.getLong(restoreAttachmentId[0],0L);
  String title=GetterUtil.getString(restoreAttachmentId[1],StringPool.BLANK);
  String attachment=GetterUtil.getString(restoreAttachmentId[2],StringPool.BLANK);
  WikiPageServiceUtil.movePageAttachmentFromTrash(nodeId,title,attachment);
}
