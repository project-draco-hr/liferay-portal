{
  PortalCacheClusterConfiguration portalCacheClusterConfiguration=Configurable.createConfigurable(PortalCacheClusterConfiguration.class,componentContext.getProperties());
  Priority[] priorities=portalCacheClusterConfiguration.priorities();
  _portalCacheClusterChannels=new ArrayList<>(priorities.length);
  for (  Priority priority : priorities) {
    PortalCacheClusterChannel portalCacheClusterChannel=_portalCacheClusterChannelFactory.createPortalCacheClusterChannel(priority);
    _portalCacheClusterChannels.add(portalCacheClusterChannel);
  }
  if (_portalCacheClusterChannelSelector == null) {
    _portalCacheClusterChannelSelector=new UniformPortalCacheClusterChannelSelector();
  }
}
