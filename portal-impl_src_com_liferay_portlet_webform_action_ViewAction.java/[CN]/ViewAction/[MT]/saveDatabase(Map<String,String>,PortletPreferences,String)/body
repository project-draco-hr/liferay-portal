{
  WebFormUtil.checkTable(databaseTableName,preferences);
  long classPK=CounterLocalServiceUtil.increment(WebFormUtil.class.getName());
  try {
    for (    String fieldLabel : fieldsMap.keySet()) {
      String fieldValue=fieldsMap.get(fieldLabel);
      ExpandoValueLocalServiceUtil.addValue(WebFormUtil.class.getName(),databaseTableName,fieldLabel,classPK,fieldValue);
    }
    return true;
  }
 catch (  Exception e) {
    _log.error("The web form data could not be saved to the database",e);
    return false;
  }
}
