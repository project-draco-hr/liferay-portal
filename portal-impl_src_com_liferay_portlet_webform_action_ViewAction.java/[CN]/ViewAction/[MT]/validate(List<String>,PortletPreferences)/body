{
  for (int i=1; i < _MAX_FIELDS; i++) {
    String fieldValue=fieldValues.get(i - 1);
    String fieldLabel=prefs.getValue("fieldLabel" + i,StringPool.BLANK);
    boolean fieldOptional=GetterUtil.getBoolean(prefs.getValue("fieldOptional" + i,StringPool.BLANK));
    if (!fieldOptional && Validator.isNotNull(fieldLabel) && Validator.isNull(fieldValue)) {
      return false;
    }
  }
  return true;
}
