{
  for (int i=1; i < 10; i++) {
    String fieldLabel=prefs.getValue("fieldLabel" + i,StringPool.BLANK);
    boolean fieldOptional=GetterUtil.getBoolean(prefs.getValue("fieldOptional" + i,StringPool.BLANK),false);
    if ((!fieldOptional) && (Validator.isNotNull(fieldLabel)) && (Validator.isNull((String)fieldValues.get(i - 1)))) {
      return false;
    }
  }
  return true;
}
