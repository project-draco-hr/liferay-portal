{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  Thread.sleep(30000);
  Thread.sleep(15000);
  selenium.waitForElementPresent("//input[@value='Extend']");
  Thread.sleep(30000);
  Thread.sleep(30000);
  selenium.waitForTextPresent("Warning! Due to inactivity, your session has expired.");
  Thread.sleep(5000);
  selenium.waitForElementPresent("link=Session Expiration Test Page");
  selenium.click(RuntimeVariables.replace("link=Session Expiration Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Sign In"),selenium.getText("sign-in"));
}
