{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  ClassName newClassName=addClassName();
  _persistence.clearCache();
  ClassName existingClassName=_persistence.findByPrimaryKey(newClassName.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingClassName.getValue(),ReflectionTestUtil.invoke(existingClassName,"getOriginalValue",new Class<?>[0])));
}
