{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.MEMBERSHIP_POLICY_ORGANIZATIONS);
  }
  ClassLoader classLoader=ClassLoaderUtil.getPortalClassLoader();
  _originalOrganizationMembershipPolicy=(OrganizationMembershipPolicy)InstanceFactory.newInstance(classLoader,PropsValues.MEMBERSHIP_POLICY_ORGANIZATIONS);
  _organizationMembershipPolicy=_originalOrganizationMembershipPolicy;
}
