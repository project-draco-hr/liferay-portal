{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.MEMBERSHIP_POLICY_ORGANIZATIONS);
  }
  ClassLoader classLoader=ClassLoaderUtil.getPortalClassLoader();
  _originalMembershipPolicy=(OrganizationMembershipPolicy)InstanceFactory.newInstance(classLoader,PropsValues.MEMBERSHIP_POLICY_ORGANIZATIONS);
  _membershipPolicy=_originalMembershipPolicy;
}
