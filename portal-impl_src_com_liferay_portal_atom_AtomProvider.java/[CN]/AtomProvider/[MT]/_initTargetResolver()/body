{
  RegexTargetResolver targetResolver=new RegexTargetResolver();
  targetResolver.setPattern(_SERVICE_PATTERN,TargetType.TYPE_SERVICE);
  targetResolver.setPattern(_COLLECTION_PATTERN,TargetType.TYPE_COLLECTION,"collection");
  targetResolver.setPattern(_COLLECTION_ENTRY_PATTERN,TargetType.TYPE_ENTRY,"collection","entry");
  targetResolver.setPattern(_COLLECTION_MEDIA_PATTERN,TargetType.TYPE_MEDIA,"collection","media");
  setTargetResolver(targetResolver);
}
