{
  _imports.removeAll(Arrays.asList(StringUtil.split(PropsUtil.get("bundle.import.exclude.packages"),CharPool.SEMICOLON)));
  _osgiManifestBuilder.addImportPackages(_imports.toArray(new String[_imports.size()]));
  _osgiManifestBuilder.addExportPackages(_exports.toArray(new String[_exports.size()]));
  _osgiManifestBuilder.addBundleClasspath(StringUtil.merge(_classPathFiles));
  _osgiManifestBuilder.addBundleActivator(ArquillianBundleActivator.class);
  return new ByteArrayAsset(_osgiManifestBuilder.openStream());
}
