{
  DDMFormFieldValueRenderer ddmFormFieldValueRenderer=DDMFormFieldValueRendererRegistryUtil.getDDMFormFieldValueRenderer(TestDDMFormFieldValueRenderer.DDM_FORM_FIELD_TYPE);
  Class<?> clazz=ddmFormFieldValueRenderer.getClass();
  Assert.assertEquals(TestDDMFormFieldValueRenderer.class.getName(),clazz.getName());
}
