{
  List entries=TagsAssetLocalServiceUtil.getTagsEntries(getAssetId());
  Iterator itr=entries.iterator();
  StringMaker sm=new StringMaker();
  while (itr.hasNext()) {
    TagsEntry entry=(TagsEntry)itr.next();
    sm.append(entry.getName());
    if (itr.hasNext()) {
      sm.append(", ");
    }
  }
  return sm.toString();
}
