{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  MDRRuleGroupInstance mdrRuleGroupInstance : mdrRuleGroupInstances) {
    EntityCacheUtil.removeResult(MDRRuleGroupInstanceModelImpl.ENTITY_CACHE_ENABLED,MDRRuleGroupInstanceImpl.class,mdrRuleGroupInstance.getPrimaryKey());
    clearUniqueFindersCache((MDRRuleGroupInstanceModelImpl)mdrRuleGroupInstance);
  }
}
