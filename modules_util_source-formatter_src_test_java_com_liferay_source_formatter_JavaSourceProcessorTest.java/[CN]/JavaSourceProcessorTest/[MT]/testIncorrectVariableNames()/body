  test("IncorrectVariableNames1.testjava",new String[]{"Protected or public constant '_TEST_1' must match " + "pattern '^[a-zA-Z0-9][_a-zA-Z0-9]*$'","Protected or public non-static field '_test2' must match " + "pattern '^[a-z0-9][_a-zA-Z0-9]*$'"},new Integer[]{22,28});
