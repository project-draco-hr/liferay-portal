{
  PortletURL editURL=PortalUtil.getControlPanelPortletURL(portletRequest,DLPortletKeys.DOCUMENT_LIBRARY_ADMIN,PortletRequest.RENDER_PHASE);
  editURL.setParameter("mvcRenderCommandName","/document_library/edit_file_entry");
  editURL.setParameter("redirect",themeDisplay.getURLCurrent());
  editURL.setParameter("fileEntryId",String.valueOf(_fileEntry.getFileEntryId()));
  return editURL.toString();
}
