{
  _className=element.attributeValue("class-name");
  _displayName=GetterUtil.getString(element.attributeValue("display-name"));
  _referrers=new HashMap<String,String>();
  String referrerClassName=element.attributeValue("referrer-class-name");
  String referrerDisplayName=GetterUtil.getString(element.attributeValue("referrer-display-name"));
  addReferrer(referrerClassName,referrerDisplayName);
}
