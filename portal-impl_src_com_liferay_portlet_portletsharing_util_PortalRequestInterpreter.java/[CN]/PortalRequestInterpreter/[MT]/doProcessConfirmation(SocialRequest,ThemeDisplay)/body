{
  try {
    String className=PortalActivityInterpreter.class.getName();
    String extraData=request.getExtraData();
    String[] extraDataArray=StringUtil.split(extraData,ShareRequestKeys.SHARE_EXTRA_DATA_DELIMETER);
    String portletTitle=extraDataArray[1];
    SocialActivityLocalServiceUtil.addActivity(request.getUserId(),0,className,request.getUserId(),ShareRequestKeys.SHARE_REQUEST,portletTitle,request.getReceiverUserId());
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return true;
}
