{
  String className=PortalUtil.getClassName(request.getClassNameId());
  if (className != null) {
    String[] extDataArray=StringUtil.split(request.getExtraData(),ShareRequestKeys.SHARE_EXTRA_DATA_DELIMETER);
    String portletTitle=extDataArray[1];
    String recieverName=PortalUtil.getUserName(request.getUserId(),null);
    String data=themeDisplay.translate("request-share-widget-message",new Object[]{recieverName,portletTitle});
    return new SocialRequestFeedEntry(data,null);
  }
  return null;
}
