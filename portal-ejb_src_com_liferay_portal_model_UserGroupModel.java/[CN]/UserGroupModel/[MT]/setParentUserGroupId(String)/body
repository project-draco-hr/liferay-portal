{
  if (((parentUserGroupId == null) && (_parentUserGroupId != null)) || ((parentUserGroupId != null) && (_parentUserGroupId == null)) || ((parentUserGroupId != null) && (_parentUserGroupId != null) && !parentUserGroupId.equals(_parentUserGroupId))) {
    if (!XSS_ALLOW_PARENTUSERGROUPID) {
      parentUserGroupId=XSSUtil.strip(parentUserGroupId);
    }
    _parentUserGroupId=parentUserGroupId;
    setModified(true);
  }
}
