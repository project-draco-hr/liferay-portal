{
  if (((userGroupId == null) && (_userGroupId != null)) || ((userGroupId != null) && (_userGroupId == null)) || ((userGroupId != null) && (_userGroupId != null) && !userGroupId.equals(_userGroupId))) {
    if (!XSS_ALLOW_USERGROUPID) {
      userGroupId=XSSUtil.strip(userGroupId);
    }
    _userGroupId=userGroupId;
    setModified(true);
  }
}
