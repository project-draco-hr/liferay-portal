{
  if (allowOmniadmin && (authenticateOmniadmin(companyId,emailAddress,screenName,userId) == SUCCESS)) {
    return SUCCESS;
  }
  if (PrefsPropsUtil.getBoolean(companyId,PropsKeys.LDAP_AUTH_REQUIRED,_ldapAuthConfiguration.required())) {
    return failureCode;
  }
 else {
    return SUCCESS;
  }
}
