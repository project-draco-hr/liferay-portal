{
  List<StagedModel> dependentStagedModels=dependentStagedModelsMap.get(Organization.class.getSimpleName());
  Assert.assertEquals(1,dependentStagedModels.size());
  Organization organization=(Organization)dependentStagedModels.get(0);
  Organization importedSubOrganization=OrganizationLocalServiceUtil.fetchOrganizationByUuidAndCompanyId(organization.getUuid(),group.getCompanyId());
  Assert.assertNotNull(importedSubOrganization);
}
