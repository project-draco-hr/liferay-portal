{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId(),TestPropsValues.getUserId());
  return DLAppLocalServiceUtil.addFileEntry(TestPropsValues.getUserId(),group.getGroupId(),folderId,sourceFileName,ContentTypes.TEXT_PLAIN,RandomTestUtil.randomBytes(TikaSafeRandomizerBumper.INSTANCE),serviceContext);
}
