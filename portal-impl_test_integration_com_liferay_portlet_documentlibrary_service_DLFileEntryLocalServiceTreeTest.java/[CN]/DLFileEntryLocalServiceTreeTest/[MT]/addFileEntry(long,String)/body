{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId(),TestPropsValues.getUserId());
  return DLAppLocalServiceUtil.addFileEntry(TestPropsValues.getUserId(),_group.getGroupId(),folderId,sourceFileName,ContentTypes.TEXT_PLAIN,RandomTestUtil.randomBytes(TikaSafeRandomizerBumper.INSTANCE),serviceContext);
