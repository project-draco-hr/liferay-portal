{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId(),TestPropsValues.getUserId());
  DLAppTestUtil.populateNotificationsServiceContext(serviceContext,Constants.ADD);
  return DLAppLocalServiceUtil.addFileEntry(TestPropsValues.getUserId(),_group.getGroupId(),folderId,sourceFileName,ContentTypes.TEXT_PLAIN,RandomTestUtil.randomBytes(),serviceContext);
}
