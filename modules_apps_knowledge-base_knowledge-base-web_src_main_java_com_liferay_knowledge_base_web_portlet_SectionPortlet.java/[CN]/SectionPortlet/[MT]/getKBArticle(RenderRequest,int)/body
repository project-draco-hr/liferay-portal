{
  long resourcePrimKey=ParamUtil.getLong(renderRequest,"resourcePrimKey");
  if (resourcePrimKey > 0) {
    return _kbArticleService.getLatestKBArticle(resourcePrimKey,status);
  }
  String urlTitle=ParamUtil.getString(renderRequest,"urlTitle");
  if (Validator.isNull(urlTitle)) {
    return null;
  }
  long groupId=_portal.getScopeGroupId(renderRequest);
  String kbFolderUrlTitle=ParamUtil.getString(renderRequest,"kbFolderUrlTitle");
  if (Validator.isNotNull(kbFolderUrlTitle)) {
    return _kbArticleLocalService.getKBArticleByUrlTitle(groupId,kbFolderUrlTitle,urlTitle);
  }
  return _kbArticleLocalService.getKBArticleByUrlTitle(groupId,KBFolderConstants.DEFAULT_PARENT_FOLDER_ID,urlTitle);
}
