{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMList.class,DDMList.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("listId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("listId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
