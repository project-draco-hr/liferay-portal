{
  DDMList newDDMList=addDDMList();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMList.class,DDMList.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("listId",newDDMList.getListId()));
  List<DDMList> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  DDMList existingDDMList=result.get(0);
  assertEquals(existingDDMList,newDDMList);
}
