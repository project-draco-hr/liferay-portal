{
  selenium.clickAt("link=Manage Pages",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_88_title_en_US",RuntimeVariables.replace("Home"));
  selenium.type("TypeSettingsProperties(javascript-1)",RuntimeVariables.replace("<script type=\"text/javascript\"> \n$layout.getTypeSettingsProperties().getProperty(\"javascript-1\") \n$layout.getTypeSettingsProperties().getProperty(\"javascript-2\") \n$layout.getTypeSettingsProperties().getProperty(\"javascript-3\") \n</script>"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertTrue(selenium.isTextPresent("<script type=\"text/javascript\"> $layout.getTypeSettingsProperties().getProperty(\"javascript-1\") $layout.getTypeSettingsProperties().getProperty(\"javascript-2\") $layout.getTypeSettingsProperties().getProperty(\"javascript-3\") </script>"));
  selenium.clickAt("link=Return to Full Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
}
