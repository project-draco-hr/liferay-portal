{
  Group group=ServiceTestUtil.addGroup("Test Group");
  JournalFolder folder=_addFolder(group.getGroupId(),0,"Test Folder");
  JournalArticle article=_addArticle(group.getGroupId(),folder.getFolderId(),"Test Article","This is a test article.");
  Assert.assertTrue(article.getFolderId() == folder.getFolderId());
  JournalFolderLocalServiceUtil.deleteFolder(folder);
  GroupLocalServiceUtil.deleteGroup(group);
}
