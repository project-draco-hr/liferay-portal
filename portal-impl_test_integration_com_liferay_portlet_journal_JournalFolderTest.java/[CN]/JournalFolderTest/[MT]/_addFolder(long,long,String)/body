{
  JournalFolder folder=JournalFolderLocalServiceUtil.fetchFolder(groupId,name);
  if (folder != null) {
    return folder;
  }
  String description="This is a test folder";
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext();
  return JournalFolderLocalServiceUtil.addFolder(TestPropsValues.getUserId(),groupId,parentFolderId,name,description,serviceContext);
}
