{
  String repositoryHome=_jcrStoreConfiguration.jackrabbitRepositoryHome();
  try {
    _transientRepository=new TransientRepository(_jcrStoreConfiguration.jackrabbitConfigFilePath(),repositoryHome);
  }
 catch (  Exception e) {
    _log.error("Problem initializing Jackrabbit JCR.",e);
    throw e;
  }
  if (_log.isInfoEnabled()) {
    _log.info("Jackrabbit JCR intialized with config file path " + _jcrStoreConfiguration.jackrabbitConfigFilePath() + " and repository home "+ repositoryHome);
  }
}
