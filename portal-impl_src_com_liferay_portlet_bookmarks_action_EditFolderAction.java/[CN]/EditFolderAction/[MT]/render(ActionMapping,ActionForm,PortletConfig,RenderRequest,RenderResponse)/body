{
  try {
    ActionUtil.getFolder(req);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchFolderException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.bookmarks.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward(getForward(req,"portlet.bookmarks.edit_folder"));
}
