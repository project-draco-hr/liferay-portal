{
  super.setAttributes(request);
  String value=_value;
  if (Validator.isNull(value)) {
    if (_type.equals("submit")) {
      value="save";
    }
 else     if (_type.equals("cancel")) {
      value="cancel";
    }
 else     if (_type.equals("reset")) {
      value="reset";
    }
  }
  setNamespacedAttribute(request,"value",value);
}
