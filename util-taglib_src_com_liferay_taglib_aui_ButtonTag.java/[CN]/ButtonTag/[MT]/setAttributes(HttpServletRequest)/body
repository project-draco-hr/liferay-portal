{
  super.setAttributes(request);
  String value=getValue();
  if (Validator.isNull(value)) {
    String type=getType();
    if (type.equals("submit")) {
      value="save";
    }
 else     if (type.equals("cancel")) {
      value="cancel";
    }
 else     if (type.equals("reset")) {
      value="reset";
    }
  }
  setNamespacedAttribute(request,"value",value);
}
