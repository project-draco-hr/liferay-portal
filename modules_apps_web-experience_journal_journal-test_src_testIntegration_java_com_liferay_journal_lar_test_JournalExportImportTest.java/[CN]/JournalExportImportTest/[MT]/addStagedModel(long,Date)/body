{
  String title=RandomTestUtil.randomString();
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(groupId);
  serviceContext.setCommand(Constants.ADD);
  serviceContext.setCreateDate(createdDate);
  serviceContext.setLayoutFullURL("http://localhost");
  serviceContext.setModifiedDate(createdDate);
  return JournalTestUtil.addArticle(groupId,JournalFolderConstants.DEFAULT_PARENT_FOLDER_ID,JournalArticleConstants.CLASSNAME_ID_DEFAULT,title,title,RandomTestUtil.randomString(),LocaleUtil.getSiteDefault(),false,false,serviceContext);
}
