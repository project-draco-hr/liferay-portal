{
  ResourceAction newResourceAction=addResourceAction();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ResourceAction.class,ResourceAction.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("resourceActionId"));
  Object newResourceActionId=newResourceAction.getResourceActionId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("resourceActionId",new Object[]{newResourceActionId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Object existingResourceActionId=result.get(0);
  assertEquals(existingResourceActionId,newResourceActionId);
}
