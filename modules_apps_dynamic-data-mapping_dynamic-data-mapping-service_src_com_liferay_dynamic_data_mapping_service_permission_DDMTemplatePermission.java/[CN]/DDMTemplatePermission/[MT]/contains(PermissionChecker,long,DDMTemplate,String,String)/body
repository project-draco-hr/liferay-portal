{
  if (Validator.isNotNull(portletId)) {
    Boolean hasPermission=StagingPermissionUtil.hasPermission(permissionChecker,groupId,DDMTemplate.class.getName(),template.getTemplateId(),portletId,actionId);
    if (hasPermission != null) {
      return hasPermission.booleanValue();
    }
  }
  return contains(permissionChecker,template,actionId);
}
