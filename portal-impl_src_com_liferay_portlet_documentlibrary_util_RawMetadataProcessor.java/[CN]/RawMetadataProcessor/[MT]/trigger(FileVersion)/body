{
  if (PropsValues.DL_FILE_ENTRY_PROCESSORS_TRIGGER_SYNCHRONOUSLY) {
    try {
      MessageBusUtil.sendSynchronousMessage(DestinationNames.DOCUMENT_LIBRARY_RAW_METADATA_PROCESSOR,fileVersion);
    }
 catch (    MessageBusException mbe) {
      if (_log.isWarnEnabled()) {
        _log.warn(mbe,mbe);
      }
    }
  }
 else {
    MessageBusUtil.sendMessage(DestinationNames.DOCUMENT_LIBRARY_RAW_METADATA_PROCESSOR,fileVersion);
  }
}
