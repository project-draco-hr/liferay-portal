{
  int property=GetterUtil.getInteger(properties.getProperty(popertyName + "[" + container+ "]"),defaultValue);
  if (property > maxValue) {
    property=maxValue;
  }
  if (_log.isInfoEnabled()) {
    _log.info("Default " + prettyPropertyName + " for "+ container+ " configured to "+ property);
  }
  return property;
}
