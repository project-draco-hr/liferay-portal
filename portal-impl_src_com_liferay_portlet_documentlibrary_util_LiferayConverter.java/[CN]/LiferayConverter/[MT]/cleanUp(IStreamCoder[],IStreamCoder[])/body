{
  if (inputIStreamCoders != null) {
    for (    IStreamCoder iStreamCoder : inputIStreamCoders) {
      if (iStreamCoder != null) {
        iStreamCoder.close();
      }
    }
  }
  if (outputIStreamCoders != null) {
    for (    IStreamCoder iStreamCoder : outputIStreamCoders) {
      if (iStreamCoder != null) {
        iStreamCoder.close();
      }
    }
  }
}
