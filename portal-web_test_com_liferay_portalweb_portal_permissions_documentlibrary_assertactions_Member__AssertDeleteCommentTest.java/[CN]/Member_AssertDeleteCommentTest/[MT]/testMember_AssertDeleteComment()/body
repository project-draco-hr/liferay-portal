{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Document Library Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Document Library Permissions Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Permissions2 Test2 Subfolder2",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.clickAt("//a/span/span",RuntimeVariables.replace("Member Permissions Edited Test Document.txt"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[5]/div/span/a/span")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Delete"),selenium.getText("//div[5]/div/span/a/span"));
}
