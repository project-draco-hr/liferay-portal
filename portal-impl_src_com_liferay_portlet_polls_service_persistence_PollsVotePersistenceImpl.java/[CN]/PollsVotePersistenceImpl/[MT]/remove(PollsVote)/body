{
  Session session=null;
  try {
    session=openSession();
    pollsVote=(PollsVote)session.merge(pollsVote);
    session.delete(pollsVote);
    session.flush();
    return pollsVote;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(PollsVote.class.getName());
  }
}
