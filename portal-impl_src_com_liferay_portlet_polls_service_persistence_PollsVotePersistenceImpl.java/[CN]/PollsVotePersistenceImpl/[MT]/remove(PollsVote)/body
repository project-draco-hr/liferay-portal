{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(pollsVote);
    }
  }
  pollsVote=removeImpl(pollsVote);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(pollsVote);
    }
  }
  return pollsVote;
}
