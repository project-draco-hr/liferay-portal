{
  Session session=null;
  try {
    session=openSession();
    PollsVote pollsVote=(PollsVote)session.get(PollsVoteImpl.class,new Long(voteId));
    if (pollsVote == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + voteId);
      }
      throw new NoSuchVoteException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + voteId);
    }
    return pollsVotePersistence.remove(pollsVote);
  }
 catch (  NoSuchVoteException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
