{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,pollsVote,merge);
    pollsVote.setNew(false);
    return pollsVote;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(PollsVote.class.getName());
  }
}
