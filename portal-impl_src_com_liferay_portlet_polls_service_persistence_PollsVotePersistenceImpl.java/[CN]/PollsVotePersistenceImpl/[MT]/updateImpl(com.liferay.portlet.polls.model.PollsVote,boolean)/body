{
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(pollsVote);
    }
 else {
      if (pollsVote.isNew()) {
        session.save(pollsVote);
      }
    }
    session.flush();
    pollsVote.setNew(false);
    return pollsVote;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(PollsVote.class.getName());
  }
}
