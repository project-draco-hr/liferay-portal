{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletURL portletURL=PortletURLFactoryUtil.create(portletRequest,PortletKeys.WIKI_ADMIN,PortalUtil.getControlPanelPlid(themeDisplay.getCompanyId()),PortletRequest.RENDER_PHASE);
  return portletURL.toString();
}
