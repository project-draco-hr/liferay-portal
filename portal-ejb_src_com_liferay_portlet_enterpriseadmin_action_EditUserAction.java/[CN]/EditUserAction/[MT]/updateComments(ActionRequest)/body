{
  String comments=ParamUtil.getString(req,"comments");
  User user=PortalUtil.getSelectedUser(req);
  Contact contact=user.getContact();
  AdminUtil.updateUser(req,user.getUserId(),user.getEmailAddress(),user.getLanguageId(),user.getTimeZoneId(),user.getGreeting(),user.getResolution(),comments,contact.getSmsSn(),contact.getAimSn(),contact.getIcqSn(),contact.getJabberSn(),contact.getMsnSn(),contact.getSkypeSn(),contact.getYmSn());
  return user;
}
