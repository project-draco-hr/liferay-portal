{
  String smsSn=ParamUtil.getString(req,"smsSn");
  User user=PortalUtil.getSelectedUser(req);
  Contact contact=user.getContact();
  AdminUtil.updateUser(req,user.getUserId(),user.getEmailAddress(),user.getLanguageId(),user.getTimeZoneId(),user.getGreeting(),user.getResolution(),user.getComments(),smsSn,contact.getAimSn(),contact.getIcqSn(),contact.getJabberSn(),contact.getMsnSn(),contact.getSkypeSn(),contact.getYmSn());
  return user;
}
