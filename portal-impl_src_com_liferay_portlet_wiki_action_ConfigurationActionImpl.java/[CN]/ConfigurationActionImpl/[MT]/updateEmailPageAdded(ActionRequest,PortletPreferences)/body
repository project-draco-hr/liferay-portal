{
  String emailPageAddedEnabled=ParamUtil.getString(req,"emailPageAddedEnabled");
  String emailPageAddedSubjectPrefix=ParamUtil.getString(req,"emailPageAddedSubjectPrefix");
  String emailPageAddedBody=ParamUtil.getString(req,"emailPageAddedBody");
  String emailPageAddedSignature=ParamUtil.getString(req,"emailPageAddedSignature");
  if (Validator.isNull(emailPageAddedSubjectPrefix)) {
    SessionErrors.add(req,"emailPageAddedSubjectPrefix");
  }
 else   if (Validator.isNull(emailPageAddedBody)) {
    SessionErrors.add(req,"emailPageAddedBody");
  }
 else {
    prefs.setValue("email-page-added-enabled",emailPageAddedEnabled);
    prefs.setValue("email-page-added-subject-prefix",emailPageAddedSubjectPrefix);
    prefs.setValue("email-page-added-body",emailPageAddedBody);
    prefs.setValue("email-page-added-signature",emailPageAddedSignature);
  }
}
