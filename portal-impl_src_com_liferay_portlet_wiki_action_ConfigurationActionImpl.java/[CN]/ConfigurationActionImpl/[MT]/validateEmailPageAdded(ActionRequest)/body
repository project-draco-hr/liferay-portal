{
  boolean emailPageAddedEnabled=GetterUtil.getBoolean(getParameter(actionRequest,"emailPageAddedEnabled"));
  String emailPageAddedSubject=getParameter(actionRequest,"emailPageAddedSubject");
  String emailPageAddedBody=getParameter(actionRequest,"emailPageAddedBody");
  if (emailPageAddedEnabled) {
    if (Validator.isNull(emailPageAddedSubject)) {
      SessionErrors.add(actionRequest,"emailPageAddedSubject");
    }
 else     if (Validator.isNull(emailPageAddedBody)) {
      SessionErrors.add(actionRequest,"emailPageAddedBody");
    }
  }
}
