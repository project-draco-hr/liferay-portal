{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Test Transaction Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Test Transaction Page",RuntimeVariables.replace("Test Transaction Page"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertTrue(selenium.isTextPresent("BarLocalServiceUtil.addBar=PASSED"));
  assertTrue(selenium.isTextPresent("BarLocalServiceUtil.addBarPortalRollback=PASSED"));
  assertTrue(selenium.isTextPresent("BarLocalServiceUtil.addBarPortletRollback=PASSED"));
}
