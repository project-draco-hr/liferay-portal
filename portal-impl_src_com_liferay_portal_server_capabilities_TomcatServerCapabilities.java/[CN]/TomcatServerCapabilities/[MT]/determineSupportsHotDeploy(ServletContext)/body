{
  DeepNamedValueScanner deepNamedValueScanner=new DeepNamedValueScanner("autoDeploy");
  deepNamedValueScanner.scan(servletContext);
  if (_log.isDebugEnabled()) {
    _log.debug("HotDeploy founded: " + deepNamedValueScanner.isFounded() + " in "+ deepNamedValueScanner.getElapsed()+ "ms by "+ deepNamedValueScanner.getMatchingCount()+ " matches");
  }
  Boolean autoDeployValue=(Boolean)deepNamedValueScanner.getMatchedValue();
  _supportsHotDeploy=autoDeployValue.booleanValue();
}
