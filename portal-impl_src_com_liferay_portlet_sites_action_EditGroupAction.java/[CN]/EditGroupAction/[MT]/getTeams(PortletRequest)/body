{
  List<Team> teams=new UniqueList<Team>();
  long[] teamsTeamIds=StringUtil.split(ParamUtil.getString(portletRequest,"teamsTeamIds"),0L);
  for (int i=0; i < teamsTeamIds.length; i++) {
    if (teamsTeamIds[i] == 0) {
      continue;
    }
    Team team=TeamLocalServiceUtil.getTeam(teamsTeamIds[i]);
    teams.add(team);
  }
  return teams;
}
