{
  Session session=null;
  try {
    session=openSession();
    JournalFeed journalFeed=(JournalFeed)session.get(JournalFeedImpl.class,Long.valueOf(id));
    if (journalFeed == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + id);
      }
      throw new NoSuchFeedException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + id);
    }
    return journalFeedPersistence.remove(journalFeed);
  }
 catch (  NoSuchFeedException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
