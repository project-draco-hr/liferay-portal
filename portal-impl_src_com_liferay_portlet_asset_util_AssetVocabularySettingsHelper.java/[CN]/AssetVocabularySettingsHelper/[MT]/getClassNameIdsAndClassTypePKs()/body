{
  String propertyValue=_properties.getProperty(_KEY_SELECTED_CLASS_NAME_IDS_AND_CLASS_TYPE_PKS);
  if (Validator.isNull(propertyValue)) {
    return new String[]{getClassNameIdAndClassTypePK(AssetCategoryConstants.ALL_CLASS_NAME_ID,AssetCategoryConstants.ALL_CLASS_TYPE_PK)};
  }
  return StringUtil.split(propertyValue);
}
