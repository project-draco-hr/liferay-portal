{
  String propertyValue=_properties.getProperty(_KEY_SELECTED_CLASS_NAME_AND_TYPE_IDS);
  if (Validator.isNull(propertyValue)) {
    return new String[]{getClassNameIdAndClassTypePK(AssetCategoryConstants.ALL_CLASS_NAME_IDS,AssetCategoryConstants.ALL_CLASS_TYPE_PKS)};
  }
  return StringUtil.split(propertyValue);
}
