{
  if (classNameAndTypeIds.length == 0) {
    return false;
  }
  if (classNameAndTypeIds[0].equals(AssetCategoryConstants.ALL_CLASS_NAMES_AND_TYPES_ID)) {
    return true;
  }
  if (classTypeId == AssetCategoryConstants.ALL_CLASS_TYPES_ID) {
    PrefixPredicateFilter prefixPredicateFilter=new PrefixPredicateFilter(classNameId + StringPool.COLON,true);
    return ArrayUtil.exists(classNameAndTypeIds,prefixPredicateFilter);
  }
 else {
    String classNameAndType=getClassNameAndTypeId(classNameId,classTypeId);
    if (ArrayUtil.contains(classNameAndTypeIds,classNameAndType)) {
      return true;
    }
    String allClassTypesId=getClassNameAndTypeId(classNameId,AssetCategoryConstants.ALL_CLASS_TYPES_ID);
    return ArrayUtil.contains(classNameAndTypeIds,allClassTypesId);
  }
}
