{
  Session session=null;
  try {
    session=openSession();
    DLFolderHBM dlFolderHBM=(DLFolderHBM)session.get(DLFolderHBM.class,folderId);
    if (dlFolderHBM == null) {
      _log.warn("No DLFolder exists with the primary key " + folderId.toString());
      throw new NoSuchFolderException("No DLFolder exists with the primary key " + folderId.toString());
    }
    session.delete(dlFolderHBM);
    session.flush();
    return DLFolderHBMUtil.model(dlFolderHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
