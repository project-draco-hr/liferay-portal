{
  Session session=null;
  try {
    session=openSession();
    session.delete(dlFolder);
    session.flush();
    DLFolderPool.removeByP_N(dlFolder.getParentFolderId(),dlFolder.getName());
    return dlFolder;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
