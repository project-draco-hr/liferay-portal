{
  if (!WikiPagePermissionChecker.contains(_wikiRequestHelper.getPermissionChecker(),wikiPage,ActionKeys.UPDATE)) {
    return false;
  }
  if (!WikiNodePermissionChecker.contains(_wikiRequestHelper.getPermissionChecker(),wikiPage.getNodeId(),ActionKeys.ADD_PAGE)) {
    return false;
  }
  return true;
}
