{
  if (!WikiPagePermissionChecker.contains(_wikiRequestHelper.getPermissionChecker(),wikiPage,ActionKeys.PERMISSIONS)) {
    return;
  }
  URLMenuItem menuItem=new URLMenuItem();
  menuItem.setIcon("icon-lock");
  menuItem.setKey(WikiUIItemKeys.PERMISSIONS);
  menuItem.setLabel("permissions");
  menuItem.setMethod("get");
  menuItem.setUseDialog(true);
  String url=null;
  try {
    url=PermissionsURLTag.doTag(null,WikiPage.class.getName(),wikiPage.getTitle(),null,String.valueOf(wikiPage.getResourcePrimKey()),LiferayWindowState.POP_UP.toString(),null,_request);
  }
 catch (  Exception e) {
    throw new SystemException("Unable to create permissions URL",e);
  }
  menuItem.setURL(url);
  menuItems.add(menuItem);
}
