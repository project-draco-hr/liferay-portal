{
  if (toId == 0) {
    return fromValue;
  }
 else   if (toId == 1) {
    return fromValue - 273.15;
  }
 else   if (toId == 2) {
    return (1.8 * fromValue) - 459.67;
  }
 else   if (toId == 3) {
    return 1.8 * fromValue;
  }
 else   if (toId == 4) {
    return .8 * (fromValue - 273.15);
  }
 else {
    return 0;
  }
}
