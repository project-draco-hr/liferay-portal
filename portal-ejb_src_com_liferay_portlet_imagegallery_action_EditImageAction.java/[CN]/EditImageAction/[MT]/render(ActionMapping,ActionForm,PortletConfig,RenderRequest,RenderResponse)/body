{
  try {
    ActionUtil.getImage(req);
  }
 catch (  Exception e) {
    if (e != null && e instanceof NoSuchImageException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.image_gallery.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward(getForward(req,"portlet.image_gallery.edit_image"));
}
