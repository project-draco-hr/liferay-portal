{
  GradleUtil.applyPlugin(project,JavaPlugin.class);
  Configuration mavenEmbedderConfiguration=addConfigurationMavenEmbedder(project);
  BuildPluginDescriptorTask buildPluginDescriptorTask=addTaskBuildPluginDescriptor(project,mavenEmbedderConfiguration);
  configureTasksJavadoc(project);
  configureTasksUpload(project,buildPluginDescriptorTask);
}
