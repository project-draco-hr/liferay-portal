{
  if (buildPluginDescriptorTask.getClassesDir() != null) {
    return;
  }
  SourceSet sourceSet=GradleUtil.getSourceSet(buildPluginDescriptorTask.getProject(),SourceSet.MAIN_SOURCE_SET_NAME);
  SourceSetOutput sourceSetOutput=sourceSet.getOutput();
  buildPluginDescriptorTask.setClassesDir(sourceSetOutput.getClassesDir());
}
