{
  String keywords=searchContext.getKeywords();
  if (Validator.isNull(keywords)) {
    return Collections.emptyMap();
  }
  addSearchExpando(searchQuery,searchContext,keywords);
  addSearchLocalizedTerm(searchQuery,searchContext,Field.ASSET_CATEGORY_TITLES,searchContext.isLike());
  return searchQuery.addTerms(Field.KEYWORDS,keywords,searchContext.isLike());
}
