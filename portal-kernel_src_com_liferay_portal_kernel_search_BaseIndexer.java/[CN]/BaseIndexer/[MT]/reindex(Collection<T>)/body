{
  if (IndexWriterHelperUtil.isIndexReadOnly() || !isIndexerEnabled() || collection.isEmpty()) {
    return;
  }
  for (  T element : collection) {
    try {
      reindex(element);
    }
 catch (    SearchException se) {
      if (_log.isErrorEnabled()) {
        _log.error("Unable to index object: " + element);
      }
    }
  }
}
