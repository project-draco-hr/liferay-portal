{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/socialrelationsn1/home/");
  selenium.waitForVisible("link=Requests Test Page");
  selenium.click(RuntimeVariables.replace("link=Requests Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//a[@id='_145_addApplication']","More"));
  selenium.clickAt("//a[@id='_145_addApplication']",RuntimeVariables.replace("More"));
  Thread.sleep(5000);
  selenium.waitForElementPresent("//div[@title='Requests']/p/a");
  selenium.clickAt("//div[@title='Requests']/p/a",RuntimeVariables.replace("Add"));
  selenium.waitForVisible("//div[2]/div/div");
  assertTrue(selenium.isVisible("//div[2]/div/div"));
}
