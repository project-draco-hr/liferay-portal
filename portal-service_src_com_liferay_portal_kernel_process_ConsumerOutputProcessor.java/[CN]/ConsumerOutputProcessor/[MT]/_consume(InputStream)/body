{
  byte[] buffer=new byte[1024];
  try {
    while (inputStream.read(buffer) != -1)     ;
  }
 catch (  IOException ioe) {
    throw new ProcessException(ioe);
  }
 finally {
    try {
      inputStream.close();
    }
 catch (    IOException ioe) {
      throw new ProcessException(ioe);
    }
  }
}
