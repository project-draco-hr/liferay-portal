{
  if (ddmFormField == null) {
    throw new MustSetValidField(ddmFormFieldValue.getName());
  }
  DDMFormValues ddmFormValues=ddmFormFieldValue.getDDMFormValues();
  DDMFormFieldEvaluationResult ddmFormFieldEvaluationResult=ddmFormEvaluationResult.geDDMFormFieldEvaluationResult(ddmFormFieldValue.getName(),ddmFormFieldValue.getInstanceId());
  validateDDMFormFieldValue(ddmFormField,ddmFormValues.getAvailableLocales(),ddmFormValues.getDefaultLocale(),ddmFormFieldValue.getValue(),ddmFormFieldEvaluationResult);
  traverseDDMFormFieldValues(ddmFormFieldValue.getNestedDDMFormFieldValues(),ddmFormField.getNestedDDMFormFieldsMap(),ddmFormEvaluationResult);
}
