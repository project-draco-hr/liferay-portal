{
  List<Team> teams=new ArrayList<>();
  long[] teamsTeamIds=ArrayUtil.unique(ParamUtil.getLongValues(portletRequest,"teamsSearchContainerPrimaryKeys"));
  for (  long teamsTeamId : teamsTeamIds) {
    if (teamsTeamId == 0) {
      continue;
    }
    Team team=teamLocalService.getTeam(teamsTeamId);
    teams.add(team);
  }
  return teams;
}
