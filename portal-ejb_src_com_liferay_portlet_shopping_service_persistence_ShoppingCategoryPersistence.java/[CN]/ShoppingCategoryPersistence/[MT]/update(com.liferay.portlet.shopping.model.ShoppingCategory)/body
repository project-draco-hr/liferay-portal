{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(shoppingCategory);
    session.flush();
    shoppingCategory.setNew(false);
    return shoppingCategory;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
