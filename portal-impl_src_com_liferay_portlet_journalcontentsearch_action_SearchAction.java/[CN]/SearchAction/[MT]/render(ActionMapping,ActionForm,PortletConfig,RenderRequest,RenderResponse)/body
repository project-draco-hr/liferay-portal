{
  ThemeDisplay themeDisplay=(ThemeDisplay)req.getAttribute(WebKeys.THEME_DISPLAY);
  LayoutTypePortlet layoutTypePortlet=themeDisplay.getLayoutTypePortlet();
  PortletConfigImpl configImpl=(PortletConfigImpl)config;
  String portletId=configImpl.getPortletId();
  if (!layoutTypePortlet.hasPortletId(portletId)) {
    res.setTitle(LanguageUtil.get(themeDisplay.getCompanyId(),themeDisplay.getLocale(),"search"));
  }
  return mapping.findForward("portlet.journal_content_search.search");
}
