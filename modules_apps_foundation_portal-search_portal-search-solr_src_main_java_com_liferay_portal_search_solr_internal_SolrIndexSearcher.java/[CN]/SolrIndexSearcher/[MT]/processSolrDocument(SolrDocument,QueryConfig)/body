{
  Document document=new DocumentImpl();
  Collection<String> fieldNames=solrDocument.getFieldNames();
  for (  String fieldName : fieldNames) {
    Collection<Object> fieldValues=solrDocument.getFieldValues(fieldName);
    Field field=new Field(fieldName,ArrayUtil.toStringArray(fieldValues.toArray(new Object[fieldValues.size()])));
    document.add(field);
  }
  populateUID(document,queryConfig);
  return document;
}
