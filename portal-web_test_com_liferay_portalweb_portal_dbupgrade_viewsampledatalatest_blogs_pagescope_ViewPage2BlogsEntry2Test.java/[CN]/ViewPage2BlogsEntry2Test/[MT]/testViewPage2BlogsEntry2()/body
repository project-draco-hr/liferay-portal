      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Blogs Test Page1")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
