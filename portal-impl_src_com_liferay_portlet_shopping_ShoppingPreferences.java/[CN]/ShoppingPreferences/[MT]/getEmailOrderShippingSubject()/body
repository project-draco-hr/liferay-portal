{
  String emailOrderShippingSubject=_portletPreferences.getValue("emailOrderShippingSubject",StringPool.BLANK);
  if (Validator.isNotNull(emailOrderShippingSubject)) {
    return emailOrderShippingSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.SHOPPING_EMAIL_ORDER_SHIPPING_SUBJECT));
  }
}
