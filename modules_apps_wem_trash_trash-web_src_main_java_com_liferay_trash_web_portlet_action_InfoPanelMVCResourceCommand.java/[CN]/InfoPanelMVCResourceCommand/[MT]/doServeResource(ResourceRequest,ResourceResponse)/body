{
  List<TrashEntry> trashEntries=ActionUtil.getTrashEntries(resourceRequest);
  resourceRequest.setAttribute(TrashWebKeys.TRASH_ENTRIES,trashEntries);
  PortletRequestDispatcher portletRequestDispatcher=getPortletRequestDispatcher(resourceRequest,"/info_panel.jsp");
  portletRequestDispatcher.include(resourceRequest,resourceResponse);
}
