{
  EntityCacheUtil.removeResult(OrganizationModelImpl.ENTITY_CACHE_ENABLED,OrganizationImpl.class,organization.getPrimaryKey());
  FinderCacheUtil.removeResult(FINDER_PATH_FIND_ALL,FINDER_ARGS_EMPTY);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_N,new Object[]{Long.valueOf(organization.getCompanyId()),organization.getName()});
}
