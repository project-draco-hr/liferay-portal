{
  try {
    clearGroups.clear(organization.getPrimaryKey());
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    FinderCacheUtil.clearCache("Groups_Orgs");
  }
  try {
    clearUsers.clear(organization.getPrimaryKey());
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    FinderCacheUtil.clearCache("Users_Orgs");
  }
  Session session=null;
  try {
    session=openSession();
    if (BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(OrganizationImpl.class,organization.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(organization);
    session.flush();
    return organization;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Organization.class.getName());
  }
}
