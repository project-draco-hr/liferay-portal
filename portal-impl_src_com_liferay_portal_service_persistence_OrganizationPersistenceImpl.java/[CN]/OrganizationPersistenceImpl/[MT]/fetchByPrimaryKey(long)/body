{
  Organization organization=(Organization)EntityCacheUtil.getResult(OrganizationModelImpl.ENTITY_CACHE_ENABLED,OrganizationImpl.class,organizationId,this);
  if (organization == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (organization == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      organization=(Organization)session.get(OrganizationImpl.class,Long.valueOf(organizationId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (organization == null)) {
        EntityCacheUtil.putResult(OrganizationModelImpl.ENTITY_CACHE_ENABLED,OrganizationImpl.class,organizationId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(organization);
      }
      closeSession(session);
    }
  }
  return organization;
}
