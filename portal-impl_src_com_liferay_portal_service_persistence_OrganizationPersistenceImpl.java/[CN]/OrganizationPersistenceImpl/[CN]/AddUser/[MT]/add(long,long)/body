{
  if (!_persistenceImpl.containsUser.contains(organizationId,userId)) {
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onBeforeAddAssociation(organizationId,com.liferay.portal.model.User.class.getName(),userId);
      }
    }
    _sqlUpdate.update(new Object[]{new Long(organizationId),new Long(userId)});
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onAfterAddAssociation(organizationId,com.liferay.portal.model.User.class.getName(),userId);
      }
    }
  }
}
