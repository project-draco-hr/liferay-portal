{
  Map<String,String[]> parameterMap=getBaseParameterMap(group.getGroupId(),layout.getPlid());
  parameterMap.put(Constants.CMD,new String[]{Constants.EXPORT});
  parameterMap.put(PortletDataHandlerKeys.PORTLET_DATA + StringPool.UNDERLINE + PortletKeys.JOURNAL,new String[]{Boolean.TRUE.toString()});
  return parameterMap;
}
