{
  Map<String,String[]> parameterMap=getBaseParameterMap(groupId,plid);
  parameterMap.put(Constants.CMD,new String[]{Constants.EXPORT});
  parameterMap.put(PortletDataHandlerKeys.PORTLET_DATA + StringPool.UNDERLINE + PortletKeys.JOURNAL,new String[]{Boolean.TRUE.toString()});
  parameterMap.put("_journal_embedded-assets",new String[]{Boolean.TRUE.toString()});
  parameterMap.put("_journal_version-history",new String[]{Boolean.TRUE.toString()});
  parameterMap.put("range",new String[]{"fromLastPublishDate"});
  return parameterMap;
}
