{
  _bundleContext=componentContext.getBundleContext();
  Dictionary<String,Object> properties=componentContext.getProperties();
  _stopTimeout=GetterUtil.getLong(properties.get("com.liferay.portal.wab.extender.stop.timeout"),60000);
  _logger=new Logger(_bundleContext);
  _eventUtil=new EventUtil(_bundleContext);
  try {
    _webBundleDeployer=new WebBundleDeployer(_bundleContext,_extendedHttpService,_saxParserFactory,_eventUtil,_logger);
    super.start(_bundleContext);
  }
 catch (  Exception e) {
    _logger.log(Logger.LOG_ERROR,e.getMessage(),e);
  }
}
