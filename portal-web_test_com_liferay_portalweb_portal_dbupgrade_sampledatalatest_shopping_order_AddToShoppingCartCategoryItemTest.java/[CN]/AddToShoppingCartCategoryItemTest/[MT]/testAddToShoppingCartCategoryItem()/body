{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/shopping-order-community/");
  selenium.waitForVisible("link=Shopping Order Page");
  selenium.clickAt("link=Shopping Order Page",RuntimeVariables.replace("Shopping Order Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//td[1]/a",RuntimeVariables.replace("Category Test"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//td[2]/a",RuntimeVariables.replace("Item Test"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("//input[@value='Add to Shopping Cart']"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForVisible("//div[@class='portlet-msg-success']");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals(RuntimeVariables.replace("Item Test\nThis is an item test.\n\nAvailability: In Stock\n\n\nPrice for 1 Items and Above:$9.99"),selenium.getText("//td[2]/a"));
}
