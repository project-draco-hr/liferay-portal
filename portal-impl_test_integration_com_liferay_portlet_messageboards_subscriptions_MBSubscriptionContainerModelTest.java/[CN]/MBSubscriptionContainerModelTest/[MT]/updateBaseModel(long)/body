{
  MBMessage message=MBMessageLocalServiceUtil.getMessage(baseModelId);
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(message.getGroupId(),TestPropsValues.getUserId());
  MBTestUtil.populateNotificationsServiceContext(serviceContext,Constants.UPDATE);
  MBMessageLocalServiceUtil.updateMessage(TestPropsValues.getUserId(),message.getMessageId(),RandomTestUtil.randomString(),serviceContext);
}
