{
  WorkflowDefinitionLink workflowDefinitionLink=fetchByG_C_C(groupId,companyId,classNameId);
  if (workflowDefinitionLink == null) {
    StringBundler msg=new StringBundler(7);
    msg.append("No WorkflowDefinitionLink exists with the key {");
    msg.append("groupId=" + groupId);
    msg.append(", ");
    msg.append("companyId=" + companyId);
    msg.append(", ");
    msg.append("classNameId=" + classNameId);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    if (_log.isWarnEnabled()) {
      _log.warn(msg.toString());
    }
    throw new NoSuchWorkflowDefinitionLinkException(msg.toString());
  }
  return workflowDefinitionLink;
}
