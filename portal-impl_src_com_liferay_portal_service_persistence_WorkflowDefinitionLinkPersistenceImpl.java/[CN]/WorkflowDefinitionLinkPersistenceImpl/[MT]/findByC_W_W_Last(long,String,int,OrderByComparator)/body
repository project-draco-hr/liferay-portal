{
  int count=countByC_W_W(companyId,workflowDefinitionName,workflowDefinitionVersion);
  List<WorkflowDefinitionLink> list=findByC_W_W(companyId,workflowDefinitionName,workflowDefinitionVersion,count - 1,count,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(8);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("companyId=");
    msg.append(companyId);
    msg.append(", workflowDefinitionName=");
    msg.append(workflowDefinitionName);
    msg.append(", workflowDefinitionVersion=");
    msg.append(workflowDefinitionVersion);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchWorkflowDefinitionLinkException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
