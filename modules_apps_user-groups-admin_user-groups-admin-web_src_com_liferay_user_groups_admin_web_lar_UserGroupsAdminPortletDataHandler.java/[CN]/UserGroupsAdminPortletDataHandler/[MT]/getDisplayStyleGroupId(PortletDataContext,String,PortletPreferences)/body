{
  try {
    TemplateHandler templateHandler=TemplateHandlerRegistryUtil.getTemplateHandler(UserGroup.class.getName());
    if (Validator.isNotNull(templateHandler)) {
      return GetterUtil.getLong(portletPreferences.getValue("displayStyleGroupId",null));
    }
  }
 catch (  Exception e) {
  }
  return 0;
}
