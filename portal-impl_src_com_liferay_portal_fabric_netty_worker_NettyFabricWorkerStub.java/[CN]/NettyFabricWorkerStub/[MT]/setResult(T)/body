{
  NoticeableFuture<Map<Path,Path>> noticeableFuture=_repository.getFiles(_outputPathMap,true);
  noticeableFuture.addFutureListener(new BaseFutureListener<Map<Path,Path>>(){
    @Override public void completeWithCancel(    Future<Map<Path,Path>> future){
      _defaultNoticeableFuture.cancel(true);
    }
    @Override public void completeWithException(    Future<Map<Path,Path>> future,    Throwable throwable){
      _defaultNoticeableFuture.setException(throwable);
    }
    @Override public void completeWithResult(    Future<Map<Path,Path>> future,    Map<Path,Path> map){
      _defaultNoticeableFuture.set(result);
    }
  }
);
}
