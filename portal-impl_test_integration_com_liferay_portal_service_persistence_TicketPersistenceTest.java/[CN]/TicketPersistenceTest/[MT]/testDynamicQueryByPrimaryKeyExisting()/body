{
  Ticket newTicket=addTicket();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Ticket.class,Ticket.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("ticketId",newTicket.getTicketId()));
  List<Ticket> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Ticket existingTicket=result.get(0);
  Assert.assertEquals(existingTicket,newTicket);
}
