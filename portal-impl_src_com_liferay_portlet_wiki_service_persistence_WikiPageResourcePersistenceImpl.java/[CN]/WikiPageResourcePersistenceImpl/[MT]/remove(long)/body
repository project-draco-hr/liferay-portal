{
  Session session=null;
  try {
    session=openSession();
    WikiPageResource wikiPageResource=(WikiPageResource)session.get(WikiPageResourceImpl.class,new Long(resourcePrimKey));
    if (wikiPageResource == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + resourcePrimKey);
      }
      throw new NoSuchPageResourceException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + resourcePrimKey);
    }
    return remove(wikiPageResource);
  }
 catch (  NoSuchPageResourceException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
