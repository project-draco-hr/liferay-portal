{
  WikiPageResource wikiPageResource=(WikiPageResource)EntityCacheUtil.getResult(WikiPageResourceModelImpl.ENTITY_CACHE_ENABLED,WikiPageResourceImpl.class,resourcePrimKey,this);
  if (wikiPageResource == null) {
    Session session=null;
    try {
      session=openSession();
      wikiPageResource=(WikiPageResource)session.get(WikiPageResourceImpl.class,Long.valueOf(resourcePrimKey));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (wikiPageResource != null) {
        cacheResult(wikiPageResource);
      }
      closeSession(session);
    }
  }
  return wikiPageResource;
}
