{
  WikiPageResource wikiPageResource=(WikiPageResource)EntityCacheUtil.getResult(WikiPageResourceModelImpl.ENTITY_CACHE_ENABLED,WikiPageResourceImpl.class,resourcePrimKey,this);
  if (wikiPageResource == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (wikiPageResource == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      wikiPageResource=(WikiPageResource)session.get(WikiPageResourceImpl.class,Long.valueOf(resourcePrimKey));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (wikiPageResource == null)) {
        EntityCacheUtil.putResult(WikiPageResourceModelImpl.ENTITY_CACHE_ENABLED,WikiPageResourceImpl.class,resourcePrimKey,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(wikiPageResource);
      }
      closeSession(session);
    }
  }
  return wikiPageResource;
}
