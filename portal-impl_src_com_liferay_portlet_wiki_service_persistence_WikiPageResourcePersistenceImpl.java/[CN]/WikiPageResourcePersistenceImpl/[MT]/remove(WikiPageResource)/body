{
  FinderCache.clearCache(WikiPageResource.class.getName());
  Session session=null;
  try {
    session=openSession();
    session.delete(wikiPageResource);
    session.flush();
    return wikiPageResource;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
