{
  ModelListener listener=null;
  if (Validator.isNotNull(_LISTENER)) {
    try {
      listener=(ModelListener)Class.forName(_LISTENER).newInstance();
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(cartId));
  }
  com.liferay.portlet.shopping.model.ShoppingCart shoppingCart=getPersistence().remove(cartId);
  if (listener != null) {
    listener.onAfterRemove(shoppingCart);
  }
  return shoppingCart;
}
