{
  long containerModelId=addContainerModel(creatorUser.getUserId(),BaseSubscriptionTestCase.PARENT_CONTAINER_MODEL_ID_DEFAULT);
  long baseModelId=addBaseModel(creatorUser.getUserId(),containerModelId);
  addSubscriptionBaseModel(baseModelId);
  removeContainerModelResourceViewPermission();
  updateBaseModel(creatorUser.getUserId(),baseModelId);
  Assert.assertEquals(0,MailServiceTestUtil.getInboxSize());
}
