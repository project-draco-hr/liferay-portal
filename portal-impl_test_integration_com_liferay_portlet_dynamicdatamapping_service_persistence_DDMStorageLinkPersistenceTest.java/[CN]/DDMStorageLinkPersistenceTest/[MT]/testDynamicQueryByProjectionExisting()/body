{
  DDMStorageLink newDDMStorageLink=addDDMStorageLink();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMStorageLink.class,DDMStorageLink.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("storageLinkId"));
  Object newStorageLinkId=newDDMStorageLink.getStorageLinkId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("storageLinkId",new Object[]{newStorageLinkId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Object existingStorageLinkId=result.get(0);
  Assert.assertEquals(existingStorageLinkId,newStorageLinkId);
}
