{
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  PortletPreferences companyPortletPreferences=PrefsPropsUtil.getPreferences(themeDisplay.getCompanyId());
  _companyPortletRatingsDefinitionDisplayContext=new CompanyPortletRatingsDefinitionDisplayContext(companyPortletPreferences,request);
  _populateRatingsTypeMaps(groupTypeSettings,request);
}
