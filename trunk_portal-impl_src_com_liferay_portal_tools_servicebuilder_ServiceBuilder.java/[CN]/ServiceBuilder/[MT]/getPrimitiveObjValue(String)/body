{
  if (colType.equals("Boolean")) {
    return ".booleanValue()";
  }
 else   if (colType.equals("Double")) {
    return ".doubleValue()";
  }
 else   if (colType.equals("Float")) {
    return ".floatValue()";
  }
 else   if (colType.equals("Integer")) {
    return ".intValue()";
  }
 else   if (colType.equals("Long")) {
    return ".longValue()";
  }
 else   if (colType.equals("Short")) {
    return ".shortValue()";
  }
  return StringPool.BLANK;
}
