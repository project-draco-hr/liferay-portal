{
  WSRPConsumer newWSRPConsumer=addWSRPConsumer();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(WSRPConsumer.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("wsrpConsumerId"));
  Object newWsrpConsumerId=newWSRPConsumer.getWsrpConsumerId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("wsrpConsumerId",new Object[]{newWsrpConsumerId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Object existingWsrpConsumerId=result.get(0);
  Assert.assertEquals(existingWsrpConsumerId,newWsrpConsumerId);
}
