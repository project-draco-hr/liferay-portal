{
  if ((pos + _TAG_SCRIPT.length + 1) <= text.length()) {
    char item;
    for (int i=0; i < _TAG_SCRIPT.length; i++) {
      item=text.charAt(pos++);
      if (Character.toLowerCase(item) != _TAG_SCRIPT[i]) {
        return false;
      }
    }
    item=text.charAt(pos);
    return !Character.isLetter(item);
  }
 else {
    return false;
  }
}
