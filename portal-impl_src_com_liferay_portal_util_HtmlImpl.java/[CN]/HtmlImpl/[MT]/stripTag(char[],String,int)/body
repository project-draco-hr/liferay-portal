{
  int x=pos + _TAG_SCRIPT.length;
  x=text.indexOf(">",x);
  if (x >= 0) {
    if (text.charAt(x - 1) != '/') {
      for (; ; ) {
        x=text.indexOf("</",x);
        if (x >= 0) {
          if (isTag(tag,text,x + 2)) {
            pos=x;
            break;
          }
 else {
            x+=2;
          }
        }
 else {
          break;
        }
      }
    }
  }
  return pos;
}
