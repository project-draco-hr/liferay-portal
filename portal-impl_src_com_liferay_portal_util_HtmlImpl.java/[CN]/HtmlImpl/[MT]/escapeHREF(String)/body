{
  if (href == null) {
    return null;
  }
  if (href.length() == 0) {
    return StringPool.BLANK;
  }
  if (href.indexOf(StringPool.COLON) == 10) {
    String protocol=href.substring(0,10).toLowerCase();
    if (protocol.equals("javascript")) {
      return StringUtil.replaceFirst(href,StringPool.COLON,"%3a");
    }
  }
  return href;
}
