{
  if (Validator.isNull(languageId)) {
    return String.valueOf(id).concat(StringPool.PERIOD).concat(version);
  }
  StringBundler sb=new StringBundler(5);
  sb.append(id);
  sb.append(StringPool.PERIOD);
  sb.append(version);
  sb.append(StringPool.PERIOD);
  sb.append(languageId);
  return sb.toString();
}
