{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MDRRuleGroupInstance.class,MDRRuleGroupInstance.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("ruleGroupInstanceId",ServiceTestUtil.nextLong()));
  List<MDRRuleGroupInstance> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
