{
  Company company=super.checkCompany(webId,mx);
  try {
    _portalKaleoManager.deployKaleoDefaults(company.getCompanyId());
  }
 catch (  PortalException pe) {
    throw pe;
  }
catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new SystemException(e);
  }
  return company;
}
