{
  try {
    RequestParameterAutoLoginConfiguration requestParameterAutoLoginConfiguration=_settingsFactory.getSettings(RequestParameterAutoLoginConfiguration.class,new CompanyServiceSettingsLocator(companyId,RequestParameterAutoLoginConstants.SERVICE_NAME));
    return requestParameterAutoLoginConfiguration;
  }
 catch (  SettingsException se) {
    _log.error("Unable to get request parameter auto login configuration",se);
  }
  return null;
}
