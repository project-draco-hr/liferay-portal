{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,region,merge);
    region.setNew(false);
    return region;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Region.class.getName());
  }
}
