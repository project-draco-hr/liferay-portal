{
  List<Region> list=findByActive(active,0,1,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(4);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("active=");
    msg.append(active);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchRegionException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
