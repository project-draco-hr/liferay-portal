{
  FinderCache.clearCache(Region.class.getName());
  Session session=null;
  try {
    session=openSession();
    session.delete(region);
    session.flush();
    return region;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
