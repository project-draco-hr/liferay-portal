{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(region);
    }
 else {
      if (region.isNew()) {
        session.save(region);
      }
 else {
        session.update(region);
      }
    }
    session.flush();
    region.setNew(false);
    return region;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Region.class.getName());
  }
}
