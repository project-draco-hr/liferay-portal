{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SCProductEntry.class,SCProductEntry.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("productEntryId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("productEntryId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
