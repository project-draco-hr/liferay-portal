{
  JSONArray expectedJSONArray=createJSONArray("value 1");
  DDMFormFieldValue ddmFormFieldValue=DDMFormValuesTestUtil.createDDMFormFieldValue("Select",new UnlocalizedValue(expectedJSONArray.toString()));
  SelectDDMFormFieldValueAccessor selectDDMFormFieldValueAccessor=new SelectDDMFormFieldValueAccessor(LocaleUtil.US);
  JSONArray actualJSONArray=selectDDMFormFieldValueAccessor.get(ddmFormFieldValue);
  Assert.assertEquals(expectedJSONArray.toString(),actualJSONArray.toString());
}
