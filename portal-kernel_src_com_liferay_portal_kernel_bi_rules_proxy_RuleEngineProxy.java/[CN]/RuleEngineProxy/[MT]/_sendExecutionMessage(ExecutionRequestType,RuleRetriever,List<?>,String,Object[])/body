{
  ExecutionRequestMessage executionRequestMessage=new ExecutionRequestMessage(executionRequestType,ruleRetriever);
  executionRequestMessage.addFact(facts);
  if (queryString != null) {
    Query query=new Query();
    query.setQueryString(queryString);
    query.addArguments(queryArguments);
    executionRequestMessage.setQuery(query);
  }
  try {
    return (List<?>)_synchronousExecutionRequestMessageSender.send(executionRequestMessage);
  }
 catch (  MessageBusException mbe) {
    throw new RuleEngineException("Unable to execute rules",mbe);
  }
}
