{
  if (header == null) {
    return null;
  }
  char[] characters=header.toCharArray();
  for (int i=0; i < characters.length; i++) {
    char c=characters[i];
    if (((c <= 31) && (c != 9)) || (c == 127) || (c > 255)) {
      characters[i]=CharPool.SPACE;
    }
  }
  return new String(characters);
}
