{
  if (header == null) {
    return null;
  }
  StringBuilder sb=null;
  for (int i=0; i < header.length(); i++) {
    char c=header.charAt(i);
    if (((c <= 31) && (c != 9)) || (c == 127) || (c > 255)) {
      if (sb == null) {
        sb=new StringBuilder(header);
      }
      sb.setCharAt(i,CharPool.SPACE);
    }
  }
  if (sb != null) {
    header=sb.toString();
  }
  return header;
}
