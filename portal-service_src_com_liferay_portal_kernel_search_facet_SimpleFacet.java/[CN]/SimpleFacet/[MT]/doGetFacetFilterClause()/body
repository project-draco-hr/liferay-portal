{
  SearchContext searchContext=getSearchContext();
  FacetConfiguration facetConfiguration=getFacetConfiguration();
  JSONObject dataJSONObject=facetConfiguration.getData();
  String value=StringPool.BLANK;
  if (isStatic() && dataJSONObject.has("value")) {
    value=dataJSONObject.getString("value");
  }
  String valueParam=GetterUtil.getString(searchContext.getAttribute(getFieldId()));
  if (!isStatic() && Validator.isNotNull(valueParam)) {
    value=valueParam;
  }
  if (Validator.isNull(value)) {
    return null;
  }
  return BooleanClauseFactoryUtil.createFilter(searchContext,getFieldName(),value,BooleanClauseOccur.MUST);
}
