{
  try {
    MethodKey methodKey=new MethodKey(DLAppServiceUtil.class.getName(),"search",_searchParameterTypes70);
    MethodHandler methodHandler=new MethodHandler(methodKey,repositoryId,searchContext,query);
    Object returnObj=null;
    try {
      returnObj=TunnelUtil.invoke(httpPrincipal,methodHandler);
    }
 catch (    Exception e) {
      if (e instanceof com.liferay.portal.kernel.search.SearchException) {
        throw (com.liferay.portal.kernel.search.SearchException)e;
      }
      throw new com.liferay.portal.kernel.exception.SystemException(e);
    }
    return (com.liferay.portal.kernel.search.Hits)returnObj;
  }
 catch (  com.liferay.portal.kernel.exception.SystemException se) {
    _log.error(se,se);
    throw se;
  }
}
