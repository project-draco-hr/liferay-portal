{
  List<String> themeIdsList=new ArrayList<String>();
  try {
    for (int i=0; i < xmls.length; i++) {
      Set<String> themeIds=_readThemes(servletContextName,servletContext,themesPath,loadFromServletContext,xmls[i],pluginPackage);
      for (      String themeId : themeIds) {
        if (!themeIdsList.contains(themeId)) {
          themeIdsList.add(themeId);
        }
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  _themesPool.clear();
  return themeIdsList;
}
