{
  String preferencesString=portalPreferences.getValue(Staging.class.getName(),StagingConstants.STAGING_RECENT_LAYOUT_IDS_MAP);
  JSONArray jsonArray=JSONFactoryUtil.createJSONArray(preferencesString);
  for (int i=0; i < jsonArray.length(); i++) {
    JSONObject jsonObject=jsonArray.getJSONObject(i);
    if (jsonObject.has(key)) {
      return GetterUtil.getLong(jsonObject.getString(key));
    }
  }
  return 0;
}
