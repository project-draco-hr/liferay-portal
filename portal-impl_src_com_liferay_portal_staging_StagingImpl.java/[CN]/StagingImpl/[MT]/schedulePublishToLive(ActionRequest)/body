{
  long stagingGroupId=ParamUtil.getLong(actionRequest,"stagingGroupId");
  Group stagingGroup=GroupLocalServiceUtil.getGroup(stagingGroupId);
  long liveGroupId=stagingGroup.getLiveGroupId();
  Map<String,String[]> parameterMap=getStagingParameters(actionRequest);
  _publishLayouts(actionRequest,stagingGroupId,liveGroupId,parameterMap,true);
}
