{
  try {
    setRecentLayoutAttribute(portalPreferences,layoutSetBranchId,plid,layoutBranchId,getRecentLayoutBranchIdKey(layoutSetBranchId,plid));
  }
 catch (  JSONException je) {
    if (_log.isWarnEnabled()) {
      _log.warn("Staging preferences are not in JSON format. " + "Unable to set recent layout branch ID",je);
    }
  }
}
