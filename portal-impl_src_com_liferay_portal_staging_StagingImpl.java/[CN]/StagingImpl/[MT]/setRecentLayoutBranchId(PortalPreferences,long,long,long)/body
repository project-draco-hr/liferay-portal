{
  try {
    setRecentLayoutAttribute(portalPreferences,layoutBranchId,getRecentLayoutBranchIdKey(layoutSetBranchId,plid));
  }
 catch (  JSONException jsone) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to set recent layout branch ID with layout " + "set branch ID " + layoutSetBranchId + " and plid "+ plid+ " with layout branch ID "+ layoutBranchId,jsone);
    }
  }
}
