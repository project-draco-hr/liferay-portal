{
  PortalPreferences portalPreferences=getPortalPreferences(user);
  try {
    return getRecentLayoutAttribute(portalPreferences,getRecentLayoutSetBranchIdKey(layoutSetId));
  }
 catch (  JSONException jsone) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to get recent layout set branch ID for user " + user.getUserId() + " and layout set ID "+ layoutSetId,jsone);
    }
  }
  return 0;
}
