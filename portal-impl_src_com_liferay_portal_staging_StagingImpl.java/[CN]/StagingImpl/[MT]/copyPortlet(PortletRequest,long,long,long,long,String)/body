{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  Map<String,String[]> parameterMap=ExportImportConfigurationParameterMapFactory.buildParameterMap(portletRequest);
  DateRange dateRange=ExportImportDateUtil.getDateRange(portletRequest,sourceGroupId,false,sourcePlid,portletId,ExportImportDateUtil.RANGE_FROM_LAST_PUBLISH_DATE);
  publishPortlet(themeDisplay.getUserId(),sourceGroupId,targetGroupId,sourcePlid,targetPlid,portletId,parameterMap,dateRange.getStartDate(),dateRange.getEndDate());
}
