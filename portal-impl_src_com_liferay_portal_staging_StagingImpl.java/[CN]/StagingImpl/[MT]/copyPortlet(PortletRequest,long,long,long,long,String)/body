{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  Map<String,String[]> parameterMap=ExportImportConfigurationParameterMapFactory.buildParameterMap(portletRequest);
  publishPortlet(themeDisplay.getUserId(),sourceGroupId,targetGroupId,sourcePlid,targetPlid,portletId,parameterMap);
}
