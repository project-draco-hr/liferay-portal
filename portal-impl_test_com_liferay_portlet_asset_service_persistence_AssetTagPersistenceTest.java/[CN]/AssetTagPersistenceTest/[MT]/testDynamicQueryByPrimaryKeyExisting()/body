{
  AssetTag newAssetTag=addAssetTag();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AssetTag.class,AssetTag.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("tagId",newAssetTag.getTagId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  AssetTag existingAssetTag=(AssetTag)result.get(0);
  assertEquals(existingAssetTag,newAssetTag);
}
