{
  int foldersCount=0;
  int articlesCount=0;
  try {
    foldersCount=JournalFolderServiceUtil.getFoldersCount(_folder.getGroupId(),_folder.getFolderId());
    articlesCount=JournalArticleServiceUtil.getArticlesCount(_folder.getGroupId(),_folder.getFolderId());
  }
 catch (  Exception e) {
  }
  if ((foldersCount + articlesCount) > 0) {
    return themeDisplay.getPathThemeImages() + "/common/folder_full_document.png";
  }
  return themeDisplay.getPathThemeImages() + "/common/folder_empty.png";
}
