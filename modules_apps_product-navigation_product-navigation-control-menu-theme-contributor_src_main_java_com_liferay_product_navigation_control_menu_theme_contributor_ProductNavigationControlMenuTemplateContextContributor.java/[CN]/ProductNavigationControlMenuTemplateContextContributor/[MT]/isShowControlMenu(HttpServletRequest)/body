{
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  if (themeDisplay.isImpersonated()) {
    return true;
  }
  if (!themeDisplay.isSignedIn()) {
    return false;
  }
  User user=themeDisplay.getUser();
  if (!user.isSetupComplete()) {
    return false;
  }
  return true;
}
