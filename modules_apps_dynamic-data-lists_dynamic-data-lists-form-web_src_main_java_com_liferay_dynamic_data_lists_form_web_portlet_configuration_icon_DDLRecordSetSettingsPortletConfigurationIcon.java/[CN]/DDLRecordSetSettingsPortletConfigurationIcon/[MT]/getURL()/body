{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(portletRequest,DDLFormPortletKeys.DYNAMIC_DATA_LISTS_FORM_ADMIN,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/admin/record_set_settings.jsp");
  portletURL.setParameter("recordSetId",String.valueOf(getRecordSetId()));
  try {
    portletURL.setWindowState(LiferayWindowState.POP_UP);
  }
 catch (  WindowStateException wse) {
  }
  return portletURL.toString();
}
