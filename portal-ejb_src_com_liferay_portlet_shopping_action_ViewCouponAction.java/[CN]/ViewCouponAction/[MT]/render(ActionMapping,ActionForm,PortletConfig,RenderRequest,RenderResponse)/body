{
  try {
    String couponId=ParamUtil.getString(req,"couponId");
    ShoppingCoupon coupon=ShoppingCouponLocalServiceUtil.getCoupon(couponId);
    req.setAttribute(WebKeys.SHOPPING_COUPON,coupon);
  }
 catch (  Exception e) {
    if (e != null && e instanceof NoSuchCouponException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.shopping.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward("portlet.shopping.view_coupon");
}
