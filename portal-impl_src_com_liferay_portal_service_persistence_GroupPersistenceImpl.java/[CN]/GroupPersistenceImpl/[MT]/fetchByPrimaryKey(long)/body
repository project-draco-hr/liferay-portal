{
  Group group=(Group)EntityCacheUtil.getResult(GroupModelImpl.ENTITY_CACHE_ENABLED,GroupImpl.class,groupId,this);
  if (group == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (group == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      group=(Group)session.get(GroupImpl.class,Long.valueOf(groupId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (group == null)) {
        EntityCacheUtil.putResult(GroupModelImpl.ENTITY_CACHE_ENABLED,GroupImpl.class,groupId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(group);
      }
      closeSession(session);
    }
  }
  return group;
}
