{
  Group result=(Group)EntityCacheUtil.getResult(GroupModelImpl.ENTITY_CACHE_ENABLED,Group.class,groupId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      Group group=(Group)session.get(GroupImpl.class,new Long(groupId));
      cacheResult(group);
      return group;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (Group)result;
  }
}
