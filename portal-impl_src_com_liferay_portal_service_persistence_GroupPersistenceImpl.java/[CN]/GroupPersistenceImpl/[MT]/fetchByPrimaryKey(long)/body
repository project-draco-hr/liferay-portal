{
  Group group=(Group)EntityCacheUtil.getResult(GroupModelImpl.ENTITY_CACHE_ENABLED,GroupImpl.class,groupId);
  if (group == _nullGroup) {
    return null;
  }
  if (group == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      group=(Group)session.get(GroupImpl.class,Long.valueOf(groupId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (group != null) {
        cacheResult(group);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(GroupModelImpl.ENTITY_CACHE_ENABLED,GroupImpl.class,groupId,_nullGroup);
      }
      closeSession(session);
    }
  }
  return group;
}
