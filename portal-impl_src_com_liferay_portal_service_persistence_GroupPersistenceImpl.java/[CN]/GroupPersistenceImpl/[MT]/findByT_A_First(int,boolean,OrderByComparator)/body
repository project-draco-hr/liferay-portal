{
  List<Group> list=findByT_A(type,active,0,1,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(6);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("type=");
    msg.append(type);
    msg.append(", active=");
    msg.append(active);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchGroupException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
