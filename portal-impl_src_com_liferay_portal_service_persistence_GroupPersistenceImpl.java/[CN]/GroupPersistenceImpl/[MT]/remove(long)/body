{
  Session session=null;
  try {
    session=openSession();
    Group group=(Group)session.get(GroupImpl.class,new Long(groupId));
    if (group == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + groupId);
      }
      throw new NoSuchGroupException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + groupId);
    }
    return remove(group);
  }
 catch (  NoSuchGroupException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
