{
  String[] listenerClassNames=StringUtil.split(GetterUtil.getString(com.liferay.portal.util.PropsUtil.get("value.object.listener.com.liferay.portal.model.Group")));
  if (listenerClassNames.length > 0) {
    try {
      List<ModelListener<Group>> listenersList=new ArrayList<ModelListener<Group>>();
      for (      String listenerClassName : listenerClassNames) {
        listenersList.add((ModelListener<Group>)InstanceFactory.newInstance(listenerClassName));
      }
      listeners=listenersList.toArray(new ModelListener[listenersList.size()]);
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  containsOrganization=new ContainsOrganization();
  addOrganization=new AddOrganization();
  clearOrganizations=new ClearOrganizations();
  removeOrganization=new RemoveOrganization();
