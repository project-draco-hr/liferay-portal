{
  try {
    for (    com.liferay.portal.model.UserGroup userGroup : userGroups) {
      removeUserGroup.remove(pk,userGroup.getPrimaryKey());
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    FinderCacheUtil.clearCache(GroupModelImpl.TABLE_GROUPS_USERGROUPS);
  }
}
