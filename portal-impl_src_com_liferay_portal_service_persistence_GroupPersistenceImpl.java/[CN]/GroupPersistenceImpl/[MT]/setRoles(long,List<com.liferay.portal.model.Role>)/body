{
  try {
    clearRoles.clear(pk);
    for (int i=0; i < roles.size(); i++) {
      com.liferay.portal.model.Role role=roles.get(i);
      addRole.add(pk,role.getPrimaryKey());
    }
  }
 catch (  DataAccessException dae) {
    throw new SystemException(dae);
  }
 finally {
    FinderCache.clearCache("Groups_Roles");
  }
}
