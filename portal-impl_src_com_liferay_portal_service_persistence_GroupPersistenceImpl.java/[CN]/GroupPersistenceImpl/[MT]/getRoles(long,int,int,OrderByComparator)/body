{
  Object[] finderArgs=new Object[]{new Long(pk),String.valueOf(start),String.valueOf(end),String.valueOf(orderByComparator)};
  List<com.liferay.portal.model.Role> list=(List<com.liferay.portal.model.Role>)FinderCacheUtil.getResult(FINDER_PATH_GET_ROLES,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      String sql=null;
      if (orderByComparator != null) {
        sql=_SQL_GETROLES.concat(ORDER_BY_CLAUSE).concat(orderByComparator.getOrderBy());
      }
 else {
        sql=_SQL_GETROLES.concat(com.liferay.portal.model.impl.RoleModelImpl.ORDER_BY_SQL);
      }
      SQLQuery q=session.createSQLQuery(sql);
      q.addEntity("Role_",com.liferay.portal.model.impl.RoleImpl.class);
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(pk);
      list=(List<com.liferay.portal.model.Role>)QueryUtil.list(q,getDialect(),start,end);
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<com.liferay.portal.model.Role>();
      }
      rolePersistence.cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_GET_ROLES,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
