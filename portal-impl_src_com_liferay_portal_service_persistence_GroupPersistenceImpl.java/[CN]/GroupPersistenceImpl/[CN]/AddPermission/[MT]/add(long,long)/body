{
  if (!_persistenceImpl.containsPermission.contains(groupId,permissionId)) {
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onBeforeAddAssociation(groupId,com.liferay.portal.model.Permission.class.getName(),permissionId);
      }
    }
    _sqlUpdate.update(new Object[]{new Long(groupId),new Long(permissionId)});
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onAfterAddAssociation(groupId,com.liferay.portal.model.Permission.class.getName(),permissionId);
      }
    }
  }
}
