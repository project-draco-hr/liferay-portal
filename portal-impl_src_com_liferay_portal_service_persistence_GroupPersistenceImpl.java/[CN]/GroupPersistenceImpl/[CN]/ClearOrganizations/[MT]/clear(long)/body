        listener.onBeforeRemoveAssociation(organization.getPrimaryKey(),Group.class.getName(),groupId);
      }
    }
  }
  _sqlUpdate.update(new Object[]{new Long(groupId)});
  if ((listeners.length > 0) || (organizationListeners.length > 0)) {
