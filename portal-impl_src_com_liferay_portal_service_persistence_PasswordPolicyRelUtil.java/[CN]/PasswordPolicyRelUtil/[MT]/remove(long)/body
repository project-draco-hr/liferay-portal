{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(passwordPolicyRelId));
  }
  com.liferay.portal.model.PasswordPolicyRel passwordPolicyRel=getPersistence().remove(passwordPolicyRelId);
  if (listener != null) {
    listener.onAfterRemove(passwordPolicyRel);
  }
  return passwordPolicyRel;
}
