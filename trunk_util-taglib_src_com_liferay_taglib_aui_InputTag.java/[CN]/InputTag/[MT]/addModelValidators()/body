{
  Class<?> model=_model;
  if (model == null) {
    model=(Class<?>)pageContext.getAttribute("aui:model-context:model");
  }
  if ((model == null) || Validator.isNotNull(_type)) {
    return;
  }
  String field=_field;
  if (Validator.isNull(field)) {
    field=_name;
  }
  List<Tuple> modelValidators=ModelHintsUtil.getValidators(model.getName(),field);
  if (modelValidators == null) {
    return;
  }
  for (  Tuple modelValidator : modelValidators) {
    String validatorName=(String)modelValidator.getObject(1);
    String validatorErrorMessage=(String)modelValidator.getObject(2);
    String validatorValue=(String)modelValidator.getObject(3);
    ValidatorTag validatorTag=new ValidatorTagImpl(validatorName,validatorErrorMessage,validatorValue);
    addValidatorTag(validatorName,validatorTag);
  }
}
