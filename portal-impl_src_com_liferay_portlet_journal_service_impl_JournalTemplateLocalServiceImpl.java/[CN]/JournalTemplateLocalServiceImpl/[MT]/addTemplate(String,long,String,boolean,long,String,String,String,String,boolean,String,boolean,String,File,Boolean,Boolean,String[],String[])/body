{
  long groupId=PortalUtil.getPortletGroupId(plid);
  return addTemplateToGroup(uuid,userId,templateId,autoTemplateId,groupId,structureId,name,description,xsl,formatXsl,langType,smallImage,smallImageURL,smallFile,addCommunityPermissions,addGuestPermissions,communityPermissions,guestPermissions);
