{
  if (JournalArticleUtil.countByG_T(template.getGroupId(),template.getTemplateId()) > 0) {
    throw new RequiredTemplateException();
  }
  ImageLocalUtil.deleteImage(template.getSmallImageId());
  ResourceLocalServiceUtil.deleteResource(template.getCompanyId(),JournalTemplate.class.getName(),ResourceImpl.SCOPE_INDIVIDUAL,template.getId());
  JournalTemplateUtil.remove(template.getPrimaryKey());
}
