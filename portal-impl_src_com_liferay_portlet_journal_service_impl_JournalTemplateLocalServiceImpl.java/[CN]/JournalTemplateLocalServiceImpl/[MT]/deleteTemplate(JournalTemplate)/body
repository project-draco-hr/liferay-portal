{
  if (journalArticlePersistence.countByG_T(template.getGroupId(),template.getTemplateId()) > 0) {
    throw new RequiredTemplateException();
  }
  ImageLocalUtil.deleteImage(template.getSmallImageId());
  resourceLocalService.deleteResource(template.getCompanyId(),JournalTemplate.class.getName(),ResourceImpl.SCOPE_INDIVIDUAL,template.getId());
  webDAVPropsLocalService.deleteProps(JournalTemplate.class.getName(),template.getPrimaryKey());
  journalTemplatePersistence.remove(template.getPrimaryKey());
}
