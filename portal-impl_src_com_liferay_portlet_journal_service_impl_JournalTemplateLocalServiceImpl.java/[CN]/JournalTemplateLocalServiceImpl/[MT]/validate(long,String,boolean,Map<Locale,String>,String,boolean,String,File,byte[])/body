{
  if (!autoTemplateId) {
    validate(templateId);
    JournalTemplate template=journalTemplatePersistence.fetchByG_T(groupId,templateId);
    if (template != null) {
      throw new DuplicateTemplateIdException();
    }
  }
  validate(nameMap,xsl,smallImage,smallImageURL,smallImageFile,smallImageBytes);
}
