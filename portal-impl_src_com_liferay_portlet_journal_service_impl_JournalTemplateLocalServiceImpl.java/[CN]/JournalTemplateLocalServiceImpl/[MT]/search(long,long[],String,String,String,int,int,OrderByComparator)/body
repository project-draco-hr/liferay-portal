{
  long[] classNameIds={PortalUtil.getClassNameId(DDMStructure.class)};
  long[] classPKs=JournalUtil.getStructureClassPKs(groupIds,structureId);
  List<DDMTemplate> ddmTemplates=ddmTemplateFinder.findByKeywords(companyId,groupIds,classNameIds,classPKs,keywords,null,null,start,end,obc);
  return JournalUtil.toJournalTemplates(ddmTemplates);
}
