{
  if (!autoTemplateId) {
    validate(templateId);
    JournalTemplate template=journalTemplatePersistence.fetchByG_T(groupId,templateId);
    if (template != null) {
      throw new DuplicateTemplateIdException();
    }
  }
  validate(name,description,xsl,smallImage,smallImageURL,smallFile,smallBytes);
}
