{
  long groupId=PortalUtil.getScopeGroupId(plid);
  return addTemplateToGroup(uuid,userId,templateId,autoTemplateId,groupId,structureId,name,description,xsl,formatXsl,langType,cacheable,smallImage,smallImageURL,smallFile,addCommunityPermissions,addGuestPermissions,communityPermissions,guestPermissions);
