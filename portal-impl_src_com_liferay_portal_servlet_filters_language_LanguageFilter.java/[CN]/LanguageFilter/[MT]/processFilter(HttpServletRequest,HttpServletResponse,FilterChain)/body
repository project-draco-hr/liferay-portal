{
  CacheResponse cacheResponse=new CacheResponse(response,StringPool.UTF8);
  processFilter(LanguageFilter.class,request,cacheResponse,filterChain);
  byte[] bytes=translateResponse(request,cacheResponse,cacheResponse.getData());
  CacheResponseData cacheResponseData=new CacheResponseData(bytes,cacheResponse.getContentType(),cacheResponse.getHeaders());
  CacheResponseUtil.write(response,cacheResponseData);
}
