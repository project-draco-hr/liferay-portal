{
  CacheResponse cacheResponse=new CacheResponse(response,StringPool.UTF8);
  processFilter(LanguageFilter.class,request,cacheResponse,filterChain);
  byte[] bytes=translateResponse(request,cacheResponse,cacheResponse.unsafeGetData(),cacheResponse.getDataLength());
  CacheResponseData cacheResponseData=new CacheResponseData(bytes,bytes.length,cacheResponse.getContentType(),cacheResponse.getHeaders());
  CacheResponseUtil.write(response,cacheResponseData);
}
