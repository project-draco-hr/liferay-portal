{
  CacheResponse cacheResponse=new CacheResponse(response,StringPool.UTF8);
  processFilter(LanguageFilter.class,request,cacheResponse,filterChain);
  String content=LanguageFilterUtil.replaceLanguage(request,cacheResponse,cacheResponse.getData());
  CacheResponseData cacheResponseData=new CacheResponseData(content.getBytes(),cacheResponse.getContentType(),cacheResponse.getHeaders());
  CacheResponseUtil.write(response,cacheResponseData);
}
