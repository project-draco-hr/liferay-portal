{
  StringBuilder sb=new StringBuilder();
  if (addresses != null) {
    for (int i=0; i < addresses.length; i++) {
      if (addresses[i] != null) {
        sb.append(addresses[i].toUnicodeString());
      }
      if ((i + 1) != addresses.length) {
        sb.append(", ");
      }
    }
  }
  return sb.toString();
}
