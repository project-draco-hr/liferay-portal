{
  if (isSupportsAlterColumnName()) {
    runSQL("alter_column_type TagsAsset title VARCHAR(255) null");
  }
 else {
    UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(_TAGS_ASSET_TABLE_NAME,_TAGS_ASSET_TABLE_COLUMNS);
