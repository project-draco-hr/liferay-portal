{
  if (isSupportsAlterColumnName()) {
    runSQL("alter_column_type TagsAsset title VARCHAR(255) null");
  }
 else {
    UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(TagsAssetImpl.TABLE_NAME,TagsAssetImpl.TABLE_COLUMNS);
    upgradeTable.setCreateSQL(TagsAssetImpl.TABLE_SQL_CREATE);
    upgradeTable.updateTable();
  }
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(TagsPropertyImpl.TABLE_NAME,TagsPropertyImpl.TABLE_COLUMNS,new TagsPropertyValueUpgradeColumnImpl("value"));
