{
  String ddmStructureKey=ParamUtil.getString(_request,"ddmStructureKey");
  if (Validator.isNotNull(ddmStructureKey)) {
    return false;
  }
  if (isNavigationMine()) {
    return false;
  }
  if (isNavigationRecent()) {
    return false;
  }
  if (isSearch()) {
    return false;
  }
  return true;
}
