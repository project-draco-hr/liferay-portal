{
  EmailAddress emailAddress=(EmailAddress)EntityCacheUtil.getResult(EmailAddressModelImpl.ENTITY_CACHE_ENABLED,EmailAddressImpl.class,emailAddressId);
  if (emailAddress == _nullEmailAddress) {
    return null;
  }
  if (emailAddress == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      emailAddress=(EmailAddress)session.get(EmailAddressImpl.class,Long.valueOf(emailAddressId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (emailAddress != null) {
        cacheResult(emailAddress);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(EmailAddressModelImpl.ENTITY_CACHE_ENABLED,EmailAddressImpl.class,emailAddressId,_nullEmailAddress);
      }
      closeSession(session);
    }
  }
  return emailAddress;
}
