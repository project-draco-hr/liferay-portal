{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,emailAddress,merge);
    emailAddress.setNew(false);
    return emailAddress;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(EmailAddress.class.getName());
  }
}
