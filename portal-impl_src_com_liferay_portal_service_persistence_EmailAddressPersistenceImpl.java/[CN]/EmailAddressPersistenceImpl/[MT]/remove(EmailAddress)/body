{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(emailAddress);
    }
  }
  emailAddress=removeImpl(emailAddress);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(emailAddress);
    }
  }
  return emailAddress;
}
