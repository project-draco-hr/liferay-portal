{
  MBMailingList newMBMailingList=addMBMailingList();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBMailingList.class,MBMailingList.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("mailingListId",newMBMailingList.getMailingListId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  MBMailingList existingMBMailingList=(MBMailingList)result.get(0);
  assertEquals(existingMBMailingList,newMBMailingList);
}
