{
  PoshiRunnerContext.readFiles();
  List<String> classCommandNames=new ArrayList<>();
  String testNameString=PropsValues.TEST_NAME;
  List<String> testNames=Arrays.asList(testNameString.split("\\s*,\\s*"));
  for (  String testName : testNames) {
    PoshiRunnerValidation.validate(testName);
    if (testName.contains("#")) {
      classCommandNames.add(testName);
    }
 else {
      String className=testName;
      Element rootElement=PoshiRunnerContext.getTestCaseRootElement(className);
      List<Element> commandElements=rootElement.elements("command");
      for (      Element commandElement : commandElements) {
        classCommandNames.add(className + "#" + commandElement.attributeValue("name"));
      }
    }
  }
  return classCommandNames;
}
