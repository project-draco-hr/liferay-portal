{
  try {
    ActionUtil.getNode(renderRequest);
    ActionUtil.getPage(renderRequest);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchNodeException || e instanceof NoSuchPageException || e instanceof PageTitleException|| e instanceof PrincipalException) {
      SessionErrors.add(renderRequest,e.getClass());
      return "/wiki/error.jsp";
    }
 else {
      throw new PortletException(e);
    }
  }
  return "/wiki/move_page.jsp";
}
