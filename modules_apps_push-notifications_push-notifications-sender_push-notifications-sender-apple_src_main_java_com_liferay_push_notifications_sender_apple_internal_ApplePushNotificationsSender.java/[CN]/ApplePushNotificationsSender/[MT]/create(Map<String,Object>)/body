{
  String certificatePath=MapUtil.getString(configuration,PortletPropsKeys.APPLE_CERTIFICATE_PATH,null);
  String certificatePassword=MapUtil.getString(configuration,PortletPropsKeys.APPLE_CERTIFICATE_PASSWORD,null);
  Boolean sandbox=(Boolean)configuration.get(PortletPropsKeys.APPLE_SANDBOX);
  return new ApplePushNotificationsSender(certificatePath,certificatePassword,sandbox);
}
