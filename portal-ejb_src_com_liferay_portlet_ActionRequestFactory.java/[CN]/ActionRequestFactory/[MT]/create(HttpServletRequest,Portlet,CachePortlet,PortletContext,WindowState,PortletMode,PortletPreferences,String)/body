{
  ActionRequestImpl actionRequestImpl=null;
  if (CachePropsUtil.COMMONS_POOL_ENABLED) {
    actionRequestImpl=(ActionRequestImpl)_instance._pool.borrowObject();
  }
 else {
    actionRequestImpl=new ActionRequestImpl();
  }
  actionRequestImpl.init(req,portlet,cachePortlet,portletCtx,windowState,portletMode,prefs,layoutId);
  return actionRequestImpl;
}
