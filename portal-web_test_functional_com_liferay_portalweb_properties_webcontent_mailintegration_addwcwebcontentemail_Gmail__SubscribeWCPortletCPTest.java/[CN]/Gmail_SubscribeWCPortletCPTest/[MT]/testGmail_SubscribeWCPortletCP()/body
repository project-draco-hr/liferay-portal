{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Control Panel");
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(5000);
  selenium.clickAt("//strong/a",RuntimeVariables.replace("Site Name"));
  selenium.waitForVisible("link=Site Name");
  assertEquals(RuntimeVariables.replace("Site Name"),selenium.getText("link=Site Name"));
  selenium.clickAt("link=Site Name",RuntimeVariables.replace("Site Name"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Web Content",RuntimeVariables.replace("Web Content"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Subscribe"),selenium.getText("//span[@class='subscribe-link']/a/span"));
  selenium.clickAt("//span[@class='subscribe-link']/a/span",RuntimeVariables.replace("Subscribe"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals(RuntimeVariables.replace("Unsubscribe"),selenium.getText("//span[@class='subscribe-link']/a/span"));
}
