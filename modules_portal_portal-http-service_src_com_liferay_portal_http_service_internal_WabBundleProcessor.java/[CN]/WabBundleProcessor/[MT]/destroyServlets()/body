{
  Map<String,ServletDefinition> servlets=_webXML.getServletDefinitions();
  List<ServletDefinition> list=new ArrayList<ServletDefinition>(servlets.values());
  Collections.reverse(list);
  for (  ServletDefinition servletDefinition : list) {
    Servlet servlet=servletDefinition.getServlet();
    try {
      if (servlet instanceof WabResourceServlet) {
        _extendedHttpService.unregister(servletDefinition.getURLPatterns().get(0),_servletContextName);
      }
 else {
        _extendedHttpService.unregisterServlet(servlet,_servletContextName);
      }
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
}
