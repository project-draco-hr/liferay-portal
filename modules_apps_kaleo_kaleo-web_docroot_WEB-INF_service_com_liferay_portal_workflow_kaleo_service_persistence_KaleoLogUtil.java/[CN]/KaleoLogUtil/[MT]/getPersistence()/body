{
  if (_persistence == null) {
    _persistence=(KaleoLogPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoLogPersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoLogUtil.class,"_persistence");
  }
  return _persistence;
}
