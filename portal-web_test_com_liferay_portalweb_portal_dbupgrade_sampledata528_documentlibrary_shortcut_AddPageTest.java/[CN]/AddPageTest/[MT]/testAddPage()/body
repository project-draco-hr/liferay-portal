{
  selenium.open("/user/joebloggs/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Communities I Own")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Communities I Own",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_29_name",RuntimeVariables.replace("Document Library Shortcut Community"));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//strong/span",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//body/div[2]/ul/li[2]/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("//body/div[2]/ul/li[2]/a"));
  selenium.waitForPageToLoad("30000");
  selenium.type("_29_name_en_US",RuntimeVariables.replace("Document Library Page"));
  selenium.clickAt("//input[@value='Add Page']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertTrue(selenium.isElementPresent("link=Document Library Page"));
}
