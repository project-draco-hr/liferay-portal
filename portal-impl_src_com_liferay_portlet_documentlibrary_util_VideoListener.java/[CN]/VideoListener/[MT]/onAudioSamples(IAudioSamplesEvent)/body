{
  IAudioSamples samples=event.getAudioSamples();
  if (_resampler == null) {
    _resampler=IAudioResampler.make(samples.getChannels(),samples.getChannels(),44100,samples.getSampleRate());
  }
  if (_resampler != null && samples.getNumSamples() > 0) {
    IAudioSamples out=IAudioSamples.make(samples.getNumSamples(),samples.getChannels());
    _resampler.resample(out,samples,samples.getNumSamples());
    AudioSamplesEvent asc=new AudioSamplesEvent(event.getSource(),out,event.getStreamIndex());
    super.onAudioSamples(asc);
    out.delete();
  }
}
