{
  super.setUp();
  _folder=DLAppLocalServiceUtil.addFolder(TestPropsValues.getUserId(),TestPropsValues.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,"Folder A","",getServiceContext());
  _subfolder=DLAppLocalServiceUtil.addFolder(TestPropsValues.getUserId(),TestPropsValues.getGroupId(),_folder.getFolderId(),"SubFolder AA","",getServiceContext());
  _basicDocument=DLFileEntryTypeLocalServiceUtil.getFileEntryType(0);
  _dlFileEntryTypes=DLFileEntryTypeLocalServiceUtil.getFileEntryTypes(DLUtil.getGroupIds(TestPropsValues.getGroupId()));
  for (  DLFileEntryType dlFileEntryType : _dlFileEntryTypes) {
    String name=dlFileEntryType.getName();
    if (name.equals(DLFileEntryTypeConstants.NAME_IMAGE)) {
      _image=dlFileEntryType;
    }
 else     if (name.equals(DLFileEntryTypeConstants.NAME_VIDEO)) {
      _video=dlFileEntryType;
    }
  }
  assertNotNull(DLFileEntryTypeConstants.NAME_BASIC_DOCUMENT + " cannot be null",_basicDocument);
  assertNotNull(DLFileEntryTypeConstants.NAME_IMAGE + " cannot be null",_image);
  assertNotNull(DLFileEntryTypeConstants.NAME_VIDEO + " cannot be null",_video);
}
