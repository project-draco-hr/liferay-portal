{
  validate(ExpandoColumnConstants.STRING_ARRAY);
  String regex="(?<=(?<!\\\\)(\\\\\\\\){0,100}),";
  String[] dataArray=getData().split(regex);
  for (int i=0; i < dataArray.length; i++) {
    dataArray[i]=StringUtil.replace(dataArray[i],"\\,",",");
    dataArray[i]=StringUtil.replace(dataArray[i],"\\\\","\\");
  }
  return dataArray;
}
