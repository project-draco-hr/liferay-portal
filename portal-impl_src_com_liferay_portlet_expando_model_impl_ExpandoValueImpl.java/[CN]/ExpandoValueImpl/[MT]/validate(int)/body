{
  ExpandoColumn column=ExpandoColumnLocalServiceUtil.getColumn(getColumnId());
  if (column.getType() != type) {
    throw new ValueDataException("Column " + getColumnId() + " has type "+ ExpandoColumnConstants.getTypeLabel(column.getType())+ " and is not compatbile with type "+ ExpandoColumnConstants.getTypeLabel(type));
  }
}
