{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      selenium.selectWindow("null");
    selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace("Shopping Test Page"));
selenium.waitForPageToLoad("30000");
selenium.clickAt("link=Coupons",RuntimeVariables.replace("Coupons"));
selenium.waitForPageToLoad("30000");
selenium.clickAt("//input[@value='Add Coupon']",RuntimeVariables.replace("Add Coupon"));
selenium.waitForPageToLoad("30000");
boolean autogenerateCodeChecked=selenium.isChecked("//input[@id='_34_autoCodeCheckbox']");
if (autogenerateCodeChecked) {
label=2;
continue;
}
selenium.clickAt("//input[@id='_34_autoCodeCheckbox']",RuntimeVariables.replace("Autogenerate Code"));
case 2:
assertTrue(selenium.isChecked("//input[@id='_34_autoCodeCheckbox']"));
selenium.type("//input[@id='_34_name']",RuntimeVariables.replace("Shopping Coupon3 Name"));
selenium.type("//textarea[@id='_34_description']",RuntimeVariables.replace("Shopping Coupon3 Description"));
selenium.type("//input[@id='_34_discount']",RuntimeVariables.replace("0.50"));
selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
selenium.waitForPageToLoad("30000");
assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
assertTrue(selenium.isPartialText("//tr[@class='portlet-section-body results-row']/td[3]","Shopping Coupon1 Name"));
assertTrue(selenium.isPartialText("//tr[@class='portlet-section-body results-row']/td[3]","Shopping Coupon1 Description"));
assertTrue(selenium.isPartialText("//tr[@class='portlet-section-alternate results-row alt']/td[3]","Shopping Coupon2 Name"));
assertTrue(selenium.isPartialText("//tr[@class='portlet-section-alternate results-row alt']/td[3]","Shopping Coupon2 Description"));
assertTrue(selenium.isPartialText("//tr[@class='portlet-section-body results-row last']/td[3]","Shopping Coupon3 Name"));
assertTrue(selenium.isPartialText("//tr[@class='portlet-section-body results-row last']/td[3]","Shopping Coupon3 Description"));
case 100:
label=-1;
}
}
}
