{
  Session session=null;
  try {
    session=openSession();
    UserGroupHBM userGroupHBM=(UserGroupHBM)session.get(UserGroupHBM.class,pk);
    if (userGroupHBM == null) {
      _log.warn("No UserGroup exists with the primary key " + pk.toString());
      throw new NoSuchUserGroupException("No UserGroup exists with the primary key " + pk.toString());
    }
    userGroupHBM.getUsers().clear();
    session.flush();
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
