{
  Session session=null;
  try {
    session=openSession();
    session.delete(userGroup);
    session.flush();
    clearUsers.clear(userGroup.getPrimaryKey());
    return userGroup;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
