{
  if (mimeTypes.length == 0) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(mimeTypes.length * 2 - 1);
  for (int i=0; i < mimeTypes.length; i++) {
    sb.append(table);
    sb.append(".mimeType = '");
    sb.append(mimeTypes[i]);
    sb.append("'");
    if ((i + 1) != mimeTypes.length) {
      sb.append(WHERE_OR);
    }
  }
  return sb.toString();
}
