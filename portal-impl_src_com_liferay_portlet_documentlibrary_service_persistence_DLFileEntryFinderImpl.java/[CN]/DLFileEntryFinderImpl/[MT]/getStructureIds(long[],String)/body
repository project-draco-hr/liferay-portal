{
  if (structureIds.length == 0) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(structureIds.length * 2 - 1);
  for (int i=0; i < structureIds.length; i++) {
    sb.append(tableName);
    sb.append(".structureId = ?");
    if ((i + 1) != structureIds.length) {
      sb.append(WHERE_OR);
    }
  }
  return sb.toString();
}
