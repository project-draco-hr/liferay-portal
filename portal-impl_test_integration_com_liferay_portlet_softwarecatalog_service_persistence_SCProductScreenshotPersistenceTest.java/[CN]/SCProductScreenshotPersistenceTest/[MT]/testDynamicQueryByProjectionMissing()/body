{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SCProductScreenshot.class,SCProductScreenshot.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("productScreenshotId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("productScreenshotId",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
