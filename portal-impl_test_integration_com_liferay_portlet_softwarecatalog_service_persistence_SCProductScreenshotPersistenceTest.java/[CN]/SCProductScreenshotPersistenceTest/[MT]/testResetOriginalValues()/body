{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  SCProductScreenshot newSCProductScreenshot=addSCProductScreenshot();
  _persistence.clearCache();
  SCProductScreenshotModelImpl existingSCProductScreenshotModelImpl=(SCProductScreenshotModelImpl)_persistence.findByPrimaryKey(newSCProductScreenshot.getPrimaryKey());
  Assert.assertEquals(existingSCProductScreenshotModelImpl.getThumbnailId(),existingSCProductScreenshotModelImpl.getOriginalThumbnailId());
  Assert.assertEquals(existingSCProductScreenshotModelImpl.getFullImageId(),existingSCProductScreenshotModelImpl.getOriginalFullImageId());
  Assert.assertEquals(existingSCProductScreenshotModelImpl.getProductEntryId(),existingSCProductScreenshotModelImpl.getOriginalProductEntryId());
  Assert.assertEquals(existingSCProductScreenshotModelImpl.getPriority(),existingSCProductScreenshotModelImpl.getOriginalPriority());
}
