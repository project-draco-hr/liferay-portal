{
  if (_log.isDebugEnabled()) {
    if (service == null) {
      _log.debug("Remove service hook " + className);
    }
 else {
      _log.debug("Add service hook " + className + " "+ service.getClass().getName());
    }
  }
  if (service == null) {
    _services.remove(className);
  }
 else {
    _services.put(className,service);
  }
}
