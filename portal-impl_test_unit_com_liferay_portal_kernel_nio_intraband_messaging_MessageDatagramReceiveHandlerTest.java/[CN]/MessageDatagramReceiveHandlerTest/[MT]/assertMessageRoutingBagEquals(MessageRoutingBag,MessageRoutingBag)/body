{
  Assert.assertEquals(expectedMessageRoutingBag.getDestinationName(),actualMessageRoutingBag.getDestinationName());
  Assert.assertEquals(expectedMessageRoutingBag.isRoutingDowncast(),actualMessageRoutingBag.isRoutingDowncast());
  Field routingTraceField=ReflectionUtil.getDeclaredField(MessageRoutingBag.class,"_routingTrace");
  Assert.assertEquals(routingTraceField.get(expectedMessageRoutingBag),routingTraceField.get(actualMessageRoutingBag));
}
