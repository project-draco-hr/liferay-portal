{
  if (obj == null) {
    return -1;
  }
  ResourceCodePK pk=(ResourceCodePK)obj;
  int value=0;
  value=companyId.compareTo(pk.companyId);
  if (value != 0) {
    return value;
  }
  value=name.compareTo(pk.name);
  if (value != 0) {
    return value;
  }
  value=scope.compareTo(pk.scope);
  if (value != 0) {
    return value;
  }
  return 0;
}
