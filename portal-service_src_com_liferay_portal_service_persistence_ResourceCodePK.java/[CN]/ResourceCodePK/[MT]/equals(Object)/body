{
  if (obj == null) {
    return false;
  }
  ResourceCodePK pk=null;
  try {
    pk=(ResourceCodePK)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  if ((companyId.equals(pk.companyId)) && (name.equals(pk.name)) && (scope.equals(pk.scope))) {
    return true;
  }
 else {
    return false;
  }
}
