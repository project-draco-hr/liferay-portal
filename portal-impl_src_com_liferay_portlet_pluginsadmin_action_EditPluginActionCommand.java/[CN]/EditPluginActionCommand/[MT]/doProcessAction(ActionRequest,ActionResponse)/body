{
  try {
    updatePluginSetting(actionRequest);
  }
 catch (  Exception e) {
    if (e instanceof PrincipalException) {
      SessionErrors.add(actionRequest,e.getClass());
      sendRedirect(actionRequest,actionResponse,"/html/portlet/plugins_admin/error.jsp");
    }
 else {
      throw e;
    }
  }
}
