{
  if (!isNull(_contextPath)) {
    url=_contextPath + url;
  }
  if (_logger.isDebugEnabled()) {
    _logger.debug("Sending PUT request to " + _login + "@"+ _hostName+ url);
    logArguments("Http parameters",parameters);
    logArguments("Http headers",headers);
  }
  HttpPut httpPut=new HttpPut(url);
  List<NameValuePair> nameValuePairs=toNameValuePairs(parameters);
  HttpEntity httpEntity=new UrlEncodedFormEntity(nameValuePairs,StandardCharsets.UTF_8);
  addHeaders(httpPut,headers);
  httpPut.setEntity(httpEntity);
  return execute(httpPut);
}
