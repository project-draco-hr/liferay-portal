{
  if (!isNull(_contextPath)) {
    url=_contextPath + url;
  }
  if (_logger.isDebugEnabled()) {
    _logger.debug("Sending POST request to " + _login + "@"+ _hostName+ url);
  }
  HttpPost httpPost=new HttpPost(url);
  List<NameValuePair> nameValuePairs=toNameValuePairs(parameters);
  HttpEntity httpEntity=new UrlEncodedFormEntity(nameValuePairs,StandardCharsets.UTF_8);
  for (  Map.Entry<String,String> entry : headers.entrySet()) {
    httpPost.addHeader(entry.getKey(),entry.getValue());
  }
  for (  Map.Entry<String,String> entry : _headers.entrySet()) {
    httpPost.addHeader(entry.getKey(),entry.getValue());
  }
  httpPost.setEntity(httpEntity);
  return execute(httpPost);
}
