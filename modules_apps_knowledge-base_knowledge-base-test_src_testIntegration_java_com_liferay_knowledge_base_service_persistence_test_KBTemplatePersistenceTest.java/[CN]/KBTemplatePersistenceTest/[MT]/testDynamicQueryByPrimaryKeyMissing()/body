{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(KBTemplate.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("kbTemplateId",RandomTestUtil.nextLong()));
  List<KBTemplate> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
