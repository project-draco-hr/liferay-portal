{
  KBTemplate newKBTemplate=addKBTemplate();
  _persistence.clearCache();
  KBTemplate existingKBTemplate=_persistence.findByPrimaryKey(newKBTemplate.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingKBTemplate.getUuid(),ReflectionTestUtil.invoke(existingKBTemplate,"getOriginalUuid",new Class<?>[0])));
  Assert.assertEquals(Long.valueOf(existingKBTemplate.getGroupId()),ReflectionTestUtil.<Long>invoke(existingKBTemplate,"getOriginalGroupId",new Class<?>[0]));
}
