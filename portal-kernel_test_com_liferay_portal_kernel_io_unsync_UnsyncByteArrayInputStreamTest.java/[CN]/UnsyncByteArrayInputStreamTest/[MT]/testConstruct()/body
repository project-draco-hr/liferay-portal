{
  UnsyncByteArrayInputStream unsyncBais=new UnsyncByteArrayInputStream(testData);
  int available=unsyncBais.available();
  assertEquals(testDataSize,available);
  unsyncBais=new UnsyncByteArrayInputStream(testData,testDataSize / 2,testDataSize / 2);
  available=unsyncBais.available();
  assertEquals(testDataSize / 2,available);
}
