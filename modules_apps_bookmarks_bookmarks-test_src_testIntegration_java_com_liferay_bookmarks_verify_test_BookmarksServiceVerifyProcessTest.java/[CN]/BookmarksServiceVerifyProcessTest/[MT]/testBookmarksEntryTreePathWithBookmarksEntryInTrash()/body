{
  BookmarksFolder parentFolder=BookmarksTestUtil.addFolder(_group.getGroupId(),RandomTestUtil.randomString());
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId());
  BookmarksEntry entry=BookmarksTestUtil.addEntry(parentFolder.getFolderId(),true,serviceContext);
  BookmarksEntryLocalServiceUtil.moveEntryToTrash(TestPropsValues.getUserId(),entry.getEntryId());
  BookmarksFolderLocalServiceUtil.deleteFolder(parentFolder.getFolderId(),false);
  doVerify();
}
