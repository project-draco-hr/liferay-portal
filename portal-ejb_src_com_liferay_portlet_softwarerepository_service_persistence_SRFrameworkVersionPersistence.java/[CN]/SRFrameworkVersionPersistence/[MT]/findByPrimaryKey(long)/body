{
  SRFrameworkVersion srFrameworkVersion=fetchByPrimaryKey(frameworkVersionId);
  if (srFrameworkVersion == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No SRFrameworkVersion exists with the primary key " + frameworkVersionId);
    }
    throw new NoSuchFrameworkVersionException("No SRFrameworkVersion exists with the primary key " + frameworkVersionId);
  }
  return srFrameworkVersion;
}
