{
  if ("configuration".equals(key)) {
    try {
      return (TemplateModel)getCachedWrapperFor(((Template)configurable).getConfiguration());
    }
 catch (    RemoteException e) {
      throw new TemplateModelException(e);
    }
  }
  if ("name".equals(key)) {
    return name;
  }
  return super.get(key);
}
