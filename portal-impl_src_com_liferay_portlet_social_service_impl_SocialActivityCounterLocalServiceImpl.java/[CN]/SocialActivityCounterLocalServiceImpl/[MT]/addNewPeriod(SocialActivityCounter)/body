{
  if (activityCounter == null) {
    return null;
  }
  activityCounter.setEndPeriod(SocialCounterPeriodUtil.getStartPeriod() - 1);
  socialActivityCounterPersistence.update(activityCounter,false);
  return addActivityCounter(activityCounter.getGroupId(),activityCounter.getClassNameId(),activityCounter.getClassPK(),activityCounter.getName(),activityCounter.getOwnerType(),activityCounter.getTotalValue());
}
