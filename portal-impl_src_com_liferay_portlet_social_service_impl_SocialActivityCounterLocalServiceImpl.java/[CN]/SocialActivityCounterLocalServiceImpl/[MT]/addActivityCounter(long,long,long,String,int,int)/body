{
  SocialActivityCounter activityCounter=null;
  try {
    activityCounter=addActivityCounter(groupId,classNameId,classPK,name,ownerType,0,overallValue);
  }
 catch (  SystemException se) {
    activityCounter=fetchLatestActivityCounter(groupId,classNameId,classPK,name,ownerType);
  }
