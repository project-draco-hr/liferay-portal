{
  if (includeCurrentPeriod) {
    offset=offset - 1;
  }
  int startPeriod=SocialCounterPeriodUtil.getStartPeriod(-offset);
  int endPeriod=-1;
  if (!includeCurrentPeriod) {
    endPeriod=SocialCounterPeriodUtil.getStartPeriod() - 1;
  }
  return getActivityCounters(groupId,name,startPeriod,endPeriod);
}
