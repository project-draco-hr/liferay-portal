{
  StringBundler sb=new StringBundler(7);
  sb.append(StringUtil.toHexString(groupId));
  sb.append(StringPool.UNDERSCORE);
  sb.append(StringUtil.toHexString(classNameId));
  sb.append(StringPool.UNDERSCORE);
  sb.append(StringUtil.toHexString(classPK));
  sb.append(StringPool.UNDERSCORE);
  sb.append(name);
  return sb.toString();
}
