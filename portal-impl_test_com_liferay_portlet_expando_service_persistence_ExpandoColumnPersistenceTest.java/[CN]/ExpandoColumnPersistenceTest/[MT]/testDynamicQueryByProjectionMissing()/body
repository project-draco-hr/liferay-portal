{
  ExpandoColumn newExpandoColumn=addExpandoColumn();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ExpandoColumn.class,ExpandoColumn.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("columnId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("columnId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
