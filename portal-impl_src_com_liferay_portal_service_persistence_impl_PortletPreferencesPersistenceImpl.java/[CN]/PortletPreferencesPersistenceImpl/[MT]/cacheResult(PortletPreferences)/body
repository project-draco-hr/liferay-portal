{
  entityCache.putResult(PortletPreferencesModelImpl.ENTITY_CACHE_ENABLED,PortletPreferencesImpl.class,portletPreferences.getPrimaryKey(),portletPreferences);
  finderCache.putResult(FINDER_PATH_FETCH_BY_O_O_P_P,new Object[]{portletPreferences.getOwnerId(),portletPreferences.getOwnerType(),portletPreferences.getPlid(),portletPreferences.getPortletId()},portletPreferences);
  portletPreferences.resetOriginalValues();
}
