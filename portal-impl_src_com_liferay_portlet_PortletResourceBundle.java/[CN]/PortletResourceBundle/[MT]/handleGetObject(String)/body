{
  try {
    if (_parentResourceBundle == null) {
      return _getJavaxPortletString(key);
    }
 else {
      return _parentResourceBundle.getObject(key);
    }
  }
 catch (  MissingResourceException mre) {
    String value=_getJavaxPortletString(key);
    if (value != null) {
      return value;
    }
 else {
      throw mre;
    }
  }
}
