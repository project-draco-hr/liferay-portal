{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByGroupId_PrevAndNext(dataProviderInstanceId,groupId,orderByComparator);
  }
  DDMDataProviderInstance ddmDataProviderInstance=findByPrimaryKey(dataProviderInstanceId);
  Session session=null;
  try {
    session=openSession();
    DDMDataProviderInstance[] array=new DDMDataProviderInstanceImpl[3];
    array[0]=filterGetByGroupId_PrevAndNext(session,ddmDataProviderInstance,groupId,orderByComparator,true);
    array[1]=ddmDataProviderInstance;
    array[2]=filterGetByGroupId_PrevAndNext(session,ddmDataProviderInstance,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
