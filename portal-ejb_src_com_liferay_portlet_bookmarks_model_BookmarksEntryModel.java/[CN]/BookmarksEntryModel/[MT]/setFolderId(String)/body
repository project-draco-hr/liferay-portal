{
  if (((folderId == null) && (_folderId != null)) || ((folderId != null) && (_folderId == null)) || ((folderId != null) && (_folderId != null) && !folderId.equals(_folderId))) {
    if (!XSS_ALLOW_FOLDERID) {
      folderId=XSSUtil.strip(folderId);
    }
    _folderId=folderId;
    setModified(true);
  }
}
