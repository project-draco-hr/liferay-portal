{
  String cmd=ParamUtil.getString(actionRequest,Constants.CMD);
  try {
    if (cmd.equals(Constants.ADD) || cmd.equals(Constants.UPDATE)) {
      updateWebsite(actionRequest);
    }
 else     if (cmd.equals(Constants.DELETE)) {
      deleteWebsite(actionRequest);
    }
    sendRedirect(actionRequest,actionResponse);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchWebsiteException || e instanceof PrincipalException) {
      SessionErrors.add(actionRequest,e.getClass().getName());
      setForward(actionRequest,"portlet.enterprise_admin.error");
    }
 else     if (e instanceof NoSuchListTypeException || e instanceof WebsiteURLException) {
      SessionErrors.add(actionRequest,e.getClass().getName());
    }
 else {
      throw e;
    }
  }
}
