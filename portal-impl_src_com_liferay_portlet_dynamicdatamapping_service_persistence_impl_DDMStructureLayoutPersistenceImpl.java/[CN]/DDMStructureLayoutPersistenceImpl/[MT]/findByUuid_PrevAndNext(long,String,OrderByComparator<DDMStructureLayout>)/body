{
  DDMStructureLayout ddmStructureLayout=findByPrimaryKey(structureLayoutId);
  Session session=null;
  try {
    session=openSession();
    DDMStructureLayout[] array=new DDMStructureLayoutImpl[3];
    array[0]=getByUuid_PrevAndNext(session,ddmStructureLayout,uuid,orderByComparator,true);
    array[1]=ddmStructureLayout;
    array[2]=getByUuid_PrevAndNext(session,ddmStructureLayout,uuid,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
