{
  if (_executorService != null) {
    return _executorService;
  }
synchronized (ProcessUtil.class) {
    if (_executorService == null) {
      _executorService=Executors.newCachedThreadPool(new NamedThreadFactory(ProcessUtil.class.getName(),Thread.MIN_PRIORITY,PortalClassLoaderUtil.getClassLoader()));
    }
  }
  return _executorService;
}
