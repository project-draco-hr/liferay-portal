{
  virtualHost=StringUtil.lowerCase(virtualHost);
  Session session=null;
  try {
    session=HibernateUtil.openSession();
    String sql=CustomSQLUtil.get(FIND_BY_C_V);
    SQLQuery q=session.createSQLQuery(sql);
    q.setCacheable(true);
    q.addEntity("LayoutSet",LayoutSet.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(companyId);
    qPos.add(virtualHost);
    Iterator itr=q.list().iterator();
    if (itr.hasNext()) {
      return (LayoutSet)itr.next();
    }
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    HibernateUtil.closeSession(session);
  }
  throw new NoSuchLayoutSetException("No LayoutSet exists with the key {companyId=" + companyId + ", "+ "virtualHost="+ virtualHost+ "}");
}
