{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select portalPreferencesId, preferences from " + "PortalPreferences");
    rs=ps.executeQuery();
    while (rs.next()) {
      long portalPreferencesId=rs.getLong("portalPreferencesId");
      String preferences=rs.getString("preferences");
      upgradeUserStagingPreferences(portalPreferencesId,preferences);
    }
  }
  finally {
    DataAccess.cleanUp(null,ps,rs);
  }
}
