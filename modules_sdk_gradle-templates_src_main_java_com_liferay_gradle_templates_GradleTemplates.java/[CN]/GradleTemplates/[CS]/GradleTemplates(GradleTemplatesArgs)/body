{
  _checkArgs(gradleTemplatesArgs);
  Map<String,String> replacements=_getReplacements(gradleTemplatesArgs);
  File dir=new File(gradleTemplatesArgs.getDestinationDir(),gradleTemplatesArgs.getName());
  _extractDirectory(TEMPLATES_STANDALONE_DIR + "/" + gradleTemplatesArgs.getTemplate(),dir,replacements);
  if (gradleTemplatesArgs.getWorkspaceDir() != null) {
    _extractDirectory(TEMPLATES_WORKSPACE_DIR + "/" + gradleTemplatesArgs.getTemplate(),dir,replacements);
    File settingsGradleFile=new File(dir,"settings.gradle");
    settingsGradleFile.delete();
  }
}
