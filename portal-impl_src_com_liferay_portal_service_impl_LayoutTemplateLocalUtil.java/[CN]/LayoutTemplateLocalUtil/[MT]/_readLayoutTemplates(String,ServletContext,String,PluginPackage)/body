{
  Set<ObjectValuePair<String,Boolean>> layoutTemplateIds=new HashSet<ObjectValuePair<String,Boolean>>();
  if (xml == null) {
    return layoutTemplateIds;
  }
  Document doc=DocumentUtil.readDocumentFromXML(xml,true);
  Element root=doc.getRootElement();
  Element standardEl=root.element("standard");
  if (standardEl != null) {
    readLayoutTemplate(servletContextName,ctx,layoutTemplateIds,standardEl,true,null,pluginPackage);
  }
  Element customEl=root.element("custom");
  if (customEl != null) {
    readLayoutTemplate(servletContextName,ctx,layoutTemplateIds,customEl,false,null,pluginPackage);
  }
  return layoutTemplateIds;
}
