{
  List<ObjectValuePair> layoutTemplateIds=new ArrayList<ObjectValuePair>();
  try {
    for (int i=0; i < xmls.length; i++) {
      Iterator<ObjectValuePair> itr=_readLayoutTemplates(servletContextName,ctx,xmls[i],pluginPackage).iterator();
      while (itr.hasNext()) {
        ObjectValuePair ovp=(ObjectValuePair)itr.next();
        if (!layoutTemplateIds.contains(ovp)) {
          layoutTemplateIds.add(ovp);
        }
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return layoutTemplateIds;
}
