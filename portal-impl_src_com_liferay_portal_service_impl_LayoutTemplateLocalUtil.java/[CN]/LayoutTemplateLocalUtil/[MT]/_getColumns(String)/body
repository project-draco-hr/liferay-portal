{
  try {
    InitColumnProcessor processor=new InitColumnProcessor();
    VelocityContext context=new VelocityContext();
    context.put("processor",processor);
    Velocity.evaluate(context,new PrintWriter(new StringWriter()),LayoutTemplateLocalUtil.class.getName(),content);
    List<String> columns=processor.getColumns();
    Collections.sort(columns);
    return columns;
  }
 catch (  Exception e) {
    _log.error(e);
    return new ArrayList<String>();
  }
}
