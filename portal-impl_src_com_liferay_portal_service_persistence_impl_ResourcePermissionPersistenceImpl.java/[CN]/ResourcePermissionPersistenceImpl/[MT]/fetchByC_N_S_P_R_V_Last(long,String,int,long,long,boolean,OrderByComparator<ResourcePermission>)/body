{
  int count=countByC_N_S_P_R_V(companyId,name,scope,primKeyId,roleId,viewActionId);
  if (count == 0) {
    return null;
  }
  List<ResourcePermission> list=findByC_N_S_P_R_V(companyId,name,scope,primKeyId,roleId,viewActionId,count - 1,count,orderByComparator);
  if (!list.isEmpty()) {
    return list.get(0);
  }
  return null;
}
