{
  StopWatch stopWatch=StopWatchLoggingHelper.startLogging(_log,"VerifyPortletPreferences.cleanUpLayoutRevisionPortletPreferences");
  CacheRegistryUtil.setActive(true);
  try {
    cleanUpLayoutRevisionPortletPreferences();
  }
  finally {
    CacheRegistryUtil.setActive(false);
  }
  StopWatchLoggingHelper.endLogging(stopWatch,_log,"VerifyPortletPreferences.cleanUpLayoutRevisionPortletPreferences");
}
