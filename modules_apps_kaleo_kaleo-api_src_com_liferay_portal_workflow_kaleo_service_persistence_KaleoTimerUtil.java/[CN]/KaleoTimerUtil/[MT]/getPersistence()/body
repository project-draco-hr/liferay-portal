{
  if (_persistence == null) {
    _persistence=(KaleoTimerPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoTimerPersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoTimerUtil.class,"_persistence");
  }
  return _persistence;
}
