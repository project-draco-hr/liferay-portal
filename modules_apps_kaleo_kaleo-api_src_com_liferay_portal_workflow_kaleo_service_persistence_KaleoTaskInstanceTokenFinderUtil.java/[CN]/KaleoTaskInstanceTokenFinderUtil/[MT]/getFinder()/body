{
  if (_finder == null) {
    _finder=(KaleoTaskInstanceTokenFinder)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoTaskInstanceTokenFinder.class.getName());
    ReferenceRegistry.registerReference(KaleoTaskInstanceTokenFinderUtil.class,"_finder");
  }
  return _finder;
}
