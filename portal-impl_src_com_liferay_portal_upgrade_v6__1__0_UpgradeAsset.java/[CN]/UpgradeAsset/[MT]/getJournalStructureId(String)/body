{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select id_ from JournalStructure where structureId = ?");
    ps.setString(1,structureId);
    rs=ps.executeQuery();
    if (rs.next()) {
      return rs.getLong("id_");
    }
    return 0;
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
