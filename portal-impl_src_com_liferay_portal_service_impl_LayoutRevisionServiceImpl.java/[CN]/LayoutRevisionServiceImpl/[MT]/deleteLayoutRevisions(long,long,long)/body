{
  LayoutSetBranch layoutSetBranch=layoutSetBranchPersistence.findByPrimaryKey(layoutSetBranchId);
  GroupPermissionUtil.check(getPermissionChecker(),layoutSetBranch.getGroupId(),ActionKeys.DELETE_LAYOUT_BRANCH);
  layoutRevisionLocalService.deleteLayoutRevisions(layoutSetBranchId,layoutBranchId,plid);
}
