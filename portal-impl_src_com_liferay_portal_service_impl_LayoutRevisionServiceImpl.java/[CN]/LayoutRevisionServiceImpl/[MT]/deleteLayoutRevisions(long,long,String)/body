{
  LayoutSetBranch layoutSetBranch=layoutSetBranchPersistence.findByPrimaryKey(layoutSetBranchId);
  GroupPermissionUtil.check(getPermissionChecker(),layoutSetBranch.getGroupId(),ActionKeys.DELETE_LAYOUT_VARIATION);
  layoutRevisionLocalService.deleteLayoutRevisions(layoutSetBranchId,plid,variationName);
}
