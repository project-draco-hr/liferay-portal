{
  _notificationTemplateType=notificationTemplateType;
  if (_calendarNotificationTemplateRemoteModel != null) {
    try {
      Class<?> clazz=_calendarNotificationTemplateRemoteModel.getClass();
      Method method=clazz.getMethod("setNotificationTemplateType",String.class);
      method.invoke(_calendarNotificationTemplateRemoteModel,notificationTemplateType);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
