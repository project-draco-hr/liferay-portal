{
  super.init(filterConfig);
  _servletContext=filterConfig.getServletContext();
  _servletContextName=GetterUtil.getString(_servletContext.getServletContextName());
  if (Validator.isNull(_servletContextName)) {
    _tempDir+="/portal";
  }
  if (PropsValues.MINIFIER_FILES_LIMIT > 0) {
    _limitedFilesCache=new LimitedFilesCache<String>(PropsValues.MINIFIER_FILES_LIMIT);
    if (_log.isDebugEnabled()) {
      _log.debug("Minifier files limit " + PropsValues.MINIFIER_FILES_LIMIT);
    }
  }
}
