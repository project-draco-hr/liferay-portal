{
  try (LoggingTimer loggingTimer=new LoggingTimer()){
    List<BlogsEntry> entries=BlogsEntryLocalServiceUtil.getNoAssetEntries();
    if (_log.isDebugEnabled()) {
      _log.debug("Processing " + entries.size() + " entries with no asset");
    }
    for (    BlogsEntry entry : entries) {
      try {
        BlogsEntryLocalServiceUtil.updateAsset(entry.getUserId(),entry,null,null,null,null);
      }
 catch (      Exception e) {
        if (_log.isWarnEnabled()) {
          _log.warn("Unable to update asset for entry " + entry.getEntryId() + ": "+ e.getMessage());
        }
      }
    }
    if (_log.isDebugEnabled()) {
      _log.debug("Assets verified for entries");
    }
  }
 }
