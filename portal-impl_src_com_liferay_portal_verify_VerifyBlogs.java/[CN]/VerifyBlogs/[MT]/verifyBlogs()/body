{
  List<BlogsEntry> entries=BlogsEntryLocalServiceUtil.getNoAssetEntries();
  if (_log.isDebugEnabled()) {
    _log.debug("Processing " + entries.size() + " entries with no tags assets");
  }
  for (int i=0; i < entries.size(); i++) {
    BlogsEntry entry=(BlogsEntry)entries.get(i);
    try {
      BlogsEntryLocalServiceUtil.updateTagsAsset(entry.getUserId(),entry,new String[0]);
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to update tags asset for entry " + entry.getEntryId() + ": "+ e.getMessage());
      }
    }
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Tags assets verified for entries");
  }
}
