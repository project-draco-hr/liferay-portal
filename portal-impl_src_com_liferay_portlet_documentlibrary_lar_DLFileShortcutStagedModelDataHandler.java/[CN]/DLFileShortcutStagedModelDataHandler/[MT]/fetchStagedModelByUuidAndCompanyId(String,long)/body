{
  List<DLFileShortcut> fileShortcuts=DLFileShortcutLocalServiceUtil.getDLFileShortcutsByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<DLFileShortcut>());
  if (ListUtil.isEmpty(fileShortcuts)) {
    return null;
  }
  return fileShortcuts.get(0);
}
