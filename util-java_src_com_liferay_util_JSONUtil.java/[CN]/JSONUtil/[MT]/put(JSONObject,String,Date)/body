{
  if (value == null) {
    try {
      jsonObj.put(key,StringPool.BLANK);
    }
 catch (    JSONException jsone) {
      if (_log.isWarnEnabled()) {
        _log.warn(jsone,jsone);
      }
    }
  }
 else {
    long time=value.getTime() / Time.SECOND;
    put(jsonObj,key,String.valueOf(time));
  }
}
