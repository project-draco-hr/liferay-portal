{
  List<ExpandoValue> rowValues=_expandoValueLocalService.getRowValues(expandoRowId);
  Map<String,String> fieldsMap=new HashMap<>();
  for (  ExpandoValue expandoValue : rowValues) {
    try {
      fieldsMap.put(expandoValue.getColumn().getName(),expandoValue.getData());
    }
 catch (    PortalException e) {
      _log.error("Unable to add expando value data ",e);
      throw e;
    }
  }
  return fieldsMap;
}
