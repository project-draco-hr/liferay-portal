{
  try {
    Configuration configuration=_configurationAdmin.getConfiguration(SettingsDefinitionUtil.getConfigurationPid(_settingsDefinition));
    Dictionary<String,?> properties=configuration.getProperties();
    if (properties == null) {
      properties=_emptDictionary;
    }
    return properties;
  }
 catch (  IOException ioe) {
    throw new RuntimeException(ioe);
  }
}
