{
  if (_log.isDebugEnabled()) {
    _log.debug("Starting lifecycle of settings class " + SettingsDefinitionUtil.getSettingsClassName(_settingsDefinition));
  }
  _registerManagedService();
  _registerConfigurationBean();
  _settingsFactory.registerSettingsDefinition(_settingsDefinition,_configurationBeanBuilder.getConfigurationBean());
  _registerSettingsProvider();
  if (_log.isInfoEnabled()) {
    _log.info("Started lifecycle of settings class " + SettingsDefinitionUtil.getSettingsClassName(_settingsDefinition));
  }
}
