{
  Dictionary<String,String> properties=new HashMapDictionary<>();
  properties.put("class.name",SettingsDefinitionUtil.getSettingsClassName(_settingsDefinition));
  Class settingsProviderServiceClass=_settingsProviderBuilder.getSettingsProviderServiceClass();
  _settingsProviderServiceRegistration=_bundleContext.registerService(settingsProviderServiceClass,_settingsProviderBuilder.getSettingsProvider(),properties);
  if (_log.isDebugEnabled()) {
    String settingsClassName=SettingsDefinitionUtil.getSettingsClassName(_settingsDefinition);
    _log.debug("Registered settings provider service " + settingsProviderServiceClass.getName() + "<"+ settingsClassName+ ">");
  }
}
