{
  boolean addGroupPermissions=true;
  Group group=importedLayout.getGroup();
  if (privateLayout && group.isUser()) {
    addGroupPermissions=false;
  }
  boolean addGuestPermissions=false;
  if (!privateLayout || layout.isTypeControlPanel()) {
    addGuestPermissions=true;
  }
  _resourceLocalService.addResources(companyId,groupId,userId,Layout.class.getName(),importedLayout.getPlid(),false,addGroupPermissions,addGuestPermissions);
}
