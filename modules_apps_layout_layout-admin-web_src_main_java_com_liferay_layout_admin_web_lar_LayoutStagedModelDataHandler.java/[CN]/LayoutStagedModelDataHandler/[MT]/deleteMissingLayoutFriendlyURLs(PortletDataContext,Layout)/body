{
  Map<Long,Long> layoutFriendlyURLIds=(Map<Long,Long>)portletDataContext.getNewPrimaryKeysMap(LayoutFriendlyURL.class);
  List<LayoutFriendlyURL> layoutFriendlyURLs=_layoutFriendlyURLLocalService.getLayoutFriendlyURLs(layout.getPlid());
  for (  LayoutFriendlyURL layoutFriendlyURL : layoutFriendlyURLs) {
    if (!layoutFriendlyURLIds.containsValue(layoutFriendlyURL.getLayoutFriendlyURLId())) {
      _layoutFriendlyURLLocalService.deleteLayoutFriendlyURL(layoutFriendlyURL);
    }
  }
}
