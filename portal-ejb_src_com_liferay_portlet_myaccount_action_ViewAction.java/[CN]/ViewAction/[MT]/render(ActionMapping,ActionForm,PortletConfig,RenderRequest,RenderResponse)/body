{
  if (req.getRemoteUser() == null) {
    return mapping.findForward("portlet.my_account.view");
  }
 else {
    if (req.getWindowState().equals(WindowState.MAXIMIZED)) {
      return mapping.findForward("portlet.my_account.edit_user");
    }
 else {
      return mapping.findForward("portlet.my_account.view");
    }
  }
}
