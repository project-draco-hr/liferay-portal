{
  ServiceContext serviceContext=new ServiceContext();
  String[] groupPermissions=new String[]{"VIEW"};
  serviceContext.setAttribute("groupPermissions",groupPermissions);
  serviceContext.setGroupPermissions(groupPermissions);
  String jsonString=JSONFactoryUtil.serialize(serviceContext);
  ServiceContext serviceContextDeserialize=(ServiceContext)JSONFactoryUtil.deserialize(jsonString);
  Assert.assertTrue(serviceContextDeserialize.getGroupPermissions() != null);
}
