{
  HttpServletRequest request=(HttpServletRequest)pageContext.getRequest();
  request.setAttribute("liferay-ui:asset-categories-selector:className",_className);
  request.setAttribute("liferay-ui:asset-categories-selector:classPK",String.valueOf(_classPK));
  request.setAttribute("liferay-ui:asset-categories-selector:focus",String.valueOf(_focus));
  request.setAttribute("liferay-ui:asset-categories-selector:contentCallback",String.valueOf(_contentCallback));
  return EVAL_BODY_BUFFERED;
}
