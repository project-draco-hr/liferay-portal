{
  if (_shardDataSourceTargetSource == null) {
    _shardDataSourceTargetSource=(ShardDataSourceTargetSource)InfrastructureUtil.getShardDataSourceTargetSource();
  }
  if (_shardSessionFactoryTargetSource == null) {
    _shardSessionFactoryTargetSource=(ShardSessionFactoryTargetSource)InfrastructureUtil.getShardSessionFactoryTargetSource();
  }
}
