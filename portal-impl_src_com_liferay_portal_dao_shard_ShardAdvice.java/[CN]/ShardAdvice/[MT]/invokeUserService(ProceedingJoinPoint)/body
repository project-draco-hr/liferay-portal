{
  String shardName=PropsValues.SHARD_DEFAULT;
  String methodName=call.getSignature().getName();
  Object[] arguments=call.getArgs();
  if (methodName.equals("searchCount")) {
    Long companyId=(Long)arguments[0];
    shardName=ShardLocalServiceUtil.getShardNameByCompanyId(companyId);
  }
 else {
    return call.proceed();
  }
  Object retVal=null;
  pushCompanyService(shardName);
  try {
    retVal=call.proceed();
  }
  finally {
    popCompanyService();
  }
  return retVal;
}
