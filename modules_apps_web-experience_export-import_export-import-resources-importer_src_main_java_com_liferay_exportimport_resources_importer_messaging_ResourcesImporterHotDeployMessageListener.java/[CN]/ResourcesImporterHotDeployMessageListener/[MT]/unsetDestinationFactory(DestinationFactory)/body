{
  if (_serviceRegistration == null) {
    return;
  }
  Bundle bundle=FrameworkUtil.getBundle(ResourcesImporterHotDeployMessageListener.class);
  BundleContext bundleContext=bundle.getBundleContext();
  Destination destination=bundleContext.getService(_serviceRegistration.getReference());
  _serviceRegistration.unregister();
  destination.destroy();
}
