{
  _clientIP=clientIP;
  if (_analyticsEventRemoteModel != null) {
    try {
      Class<?> clazz=_analyticsEventRemoteModel.getClass();
      Method method=clazz.getMethod("setClientIP",String.class);
      method.invoke(_analyticsEventRemoteModel,clientIP);
