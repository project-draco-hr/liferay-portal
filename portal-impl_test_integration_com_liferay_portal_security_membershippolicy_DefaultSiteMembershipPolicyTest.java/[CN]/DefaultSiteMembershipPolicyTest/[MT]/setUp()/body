{
  _parentGroup=GroupTestUtil.addGroup();
  UserLocalServiceUtil.unsetGroupUsers(_parentGroup.getGroupId(),new long[]{TestPropsValues.getUserId()},null);
  _group=GroupTestUtil.addGroup(_parentGroup.getGroupId());
  UserLocalServiceUtil.unsetGroupUsers(_group.getGroupId(),new long[]{TestPropsValues.getUserId()},null);
}
