{
  if (_persistenceImpl.containsUser.contains(userGroupId,userId)) {
    ModelListener[] userListeners=userPersistence.getListeners();
    for (    ModelListener listener : listeners) {
      listener.onBeforeRemoveAssociation(userGroupId,com.liferay.portal.model.User.class.getName(),userId);
    }
    for (    ModelListener listener : userListeners) {
      listener.onBeforeRemoveAssociation(userId,UserGroup.class.getName(),userGroupId);
    }
    _sqlUpdate.update(new Object[]{new Long(userGroupId),new Long(userId)});
    for (    ModelListener listener : listeners) {
      listener.onAfterRemoveAssociation(userGroupId,com.liferay.portal.model.User.class.getName(),userId);
    }
    for (    ModelListener listener : userListeners) {
      listener.onAfterRemoveAssociation(userId,UserGroup.class.getName(),userGroupId);
    }
  }
}
