{
  EntityCacheUtil.removeResult(UserGroupModelImpl.ENTITY_CACHE_ENABLED,UserGroupImpl.class,userGroup.getPrimaryKey());
  FinderCacheUtil.removeResult(FINDER_PATH_FIND_ALL,FINDER_ARGS_EMPTY);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_N,new Object[]{Long.valueOf(userGroup.getCompanyId()),userGroup.getName()});
}
