{
  UserGroup userGroup=fetchByC_P_First(companyId,parentUserGroupId,orderByComparator);
  if (userGroup != null) {
    return userGroup;
  }
  StringBundler msg=new StringBundler(6);
  msg.append(_NO_SUCH_ENTITY_WITH_KEY);
