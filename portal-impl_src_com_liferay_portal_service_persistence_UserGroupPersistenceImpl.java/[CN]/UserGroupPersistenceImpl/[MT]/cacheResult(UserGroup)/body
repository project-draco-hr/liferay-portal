{
  EntityCacheUtil.putResult(UserGroupModelImpl.ENTITY_CACHE_ENABLED,UserGroupImpl.class,userGroup.getPrimaryKey(),userGroup);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_N,new Object[]{Long.valueOf(userGroup.getCompanyId()),userGroup.getName()},userGroup);
  userGroup.resetOriginalValues();
}
