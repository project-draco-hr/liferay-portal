{
  try {
    clearUsers.clear(userGroup.getPrimaryKey());
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    FinderCacheUtil.clearCache("Users_UserGroups");
  }
  Session session=null;
  try {
    session=openSession();
    if (BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(UserGroupImpl.class,userGroup.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(userGroup);
    session.flush();
    return userGroup;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(UserGroup.class.getName());
  }
}
