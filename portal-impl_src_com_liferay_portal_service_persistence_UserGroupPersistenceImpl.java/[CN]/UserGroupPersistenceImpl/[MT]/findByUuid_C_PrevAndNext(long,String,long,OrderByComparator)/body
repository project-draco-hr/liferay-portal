{
  UserGroup userGroup=findByPrimaryKey(userGroupId);
  Session session=null;
  try {
    session=openSession();
    UserGroup[] array=new UserGroupImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,userGroup,uuid,companyId,orderByComparator,true);
    array[1]=userGroup;
    array[2]=getByUuid_C_PrevAndNext(session,userGroup,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
