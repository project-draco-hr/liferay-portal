{
  Session session=null;
  try {
    session=openSession();
    session.delete(userGroup);
    session.flush();
    clearUsers.clear(userGroup.getPrimaryKey());
    return userGroup;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(UserGroup.class.getName());
  }
}
