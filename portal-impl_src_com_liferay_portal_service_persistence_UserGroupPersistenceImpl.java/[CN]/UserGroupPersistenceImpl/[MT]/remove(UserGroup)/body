{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(userGroup);
    }
  }
  userGroup=removeImpl(userGroup);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(userGroup);
    }
  }
  return userGroup;
}
