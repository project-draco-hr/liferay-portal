{
  UserGroup result=(UserGroup)EntityCacheUtil.getResult(UserGroupModelImpl.ENTITY_CACHE_ENABLED,UserGroupImpl.class,userGroupId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      UserGroup userGroup=(UserGroup)session.get(UserGroupImpl.class,new Long(userGroupId));
      if (userGroup != null) {
        cacheResult(userGroup);
      }
      return userGroup;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (UserGroup)result;
  }
}
