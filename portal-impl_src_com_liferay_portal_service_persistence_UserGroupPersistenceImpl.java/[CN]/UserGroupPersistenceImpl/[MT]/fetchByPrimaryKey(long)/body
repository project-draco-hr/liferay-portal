{
  UserGroup userGroup=(UserGroup)EntityCacheUtil.getResult(UserGroupModelImpl.ENTITY_CACHE_ENABLED,UserGroupImpl.class,userGroupId,this);
  if (userGroup == null) {
    Session session=null;
    try {
      session=openSession();
      userGroup=(UserGroup)session.get(UserGroupImpl.class,Long.valueOf(userGroupId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (userGroup != null) {
        cacheResult(userGroup);
      }
      closeSession(session);
    }
  }
  return userGroup;
}
