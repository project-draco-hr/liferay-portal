{
  ServiceWrapper<DDMStructurePermissionSupport> structurePermissionSupportServiceWrapper=_ddmPermissionSupportTracker.getDDMStructurePermissionSupportServiceWrapper(classNameId);
  return permissionChecker.hasPermission(groupId,getResourceName(structurePermissionSupportServiceWrapper),groupId,getAddStructureActionId(structurePermissionSupportServiceWrapper));
}
