{
  this.jsw=jsw;
  this.ctxt=ctxt;
  this.jspcMode=jspcMode;
  this.options=ctxt.getOptions();
  this.log=Logger.getLogger(Compiler.class.getName());
  if (jspcMode) {
    log.setLevel(Level.OFF);
  }
  this.smapUtil=new SmapUtil(ctxt);
  this.errDispatcher=new ErrorDispatcher(jspcMode);
  initJavaCompiler();
}
