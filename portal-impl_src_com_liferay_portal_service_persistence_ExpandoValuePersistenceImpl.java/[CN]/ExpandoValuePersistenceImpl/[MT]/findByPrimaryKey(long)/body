{
  ExpandoValue expandoValue=fetchByPrimaryKey(valueId);
  if (expandoValue == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ExpandoValue exists with the primary key " + valueId);
    }
    throw new NoSuchExpandoValueException("No ExpandoValue exists with the primary key " + valueId);
  }
  return expandoValue;
}
