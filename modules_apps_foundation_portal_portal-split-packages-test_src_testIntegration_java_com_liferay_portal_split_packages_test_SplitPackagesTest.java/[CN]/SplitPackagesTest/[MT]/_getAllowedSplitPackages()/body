{
  Map<ExportPackage,Set<String>> allowedSplitPackages=new HashMap<>();
  for (  String line : StringUtil.splitLines(StringUtil.read(SplitPackagesTest.class.getResourceAsStream("dependencies/allowed_split_packages.txt")))) {
    String[] lineParts=StringUtil.split(line,StringPool.SEMICOLON);
    allowedSplitPackages.put(new ExportPackage(lineParts[0],lineParts[1]),SetUtil.fromArray(StringUtil.split(lineParts[2])));
  }
  return allowedSplitPackages;
}
