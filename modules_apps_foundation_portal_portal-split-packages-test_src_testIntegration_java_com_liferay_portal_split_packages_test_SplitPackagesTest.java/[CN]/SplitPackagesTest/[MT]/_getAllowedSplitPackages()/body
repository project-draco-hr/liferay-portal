{
  Map<ExportPackage,Set<String>> allowedSplitPackages=new HashMap<>();
  for (  String splitPackagesLine : StringUtil.splitLines(StringUtil.read(SplitPackagesTest.class.getResourceAsStream("allowed_split_packages.txt")))) {
    String[] splitPackagesParts=StringUtil.split(splitPackagesLine,StringPool.SEMICOLON);
    allowedSplitPackages.put(new ExportPackage(splitPackagesParts[0],splitPackagesParts[1]),SetUtil.fromArray(StringUtil.split(splitPackagesParts[2])));
  }
  return allowedSplitPackages;
}
