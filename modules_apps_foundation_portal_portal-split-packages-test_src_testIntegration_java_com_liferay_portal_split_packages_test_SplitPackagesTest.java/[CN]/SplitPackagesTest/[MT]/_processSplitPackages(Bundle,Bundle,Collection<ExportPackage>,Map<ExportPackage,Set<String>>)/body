{
  for (  ExportPackage duplicateExportPackage : duplicateExportPackages) {
    boolean hasSplitPackages=false;
    Set<String> symbolicNames=allowedSplitPackageNames.get(duplicateExportPackage);
    if ((symbolicNames == null) || !symbolicNames.contains(currentBundle.getSymbolicName()) || !symbolicNames.contains(previousBundle.getSymbolicName())) {
      hasSplitPackages=true;
    }
    Assert.assertFalse("Detected split packages " + duplicateExportPackage + " in "+ previousBundle+ " and "+ currentBundle,hasSplitPackages);
  }
}
