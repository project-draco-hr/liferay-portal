{
  JSONArray queueItems=getQueueItemsJSONArray();
  for (int i=0; i < queueItems.length(); i++) {
    JSONObject queueItem=queueItems.getJSONObject(i);
    String queueItemName=queueItem.getJSONObject("task").getString("name");
    Map<String,String> buildParameters=getParameters(queueItem);
    if (queueItemName.equals(jobName) && buildParameters.equals(parameters)) {
      return queueItem;
    }
  }
  return null;
}
