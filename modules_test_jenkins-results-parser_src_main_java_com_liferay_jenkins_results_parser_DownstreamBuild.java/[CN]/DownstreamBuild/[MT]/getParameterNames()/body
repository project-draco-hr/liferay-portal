{
  Set<String> parameterNames=new HashSet<>();
  JSONObject jsonObject=JenkinsResultsParserUtil.toJSONObject("http://" + master + "/build/"+ jobName+ "/api/json?tree=actions[parameterDefinitions"+ "[name,type,value]]");
  JSONArray parameterDefinitions=jsonObject.getJSONArray("actions").getJSONObject(0).getJSONArray("parameterDefinitions");
  for (int i=0; i < parameterDefinitions.length(); i++) {
    JSONObject parameterDefinition=parameterDefinitions.getJSONObject(i);
    if (parameterDefinition.getString("type").equals("StringParameterDefinition")) {
      parameterNames.add(parameterDefinition.getString("name"));
    }
  }
  return parameterNames;
}
