{
  try {
    GoogleDocsMetadataHelper googleDocsMetadataHelper=new GoogleDocsMetadataHelper(fileVersion);
    if (googleDocsMetadataHelper.isGoogleDocs()) {
      return new GoogleDocsDLFileVersionDisplayContext(parentDLFileEntryActionsDisplayContext,request,response,fileVersion);
    }
  }
 catch (  PortalException pe) {
    throw new SystemException(pe);
  }
  return parentDLFileEntryActionsDisplayContext;
}
