{
  User user=(User)obj;
  try {
    PermissionChecker permissionChecker=PermissionThreadLocal.getPermissionChecker();
    if ((isChecked(user) && SiteMembershipPolicyUtil.isMembershipRequired(user.getUserId(),_group.getGroupId())) || (!isChecked(user) && !SiteMembershipPolicyUtil.isMembershipAllowed(user.getUserId(),_group.getGroupId())) || SiteMembershipPolicyUtil.isMembershipProtected(permissionChecker,user.getUserId(),_group.getGroupId())) {
      return true;
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
 finally {
    return super.isDisabled(obj);
  }
}
