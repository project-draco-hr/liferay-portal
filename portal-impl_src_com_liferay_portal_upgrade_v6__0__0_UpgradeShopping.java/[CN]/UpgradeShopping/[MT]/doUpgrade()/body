{
  StringBundler sb=new StringBundler(3);
  sb.append("update ShoppingItem set groupId = (select groupId from ");
  sb.append("ShoppingCategory where ShoppingCategory.categoryId = ");
  sb.append("ShoppingItem.categoryId)");
  runSQL(sb.toString());
  alterColumnType(ShoppingItemTable.class,new String[]{"smallImageURL","STRING null"},new String[]{"mediumImageURL","STRING null"},new String[]{"largeImageURL","STRING null"});
}
