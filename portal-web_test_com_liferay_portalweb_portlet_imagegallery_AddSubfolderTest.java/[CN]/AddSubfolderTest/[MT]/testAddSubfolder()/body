{
  selenium.click("//b");
  selenium.waitForPageToLoad("30000");
  selenium.click("//input[@value='Add Subfolder']");
  selenium.waitForPageToLoad("30000");
  selenium.type("_31_name","Subfolder");
  selenium.typeKeys("_31_description","This is a subfolder!");
  selenium.click("//input[@value='Save']");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//b")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
