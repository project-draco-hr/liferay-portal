{
  CacheExtension cacheExtension=GradleUtil.getExtension(updateVersionTask.getProject(),CacheExtension.class);
  DomainObjectCollection<TaskCache> taskCaches=cacheExtension.getTasks();
  taskCaches.all(new Action<TaskCache>(){
    @Override public void execute(    TaskCache taskCache){
      updateVersionTask.finalizedBy(taskCache.getRefreshDigestTaskName());
    }
  }
);
}
