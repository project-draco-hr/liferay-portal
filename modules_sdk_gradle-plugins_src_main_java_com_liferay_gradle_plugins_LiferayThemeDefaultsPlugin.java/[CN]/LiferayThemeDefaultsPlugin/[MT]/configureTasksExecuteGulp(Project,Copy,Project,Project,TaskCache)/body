{
  TaskContainer taskContainer=project.getTasks();
  taskContainer.withType(ExecuteGulpTask.class,new Action<ExecuteGulpTask>(){
    @Override public void execute(    ExecuteGulpTask executeGulpTask){
      TaskCache executeGulpTaskCache=null;
      for (      TaskCache taskCache : taskCaches) {
        if (executeGulpTask.equals(taskCache.getTask())) {
          executeGulpTaskCache=taskCache;
          break;
        }
      }
      configureTaskExecuteGulp(executeGulpTask,expandFrontendCSSCommonTask,frontendThemeStyledProject,frontendThemeUnstyledProject,executeGulpTaskCache);
    }
  }
);
}
