{
  ReplaceRegexTask replaceRegexTask=GradleUtil.addTask(project,UPDATE_THEME_VERSION_TASK_NAME,ReplaceRegexTask.class);
  replaceRegexTask.match("\\n\\t\"version\": \"(.+)\"","package.json");
  replaceRegexTask.setDescription("Updates the project version in the package.json file.");
  replaceRegexTask.setReplacement(IncrementVersionClosure.MICRO_INCREMENT);
  return replaceRegexTask;
}
