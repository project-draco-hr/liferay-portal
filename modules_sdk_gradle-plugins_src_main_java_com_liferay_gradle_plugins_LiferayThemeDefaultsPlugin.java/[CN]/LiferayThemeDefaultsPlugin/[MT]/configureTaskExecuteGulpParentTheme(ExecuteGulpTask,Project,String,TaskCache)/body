{
  if (themeProject == null) {
    if (_logger.isWarnEnabled()) {
      _logger.warn("Unable to configure " + name + " parent theme");
    }
    return;
  }
  File dir=themeProject.file("src/main/resources/META-INF/resources/_" + name);
  executeGulpTask.args("--" + name + "-path="+ FileUtil.getAbsolutePath(dir));
  addTaskSkippedDependency(executeGulpTask,taskCache,themeProject.getPath() + ":" + JavaPlugin.CLASSES_TASK_NAME);
  if (taskCache != null) {
    taskCache.testFile(dir);
  }
}
