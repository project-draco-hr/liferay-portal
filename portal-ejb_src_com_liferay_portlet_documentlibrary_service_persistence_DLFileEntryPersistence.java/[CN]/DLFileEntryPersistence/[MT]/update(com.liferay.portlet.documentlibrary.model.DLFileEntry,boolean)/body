{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(dlFileEntry);
    }
 else {
      if (dlFileEntry.isNew()) {
        session.save(dlFileEntry);
      }
    }
    session.flush();
    dlFileEntry.setNew(false);
    return dlFileEntry;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
