{
  DLFileEntry dlFileEntry=fetchByPrimaryKey(dlFileEntryPK);
  if (dlFileEntry == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No DLFileEntry exists with the primary key " + dlFileEntryPK.toString());
    }
    throw new NoSuchFileEntryException("No DLFileEntry exists with the primary key " + dlFileEntryPK.toString());
  }
  return dlFileEntry;
}
