{
  int[] statuses=new int[]{WorkflowConstants.STATUS_APPROVED,WorkflowConstants.STATUS_PENDING,WorkflowConstants.STATUS_SCHEDULED};
  ThemeDisplay themeDisplay=(ThemeDisplay)_request.getAttribute(WebKeys.THEME_DISPLAY);
  return AssetEntryLocalServiceUtil.searchCount(tag.getCompanyId(),null,themeDisplay.getUserId(),null,0,null,null,null,null,tag.getName(),true,true,statuses,false);
}
