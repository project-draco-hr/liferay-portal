{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(KBArticle.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("kbArticleId",RandomTestUtil.nextLong()));
  List<KBArticle> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
