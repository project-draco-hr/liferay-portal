{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(KBArticle.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("kbArticleId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("kbArticleId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
