{
  TaskContainer taskContainer=project.getTasks();
  taskContainer.withType(Test.class,new Action<Test>(){
    @Override public void execute(    Test test){
      WhipExtension whipExtension=GradleUtil.getExtension(test.getProject(),WhipExtension.class);
      whipExtension.applyTo(test);
    }
  }
);
}
