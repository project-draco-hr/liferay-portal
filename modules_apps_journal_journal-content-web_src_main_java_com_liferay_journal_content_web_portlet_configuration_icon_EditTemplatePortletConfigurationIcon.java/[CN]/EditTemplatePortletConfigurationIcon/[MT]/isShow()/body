{
  try {
    if (!_journalContentDisplayContext.isShowEditTemplateIcon()) {
      return false;
    }
    DDMTemplate ddmTemplate=_journalContentDisplayContext.getDDMTemplate();
    if ((ddmTemplate != null) && !ddmTemplate.isNew()) {
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
