{
  if (intervalJob == null) {
    return;
  }
  try {
    if (_scheduler.isShutdown()) {
      return;
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  try {
    String jobName=intervalJob.getClass().getName() + StringPool.AT + intervalJob.hashCode();
    _scheduler.unscheduleJob(jobName,Scheduler.DEFAULT_GROUP);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
