{
  DDMStructureLayout structureLayout=ddmStructureLayoutPersistence.findByPrimaryKey(structureLayoutId);
  structureLayout.setModifiedDate(serviceContext.getModifiedDate(null));
  structureLayout.setDefinition(DDMFormLayoutJSONSerializerUtil.serialize(ddmFormLayout));
  return ddmStructureLayoutPersistence.update(structureLayout);
}
