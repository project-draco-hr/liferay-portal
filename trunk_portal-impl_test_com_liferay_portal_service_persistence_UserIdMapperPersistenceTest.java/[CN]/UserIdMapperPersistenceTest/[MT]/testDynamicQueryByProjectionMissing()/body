{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(UserIdMapper.class,UserIdMapper.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("userIdMapperId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("userIdMapperId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
