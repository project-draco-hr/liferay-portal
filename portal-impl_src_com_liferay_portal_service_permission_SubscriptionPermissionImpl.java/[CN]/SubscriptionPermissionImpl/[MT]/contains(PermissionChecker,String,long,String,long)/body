{
  if (subscriptionClassName == null) {
    return false;
  }
  MBThread discussionThread=null;
  try {
    MBDiscussion discussion=MBDiscussionLocalServiceUtil.getDiscussion(subscriptionClassName,subscriptionClassPK);
    discussionThread=MBThreadLocalServiceUtil.fetchThread(discussion.getThreadId());
  }
 catch (  NoSuchDiscussionException nsde) {
  }
  if (Validator.isNotNull(inferredClassName)) {
    Boolean hasPermission=hasPermission(permissionChecker,inferredClassName,inferredClassPK,ActionKeys.VIEW,discussionThread);
    if ((hasPermission == null) || !hasPermission) {
      return false;
    }
  }
  Boolean hasPermission=hasPermission(permissionChecker,subscriptionClassName,subscriptionClassPK,ActionKeys.SUBSCRIBE,discussionThread);
  if (hasPermission != null) {
    return hasPermission;
  }
  return true;
}
