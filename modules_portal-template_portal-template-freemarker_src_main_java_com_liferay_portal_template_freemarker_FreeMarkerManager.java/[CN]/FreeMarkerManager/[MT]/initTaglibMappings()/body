{
  Enumeration<URL> enumeration=_bundle.findEntries("/","*taglib-mapping.properties",false);
  if (enumeration == null) {
    return;
  }
  while (enumeration.hasMoreElements()) {
    URL url=enumeration.nextElement();
    try (InputStream inputStream=url.openStream()){
      Properties properties=PropertiesUtil.load(inputStream,StringPool.UTF8);
      _taglibMappings.putAll(PropertiesUtil.toMap(properties));
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
}
