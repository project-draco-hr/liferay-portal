{
  DDMFormValues ddmFormValues=new DDMFormValues(null);
  ddmFormValues.addAvailableLocale(LocaleUtil.US);
  ddmFormValues.setDefaultLocale(LocaleUtil.US);
  ddmFormValues.addDDMFormFieldValue(createDDMFormFieldValue("baga","Text1","Text 1 Value"));
  ddmFormValues.addDDMFormFieldValue(createDDMFormFieldValue("hagt","Text2","Text 2 Value"));
  return ddmFormValues;
}
