{
  Map<String,Fields> fieldsMap=new HashMap<>();
  Fields fields=new Fields();
  fields.put(createField(ddmStructure.getStructureId(),"Text1","Text 1 Value"));
  fields.put(createField(ddmStructure.getStructureId(),"Text2","Text 2 Value"));
  fields.put(new Field(ddmStructure.getStructureId(),DDMImpl.FIELDS_DISPLAY_NAME,"Text1_INSTANCE_baga,Text2_INSTANCE_hagt"));
  fieldsMap.put(ddmStructure.getStructureKey(),fields);
  return fieldsMap;
}
