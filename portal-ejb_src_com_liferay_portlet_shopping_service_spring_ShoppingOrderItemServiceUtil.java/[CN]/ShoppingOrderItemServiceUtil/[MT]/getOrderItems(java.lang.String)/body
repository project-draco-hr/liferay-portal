{
  try {
    ShoppingOrderItemService shoppingOrderItemService=ShoppingOrderItemServiceFactory.getService();
    return shoppingOrderItemService.getOrderItems(orderId);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
