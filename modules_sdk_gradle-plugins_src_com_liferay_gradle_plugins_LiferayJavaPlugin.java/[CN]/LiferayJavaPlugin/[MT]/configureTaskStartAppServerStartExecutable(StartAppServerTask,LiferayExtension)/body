{
  if (Validator.isNotNull(startAppServerTask.getAppServerStartExecutable()) || Validator.isNull(startAppServerTask.getAppServerType())) {
    return;
  }
  AppServer appServer=liferayExtension.getAppServer(startAppServerTask.getAppServerType());
  startAppServerTask.setAppServerStartExecutable(appServer.getStartExecutable());
}
