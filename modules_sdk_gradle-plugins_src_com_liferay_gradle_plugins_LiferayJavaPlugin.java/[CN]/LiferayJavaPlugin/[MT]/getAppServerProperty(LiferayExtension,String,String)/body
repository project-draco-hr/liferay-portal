{
  ConfigObject appServers=liferayExtension.getAppServers();
  Map<String,String> appServerProperties=(Map<String,String>)appServers.getProperty(appServerType);
  String value=appServerProperties.get(key);
  if (Validator.isNull(value)) {
    throw new GradleException("Unable to get property " + key + " for "+ appServerType);
  }
  return value;
}
