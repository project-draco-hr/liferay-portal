{
  boolean addDefaultDependencies=getProperty(project,ADD_DEFAULT_DEPENDENCIES_PROPERTY_NAME,true);
  if (!addDefaultDependencies) {
    return;
  }
  for (  String dependencyNotation : COMPILE_DEPENDENCY_NOTATIONS) {
    GradleUtil.addDependency(project,JavaPlugin.COMPILE_CONFIGURATION_NAME,dependencyNotation);
  }
}
