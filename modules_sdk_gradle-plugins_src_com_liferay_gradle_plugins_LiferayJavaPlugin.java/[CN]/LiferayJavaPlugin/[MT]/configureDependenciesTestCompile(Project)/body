{
  boolean addTestDefaultDependencies=getProperty(project,ADD_TEST_DEFAULT_DEPENDENCIES_PROPERTY_NAME,true);
  if (!addTestDefaultDependencies) {
    return;
  }
  for (  String dependencyNotation : _TEST_COMPILE_DEPENDENCY_NOTATIONS) {
    GradleUtil.addDependency(project,JavaPlugin.TEST_COMPILE_CONFIGURATION_NAME,dependencyNotation);
  }
}
