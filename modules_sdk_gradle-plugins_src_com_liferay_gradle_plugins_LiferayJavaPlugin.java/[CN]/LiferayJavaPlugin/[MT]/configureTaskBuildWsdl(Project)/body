{
  BuildWsdlTask buildWsdlTask=(BuildWsdlTask)GradleUtil.getTask(project,BUILD_WSDL_TASK_NAME);
  configureTaskBuildWsdlDestinationDir(buildWsdlTask);
  configureTaskBuildWsdlRootDirs(buildWsdlTask);
  buildWsdlTask.addTasks();
  TaskOutputs taskOutputs=buildWsdlTask.getOutputs();
  GradleUtil.addDependency(buildWsdlTask.getProject(),JavaPlugin.COMPILE_CONFIGURATION_NAME,taskOutputs.getFiles());
}
