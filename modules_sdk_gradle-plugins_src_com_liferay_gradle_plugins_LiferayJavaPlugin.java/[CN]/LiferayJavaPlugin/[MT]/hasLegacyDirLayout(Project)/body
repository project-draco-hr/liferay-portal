{
  File dir=project.file("src/" + SourceSet.MAIN_SOURCE_SET_NAME);
  if (dir.exists()) {
    return false;
  }
  dir=project.file("src/" + SourceSet.TEST_SOURCE_SET_NAME);
  if (dir.exists()) {
    return false;
  }
  dir=project.file("src/" + TEST_INTEGRATION_SOURCE_SET_NAME);
  if (dir.exists()) {
    return false;
  }
  return true;
}
