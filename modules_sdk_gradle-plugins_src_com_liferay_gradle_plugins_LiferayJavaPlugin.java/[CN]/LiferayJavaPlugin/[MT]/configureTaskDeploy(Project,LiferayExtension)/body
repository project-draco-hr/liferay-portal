{
  final Copy copy=(Copy)GradleUtil.getTask(project,DEPLOY_TASK_NAME);
  copy.into(project.getProjectDir());
  copy.into(project.relativePath(liferayExtension.getDeployDir()),new Closure<Void>(null){
    @SuppressWarnings("unused") public void doCall(    CopySpec copySpec){
      configureTaskDeployFrom(copy,copySpec);
    }
  }
);
}
