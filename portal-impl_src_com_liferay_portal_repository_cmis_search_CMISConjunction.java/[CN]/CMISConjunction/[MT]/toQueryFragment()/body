{
  StringBundler sb=new StringBundler();
  if (!isEmpty()) {
    boolean first=true;
    for (    CMISCriterion criterion : getCriterions()) {
      if (!first) {
        sb.append(" AND ");
      }
      sb.append(criterion.toQueryFragment());
      first=false;
    }
  }
  return sb.toString();
}
