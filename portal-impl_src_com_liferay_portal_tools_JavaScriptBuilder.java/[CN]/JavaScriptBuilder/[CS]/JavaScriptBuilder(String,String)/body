{
  try {
    StringMaker sm=new StringMaker();
    String[] files=PropsUtil.getArray(PropsUtil.JAVASCRIPT_FILES);
    for (int i=0; i < files.length; i++) {
      String content=FileUtil.read(jsDir + files[i]);
      sm.append(content);
      sm.append("\n");
    }
    FileUtil.write(mergedFile,sm.toString());
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
