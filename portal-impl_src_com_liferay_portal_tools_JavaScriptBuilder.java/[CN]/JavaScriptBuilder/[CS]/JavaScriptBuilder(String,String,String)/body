{
  try {
    StringBuilder sb=new StringBuilder();
    String[] files=PropsUtil.getArray(jsProperty);
    for (int i=0; i < files.length; i++) {
      String content=FileUtil.read(jsDir + files[i]);
      sb.append(content);
      sb.append("\n");
    }
    FileUtil.write(mergedFile,sb.toString());
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
