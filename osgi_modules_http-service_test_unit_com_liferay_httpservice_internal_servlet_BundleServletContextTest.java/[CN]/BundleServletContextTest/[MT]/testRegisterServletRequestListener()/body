{
  List<ServletRequestListener> servletRequestListeners=_bundleServletContext.getServletRequestListeners();
  int oldSize=servletRequestListeners.size();
  registerListener(_servletRequestListener);
  servletRequestListeners=_bundleServletContext.getServletRequestListeners();
  int currentSize=servletRequestListeners.size();
  Assert.assertEquals(oldSize + 1,currentSize);
}
