{
  Dictionary<String,Object> properties=new HashMapDictionary<>();
  Class<?> clazz=backgroundTaskExecutor.getClass();
  properties.put(BackgroundTaskExecutorRegistry.BACKGROUND_TASK_EXECUTOR_REGISTRY_KEY,clazz.getName());
  ServiceRegistration<BackgroundTaskExecutor> serviceRegistration=bundleContext.registerService(BackgroundTaskExecutor.class,backgroundTaskExecutor,properties);
  _serviceRegistrations.add(serviceRegistration);
}
