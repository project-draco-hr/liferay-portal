{
  WorkflowLink newWorkflowLink=addWorkflowLink();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(WorkflowLink.class,WorkflowLink.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("workflowLinkId",newWorkflowLink.getWorkflowLinkId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  WorkflowLink existingWorkflowLink=(WorkflowLink)result.get(0);
  assertEquals(existingWorkflowLink,newWorkflowLink);
}
