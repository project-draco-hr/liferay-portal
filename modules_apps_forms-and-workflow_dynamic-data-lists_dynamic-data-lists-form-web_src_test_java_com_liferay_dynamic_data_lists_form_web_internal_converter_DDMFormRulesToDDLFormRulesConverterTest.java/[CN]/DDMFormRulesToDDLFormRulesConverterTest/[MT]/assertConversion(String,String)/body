{
  String serializedDDMFormRules=read(fromFileName);
  DDMFormRule[] ddmFormRules=deserialize(serializedDDMFormRules,DDMFormRule[].class);
  List<DDLFormRule> actualDDLFormRules=_ddmFormRulesToDDLFormRulesConverter.convert(ListUtil.toList(ddmFormRules));
  JSONAssert.assertEquals(read(toFileName),serialize(actualDDLFormRules),false);
}
