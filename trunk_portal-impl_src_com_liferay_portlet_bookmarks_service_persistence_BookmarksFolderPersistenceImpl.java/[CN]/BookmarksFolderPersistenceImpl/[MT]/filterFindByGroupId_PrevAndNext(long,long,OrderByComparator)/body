{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByGroupId_PrevAndNext(folderId,groupId,orderByComparator);
  }
  BookmarksFolder bookmarksFolder=findByPrimaryKey(folderId);
  Session session=null;
  try {
    session=openSession();
    BookmarksFolder[] array=new BookmarksFolderImpl[3];
    array[0]=filterGetByGroupId_PrevAndNext(session,bookmarksFolder,groupId,orderByComparator,true);
    array[1]=bookmarksFolder;
    array[2]=filterGetByGroupId_PrevAndNext(session,bookmarksFolder,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
