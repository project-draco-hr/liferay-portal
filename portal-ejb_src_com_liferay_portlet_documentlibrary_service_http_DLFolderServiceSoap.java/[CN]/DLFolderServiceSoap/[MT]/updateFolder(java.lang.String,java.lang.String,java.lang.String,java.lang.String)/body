{
  try {
    com.liferay.portlet.documentlibrary.model.DLFolder returnValue=DLFolderServiceUtil.updateFolder(folderId,parentFolderId,name,description);
    return com.liferay.portlet.documentlibrary.model.DLFolderSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    String stackTrace=StackTraceUtil.getStackTrace(e);
    _log.error(stackTrace);
    throw new RemoteException(stackTrace);
