{
  Group group=GroupTestUtil.addGroup();
  Folder parentFolder=DLAppTestUtil.addFolder(group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,"parentFolder");
  FileEntry fileEntry=DLAppTestUtil.addFileEntry(group.getGroupId(),parentFolder.getFolderId(),false,"fileName");
  DLAppServiceUtil.moveFileEntryToTrash(fileEntry.getFileEntryId());
  DLFolderLocalServiceUtil.deleteFolder(parentFolder.getFolderId(),false);
  doVerify();
}
