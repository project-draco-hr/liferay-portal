{
  Folder parentFolder=DLAppTestUtil.addFolder(_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID);
  FileEntry fileEntry=DLAppTestUtil.addFileEntry(_group.getGroupId(),parentFolder.getFolderId(),RandomTestUtil.randomString());
  DLAppServiceUtil.moveFileEntryToTrash(fileEntry.getFileEntryId());
  DLFolderLocalServiceUtil.deleteFolder(parentFolder.getFolderId(),false);
  doVerify();
}
