{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  MBThread newMBThread=addMBThread();
  _persistence.clearCache();
  MBThreadModelImpl existingMBThreadModelImpl=(MBThreadModelImpl)_persistence.findByPrimaryKey(newMBThread.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingMBThreadModelImpl.getUuid(),existingMBThreadModelImpl.getOriginalUuid()));
  Assert.assertEquals(existingMBThreadModelImpl.getGroupId(),existingMBThreadModelImpl.getOriginalGroupId());
  Assert.assertEquals(existingMBThreadModelImpl.getRootMessageId(),existingMBThreadModelImpl.getOriginalRootMessageId());
}
