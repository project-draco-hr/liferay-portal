{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBThread.class,MBThread.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("threadId",ServiceTestUtil.nextLong()));
  List<MBThread> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
