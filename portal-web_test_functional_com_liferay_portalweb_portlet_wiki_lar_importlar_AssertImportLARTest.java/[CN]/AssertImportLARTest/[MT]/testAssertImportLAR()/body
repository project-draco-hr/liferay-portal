{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Wiki Test Page");
  selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace("Wiki Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("//ul[@class='top-links-navigation']/li[contains(.,'All Pages')]/span/a"));
  selenium.clickAt("//ul[@class='top-links-navigation']/li[contains(.,'All Pages')]/span/a",RuntimeVariables.replace("All Pages"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=Test Wiki Article"));
  selenium.clickAt("link=Test Wiki Article",RuntimeVariables.replace("Test Wiki Article"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Test Wiki Article"),selenium.getText("xPath=(//div[@class='portlet-body']/div[2]/h1/span)"));
  assertEquals(RuntimeVariables.replace("this is italics"),selenium.getText("xPath=(//div[@class='wiki-body']/p/em)"));
  assertEquals(RuntimeVariables.replace("bold"),selenium.getText("xPath=(//div[@class='wiki-body']/p/strong)"));
  assertEquals(RuntimeVariables.replace("Link to Website"),selenium.getText("xPath=(//div[@class='wiki-body']/p/a)"));
  assertTrue(selenium.isPartialText("xPath=(//div[@class='wiki-body']/ul/li)","this is a list item"));
  assertTrue(selenium.isPartialText("xPath=(//div[@class='wiki-body']/ul/li/ul)","this is a sub list item"));
  assertEquals(RuntimeVariables.replace("Test"),selenium.getText("xPath=(//div[@class='child-pages']/ul/li/a)"));
  selenium.clickAt("//div[@class='child-pages']/ul/li/a",RuntimeVariables.replace("Test"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Test Child Article\\\\ #"),selenium.getText("xPath=(//div[@class='wiki-body']/h2)"));
  assertEquals(RuntimeVariables.replace("this is italics"),selenium.getText("xPath=(//div[@class='wiki-body']/p/em)"));
  assertEquals(RuntimeVariables.replace("bold"),selenium.getText("xPath=(//div[@class='wiki-body']/p/strong)"));
  assertEquals(RuntimeVariables.replace("Link to website"),selenium.getText("xPath=(//div[@class='wiki-body']/p/a)"));
  assertTrue(selenium.isPartialText("xPath=(//div[@class='wiki-body']/ul/li)","this is a list item"));
  assertTrue(selenium.isPartialText("xPath=(//div[@class='wiki-body']/ul/li/ul/li)","this is a sub list item"));
  selenium.clickAt("link=Second Edited Wiki Test",RuntimeVariables.replace("Second Edited Wiki Test"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Yes this is a second test article\\\\ #"),selenium.getText("//div[@class='wiki-body']/h4"));
  assertEquals(RuntimeVariables.replace("I love liferay! This wiki has been EDITED!"),selenium.getText("//div[@class='wiki-body']/p"));
  assertEquals(RuntimeVariables.replace("Link Me 1"),selenium.getText("xPath=(//div[@class='child-pages']/ul/li/a)[1]"));
  assertEquals(RuntimeVariables.replace("Link Me 2"),selenium.getText("xPath=(//div[@class='child-pages']/ul/li/a)[2]"));
  selenium.clickAt("xPath=(//div[@class='child-pages']/ul/li/a)[1]",RuntimeVariables.replace("Link Me 1"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("xPath=(//div[@class='wiki-body']/p)","Hi Administrator! Hope you are well! Please link me to another page!"));
  assertTrue(selenium.isPartialText("xPath=(//div[@class='wiki-body']/p)","-testing"));
  assertEquals(RuntimeVariables.replace("Frontpage"),selenium.getText("xPath=(//div[@class='wiki-body']/p[2])"));
  assertEquals(RuntimeVariables.replace("Link Me 2"),selenium.getText("xPath=(//div[@class='wiki-body']/p/a)[2]"));
  selenium.clickAt("xPath=(//div[@class='wiki-body']/p/a)[2]",RuntimeVariables.replace("Link Me 2"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("xPath=(//div[@class='wiki-body']/p)","Hi Administrator!"));
  assertTrue(selenium.isPartialText("xPath=(//div[@class='wiki-body']/p)","I made another mistake! Oh me. Please link this article to another!"));
  assertTrue(selenium.isPartialText("xPath=(//div[@class='wiki-body']/p)","-rj"));
  assertEquals(RuntimeVariables.replace("Frontpage"),selenium.getText("xPath=(//div[@class='wiki-body']/p/a)"));
  assertEquals(RuntimeVariables.replace("Link Me 1"),selenium.getText("xPath=(//div[@class='wiki-body']/p/a)[2]"));
}
