{
  try {
    com.liferay.portlet.dynamicdatamapping.model.DDMStructureEntry returnValue=DDMStructureEntryServiceUtil.getStructureEntry(groupId,structureId);
    return com.liferay.portlet.dynamicdatamapping.model.DDMStructureEntrySoap.toSoapModel(returnValue);
