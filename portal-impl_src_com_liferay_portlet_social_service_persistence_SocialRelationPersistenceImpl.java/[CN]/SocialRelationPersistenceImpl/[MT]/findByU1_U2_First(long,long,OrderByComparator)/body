{
  List<SocialRelation> list=findByU1_U2(userId1,userId2,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(6);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("userId1=");
    msg.append(userId1);
    msg.append(", userId2=");
    msg.append(userId2);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchRelationException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
