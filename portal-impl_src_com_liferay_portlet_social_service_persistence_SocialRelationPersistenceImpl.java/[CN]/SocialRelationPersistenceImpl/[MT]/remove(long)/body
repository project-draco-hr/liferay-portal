{
  Session session=null;
  try {
    session=openSession();
    SocialRelation socialRelation=(SocialRelation)session.get(SocialRelationImpl.class,Long.valueOf(relationId));
    if (socialRelation == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + relationId);
      }
      throw new NoSuchRelationException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + relationId);
    }
    return socialRelationPersistence.remove(socialRelation);
  }
 catch (  NoSuchRelationException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
