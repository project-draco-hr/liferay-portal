{
  Session session=null;
  try {
    session=openSession();
    session.delete(socialRelation);
    session.flush();
    return socialRelation;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(SocialRelation.class.getName());
  }
}
