{
  Object[] finderArgs=new Object[]{new Long(userId1),new Integer(type)};
  Object result=FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_U1_T,finderArgs,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portlet.social.model.SocialRelation WHERE ");
      query.append("userId1 = ?");
      query.append(" AND ");
      query.append("type_ = ?");
      query.append(" ");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(userId1);
      qPos.add(type);
      List<SocialRelation> list=q.list();
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_U1_T,finderArgs,list);
      cacheResult(list);
      return list;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (List<SocialRelation>)result;
  }
}
