{
  SocialRelation socialRelation=(SocialRelation)EntityCacheUtil.getResult(SocialRelationModelImpl.ENTITY_CACHE_ENABLED,SocialRelationImpl.class,relationId,this);
  if (socialRelation == null) {
    Session session=null;
    try {
      session=openSession();
      socialRelation=(SocialRelation)session.get(SocialRelationImpl.class,new Long(relationId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (socialRelation != null) {
        cacheResult(socialRelation);
      }
      closeSession(session);
    }
  }
  return socialRelation;
}
