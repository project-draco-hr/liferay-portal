{
  SocialRelation socialRelation=(SocialRelation)EntityCacheUtil.getResult(SocialRelationModelImpl.ENTITY_CACHE_ENABLED,SocialRelationImpl.class,relationId,this);
  if (socialRelation == _nullSocialRelation) {
    return null;
  }
  if (socialRelation == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      socialRelation=(SocialRelation)session.get(SocialRelationImpl.class,Long.valueOf(relationId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (!hasException && (socialRelation == null)) {
        EntityCacheUtil.putResult(SocialRelationModelImpl.ENTITY_CACHE_ENABLED,SocialRelationImpl.class,relationId,_nullSocialRelation);
      }
 else {
        cacheResult(socialRelation);
      }
      closeSession(session);
    }
  }
  return socialRelation;
}
