{
  ModelListener listener=null;
  if (Validator.isNotNull(_LISTENER)) {
    try {
      listener=(ModelListener)Class.forName(_LISTENER).newInstance();
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(orderId));
  }
  com.liferay.portlet.shopping.model.ShoppingOrder shoppingOrder=getPersistence().remove(orderId);
  if (listener != null) {
    listener.onAfterRemove(shoppingOrder);
  }
  return shoppingOrder;
}
