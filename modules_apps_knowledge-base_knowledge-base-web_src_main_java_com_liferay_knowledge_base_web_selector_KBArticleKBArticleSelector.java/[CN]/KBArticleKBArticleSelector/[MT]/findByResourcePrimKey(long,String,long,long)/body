{
  KBArticle ancestorKBArticle=KBArticleLocalServiceUtil.fetchLatestKBArticle(ancestorResourcePrimKey,WorkflowConstants.STATUS_APPROVED);
  if (ancestorKBArticle == null) {
    return new KBArticleSelection(null,false);
  }
  KBArticle kbArticle=KBArticleLocalServiceUtil.fetchLatestKBArticle(resourcePrimKey,WorkflowConstants.STATUS_APPROVED);
  return getClosestMatchingDescendantKBArticle(groupId,ancestorKBArticle,kbArticle);
}
