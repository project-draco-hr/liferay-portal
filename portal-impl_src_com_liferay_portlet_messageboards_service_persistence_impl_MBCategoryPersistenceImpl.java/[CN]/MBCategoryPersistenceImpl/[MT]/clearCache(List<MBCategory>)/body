{
  finderCache.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  finderCache.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  MBCategory mbCategory : mbCategories) {
    entityCache.removeResult(MBCategoryModelImpl.ENTITY_CACHE_ENABLED,MBCategoryImpl.class,mbCategory.getPrimaryKey());
    clearUniqueFindersCache((MBCategoryModelImpl)mbCategory);
  }
}
