{
  MBCategory mbCategory=findByPrimaryKey(categoryId);
  Session session=null;
  try {
    session=openSession();
    MBCategory[] array=new MBCategoryImpl[3];
    array[0]=getByGroupId_PrevAndNext(session,mbCategory,groupId,orderByComparator,true);
    array[1]=mbCategory;
    array[2]=getByGroupId_PrevAndNext(session,mbCategory,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
