{
  mbCategory=toUnwrappedModel(mbCategory);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(mbCategory)) {
      mbCategory=(MBCategory)session.get(MBCategoryImpl.class,mbCategory.getPrimaryKeyObj());
    }
    if (mbCategory != null) {
      session.delete(mbCategory);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (mbCategory != null) {
    clearCache(mbCategory);
  }
  return mbCategory;
}
