{
  Collection<MBDisplayContextFactory> mbDisplayContextFactories=_mbDisplayContextFactories.values();
  MBHomeDisplayContext mbHomeDisplayContext=new DefaultMBHomeDisplayContext(request,response);
  for (  MBDisplayContextFactory mbDisplayContextFactory : mbDisplayContextFactories) {
    mbHomeDisplayContext=mbDisplayContextFactory.getMBHomeDisplayContext(mbHomeDisplayContext,request,response);
  }
  return mbHomeDisplayContext;
}
