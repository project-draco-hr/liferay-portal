{
  if (type != ExpandoColumnImpl.BOOLEAN && type != ExpandoColumnImpl.BOOLEAN_ARRAY && type != ExpandoColumnImpl.DATE && type != ExpandoColumnImpl.DOUBLE && type != ExpandoColumnImpl.DOUBLE_ARRAY && type != ExpandoColumnImpl.FLOAT && type != ExpandoColumnImpl.FLOAT_ARRAY && type != ExpandoColumnImpl.INTEGER && type != ExpandoColumnImpl.INTEGER_ARRAY && type != ExpandoColumnImpl.LONG && type != ExpandoColumnImpl.LONG_ARRAY && type != ExpandoColumnImpl.SHORT && type != ExpandoColumnImpl.SHORT_ARRAY && type != ExpandoColumnImpl.STRING) {
    throw new ExpandoColumnTypeException();
  }
}
