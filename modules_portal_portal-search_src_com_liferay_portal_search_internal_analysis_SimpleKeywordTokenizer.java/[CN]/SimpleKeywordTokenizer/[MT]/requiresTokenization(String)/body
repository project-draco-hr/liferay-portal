{
  int start=keyword.indexOf(CharPool.QUOTE);
  int end=keyword.indexOf(CharPool.QUOTE,start + 1);
  if (!((keyword.indexOf(CharPool.QUOTE) == 0) && (keyword.lastIndexOf(CharPool.QUOTE) == (keyword.length() - 1)))) {
    if ((start > -1) && (end > start)) {
      return true;
    }
  }
  return false;
}
