{
  String artcleResourceUuid=referenceElement.attributeValue("article-resource-uuid");
  try {
    boolean valid=validateMissingReference(artcleResourceUuid,portletDataContext.getCompanyId(),portletDataContext.getScopeGroupId());
    if (!valid) {
      valid=validateMissingReference(artcleResourceUuid,portletDataContext.getCompanyId(),portletDataContext.getCompanyGroupId());
    }
    return valid;
  }
 catch (  Exception e) {
    return false;
  }
}
