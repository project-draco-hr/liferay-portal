{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=Web Content"));
  assertTrue(selenium.isElementPresent("link=AP Web Content Name"));
  selenium.click(RuntimeVariables.replace("//img[@alt='Delete']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this[\\s\\S]$"));
  assertTrue(selenium.isTextPresent("You have successfully updated the setup."));
  assertFalse(selenium.isElementPresent("link=Web Content"));
  assertFalse(selenium.isElementPresent("link=AP Web Content Name"));
  assertTrue(selenium.isTextPresent("No assets selected."));
}
