{
  AtomicInteger count=registerCounterRepositoryEventListener(_repositoryClassDefinition,RepositoryEventType.Add.class,FileEntry.class);
  int n=Math.abs(RandomTestUtil.nextInt());
  for (int i=0; i < n; i++) {
    _repositoryClassDefinition.trigger(RepositoryEventType.Add.class,FileEntry.class,null);
  }
  Assert.assertEquals(n,count.get());
}
