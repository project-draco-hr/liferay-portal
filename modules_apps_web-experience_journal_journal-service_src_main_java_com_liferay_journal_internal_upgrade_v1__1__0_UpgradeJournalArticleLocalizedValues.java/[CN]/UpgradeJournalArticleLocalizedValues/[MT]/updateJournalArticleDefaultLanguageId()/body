{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps1=connection.prepareStatement("select id_, title from JournalArticle");PreparedStatement ps2=AutoBatchPreparedStatementUtil.concurrentAutoBatch(connection,"update JournalArticle set defaultLanguageId = ? " + "where id_ = ?");ResultSet rs=ps1.executeQuery()){
    Locale defaultLocale=LocaleUtil.getSiteDefault();
    while (rs.next()) {
      String defaultLanguageId=LocalizationUtil.getDefaultLanguageId(rs.getString(2),defaultLocale);
      ps2.setString(1,defaultLanguageId);
      ps2.setLong(2,rs.getLong(1));
      ps2.addBatch();
    }
    ps2.executeBatch();
  }
 }
