{
  DynamicQuery dynamicQuery=dlSyncEventLocalService.dynamicQuery();
  dynamicQuery.add(RestrictionsFactoryUtil.eq("typePK",typePk));
  List<DLSyncEvent> dlSyncEvents=dlSyncEventLocalService.dynamicQuery(dynamicQuery);
  if (dlSyncEvents.isEmpty()) {
    return syncDLObject;
  }
  DLSyncEvent dlSyncEvent=dlSyncEvents.get(0);
  syncDLObject.setModifiedTime(dlSyncEvent.getModifiedTime());
  return syncDLObject;
}
