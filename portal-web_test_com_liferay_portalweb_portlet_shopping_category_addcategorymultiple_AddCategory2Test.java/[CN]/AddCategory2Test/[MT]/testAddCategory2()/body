{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Shopping Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Category']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_34_name",RuntimeVariables.replace("Category2 Test2"));
  selenium.type("_34_description",RuntimeVariables.replace("This is a category2 test2."));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertTrue(selenium.isTextPresent("Category2 Test2"));
  assertTrue(selenium.isTextPresent("This is a category2 test2."));
}
