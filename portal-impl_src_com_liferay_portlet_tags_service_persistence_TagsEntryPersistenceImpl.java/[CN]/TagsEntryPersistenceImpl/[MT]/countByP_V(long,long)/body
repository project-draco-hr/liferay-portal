{
  Object[] finderArgs=new Object[]{new Long(parentEntryId),new Long(vocabularyId)};
  Long count=(Long)FinderCacheUtil.getResult(FINDER_PATH_COUNT_BY_P_V,finderArgs,this);
  if (count == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT COUNT(tagsEntry) ");
      query.append("FROM TagsEntry tagsEntry WHERE ");
      query.append("tagsEntry.parentEntryId = ?");
      query.append(" AND ");
      query.append("tagsEntry.vocabularyId = ?");
      query.append(" ");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(parentEntryId);
      qPos.add(vocabularyId);
      count=(Long)q.uniqueResult();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (count == null) {
        count=Long.valueOf(0);
      }
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_P_V,finderArgs,count);
      closeSession(session);
    }
  }
  return count.intValue();
}
