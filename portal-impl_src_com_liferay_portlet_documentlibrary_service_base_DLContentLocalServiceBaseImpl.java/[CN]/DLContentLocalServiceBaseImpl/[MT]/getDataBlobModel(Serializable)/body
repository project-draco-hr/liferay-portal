{
  Session session=null;
  try {
    session=dlContentPersistence.openSession();
    return (com.liferay.portlet.documentlibrary.model.DLContentDataBlobModel)session.get(DLContentDataBlobModel.class,primaryKey);
  }
 catch (  Exception e) {
    throw dlContentPersistence.processException(e);
  }
 finally {
    dlContentPersistence.closeSession(session);
  }
}
