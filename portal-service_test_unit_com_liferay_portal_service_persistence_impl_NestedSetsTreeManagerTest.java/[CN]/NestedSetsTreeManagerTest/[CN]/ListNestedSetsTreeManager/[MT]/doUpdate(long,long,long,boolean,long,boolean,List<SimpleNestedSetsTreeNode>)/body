{
  for (  SimpleNestedSetsTreeNode simpleNestedSetsTreeNode : _simpleNestedSetsTreeNodes) {
    if ((inList != null) && !inList.contains(simpleNestedSetsTreeNode)) {
      continue;
    }
    long nestedSetsLeft=simpleNestedSetsTreeNode._nestedSetsLeft;
    if (_isInRange(nestedSetsLeft,start,startIncluside,end,endInclusive)) {
      simpleNestedSetsTreeNode.setNestedSetsLeft(nestedSetsLeft + delta);
    }
    long nestedSetsRight=simpleNestedSetsTreeNode._nestedSetsRight;
    if (_isInRange(nestedSetsRight,start,startIncluside,end,endInclusive)) {
      simpleNestedSetsTreeNode.setNestedSetsRight(nestedSetsRight + delta);
    }
  }
}
