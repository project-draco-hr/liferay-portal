{
  for (  SimpleNestedSetsTreeNode simpleNestedSetsTreeNode : _simpleNestedSetsTreeNodes) {
    if (leftOrRight) {
      long nestedSetsLeft=simpleNestedSetsTreeNode.getNestedSetsLeft();
      if (inclusive) {
        if (nestedSetsLeft >= limit) {
          simpleNestedSetsTreeNode.setNestedSetsLeft(nestedSetsLeft + delta);
        }
      }
 else {
        if (nestedSetsLeft > limit) {
          simpleNestedSetsTreeNode.setNestedSetsLeft(nestedSetsLeft + delta);
        }
      }
    }
 else {
      long nestedSetsRight=simpleNestedSetsTreeNode.getNestedSetsRight();
      if (inclusive) {
        if (nestedSetsRight >= limit) {
          simpleNestedSetsTreeNode.setNestedSetsRight(nestedSetsRight + delta);
        }
      }
 else {
        if (nestedSetsRight > limit) {
          simpleNestedSetsTreeNode.setNestedSetsRight(nestedSetsRight + delta);
        }
      }
    }
  }
}
