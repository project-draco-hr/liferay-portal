{
  List<SimpleNestedSetsTreeNode> simpleNestedSetsTreeNodes=new ArrayList<SimpleNestedSetsTreeNode>(Arrays.asList(childSimpleNestedSetsTreeNodes));
  simpleNestedSetsTreeNodes.add(simpleNestedSetsTreeNode);
  Collections.sort(simpleNestedSetsTreeNodes);
  Assert.assertEquals(simpleNestedSetsTreeNodes,_nestedSetsTreeManager.getDescendants(simpleNestedSetsTreeNode));
}
