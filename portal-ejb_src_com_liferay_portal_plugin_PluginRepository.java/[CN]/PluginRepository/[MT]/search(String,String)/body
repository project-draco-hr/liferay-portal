{
  List result=new ArrayList();
  for (Iterator iterator=_plugins.iterator(); iterator.hasNext(); ) {
    Plugin plugin=(Plugin)iterator.next();
    if ((Validator.isNull(type) || type.equals(plugin.getType())) && (Validator.isNull(tag) || plugin.getTags().contains(tag))) {
      result.add(plugin);
    }
  }
  return result;
}
