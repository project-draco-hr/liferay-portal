{
  E element=null;
  _takeLock.lockInterruptibly();
  try {
    while (_count.get() == 0) {
      _notEmptyCondition.await();
    }
    element=_dequeue();
    if (_count.getAndDecrement() > 1) {
      _notEmptyCondition.signal();
    }
  }
  finally {
    _takeLock.unlock();
  }
  return element;
}
