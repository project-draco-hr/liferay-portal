{
  boolean truthValue=false;
  String name=_lhs.toString();
  String rhsValue=_rhs.toString();
  if (name.startsWith(UserAttributesUtil.USER_ATTRIBUTE_PREFIX)) {
    String wildcard=Matcher.quoteReplacement(".*");
    boolean regex=false;
    if (rhsValue.contains("*")) {
      regex=true;
      rhsValue=rhsValue.replaceAll("\\*",wildcard);
    }
    String lhsValue=UserAttributesUtil.getAttribute(basicFact.getUserId(),name);
    if (regex && lhsValue.matches(rhsValue)) {
      truthValue=true;
    }
 else     if (lhsValue.equalsIgnoreCase(rhsValue)) {
      truthValue=true;
    }
  }
 else   if (name.equals(DateTimeUtil.DATE_ATTRIBUTE)) {
    String timeZoneId=UserAttributesUtil.getTimeZoneId(basicFact.getUserId());
    int result=DateTimeUtil.compareDateWithToday(timeZoneId,rhsValue);
    if (result == 0) {
      truthValue=true;
    }
  }
 else   if (name.equals(DateTimeUtil.TIME_ATTRIBUTE)) {
    String timeZoneId=UserAttributesUtil.getTimeZoneId(basicFact.getUserId());
    int result=DateTimeUtil.compareTimeWithNow(timeZoneId,rhsValue);
    if (result == 0) {
      truthValue=true;
    }
  }
 else   if (name.equals(DateTimeUtil.DAY_ATTRIBUTE)) {
    String timeZoneId=UserAttributesUtil.getTimeZoneId(basicFact.getUserId());
    int result=DateTimeUtil.compareDayWithToday(timeZoneId,rhsValue);
    if (result == 0) {
      truthValue=true;
    }
  }
  return truthValue;
}
