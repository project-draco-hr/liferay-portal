{
  if (contentField.equals(JournalFeedConstants.WEB_CONTENT_DESCRIPTION) || contentField.equals(JournalFeedConstants.RENDERED_WEB_CONTENT)) {
    return true;
  }
 else {
    try {
      JournalStructure structure=journalStructurePersistence.findByG_S(groupId,structureId);
      Document document=SAXReaderUtil.read(structure.getXsd());
      StringBundler sb=new StringBundler(3);
      sb.append("//dynamic-element[@name=");
      String escapedContentField=HtmlUtil.escapeXPathAttribute(contentField);
      sb.append(escapedContentField);
      sb.append("]");
      XPath xPathSelector=SAXReaderUtil.createXPath(sb.toString());
      Node node=xPathSelector.selectSingleNode(document);
      if (node != null) {
        return true;
      }
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
  return false;
}
