{
  if (contentField.equals(JournalFeedConstants.WEB_CONTENT_DESCRIPTION) || contentField.equals(JournalFeedConstants.RENDERED_WEB_CONTENT)) {
    return true;
  }
 else {
    try {
      JournalStructure structure=journalStructurePersistence.findByG_S(groupId,structureId);
      Document doc=SAXReaderUtil.read(structure.getXsd());
      XPath xpathSelector=SAXReaderUtil.createXPath("//dynamic-element[@name='" + contentField + "']");
      Element el=(Element)xpathSelector.selectSingleNode(doc);
      if (el != null) {
        return true;
      }
    }
 catch (    Exception e) {
    }
  }
  return false;
}
