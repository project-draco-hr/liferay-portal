{
  resourceLocalService.deleteResource(feed.getCompanyId(),JournalFeed.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,feed.getId());
  JournalContentFeed journalContentFeed=new JournalContentFeed(feed);
  ContentFeedService contentFeedService=MirageServiceFactory.getContentFeedService();
  try {
    contentFeedService.deleteContentFeed(journalContentFeed);
  }
 catch (  CMSException cmse) {
    _throwException(cmse);
  }
}
