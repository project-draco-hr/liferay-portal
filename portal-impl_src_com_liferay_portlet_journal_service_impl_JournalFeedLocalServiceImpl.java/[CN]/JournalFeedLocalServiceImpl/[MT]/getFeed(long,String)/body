{
  JournalFeed feed=new JournalFeedImpl();
  feed.setGroupId(groupId);
  feed.setFeedId(feedId);
  JournalContentFeed journalContentFeed=new JournalContentFeed(feed);
  OptionalJournalFeedCriteria criteria=new OptionalJournalFeedCriteria(OptionalJournalFeedCriteria.FIND_BY_G_F);
  ContentFeedService contentFeedService=MirageServiceFactory.getContentFeedService();
  try {
    journalContentFeed=(JournalContentFeed)contentFeedService.getContentFeed(journalContentFeed,criteria);
  }
 catch (  CMSException ex) {
    _throwException(ex);
  }
  return journalContentFeed.getFeed();
}
