{
  SmartCriteria criteria=new SmartCriteria();
  criteria.add(MirageFeedCriteria.QUERY,MirageFeedCriteria.FIND_ALL);
  ContentFeedService contentFeedService=MirageServiceFactory.getContentFeedService();
  try {
    List<ContentFeed> contentFeeds=contentFeedService.searchContentFeeds(criteria.getCriteria());
    return getFeeds(contentFeeds);
  }
 catch (  CMSException cmse) {
    throw (SystemException)cmse.getCause();
  }
}
