{
  SearchCriteria criteria=new SearchCriteria();
  List<SearchFieldValue> searchFields=new ArrayList<SearchFieldValue>();
  _addSearchField(searchFields,OptionalJournalFeedCriteria.FINDER,OptionalJournalFeedCriteria.FIND_ALL);
  criteria.setSearchFieldValues(searchFields);
  ContentFeedService contentFeedService=MirageServiceFactory.getContentFeedService();
  try {
    List<ContentFeed> contentFeeds=contentFeedService.searchContentFeeds(criteria);
    return _getFeedsFromContentFeeds(contentFeeds);
  }
 catch (  CMSException ex) {
    throw (SystemException)ex.getCause();
  }
}
