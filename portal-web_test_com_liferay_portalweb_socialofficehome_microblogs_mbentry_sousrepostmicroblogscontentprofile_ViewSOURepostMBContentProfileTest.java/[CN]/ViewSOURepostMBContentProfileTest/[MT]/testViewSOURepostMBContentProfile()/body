{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/socialoffice01/so/profile");
  selenium.waitForVisible("//div[@class='lfr-contact-name']/a");
  assertEquals(RuntimeVariables.replace("Social01 Office01 User01"),selenium.getText("//div[@class='lfr-contact-name']/a"));
  selenium.waitForVisible("//div[@class='activity-title']");
  assertEquals(RuntimeVariables.replace("Reposted From Joe: Microblogs Post"),selenium.getText("//div[@class='activity-title']"));
  selenium.open("/web/socialoffice01/so/microblogs");
  selenium.waitForVisible("//div[@class='user-name']");
  assertEquals(RuntimeVariables.replace("Social01 Office01 User01 Reposted From Joe Bloggs"),selenium.getText("//div[@class='user-name']"));
  assertEquals(RuntimeVariables.replace("Social01 Office01 User01"),selenium.getText("//div[@class='user-name']/span"));
  assertEquals(RuntimeVariables.replace("Microblogs Post"),selenium.getText("//div[@class='content']"));
}
