{
  if (primary) {
    Iterator itr=WebsiteUtil.findByC_C_C_P(companyId,className,classPK,primary).iterator();
    while (itr.hasNext()) {
      Website website=(Website)itr.next();
      if ((websiteId <= 0) || (website.getWebsiteId() != websiteId)) {
        website.setPrimary(false);
        WebsiteUtil.update(website);
      }
    }
  }
}
