{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ExpandoTable.class,ExpandoTable.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("tableId",RandomTestUtil.nextLong()));
  List<ExpandoTable> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
