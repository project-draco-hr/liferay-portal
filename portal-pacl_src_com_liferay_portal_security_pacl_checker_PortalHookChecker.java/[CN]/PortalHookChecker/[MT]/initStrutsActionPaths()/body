{
  _strutsActionPaths=getPropertySet("security-manager-hook-struts-action-paths");
  if (_log.isDebugEnabled()) {
    Set<String> strutsActionPaths=new TreeSet<String>(_strutsActionPaths);
    for (    String strutsActionPath : strutsActionPaths) {
      _log.debug("Allowing Struts action path " + strutsActionPath);
    }
  }
}
