{
  Assert.assertEquals(_DEFAULT_VALUE,_baseSettings.getValue(_KEY,_DEFAULT_VALUE));
  Assert.assertArrayEquals(_DEFAULT_VALUES,_baseSettings.getValues(_KEY,_DEFAULT_VALUES));
  _parentSettings.setValue(_KEY,_VALUE);
  Assert.assertEquals(_VALUE,_baseSettings.getValue(_KEY,_DEFAULT_VALUE));
  _parentSettings.setValues(_KEY,_VALUES);
  Assert.assertArrayEquals(_VALUES,_baseSettings.getValues(_KEY,_DEFAULT_VALUES));
}
