{
  List<ClassType> classTypes=new ArrayList<ClassType>();
  List<DDMStructure> ddmStructures=DDMStructureServiceUtil.getStructures(groupIds,PortalUtil.getClassNameId(_className));
  for (  DDMStructure ddmStructure : ddmStructures) {
    classTypes.add(new DDMStructureClassType(ddmStructure.getStructureId(),ddmStructure.getName(locale),LocaleUtil.toLanguageId(locale)));
  }
  return classTypes;
}
