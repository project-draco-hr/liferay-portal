{
  List entries=BlogsEntryLocalServiceUtil.getGroupEntries(groupId,0,max);
  Iterator itr=entries.iterator();
  while (itr.hasNext()) {
    BlogsEntry entry=(BlogsEntry)itr.next();
    if (!BlogsEntryPermission.contains(getPermissionChecker(),entry,ActionKeys.VIEW)) {
      itr.remove();
    }
  }
  return entries;
}
