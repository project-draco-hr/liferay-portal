{
  Organization organization=organizationPersistence.findByPrimaryKey(organizationId);
  String name=organization.getName();
  String description=name;
  List<BlogsEntry> blogsEntries;
  if (onlyPublished)   blogsEntries=getPublishedOrganizationEntries(organizationId,max);
 else   blogsEntries=getOrganizationEntries(organizationId,max);
  return exportToRSS(name,description,type,version,displayStyle,feedURL,entryURL,blogsEntries,themeDisplay);
}
