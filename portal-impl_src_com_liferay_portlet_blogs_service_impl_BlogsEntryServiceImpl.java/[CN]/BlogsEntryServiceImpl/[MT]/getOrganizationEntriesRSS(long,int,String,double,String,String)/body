{
  Organization organization=OrganizationLocalServiceUtil.getOrganization(organizationId);
  Company company=CompanyUtil.findByPrimaryKey(organization.getCompanyId());
  String name=company.getName();
  List blogsEntries=getOrganizationEntries(organizationId,max);
  return exportToRSS(name,null,type,version,feedURL,entryURL,blogsEntries);
}
