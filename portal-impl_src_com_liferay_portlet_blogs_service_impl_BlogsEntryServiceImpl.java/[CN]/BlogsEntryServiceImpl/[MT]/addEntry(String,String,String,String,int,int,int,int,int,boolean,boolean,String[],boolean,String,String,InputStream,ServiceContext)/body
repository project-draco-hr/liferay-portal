{
  BlogsPermission.check(getPermissionChecker(),serviceContext.getScopeGroupId(),ActionKeys.ADD_ENTRY);
  return blogsEntryLocalService.addEntry(getUserId(),title,subtitle,description,content,displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute,allowPingbacks,allowTrackbacks,trackbacks,smallImage,smallImageURL,smallImageFileName,smallImageInputStream,serviceContext);
}
