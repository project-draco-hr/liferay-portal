{
  Organization organization=organizationPersistence.findByPrimaryKey(organizationId);
  String name=organization.getName();
  List<BlogsEntry> blogsEntries=getOrganizationEntries(organizationId,displayDate,status,max);
  return exportToRSS(name,name,type,version,displayStyle,feedURL,entryURL,blogsEntries,themeDisplay);
}
