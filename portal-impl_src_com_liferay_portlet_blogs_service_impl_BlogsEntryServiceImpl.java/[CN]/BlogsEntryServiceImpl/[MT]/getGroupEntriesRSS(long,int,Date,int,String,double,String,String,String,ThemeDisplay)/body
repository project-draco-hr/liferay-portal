{
  Group group=groupPersistence.findByPrimaryKey(groupId);
  String name=HtmlUtil.escape(group.getDescriptiveName());
  String description=name;
  List<BlogsEntry> blogsEntries=getGroupEntries(groupId,status,displayDate,max);
  return exportToRSS(name,description,type,version,displayStyle,feedURL,entryURL,blogsEntries,themeDisplay);
}
