{
  Group group=groupPersistence.findByPrimaryKey(groupId);
  String name=HtmlUtil.escape(group.getDescriptiveName());
  List<BlogsEntry> blogsEntries=getGroupEntries(groupId,displayDate,status,max);
  return exportToRSS(name,name,type,version,displayStyle,feedURL,entryURL,blogsEntries,themeDisplay);
}
