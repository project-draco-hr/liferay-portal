{
  BlogsPermission.check(getPermissionChecker(),serviceContext.getScopeGroupId(),ActionKeys.ADD_ENTRY);
  return blogsEntryLocalService.addEntry(getUserId(),title,description,content,displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute,allowPingbacks,allowTrackbacks,trackbacks,smallImage,smallImageURL,smallFileInputStream,smallFileName,serviceContext);
}
