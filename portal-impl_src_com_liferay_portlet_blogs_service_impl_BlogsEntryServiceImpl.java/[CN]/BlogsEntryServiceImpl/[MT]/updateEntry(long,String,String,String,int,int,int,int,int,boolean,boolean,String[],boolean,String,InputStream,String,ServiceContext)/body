{
  BlogsEntryPermission.check(getPermissionChecker(),entryId,ActionKeys.UPDATE);
  return blogsEntryLocalService.updateEntry(getUserId(),entryId,title,description,content,displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute,allowPingbacks,allowTrackbacks,trackbacks,smallImage,smallImageURL,smallFileInputStream,smallFileName,serviceContext);
}
