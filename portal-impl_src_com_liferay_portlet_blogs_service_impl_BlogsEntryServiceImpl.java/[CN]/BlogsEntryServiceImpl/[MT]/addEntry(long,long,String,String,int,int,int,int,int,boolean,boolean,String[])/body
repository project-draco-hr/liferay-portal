{
  PortletPermissionUtil.check(getPermissionChecker(),plid,PortletKeys.BLOGS,ActionKeys.ADD_ENTRY);
  return BlogsEntryLocalServiceUtil.addEntry(getUserId(),plid,categoryId,title,content,displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute,tagsEntries,addCommunityPermissions,addGuestPermissions);
}
