{
  List entries=new ArrayList();
  Iterator itr=BlogsEntryLocalServiceUtil.getCompanyEntries(companyId,0,_MAX_END).iterator();
  while (itr.hasNext() && (entries.size() < max)) {
    BlogsEntry entry=(BlogsEntry)itr.next();
    if (BlogsEntryPermission.contains(getPermissionChecker(),entry,ActionKeys.VIEW)) {
      entries.add(entry);
    }
  }
  return entries;
}
