{
  if (_rootLayout != null) {
    return _rootLayout;
  }
  if (Validator.isNotNull(getRootLayoutUuid())) {
    Layout layout=_themeDisplay.getLayout();
    _rootLayout=LayoutLocalServiceUtil.fetchLayoutByUuidAndGroupId(getRootLayoutUuid(),_themeDisplay.getScopeGroupId(),layout.isPrivateLayout());
  }
  return _rootLayout;
}
