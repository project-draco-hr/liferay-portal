{
  StringBundler sb=new StringBundler();
  _buildSiteMap(_themeDisplay.getLayout(),getRootLayouts(),getRootLayout(),isIncludeRootInTree(),_siteMapPortletInstanceConfiguration.displayDepth(),_siteMapPortletInstanceConfiguration.showCurrentPage(),_siteMapPortletInstanceConfiguration.useHtmlTitle(),_siteMapPortletInstanceConfiguration.showHiddenPages(),1,_themeDisplay,sb);
  return sb.toString();
}
