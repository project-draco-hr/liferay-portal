{
  FormStructureLink formStructureLink=findByPrimaryKey(formStructureLinkId);
  Session session=null;
  try {
    session=openSession();
    FormStructureLink[] array=new FormStructureLinkImpl[3];
    array[0]=getByFormStructureId_PrevAndNext(session,formStructureLink,formStructureId,orderByComparator,true);
    array[1]=formStructureLink;
    array[2]=getByFormStructureId_PrevAndNext(session,formStructureLink,formStructureId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
