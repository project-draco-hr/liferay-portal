{
  DDMDataProviderImpl ddmDataProviderImpl=new DDMDataProviderImpl();
  ddmDataProviderImpl.setUuid(getUuid());
  ddmDataProviderImpl.setDataProviderId(getDataProviderId());
  ddmDataProviderImpl.setGroupId(getGroupId());
  ddmDataProviderImpl.setCompanyId(getCompanyId());
  ddmDataProviderImpl.setUserId(getUserId());
  ddmDataProviderImpl.setUserName(getUserName());
  ddmDataProviderImpl.setCreateDate(getCreateDate());
  ddmDataProviderImpl.setModifiedDate(getModifiedDate());
  ddmDataProviderImpl.setName(getName());
  ddmDataProviderImpl.setDescription(getDescription());
  ddmDataProviderImpl.setDefinition(getDefinition());
  ddmDataProviderImpl.resetOriginalValues();
  return ddmDataProviderImpl;
}
