{
  try {
    new NettyFabricWorkerExecutionChannelHandler(null,null,0);
    Assert.fail();
  }
 catch (  NullPointerException npe) {
    Assert.assertEquals("Repository is null",npe.getMessage());
  }
  try {
    new NettyFabricWorkerExecutionChannelHandler(new MockRepository<Channel>(),null,0);
    Assert.fail();
  }
 catch (  NullPointerException npe) {
    Assert.assertEquals("Fabric agent is null",npe.getMessage());
  }
  new NettyFabricWorkerExecutionChannelHandler(new MockRepository<Channel>(),new LocalFabricAgent(new EmbeddedProcessExecutor()),0);
}
