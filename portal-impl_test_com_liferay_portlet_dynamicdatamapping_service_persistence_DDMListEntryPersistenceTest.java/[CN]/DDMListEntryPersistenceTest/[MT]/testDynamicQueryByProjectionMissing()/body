{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMListEntry.class,DDMListEntry.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("listEntryId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("listEntryId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
