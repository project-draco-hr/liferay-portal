{
  addStructure(_classNameId,"Contact","Contact");
  addStructure(_classNameId,"Event","Event");
  List<DDMStructure> structures=DDMStructureLocalServiceUtil.search(TestPropsValues.getCompanyId(),new long[]{group.getGroupId()},_classNameId,"Contact","Event",null,DDMStructureConstants.TYPE_DEFAULT,WorkflowConstants.STATUS_APPROVED,true,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  Assert.assertEquals(0,structures.size());
}
