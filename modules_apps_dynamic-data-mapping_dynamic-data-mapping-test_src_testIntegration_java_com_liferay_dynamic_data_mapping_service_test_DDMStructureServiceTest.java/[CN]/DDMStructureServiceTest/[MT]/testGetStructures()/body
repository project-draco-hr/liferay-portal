{
  addStructure(_classNameId,StringUtil.randomString());
  addStructure(_classNameId,StringUtil.randomString());
  addStructure(_classNameId,StringUtil.randomString());
  long[] groupIds=new long[]{group.getGroupId(),_otherGroup.getGroupId()};
  List<DDMStructure> ddmStructures=DDMStructureServiceUtil.getStructures(TestPropsValues.getCompanyId(),groupIds,_classNameId,WorkflowConstants.STATUS_ANY);
  Assert.assertEquals(3,ddmStructures.size());
}
