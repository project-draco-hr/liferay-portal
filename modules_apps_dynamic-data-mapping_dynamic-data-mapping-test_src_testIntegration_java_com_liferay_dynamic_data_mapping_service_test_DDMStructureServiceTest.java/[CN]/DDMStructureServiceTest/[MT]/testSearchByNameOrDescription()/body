{
  addStructure(_classNameId,"Contact","Contact");
  addStructure(_classNameId,"Event","Event");
  List<DDMStructure> structures=DDMStructureLocalServiceUtil.search(TestPropsValues.getCompanyId(),new long[]{group.getGroupId()},_classNameId,"Contact","Event",null,DDMStructureConstants.TYPE_DEFAULT,WorkflowConstants.STATUS_APPROVED,false,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StructureIdComparator(true));
  Assert.assertEquals("Contact",getStructureName(structures.get(0)));
  Assert.assertEquals("Event",getStructureName(structures.get(1)));
}
