{
  addStructure(_classNameId,StringUtil.randomString());
  addStructure(_classNameId,StringUtil.randomString());
  addStructure(_classNameId,StringUtil.randomString());
  long[] groupIds=new long[]{group.getGroupId(),_otherGroup.getGroupId()};
  int ddmStructuresCount=DDMStructureServiceUtil.searchCount(TestPropsValues.getCompanyId(),groupIds,_classNameId,StringPool.BLANK,WorkflowConstants.STATUS_ANY);
  Assert.assertEquals(3,ddmStructuresCount);
}
