{
  addStructure(_classNameId,StringUtil.randomString());
  addStructure(_classNameId,StringUtil.randomString());
  addStructure(_classNameId,StringUtil.randomString());
  long[] groupIds=new long[]{group.getGroupId(),_otherGroup.getGroupId()};
