{
  HttpSession session=request.getSession();
  Map<String,String> sessionAuthenticationTokensMap=(Map<String,String>)session.getAttribute(WebKeys.AUTHENTICATION_TOKEN);
  if (sessionAuthenticationTokensMap == null) {
    sessionAuthenticationTokensMap=new HashMap<String,String>();
    session.setAttribute(WebKeys.AUTHENTICATION_TOKEN,sessionAuthenticationTokensMap);
  }
  return sessionAuthenticationTokensMap;
}
