{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(liferayPortletRequest,MBPortletKeys.MESSAGE_BOARDS,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcRenderCommandName","/message_boards/edit_message");
  portletURL.setParameter("messageId",String.valueOf(_message.getMessageId()));
  return portletURL;
}
