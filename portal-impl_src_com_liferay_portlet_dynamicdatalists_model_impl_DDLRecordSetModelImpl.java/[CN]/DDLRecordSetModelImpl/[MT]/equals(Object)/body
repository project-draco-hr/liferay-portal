{
  if (obj == null) {
    return false;
  }
  DDLRecordSet ddlRecordSet=null;
  try {
    ddlRecordSet=(DDLRecordSet)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=ddlRecordSet.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
