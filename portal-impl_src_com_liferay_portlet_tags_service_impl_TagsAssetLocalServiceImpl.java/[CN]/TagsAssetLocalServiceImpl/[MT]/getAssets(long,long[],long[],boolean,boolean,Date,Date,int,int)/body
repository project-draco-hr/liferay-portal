{
  if (andOperator) {
    return TagsAssetFinder.findByAndEntryIds(groupId,entryIds,notEntryIds,null,null,null,null,excludeZeroViewCount,publishDate,expirationDate,begin,end);
  }
 else {
    return TagsAssetFinder.findByOrEntryIds(groupId,entryIds,notEntryIds,null,null,null,null,excludeZeroViewCount,publishDate,expirationDate,begin,end);
  }
}
