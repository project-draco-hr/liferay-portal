{
  if (andOperator) {
    return TagsAssetFinder.findByAndEntryIds(entryIds,notEntryIds,null,null,null,null,excludeZeroViewCount,publishDate,expirationDate,begin,end);
  }
 else {
    return TagsAssetFinder.findByOrEntryIds(entryIds,notEntryIds,null,null,null,null,excludeZeroViewCount,publishDate,expirationDate,begin,end);
  }
}
