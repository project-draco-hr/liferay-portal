{
  if (andOperator) {
    return tagsAssetFinder.findByAndEntryIds(groupId,classNameIds,entryIds,notEntryIds,null,null,null,null,excludeZeroViewCount,publishDate,expirationDate,begin,end);
  }
 else {
    return tagsAssetFinder.findByOrEntryIds(groupId,classNameIds,entryIds,notEntryIds,null,null,null,null,excludeZeroViewCount,publishDate,expirationDate,begin,end);
  }
}
