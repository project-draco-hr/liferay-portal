{
  if (classPK <= 0) {
    return null;
  }
  long classNameId=PortalUtil.getClassNameId(className);
  TagsAsset asset=tagsAssetPersistence.fetchByC_C(classNameId,classPK);
  if (asset != null) {
    asset.setViewCount(asset.getViewCount() + 1);
    tagsAssetPersistence.update(asset);
  }
  return asset;
}
