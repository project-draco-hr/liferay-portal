{
  if (andOperator) {
    return tagsAssetFinder.countByAndEntryIds(groupId,classNameIds,entryIds,notEntryIds,excludeZeroViewCount,publishDate,expirationDate);
  }
 else {
    return tagsAssetFinder.countByOrEntryIds(groupId,classNameIds,entryIds,notEntryIds,excludeZeroViewCount,publishDate,expirationDate);
  }
}
