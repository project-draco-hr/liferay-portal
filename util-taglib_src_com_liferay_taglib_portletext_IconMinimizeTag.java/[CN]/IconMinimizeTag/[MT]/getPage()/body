{
  if (FileAvailabilityUtil.isAvailable(servletContext,_PAGE)) {
    return _PAGE;
  }
  PortletDisplay portletDisplay=(PortletDisplay)pageContext.getAttribute("portletDisplay");
  if (!portletDisplay.isShowMinIcon()) {
    return null;
  }
  setCssClass("portlet-minimize portlet-minimize-icon");
  String image=null;
  String message=null;
  if (portletDisplay.isStateMin()) {
    image="resize-vertical";
    message="restore";
  }
 else {
    image="minus";
    message="minimize";
  }
  setImage("../aui/".concat(image));
  setMessage(message);
  String onClick="Liferay.Portlet.minimize('#p_p_id_".concat(portletDisplay.getId()).concat("_', this); return false;");
  setOnClick(onClick);
  setToolTip(false);
  setUrl(portletDisplay.getURLMin());
  return super.getPage();
}
