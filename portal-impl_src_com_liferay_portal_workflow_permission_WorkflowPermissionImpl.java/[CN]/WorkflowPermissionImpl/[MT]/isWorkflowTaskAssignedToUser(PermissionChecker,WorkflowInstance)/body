{
  List<WorkflowTask> workflowTasks=WorkflowTaskManagerUtil.getWorkflowTasksByWorkflowInstance(permissionChecker.getCompanyId(),permissionChecker.getUserId(),workflowInstance.getWorkflowInstanceId(),Boolean.FALSE,0,100,null);
  if (!workflowTasks.isEmpty()) {
    return true;
  }
  return false;
}
