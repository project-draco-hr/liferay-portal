{
  Counter counter=(Counter)EntityCacheUtil.getResult(CounterModelImpl.ENTITY_CACHE_ENABLED,CounterImpl.class,name,this);
  if (counter == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (counter == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      counter=(Counter)session.get(CounterImpl.class,name);
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (counter == null)) {
        EntityCacheUtil.putResult(CounterModelImpl.ENTITY_CACHE_ENABLED,CounterImpl.class,name,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(counter);
      }
      closeSession(session);
    }
  }
  return counter;
}
