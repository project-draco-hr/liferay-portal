{
  Counter counter=(Counter)EntityCacheUtil.getResult(CounterModelImpl.ENTITY_CACHE_ENABLED,CounterModelImpl.class,name,this);
  if (counter == null) {
    Session session=null;
    try {
      session=openSession();
      counter=(Counter)session.get(CounterModelImpl.class,name);
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (counter != null) {
        cacheResult(counter);
      }
      closeSession(session);
    }
  }
  return counter;
}
