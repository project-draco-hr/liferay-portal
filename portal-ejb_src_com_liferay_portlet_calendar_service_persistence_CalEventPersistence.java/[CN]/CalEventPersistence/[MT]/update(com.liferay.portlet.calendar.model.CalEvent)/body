{
  Session session=null;
  try {
    session=openSession();
    if (calEvent.isNew()) {
      session.save(calEvent);
    }
    session.flush();
    calEvent.setNew(false);
    return calEvent;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
