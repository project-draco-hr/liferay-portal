{
  String className=activity.getClassName();
  long classPK=activity.getClassPK();
  String viewEntryInTrashURL=getViewEntryInTrashURL(className,classPK,serviceContext);
  if (viewEntryInTrashURL != null) {
    return viewEntryInTrashURL;
  }
  PortletURL viewEntryPortletURL=getViewEntryPortletURL(className,classPK,serviceContext);
  if (viewEntryPortletURL != null) {
    return viewEntryPortletURL.toString();
  }
  return null;
}
