{
  try {
    WorkflowResultContainer<Object> response=(WorkflowResultContainer<Object>)_synchronousMessageSender.send(WorkflowInstanceRequest.createRemoveWorkflowInstanceRequest(workflowInstanceId));
    if (response.hasError()) {
      throw response.getException();
    }
  }
 catch (  MessageBusException ex) {
    throw new WorkflowException("Unable to remove workflow instance:",ex);
  }
}
