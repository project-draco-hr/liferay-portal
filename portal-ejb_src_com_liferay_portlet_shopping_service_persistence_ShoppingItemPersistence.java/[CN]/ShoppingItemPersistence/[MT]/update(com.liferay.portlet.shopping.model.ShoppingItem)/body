{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(shoppingItem);
    session.flush();
    shoppingItem.setNew(false);
    return shoppingItem;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
