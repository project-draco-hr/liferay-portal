{
  Session session=null;
  try {
    session=openSession();
    session.delete(shoppingItem);
    session.flush();
    ShoppingItemPool.removeByC_S(shoppingItem.getCompanyId(),shoppingItem.getSku());
    return shoppingItem;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
