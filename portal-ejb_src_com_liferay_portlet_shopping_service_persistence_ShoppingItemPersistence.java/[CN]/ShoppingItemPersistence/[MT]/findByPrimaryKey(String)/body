{
  Session session=null;
  try {
    session=openSession();
    ShoppingItemHBM shoppingItemHBM=(ShoppingItemHBM)session.get(ShoppingItemHBM.class,itemId);
    if (shoppingItemHBM == null) {
      _log.warn("No ShoppingItem exists with the primary key " + itemId.toString());
      throw new NoSuchItemException("No ShoppingItem exists with the primary key " + itemId.toString());
    }
    return ShoppingItemHBMUtil.model(shoppingItemHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
