{
  LayoutSet layoutSet=fetchByPrimaryKey(layoutSetId);
  if (layoutSet == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No LayoutSet exists with the primary key " + layoutSetId);
    }
    throw new NoSuchLayoutSetException("No LayoutSet exists with the primary key " + layoutSetId);
  }
  return layoutSet;
}
