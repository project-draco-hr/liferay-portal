{
  try {
    AssetRenderer<?> assetRenderer=getAssetRenderer(classPK);
    if (assetRenderer != null) {
      return assetRenderer.getURLViewInContext(liferayPortletRequest,liferayPortletResponse,noSuchEntryRedirect);
    }
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn(e,e);
    }
  }
  return null;
}
