{
  StringBundler sb=new StringBundler(11);
  sb.append("{class=");
  Class<?> clazz=getClass();
  sb.append(clazz.getName());
  sb.append(", name=");
  sb.append(getName());
  if (_property != null) {
    sb.append(", property=");
    sb.append(_property);
  }
  sb.append(", servletContextName=");
  sb.append(getServletContextName());
  sb.append(", subject=");
  sb.append(getSubject());
  sb.append("}");
  return sb.toString();
}
