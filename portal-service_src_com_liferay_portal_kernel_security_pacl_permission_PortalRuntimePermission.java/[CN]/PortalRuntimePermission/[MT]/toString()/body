{
  StringBundler sb=new StringBundler(9);
  sb.append("(");
  sb.append(getClass().getName());
  sb.append(" ");
  sb.append(getName());
  sb.append(" ");
  sb.append(getServletContextName());
  sb.append(" ");
  sb.append(getSubject());
  if (_property != null) {
    sb.append("#");
    sb.append(_property);
  }
  sb.append(")");
  return sb.toString();
}
