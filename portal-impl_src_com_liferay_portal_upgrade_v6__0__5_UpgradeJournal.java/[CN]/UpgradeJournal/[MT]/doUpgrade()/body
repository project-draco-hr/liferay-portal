{
  if (isSupportsAlterColumnType()) {
    runSQL("alter_column_type JournalArticle title VARCHAR(300) null");
  }
 else {
    UpgradeTable upgradeTable=UpgradeTableFactoryUtil.getUpgradeTable(JournalArticleTable.TABLE_NAME,JournalArticleTable.TABLE_COLUMNS);
    upgradeTable.setCreateSQL(JournalArticleTable.TABLE_SQL_CREATE);
    upgradeTable.setIndexesSQL(JournalArticleTable.TABLE_SQL_ADD_INDEXES);
    upgradeTable.updateTable();
  }
}
