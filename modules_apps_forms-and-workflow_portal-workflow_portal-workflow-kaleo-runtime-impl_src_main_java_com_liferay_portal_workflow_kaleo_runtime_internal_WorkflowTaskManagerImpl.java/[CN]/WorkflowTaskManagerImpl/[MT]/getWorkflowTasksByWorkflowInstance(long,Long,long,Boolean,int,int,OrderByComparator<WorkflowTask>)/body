{
  try {
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setCompanyId(companyId);
    if (userId != null) {
      serviceContext.setUserId(userId);
    }
    List<KaleoTaskInstanceToken> kaleoTaskInstanceTokens=_kaleoTaskInstanceTokenLocalService.getKaleoTaskInstanceTokens(workflowInstanceId,completed,start,end,KaleoTaskInstanceTokenOrderByComparator.getOrderByComparator(orderByComparator,_kaleoWorkflowModelConverter),serviceContext);
    return toWorkflowTasks(kaleoTaskInstanceTokens);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
