{
  try {
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setCompanyId(companyId);
    serviceContext.setUserId(userId);
    List<KaleoTaskInstanceToken> kaleoTaskInstanceTokens=_kaleoTaskInstanceTokenLocalService.search(keywords,completed,searchByUserRoles,start,end,KaleoTaskInstanceTokenOrderByComparator.getOrderByComparator(orderByComparator,_kaleoWorkflowModelConverter),serviceContext);
    return toWorkflowTasks(kaleoTaskInstanceTokens);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
