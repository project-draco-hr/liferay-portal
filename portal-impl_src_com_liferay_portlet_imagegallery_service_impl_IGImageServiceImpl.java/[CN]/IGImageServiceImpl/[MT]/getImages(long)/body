{
  List<IGImage> images=igImageLocalService.getImages(folderId);
  images=ListUtil.copy(images);
  Iterator<IGImage> itr=images.iterator();
  while (itr.hasNext()) {
    IGImage image=itr.next();
    if (!IGImagePermission.contains(getPermissionChecker(),image,ActionKeys.VIEW)) {
      itr.remove();
    }
  }
  return images;
}
