{
  if (label.equals(ACTIVE_LABEL)) {
    return ACTIVE;
  }
 else   if (label.equals(INSTALLED_LABEL)) {
    return INSTALLED;
  }
 else   if (label.equals(RESOLVED_LABEL)) {
    return RESOLVED;
  }
 else   if (label.equals(UNINSTALLED_LABEL)) {
    return UNINSTALLED;
  }
 else {
    return 0;
  }
}
