{
  String orderBy=getOrderBy();
  if (orderBy == null) {
    return false;
  }
  int x=orderBy.indexOf(StringPool.PERIOD + field + StringPool.SPACE);
  if (x == -1) {
    return false;
  }
  int y=orderBy.indexOf(_ORDER_BY_ASC,x);
  if (y == -1) {
    return false;
  }
  int z=orderBy.indexOf(_ORDER_BY_DESC,x);
  if ((z >= 0) && (z < y)) {
    return false;
  }
 else {
    return true;
  }
}
