{
  String emailEntryAddedEnabled=preferences.getValue("email-entry-added-enabled",StringPool.BLANK);
  if (Validator.isNotNull(emailEntryAddedEnabled)) {
    return GetterUtil.getBoolean(emailEntryAddedEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.BLOGS_EMAIL_ENTRY_ADDED_ENABLED));
  }
}
