{
  String emailEntryUpdatedEnabled=preferences.getValue("email-entry-updated-enabled",StringPool.BLANK);
  if (Validator.isNotNull(emailEntryUpdatedEnabled)) {
    return GetterUtil.getBoolean(emailEntryUpdatedEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.BLOGS_EMAIL_ENTRY_UPDATED_ENABLED));
  }
}
