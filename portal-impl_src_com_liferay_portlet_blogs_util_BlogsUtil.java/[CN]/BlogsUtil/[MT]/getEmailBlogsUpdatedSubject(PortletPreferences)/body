{
  String emailBlogsUpdatedSubject=preferences.getValue("email-blogs-updated-subject",StringPool.BLANK);
  if (Validator.isNotNull(emailBlogsUpdatedSubject)) {
    return emailBlogsUpdatedSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.BLOGS_EMAIL_ENTRY_UPDATED_SUBJECT));
  }
}
