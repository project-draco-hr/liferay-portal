{
  String emailBlogsAddedEnabled=preferences.getValue("email-blogs-added-enabled",StringPool.BLANK);
  if (Validator.isNotNull(emailBlogsAddedEnabled)) {
    return GetterUtil.getBoolean(emailBlogsAddedEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.BLOGS_EMAIL_ENTRY_ADDED_ENABLED));
  }
}
