{
  String emailEntryUpdatedSubject=preferences.getValue("emailEntryUpdatedSubject",StringPool.BLANK);
  if (Validator.isNotNull(emailEntryUpdatedSubject)) {
    return emailEntryUpdatedSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.BLOGS_EMAIL_ENTRY_UPDATED_SUBJECT));
  }
}
