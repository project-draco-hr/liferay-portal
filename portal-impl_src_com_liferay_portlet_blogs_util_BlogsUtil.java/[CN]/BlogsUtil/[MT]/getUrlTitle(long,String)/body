{
  if (title == null) {
    return String.valueOf(entryId);
  }
  title=title.trim().toLowerCase();
  if (Validator.isNull(title) || Validator.isNumber(title) || title.equals("rss")) {
    return String.valueOf(entryId);
  }
 else {
    return FriendlyURLNormalizerUtil.normalize(title,_URL_TITLE_REPLACE_CHARS);
  }
}
