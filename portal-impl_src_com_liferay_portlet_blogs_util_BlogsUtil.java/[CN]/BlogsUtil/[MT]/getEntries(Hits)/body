{
  List<BlogsEntry> entries=new ArrayList<BlogsEntry>();
  for (  Document document : hits.getDocs()) {
    long entryClassPK=GetterUtil.getLong(document.get(Field.ENTRY_CLASS_PK));
    try {
      BlogsEntry entry=BlogsEntryLocalServiceUtil.getEntry(entryClassPK);
      entries.add(entry);
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Blogs search index is stale and contains entry " + entryClassPK);
      }
    }
  }
  return entries;
}
