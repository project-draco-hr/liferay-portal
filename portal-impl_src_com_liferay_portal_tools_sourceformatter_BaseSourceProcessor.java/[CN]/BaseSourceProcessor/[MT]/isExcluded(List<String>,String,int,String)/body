{
  if (ListUtil.isEmpty(exclusions)) {
    return false;
  }
  if (exclusions.contains(fileName)) {
    return true;
  }
  if ((lineCount > 0) && exclusions.contains(fileName + StringPool.AT + lineCount)) {
    return true;
  }
  if (Validator.isNotNull(javaTermName) && exclusions.contains(fileName + StringPool.AT + javaTermName)) {
    return true;
  }
  return false;
}
