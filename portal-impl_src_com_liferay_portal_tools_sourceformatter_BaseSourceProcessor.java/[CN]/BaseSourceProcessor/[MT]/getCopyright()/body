{
  if (_copyright != null) {
    return _copyright;
  }
  _copyright=fileUtil.read("copyright.txt");
  if (Validator.isNull(_copyright)) {
    _copyright=fileUtil.read("../copyright.txt");
  }
  if (Validator.isNull(_copyright)) {
    _copyright=fileUtil.read("../../copyright.txt");
  }
  if (Validator.isNull(_copyright)) {
    _copyright=fileUtil.read("../../../copyright.txt");
  }
  if (Validator.isNull(_copyright)) {
    _copyright=StringPool.BLANK;
  }
  return _copyright;
}
