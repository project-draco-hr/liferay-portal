{
  if (Validator.isNotNull(_copyright)) {
    return _copyright;
  }
  _copyright=fileUtil.read("copyright.txt");
  if (Validator.isNull(_copyright)) {
    _copyright=fileUtil.read("../copyright.txt");
  }
  if (Validator.isNull(_copyright)) {
    _copyright=fileUtil.read("../../copyright.txt");
  }
  return _copyright;
}
