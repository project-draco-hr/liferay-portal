{
  boolean insideQuotes=false;
  StringBundler sb=new StringBundler();
  for (int i=0; i < s.length(); i++) {
    char c=s.charAt(i);
    if (insideQuotes) {
      if (c == delimeter) {
        if ((c > 1) && (s.charAt(i - 1) == CharPool.BACK_SLASH) && (s.charAt(i - 2) != CharPool.BACK_SLASH)) {
          continue;
        }
        insideQuotes=false;
      }
    }
 else     if (c == delimeter) {
      insideQuotes=true;
    }
 else {
      sb.append(c);
    }
  }
  return sb.toString();
}
