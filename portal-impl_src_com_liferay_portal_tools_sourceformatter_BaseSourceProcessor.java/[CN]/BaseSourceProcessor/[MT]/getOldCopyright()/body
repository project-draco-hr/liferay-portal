{
  if (_oldCopyright != null) {
    return _oldCopyright;
  }
  _oldCopyright=fileUtil.read("old-copyright.txt");
  if (Validator.isNull(_oldCopyright)) {
    _oldCopyright=fileUtil.read("../old-copyright.txt");
  }
  if (Validator.isNull(_oldCopyright)) {
    _oldCopyright=fileUtil.read("../../old-copyright.txt");
  }
  if (Validator.isNull(_oldCopyright)) {
    _oldCopyright=fileUtil.read("../../../old-copyright.txt");
  }
  if (Validator.isNull(_oldCopyright)) {
    _oldCopyright=StringPool.BLANK;
  }
  return _oldCopyright;
}
