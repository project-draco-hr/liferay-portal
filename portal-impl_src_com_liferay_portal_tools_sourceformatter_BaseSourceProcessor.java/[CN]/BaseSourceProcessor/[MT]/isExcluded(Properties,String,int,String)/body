{
  if (properties == null) {
    return false;
  }
  if (properties.getProperty(fileName) != null) {
    return true;
  }
  if ((lineCount > 0) && (properties.getProperty(fileName + StringPool.AT + lineCount) != null)) {
    return true;
  }
  if (Validator.isNotNull(javaTermName) && (properties.getProperty(fileName + StringPool.AT + javaTermName) != null)) {
    return true;
  }
  return false;
}
