{
  if (portalSource) {
    return content;
  }
  String expectedParameter=getProperty("languageutil.expected.parameter");
  String incorrectParameter=getProperty("languageutil.incorrect.parameter");
  if (!content.contains("LanguageUtil.format(" + incorrectParameter + ", ") && !content.contains("LanguageUtil.get(" + incorrectParameter + ", ")) {
    return content;
  }
  if (autoFix) {
    content=StringUtil.replace(content,new String[]{"LanguageUtil.format(" + incorrectParameter + ", ","LanguageUtil.get(" + incorrectParameter + ", "},new String[]{"LanguageUtil.format(" + expectedParameter + ", ","LanguageUtil.get(" + expectedParameter + ", "});
  }
 else {
    processErrorMessage(fileName,"(Unicode)LanguageUtil.format/get methods require " + expectedParameter + " parameter instead of "+ incorrectParameter+ " "+ fileName);
  }
  return content;
}
