{
  try {
    Iterator itr=getAllAvailablePluginPackages().iterator();
    while (itr.hasNext()) {
      PluginPackage pluginPackage=(PluginPackage)itr.next();
      _indexPluginPackage(pluginPackage);
    }
  }
 catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new SystemException(e);
  }
}
