{
  if (!PrefsPropsUtil.getBoolean(PropsUtil.PLUGIN_NOTIFICATIONS_ENABLED,PropsValues.PLUGIN_NOTIFICATIONS_ENABLED)) {
    return false;
  }
  if (_updateAvailable != null) {
    return _updateAvailable.booleanValue();
  }
  Iterator itr=_installedPluginPackages.getPluginPackages().iterator();
  while (itr.hasNext()) {
    PluginPackage pluginPackage=(PluginPackage)itr.next();
    PluginPackage availablePluginPackage=null;
    if (_isIgnored(pluginPackage)) {
      continue;
    }
    try {
      availablePluginPackage=PluginPackageUtil.getLatestAvailablePluginPackage(pluginPackage.getGroupId(),pluginPackage.getArtifactId());
      if ((availablePluginPackage != null) && Version.getInstance(availablePluginPackage.getVersion()).isLaterVersionThan(pluginPackage.getVersion())) {
        _updateAvailable=Boolean.TRUE;
        break;
      }
    }
 catch (    Exception e) {
    }
  }
  if (_updateAvailable == null) {
    _updateAvailable=Boolean.FALSE;
  }
  return _updateAvailable.booleanValue();
}
