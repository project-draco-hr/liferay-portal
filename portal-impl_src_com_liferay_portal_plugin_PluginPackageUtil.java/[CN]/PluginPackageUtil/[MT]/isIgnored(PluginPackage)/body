{
  String[] ignored=PrefsPropsUtil.getStringArray(PropsUtil.PLUGIN_NOTIFICATIONS_PACKAGES_IGNORED);
  String packageId=pluginPackage.getPackageId();
  for (int i=0; i < ignored.length; i++) {
    String ignoredPackageId=ignored[i];
    if (ignoredPackageId.endsWith(StringPool.STAR)) {
      String prefix=ignoredPackageId.substring(0,ignoredPackageId.length() - 2);
      if (packageId.startsWith(prefix)) {
        return true;
      }
    }
 else {
      if (packageId.equals(ignoredPackageId)) {
        return true;
      }
    }
  }
  return false;
}
