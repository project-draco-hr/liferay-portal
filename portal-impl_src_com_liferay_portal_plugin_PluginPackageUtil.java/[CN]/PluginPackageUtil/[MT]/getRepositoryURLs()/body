{
  try {
    String[] trusted=PrefsPropsUtil.getStringArray(PropsUtil.PLUGIN_REPOSITORIES_TRUSTED);
    String[] untrusted=PrefsPropsUtil.getStringArray(PropsUtil.PLUGIN_REPOSITORIES_UNTRUSTED);
    String[] result=new String[trusted.length + untrusted.length];
    ArrayUtil.combine(trusted,untrusted,result);
    return result;
  }
 catch (  Exception e) {
    throw new PluginPackageException("Unable to read repository list",e);
  }
}
