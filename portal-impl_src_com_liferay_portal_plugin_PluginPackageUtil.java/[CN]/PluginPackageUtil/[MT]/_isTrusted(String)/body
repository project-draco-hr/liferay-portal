{
  try {
    String[] trusted=PrefsPropsUtil.getStringArray(PropsUtil.PLUGIN_REPOSITORIES_TRUSTED,StringPool.NEW_LINE,PropsValues.PLUGIN_REPOSITORIES_TRUSTED);
    if (ArrayUtil.contains(trusted,repositoryURL)) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  Exception e) {
    throw new PluginPackageException("Unable to read repository list",e);
  }
}
