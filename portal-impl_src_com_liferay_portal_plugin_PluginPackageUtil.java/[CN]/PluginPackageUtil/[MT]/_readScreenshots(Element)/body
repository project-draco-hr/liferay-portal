{
  List result=new ArrayList();
  if (parent != null) {
    List screenshots=parent.elements("screenshot");
    Iterator itr=screenshots.iterator();
    while (itr.hasNext()) {
      Element screenshotEl=(Element)itr.next();
      Screenshot screenshot=new Screenshot();
      screenshot.setThumbnailURL(screenshotEl.element("thumbnail-url").getText());
      screenshot.setLargeImageURL(screenshotEl.element("large-image-url").getText());
      result.add(screenshot);
    }
  }
  return result;
}
