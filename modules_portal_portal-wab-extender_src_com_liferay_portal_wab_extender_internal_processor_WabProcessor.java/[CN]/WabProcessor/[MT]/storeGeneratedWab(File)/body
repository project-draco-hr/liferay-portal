{
  String name=_file.getName();
  String extension=FileUtil.getExtension(name);
  int index=name.lastIndexOf(StringPool.PERIOD);
  StringBundler sb=new StringBundler(5);
  sb.append(name.substring(0,index));
  sb.append(StringPool.DASH);
  Format simpleDateFormat=FastDateFormatFactoryUtil.getSimpleDateFormat(PropsValues.INDEX_DATE_FORMAT_PATTERN);
  sb.append(simpleDateFormat.format(new Date()));
  sb.append(StringPool.PERIOD);
  sb.append(extension);
  File destination=new File(PropsValues.MODULE_FRAMEWORK_WEB_EXTENDER_GENERATED_WABS_STORE_DIR);
  destination.mkdirs();
  FileUtil.copyFile(file,new File(destination,sb.toString()));
}
