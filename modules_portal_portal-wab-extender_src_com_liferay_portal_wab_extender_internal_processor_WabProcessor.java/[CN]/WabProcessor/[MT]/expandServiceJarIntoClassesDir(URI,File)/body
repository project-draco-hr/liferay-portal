{
  File webInfClasses=new File(uri.resolve("WEB-INF/classes").getPath());
  ZipInputStream zipInputStream=new ZipInputStream(new FileInputStream(zipFile));
  try {
    for (ZipEntry zipEntry; (zipEntry=zipInputStream.getNextEntry()) != null; zipInputStream.closeEntry()) {
      if (zipEntry.isDirectory()) {
        File newFile=new File(webInfClasses,zipEntry.getName());
        newFile.mkdirs();
        continue;
      }
      File newFile=new File(webInfClasses,zipEntry.getName());
      newFile.getParentFile().mkdirs();
      FileOutputStream fileOutputStream=new FileOutputStream(newFile);
      try {
        StreamUtil.transfer(zipInputStream,fileOutputStream,false);
      }
  finally {
        fileOutputStream.close();
      }
    }
  }
  finally {
    zipInputStream.close();
  }
}
