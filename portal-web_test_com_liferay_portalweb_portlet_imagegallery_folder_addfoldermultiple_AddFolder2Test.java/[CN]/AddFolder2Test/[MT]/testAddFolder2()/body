{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Image Gallery Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Image Gallery Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//div[@class='lfr-component lfr-menu-list right ']/ul/li[2]/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_31_name",RuntimeVariables.replace("Test2 Folder2"));
  selenium.type("_31_description",RuntimeVariables.replace("This is Test2 Folder2."));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertTrue(selenium.isTextPresent("Test2 Folder2"));
}
