{
  org.osgi.framework.ServiceReference<T>[] originalServiceReferences=(org.osgi.framework.ServiceReference<T>[])_bundleContext.getServiceReferences(className,filter);
  if (originalServiceReferences == null) {
    return null;
  }
  ServiceReference<T>[] newServiceReferences=new ServiceReference[originalServiceReferences.length];
  for (int i=0; i < originalServiceReferences.length; i++) {
    newServiceReferences[i]=new ServiceReferenceWrapper<T>(originalServiceReferences[i]);
  }
  return newServiceReferences;
}
