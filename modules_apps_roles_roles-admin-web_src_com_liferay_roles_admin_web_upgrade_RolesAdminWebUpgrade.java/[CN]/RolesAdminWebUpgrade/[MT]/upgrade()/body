{
  UpgradePortletId upgradePortletId=new UpgradePortletId(){
    @Override protected String[][] getRenamePortletIdsArray(){
      return new String[][]{new String[]{"128",RolesAdminPortletKeys.ROLES_ADMIN}};
    }
  }
;
  _releaseLocalService.updateRelease("com.liferay.roles.admin.web",Collections.<UpgradeProcess>singletonList(upgradePortletId),1,1,false);
}
