{
  try {
    com.liferay.portlet.tasks.model.TasksProposal returnValue=TasksProposalServiceUtil.updateProposal(groupId,proposalId,description,dueDateMonth,dueDateDay,dueDateYear,dueDateHour,dueDateMinute);
    return com.liferay.portlet.tasks.model.TasksProposalSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
