{
  layoutBranchPersistence.remove(layoutBranch);
  resourceLocalService.deleteResource(layoutBranch.getCompanyId(),LayoutBranch.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,layoutBranch.getLayoutBranchId());
  layoutRevisionLocalService.deleteLayoutBranchLayoutRevisions(layoutBranch.getLayoutBranchId());
}
