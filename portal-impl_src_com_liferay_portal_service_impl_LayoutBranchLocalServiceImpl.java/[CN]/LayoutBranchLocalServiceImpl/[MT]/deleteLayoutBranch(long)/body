{
  LayoutBranch layoutBranch=layoutBranchPersistence.findByPrimaryKey(layoutBranchId);
  layoutRevisionLocalService.deleteLayoutRevisions(layoutBranch.getLayoutSetBranchId(),layoutBranchId,layoutBranch.getPlid());
  recentLayoutBranchLocalService.deleteRecentLayoutBranches(layoutBranch.getLayoutBranchId());
  return deleteLayoutBranch(layoutBranch);
}
