{
  StringBundler sb=new StringBundler(9);
  sb.append("{clusterNode=");
  sb.append(_clusterNode);
  if (hasException()) {
    sb.append(", exception=");
    sb.append(_exception);
  }
 else {
    sb.append(", result=");
    sb.append(_result);
  }
  sb.append(", uuid=");
  sb.append(_uuid);
  sb.append("}");
  return sb.toString();
}
