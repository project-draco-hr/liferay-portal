{
  StringBundler sb=new StringBundler(9);
  sb.append("{clusterMessageType=");
  sb.append(_clusterMessageType);
  if (hasException()) {
    sb.append(", exception=");
    sb.append(_exception);
  }
  sb.append(", multicast=");
  sb.append(_multicast);
  if (!hasException()) {
    sb.append(", result=");
    sb.append(_result);
  }
  sb.append(", uuid=");
  sb.append(_uuid);
  sb.append("}");
  return sb.toString();
}
