{
  String[] groupPermissions={};
  String[] guestPermissions={ActionKeys.VIEW};
  ModelPermissions modelPermissions=ModelPermissionsFactory.create(_COMPANY_ID,1,groupPermissions,guestPermissions);
  Collection<Role> roles=modelPermissions.getRoles();
  Assert.assertEquals(1,roles.size());
  Role role=roles.iterator().next();
  Assert.assertEquals(RoleConstants.GUEST,role.getName());
  Assert.assertEquals(ListUtil.fromArray(guestPermissions),modelPermissions.getActionIds(role));
}
