{
  String[] groupPermissions={ActionKeys.VIEW};
  String[] guestPermissions={ActionKeys.VIEW};
  ModelPermissions modelPermissions=ModelPermissionsFactory.create(groupPermissions,guestPermissions);
  Collection<String> roleNames=modelPermissions.getRoleNames();
  Assert.assertEquals(2,roleNames.size());
  List<String> viewActionIdRoles=modelPermissions.getRolesWithPermission(ActionKeys.VIEW);
  Assert.assertEquals(2,viewActionIdRoles.size());
}
