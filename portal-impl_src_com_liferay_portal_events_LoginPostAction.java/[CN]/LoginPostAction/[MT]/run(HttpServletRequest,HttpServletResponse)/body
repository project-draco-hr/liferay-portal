{
  try {
    if (_log.isDebugEnabled()) {
      _log.debug("Running " + request.getRemoteUser());
    }
    HttpSession session=request.getSession();
    LiveUsers.signIn(request);
    session.removeAttribute(Globals.LOCALE_KEY);
  }
 catch (  Exception e) {
    throw new ActionException(e);
  }
}
