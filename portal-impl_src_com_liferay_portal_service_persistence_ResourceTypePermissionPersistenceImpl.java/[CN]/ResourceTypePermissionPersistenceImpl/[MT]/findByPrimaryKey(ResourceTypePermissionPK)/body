{
  ResourceTypePermission resourceTypePermission=fetchByPrimaryKey(resourceTypePermissionPK);
  if (resourceTypePermission == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + resourceTypePermissionPK);
    }
    throw new NoSuchResourceTypePermissionException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + resourceTypePermissionPK);
  }
  return resourceTypePermission;
}
