{
  ResourceTypePermission resourceTypePermission=fetchByPrimaryKey(resourceTypePermissionId);
  if (resourceTypePermission == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + resourceTypePermissionId);
    }
    throw new NoSuchResourceTypePermissionException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + resourceTypePermissionId);
  }
  return resourceTypePermission;
}
