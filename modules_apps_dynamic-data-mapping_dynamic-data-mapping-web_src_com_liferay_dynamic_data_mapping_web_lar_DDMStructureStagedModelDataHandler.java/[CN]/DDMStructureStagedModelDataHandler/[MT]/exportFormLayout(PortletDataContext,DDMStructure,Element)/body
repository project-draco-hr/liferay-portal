{
  DDMStructureVersion structureVersion=structure.getStructureVersion();
  DDMStructureLayout structureLayout=DDMStructureLayoutLocalServiceUtil.getStructureLayoutByStructureVersionId(structureVersion.getStructureVersionId());
  String formLayoutPath=ExportImportPathUtil.getModelPath(structure,"form-layout.json");
  structureElement.addAttribute("form-layout-path",formLayoutPath);
  portletDataContext.addZipEntry(formLayoutPath,structureLayout.getDefinition());
}
