{
  List<SimpleNestedSetsTreeNode> simpleNestedSetsTreeNodes=new ArrayList<>(Arrays.asList(ancestorSimpleNestedSetsTreeNodes));
  simpleNestedSetsTreeNodes.add(simpleNestedSetsTreeNode);
  Collections.sort(simpleNestedSetsTreeNodes);
  Assert.assertEquals(simpleNestedSetsTreeNodes,_nestedSetsTreeManager.getAncestors(simpleNestedSetsTreeNode));
}
