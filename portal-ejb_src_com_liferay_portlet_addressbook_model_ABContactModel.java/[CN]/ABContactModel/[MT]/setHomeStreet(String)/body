{
  if (((homeStreet == null) && (_homeStreet != null)) || ((homeStreet != null) && (_homeStreet == null)) || ((homeStreet != null) && (_homeStreet != null) && !homeStreet.equals(_homeStreet))) {
    if (!XSS_ALLOW_HOMESTREET) {
      homeStreet=XSSUtil.strip(homeStreet);
    }
    _homeStreet=homeStreet;
    setModified(true);
  }
}
