{
  if (((businessState == null) && (_businessState != null)) || ((businessState != null) && (_businessState == null)) || ((businessState != null) && (_businessState != null) && !businessState.equals(_businessState))) {
    if (!XSS_ALLOW_BUSINESSSTATE) {
      businessState=XSSUtil.strip(businessState);
    }
    _businessState=businessState;
    setModified(true);
  }
}
