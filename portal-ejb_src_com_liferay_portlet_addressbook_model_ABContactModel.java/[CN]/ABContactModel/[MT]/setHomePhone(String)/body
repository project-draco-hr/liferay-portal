{
  if (((homePhone == null) && (_homePhone != null)) || ((homePhone != null) && (_homePhone == null)) || ((homePhone != null) && (_homePhone != null) && !homePhone.equals(_homePhone))) {
    if (!XSS_ALLOW_HOMEPHONE) {
      homePhone=XSSUtil.strip(homePhone);
    }
    _homePhone=homePhone;
    setModified(true);
  }
}
