{
  if (((homePager == null) && (_homePager != null)) || ((homePager != null) && (_homePager == null)) || ((homePager != null) && (_homePager != null) && !homePager.equals(_homePager))) {
    if (!XSS_ALLOW_HOMEPAGER) {
      homePager=XSSUtil.strip(homePager);
    }
    _homePager=homePager;
    setModified(true);
  }
}
