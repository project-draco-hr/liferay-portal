{
  if (((businessCompany == null) && (_businessCompany != null)) || ((businessCompany != null) && (_businessCompany == null)) || ((businessCompany != null) && (_businessCompany != null) && !businessCompany.equals(_businessCompany))) {
    if (!XSS_ALLOW_BUSINESSCOMPANY) {
      businessCompany=XSSUtil.strip(businessCompany);
    }
    _businessCompany=businessCompany;
    setModified(true);
  }
}
