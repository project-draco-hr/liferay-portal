{
  if (((homeCountry == null) && (_homeCountry != null)) || ((homeCountry != null) && (_homeCountry == null)) || ((homeCountry != null) && (_homeCountry != null) && !homeCountry.equals(_homeCountry))) {
    if (!XSS_ALLOW_HOMECOUNTRY) {
      homeCountry=XSSUtil.strip(homeCountry);
    }
    _homeCountry=homeCountry;
    setModified(true);
  }
}
