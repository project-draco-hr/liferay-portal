{
  if (((businessPhone == null) && (_businessPhone != null)) || ((businessPhone != null) && (_businessPhone == null)) || ((businessPhone != null) && (_businessPhone != null) && !businessPhone.equals(_businessPhone))) {
    if (!XSS_ALLOW_BUSINESSPHONE) {
      businessPhone=XSSUtil.strip(businessPhone);
    }
    _businessPhone=businessPhone;
    setModified(true);
  }
}
