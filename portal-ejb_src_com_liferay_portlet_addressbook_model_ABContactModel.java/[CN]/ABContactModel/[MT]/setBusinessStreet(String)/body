{
  if (((businessStreet == null) && (_businessStreet != null)) || ((businessStreet != null) && (_businessStreet == null)) || ((businessStreet != null) && (_businessStreet != null) && !businessStreet.equals(_businessStreet))) {
    if (!XSS_ALLOW_BUSINESSSTREET) {
      businessStreet=XSSUtil.strip(businessStreet);
    }
    _businessStreet=businessStreet;
    setModified(true);
  }
}
