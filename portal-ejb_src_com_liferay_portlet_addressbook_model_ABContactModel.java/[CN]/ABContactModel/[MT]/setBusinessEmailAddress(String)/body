{
  if (((businessEmailAddress == null) && (_businessEmailAddress != null)) || ((businessEmailAddress != null) && (_businessEmailAddress == null)) || ((businessEmailAddress != null) && (_businessEmailAddress != null) && !businessEmailAddress.equals(_businessEmailAddress))) {
    if (!XSS_ALLOW_BUSINESSEMAILADDRESS) {
      businessEmailAddress=XSSUtil.strip(businessEmailAddress);
    }
    _businessEmailAddress=businessEmailAddress;
    setModified(true);
  }
}
