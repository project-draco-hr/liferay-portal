{
  if (((businessPager == null) && (_businessPager != null)) || ((businessPager != null) && (_businessPager == null)) || ((businessPager != null) && (_businessPager != null) && !businessPager.equals(_businessPager))) {
    if (!XSS_ALLOW_BUSINESSPAGER) {
      businessPager=XSSUtil.strip(businessPager);
    }
    _businessPager=businessPager;
    setModified(true);
  }
}
