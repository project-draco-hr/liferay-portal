{
  if (((homeCell == null) && (_homeCell != null)) || ((homeCell != null) && (_homeCell == null)) || ((homeCell != null) && (_homeCell != null) && !homeCell.equals(_homeCell))) {
    if (!XSS_ALLOW_HOMECELL) {
      homeCell=XSSUtil.strip(homeCell);
    }
    _homeCell=homeCell;
    setModified(true);
  }
}
