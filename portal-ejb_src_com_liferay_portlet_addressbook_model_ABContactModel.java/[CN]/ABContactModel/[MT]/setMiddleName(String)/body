{
  if (((middleName == null) && (_middleName != null)) || ((middleName != null) && (_middleName == null)) || ((middleName != null) && (_middleName != null) && !middleName.equals(_middleName))) {
    if (!XSS_ALLOW_MIDDLENAME) {
      middleName=XSSUtil.strip(middleName);
    }
    _middleName=middleName;
    setModified(true);
  }
}
