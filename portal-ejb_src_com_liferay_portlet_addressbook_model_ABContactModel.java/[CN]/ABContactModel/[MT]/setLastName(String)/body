{
  if (((lastName == null) && (_lastName != null)) || ((lastName != null) && (_lastName == null)) || ((lastName != null) && (_lastName != null) && !lastName.equals(_lastName))) {
    if (!XSS_ALLOW_LASTNAME) {
      lastName=XSSUtil.strip(lastName);
    }
    _lastName=lastName;
    setModified(true);
  }
}
