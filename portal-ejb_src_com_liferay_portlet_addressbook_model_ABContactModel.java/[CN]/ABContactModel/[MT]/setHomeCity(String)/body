{
  if (((homeCity == null) && (_homeCity != null)) || ((homeCity != null) && (_homeCity == null)) || ((homeCity != null) && (_homeCity != null) && !homeCity.equals(_homeCity))) {
    if (!XSS_ALLOW_HOMECITY) {
      homeCity=XSSUtil.strip(homeCity);
    }
    _homeCity=homeCity;
    setModified(true);
  }
}
