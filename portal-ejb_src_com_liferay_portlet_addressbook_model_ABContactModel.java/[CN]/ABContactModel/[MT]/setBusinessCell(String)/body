{
  if (((businessCell == null) && (_businessCell != null)) || ((businessCell != null) && (_businessCell == null)) || ((businessCell != null) && (_businessCell != null) && !businessCell.equals(_businessCell))) {
    if (!XSS_ALLOW_BUSINESSCELL) {
      businessCell=XSSUtil.strip(businessCell);
    }
    _businessCell=businessCell;
    setModified(true);
  }
