{
  if (((instantMessenger == null) && (_instantMessenger != null)) || ((instantMessenger != null) && (_instantMessenger == null)) || ((instantMessenger != null) && (_instantMessenger != null) && !instantMessenger.equals(_instantMessenger))) {
    if (!XSS_ALLOW_INSTANTMESSENGER) {
      instantMessenger=XSSUtil.strip(instantMessenger);
    }
    _instantMessenger=instantMessenger;
    setModified(true);
  }
}
