{
  if (((businessZip == null) && (_businessZip != null)) || ((businessZip != null) && (_businessZip == null)) || ((businessZip != null) && (_businessZip != null) && !businessZip.equals(_businessZip))) {
    if (!XSS_ALLOW_BUSINESSZIP) {
      businessZip=XSSUtil.strip(businessZip);
    }
    _businessZip=businessZip;
    setModified(true);
  }
}
