{
  if (((businessCountry == null) && (_businessCountry != null)) || ((businessCountry != null) && (_businessCountry == null)) || ((businessCountry != null) && (_businessCountry != null) && !businessCountry.equals(_businessCountry))) {
    if (!XSS_ALLOW_BUSINESSCOUNTRY) {
      businessCountry=XSSUtil.strip(businessCountry);
    }
    _businessCountry=businessCountry;
    setModified(true);
  }
}
