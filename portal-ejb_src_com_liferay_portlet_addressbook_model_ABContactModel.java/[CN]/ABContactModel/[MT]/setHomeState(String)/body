{
  if (((homeState == null) && (_homeState != null)) || ((homeState != null) && (_homeState == null)) || ((homeState != null) && (_homeState != null) && !homeState.equals(_homeState))) {
    if (!XSS_ALLOW_HOMESTATE) {
      homeState=XSSUtil.strip(homeState);
    }
    _homeState=homeState;
    setModified(true);
  }
}
