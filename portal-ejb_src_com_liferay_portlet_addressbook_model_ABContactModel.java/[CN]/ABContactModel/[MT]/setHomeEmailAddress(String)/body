{
  if (((homeEmailAddress == null) && (_homeEmailAddress != null)) || ((homeEmailAddress != null) && (_homeEmailAddress == null)) || ((homeEmailAddress != null) && (_homeEmailAddress != null) && !homeEmailAddress.equals(_homeEmailAddress))) {
    if (!XSS_ALLOW_HOMEEMAILADDRESS) {
      homeEmailAddress=XSSUtil.strip(homeEmailAddress);
    }
    _homeEmailAddress=homeEmailAddress;
    setModified(true);
  }
}
