{
  if (((businessCity == null) && (_businessCity != null)) || ((businessCity != null) && (_businessCity == null)) || ((businessCity != null) && (_businessCity != null) && !businessCity.equals(_businessCity))) {
    if (!XSS_ALLOW_BUSINESSCITY) {
      businessCity=XSSUtil.strip(businessCity);
    }
    _businessCity=businessCity;
    setModified(true);
  }
}
