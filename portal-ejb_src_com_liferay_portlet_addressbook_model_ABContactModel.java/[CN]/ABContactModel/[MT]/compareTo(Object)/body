{
  if (obj == null) {
    return -1;
  }
  ABContact abContact=(ABContact)obj;
  int value=0;
  value=getFirstName().toLowerCase().compareTo(abContact.getFirstName().toLowerCase());
  if (value != 0) {
    return value;
  }
  value=getLastName().toLowerCase().compareTo(abContact.getLastName().toLowerCase());
  if (value != 0) {
    return value;
  }
  value=getEmailAddress().toLowerCase().compareTo(abContact.getEmailAddress().toLowerCase());
  if (value != 0) {
    return value;
  }
  return 0;
}
