{
  ThemeDisplay themeDisplay=(ThemeDisplay)liferayPortletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletURL addAssetURL=null;
  if (JournalPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),ActionKeys.ADD_ARTICLE)) {
    addAssetURL=liferayPortletResponse.createRenderURL(PortletKeys.JOURNAL);
    addAssetURL.setParameter("struts_action","/journal/edit_article");
    addAssetURL.setParameter("groupId",String.valueOf(themeDisplay.getScopeGroupId()));
  }
  return addAssetURL;
}
