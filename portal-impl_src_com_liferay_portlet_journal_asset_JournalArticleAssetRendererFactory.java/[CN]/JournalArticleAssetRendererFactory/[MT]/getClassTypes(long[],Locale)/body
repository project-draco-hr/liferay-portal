{
  Map<Long,String> classTypes=new HashMap<Long,String>();
  for (  long groupId : groupIds) {
    List<DDMStructure> ddmStructures=DDMStructureLocalServiceUtil.getStructures(groupId,PortalUtil.getClassNameId(JournalArticle.class.getName()));
    for (    DDMStructure ddmStructure : ddmStructures) {
      classTypes.put(ddmStructure.getStructureId(),ddmStructure.getName(locale));
    }
  }
  return classTypes;
}
