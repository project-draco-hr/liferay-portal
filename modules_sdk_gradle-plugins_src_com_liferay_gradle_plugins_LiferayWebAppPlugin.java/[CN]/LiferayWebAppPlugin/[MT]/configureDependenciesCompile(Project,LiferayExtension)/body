{
  super.configureDependenciesCompile(project,liferayExtension);
  if (liferayExtension instanceof LiferayThemeExtension) {
    for (    String dependencyNotation : _THEME_COMPILE_DEPENDENCY_NOTATIONS) {
      GradleUtil.addDependency(project,JavaPlugin.COMPILE_CONFIGURATION_NAME,dependencyNotation);
    }
  }
  project.afterEvaluate(new Action<Project>(){
    @Override public void execute(    Project project){
      File serviceJarFile=new File(getWebAppDir(project),"WEB-INF/lib/" + project.getName() + "-service.jar");
      if (serviceJarFile.exists()) {
        GradleUtil.addDependency(project,JavaPlugin.COMPILE_CONFIGURATION_NAME,serviceJarFile);
      }
    }
  }
);
}
