{
  append("<img src=\"");
  append(HtmlUtil.escape(imageNode.getLink()));
  append("\" ");
  if (imageNode.hasAltCollectionNode()) {
    append("alt=\"");
    CollectionNode altCollectionNode=imageNode.getAltNode();
    traverse(altCollectionNode.getASTNodes());
    append("\"");
  }
  append("/>");
}
