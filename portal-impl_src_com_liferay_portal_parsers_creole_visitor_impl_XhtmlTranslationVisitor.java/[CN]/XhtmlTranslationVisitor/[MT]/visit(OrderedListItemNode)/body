{
  int nodeLevel=orderedListItemNode.getLevel();
  int diff=nodeLevel - _parentLevel;
  if (nodeLevel != _parentLevel) {
    if (nodeLevel < _parentLevel) {
      for (int i=0; i > diff; i--) {
        append("</ol>");
      }
    }
 else {
      for (int i=0; i < diff; i++) {
        append("<ol>");
      }
    }
  }
  traverse(orderedListItemNode.getChildASTNodes(),"<li>","</li>");
  _parentLevel=nodeLevel;
}
