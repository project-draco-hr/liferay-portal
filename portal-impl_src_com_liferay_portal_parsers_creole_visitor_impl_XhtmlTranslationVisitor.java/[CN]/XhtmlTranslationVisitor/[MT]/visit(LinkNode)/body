{
  write("<a href=\"");
  write(HtmlUtil.escape(linkNode.getLink()));
  write("\">");
  if (linkNode.hasAltCollectionNode()) {
    CollectionNode altCollectionNode=linkNode.getAltCollectionNode();
    traverse(altCollectionNode.getASTNodes());
  }
 else {
    write(HtmlUtil.escape(linkNode.getLink()));
  }
  write("</a>");
}
