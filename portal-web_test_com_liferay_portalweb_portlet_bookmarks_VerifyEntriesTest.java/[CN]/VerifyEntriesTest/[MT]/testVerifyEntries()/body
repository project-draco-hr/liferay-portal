{
  selenium.click(RuntimeVariables.replace("link=My Entries"));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=exact:http://www.digg.com")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
