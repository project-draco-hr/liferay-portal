{
  String testValue="testValue";
  FreeMarkerTemplate freeMarkerTemplate=new FreeMarkerTemplate(_TEMPLATE_FILE_NAME,null,null,null,null,_configuration,_templateContextHelper,_stringTemplateLoader);
  freeMarkerTemplate.put(_TEST_KEY,testValue);
  freeMarkerTemplate.prepare(null);
  Object result=freeMarkerTemplate.get(testValue);
  assertNotNull(result);
  assertTrue(result instanceof String);
  String stringResult=(String)result;
  assertEquals(testValue,stringResult);
}
