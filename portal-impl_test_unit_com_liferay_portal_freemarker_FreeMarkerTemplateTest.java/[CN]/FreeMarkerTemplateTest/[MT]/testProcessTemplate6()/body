{
  Template template=new FreeMarkerTemplate(new MockTemplateResource(_WRONG_TEMPLATE_ID),new MockTemplateResource(_WRONG_ERROR_TEMPLATE_ID),null,_configuration,_templateContextHelper,null);
  template.put(_TEST_KEY,_TEST_VALUE);
  UnsyncStringWriter unsyncStringWriter=new UnsyncStringWriter();
  try {
    template.processTemplate(unsyncStringWriter);
    fail();
  }
 catch (  Exception e) {
    if (e instanceof TemplateException) {
      String message=e.getMessage();
      assertTrue(message.contains(_WRONG_ERROR_TEMPLATE_ID));
      return;
    }
    fail();
  }
}
