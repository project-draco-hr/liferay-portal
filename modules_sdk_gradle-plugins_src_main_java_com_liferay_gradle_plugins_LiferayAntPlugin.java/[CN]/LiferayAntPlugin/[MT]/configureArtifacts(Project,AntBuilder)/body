{
  ArtifactHandler artifacts=project.getArtifacts();
  File pluginFile=project.file(antBuilder.getProperty("plugin.file"));
  artifacts.add(Dependency.ARCHIVES_CONFIGURATION,pluginFile,new Closure<Void>(null){
    @SuppressWarnings("unused") public void doCall(    ConfigurablePublishArtifact configurablePublishArtifact){
      configurablePublishArtifact.builtBy(_WAR_TASK_NAME);
    }
  }
);
}
