{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long[] deleteOrderIds=StringUtil.split(ParamUtil.getString(actionRequest,"deleteOrderIds"),0L);
  for (  long deleteOrderId : deleteOrderIds) {
    _shoppingOrderService.deleteOrder(themeDisplay.getScopeGroupId(),deleteOrderId);
  }
}
