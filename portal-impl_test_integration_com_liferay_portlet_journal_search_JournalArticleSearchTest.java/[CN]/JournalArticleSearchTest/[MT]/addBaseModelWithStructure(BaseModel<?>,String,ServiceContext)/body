{
  String xsd=DDMStructureTestUtil.getSampleStructureXSD("name");
  _ddmStructure=DDMStructureTestUtil.addDDMStructure(serviceContext.getScopeGroupId(),JournalArticle.class.getName(),xsd);
  DDMTemplate ddmTemplate=DDMTemplateTestUtil.addDDMTemplate(serviceContext.getScopeGroupId(),_ddmStructure.getStructureId());
  String content=DDMStructureTestUtil.getSampleStructuredContent("name",getSearchKeywords());
  return JournalTestUtil.addArticleWithXMLContent(serviceContext.getScopeGroupId(),content,_ddmStructure.getStructureKey(),ddmTemplate.getTemplateKey());
}
