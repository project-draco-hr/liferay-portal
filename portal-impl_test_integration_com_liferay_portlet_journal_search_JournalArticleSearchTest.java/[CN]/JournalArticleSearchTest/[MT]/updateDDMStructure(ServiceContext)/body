{
  String definition=DDMStructureTestUtil.getSampleStructureDefinition("title");
  DDMForm ddmForm=DDMFormXSDDeserializerUtil.deserialize(definition);
  DDMStructureLocalServiceUtil.updateStructure(_ddmStructure.getStructureId(),_ddmStructure.getParentStructureId(),_ddmStructure.getNameMap(),_ddmStructure.getDescriptionMap(),ddmForm,serviceContext);
}
