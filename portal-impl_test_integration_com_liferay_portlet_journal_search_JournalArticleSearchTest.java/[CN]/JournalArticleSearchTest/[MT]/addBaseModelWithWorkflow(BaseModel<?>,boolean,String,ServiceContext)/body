{
  JournalFolder folder=(JournalFolder)parentBaseModel;
  long folderId=JournalFolderConstants.DEFAULT_PARENT_FOLDER_ID;
  if (folder != null) {
    folderId=folder.getFolderId();
  }
  return JournalTestUtil.addArticleWithWorkflow(group.getGroupId(),folderId,keywords,RandomTestUtil.randomString(50),approved,serviceContext);
}
