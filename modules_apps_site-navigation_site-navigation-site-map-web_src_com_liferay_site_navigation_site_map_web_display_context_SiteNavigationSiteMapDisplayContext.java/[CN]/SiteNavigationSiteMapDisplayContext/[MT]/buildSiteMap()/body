{
  StringBundler sb=new StringBundler();
  _buildSiteMap(_themeDisplay.getLayout(),getRootLayouts(),getRootLayout(),isIncludeRootInTree(),_siteNavigationSiteMapPortletInstanceConfiguration.displayDepth(),_siteNavigationSiteMapPortletInstanceConfiguration.showCurrentPage(),_siteNavigationSiteMapPortletInstanceConfiguration.useHtmlTitle(),_siteNavigationSiteMapPortletInstanceConfiguration.showHiddenPages(),1,_themeDisplay,sb);
  return sb.toString();
}
