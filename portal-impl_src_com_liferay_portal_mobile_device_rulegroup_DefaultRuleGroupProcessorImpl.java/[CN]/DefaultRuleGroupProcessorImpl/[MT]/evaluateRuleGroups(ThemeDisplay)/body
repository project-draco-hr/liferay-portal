{
  long plid=themeDisplay.getLayout().getPlid();
  long layoutSetId=themeDisplay.getLayoutSet().getLayoutSetId();
  MDRRuleGroupInstance ruleGroupInstance=evaluateRuleGroupInstances(Layout.class.getName(),plid,themeDisplay);
  if (ruleGroupInstance == null) {
    ruleGroupInstance=evaluateRuleGroupInstances(LayoutSet.class.getName(),layoutSetId,themeDisplay);
  }
  return ruleGroupInstance;
}
