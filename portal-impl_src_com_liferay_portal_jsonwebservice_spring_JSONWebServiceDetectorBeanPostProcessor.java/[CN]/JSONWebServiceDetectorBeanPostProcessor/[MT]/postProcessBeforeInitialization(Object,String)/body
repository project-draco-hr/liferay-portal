{
  if (!PropsValues.JSON_WEB_SERVICE_ENABLED || !beanName.endsWith("Service")) {
    return bean;
  }
  JSONWebService jsonWebService=AnnotationLocator.locate(bean.getClass(),JSONWebService.class);
  if (jsonWebService != null) {
    try {
      onJSONWebServiceBean(bean,jsonWebService);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
  return bean;
}
