{
  Account account=findByPrimaryKey(accountId);
  Session session=null;
  try {
    session=openSession();
    Account[] array=new AccountImpl[3];
    array[0]=getByUserId_PrevAndNext(session,account,userId,orderByComparator,true);
    array[1]=account;
    array[2]=getByUserId_PrevAndNext(session,account,userId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
