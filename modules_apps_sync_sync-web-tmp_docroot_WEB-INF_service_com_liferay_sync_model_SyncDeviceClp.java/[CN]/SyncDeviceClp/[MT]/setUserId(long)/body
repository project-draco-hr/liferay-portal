{
  _userId=userId;
  if (_syncDeviceRemoteModel != null) {
    try {
      Class<?> clazz=_syncDeviceRemoteModel.getClass();
      Method method=clazz.getMethod("setUserId",long.class);
      method.invoke(_syncDeviceRemoteModel,userId);
    }
