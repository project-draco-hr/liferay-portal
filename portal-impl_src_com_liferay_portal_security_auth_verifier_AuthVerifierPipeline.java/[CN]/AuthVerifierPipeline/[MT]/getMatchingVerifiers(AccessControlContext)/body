{
  HttpServletRequest httpServletRequest=accessControlContext.getHttpServletRequest();
  List<AuthVerifierConfiguration> result=new ArrayList<AuthVerifierConfiguration>();
  String requestURI=httpServletRequest.getRequestURI();
  for (  AuthVerifierConfiguration authVerifierConfiguration : _verifiersPipeline) {
    if (canApply(authVerifierConfiguration,requestURI)) {
      result.add(authVerifierConfiguration);
    }
  }
  return result;
}
