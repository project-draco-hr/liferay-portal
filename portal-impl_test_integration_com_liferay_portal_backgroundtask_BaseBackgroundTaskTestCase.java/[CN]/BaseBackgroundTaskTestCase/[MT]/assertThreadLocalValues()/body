{
  Assert.assertEquals(new Long(_companyId),CompanyThreadLocal.getCompanyId());
  Assert.assertEquals(_clusterInvokeEnabled,ClusterInvokeThreadLocal.isEnabled());
  Assert.assertEquals(_defaultLocale,LocaleThreadLocal.getDefaultLocale());
  Assert.assertEquals(new Long(_groupId),GroupThreadLocal.getGroupId());
  Assert.assertEquals(_principalName,PrincipalThreadLocal.getName());
  Assert.assertEquals(_siteDefaultLocale,LocaleThreadLocal.getSiteDefaultLocale());
  Assert.assertEquals(_themeDisplayLocale,LocaleThreadLocal.getThemeDisplayLocale());
  Assert.assertNull(PrincipalThreadLocal.getPassword());
}
