{
  _bundleContext=bundleContext;
  try {
    if (SyncServiceConfigurationValues.SYNC_VERIFY) {
      VerifyUtil.verify();
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  _serviceRegistration=registerMessageListener(DestinationNames.DOCUMENT_LIBRARY_SYNC_EVENT_PROCESSOR);
  if (SyncServiceConfigurationValues.SYNC_FILE_DIFF_CACHE_ENABLED) {
    registerMessageListener(SyncDLFileVersionDiffMessageListener.DESTINATION_NAME);
  }
  consumeDLSyncEvents();
}
