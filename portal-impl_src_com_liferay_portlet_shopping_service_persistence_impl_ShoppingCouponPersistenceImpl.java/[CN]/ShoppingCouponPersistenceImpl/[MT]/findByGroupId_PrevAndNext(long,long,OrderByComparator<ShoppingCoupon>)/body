{
  ShoppingCoupon shoppingCoupon=findByPrimaryKey(couponId);
  Session session=null;
  try {
    session=openSession();
    ShoppingCoupon[] array=new ShoppingCouponImpl[3];
    array[0]=getByGroupId_PrevAndNext(session,shoppingCoupon,groupId,orderByComparator,true);
    array[1]=shoppingCoupon;
    array[2]=getByGroupId_PrevAndNext(session,shoppingCoupon,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
