{
  List<Document> documents=_document.getAllVersions();
  List<FileVersion> fileVersions=new ArrayList<FileVersion>(documents.size());
  try {
    for (    Document document : documents) {
      FileVersion fileVersion=CMISRepositoryLocalServiceUtil.toFileVersion(getRepositoryId(),document);
      fileVersions.add(fileVersion);
    }
  }
 catch (  PortalException pe) {
    throw new RepositoryException(pe);
  }
  return fileVersions;
}
