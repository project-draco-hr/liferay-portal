{
  try {
    for (    Map.Entry<String,Preference> preference : _preferences.getPreferences().entrySet()) {
      String key=preference.getKey();
      if (key.startsWith(namespace) && !_preferences.isReadOnly(key)) {
        _preferences.reset(key);
      }
    }
    _preferences.store();
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
