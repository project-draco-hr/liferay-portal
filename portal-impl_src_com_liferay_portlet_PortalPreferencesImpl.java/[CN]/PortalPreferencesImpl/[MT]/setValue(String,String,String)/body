{
  if (Validator.isNull(key) || (key.equals(_RANDOM_KEY))) {
    return;
  }
  key=_encodeKey(namespace,key);
  try {
    if (value != null) {
      _preferences.setValue(key,value);
    }
 else {
      _preferences.reset(key);
    }
    if (_signedIn) {
      _preferences.store();
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
