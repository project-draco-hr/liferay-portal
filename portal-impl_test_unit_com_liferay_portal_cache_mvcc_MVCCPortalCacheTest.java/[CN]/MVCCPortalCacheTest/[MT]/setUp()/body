{
  _portalCache=new TestPortalCache<>(_PORTAL_CACHE_NAME);
  _mvccPortalCache=new MVCCPortalCache<>((LowLevelCache<String,MVCCModel>)_portalCache);
  _testCacheListener=new TestPortalCacheListener<>();
