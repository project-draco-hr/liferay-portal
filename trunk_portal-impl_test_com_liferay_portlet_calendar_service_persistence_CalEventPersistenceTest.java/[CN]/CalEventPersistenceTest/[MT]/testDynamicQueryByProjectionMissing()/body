{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(CalEvent.class,CalEvent.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("eventId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("eventId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
