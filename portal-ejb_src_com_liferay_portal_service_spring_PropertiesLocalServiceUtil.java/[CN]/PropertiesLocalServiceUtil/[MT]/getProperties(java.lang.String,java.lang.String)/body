{
  try {
    PropertiesLocalService propertiesLocalService=PropertiesLocalServiceFactory.getService();
    return propertiesLocalService.getProperties(companyId,type);
  }
 catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
