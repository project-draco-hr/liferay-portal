{
  List<KeyValuePair> currentLanguageList=new ArrayList<>();
  String[] languageIds=getLanguageIds();
  for (  String languageId : languageIds) {
    currentLanguageList.add(new KeyValuePair(languageId,LocaleUtil.fromLanguageId(languageId).getDisplayName(_themeDisplay.getLocale())));
  }
  return currentLanguageList;
}
