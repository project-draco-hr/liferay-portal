{
  Api api=new DefaultApi10a(){
    public String getAccessTokenEndpoint(){
      return accessURL;
    }
    public String getRequestTokenEndpoint(){
      return requestURL;
    }
  }
;
  if (callbackURL == null) {
    callbackURL=OAuthConstants.OUT_OF_BAND;
  }
  _oAuthService=api.createService(key,secret,callbackURL,scope);
}
