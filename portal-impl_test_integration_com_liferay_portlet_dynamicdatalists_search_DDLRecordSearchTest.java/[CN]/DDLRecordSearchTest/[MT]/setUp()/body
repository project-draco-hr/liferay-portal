{
  Group group=GroupTestUtil.addGroup();
  DDLRecordSetTestHelper ddlRecordSetTestHelper=new DDLRecordSetTestHelper(group);
  DDMStructureTestHelper ddmStructureTestHelper=new DDMStructureTestHelper(group);
  DDLRecordSet ddlRecordSet=ddlRecordSetTestHelper.addRecordSet(ddmStructureTestHelper.addStructure(this.getClass()));
  _ddlRecordTestHelper=new DDLRecordTestHelper(ddlRecordSet,group);
  _group=group;
  _searchContext=_getSearchContext(group,ddlRecordSet);
}
