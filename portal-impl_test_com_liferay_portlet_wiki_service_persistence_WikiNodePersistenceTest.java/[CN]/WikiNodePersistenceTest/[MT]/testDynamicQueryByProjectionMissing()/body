{
  WikiNode newWikiNode=addWikiNode();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(WikiNode.class,WikiNode.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("nodeId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("nodeId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
