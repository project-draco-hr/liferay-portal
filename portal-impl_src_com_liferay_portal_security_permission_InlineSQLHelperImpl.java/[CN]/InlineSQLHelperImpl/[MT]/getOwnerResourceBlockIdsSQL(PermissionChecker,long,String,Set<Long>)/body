{
  if (ownerResourceBlockIds.size() < PropsValues.PERMISSIONS_INLINE_SQL_RESOURCE_BLOCK_QUERY_THRESHHOLD) {
    return StringUtil.merge(ownerResourceBlockIds);
  }
  return StringUtil.replace(CustomSQLUtil.get(FIND_BY_RESOURCE_BLOCK_ID),new String[]{"[$COMPANY_ID$]","[$GROUP_ID$]","[$RESOURCE_BLOCK_NAME$]","[$ROLE_ID$]"},new String[]{String.valueOf(permissionChecker.getCompanyId()),String.valueOf(checkGroupId),className,StringUtil.valueOf(permissionChecker.getOwnerRoleId())});
}
