{
  if (!PropsValues.PERMISSIONS_INLINE_SQL_CHECK_ENABLED) {
    return false;
  }
  PermissionChecker permissionChecker=PermissionThreadLocal.getPermissionChecker();
  if (permissionChecker == null) {
    throw new IllegalStateException("Unable to use InlineSQLHelper without PermissionChecker " + "being properly initialized");
  }
  if (groupId > 0) {
    if (permissionChecker.isGroupAdmin(groupId) || permissionChecker.isGroupOwner(groupId)) {
      return false;
    }
  }
 else {
    if (permissionChecker.isCompanyAdmin()) {
      return false;
    }
  }
  return true;
}
