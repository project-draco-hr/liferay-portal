{
  if (((remoteAddr == null) && (_remoteAddr != null)) || ((remoteAddr != null) && (_remoteAddr == null)) || ((remoteAddr != null) && (_remoteAddr != null) && !remoteAddr.equals(_remoteAddr))) {
    if (!XSS_ALLOW_REMOTEADDR) {
      remoteAddr=XSSUtil.strip(remoteAddr);
    }
    _remoteAddr=remoteAddr;
  }
}
