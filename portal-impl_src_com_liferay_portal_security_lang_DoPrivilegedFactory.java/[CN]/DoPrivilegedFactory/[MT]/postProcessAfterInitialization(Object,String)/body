{
  if (SecurityManagerUtil.isPACLDisabled()) {
    return bean;
  }
  if (!_shouldWrap(bean,beanName)) {
    return bean;
  }
  if (isEarlyBeanReference(beanName)) {
    if (_log.isDebugEnabled()) {
      _log.debug("Postpone wrapping as bean " + beanName + " is just an early reference now");
    }
    return bean;
  }
  if (_log.isDebugEnabled()) {
    Class<?> clazz=bean.getClass();
    _log.debug("Wrapping calls to bean " + beanName + " of type "+ clazz+ " with access controller checking");
  }
  return wrap(bean);
}
