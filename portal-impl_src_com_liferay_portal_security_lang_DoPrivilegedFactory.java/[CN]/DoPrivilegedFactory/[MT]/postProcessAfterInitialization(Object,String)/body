{
  if (SecurityManagerUtil.isPACLDisabled()) {
    return bean;
  }
  Class<?> clazz=bean.getClass();
  if (!_isDoPrivileged(clazz)) {
    return bean;
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Wrapping calls to bean " + beanName + " of type "+ clazz+ " with access controller checking");
  }
  return wrap(bean);
}
