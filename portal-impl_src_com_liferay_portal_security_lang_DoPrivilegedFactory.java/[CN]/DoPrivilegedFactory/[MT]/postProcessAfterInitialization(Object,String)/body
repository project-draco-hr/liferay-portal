{
  if (SecurityManagerUtil.isPACLDisabled()) {
    return bean;
  }
  Class<?> beanClass=bean.getClass();
  if (!isDoPrivileged(beanClass)) {
    return bean;
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Wrapping calls to bean " + beanName + " of type "+ bean.getClass()+ " with access controller checking");
  }
  return wrap(bean);
}
