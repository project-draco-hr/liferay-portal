{
  _interfaces=ArrayUtil.append(_interfaces,DoPrivilegedBean.class);
  try {
    return (T)ProxyUtil.newProxyInstance(ClassLoaderUtil.getPortalClassLoader(),_interfaces,new DoPrivilegedHandler(_bean));
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn(e,e);
    }
  }
  return _bean;
}
