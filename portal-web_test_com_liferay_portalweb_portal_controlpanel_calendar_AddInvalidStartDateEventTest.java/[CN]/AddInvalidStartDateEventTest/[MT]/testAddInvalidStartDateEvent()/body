{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Calendar")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Calendar"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("//input[@value='Add Event']"));
  selenium.waitForPageToLoad("30000");
  selenium.typeKeys("_8_title",RuntimeVariables.replace("Invalid Start Date Test Event"));
  selenium.type("_8_title",RuntimeVariables.replace("Invalid Start Date Test Event"));
  selenium.select("_8_startDateMonth",RuntimeVariables.replace("label=February"));
  selenium.select("_8_startDateDay",RuntimeVariables.replace("label=31"));
  selenium.click(RuntimeVariables.replace("//input[@value='Save']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have entered invalid data. Please try again."));
  assertTrue(selenium.isTextPresent("Please enter a valid start date."));
}
