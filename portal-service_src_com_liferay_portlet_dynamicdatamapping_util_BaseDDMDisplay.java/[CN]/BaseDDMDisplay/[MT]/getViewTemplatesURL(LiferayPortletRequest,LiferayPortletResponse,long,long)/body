{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),PortletKeys.DYNAMIC_DATA_MAPPING,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/dynamic_data_mapping/view_template");
  portletURL.setParameter("classNameId",String.valueOf(classNameId));
  portletURL.setParameter("classPK",String.valueOf(classPK));
  return portletURL.toString();
}
