{
  Session session=null;
  try {
    session=openSession();
    StringBundler query=null;
    if (orderByComparator != null) {
      query=new StringBundler(4 + (orderByComparator.getOrderByFields().length * 3));
    }
 else {
      query=new StringBundler(4);
    }
    query.append(_FILTER_SELECT_JOURNALARTICLE_WHERE);
    query.append(_FINDER_COLUMN_G_S_GROUPID_2);
    if (structureId == null) {
      query.append(_FINDER_COLUMN_G_S_STRUCTUREID_1);
    }
 else {
      if (structureId.equals(StringPool.BLANK)) {
        query.append(_FINDER_COLUMN_G_S_STRUCTUREID_3);
      }
 else {
        query.append(_FINDER_COLUMN_G_S_STRUCTUREID_2);
      }
    }
    if (orderByComparator != null) {
      appendOrderByComparator(query,_ORDER_BY_ENTITY_ALIAS,orderByComparator);
    }
 else {
      query.append(JournalArticleModelImpl.ORDER_BY_JPQL);
    }
    String sql=InlineSQLHelperUtil.replacePermissionCheck(query.toString(),JournalArticle.class.getName(),_FILTER_COLUMN_ID,_FILTER_COLUMN_USERID,groupId);
    SQLQuery q=session.createSQLQuery(sql);
    q.addEntity(_ENTITY_ALIAS,JournalArticleImpl.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(groupId);
    if (structureId != null) {
      qPos.add(structureId);
    }
    return (List<JournalArticle>)QueryUtil.list(q,getDialect(),start,end);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
