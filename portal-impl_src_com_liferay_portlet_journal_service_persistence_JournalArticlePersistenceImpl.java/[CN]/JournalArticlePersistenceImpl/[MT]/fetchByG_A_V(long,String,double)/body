{
  Session session=null;
  try {
    session=openSession();
    StringMaker query=new StringMaker();
    query.append("FROM com.liferay.portlet.journal.model.JournalArticle WHERE ");
    query.append("groupId = ?");
    query.append(" AND ");
    if (articleId == null) {
      query.append("articleId IS NULL");
    }
 else {
      query.append("articleId = ?");
    }
    query.append(" AND ");
    query.append("version = ?");
    query.append(" ");
    query.append("ORDER BY ");
    query.append("articleId ASC").append(", ");
    query.append("version DESC");
    Query q=session.createQuery(query.toString());
    q.setCacheable(true);
    int queryPos=0;
    q.setLong(queryPos++,groupId);
    if (articleId != null) {
      q.setString(queryPos++,articleId);
    }
    q.setDouble(queryPos++,version);
    List list=q.list();
    if (list.size() == 0) {
      return null;
    }
    JournalArticle journalArticle=(JournalArticle)list.get(0);
    return journalArticle;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
