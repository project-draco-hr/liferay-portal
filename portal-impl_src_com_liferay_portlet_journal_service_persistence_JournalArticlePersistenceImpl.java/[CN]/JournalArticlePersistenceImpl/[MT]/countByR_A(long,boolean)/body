{
  Object[] finderArgs=new Object[]{new Long(resourcePrimKey),Boolean.valueOf(approved)};
  Long count=(Long)FinderCacheUtil.getResult(FINDER_PATH_COUNT_BY_R_A,finderArgs,this);
  if (count == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT COUNT(journalArticle) ");
      query.append("FROM JournalArticle journalArticle WHERE ");
      query.append("journalArticle.resourcePrimKey = ?");
      query.append(" AND ");
      query.append("journalArticle.approved = ?");
      query.append(" ");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(resourcePrimKey);
      qPos.add(approved);
      count=(Long)q.uniqueResult();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (count == null) {
        count=Long.valueOf(0);
      }
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_R_A,finderArgs,count);
      closeSession(session);
    }
  }
  return count.intValue();
}
