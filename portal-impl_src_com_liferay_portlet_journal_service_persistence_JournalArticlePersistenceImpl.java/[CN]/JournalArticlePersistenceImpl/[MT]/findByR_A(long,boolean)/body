{
  Object[] finderArgs=new Object[]{new Long(resourcePrimKey),Boolean.valueOf(approved)};
  List<JournalArticle> list=(List<JournalArticle>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_R_A,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT journalArticle FROM JournalArticle journalArticle WHERE ");
      query.append("journalArticle.resourcePrimKey = ?");
      query.append(" AND ");
      query.append("journalArticle.approved = ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("journalArticle.articleId ASC, ");
      query.append("journalArticle.version DESC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(resourcePrimKey);
      qPos.add(approved);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<JournalArticle>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_R_A,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
