{
  int count=countByG_UT_S(groupId,urlTitle,status);
  List<JournalArticle> list=findByG_UT_S(groupId,urlTitle,status,count - 1,count,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(7);
    msg.append("No JournalArticle exists with the key {");
    msg.append("groupId=" + groupId);
    msg.append(", ");
    msg.append("urlTitle=" + urlTitle);
    msg.append(", ");
    msg.append("status=" + status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchArticleException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
