      FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_G_UT,args);
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_G_UT,args);
    }
    if ((journalArticleModelImpl.getColumnBitmask() & FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_G_S.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{Long.valueOf(journalArticleModelImpl.getOriginalGroupId()),journalArticleModelImpl.getOriginalStructureId()};
      FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_G_S,args);
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_G_S,args);
