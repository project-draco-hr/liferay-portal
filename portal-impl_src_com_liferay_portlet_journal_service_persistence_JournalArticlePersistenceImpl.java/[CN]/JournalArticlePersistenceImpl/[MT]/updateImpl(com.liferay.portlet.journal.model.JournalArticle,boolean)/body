      FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_G_UT_ST,args);
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_G_UT_ST,args);
