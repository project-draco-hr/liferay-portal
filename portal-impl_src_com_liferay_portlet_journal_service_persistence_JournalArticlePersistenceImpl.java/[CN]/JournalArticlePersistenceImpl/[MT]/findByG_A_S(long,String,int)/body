{
  Object[] finderArgs=new Object[]{new Long(groupId),articleId,new Integer(status)};
  List<JournalArticle> list=(List<JournalArticle>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_G_A_S,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBundler query=new StringBundler();
      query.append(_SQL_SELECT_JOURNALARTICLE_WHERE);
      query.append("journalArticle.groupId = ?");
      query.append(" AND ");
      if (articleId == null) {
        query.append("journalArticle.articleId IS NULL");
      }
 else {
        if (articleId.equals(StringPool.BLANK)) {
          query.append("(journalArticle.articleId IS NULL OR ");
        }
        query.append("journalArticle.articleId = ?");
        if (articleId.equals(StringPool.BLANK)) {
          query.append(")");
        }
      }
      query.append(" AND ");
      query.append("journalArticle.status = ?");
      query.append(" ORDER BY ");
      query.append("journalArticle.articleId ASC, ");
      query.append("journalArticle.version DESC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      if (articleId != null) {
        qPos.add(articleId);
      }
      qPos.add(status);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<JournalArticle>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_G_A_S,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
