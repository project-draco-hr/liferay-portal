{
  JournalArticle journalArticle=(JournalArticle)EntityCacheUtil.getResult(JournalArticleModelImpl.ENTITY_CACHE_ENABLED,JournalArticleImpl.class,id);
  if (journalArticle == _nullJournalArticle) {
    return null;
  }
  if (journalArticle == null) {
    Session session=null;
    try {
      session=openSession();
      journalArticle=(JournalArticle)session.get(JournalArticleImpl.class,Long.valueOf(id));
      if (journalArticle != null) {
        cacheResult(journalArticle);
      }
 else {
        EntityCacheUtil.putResult(JournalArticleModelImpl.ENTITY_CACHE_ENABLED,JournalArticleImpl.class,id,_nullJournalArticle);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(JournalArticleModelImpl.ENTITY_CACHE_ENABLED,JournalArticleImpl.class,id);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return journalArticle;
}
