{
  JournalArticle result=(JournalArticle)EntityCacheUtil.getResult(JournalArticleModelImpl.ENTITY_CACHE_ENABLED,JournalArticle.class,id,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      JournalArticle journalArticle=(JournalArticle)session.get(JournalArticleImpl.class,new Long(id));
      cacheResult(journalArticle);
      return journalArticle;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (JournalArticle)result;
  }
}
