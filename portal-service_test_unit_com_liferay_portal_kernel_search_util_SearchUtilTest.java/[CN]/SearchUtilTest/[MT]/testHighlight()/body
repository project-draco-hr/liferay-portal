{
  StringBundler sb=new StringBundler(7);
  sb.append(SearchUtil.HIGHLIGHTS[0]);
  sb.append("Hello");
  sb.append(SearchUtil.HIGHLIGHTS[1]);
  sb.append(" World ");
  sb.append(SearchUtil.HIGHLIGHTS[0]);
  sb.append("Liferay");
  sb.append(SearchUtil.HIGHLIGHTS[1]);
  Assert.assertEquals(sb.toString(),SearchUtil.highlight("Hello World Liferay",new String[]{"Hello","Liferay"}));
}
