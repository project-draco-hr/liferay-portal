{
  clusterLinkConfiguration=Configurable.createConfigurable(ClusterLinkConfiguration.class,properties);
  if (!clusterLinkConfiguration.enabled() && (_clusterChannel != null)) {
    deactivate();
  }
 else   if (clusterLinkConfiguration.enabled() && (_clusterChannel == null)) {
    String controlChannelProperties=getControlChannelProperties(properties);
    initialize(controlChannelProperties);
  }
}
