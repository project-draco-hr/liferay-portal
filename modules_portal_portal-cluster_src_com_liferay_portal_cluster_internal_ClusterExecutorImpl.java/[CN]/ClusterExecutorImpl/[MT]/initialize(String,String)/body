{
  if (!isEnabled()) {
    return;
  }
  if (Validator.isNull(controlChannelProperties)) {
    throw new IllegalStateException(PropsKeys.CLUSTER_LINK_CHANNEL_PROPERTIES_CONTROL + " or " + ClusterPropsKeys.CHANNEL_PROPERTIES_CONTROL+ " not set.");
  }
  if (Validator.isNull(controlChannelName)) {
    throw new IllegalStateException(PropsKeys.CLUSTER_LINK_CHANNEL_NAME_CONTROL + " or " + ClusterPropsKeys.CHANNEL_NAME_CONTROL+ " not set.");
  }
  _executorService=_portalExecutorManager.getPortalExecutor(ClusterExecutorImpl.class.getName());
  ClusterRequestReceiver clusterReceiver=new ClusterRequestReceiver(this);
  _clusterChannel=_clusterChannelFactory.createClusterChannel(controlChannelProperties,controlChannelName,clusterReceiver);
  ClusterNode localClusterNode=new ClusterNode(generateClusterNodeId(),_clusterChannel.getBindInetAddress());
  _localClusterNodeStatus=new ClusterNodeStatus(localClusterNode,_clusterChannel.getLocalAddress());
  _memberJoined(_localClusterNodeStatus);
  sendNotifyRequest();
  clusterReceiver.openLatch();
  configurePortalInstanceCommunications();
  manageDebugClusterEventListener();
}
