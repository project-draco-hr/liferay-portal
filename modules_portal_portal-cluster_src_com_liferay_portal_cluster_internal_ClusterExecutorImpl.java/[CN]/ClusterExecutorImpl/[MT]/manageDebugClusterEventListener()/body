{
  if (clusterLinkConfiguration.debugEnabled() && (_debugClusterEventListener == null)) {
    _debugClusterEventListener=new DebuggingClusterEventListenerImpl();
    addClusterEventListener(_debugClusterEventListener);
  }
 else   if (!clusterLinkConfiguration.debugEnabled() && (_debugClusterEventListener != null)) {
    removeClusterEventListener(_debugClusterEventListener);
  }
}
