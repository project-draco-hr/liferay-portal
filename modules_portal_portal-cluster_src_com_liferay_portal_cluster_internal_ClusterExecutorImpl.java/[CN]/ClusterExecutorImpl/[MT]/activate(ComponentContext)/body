{
  clusterExecutorConfiguration=Configurable.createConfigurable(ClusterExecutorConfiguration.class,componentContext.getProperties());
  String controlChannelProperties=getControlChannelProperties(componentContext.getProperties());
  BundleContext bundleContext=componentContext.getBundleContext();
  ClusterExecutorPortalInetSocketAddressEventListener clusterExecutorPortalInetSocketAddressEventListener=new ClusterExecutorPortalInetSocketAddressEventListener();
  _serviceRegistration=bundleContext.registerService(PortalInetSocketAddressEventListener.class,clusterExecutorPortalInetSocketAddressEventListener,new HashMapDictionary<String,Object>());
  initialize(controlChannelProperties);
}
