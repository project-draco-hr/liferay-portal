{
  int userNotificationTimeout=0;
  SPAConfiguration spaConfiguration=_spaConfigurationActivator.getSPAConfiguration();
  userNotificationTimeout=GetterUtil.getInteger(spaConfiguration.userNotificationTimeout(),userNotificationTimeout);
  if (userNotificationTimeout > 0) {
    userNotificationTimeout*=Time.SECOND;
  }
  return userNotificationTimeout;
}
