{
  long cacheExpirationTime=-1;
  SPAConfiguration configuration=_spaConfigurationActivator.getSPAConfiguration();
  cacheExpirationTime=GetterUtil.getLong(configuration.cacheExpirationTime(),cacheExpirationTime);
  if (cacheExpirationTime > -1) {
    cacheExpirationTime*=Time.MINUTE;
  }
  return cacheExpirationTime;
}
