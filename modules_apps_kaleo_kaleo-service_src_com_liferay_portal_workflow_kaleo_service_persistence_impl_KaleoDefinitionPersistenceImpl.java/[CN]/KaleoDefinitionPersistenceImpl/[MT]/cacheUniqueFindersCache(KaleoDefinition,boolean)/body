{
  if (isNew) {
    Object[] args=new Object[]{kaleoDefinition.getCompanyId(),kaleoDefinition.getName(),kaleoDefinition.getVersion()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_C_N_V,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_N_V,args,kaleoDefinition);
  }
 else {
    KaleoDefinitionModelImpl kaleoDefinitionModelImpl=(KaleoDefinitionModelImpl)kaleoDefinition;
    if ((kaleoDefinitionModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_C_N_V.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{kaleoDefinition.getCompanyId(),kaleoDefinition.getName(),kaleoDefinition.getVersion()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_C_N_V,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_N_V,args,kaleoDefinition);
    }
  }
}
