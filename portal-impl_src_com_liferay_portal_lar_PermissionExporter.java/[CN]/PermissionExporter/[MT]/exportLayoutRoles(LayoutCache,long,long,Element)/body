{
  String resourceName=Layout.class.getName();
  exportGroupRoles(layoutCache,companyId,groupId,resourceName,"community",rolesEl);
  exportUserRoles(layoutCache,companyId,groupId,resourceName,rolesEl);
  exportInheritedRoles(layoutCache,companyId,groupId,resourceName,"organization",rolesEl);
  exportInheritedRoles(layoutCache,companyId,groupId,resourceName,"user-group",rolesEl);
}
