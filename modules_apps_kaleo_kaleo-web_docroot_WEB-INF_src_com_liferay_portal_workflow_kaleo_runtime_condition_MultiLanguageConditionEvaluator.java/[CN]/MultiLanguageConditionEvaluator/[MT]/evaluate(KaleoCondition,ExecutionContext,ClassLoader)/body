{
  ScriptLanguage scriptLanguage=ScriptLanguage.parse(kaleoCondition.getScriptLanguage());
  ConditionEvaluator conditionEvaluator=_conditionEvaluators.get(scriptLanguage);
  if (conditionEvaluator == null) {
    throw new IllegalArgumentException("No condition evaluator found for script language " + scriptLanguage);
  }
  return conditionEvaluator.evaluate(kaleoCondition,executionContext,classLoaders);
}
