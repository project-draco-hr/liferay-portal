{
  List<Theme> themeList=new ArrayList<Theme>();
  try {
    for (    String xml : xmls) {
      Set<Theme> themes=_readThemes(servletContextName,servletContext,themesPath,loadFromServletContext,xml,pluginPackage);
      for (      Theme theme : themes) {
        if (!themeList.contains(theme)) {
          themeList.add(theme);
        }
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  _themesPool.clear();
  return themeList;
}
