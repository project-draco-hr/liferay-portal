{
  List<String> themeIds=new ArrayList<String>();
  try {
    for (int i=0; i < xmls.length; i++) {
      Set<String> themes=_readThemes(servletContextName,servletContext,themesPath,loadFromServletContext,xmls[i],pluginPackage);
      Iterator<String> itr=themes.iterator();
      while (itr.hasNext()) {
        String themeId=itr.next();
        if (!themeIds.contains(themeId)) {
          themeIds.add(themeId);
        }
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  _themesPool.clear();
  return themeIds;
}
