{
  List<Theme> themes=new ArrayList<Theme>();
  try {
    for (    String xml : xmls) {
      themes.addAll(_readThemes(servletContextName,servletContext,themesPath,loadFromServletContext,xml,pluginPackage));
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  _themesPool.clear();
  return ListUtil.unique(themes);
}
