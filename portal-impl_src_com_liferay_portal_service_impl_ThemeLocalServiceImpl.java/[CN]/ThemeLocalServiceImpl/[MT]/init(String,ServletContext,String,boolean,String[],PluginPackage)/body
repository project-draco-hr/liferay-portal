{
  Set<Theme> themes=new LinkedHashSet<Theme>();
  try {
    for (    String xml : xmls) {
      themes.addAll(_readThemes(servletContextName,servletContext,themesPath,loadFromServletContext,xml,pluginPackage));
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  _themesPool.clear();
  return new ArrayList<Theme>(themes);
}
