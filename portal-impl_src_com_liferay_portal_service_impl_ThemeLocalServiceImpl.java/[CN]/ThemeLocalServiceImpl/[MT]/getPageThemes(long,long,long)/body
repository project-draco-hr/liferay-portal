{
  List<Theme> themes=getThemes(companyId);
  themes=PluginUtil.restrictPlugins(themes,companyId,userId);
  Iterator<Theme> itr=themes.iterator();
  while (itr.hasNext()) {
    Theme theme=itr.next();
    if (!theme.isPageTheme() || !theme.isGroupAvailable(groupId)) {
      itr.remove();
    }
  }
  return themes;
}
