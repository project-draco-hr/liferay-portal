{
  Iterator<Map.Entry<String,Theme>> itr=_themes.entrySet().iterator();
  List<Theme> warThemes=new ArrayList();
  while (itr.hasNext()) {
    Map.Entry<String,Theme> entry=itr.next();
    Theme theme=entry.getValue();
    if (theme.isWARFile()) {
      warThemes.add(theme);
    }
  }
  return warThemes;
}
