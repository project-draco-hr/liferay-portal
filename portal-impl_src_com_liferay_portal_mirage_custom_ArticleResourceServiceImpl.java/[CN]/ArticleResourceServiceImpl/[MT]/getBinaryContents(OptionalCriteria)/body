{
  List<BinaryContent> binaryContents=new ArrayList<BinaryContent>();
  try {
    OptionalJournalResourceCriteria journalCriteria=(OptionalJournalResourceCriteria)criteria;
    String groupId=_getValue(journalCriteria,OptionalJournalResourceCriteria.GROUP_ID);
    List<JournalArticleResource> journalResources=_getArticleResources(Long.parseLong(groupId));
    _populateBinaryContents(binaryContents,journalResources);
    return binaryContents;
  }
 catch (  SystemException ex) {
    throw new CMSException(ex.getMessage(),ex);
  }
}
