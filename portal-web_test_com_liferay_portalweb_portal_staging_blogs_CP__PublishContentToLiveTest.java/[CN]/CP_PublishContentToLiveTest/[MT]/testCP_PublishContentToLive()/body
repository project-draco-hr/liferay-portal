    Thread.sleep(1000);
  }
  selenium.clickAt("_88_PORTLET_DATA_33",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Pages")) {
        break;
      }
    }
 catch (    Exception e) {
