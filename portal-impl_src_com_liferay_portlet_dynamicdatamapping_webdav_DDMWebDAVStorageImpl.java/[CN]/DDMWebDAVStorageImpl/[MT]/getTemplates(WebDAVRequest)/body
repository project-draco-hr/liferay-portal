{
  List<Resource> resources=new ArrayList<Resource>();
  long groupId=webDavRequest.getGroupId();
  long classNameId=PortalUtil.getClassNameId(DDMStructure.class.getName());
  List<DDMTemplate> templates=DDMTemplateLocalServiceUtil.getTemplates(groupId,classNameId);
  for (  DDMTemplate template : templates) {
    Resource resource=toResource(webDavRequest,template,true);
    resources.add(resource);
  }
  return resources;
}
