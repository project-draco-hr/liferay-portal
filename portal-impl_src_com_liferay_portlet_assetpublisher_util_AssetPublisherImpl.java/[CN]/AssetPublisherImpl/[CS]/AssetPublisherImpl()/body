{
  for (  String queryProcessorClassName : PropsValues.ASSET_PUBLISHER_QUERY_PROCESSORS) {
    try {
      AssetQueryProcessor queryProcessor=(AssetQueryProcessor)InstanceFactory.newInstance(queryProcessorClassName);
      _queryProcessors.put(queryProcessorClassName,queryProcessor);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
}
