{
  if (fromVersionLabel.equals(toVersionLabel)) {
    return;
  }
  recoverAndRetryOnFailure(createDeleteFileStoreAction(companyId,repositoryId,fileName,toVersionLabel),new StoreAction(){
    @Override public void execute() throws PortalException {
      _store.copyFileVersion(companyId,repositoryId,fileName,fromVersionLabel,toVersionLabel);
    }
  }
);
}
