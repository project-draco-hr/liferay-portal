{
  String taskName="save" + StringUtil.capitalize(taskCache.getName()) + "Cache";
  Copy copy=GradleUtil.addTask(taskCache.getProject(),taskName,Copy.class);
  copy.dependsOn(BasePlugin.CLEAN_TASK_NAME + StringUtil.capitalize(taskName));
  copy.from(taskCache.getFiles());
  copy.into(taskCache.getCacheDir());
  return copy;
}
