{
  MembershipRequest membershipRequest=fetchByPrimaryKey(membershipRequestId);
  if (membershipRequest == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + membershipRequestId);
    }
    throw new NoSuchMembershipRequestException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + membershipRequestId);
  }
  return membershipRequest;
}
