{
  boolean remoteInvoke=AggregatedCacheListener.getRemoteInvokeThreadLocal();
  AggregatedCacheListener.setRemoteInvokeThreadLocal(doNotNotifyCacheReplicators);
  try {
    return super.remove(key,doNotNotifyCacheReplicators);
  }
  finally {
    AggregatedCacheListener.setRemoteInvokeThreadLocal(remoteInvoke);
  }
}
