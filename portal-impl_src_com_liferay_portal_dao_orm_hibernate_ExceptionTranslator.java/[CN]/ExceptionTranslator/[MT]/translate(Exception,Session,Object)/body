{
  if (e instanceof org.hibernate.StaleObjectStateException) {
    BaseModel<?> baseModel=(BaseModel<?>)object;
    Object currentObject=session.get(object.getClass(),baseModel.getPrimaryKeyObj());
    return new ORMException(object + " is stale in comparison to " + currentObject,e);
  }
 else {
    return new ORMException(e);
  }
}
