{
  KBComment kbComment=(KBComment)entityCache.getResult(KBCommentModelImpl.ENTITY_CACHE_ENABLED,KBCommentImpl.class,primaryKey);
  if (kbComment == _nullKBComment) {
    return null;
  }
  if (kbComment == null) {
    Session session=null;
    try {
      session=openSession();
      kbComment=(KBComment)session.get(KBCommentImpl.class,primaryKey);
      if (kbComment != null) {
        cacheResult(kbComment);
      }
 else {
        entityCache.putResult(KBCommentModelImpl.ENTITY_CACHE_ENABLED,KBCommentImpl.class,primaryKey,_nullKBComment);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(KBCommentModelImpl.ENTITY_CACHE_ENABLED,KBCommentImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return kbComment;
}
