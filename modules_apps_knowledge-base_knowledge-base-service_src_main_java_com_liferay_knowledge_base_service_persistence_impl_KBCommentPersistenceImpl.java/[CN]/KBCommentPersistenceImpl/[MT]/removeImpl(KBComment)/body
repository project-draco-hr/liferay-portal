{
  kbComment=toUnwrappedModel(kbComment);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(kbComment)) {
      kbComment=(KBComment)session.get(KBCommentImpl.class,kbComment.getPrimaryKeyObj());
    }
    if (kbComment != null) {
      session.delete(kbComment);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (kbComment != null) {
    clearCache(kbComment);
  }
  return kbComment;
}
