{
  RTLCSSConverter rtlCssConverter=new RTLCSSConverter();
  Assert.assertNotNull(rtlCssConverter);
  Assert.assertEquals("p{margin-left: 5px}",rtlCssConverter.process("/* @noflip */ p {margin-left: 5px}"));
  Assert.assertEquals("p{margin-left: 5px}",rtlCssConverter.process("/*@noflip*/p {margin-left: 5px}"));
  Assert.assertEquals("p{margin-right:5px}",rtlCssConverter.process("p {margin-left: 5px;/*@noflip*/}"));
  Assert.assertEquals("p{margin-right:5px}",rtlCssConverter.process("p{margin-left: 5px}\n/*@noflip*/"));
}
