{
  RTLCSSConverter rtlCssConverter=new RTLCSSConverter();
  Assert.assertNotNull(rtlCssConverter);
  Assert.assertEquals("p{direction:ltr}",rtlCssConverter.process("p{direction:rtl}"));
  Assert.assertEquals("p{direction:rtl}",rtlCssConverter.process("p{direction:ltr}"));
  Assert.assertEquals("p{direction:foo}",rtlCssConverter.process("p{direction:foo}"));
}
