{
  SocialActivitySet socialActivitySet=socialActivitySetPersistence.findByPrimaryKey(activitySetId);
  socialActivitySet.setActivityCount(socialActivitySet.getActivityCount() + 1);
  SocialActivity activity=socialActivityPersistence.findByPrimaryKey(activityId);
  socialActivitySet.setModifiedDate(activity.getCreateDate());
  return socialActivitySetPersistence.update(socialActivitySet);
}
