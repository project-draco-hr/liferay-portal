{
  ArrayList<JGroupSerializable> jGroupSerializables=new ArrayList<JGroupSerializable>();
  for (  Object eventMessage : eventMessages) {
    if (eventMessage instanceof JGroupEventMessage) {
      JGroupEventMessage jGroupEventMessage=(JGroupEventMessage)eventMessage;
      JGroupSerializable jGroupSerializable=toJGroupSerializable(jGroupEventMessage);
      jGroupSerializables.add(jGroupSerializable);
    }
 else {
      if (_log.isDebugEnabled()) {
        _log.debug(eventMessage + "is not a JGroupEventMessage type");
      }
    }
  }
  try {
    _jChannel.send(address,null,jGroupSerializables);
  }
 catch (  Throwable t) {
    throw new RemoteException(t.getMessage());
  }
}
