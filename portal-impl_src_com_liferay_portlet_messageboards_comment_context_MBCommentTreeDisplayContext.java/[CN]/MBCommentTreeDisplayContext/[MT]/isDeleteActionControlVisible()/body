{
  return MBDiscussionPermission.contains(_discussionRequestHelper.getPermissionChecker(),_discussionRequestHelper.getCompanyId(),_discussionRequestHelper.getScopeGroupId(),_discussionTaglibHelper.getPermissionClassName(),_discussionTaglibHelper.getPermissionClassPK(),_comment.getCommentId(),_comment.getUserId(),ActionKeys.DELETE_DISCUSSION);
}
