{
  Attributes attributes=new BasicAttributes(true);
  Attribute objectClass=new BasicAttribute(_OBJECT_CLASS);
  String postfix=LDAPSettingsUtil.getPropertyPostfix(ldapServerId);
  String[] defaultObjectClasses=PrefsPropsUtil.getStringArray(user.getCompanyId(),PropsKeys.LDAP_USER_DEFAULT_OBJECT_CLASSES + postfix,StringPool.COMMA);
  for (int i=0; i < defaultObjectClasses.length; i++) {
    objectClass.add(defaultObjectClasses[i]);
  }
  attributes.put(objectClass);
  addAttributeMapping(userMappings.getProperty(UserConverterKeys.FIRST_NAME),user.getFirstName(),attributes);
  addAttributeMapping(userMappings.getProperty(UserConverterKeys.LAST_NAME),user.getLastName(),attributes);
  addAttributeMapping(userMappings.getProperty(UserConverterKeys.PASSWORD),user.getPasswordUnencrypted(),attributes);
  addAttributeMapping(userMappings.getProperty(UserConverterKeys.EMAIL_ADDRESS),user.getEmailAddress(),attributes);
  return attributes;
}
