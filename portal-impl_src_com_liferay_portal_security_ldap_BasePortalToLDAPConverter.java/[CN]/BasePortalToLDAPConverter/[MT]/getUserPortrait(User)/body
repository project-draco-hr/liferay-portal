{
  byte[] imageBytes=null;
  if (user.getPortraitId() != 0) {
    Image image=null;
    try {
      image=ImageLocalServiceUtil.getImage(user.getPortraitId());
      if (image != null) {
        imageBytes=image.getTextObj();
      }
    }
 catch (    Exception e) {
    }
  }
  return imageBytes;
}
