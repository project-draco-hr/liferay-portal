{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),KaleoLogLocalService.class.getName());
    if (invokableLocalService instanceof KaleoLogLocalService) {
      _service=(KaleoLogLocalService)invokableLocalService;
    }
 else {
      _service=new KaleoLogLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(KaleoLogLocalServiceUtil.class,"_service");
  }
  return _service;
}
