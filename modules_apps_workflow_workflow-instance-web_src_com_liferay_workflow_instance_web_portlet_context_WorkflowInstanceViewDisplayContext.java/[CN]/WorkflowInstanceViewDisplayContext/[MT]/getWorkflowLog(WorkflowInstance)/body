{
  List<WorkflowLog> workflowLogs=WorkflowLogManagerUtil.getWorkflowLogsByWorkflowInstance(_company.getCompanyId(),workflowInstance.getWorkflowInstanceId(),null,0,1,WorkflowComparatorFactoryUtil.getLogCreateDateComparator());
  WorkflowLog workflowLog=null;
  if (!workflowLogs.isEmpty()) {
    workflowLog=workflowLogs.get(0);
  }
  return workflowLog;
}
