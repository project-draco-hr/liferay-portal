{
  selenium.open("/web/socialrelationsn1/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Summary Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Summary Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//p[@class='add-as-friend']/span/a[2]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("//p[@class='add-as-friend']/span/a[2]",RuntimeVariables.replace("ADD AS FRIEND"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Friend Requested"),selenium.getText("//div[@class='portlet-msg-info add-as-friend pending']"));
  assertEquals(RuntimeVariables.replace("Your request processed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
}
