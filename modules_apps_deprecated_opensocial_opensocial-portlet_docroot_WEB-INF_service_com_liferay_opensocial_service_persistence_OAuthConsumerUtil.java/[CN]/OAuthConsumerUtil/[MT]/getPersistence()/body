{
  if (_persistence == null) {
    _persistence=(OAuthConsumerPersistence)PortletBeanLocatorUtil.locate(com.liferay.opensocial.service.ClpSerializer.getServletContextName(),OAuthConsumerPersistence.class.getName());
    ReferenceRegistry.registerReference(OAuthConsumerUtil.class,"_persistence");
  }
  return _persistence;
}
