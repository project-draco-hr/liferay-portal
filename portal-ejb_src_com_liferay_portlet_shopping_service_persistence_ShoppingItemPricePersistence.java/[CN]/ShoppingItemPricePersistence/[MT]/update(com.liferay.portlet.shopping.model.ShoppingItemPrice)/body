{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(shoppingItemPrice);
    session.flush();
    shoppingItemPrice.setNew(false);
    return shoppingItemPrice;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
