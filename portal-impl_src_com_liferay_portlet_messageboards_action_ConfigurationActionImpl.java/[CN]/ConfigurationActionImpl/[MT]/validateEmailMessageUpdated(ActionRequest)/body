{
  boolean emailMessageUpdatedEnabled=GetterUtil.getBoolean(getParameter(actionRequest,"emailMessageUpdatedEnabled"));
  String emailMessageUpdatedSubject=getParameter(actionRequest,"emailMessageUpdatedSubject");
  String emailMessageUpdatedBody=getParameter(actionRequest,"emailMessageUpdatedBody");
  if (emailMessageUpdatedEnabled) {
    if (Validator.isNull(emailMessageUpdatedSubject)) {
      SessionErrors.add(actionRequest,"emailMessageUpdatedSubject");
    }
 else     if (Validator.isNull(emailMessageUpdatedBody)) {
      SessionErrors.add(actionRequest,"emailMessageUpdatedBody");
    }
  }
}
