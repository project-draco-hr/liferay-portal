{
  if (_showOpen) {
    return true;
  }
  if (isPersistState()) {
    String state=SessionClicks.get(request,PanelCategory.class.getName() + getId(),"closed");
    if (Validator.equals(state,"open")) {
      return true;
    }
  }
  if (panelApps.isEmpty()) {
    return false;
  }
  return _panelCategory.isActive(request,panelCategoryHelper,group);
}
