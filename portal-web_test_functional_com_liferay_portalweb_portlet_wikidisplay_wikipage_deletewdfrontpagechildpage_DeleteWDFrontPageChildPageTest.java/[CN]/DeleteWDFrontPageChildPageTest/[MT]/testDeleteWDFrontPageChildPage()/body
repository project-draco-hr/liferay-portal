{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Wiki Display Test Page",RuntimeVariables.replace("Wiki Display Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Wiki FrontPage Content"),selenium.getText("//div[@class='wiki-body']/p"));
  assertEquals(RuntimeVariables.replace("Children Pages"),selenium.getText("//div[@class='child-pages']/h2"));
  assertEquals(RuntimeVariables.replace("Wiki FrontPage ChildPage Title"),selenium.getText("//div[@class='child-pages']/ul/li/a"));
  selenium.clickAt("//div[@class='child-pages']/ul/li/a",RuntimeVariables.replace("Wiki FrontPage ChildPage Title"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForVisible("//div[@class='page-actions top-actions']/span/a[contains(.,'Details')]");
  selenium.clickAt("//div[@class='page-actions top-actions']/span/a[contains(.,'Details')]",RuntimeVariables.replace("Details"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForVisible("//ul[@class='lfr-component taglib-icon-list']/li/a[contains(.,'Delete')]");
  selenium.click(RuntimeVariables.replace("//ul[@class='lfr-component taglib-icon-list']/li/a[contains(.,'Delete')]"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this[\\s\\S] It will be deleted immediately.$"));
  assertFalse(selenium.isTextPresent("Children Pages"));
  assertFalse(selenium.isTextPresent("Wiki FrontPage ChildPage Title"));
}
