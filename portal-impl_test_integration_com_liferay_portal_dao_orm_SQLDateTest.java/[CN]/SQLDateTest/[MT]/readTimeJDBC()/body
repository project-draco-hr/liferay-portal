{
  try (Connection connection=DataAccess.getConnection();Statement statement=connection.createStatement();ResultSet resultSet=statement.executeQuery(_READ_RELEASE_MODIFIED_DATE)){
    Assert.assertTrue(resultSet.next());
    Timestamp timestamp=resultSet.getTimestamp("modifiedDate");
    Assert.assertFalse(resultSet.next());
    return timestamp.getTime();
  }
 }
