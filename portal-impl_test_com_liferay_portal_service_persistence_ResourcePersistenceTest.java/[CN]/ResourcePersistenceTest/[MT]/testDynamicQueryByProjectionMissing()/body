{
  Resource newResource=addResource();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Resource.class,Resource.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("resourceId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("resourceId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
