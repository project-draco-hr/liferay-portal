{
  for (int i=0; i < assetEntryXmls.length; i++) {
    String assetEntry=assetEntryXmls[i];
    Document document=SAXReaderUtil.read(assetEntry);
    Element rootElement=document.getRootElement();
    Element assetTypeElementUuid=rootElement.element("asset-entry-uuid");
    String assetUuid=assetTypeElementUuid.getStringValue();
    List<JournalArticle> journalArticles=JournalArticleUtil.findByUuid(assetUuid);
    if (journalArticles.isEmpty()) {
      continue;
    }
    JournalArticle journalArticle=journalArticles.get(0);
    JournalArticleResource journalArticleResource=JournalArticleResourceUtil.findByPrimaryKey(journalArticle.getResourcePrimKey());
    rootElement.remove(assetTypeElementUuid);
    assetTypeElementUuid.setText(journalArticleResource.getUuid());
    rootElement.add(assetTypeElementUuid);
    document.setRootElement(rootElement);
    assetEntryXmls[i]=document.formattedString(StringPool.BLANK);
  }
}
