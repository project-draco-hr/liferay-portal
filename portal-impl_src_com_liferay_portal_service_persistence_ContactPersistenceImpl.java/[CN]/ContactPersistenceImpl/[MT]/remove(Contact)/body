{
  Session session=null;
  try {
    session=openSession();
    contact=(Contact)session.merge(contact);
    session.delete(contact);
    session.flush();
    return contact;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Contact.class.getName());
  }
}
