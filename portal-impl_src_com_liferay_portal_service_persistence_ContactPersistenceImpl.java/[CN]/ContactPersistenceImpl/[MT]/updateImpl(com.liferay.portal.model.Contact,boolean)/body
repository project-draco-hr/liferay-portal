{
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(contact);
    }
 else {
      if (contact.isNew()) {
        session.save(contact);
      }
    }
    session.flush();
    contact.setNew(false);
    return contact;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Contact.class.getName());
  }
}
