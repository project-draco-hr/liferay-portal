{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,contact,merge);
    contact.setNew(false);
    return contact;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Contact.class.getName());
  }
}
