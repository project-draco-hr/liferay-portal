{
  Callable<DDLRecordSet> callable=new Callable<DDLRecordSet>(){
    @Override public DDLRecordSet call() throws Exception {
      long recordSetId=ParamUtil.getLong(resourceRequest,"recordSetId");
      if (recordSetId == 0) {
        return addUpdateRecordSetMVCCommandHelper.addRecordSet(resourceRequest,resourceResponse);
      }
 else {
        return addUpdateRecordSetMVCCommandHelper.updateRecordSet(resourceRequest,resourceResponse);
      }
    }
  }
;
  return TransactionInvokerUtil.invoke(_transactionConfig,callable);
}
