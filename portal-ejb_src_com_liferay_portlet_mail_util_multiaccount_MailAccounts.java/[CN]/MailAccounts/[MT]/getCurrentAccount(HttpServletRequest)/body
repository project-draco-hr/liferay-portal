{
  HttpSession ses=req.getSession();
  MailAccount previousAccount=(MailAccount)ses.getAttribute(WebKeys.MAIL_CURRENT_ACCOUNT);
  if (previousAccount == null) {
    MailAccount defaultAccount=_getAccount(req,_getDefaultAccountName());
    ses.setAttribute(WebKeys.MAIL_CURRENT_ACCOUNT,defaultAccount);
    previousAccount=defaultAccount;
  }
  return previousAccount;
}
