{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AnnouncementsDelivery.class,AnnouncementsDelivery.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("deliveryId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("deliveryId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
