{
  JSONArray expectedJSONArray=createJSONArray("value 1");
  DDMFormFieldValue ddmFormFieldValue=DDMFormValuesTestUtil.createDDMFormFieldValue("CheckboxMultiple",new UnlocalizedValue(expectedJSONArray.toString()));
  JSONArray actualJSONArray=_checkboxMultipleDDMFormFieldValueAccessor.getValue(ddmFormFieldValue,LocaleUtil.US);
  Assert.assertEquals(expectedJSONArray.toString(),actualJSONArray.toString());
}
