{
  SCFrameworkVersion scFrameworkVersion=findByPrimaryKey(frameworkVersionId);
  Session session=null;
  try {
    session=openSession();
    SCFrameworkVersion[] array=new SCFrameworkVersionImpl[3];
    array[0]=getByG_A_PrevAndNext(session,scFrameworkVersion,groupId,active,orderByComparator,true);
    array[1]=scFrameworkVersion;
    array[2]=getByG_A_PrevAndNext(session,scFrameworkVersion,groupId,active,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
