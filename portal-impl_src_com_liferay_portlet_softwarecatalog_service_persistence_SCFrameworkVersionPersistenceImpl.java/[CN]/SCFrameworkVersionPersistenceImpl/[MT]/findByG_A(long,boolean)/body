{
  Object[] finderArgs=new Object[]{new Long(groupId),Boolean.valueOf(active)};
  Object result=FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_G_A,finderArgs,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portlet.softwarecatalog.model.SCFrameworkVersion WHERE ");
      query.append("groupId = ?");
      query.append(" AND ");
      query.append("active_ = ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("name DESC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      qPos.add(active);
      List<SCFrameworkVersion> list=q.list();
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_G_A,finderArgs,list);
      return list;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (List<SCFrameworkVersion>)result;
  }
}
