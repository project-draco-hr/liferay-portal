{
  FinderCacheUtil.clearCache("SCFrameworkVersi_SCProductVers");
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(scFrameworkVersion);
    }
 else {
      if (scFrameworkVersion.isNew()) {
        session.save(scFrameworkVersion);
      }
    }
    session.flush();
    scFrameworkVersion.setNew(false);
    return scFrameworkVersion;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(SCFrameworkVersion.class.getName());
  }
}
