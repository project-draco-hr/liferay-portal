{
  String message=ioe.getMessage();
  if (message == null) {
    return false;
  }
  if (message.contains("already exists. To update the plugin, uninstall it first")) {
    if (_log.isDebugEnabled()) {
      _log.debug("Skipping plugin " + _pluginName + " because it is already installed",ioe);
    }
    return true;
  }
  return false;
}
