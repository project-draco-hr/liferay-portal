{
  _bcc=bcc;
  if (_messageRemoteModel != null) {
    try {
      Class<?> clazz=_messageRemoteModel.getClass();
      Method method=clazz.getMethod("setBcc",String.class);
      method.invoke(_messageRemoteModel,bcc);
    }
