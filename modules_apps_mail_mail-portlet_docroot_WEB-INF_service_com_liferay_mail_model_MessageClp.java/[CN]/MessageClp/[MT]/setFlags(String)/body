{
  _flags=flags;
  if (_messageRemoteModel != null) {
    try {
      Class<?> clazz=_messageRemoteModel.getClass();
      Method method=clazz.getMethod("setFlags",String.class);
      method.invoke(_messageRemoteModel,flags);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
