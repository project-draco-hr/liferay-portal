{
  if (parameters.length < 3) {
    throw new IllegalArgumentException("Three or more parameters are expected");
  }
  DDMFormFieldEvaluationResult ddmFormFieldEvaluationResult=(DDMFormFieldEvaluationResult)parameters[0];
  String propertyName=parameters[1].toString();
  ddmFormFieldEvaluationResult.setProperty(propertyName,parameters[2]);
  if (propertyName.equals("valid") && (parameters.length > 3)) {
    ddmFormFieldEvaluationResult.setErrorMessage(parameters[3].toString());
  }
  return true;
}
