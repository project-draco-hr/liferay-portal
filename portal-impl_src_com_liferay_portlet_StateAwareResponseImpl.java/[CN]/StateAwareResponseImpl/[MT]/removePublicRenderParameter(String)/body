{
  if (name == null) {
    throw new IllegalArgumentException();
  }
  PublicRenderParameter publicRenderParameter=getPortlet().getPublicRenderParameter(name);
  if (publicRenderParameter == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("Public parameter " + name + "does not exist");
    }
    return;
  }
  com.liferay.portal.kernel.xml.QName qName=publicRenderParameter.getQName();
  String key=PortletQNameUtil.getPublicRenderParameterName(qName);
  _publicRenderParameters.remove(key);
}
