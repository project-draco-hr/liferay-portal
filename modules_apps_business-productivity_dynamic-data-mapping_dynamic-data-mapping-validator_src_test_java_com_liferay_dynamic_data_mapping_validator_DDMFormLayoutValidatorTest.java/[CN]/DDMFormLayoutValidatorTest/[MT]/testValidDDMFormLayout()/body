{
  DDMFormLayoutColumn ddmFormLayoutColumn=createDDMFormLayoutColumn(12,"field");
  DDMFormLayoutRow ddmFormLayoutRow=createDDMFormLayoutRow(ddmFormLayoutColumn);
  LocalizedValue title=createLocalizedValue("Page1",LocaleUtil.US);
  DDMFormLayoutPage ddmFormLayoutPage=createDDMFormLayoutPage(ddmFormLayoutRow,title);
  DDMFormLayout ddmFormLayout=createDDMFormLayout(ddmFormLayoutPage,LocaleUtil.US);
  _ddmFormLayoutValidator.validate(ddmFormLayout);
}
