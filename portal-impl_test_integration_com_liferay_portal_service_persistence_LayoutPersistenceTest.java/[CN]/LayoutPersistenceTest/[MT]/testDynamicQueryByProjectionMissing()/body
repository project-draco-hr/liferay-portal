{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Layout.class,Layout.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("plid"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("plid",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
