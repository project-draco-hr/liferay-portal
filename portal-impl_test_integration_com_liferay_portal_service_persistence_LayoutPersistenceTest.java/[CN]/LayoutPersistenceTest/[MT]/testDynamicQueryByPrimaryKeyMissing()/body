{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Layout.class,Layout.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("plid",ServiceTestUtil.nextLong()));
  List<Layout> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
