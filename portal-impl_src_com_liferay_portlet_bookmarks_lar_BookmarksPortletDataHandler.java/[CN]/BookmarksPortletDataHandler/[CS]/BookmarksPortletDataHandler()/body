{
  setAlwaysExportable(true);
  setExportControl(new PortletDataHandlerBoolean(NAMESPACE,"folders-and-entries",true,true));
  setExportMetadataControl(new PortletDataHandlerBoolean(NAMESPACE,"bookmarks",true,new PortletDataHandlerControl[]{new PortletDataHandlerBoolean(NAMESPACE,"categories"),new PortletDataHandlerBoolean(NAMESPACE,"ratings"),new PortletDataHandlerBoolean(NAMESPACE,"tags")}));
  setImportControls(getExportControls());
  setImportMetadataControls(getExportMetadataControls());
  setPublishToLiveByDefault(true);
}
