{
  setAlwaysExportable(true);
  setExportControls(new PortletDataHandlerBoolean(NAMESPACE,"folders",true,true),new PortletDataHandlerBoolean(NAMESPACE,"entries",true,true));
  setExportMetadataControls(new PortletDataHandlerBoolean(NAMESPACE,"bookmarks",true,new PortletDataHandlerControl[]{new PortletDataHandlerBoolean(NAMESPACE,"categories"),new PortletDataHandlerBoolean(NAMESPACE,"ratings"),new PortletDataHandlerBoolean(NAMESPACE,"tags")}));
  setPublishToLiveByDefault(true);
}
