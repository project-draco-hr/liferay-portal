{
  ServiceWrapper<DDMTemplatePermissionSupport> templatePermissionSupportServiceWrapper=_ddmPermissionSupportTracker.getDDMTemplatePermissionSupportServiceWrapper(resourceClassNameId);
  return permissionChecker.hasPermission(groupId,getResourceName(templatePermissionSupportServiceWrapper,classNameId),groupId,getAddTemplateActionId(templatePermissionSupportServiceWrapper));
}
