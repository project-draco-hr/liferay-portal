{
  List<Module> modules=ModuleLocalServiceUtil.getModules(QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  for (  Module module : modules) {
    if (Validator.isNull(module.getContextName())) {
      continue;
    }
    runSQL("update Marketplace_Module set contextName = '" + ContextUtil.getContextName(module.getContextName()) + "' where moduleId = "+ module.getModuleId());
  }
}
