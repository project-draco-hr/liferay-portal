{
  selenium.clickAt("link=Define Permissions",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.select("add-permissions",RuntimeVariables.replace("label=Announcements"));
  selenium.waitForPageToLoad("30000");
  selenium.uncheck("_128_rowIds");
  selenium.uncheck("//input[@name='_128_rowIds' and @value='com.liferay.portlet.announcements.model.AnnouncementsEntryUPDATE']");
  selenium.check("//input[@name='_128_rowIds' and @value='com.liferay.portlet.announcements.model.AnnouncementsEntryVIEW']");
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("The role permissions were updated."));
}
