{
  BookmarksFolder folder=BookmarksTestUtil.addFolder(_group.getGroupId(),RandomTestUtil.randomString());
  long initialParentFolderId=folder.getParentFolderId();
  BookmarksFolder destinationFolder=BookmarksTestUtil.addFolder(_group.getGroupId(),RandomTestUtil.randomString());
  folder=BookmarksFolderLocalServiceUtil.moveFolder(folder.getFolderId(),destinationFolder.getFolderId());
  Assert.assertNotEquals(initialParentFolderId,folder.getParentFolderId());
  Assert.assertEquals(destinationFolder.getFolderId(),folder.getParentFolderId());
}
