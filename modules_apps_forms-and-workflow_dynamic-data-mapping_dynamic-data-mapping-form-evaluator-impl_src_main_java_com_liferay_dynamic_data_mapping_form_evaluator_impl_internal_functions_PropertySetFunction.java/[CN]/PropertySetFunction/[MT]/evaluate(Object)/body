{
  if (parameters.length != 2) {
    throw new IllegalArgumentException("Two parameters are expected");
  }
  String ddmFormFieldName=parameters[0].toString();
  List<DDMFormFieldEvaluationResult> ddmFormFieldEvaluationResults=getDDMFormFieldEvaluationResults(ddmFormFieldName);
  for (  DDMFormFieldEvaluationResult ddmFormFieldEvaluationResult : ddmFormFieldEvaluationResults) {
    ddmFormFieldEvaluationResult.setProperty(_propertyName,parameters[1]);
  }
  return true;
}
