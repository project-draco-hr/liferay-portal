{
  ElasticsearchConfiguration elasticsearchConfiguration=Configurable.createConfigurable(ElasticsearchConfiguration.class,properties);
  setAdditionalIndexConfigurations(elasticsearchConfiguration.additionalIndexConfigurations());
  setAdditionalTypeMappings(elasticsearchConfiguration.additionalTypeMappings());
  Map<String,String> typeMappings=getTypeMappings(properties);
  setTypeMappings(typeMappings);
}
