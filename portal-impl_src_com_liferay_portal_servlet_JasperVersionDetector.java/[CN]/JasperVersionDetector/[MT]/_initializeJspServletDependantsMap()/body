{
  try {
    Class<?> clazz=Class.forName("org.apache.jasper.servlet.JspServletWrapper");
    Method method=ReflectionUtil.getDeclaredMethod(clazz,"getDependants");
    Class<?> returnType=method.getReturnType();
    _jspServletDependantsMap=Map.class.isAssignableFrom(returnType);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
