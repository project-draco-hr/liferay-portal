{
  String[] contentMetadataEntries=new String[0];
  contentMetadataEntries=upgradeBooleanSelectableEntry(contentMetadataEntries,portletPreferences,"enableCommentRatings");
  contentMetadataEntries=upgradeBooleanSelectableEntry(contentMetadataEntries,portletPreferences,"enableComments");
  contentMetadataEntries=upgradeBooleanSelectableEntry(contentMetadataEntries,portletPreferences,"enableRatings");
  contentMetadataEntries=upgradeBooleanSelectableEntry(contentMetadataEntries,portletPreferences,"enableRelatedAssets");
  portletPreferences.setValue("contentMetadataEntries",StringUtil.merge(contentMetadataEntries));
}
