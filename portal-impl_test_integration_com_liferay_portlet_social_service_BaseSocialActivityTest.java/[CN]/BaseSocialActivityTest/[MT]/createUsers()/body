{
  if (_userCreator != null) {
    UserLocalServiceUtil.deleteUser(_userCreator);
  }
  _userCreator=ServiceTestUtil.addUser("creator",false,new long[]{_group.getGroupId()});
  if (_userActor != null) {
    UserLocalServiceUtil.deleteUser(_userActor);
  }
  _userActor=ServiceTestUtil.addUser("actor",false,new long[]{_group.getGroupId()});
}
