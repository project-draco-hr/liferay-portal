{
  SearchContainerTag parent=(SearchContainerTag)findAncestorWithClass(this,SearchContainerTag.class);
  if (parent == null) {
    throw new JspException("Requires the liferay-ui:search-container tag");
  }
 else   if (!parent.getHasResults()) {
    throw new JspException("Requires the liferay-ui:search-container-results tag");
  }
  _resultRows=parent.getSearchContainer().getResultRows();
  _results=parent.getSearchContainer().getResults();
  if ((_results != null) && (!_results.isEmpty())) {
    _processRow();
    return EVAL_BODY_INCLUDE;
  }
 else {
    return SKIP_BODY;
  }
}
