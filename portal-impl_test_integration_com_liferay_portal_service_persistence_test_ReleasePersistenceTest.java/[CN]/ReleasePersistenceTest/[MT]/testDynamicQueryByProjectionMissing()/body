{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Release.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("releaseId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("releaseId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
