{
  WikiPagePermission.check(getPermissionChecker(),nodeId,title,ActionKeys.DELETE);
  WikiPage wikiPage=WikiPageLocalServiceUtil.getPage(nodeId,title);
  DLFileEntry dlFileEntry=DLFileEntryLocalServiceUtil.getFileEntry(wikiPage.getGroupId(),wikiPage.getAttachmentsFolderId(),fileName);
  wikiPageLocalService.movePageAttachmentToTrash(getUserId(),dlFileEntry.getFileEntryId());
  return dlFileEntry.getFileEntryId();
}
