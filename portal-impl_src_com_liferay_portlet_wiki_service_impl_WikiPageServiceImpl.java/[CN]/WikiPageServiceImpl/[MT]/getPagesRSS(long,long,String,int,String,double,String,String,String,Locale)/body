{
  WikiPagePermission.check(getPermissionChecker(),nodeId,title,ActionKeys.VIEW);
  List<WikiPage> pages=wikiPageLocalService.getPages(nodeId,title,0,_MAX_END,new PageCreateDateComparator(true));
  return exportToRSS(companyId,title,null,type,version,displayStyle,feedURL,entryURL,pages,true,locale);
}
