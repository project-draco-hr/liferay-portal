{
  WikiNodePermission.check(getPermissionChecker(),nodeId,ActionKeys.VIEW);
  WikiNode node=wikiNodePersistence.findByPrimaryKey(nodeId);
  List<WikiPage> pages=getNodePages(nodeId,max);
  return exportToRSS(node.getCompanyId(),node.getName(),node.getDescription(),type,version,displayStyle,feedURL,entryURL,pages,false,null);
}
