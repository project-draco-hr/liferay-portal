{
  if (!hasColumn("KBComment","helpful")) {
    return;
  }
  runSQL("update KBComment set userRating = " + KBCommentConstants.USER_RATING_LIKE + " where helpful = TRUE");
  runSQL("update KBComment set userRating = " + KBCommentConstants.USER_RATING_DISLIKE + " where helpful = FALSE");
  runSQL("alter table KBComment drop column helpful");
}
