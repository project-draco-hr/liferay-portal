{
  MDRAction mdrAction=(MDRAction)entityCache.getResult(MDRActionModelImpl.ENTITY_CACHE_ENABLED,MDRActionImpl.class,primaryKey);
  if (mdrAction == _nullMDRAction) {
    return null;
  }
  if (mdrAction == null) {
    Session session=null;
    try {
      session=openSession();
      mdrAction=(MDRAction)session.get(MDRActionImpl.class,primaryKey);
      if (mdrAction != null) {
        cacheResult(mdrAction);
      }
 else {
        entityCache.putResult(MDRActionModelImpl.ENTITY_CACHE_ENABLED,MDRActionImpl.class,primaryKey,_nullMDRAction);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(MDRActionModelImpl.ENTITY_CACHE_ENABLED,MDRActionImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return mdrAction;
}
