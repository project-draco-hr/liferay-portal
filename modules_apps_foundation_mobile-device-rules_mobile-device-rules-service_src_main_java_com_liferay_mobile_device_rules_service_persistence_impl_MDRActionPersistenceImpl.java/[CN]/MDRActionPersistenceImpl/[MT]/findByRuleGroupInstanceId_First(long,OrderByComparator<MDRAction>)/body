{
  MDRAction mdrAction=fetchByRuleGroupInstanceId_First(ruleGroupInstanceId,orderByComparator);
  if (mdrAction != null) {
    return mdrAction;
  }
  StringBundler msg=new StringBundler(4);
  msg.append(_NO_SUCH_ENTITY_WITH_KEY);
