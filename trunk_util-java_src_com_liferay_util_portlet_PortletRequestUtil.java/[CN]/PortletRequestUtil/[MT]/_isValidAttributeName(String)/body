{
  if (name.equalsIgnoreCase("j_password") || name.equalsIgnoreCase("LAYOUT_CONTENT") || name.equalsIgnoreCase("LAYOUTS")|| name.equalsIgnoreCase("PORTLET_RENDER_PARAMETERS")|| name.equalsIgnoreCase("USER_PASSWORD")|| name.startsWith("javax.")|| name.startsWith("liferay-ui:")) {
    return false;
  }
 else {
    return true;
  }
}
