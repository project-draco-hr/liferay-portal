{
  if (!isVendorMySQL() && !isVendorOracle()) {
    return keywords;
  }
  StringBuilder sb=new StringBuilder(keywords);
  for (int i=0; i < sb.length(); ++i) {
    char c=sb.charAt(i);
    if (c == CharPool.BACK_SLASH) {
      i++;
      continue;
    }
    if ((c == CharPool.UNDERLINE) || (c == CharPool.PERCENT)) {
      sb.insert(i,CharPool.BACK_SLASH);
      i++;
      continue;
    }
  }
  return sb.toString();
}
