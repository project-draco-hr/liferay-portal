{
  JSONObject extraDataJSONObject=JSONFactoryUtil.createJSONObject(extraData);
  boolean privateLayout=extraDataJSONObject.getBoolean("privateLayout");
  Layout layout=LayoutLocalServiceUtil.fetchLayoutByUuidAndGroupId(uuid,groupId,privateLayout);
  if (layout != null) {
    LayoutLocalServiceUtil.deleteLayout(layout,true,new ServiceContext());
  }
}
