{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,phone,merge);
    phone.setNew(false);
    return phone;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Phone.class.getName());
  }
}
