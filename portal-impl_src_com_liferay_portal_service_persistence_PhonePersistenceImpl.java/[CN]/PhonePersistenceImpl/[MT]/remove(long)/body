{
  Session session=null;
  try {
    session=openSession();
    Phone phone=(Phone)session.get(PhoneImpl.class,Long.valueOf(phoneId));
    if (phone == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + phoneId);
      }
      throw new NoSuchPhoneException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + phoneId);
    }
    return phonePersistence.remove(phone);
  }
 catch (  NoSuchPhoneException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
