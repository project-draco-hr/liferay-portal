{
  Session session=null;
  try {
    session=openSession();
    session.delete(phone);
    session.flush();
    return phone;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Phone.class.getName());
  }
}
