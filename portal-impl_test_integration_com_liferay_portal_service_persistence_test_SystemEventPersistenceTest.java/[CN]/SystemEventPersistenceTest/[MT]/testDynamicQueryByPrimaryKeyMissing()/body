{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SystemEvent.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("systemEventId",RandomTestUtil.nextLong()));
  List<SystemEvent> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
