{
  RecentLayoutBranch recentLayoutBranch=_recentLayoutBranchLocalService.fetchRecentLayoutBranch(userId,layoutSetBranchId,plid);
  if (recentLayoutBranch != null) {
    return recentLayoutBranch.getLayoutBranchId();
  }
  try {
    LayoutBranch masterLayoutBranch=_layoutBranchLocalService.getMasterLayoutBranch(layoutSetBranchId,plid);
    return masterLayoutBranch.getLayoutBranchId();
  }
 catch (  NoSuchLayoutBranchException nslbe) {
  }
  return 0;
}
