{
  if (_lockManager.isLocked(Staging.class.getName(),groupId)) {
    Lock lock=_lockManager.getLock(Staging.class.getName(),groupId);
    throw new DuplicateLockException(lock);
  }
  _lockManager.lock(userId,Staging.class.getName(),String.valueOf(groupId),StagingImpl.class.getName(),false,StagingConstants.LOCK_EXPIRATION_TIME);
}
