{
  System.setProperty(PropsKeys.INTRABAND_IMPL,"NoSuchClass");
  try {
    IntrabandFactoryUtil.createIntraband();
    Assert.fail();
  }
 catch (  RuntimeException re) {
    Assert.assertEquals("Unable to instantiate NoSuchClass",re.getMessage());
    Throwable throwable=re.getCause();
    Assert.assertSame(ClassNotFoundException.class,throwable.getClass());
  }
 finally {
    System.clearProperty(PropsKeys.INTRABAND_IMPL);
  }
}
