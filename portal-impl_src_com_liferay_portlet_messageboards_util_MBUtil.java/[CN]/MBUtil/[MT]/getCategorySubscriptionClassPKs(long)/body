{
  List<Subscription> categorySubscriptions=SubscriptionLocalServiceUtil.getUserSubscriptions(userId,MBCategory.class.getName());
  Set<Long> categorySubscriptionClassPKs=new HashSet<Long>(categorySubscriptions.size());
  for (  Subscription subscription : categorySubscriptions) {
    categorySubscriptionClassPKs.add(subscription.getClassPK());
  }
  return categorySubscriptionClassPKs;
}
