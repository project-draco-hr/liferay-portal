{
  String emailMessageUpdatedSignature=prefs.getValue("email-message-updated-signature",StringPool.BLANK);
  if (Validator.isNotNull(emailMessageUpdatedSignature)) {
    return emailMessageUpdatedSignature;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsUtil.MESSAGE_BOARDS_EMAIL_MESSAGE_UPDATED_SIGNATURE));
  }
}
