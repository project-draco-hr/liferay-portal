{
  int x=mailId.indexOf(StringPool.LESS_THAN) + 1;
  int y=mailId.indexOf(StringPool.AT);
  long messageId=0;
  if ((x > 0) && (y != -1)) {
    String right=mailId.substring(x,y);
    int z=right.indexOf(StringPool.PERIOD);
    if (z != -1) {
      messageId=GetterUtil.getLong(right.substring(z));
    }
  }
  return messageId;
}
