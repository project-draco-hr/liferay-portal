{
  int x=mailId.indexOf(CharPool.LESS_THAN) + 1;
  int y=mailId.indexOf(CharPool.AT);
  long messageId=0;
  if ((x > 0) && (y != -1)) {
    String temp=mailId.substring(x,y);
    int z=temp.lastIndexOf(CharPool.PERIOD);
    if (z != -1) {
      messageId=GetterUtil.getLong(temp.substring(z + 1));
    }
  }
  return messageId;
}
