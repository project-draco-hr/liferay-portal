{
  int x=mailId.indexOf(StringPool.LESS_THAN) + 1;
  int y=mailId.indexOf(StringPool.AT);
  long messageId=0;
  if ((x > 0) && (y != -1)) {
    String temp=mailId.substring(x,y);
    int z=temp.lastIndexOf(StringPool.PERIOD);
    if (z != -1) {
      messageId=GetterUtil.getLong(temp.substring(z) + 1);
    }
  }
  return messageId;
}
