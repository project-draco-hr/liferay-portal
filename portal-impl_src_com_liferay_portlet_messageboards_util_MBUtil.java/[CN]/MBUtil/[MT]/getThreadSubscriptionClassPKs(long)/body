{
  List<Subscription> threadSubscriptions=SubscriptionLocalServiceUtil.getUserSubscriptions(userId,MBThread.class.getName());
  Set<Long> threadSubscriptionClassPKs=new HashSet<Long>(threadSubscriptions.size());
  for (  Subscription subscription : threadSubscriptions) {
    threadSubscriptionClassPKs.add(subscription.getClassPK());
  }
  return threadSubscriptionClassPKs;
}
