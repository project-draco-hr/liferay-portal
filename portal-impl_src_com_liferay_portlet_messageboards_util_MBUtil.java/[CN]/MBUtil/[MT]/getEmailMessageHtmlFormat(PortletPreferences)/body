{
  String emailMessageHtmlFormat=prefs.getValue("email-message-html-format",StringPool.BLANK);
  if (Validator.isNotNull(emailMessageHtmlFormat)) {
    return GetterUtil.getBoolean(emailMessageHtmlFormat);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsUtil.MESSAGE_BOARDS_EMAIL_MESSAGE_HTML_FORMAT));
  }
}
