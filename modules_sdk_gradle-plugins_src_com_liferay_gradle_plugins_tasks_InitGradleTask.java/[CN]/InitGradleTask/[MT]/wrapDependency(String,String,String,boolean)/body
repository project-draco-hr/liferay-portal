{
  StringBuilder sb=new StringBuilder();
  sb.append("\t\t[group: \"");
  sb.append(group);
  sb.append("\", name: \"");
  sb.append(name);
  sb.append("\", version: \"");
  sb.append(version);
  sb.append("\"");
  if (!transitive) {
    sb.append(", transitive: false");
  }
  sb.append("],");
  return sb.toString();
}
