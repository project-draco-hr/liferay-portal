{
  ExpandoTable table=expandoTablePersistence.fetchByC_C_N(companyId,classNameId,tableName);
  if (table == null) {
    return;
  }
  ExpandoColumn column=expandoColumnPersistence.fetchByT_N(table.getTableId(),columnName);
  if (column == null) {
    return;
  }
  ExpandoValue value=expandoValuePersistence.fetchByT_C_C(table.getTableId(),column.getColumnId(),classPK);
  if (value != null) {
    deleteValue(value.getValueId());
  }
}
