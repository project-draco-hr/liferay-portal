{
  String emailAssetEntryAddedEnabled=portletPreferences.getValue("emailAssetEntryAddedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailAssetEntryAddedEnabled)) {
    return GetterUtil.getBoolean(emailAssetEntryAddedEnabled);
  }
 else {
    return AssetPublisherWebConfigurationValues.EMAIL_ASSET_ENTRY_ADDED_ENABLED;
  }
}
