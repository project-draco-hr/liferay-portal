{
  if (_request != null) {
    HttpSession session=_request.getSession(true);
    session.removeAttribute(PERCENT + _progressId);
  }
 else {
    PortletSession portletSession=_portletRequest.getPortletSession(true);
    portletSession.removeAttribute(PERCENT + _progressId,PortletSession.APPLICATION_SCOPE);
  }
}
