{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=dataSource.getConnection();
    ps=con.prepareStatement(sql);
    for (int i=0; i < args.length; i++) {
      Object arg=args[i];
      ps.setObject(i + 1,arg);
    }
    return ps.executeUpdate();
  }
  finally {
    _dbManager.cleanUp(con,ps,rs);
  }
}
