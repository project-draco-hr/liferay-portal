{
  selenium.open("/user/joebloggs/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Organizations")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Organizations",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_79_keywords",RuntimeVariables.replace("Web Content Display"));
  selenium.clickAt("//input[@value='Search Organizations']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//strong/span",RuntimeVariables.replace("Actions"));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//li[3]/nobr/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Manage Pages"),selenium.getText("//li[3]/nobr/a"));
  selenium.clickAt("//li[3]/nobr/a",RuntimeVariables.replace("Manage Pages"));
  selenium.waitForPageToLoad("30000");
  selenium.type("_79_name_en_US",RuntimeVariables.replace("Page Staging Organization Web Content Display"));
  selenium.clickAt("//input[@value='Add Page']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request processed successfully."),selenium.getText("//div[2]/div/div/div"));
}
