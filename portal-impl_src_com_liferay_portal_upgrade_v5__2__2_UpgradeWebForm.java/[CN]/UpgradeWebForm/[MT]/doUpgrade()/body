{
  long oldClassNameId=getClassNameId(_OLD_WEBFORM_CLASS_NAME);
  if (oldClassNameId == 0) {
    return;
  }
  long newClassNameId=getClassNameId(_NEW_WEBFORM_CLASS_NAME);
  if (newClassNameId == 0) {
    runSQL("update ClassName_ set value = '" + _NEW_WEBFORM_CLASS_NAME + "' where value = '"+ _OLD_WEBFORM_CLASS_NAME+ "'");
  }
 else {
    runSQL("update ExpandoTable set classNameId = '" + newClassNameId + "' where classNameId = '"+ oldClassNameId+ "'");
    runSQL("update ExpandoValue set classNameId = '" + newClassNameId + "' where classNameId = '"+ oldClassNameId+ "'");
    runSQL("delete from ClassName_ where value = '" + _OLD_WEBFORM_CLASS_NAME + "'");
  }
}
