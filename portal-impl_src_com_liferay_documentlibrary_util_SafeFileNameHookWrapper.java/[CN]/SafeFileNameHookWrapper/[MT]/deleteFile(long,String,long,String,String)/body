{
  String safeFileName=FileUtil.encodeSafeFileName(fileName);
  if (!safeFileName.equals(fileName) && _hook.hasFile(companyId,repositoryId,fileName,versionNumber)) {
    _hook.deleteFile(companyId,portletId,repositoryId,fileName,versionNumber);
    return;
  }
  _hook.deleteFile(companyId,portletId,repositoryId,safeFileName,versionNumber);
}
