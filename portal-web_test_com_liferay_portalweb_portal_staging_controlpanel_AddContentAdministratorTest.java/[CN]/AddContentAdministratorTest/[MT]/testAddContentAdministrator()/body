{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Users")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Users",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Add")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Add",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(5000);
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("_125_screenName")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.typeKeys("_125_screenName",RuntimeVariables.replace("ContentAdministrator"));
  selenium.type("_125_screenName",RuntimeVariables.replace("ContentAdministrator"));
  selenium.type("_125_emailAddress",RuntimeVariables.replace("CA@liferay.com"));
  selenium.select("_125_prefixId",RuntimeVariables.replace("label=Mr."));
  selenium.type("_125_firstName",RuntimeVariables.replace("Administrator"));
  selenium.type("_125_lastName",RuntimeVariables.replace("Liferay"));
  selenium.select("_125_suffixId",RuntimeVariables.replace("label=PhD."));
  selenium.select("_125_birthdayMonth",RuntimeVariables.replace("label=May"));
  selenium.select("_125_birthdayDay",RuntimeVariables.replace("label=22"));
  selenium.select("_125_birthdayYear",RuntimeVariables.replace("label=1985"));
  selenium.select("_125_male",RuntimeVariables.replace("label=Male"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("passwordLink",RuntimeVariables.replace(""));
  Thread.sleep(5000);
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("_125_password1")) {
        break;
      }
    }
 catch (    Exception e) {
