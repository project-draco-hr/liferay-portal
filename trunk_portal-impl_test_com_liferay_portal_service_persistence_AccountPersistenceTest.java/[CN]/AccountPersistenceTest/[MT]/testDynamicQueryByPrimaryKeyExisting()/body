{
  Account newAccount=addAccount();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Account.class,Account.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("accountId",newAccount.getAccountId()));
  List<Account> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Account existingAccount=result.get(0);
  assertEquals(existingAccount,newAccount);
}
