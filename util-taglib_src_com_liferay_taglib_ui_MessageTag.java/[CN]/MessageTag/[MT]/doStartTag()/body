{
  try {
    String value=StringPool.BLANK;
    if (_arguments == null) {
      value=LanguageUtil.get(pageContext,_key);
    }
 else {
      value=LanguageUtil.format(pageContext,_key,_arguments,_translateArguments);
    }
    pageContext.getOut().print(value);
  }
 catch (  Exception e) {
    throw new JspException(e);
  }
  return SKIP_BODY;
}
