{
  String sql=SQLServerLimitStringUtil.getLimitString("( SELECT articleId, userName FROM JournalArticle )" + " UNION ALL ( SELECT articleId, userName FROM JournalArticle )",10,30);
  Assert.assertTrue(sql.indexOf("30") > 0);
  Assert.assertTrue(sql.indexOf("11") > 0);
  Assert.assertTrue(sql.indexOf("top") != 0);
}
