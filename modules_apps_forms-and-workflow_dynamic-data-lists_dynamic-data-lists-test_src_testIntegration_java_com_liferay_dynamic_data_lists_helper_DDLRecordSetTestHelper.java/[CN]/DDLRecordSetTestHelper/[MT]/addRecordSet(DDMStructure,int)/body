{
  Map<Locale,String> nameMap=new HashMap<>();
  nameMap.put(LocaleUtil.US,RandomTestUtil.randomString());
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId());
  return DDLRecordSetLocalServiceUtil.addRecordSet(TestPropsValues.getUserId(),_group.getGroupId(),ddmStructure.getStructureId(),null,nameMap,null,DDLRecordSetConstants.MIN_DISPLAY_ROWS_DEFAULT,scope,serviceContext);
}
