{
  String templateKey=ServiceTestUtil.randomString();
  String language=TemplateConstants.LANG_TYPE_VM;
  try {
    addTemplate(_classNameId,0,templateKey,"Test Template 1",DDMTemplateConstants.TEMPLATE_TYPE_FORM,DDMTemplateConstants.TEMPLATE_MODE_CREATE,language,getTestTemplateScript(language));
