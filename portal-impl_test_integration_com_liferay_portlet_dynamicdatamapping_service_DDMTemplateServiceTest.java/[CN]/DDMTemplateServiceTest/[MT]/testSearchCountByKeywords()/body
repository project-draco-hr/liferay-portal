{
  long companyId=TestPropsValues.getCompanyId();
  long groupId=TestPropsValues.getGroupId();
  int initCount=DDMTemplateLocalServiceUtil.searchCount(companyId,groupId,_testClassNameId,0,null,null,null);
  addListTemplate(_testClassNameId,0,"Test Template");
  int count=DDMTemplateLocalServiceUtil.searchCount(companyId,groupId,_testClassNameId,0,null,null,null);
  Assert.assertEquals(initCount + 1,count);
}
