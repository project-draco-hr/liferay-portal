{
  int initialCount=DDMTemplateLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),group.getGroupId(),_classNameId,0,"Test Template",null,null,null,null,false);
  addListTemplate(_classNameId,0,"Test Template");
  int count=DDMTemplateLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),group.getGroupId(),_classNameId,0,"Test Template",null,null,null,null,false);
  Assert.assertEquals(initialCount + 1,count);
}
