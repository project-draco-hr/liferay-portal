{
  long companyId=TestPropsValues.getCompanyId();
  long groupId=TestPropsValues.getGroupId();
  String name="Test Template";
  int initCount=DDMTemplateLocalServiceUtil.searchCount(companyId,groupId,_testClassNameId,0,name,null,null,null,null,false);
  addListTemplate(_testClassNameId,0,name);
  int count=DDMTemplateLocalServiceUtil.searchCount(companyId,groupId,_testClassNameId,0,name,null,null,null,null,false);
  Assert.assertEquals(initCount + 1,count);
}
