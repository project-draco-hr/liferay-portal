{
  DDMTemplate template=addDisplayTemplate(_CLASS_NAME_ID,0,"Test Template 1");
  addDisplayTemplate(_CLASS_NAME_ID,0,"Test Template 2");
  List<DDMTemplate> templates=DDMTemplateLocalServiceUtil.search(template.getCompanyId(),template.getGroupId(),template.getClassNameId(),template.getClassPK(),null,template.getType(),template.getMode(),0,1,null);
  Assert.assertEquals(1,templates.size());
}
