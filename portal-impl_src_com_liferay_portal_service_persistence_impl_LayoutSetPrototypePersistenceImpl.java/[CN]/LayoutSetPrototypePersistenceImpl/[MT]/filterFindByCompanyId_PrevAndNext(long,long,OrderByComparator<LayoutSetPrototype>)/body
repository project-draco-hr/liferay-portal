{
  if (!InlineSQLHelperUtil.isEnabled(companyId,0)) {
    return findByCompanyId_PrevAndNext(layoutSetPrototypeId,companyId,orderByComparator);
  }
  LayoutSetPrototype layoutSetPrototype=findByPrimaryKey(layoutSetPrototypeId);
  Session session=null;
  try {
    session=openSession();
    LayoutSetPrototype[] array=new LayoutSetPrototypeImpl[3];
    array[0]=filterGetByCompanyId_PrevAndNext(session,layoutSetPrototype,companyId,orderByComparator,true);
    array[1]=layoutSetPrototype;
    array[2]=filterGetByCompanyId_PrevAndNext(session,layoutSetPrototype,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
