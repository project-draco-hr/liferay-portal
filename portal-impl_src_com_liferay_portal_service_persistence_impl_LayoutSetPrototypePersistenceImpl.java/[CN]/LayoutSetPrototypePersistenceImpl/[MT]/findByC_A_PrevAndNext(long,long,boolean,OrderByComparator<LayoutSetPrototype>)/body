{
  LayoutSetPrototype layoutSetPrototype=findByPrimaryKey(layoutSetPrototypeId);
  Session session=null;
  try {
    session=openSession();
    LayoutSetPrototype[] array=new LayoutSetPrototypeImpl[3];
    array[0]=getByC_A_PrevAndNext(session,layoutSetPrototype,companyId,active,orderByComparator,true);
    array[1]=layoutSetPrototype;
    array[2]=getByC_A_PrevAndNext(session,layoutSetPrototype,companyId,active,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
