{
  Collection<ServiceRegistration<T>> serviceRegistrations=new ArrayList<>();
  for (int i=0; i < services.length; i++) {
    Dictionary<String,Object> properties=new Hashtable<>();
    if (property != null) {
      properties.put(property,i + 1);
    }
    serviceRegistrations.add(registerService(clazz,services[i],properties));
  }
  return serviceRegistrations;
}
