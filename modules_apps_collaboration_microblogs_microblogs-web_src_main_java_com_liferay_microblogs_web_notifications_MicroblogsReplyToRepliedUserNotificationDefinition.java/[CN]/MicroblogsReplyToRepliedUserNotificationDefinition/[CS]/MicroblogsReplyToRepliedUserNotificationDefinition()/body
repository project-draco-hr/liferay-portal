{
  super(MicroblogsPortletKeys.MICROBLOGS,0,MicroblogsEntryConstants.NOTIFICATION_TYPE_REPLY_TO_REPLIED,"receive-a-notification-when-someone-comments-on-a-microblog-you-" + "commented-on");
  addUserNotificationDeliveryType(new UserNotificationDeliveryType("mobile",UserNotificationDeliveryConstants.TYPE_PUSH,true,true));
