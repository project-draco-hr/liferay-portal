{
  StringBuffer sb=new StringBuffer();
  Object[][] columns=getColumns();
  Long id=new Long(CounterLocalServiceUtil.increment(Counter.class.getName()));
  if (_usePKMap) {
    appendPKMap(new Long(rs.getLong((String)columns[0][0])),id);
  }
  appendColumn(sb,id);
  for (int i=1; i < columns.length; i++) {
    boolean last=false;
    if (i == columns.length - 1) {
      last=true;
    }
    appendColumn(sb,rs,(String)columns[i][0],(Integer)columns[i][1],last);
  }
  return sb.toString();
}
