{
  UserTracker userTracker=fetchByPrimaryKey(userTrackerId);
  if (userTracker == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No UserTracker exists with the primary key " + userTrackerId);
    }
    throw new NoSuchUserTrackerException("No UserTracker exists with the primary key " + userTrackerId);
  }
  return userTracker;
}
