{
  if (Validator.isNotNull(name)) {
    throw new NameNotFoundException();
  }
  Attributes attributes=new BasicAttributes(true);
  for (int i=0; i < ids.length; i++) {
    Attribute attribute=_attributes.get(ids[i]);
    if (attribute != null) {
      attributes.put(attribute);
    }
  }
  return attributes;
}
