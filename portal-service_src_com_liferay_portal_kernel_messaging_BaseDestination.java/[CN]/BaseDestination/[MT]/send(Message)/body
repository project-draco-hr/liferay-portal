{
  if (_messageListeners.isEmpty()) {
    if (_log.isDebugEnabled()) {
      _log.debug("No message listeners for destination " + getName());
    }
    return;
  }
  ThreadPoolExecutor threadPoolExecutor=getThreadPoolExecutor();
  if (threadPoolExecutor.isShutdown()) {
    throw new IllegalStateException("Destination " + getName() + " is shutdown and cannot "+ "receive more messages");
  }
  if ((_maximumQueueSize > -1) && (threadPoolExecutor.getQueue().size() > _maximumQueueSize)) {
    throw new IllegalStateException(threadPoolExecutor.getQueue().size() + " messages exceeds the maximum queue size of " + _maximumQueueSize);
  }
  dispatch(_messageListeners,message);
}
