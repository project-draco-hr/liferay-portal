{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(account);
    }
 else {
      if (account.isNew()) {
        session.save(account);
      }
    }
    session.flush();
    account.setNew(false);
    return account;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
