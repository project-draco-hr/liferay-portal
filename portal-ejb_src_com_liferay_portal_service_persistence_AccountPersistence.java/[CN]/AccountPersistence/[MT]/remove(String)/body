{
  Session session=null;
  try {
    session=openSession();
    AccountHBM accountHBM=(AccountHBM)session.get(AccountHBM.class,accountId);
    if (accountHBM == null) {
      _log.warn("No Account exists with the primary key " + accountId.toString());
      throw new NoSuchAccountException("No Account exists with the primary key " + accountId.toString());
    }
    session.delete(accountHBM);
    session.flush();
    return AccountHBMUtil.model(accountHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
