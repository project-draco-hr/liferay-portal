{
  Session session=null;
  try {
    session=openSession();
    Account account=(Account)session.get(AccountImpl.class,accountId);
    if (account == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No Account exists with the primary key " + accountId);
      }
      throw new NoSuchAccountException("No Account exists with the primary key " + accountId);
    }
    return remove(account);
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
