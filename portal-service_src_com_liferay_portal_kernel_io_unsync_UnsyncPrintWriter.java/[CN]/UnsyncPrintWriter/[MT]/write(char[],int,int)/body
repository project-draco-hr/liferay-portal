{
  if (_writer == null) {
    _hasError=true;
  }
 else {
    try {
      _writer.write(charArray,offset,length);
    }
 catch (    InterruptedIOException iioe) {
      Thread currentThread=Thread.currentThread();
      currentThread.interrupt();
    }
catch (    IOException ioe) {
      _hasError=true;
    }
  }
}
