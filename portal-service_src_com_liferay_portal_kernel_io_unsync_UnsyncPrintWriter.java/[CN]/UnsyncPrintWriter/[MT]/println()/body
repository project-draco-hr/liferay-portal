{
  if (_out == null) {
    _hasError=true;
  }
 else {
    try {
      _out.write(_LINE_SEPARATOR);
      if (_autoFlush) {
        _out.flush();
      }
    }
 catch (    InterruptedIOException x) {
      Thread.currentThread().interrupt();
    }
catch (    IOException x) {
      _hasError=true;
    }
  }
}
