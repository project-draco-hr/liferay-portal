{
  if (_writer == null) {
    _hasError=true;
  }
 else {
    try {
      _writer.write(_LINE_SEPARATOR);
      if (_autoFlush) {
        _writer.flush();
      }
    }
 catch (    InterruptedIOException iioe) {
      Thread currentThread=Thread.currentThread();
      currentThread.interrupt();
    }
catch (    IOException ioe) {
      _hasError=true;
    }
  }
}
