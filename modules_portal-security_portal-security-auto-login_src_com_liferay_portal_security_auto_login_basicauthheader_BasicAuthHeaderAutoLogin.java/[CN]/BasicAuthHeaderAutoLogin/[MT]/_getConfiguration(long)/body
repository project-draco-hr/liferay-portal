{
  try {
    BasicAuthHeaderAutoLoginConfiguration configuration=_settingsFactory.getSettings(BasicAuthHeaderAutoLoginConfiguration.class,new CompanyServiceSettingsLocator(companyId,BasicAuthHeaderAutoLoginConstants.SERVICE_NAME));
    return configuration;
  }
 catch (  SettingsException se) {
    _log.error("Unable to get BasicAuthHeaderAutoLogin configuration",se);
  }
  return null;
}
