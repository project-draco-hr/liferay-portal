{
  Session session=null;
  try {
    session=openSession();
    if (igFolder.isNew()) {
      session.save(igFolder);
    }
    session.flush();
    igFolder.setNew(false);
    return igFolder;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
