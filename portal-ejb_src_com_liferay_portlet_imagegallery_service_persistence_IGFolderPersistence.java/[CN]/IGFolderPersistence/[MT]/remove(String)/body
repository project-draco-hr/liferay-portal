{
  Session session=null;
  try {
    session=openSession();
    IGFolder igFolder=(IGFolder)session.get(IGFolder.class,folderId);
    if (igFolder == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No IGFolder exists with the primary key " + folderId);
      }
      throw new NoSuchFolderException("No IGFolder exists with the primary key " + folderId);
    }
    return remove(igFolder);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
