{
  if (userId == null) {
    return false;
  }
  String[] omniAdminUsers=PropsUtil.getArray(PropsUtil.OMNIADMIN_USERS);
  if (omniAdminUsers.length > 0) {
    for (int i=0; i < omniAdminUsers.length; i++) {
      if (omniAdminUsers[i].equals(userId)) {
        return true;
      }
    }
    return false;
  }
 else {
    try {
      User user=UserLocalServiceUtil.getUserById(userId);
      return RoleLocalServiceUtil.hasUserRole(userId,user.getCompanyId(),RoleImpl.ADMINISTRATOR);
    }
 catch (    Exception e) {
      _log.error(e);
      return false;
    }
  }
}
