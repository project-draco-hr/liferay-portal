{
  _portalCacheClusterChannels=new ArrayList<>(_channelNumber);
  for (int i=0; i < _channelNumber; i++) {
    _portalCacheClusterChannels.add(_portalCacheClusterChannelFactory.createPortalCacheClusterChannel());
  }
  if (_portalCacheClusterChannelSelector == null) {
    _portalCacheClusterChannelSelector=new UniformPortalCacheClusterChannelSelector();
  }
}
