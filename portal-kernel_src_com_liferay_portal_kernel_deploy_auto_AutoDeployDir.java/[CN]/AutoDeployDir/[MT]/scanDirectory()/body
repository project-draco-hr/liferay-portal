{
  File[] files=_deployDir.listFiles();
  for (int i=0; i < files.length; i++) {
    File file=files[i];
    String fileName=file.getName().toLowerCase();
    if ((file.isFile()) && (fileName.endsWith(".war") || fileName.endsWith(".zip") || fileName.endsWith(".xml"))) {
      processFile(file);
    }
  }
}
