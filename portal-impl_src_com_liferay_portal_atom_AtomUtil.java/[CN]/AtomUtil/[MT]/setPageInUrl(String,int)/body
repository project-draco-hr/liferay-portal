{
  int pageIndex=url.indexOf("page=");
  if (pageIndex == -1) {
    int markIndex=url.indexOf('?');
    if (markIndex == -1) {
      url+="?";
    }
 else {
      url+="&";
    }
    return url + "page=" + page;
  }
  int endIndex=url.indexOf('&',pageIndex);
  if (endIndex == -1) {
    url=url.substring(0,pageIndex);
  }
 else {
    url=url.substring(0,pageIndex) + url.substring(endIndex + 1);
    url+='&';
  }
  url+="page=" + page;
  return url;
}
