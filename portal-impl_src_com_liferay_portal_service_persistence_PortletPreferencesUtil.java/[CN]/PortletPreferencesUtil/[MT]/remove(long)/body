{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(portletPreferencesId));
  }
  com.liferay.portal.model.PortletPreferences portletPreferences=getPersistence().remove(portletPreferencesId);
  if (listener != null) {
    listener.onAfterRemove(portletPreferences);
  }
  return portletPreferences;
}
