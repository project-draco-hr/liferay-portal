{
  FormStructure formStructure=findByPrimaryKey(id);
  Session session=null;
  try {
    session=openSession();
    FormStructure[] array=new FormStructureImpl[3];
    array[0]=getByUuid_PrevAndNext(session,formStructure,uuid,orderByComparator,true);
    array[1]=formStructure;
    array[2]=getByUuid_PrevAndNext(session,formStructure,uuid,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
