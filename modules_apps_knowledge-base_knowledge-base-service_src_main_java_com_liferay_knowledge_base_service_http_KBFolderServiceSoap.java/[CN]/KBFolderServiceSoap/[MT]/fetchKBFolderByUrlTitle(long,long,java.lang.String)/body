{
  try {
    com.liferay.knowledge.base.model.KBFolder returnValue=KBFolderServiceUtil.fetchKBFolderByUrlTitle(groupId,parentKbFolderId,urlTitle);
    return com.liferay.knowledge.base.model.KBFolderSoap.toSoapModel(returnValue);
