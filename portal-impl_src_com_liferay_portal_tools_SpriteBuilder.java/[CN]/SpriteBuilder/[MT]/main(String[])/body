{
  File inputDir=new File(System.getProperty("sprite.input.dir"));
  String[] inputDirExcludes=StringUtil.split(System.getProperty("sprite.input.dir.excludes"));
  String outputFileName=System.getProperty("sprite.output.file");
  String propertiesFileName=System.getProperty("sprite.properties.file");
  int maxHeight=GetterUtil.getInteger(System.getProperty("sprite.max.height"));
  int maxWidth=GetterUtil.getInteger(System.getProperty("sprite.max.width"));
  new SpriteBuilder(inputDir,inputDirExcludes,outputFileName,propertiesFileName,maxHeight,maxWidth);
}
