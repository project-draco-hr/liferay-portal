{
  String sql=CustomSQLUtil.get(_UPDATE_DL_CONTENT);
  sql=StringUtil.replace(sql,new String[]{"[$COMPANY_ID$]","[$NEW_PATH$]","[$NEW_REPOSITORY_ID$]","[$OLD_PATH$]","[$OLD_REPOSITORY_ID$]"},new String[]{String.valueOf(companyId),newPath,String.valueOf(newRepositoryId),oldPath,String.valueOf(oldRepositoryId)});
  runSQL(sql);
}
