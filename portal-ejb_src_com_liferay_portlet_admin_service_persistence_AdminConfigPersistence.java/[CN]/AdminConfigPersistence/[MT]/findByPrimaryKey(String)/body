{
  com.liferay.portlet.admin.model.AdminConfig adminConfig=AdminConfigPool.get(configId);
  Session session=null;
  try {
    if (adminConfig == null) {
      session=openSession();
      AdminConfigHBM adminConfigHBM=(AdminConfigHBM)session.get(AdminConfigHBM.class,configId);
      if (adminConfigHBM == null) {
        _log.warn("No AdminConfig exists with the primary key " + configId.toString());
        throw new NoSuchConfigException("No AdminConfig exists with the primary key " + configId.toString());
      }
      adminConfig=AdminConfigHBMUtil.model(adminConfigHBM,false);
    }
    return adminConfig;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
