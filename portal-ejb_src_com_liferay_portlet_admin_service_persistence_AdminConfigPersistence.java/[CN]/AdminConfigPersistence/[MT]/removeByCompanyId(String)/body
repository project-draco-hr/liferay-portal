{
  Session session=null;
  try {
    session=openSession();
    StringBuffer query=new StringBuffer();
    query.append("FROM AdminConfig IN CLASS com.liferay.portlet.admin.service.persistence.AdminConfigHBM WHERE ");
    query.append("companyId = ?");
    query.append(" ");
    Query q=session.createQuery(query.toString());
    int queryPos=0;
    q.setString(queryPos++,companyId);
    Iterator itr=q.list().iterator();
    while (itr.hasNext()) {
      AdminConfigHBM adminConfigHBM=(AdminConfigHBM)itr.next();
      AdminConfigPool.remove((String)adminConfigHBM.getPrimaryKey());
      session.delete(adminConfigHBM);
    }
    session.flush();
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
