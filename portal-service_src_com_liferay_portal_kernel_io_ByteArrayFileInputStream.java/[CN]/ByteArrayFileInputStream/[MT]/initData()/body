{
  if (data != null) {
    return;
  }
  int arraySize=(int)this.fileSize;
  data=new byte[arraySize];
  FileInputStream fis=new FileInputStream(file);
  int offset=0;
  int length=0;
  try {
    while (offset < arraySize) {
      length=fis.read(data,offset,arraySize - offset);
      offset+=length;
    }
  }
  finally {
    fis.close();
  }
}
