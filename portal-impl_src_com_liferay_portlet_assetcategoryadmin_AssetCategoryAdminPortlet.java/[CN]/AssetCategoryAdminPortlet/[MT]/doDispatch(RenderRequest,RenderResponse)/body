{
  if (SessionErrors.contains(renderRequest,NoSuchCategoryException.class.getName()) || SessionErrors.contains(renderRequest,NoSuchVocabularyException.class.getName()) || SessionErrors.contains(renderRequest,PrincipalException.class.getName())) {
    include("/error.jsp",renderRequest,renderResponse);
  }
 else {
    super.doDispatch(renderRequest,renderResponse);
  }
}
