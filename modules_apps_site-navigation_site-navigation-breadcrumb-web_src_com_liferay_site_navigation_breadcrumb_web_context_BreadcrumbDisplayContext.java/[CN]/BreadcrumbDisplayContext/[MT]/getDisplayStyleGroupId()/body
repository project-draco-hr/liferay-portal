{
  if (_displayStyleGroupId != 0) {
    return _displayStyleGroupId;
  }
  ThemeDisplay themeDisplay=(ThemeDisplay)_request.getAttribute(WebKeys.THEME_DISPLAY);
  _displayStyleGroupId=ParamUtil.getLong(_request,"displayStyleGroupId",_breadcrumbPortletInstanceConfiguration.displayStyleGroupId(themeDisplay.getSiteGroupId()));
  return _displayStyleGroupId;
}
