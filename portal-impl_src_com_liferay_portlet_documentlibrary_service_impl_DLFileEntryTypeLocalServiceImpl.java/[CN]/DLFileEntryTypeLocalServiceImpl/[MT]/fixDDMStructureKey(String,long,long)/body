{
  DDMStructure ddmStructure=DDMStructureManagerUtil.fetchStructure(groupId,classNameLocalService.getClassNameId(DLFileEntryMetadata.class),DLUtil.getDeprecatedDDMStructureKey(fileEntryTypeId));
  if (ddmStructure != null) {
    DDMStructureManagerUtil.updateStructureKey(ddmStructure.getStructureId(),DLUtil.getDDMStructureKey(fileEntryTypeUuid));
  }
}
