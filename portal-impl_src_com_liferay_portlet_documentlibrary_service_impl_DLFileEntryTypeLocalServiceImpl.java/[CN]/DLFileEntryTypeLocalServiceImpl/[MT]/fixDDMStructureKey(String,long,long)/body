{
  DDMStructure ddmStructure=ddmStructureLocalService.fetchStructure(groupId,classNameLocalService.getClassNameId(DLFileEntryMetadata.class),DLUtil.getDeprecatedDDMStructureKey(fileEntryTypeId));
  if (ddmStructure != null) {
    ddmStructure.setStructureKey(DLUtil.getDDMStructureKey(fileEntryTypeUuid));
    ddmStructureLocalService.updateDDMStructure(ddmStructure);
  }
}
