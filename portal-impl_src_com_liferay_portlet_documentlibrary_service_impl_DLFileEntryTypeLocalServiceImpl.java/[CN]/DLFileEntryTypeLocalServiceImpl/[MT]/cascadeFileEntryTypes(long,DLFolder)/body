{
  long groupId=dlFolder.getGroupId();
  long folderId=dlFolder.getFolderId();
  List<DLFileEntryType> fileEntryTypes=getFileEntryTypesByFolder(groupId,folderId,true);
  List<Long> fileEntryTypeIds=getFileEntryTypeIds(fileEntryTypes);
  long defaultFileEntryTypeId=getDefaultFileEntryType(groupId,folderId);
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setUserId(userId);
  serviceContext.setScopeGroupId(groupId);
  serviceContext.setCompanyId(dlFolder.getCompanyId());
  serviceContext.setAttribute("fileEntryTypeId",defaultFileEntryTypeId);
  cascadeFileEntryTypes(groupId,folderId,fileEntryTypeIds,serviceContext);
}
