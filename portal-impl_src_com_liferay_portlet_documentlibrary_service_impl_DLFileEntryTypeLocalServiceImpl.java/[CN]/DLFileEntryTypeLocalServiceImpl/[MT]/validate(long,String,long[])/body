{
  DLFileEntryType dlFileEntryType=dlFileEntryTypePersistence.fetchByG_N(groupId,fileEntryTypeName);
  if (dlFileEntryType != null) {
    throw new DuplicateFileEntryTypeException(fileEntryTypeName);
  }
  if (ddmStructureIds.length == 0) {
    throw new NoSuchMetadataSetException();
  }
  for (  long ddmStructureId : ddmStructureIds) {
    DDMStructure ddmStructure=ddmStructurePersistence.fetchByPrimaryKey(ddmStructureId);
    if (ddmStructure == null) {
      throw new NoSuchMetadataSetException();
    }
  }
}
