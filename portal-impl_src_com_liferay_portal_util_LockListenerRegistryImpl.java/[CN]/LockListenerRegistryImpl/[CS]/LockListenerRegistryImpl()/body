{
  String[] lockListenerClassNames=PropsUtil.getArray(PropsKeys.LOCK_LISTENERS);
  for (  String lockListenerClassName : lockListenerClassNames) {
    try {
      LockListener lockListener=(LockListener)InstanceFactory.newInstance(lockListenerClassName);
      register(lockListener);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
}
