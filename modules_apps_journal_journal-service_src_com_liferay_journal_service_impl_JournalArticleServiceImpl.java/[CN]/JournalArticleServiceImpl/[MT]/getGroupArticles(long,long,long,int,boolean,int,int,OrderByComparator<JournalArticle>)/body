{
  List<Long> folderIds=new ArrayList<>();
  if (rootFolderId != JournalFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    folderIds=journalFolderService.getFolderIds(groupId,rootFolderId);
  }
  QueryDefinition<JournalArticle> queryDefinition=new QueryDefinition<>(status,start,end,orderByComparator);
  return journalArticleFinder.filterFindByG_U_F_C(groupId,userId,folderIds,JournalArticleConstants.CLASSNAME_ID_DEFAULT,includeOwner,queryDefinition);
}
