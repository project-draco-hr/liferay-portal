{
  try {
    com.liferay.portlet.imagegallery.model.IGFolder returnValue=IGFolderServiceUtil.updateFolder(folderId,parentFolderId,name,description,mergeWithParentFolder);
    return com.liferay.portlet.imagegallery.model.IGFolderSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
