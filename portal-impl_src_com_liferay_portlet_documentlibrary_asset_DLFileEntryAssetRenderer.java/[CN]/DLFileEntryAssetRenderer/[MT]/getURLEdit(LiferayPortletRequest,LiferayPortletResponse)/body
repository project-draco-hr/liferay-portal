{
  PortletURL editPortletURL=liferayPortletResponse.createRenderURL(PortletKeys.DOCUMENT_LIBRARY);
  editPortletURL.setParameter("struts_action","/document_library/edit_file_entry");
  editPortletURL.setParameter("folderId",String.valueOf(_fileEntry.getFolderId()));
  editPortletURL.setParameter("name",String.valueOf(_fileEntry.getName()));
  return editPortletURL;
}
