{
  DLFileEntry dlFileEntry=(DLFileEntry)_fileEntry.getModel();
  if ((dlFileEntry != null) && dlFileEntry.isInTrashFolder()) {
    renderRequest.setAttribute(WebKeys.DOCUMENT_LIBRARY_FILE_ENTRY,_fileEntry);
    renderRequest.setAttribute(WebKeys.DOCUMENT_LIBRARY_FILE_VERSION,_fileVersion);
    return "/html/portlet/document_library/trash/file_entry_restore.jsp";
  }
 else {
    return null;
  }
}
