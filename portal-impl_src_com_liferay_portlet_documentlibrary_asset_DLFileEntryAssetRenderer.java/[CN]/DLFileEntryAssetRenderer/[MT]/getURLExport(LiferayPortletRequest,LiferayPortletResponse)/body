{
  PortletURL exportPortletURL=liferayPortletResponse.createActionURL();
  exportPortletURL.setParameter("struts_action","/asset_publisher/get_file");
  exportPortletURL.setParameter("groupId",String.valueOf(_entry.getGroupId()));
  exportPortletURL.setParameter("folderId",String.valueOf(_entry.getFolderId()));
  exportPortletURL.setParameter("title",String.valueOf(_entry.getTitle()));
  return exportPortletURL;
}
