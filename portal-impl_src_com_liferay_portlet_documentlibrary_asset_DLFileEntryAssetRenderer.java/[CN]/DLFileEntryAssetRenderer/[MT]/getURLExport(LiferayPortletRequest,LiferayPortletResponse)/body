{
  PortletURL exportPortletURL=liferayPortletResponse.createActionURL();
  exportPortletURL.setParameter("struts_action","/asset_publisher/get_file");
  exportPortletURL.setParameter("groupId",String.valueOf(_fileEntry.getGroupId()));
  exportPortletURL.setParameter("folderId",String.valueOf(_fileEntry.getFolderId()));
  exportPortletURL.setParameter("title",String.valueOf(_fileEntry.getTitle()));
  return exportPortletURL;
}
