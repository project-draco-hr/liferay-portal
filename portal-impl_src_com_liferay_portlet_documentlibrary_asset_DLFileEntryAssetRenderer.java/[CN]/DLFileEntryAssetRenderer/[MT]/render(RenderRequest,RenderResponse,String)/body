{
  if (template.equals(TEMPLATE_ABSTRACT) || template.equals(TEMPLATE_FULL_CONTENT)) {
    renderRequest.setAttribute(WebKeys.DOCUMENT_LIBRARY_FILE_ENTRY,_entry);
    renderRequest.setAttribute(WebKeys.DOCUMENT_LIBRARY_FILE_VERSION,_version);
    return "/html/portlet/document_library/asset/" + template + ".jsp";
  }
 else {
    return null;
  }
}
