{
  if (template.equals(TEMPLATE_ABSTRACT) || template.equals(TEMPLATE_FULL_CONTENT)) {
    renderRequest.setAttribute(WebKeys.DOCUMENT_LIBRARY_FILE_ENTRY,_fileEntry);
    String version=ParamUtil.getString(renderRequest,"version");
    if ((_type == AssetRendererFactory.TYPE_LATEST) || Validator.isNotNull(version)) {
      renderRequest.setAttribute(WebKeys.DOCUMENT_LIBRARY_FILE_VERSION,_fileVersion);
    }
    return "/html/portlet/document_library/asset/" + template + ".jsp";
  }
 else {
    return null;
  }
}
