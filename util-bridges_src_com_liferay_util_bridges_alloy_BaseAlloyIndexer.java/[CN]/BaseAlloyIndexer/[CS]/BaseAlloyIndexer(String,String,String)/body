{
  this.portletId=portletId;
  classNames=new String[]{className};
  String[] classNameParameters=StringUtil.split(className,StringPool.PERIOD);
  String methodName=classNameParameters[classNameParameters.length - 1];
  getModelMethodKey=new MethodKey(serviceClassName,"get" + methodName,long.class);
  getModelsCountMethodKey=new MethodKey(serviceClassName,"get" + TextFormatter.formatPlural(methodName) + "Count");
  getModelsMethodKey=new MethodKey(serviceClassName,"get" + TextFormatter.formatPlural(methodName),int.class,int.class);
  try {
    MethodCache.get(getModelMethodKey);
    MethodCache.get(getModelsCountMethodKey);
    MethodCache.get(getModelsMethodKey);
  }
 catch (  Exception e) {
  }
}
