{
  try {
    BatchEvent batchEvent=_batchEvents.get(syncAccountId);
    if ((batchEvent != null) && !batchEvent.isClosed()) {
      return batchEvent;
    }
    batchEvent=new BatchEvent(syncAccountId);
    _batchEvents.put(syncAccountId,batchEvent);
    return batchEvent;
  }
 catch (  Exception e) {
    return null;
  }
}
