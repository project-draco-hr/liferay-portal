{
  FileVersion fileVersion=(FileVersion)message.getPayload();
  try {
    RawMetadataProcessorUtil.saveMetadata(fileVersion);
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to save metadata for file version " + fileVersion.getFileVersionId(),e);
    }
  }
  if (PropsValues.DL_FILE_ENTRY_PROCESSORS_TRIGGER_SYNCHRONOUSLY) {
    MessageBusUtil.sendMessage(message.getResponseDestinationName(),message);
  }
}
