{
  if (userContext == null || userContext.getUserContextKey() == null || userContext.getUserContextKey().equals(WSRPSpecKeys.WSRP_GUEST_KEY)) {
    User user=null;
    try {
      user=_getAuthlessUser();
      return user.getScreenName();
    }
 catch (    ProducerException pe) {
      _log.error(pe,pe);
    }
  }
  return userContext.getUserContextKey();
}
