{
  if (userContext == null || userContext.getUserContextKey() == null) {
    return _getAuthlessUser().getScreenName();
  }
  String userKey=userContext.getUserContextKey();
  if (userKey.equals(WSRPSpecKeys.WSRP_GUEST_KEY) || userKey.equals(_getAuthlessUser().getScreenName())) {
    return _getAuthlessUser().getScreenName();
  }
 else {
    return userContext.getUserContextKey() + StringPool.UNDERLINE + registrationHandle+ StringPool.UNDERLINE+ _producer.getProducerKey();
  }
}
