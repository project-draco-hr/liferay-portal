{
  if (!(resourceResponse instanceof MimeResponse)) {
    return false;
  }
  if (!isRSSFeedsEnabled(resourceRequest)) {
    try {
      PortalUtil.sendRSSFeedsDisabledError(resourceRequest,resourceResponse);
    }
 catch (    Exception e) {
    }
    return false;
  }
  MimeResponse mimeResponse=(MimeResponse)resourceResponse;
  try {
    PortletResponseUtil.sendFile(resourceRequest,mimeResponse,null,getRSS(resourceRequest,resourceResponse),ContentTypes.TEXT_XML_UTF8);
  }
 catch (  Exception e) {
    throw new PortletException(e);
  }
  return true;
}
