{
  if (_dispatcher == Dispatcher.ERROR) {
    return (String)request.getAttribute(JavaConstants.JAVAX_SERVLET_ERROR_REQUEST_URI);
  }
 else   if (_dispatcher == Dispatcher.INCLUDE) {
    return (String)request.getAttribute(JavaConstants.JAVAX_SERVLET_INCLUDE_REQUEST_URI);
  }
 else {
    return request.getRequestURI();
  }
}
