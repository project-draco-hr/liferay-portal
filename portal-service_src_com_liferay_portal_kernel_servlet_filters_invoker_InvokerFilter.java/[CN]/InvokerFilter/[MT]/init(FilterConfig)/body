{
  try {
    ServletContext servletContext=filterConfig.getServletContext();
    readLiferayFilterWebXML(servletContext,"/WEB-INF/liferay-web.xml");
    servletContext.setAttribute(getClass().getName(),this);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new ServletException(e);
  }
}
