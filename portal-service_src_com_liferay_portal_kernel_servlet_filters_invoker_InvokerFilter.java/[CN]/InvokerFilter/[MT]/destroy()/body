{
  for (  Map.Entry<String,Filter> entry : _filters.entrySet()) {
    Filter filter=entry.getValue();
    try {
      filter.destroy();
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
  if (_filterChains != null) {
    _filterChains.clear();
  }
  _filterConfigs.clear();
  _filterMappings.clear();
  _filters.clear();
}
