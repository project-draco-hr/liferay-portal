{
  List categories=new ArrayList();
  Iterator itr=mbCategoryLocalService.getCategories(groupId,parentCategoryId,begin,end).iterator();
  while (itr.hasNext()) {
    MBCategory category=(MBCategory)itr.next();
    if (MBCategoryPermission.contains(getPermissionChecker(),category,ActionKeys.VIEW)) {
      categories.add(category);
    }
  }
  return categories;
}
