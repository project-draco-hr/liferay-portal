{
  MBCategoryPermission.check(getPermissionChecker(),categoryId,ActionKeys.UPDATE);
  return mbCategoryLocalService.updateCategory(getUserId(),categoryId,parentCategoryId,name,description,mergeWithParentCategory,mailingListAddress,mailAddress,mailInProtocol,mailInServerName,mailInUseSSL,mailInServerPort,mailInUserName,mailInPassword,mailInReadInterval,mailOutConfigured,mailOutServerName,mailOutUseSSL,mailOutServerPort,mailOutUserName,mailOutPassword);
}
