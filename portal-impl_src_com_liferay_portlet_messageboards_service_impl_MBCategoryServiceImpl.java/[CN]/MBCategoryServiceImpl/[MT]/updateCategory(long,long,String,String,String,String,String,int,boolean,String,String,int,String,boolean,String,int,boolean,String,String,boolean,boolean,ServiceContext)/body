{
  MBCategoryPermission.check(getPermissionChecker(),categoryId,ActionKeys.UPDATE);
  return mbCategoryLocalService.updateCategory(categoryId,parentCategoryId,name,description,emailAddress,inProtocol,inServerName,inServerPort,inUseSSL,inUserName,inPassword,inReadInterval,outEmailAddress,outCustom,outServerName,outServerPort,outUseSSL,outUserName,outPassword,mailingListActive,mergeWithParentCategory,serviceContext);
}
