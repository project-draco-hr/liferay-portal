{
  Folder folder=_getFolder(themeDisplay,portletRequest);
  MenuItem portletTitleAddFolderMenuItem=_dlPortletToolbarContributor.getPortletTitleAddFolderMenuItem(themeDisplay,portletRequest,folder);
  if (portletTitleAddFolderMenuItem != null) {
    menuItems.add(portletTitleAddFolderMenuItem);
  }
}
