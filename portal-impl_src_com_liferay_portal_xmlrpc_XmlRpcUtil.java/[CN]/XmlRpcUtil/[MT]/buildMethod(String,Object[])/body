{
  StringBundler sb=new StringBundler();
  sb.append("<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n");
  sb.append("<methodCall>\n");
  sb.append("<methodName>" + methodName + "</methodName>\n");
  sb.append("<params>\n");
  if (args != null) {
    for (    Object arg : args) {
      sb.append("<param>" + wrapValue(arg) + "</param>\n");
    }
  }
  sb.append("</params>\n");
  sb.append("</methodCall>");
  return sb.toString();
}
