{
  List<String> badSymbols=new ArrayList<>();
  String[] symbols=portletPreferences.getValues("symbols",new String[0]);
  for (  String symbol : symbols) {
    CurrencyConverter currencyConverter=CurrencyConverterUtil.getCurrencyConverter(symbol);
    if (currencyConverter == null) {
      badSymbols.add(symbol);
    }
  }
  if (!badSymbols.isEmpty()) {
    throw new ValidatorException("Unable to retrieve symbols",badSymbols);
  }
}
