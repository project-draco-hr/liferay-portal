{
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext();
  serviceContext.setScopeGroupId(_group.getGroupId());
  BaseModel<?> parentBaseModel=getParentBaseModel(_group,serviceContext);
  SearchContext searchContext=ServiceTestUtil.getSearchContext();
  searchContext.setGroupIds(assetEntryQuery.getGroupIds());
  int initialEntries=searchCount(assetEntryQuery,searchContext);
  addBaseModelWithClassType(parentBaseModel,getSearchKeywords(),serviceContext);
  addBaseModelWithClassType(parentBaseModel,getSearchKeywords(),serviceContext);
  searchContext.setClassTypeIds(getClassTypeIds());
  Assert.assertEquals(initialEntries + expectedResults,searchCount(assetEntryQuery,searchContext));
}
