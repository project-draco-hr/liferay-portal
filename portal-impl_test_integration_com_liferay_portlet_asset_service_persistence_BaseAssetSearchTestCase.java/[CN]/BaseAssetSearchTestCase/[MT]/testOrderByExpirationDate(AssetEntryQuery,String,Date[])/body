{
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext(_group.getGroupId());
  BaseModel<?> parentBaseModel=getParentBaseModel(_group,serviceContext);
  SearchContext searchContext=ServiceTestUtil.getSearchContext();
  searchContext.setGroupIds(assetEntryQuery.getGroupIds());
  for (  Date expirationDate : expirationDates) {
    addBaseModel(parentBaseModel,ServiceTestUtil.randomString(),expirationDate,serviceContext);
  }
  assetEntryQuery.setOrderByCol1("expirationDate");
  assetEntryQuery.setOrderByType1(orderByType);
  Arrays.sort(expirationDates);
  DateFormat dateFormat=DateFormatFactoryUtil.getSimpleDateFormat(PropsValues.INDEX_DATE_FORMAT_PATTERN);
  Document[] documents=search(assetEntryQuery,searchContext);
  for (int i=0; i < documents.length; i++) {
    Document document=documents[i];
    String field=document.get("expirationDate");
    int index=i;
    if (orderByType.equals("desc")) {
      index=documents.length - 1 - i;
    }
    Assert.assertEquals(field,dateFormat.format(expirationDates[index]));
  }
}
