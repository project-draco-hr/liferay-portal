{
  Path dirNamePath=dirPath.getFileName();
  String fileName=".version-override-" + dirNamePath.toString() + ".properties";
  while (true) {
    Path path=dirPath.resolve(fileName);
    if (Files.exists(path)) {
      return path;
    }
    dirPath=dirPath.getParent();
    if (dirPath == null) {
      return null;
    }
  }
}
