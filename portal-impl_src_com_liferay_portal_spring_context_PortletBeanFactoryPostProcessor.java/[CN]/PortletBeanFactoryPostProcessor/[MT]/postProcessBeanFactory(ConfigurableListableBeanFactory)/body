{
  ClassLoader beanClassLoader=AggregateClassLoader.getAggregateClassLoader(PortletClassLoaderUtil.getClassLoader(),PortalClassLoaderUtil.getClassLoader());
  if (ServerDetector.isJBoss()) {
    beanClassLoader=new JBossClassLoader(beanClassLoader);
  }
  beanFactory.setBeanClassLoader(beanClassLoader);
}
