{
  lock.lock();
  try {
    if (jumps == null) {
      jumps=new ArrayList();
    }
    if (jumps.size() <= jumpNumber) {
      for (int i=jumps.size(); i <= jumpNumber; jumps.add(new JumpData(i++)))       ;
    }
    return (JumpData)jumps.get(jumpNumber);
  }
  finally {
    lock.unlock();
  }
}
