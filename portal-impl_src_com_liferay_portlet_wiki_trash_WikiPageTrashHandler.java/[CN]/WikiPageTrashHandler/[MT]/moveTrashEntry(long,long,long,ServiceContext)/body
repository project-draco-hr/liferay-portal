{
  WikiPage page=WikiPageLocalServiceUtil.getPage(classPK);
  WikiPage parentPage=WikiPageLocalServiceUtil.getPage(containerModelId);
  String parentPageTitle=parentPage.getTitle();
  if (parentPage.isInTrash()) {
    parentPageTitle=StringPool.BLANK;
  }
  WikiPageLocalServiceUtil.changeParentAndRestoreFromTrash(userId,page.getNodeId(),page.getTitle(),parentPageTitle,serviceContext);
}
