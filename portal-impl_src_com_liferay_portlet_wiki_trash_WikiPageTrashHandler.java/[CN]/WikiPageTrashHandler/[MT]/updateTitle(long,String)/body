{
  WikiPage page=WikiPageLocalServiceUtil.getPage(classPK);
  page.setTitle(name);
  WikiPageLocalServiceUtil.updateWikiPage(page,false);
  WikiPageResource pageResource=WikiPageResourceLocalServiceUtil.getPageResource(page.getResourcePrimKey());
  pageResource.setTitle(name);
  WikiPageResourceLocalServiceUtil.updateWikiPageResource(pageResource,false);
}
