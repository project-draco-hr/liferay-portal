{
  FileEntry fileEntry=PortletFileRepositoryUtil.getPortletFileEntry(classPK);
  DLFileEntry dlFileEntry=(DLFileEntry)fileEntry.getModel();
  WikiPage page=WikiPageLocalServiceUtil.getPage(dlFileEntry.getClassPK());
  WikiPageServiceUtil.restorePageAttachmentFromTrash(page.getNodeId(),page.getTitle(),fileEntry.getTitle());
}
