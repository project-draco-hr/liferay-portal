{
  DLDocumentMetadataSet newDLDocumentMetadataSet=addDLDocumentMetadataSet();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLDocumentMetadataSet.class,DLDocumentMetadataSet.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("metadataSetId"));
  Object newMetadataSetId=newDLDocumentMetadataSet.getMetadataSetId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("metadataSetId",new Object[]{newMetadataSetId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Object existingMetadataSetId=result.get(0);
  assertEquals(existingMetadataSetId,newMetadataSetId);
}
