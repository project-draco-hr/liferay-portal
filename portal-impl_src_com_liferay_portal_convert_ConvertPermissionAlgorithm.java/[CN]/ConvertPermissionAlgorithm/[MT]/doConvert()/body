{
  try {
    BatchSessionUtil.setEnabled(true);
    PermissionThreadLocal.setSkipAddResource(true);
    _convert();
  }
 catch (  Exception e) {
    _log.fatal(e,e);
  }
 finally {
    CacheRegistry.clear();
    BatchSessionUtil.setEnabled(false);
    PermissionThreadLocal.setSkipAddResource(false);
  }
}
