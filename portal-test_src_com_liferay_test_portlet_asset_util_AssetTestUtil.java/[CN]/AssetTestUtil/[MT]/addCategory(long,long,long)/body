{
  Map<Locale,String> titleMap=new HashMap<Locale,String>();
  Locale locale=LocaleUtil.getSiteDefault();
  titleMap.put(locale,RandomTestUtil.randomString());
  Map<Locale,String> descriptionMap=new HashMap<Locale,String>();
  descriptionMap.put(locale,RandomTestUtil.randomString());
  String[] categoryProperties=null;
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext(groupId,TestPropsValues.getUserId());
  return AssetCategoryLocalServiceUtil.addCategory(TestPropsValues.getUserId(),parentCategoryId,titleMap,descriptionMap,vocabularyId,categoryProperties,serviceContext);
}
