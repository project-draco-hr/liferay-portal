{
  String content="Enterprise. Open Source. For Life.";
  Response response=testUpload(content.getBytes());
  Assert.assertEquals(200,response.getCode());
  JSONObject jsonObject=JSONFactoryUtil.createJSONObject(response.getBody());
  Assert.assertTrue(jsonObject.getBoolean("success"));
  String key=group.getGroupId() + "_0_" + TestUploadPortlet.PARAMETER_NAME;
  TestFileEntry actualTestFileEntry=_testUploadPortlet.get(key);
  Assert.assertNotNull(actualTestFileEntry);
  Assert.assertEquals(content,StringUtil.read(actualTestFileEntry.getInputStream()));
}
