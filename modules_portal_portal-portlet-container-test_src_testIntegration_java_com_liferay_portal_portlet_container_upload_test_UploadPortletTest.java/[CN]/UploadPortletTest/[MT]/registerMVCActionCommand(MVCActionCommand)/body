{
  Bundle bundle=FrameworkUtil.getBundle(getClass());
  BundleContext bundleContext=bundle.getBundleContext();
  Dictionary<String,Object> properties=new HashMapDictionary<>();
  properties.put("javax.portlet.name",TestUploadPortlet.PORTLET_NAME);
  properties.put("mvc.command.name",TestUploadPortlet.MVC_COMMAND_NAME);
  ServiceRegistration<MVCActionCommand> serviceRegistration=bundleContext.registerService(MVCActionCommand.class,mvcActionCommand,properties);
  serviceRegistrations.add(serviceRegistration);
}
