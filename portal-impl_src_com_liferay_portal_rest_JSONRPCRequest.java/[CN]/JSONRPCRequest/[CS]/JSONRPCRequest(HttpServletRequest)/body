{
  _valid=true;
  try {
    String requestBody=ServletUtil.readRequestBody(request);
    HashMap map=(HashMap)new JSONDeserializer().use(null,HashMap.class).use("parameters",HashMap.class).deserialize(requestBody);
    _method=(String)map.get("method");
    _parameters=(Map<String,String>)map.get("params");
  }
 catch (  IOException ioe) {
    _log.error(ioe,ioe);
    _valid=false;
  }
}
