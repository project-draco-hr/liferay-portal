{
  selenium.select("_26_id","label=English to French");
  selenium.type("_26_text","My name is Liferay Translator, fluent in over 6 million forms of communication.");
  selenium.click("//input[@value='Translate']");
  selenium.waitForPageToLoad("30000");
  verifyTrue(selenium.isTextPresent("Mon nom est traducteur de Liferay, fluent dans plus de 6 millions de formes de communication."));
}
