{
  selenium.open("/web/guest/home/");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  selenium.clickAt("link=Web Content",RuntimeVariables.replace("Web Content"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertEquals(RuntimeVariables.replace("You do not have the roles required to access this portlet."),selenium.getText("//div[@class='portlet-msg-error']"));
  assertFalse(selenium.isTextPresent("Web Content Name"));
}
