{
  OrderByComparator<EmailAddress> obc=OrderByComparatorFactoryUtil.create(EmailAddressModelImpl.TABLE_NAME,"address",true);
  Assert.assertEquals("EmailAddress.address ASC",obc.getOrderBy());
  obc=OrderByComparatorFactoryUtil.create(EmailAddressModelImpl.TABLE_NAME,"address",false);
  Assert.assertEquals("EmailAddress.address DESC",obc.getOrderBy());
}
