{
  Subscription subscription=subscriptionPersistence.fetchByPrimaryKey(subscriptionId);
  String className=PortalUtil.getClassName(subscription.getClassNameId());
  if (assetEntryPersistence.countByC_C(subscription.getClassNameId(),subscription.getClassPK()) > 0) {
    socialEquityLogLocalService.deactivateEquityLogs(subscription.getUserId(),className,subscription.getClassPK(),ActionKeys.SUBSCRIBE,StringPool.BLANK);
  }
  subscriptionPersistence.remove(subscriptionId);
}
