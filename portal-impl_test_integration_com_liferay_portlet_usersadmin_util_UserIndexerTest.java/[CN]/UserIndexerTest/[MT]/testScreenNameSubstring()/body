{
  addUserScreenName("Open4Life");
  User user=assertSearchOneUser("open lite");
  Assert.assertEquals("open4life",user.getScreenName());
  user=assertSearchOneUser("OPE");
  Assert.assertEquals("open4life",user.getScreenName());
  user=assertSearchOneUser("4lif");
  Assert.assertEquals("open4life",user.getScreenName());
}
