{
  Assume.assumeTrue(isAPIWithoutQueryParserImplementedForSearchEngine());
  User user=addUser();
  User user1=assertSearchOneUser("@");
  Assert.assertEquals(user.getEmailAddress(),user1.getEmailAddress());
  assertHits("@" + RandomTestUtil.randomString(),0);
  assertHits("!",0);
  assertHits("!" + RandomTestUtil.randomString(),0);
}
