{
  List<BookmarksFolder> folders=bookmarksFolderPersistence.filterFindByG_P_S(groupId,folderId,WorkflowConstants.STATUS_APPROVED);
  for (  BookmarksFolder folder : folders) {
    if (folder.isInTrashContainer()) {
      continue;
    }
    folderIds.add(folder.getFolderId());
    getSubfolderIds(folderIds,folder.getGroupId(),folder.getFolderId());
  }
}
