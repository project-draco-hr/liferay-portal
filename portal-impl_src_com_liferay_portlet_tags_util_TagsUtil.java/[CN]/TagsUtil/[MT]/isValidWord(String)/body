{
  if (Validator.isNull(word)) {
    return false;
  }
 else {
    char[] wordCharArray=word.toCharArray();
    for (int i=0; i < wordCharArray.length; i++) {
      char c=wordCharArray[i];
      if (!Validator.isChar(c) && !Validator.isDigit(c) && (c != CharPool.CLOSE_PARENTHESIS)&& (c != CharPool.DASH)&& (c != CharPool.OPEN_PARENTHESIS)&& (c != CharPool.PERIOD)&& (c != CharPool.SPACE)&& (c != CharPool.UNDERLINE)) {
        return false;
      }
    }
  }
  return true;
}
