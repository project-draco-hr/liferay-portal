{
  if (Validator.isNull(word)) {
    return false;
  }
 else {
    char[] wordCharArray=word.toCharArray();
    for (    char c : wordCharArray) {
      for (      char invalidChar : INVALID_CHARACTERS) {
        if (c == invalidChar) {
          return false;
        }
      }
    }
  }
  return true;
}
