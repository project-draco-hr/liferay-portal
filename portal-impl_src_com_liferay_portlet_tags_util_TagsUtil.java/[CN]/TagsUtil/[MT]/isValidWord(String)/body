{
  if (Validator.isNull(word)) {
    return false;
  }
 else {
    char[] c=word.toCharArray();
    for (int i=0; i < c.length; i++) {
      if (!Validator.isChar(c[i]) && !Validator.isDigit(c[i]) && (c[i] != ' ')&& (c[i] != '.')&& (c[i] != '-')&& (c[i] != '_')&& (c[i] != '(')&& (c[i] != ')')) {
        return false;
      }
    }
  }
  return true;
}
