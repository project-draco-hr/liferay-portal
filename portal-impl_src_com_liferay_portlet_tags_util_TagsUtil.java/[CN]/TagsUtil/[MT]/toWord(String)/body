{
  if (Validator.isNull(text)) {
    return text;
  }
 else {
    char[] textCharArray=text.toCharArray();
    for (int i=0; i < textCharArray.length; i++) {
      char c=textCharArray[i];
      if (!Validator.isChar(c) && !Validator.isDigit(c) && (c != CharPool.CLOSE_PARENTHESIS)&& (c != CharPool.DASH)&& (c != CharPool.OPEN_PARENTHESIS)&& (c != CharPool.PERIOD)&& (c != CharPool.SPACE)&& (c != CharPool.UNDERLINE)) {
        textCharArray[i]=CharPool.SPACE;
      }
    }
    return new String(textCharArray);
  }
}
