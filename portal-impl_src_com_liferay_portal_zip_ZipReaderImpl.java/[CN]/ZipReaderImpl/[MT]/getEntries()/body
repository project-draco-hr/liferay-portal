{
  List<String> folderEntries=new ArrayList<String>();
  File[] files=(File[])_zipFile.listFiles();
  for (  File file : files) {
    if (!file.isDirectory()) {
      folderEntries.add(file.getEnclEntryName());
    }
 else {
      processDirectory(file,folderEntries);
    }
  }
  return folderEntries;
}
