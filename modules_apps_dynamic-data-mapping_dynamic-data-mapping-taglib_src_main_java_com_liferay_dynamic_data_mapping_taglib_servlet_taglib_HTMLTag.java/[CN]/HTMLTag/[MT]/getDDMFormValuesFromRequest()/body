{
  String serializedDDMFormValues=ParamUtil.getString(request,getDDMFormValuesInputName());
  if (Validator.isNotNull(serializedDDMFormValues)) {
    DDMForm ddmForm=getDDMForm();
    try {
      return DDMFormValuesJSONDeserializerUtil.deserialize(ddmForm,serializedDDMFormValues);
    }
 catch (    PortalException pe) {
      if (_log.isDebugEnabled()) {
        _log.debug(pe,pe);
      }
    }
  }
  return null;
}
