{
  String cmd=ParamUtil.getString(resourceRequest,Constants.CMD);
  PortletRequestDispatcher portletRequestDispatcher=null;
  if (cmd.equals(Constants.IMPORT)) {
    portletRequestDispatcher=getPortletRequestDispatcher(resourceRequest,"/import/processes_list/import_layouts_processes.jsp");
  }
 else {
    portletRequestDispatcher=getPortletRequestDispatcher(resourceRequest,"/import/new_import/import_layouts_resources.jsp");
  }
  portletRequestDispatcher.include(resourceRequest,resourceResponse);
}
