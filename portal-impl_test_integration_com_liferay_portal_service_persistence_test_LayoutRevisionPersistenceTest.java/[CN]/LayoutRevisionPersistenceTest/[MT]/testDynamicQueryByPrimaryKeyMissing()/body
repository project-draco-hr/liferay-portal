{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(LayoutRevision.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("layoutRevisionId",RandomTestUtil.nextLong()));
  List<LayoutRevision> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
