{
  Assert.assertEquals("foo",StringUtil.removeChar("foo",'-'));
  Assert.assertEquals("foo.bar.baz.qux",StringUtil.removeChar("foo.bar.baz.qux",'-'));
  Assert.assertEquals("foobar",StringUtil.removeChar("-foo-bar-",'-'));
  Assert.assertEquals("foobarbazqux",StringUtil.removeChar("foo-bar-baz-qux",'-'));
}
