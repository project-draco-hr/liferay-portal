{
  if (checkPermissions) {
    _checkPermission();
  }
  Bundle bundle=getBundle(bundleId);
  if (bundle == null) {
    throw new PortalException("No bundle with ID " + bundleId);
  }
  try {
    bundle.start(options);
  }
 catch (  BundleException be) {
    _log.error(be,be);
    throw new PortalException(be);
  }
}
