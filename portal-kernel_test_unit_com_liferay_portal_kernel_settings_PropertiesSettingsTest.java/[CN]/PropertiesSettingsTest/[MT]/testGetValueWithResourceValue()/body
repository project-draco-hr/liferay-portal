{
  _properties.put(_SINGLE_KEY,_RESOURCE_SINGLE_VALUE);
  when(_mockLocationVariableResolver.isLocationVariable(_RESOURCE_SINGLE_VALUE)).thenReturn(true);
  final String expectedValue="resourceValue";
  when(_mockLocationVariableResolver.resolve(_RESOURCE_SINGLE_VALUE)).thenReturn(expectedValue);
  Assert.assertEquals(expectedValue,_propertiesSettings.getValue(_SINGLE_KEY,null));
}
