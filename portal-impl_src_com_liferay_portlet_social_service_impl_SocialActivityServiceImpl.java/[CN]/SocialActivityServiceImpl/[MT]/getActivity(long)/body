{
  SocialActivity activity=socialActivityLocalService.getActivity(activityId);
  List<SocialActivityInterpreter> activityInterpreters=socialActivityInterpreterLocalService.getActivityInterpreters(StringPool.BLANK);
  if (!hasPermission(activity,activityInterpreters)) {
    throw new PrincipalException.MustHavePermission(0,SocialActivity.class.getName(),activityId);
  }
  return activity;
}
