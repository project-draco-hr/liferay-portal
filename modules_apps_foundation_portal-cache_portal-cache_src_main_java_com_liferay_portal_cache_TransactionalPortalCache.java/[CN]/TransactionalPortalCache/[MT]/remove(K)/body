{
  if (TransactionalPortalCacheHelper.isEnabled()) {
    if (key == null) {
      throw new NullPointerException("Key is null");
    }
    TransactionalPortalCacheHelper.put(portalCache,key,(V)TransactionalPortalCacheHelper.getNullHolder(),DEFAULT_TIME_TO_LIVE);
  }
 else {
    portalCache.remove(key);
  }
}
