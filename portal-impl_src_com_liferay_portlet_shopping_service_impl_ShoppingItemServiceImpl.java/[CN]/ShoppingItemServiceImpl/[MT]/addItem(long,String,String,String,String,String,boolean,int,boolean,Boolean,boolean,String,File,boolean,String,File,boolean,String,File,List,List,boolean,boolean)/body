{
  ShoppingCategoryPermission.check(getPermissionChecker(),categoryId,ActionKeys.ADD_ITEM);
  return shoppingItemLocalService.addItem(getUserId(),categoryId,sku,name,description,properties,fieldsQuantities,requiresShipping,stockQuantity,featured,sale,smallImage,smallImageURL,smallFile,mediumImage,mediumImageURL,mediumFile,largeImage,largeImageURL,largeFile,itemFields,itemPrices,addCommunityPermissions,addGuestPermissions);
}
