{
  ShoppingItemPermission.check(getPermissionChecker(),itemId,ActionKeys.UPDATE);
  return shoppingItemLocalService.updateItem(getUserId(),itemId,categoryId,sku,name,description,properties,fieldsQuantities,requiresShipping,stockQuantity,featured,sale,smallImage,smallImageURL,smallFile,mediumImage,mediumImageURL,mediumFile,largeImage,largeImageURL,largeFile,itemFields,itemPrices,serviceContext);
}
