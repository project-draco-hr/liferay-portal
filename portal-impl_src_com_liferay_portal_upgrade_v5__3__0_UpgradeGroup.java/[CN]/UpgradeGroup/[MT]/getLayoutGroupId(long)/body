{
  long groupId=0;
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement(_GET_LAYOUT_GROUP_ID);
    ps.setLong(1,plid);
    rs=ps.executeQuery();
    while (rs.next()) {
      groupId=rs.getLong("groupId");
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return groupId;
}
