{
  List images=IGImageLocalServiceUtil.getNoAssetImages();
  if (_log.isDebugEnabled()) {
    _log.debug("Processing " + images.size() + " images with no tags assets");
  }
  for (int i=0; i < images.size(); i++) {
    IGImage image=(IGImage)images.get(i);
    try {
      IGImageLocalServiceUtil.updateTagsAsset(image.getUserId(),image,new String[0]);
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to update tags asset for image " + image.getImageId() + ": "+ e.getMessage());
      }
    }
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Tags assets verified for images");
  }
}
