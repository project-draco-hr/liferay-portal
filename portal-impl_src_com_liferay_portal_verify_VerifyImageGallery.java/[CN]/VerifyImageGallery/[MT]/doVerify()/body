{
  List<IGImage> images=IGImageLocalServiceUtil.getNoAssetImages();
  if (_log.isDebugEnabled()) {
    _log.debug("Processing " + images.size() + " images with no asset");
  }
  for (  IGImage image : images) {
    try {
      IGImageLocalServiceUtil.updateAsset(image.getUserId(),image,null,null,null);
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to update asset for image " + image.getImageId() + ": "+ e.getMessage());
      }
    }
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Assets verified for images");
  }
}
