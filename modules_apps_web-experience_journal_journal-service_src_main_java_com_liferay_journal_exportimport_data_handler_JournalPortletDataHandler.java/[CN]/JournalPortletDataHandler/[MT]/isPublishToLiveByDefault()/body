{
  try {
    long companyId=CompanyThreadLocal.getCompanyId();
    JournalServiceConfiguration journalServiceConfiguration=ConfigurationProviderUtil.getCompanyConfiguration(JournalServiceConfiguration.class,companyId);
    return journalServiceConfiguration.publishToLiveByDefaultEnabled();
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return true;
}
