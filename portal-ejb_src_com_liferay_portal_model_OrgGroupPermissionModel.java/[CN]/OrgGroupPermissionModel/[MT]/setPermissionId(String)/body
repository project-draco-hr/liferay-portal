{
  if (((permissionId == null) && (_permissionId != null)) || ((permissionId != null) && (_permissionId == null)) || ((permissionId != null) && (_permissionId != null) && !permissionId.equals(_permissionId))) {
    if (!XSS_ALLOW_PERMISSIONID) {
      permissionId=XSSUtil.strip(permissionId);
