{
  Map<String,Object> properties=new HashMap<>();
  Locale defaultLocale=ddmFormValues.getDefaultLocale();
  for (  DDMFormFieldValue ddmFormFieldValue : ddmFormValues.getDDMFormFieldValues()) {
    Value value=ddmFormFieldValue.getValue();
    properties.put(ddmFormFieldValue.getName(),value.getString(defaultLocale));
  }
  return new DDMDataProviderContext(properties);
}
