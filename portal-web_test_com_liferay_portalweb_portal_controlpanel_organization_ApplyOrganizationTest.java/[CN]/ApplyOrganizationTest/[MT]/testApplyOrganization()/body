{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      selenium.open("/web/guest/home/");
    for (int second=0; ; second++) {
      if (second >= 60) {
        fail("timeout");
      }
      try {
        if (selenium.isElementPresent("link=Control Panel")) {
          break;
        }
      }
 catch (      Exception e) {
      }
      Thread.sleep(1000);
    }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace(""));
selenium.waitForPageToLoad("30000");
selenium.clickAt("link=Organizations",RuntimeVariables.replace(""));
selenium.waitForPageToLoad("30000");
boolean Basic1Present=selenium.isVisible("link=\u00ab Basic");
if (!Basic1Present) {
label=2;
continue;
}
selenium.clickAt("link=\u00ab Basic",RuntimeVariables.replace(""));
case 2:
selenium.type("_126_keywords",RuntimeVariables.replace("Selenium"));
selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace(""));
