{
  _description=description;
  if (_kaleoNodeRemoteModel != null) {
    try {
      Class<?> clazz=_kaleoNodeRemoteModel.getClass();
      Method method=clazz.getMethod("setDescription",String.class);
      method.invoke(_kaleoNodeRemoteModel,description);
