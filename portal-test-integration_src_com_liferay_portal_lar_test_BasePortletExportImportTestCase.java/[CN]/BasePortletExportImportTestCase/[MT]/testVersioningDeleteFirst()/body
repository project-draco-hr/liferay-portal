{
  if (!isVersioningEnabled()) {
    return;
  }
  StagedModel stagedModel=addStagedModel(group.getGroupId());
  stagedModel=addVersion(stagedModel);
  exportImportPortlet(getPortletId());
  validateVersions();
  deleteFirstVersion(stagedModel);
  exportImportPortlet(getPortletId());
  validateVersions();
}
