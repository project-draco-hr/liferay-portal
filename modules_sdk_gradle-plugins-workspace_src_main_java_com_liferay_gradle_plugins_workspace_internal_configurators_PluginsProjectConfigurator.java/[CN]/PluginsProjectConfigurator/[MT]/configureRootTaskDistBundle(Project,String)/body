{
  CopySpec copySpec=(CopySpec)GradleUtil.getTask(project.getRootProject(),rootTaskName);
  copySpec.into("osgi/modules",new Closure<Void>(project){
    @SuppressWarnings("unused") public void doCall(    CopySpec copySpec){
      ConfigurableFileTree configurableFileTree=project.fileTree("dist");
      configurableFileTree.builtBy(WarPlugin.WAR_TASK_NAME);
      configurableFileTree.include("*.war");
      copySpec.from(configurableFileTree);
    }
  }
);
}
