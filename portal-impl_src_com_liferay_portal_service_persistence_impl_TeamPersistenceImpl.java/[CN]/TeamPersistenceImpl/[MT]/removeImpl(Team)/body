{
  team=toUnwrappedModel(team);
  teamToUserTableMapper.deleteLeftPrimaryKeyTableMappings(0,team.getPrimaryKey());
  teamToUserGroupTableMapper.deleteLeftPrimaryKeyTableMappings(0,team.getPrimaryKey());
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(team)) {
      team=(Team)session.get(TeamImpl.class,team.getPrimaryKeyObj());
    }
    if (team != null) {
      session.delete(team);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (team != null) {
    clearCache(team);
  }
  return team;
}
