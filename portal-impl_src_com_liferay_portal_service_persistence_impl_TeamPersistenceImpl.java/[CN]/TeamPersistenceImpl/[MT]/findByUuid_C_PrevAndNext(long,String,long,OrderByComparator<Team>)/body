{
  Team team=findByPrimaryKey(teamId);
  Session session=null;
  try {
    session=openSession();
    Team[] array=new TeamImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,team,uuid,companyId,orderByComparator,true);
    array[1]=team;
    array[2]=getByUuid_C_PrevAndNext(session,team,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
