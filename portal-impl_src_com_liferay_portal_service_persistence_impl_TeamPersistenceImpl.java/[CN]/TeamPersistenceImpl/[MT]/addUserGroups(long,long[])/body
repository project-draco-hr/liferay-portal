{
  long companyId=0;
  Team team=fetchByPrimaryKey(pk);
  if (team == null) {
    companyId=companyProvider.getCompanyId();
  }
 else {
    companyId=team.getCompanyId();
  }
  for (  long userGroupPK : userGroupPKs) {
    teamToUserGroupTableMapper.addTableMapping(companyId,pk,userGroupPK);
  }
}
