{
  Team team=findByPrimaryKey(teamId);
  Session session=null;
  try {
    session=openSession();
    Team[] array=new TeamImpl[3];
    array[0]=getByGroupId_PrevAndNext(session,team,groupId,orderByComparator,true);
    array[1]=team;
    array[2]=getByGroupId_PrevAndNext(session,team,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
