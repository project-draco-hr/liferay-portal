{
  if (userId <= 0) {
    return journalArticlePersistence.filterFindByG_C_NotST(groupId,JournalArticleConstants.CLASSNAME_ID_DEFAULT,WorkflowConstants.STATUS_IN_TRASH,start,end,orderByComparator);
  }
 else {
    return journalArticlePersistence.filterFindByG_U_C_NotST(groupId,userId,JournalArticleConstants.CLASSNAME_ID_DEFAULT,WorkflowConstants.STATUS_IN_TRASH,start,end,orderByComparator);
  }
}
