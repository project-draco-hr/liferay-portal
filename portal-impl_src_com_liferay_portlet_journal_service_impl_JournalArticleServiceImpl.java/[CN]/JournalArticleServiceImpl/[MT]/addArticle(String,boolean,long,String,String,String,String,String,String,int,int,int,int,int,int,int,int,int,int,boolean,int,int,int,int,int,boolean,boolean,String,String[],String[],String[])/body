{
  PortletPermissionUtil.check(getPermissionChecker(),plid,PortletKeys.JOURNAL,ActionKeys.ADD_ARTICLE);
  return journalArticleLocalService.addArticle(getUserId(),articleId,autoArticleId,plid,title,description,content,type,structureId,templateId,displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute,expirationDateMonth,expirationDateDay,expirationDateYear,expirationDateHour,expirationDateMinute,neverExpire,reviewDateMonth,reviewDateDay,reviewDateYear,reviewDateHour,reviewDateMinute,neverReview,indexable,false,null,null,null,articleURL,null,tagsEntries,communityPermissions,guestPermissions);
}
