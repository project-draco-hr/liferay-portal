{
  List<Long> folderIds=new ArrayList<Long>();
  if (rootFolderId == JournalFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    folderIds=journalFolderService.getFolderIds(groupId,rootFolderId);
  }
  QueryDefinition queryDefinition=new QueryDefinition(WorkflowConstants.STATUS_ANY,start,end,orderByComparator);
  if (userId <= 0) {
    return journalArticleFinder.filterFindByG_F(groupId,folderIds,queryDefinition);
  }
 else {
    return journalArticleFinder.filterFindByG_U_F_C(groupId,userId,folderIds,JournalArticleConstants.CLASSNAME_ID_DEFAULT,queryDefinition);
  }
}
