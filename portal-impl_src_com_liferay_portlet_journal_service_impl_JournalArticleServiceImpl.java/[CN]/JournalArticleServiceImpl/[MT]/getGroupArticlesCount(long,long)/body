{
  if (userId <= 0) {
    return journalArticlePersistence.filterCountByG_C_NotST(groupId,JournalArticleConstants.CLASSNAME_ID_DEFAULT,WorkflowConstants.STATUS_IN_TRASH);
  }
 else {
    return journalArticlePersistence.filterCountByG_U_C_NotST(groupId,userId,JournalArticleConstants.CLASSNAME_ID_DEFAULT,WorkflowConstants.STATUS_IN_TRASH);
  }
}
