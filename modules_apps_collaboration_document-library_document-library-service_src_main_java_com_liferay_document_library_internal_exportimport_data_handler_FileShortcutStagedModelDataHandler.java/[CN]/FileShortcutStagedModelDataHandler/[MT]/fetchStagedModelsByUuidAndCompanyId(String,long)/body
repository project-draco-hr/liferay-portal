{
  List<DLFileShortcut> dlFileShortcuts=_dlFileShortcutLocalService.getDLFileShortcutsByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<DLFileShortcut>());
  List<FileShortcut> fileShortcuts=new ArrayList<>();
  for (  DLFileShortcut dlFileShortcut : dlFileShortcuts) {
    fileShortcuts.add(new LiferayFileShortcut(dlFileShortcut));
  }
  return fileShortcuts;
}
