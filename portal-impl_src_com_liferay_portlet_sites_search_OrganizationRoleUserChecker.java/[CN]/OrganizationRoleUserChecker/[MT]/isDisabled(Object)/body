{
  User user=(User)obj;
  Set<Role> mandatoryRoles=MembershipPolicyUtil.getMandatoryRoles(_organization,user);
  if ((isChecked(user) && mandatoryRoles.contains(_role)) || (!isChecked(user) && !MembershipPolicyUtil.isMembershipAllowed(_organization,_role,user))) {
    return true;
  }
  return super.isDisabled(obj);
}
