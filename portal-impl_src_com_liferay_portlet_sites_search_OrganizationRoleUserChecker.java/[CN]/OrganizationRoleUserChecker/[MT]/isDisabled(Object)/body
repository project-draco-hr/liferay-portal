{
  User user=(User)obj;
  try {
    if ((isChecked(user) && OrganizationMembershipPolicyUtil.isRoleRequired(user.getUserId(),_organization.getOrganizationId(),_role.getRoleId())) || (!isChecked(user) && !OrganizationMembershipPolicyUtil.isRoleAllowed(user.getUserId(),_organization.getOrganizationId(),_role.getRoleId()))) {
      return true;
    }
  }
  finally {
    return super.isDisabled(obj);
  }
}
