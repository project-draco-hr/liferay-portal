{
  return new WikiPageExportActionableDynamicQuery(portletDataContext){
    @Override protected void addCriteria(    DynamicQuery dynamicQuery){
      super.addCriteria(dynamicQuery);
      Property nodeIdProperty=PropertyFactoryUtil.forName("nodeId");
      dynamicQuery.add(nodeIdProperty.eq(nodeId));
    }
    @Override protected void performAction(    Object object) throws PortalException, SystemException {
      WikiPage page=(WikiPage)object;
      StagedModelDataHandlerUtil.exportReferenceStagedModel(portletDataContext,portletId,page);
    }
  }
;
}
