{
  if (_log.isDebugEnabled()) {
    StringBundler sb=new StringBundler(13);
    sb.append("getLayoutsJSON(groupId=");
    sb.append(groupId);
    sb.append(", privateLayout=");
    sb.append(privateLayout);
    sb.append(", parentLayoutId=");
    sb.append(parentLayoutId);
    sb.append(", expandedLayoutIds=");
    sb.append(expandedLayoutIds);
    sb.append(", incomplete=");
    sb.append(incomplete);
    sb.append(", treeId=");
    sb.append(treeId);
    sb.append(StringPool.CLOSE_PARENTHESIS);
    _log.debug(sb.toString());
  }
  LayoutTreeNodes layoutTreeNodes=_getLayoutTreeNodes(request,groupId,privateLayout,parentLayoutId,incomplete,expandedLayoutIds,treeId);
  return _toJSON(request,groupId,layoutTreeNodes);
}
