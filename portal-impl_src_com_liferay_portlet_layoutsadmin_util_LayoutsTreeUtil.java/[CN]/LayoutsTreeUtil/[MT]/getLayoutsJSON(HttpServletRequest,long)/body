{
  List<LayoutTreeNode> layoutTreeNodes=new ArrayList<LayoutTreeNode>();
  layoutTreeNodes.addAll(_getLayoutTreeNodes(request,groupId,true,LayoutConstants.DEFAULT_PARENT_LAYOUT_ID,false,null));
  layoutTreeNodes.addAll(_getLayoutTreeNodes(request,groupId,false,LayoutConstants.DEFAULT_PARENT_LAYOUT_ID,false,null));
  return _toJSON(request,groupId,layoutTreeNodes);
}
