{
  PredicateFilter predicateFilter=new PredicateFilter<Map.Entry<String,Object>>(){
    @Override public boolean filter(    Map.Entry<String,Object> entry){
      if (_isValidAttributeName(entry.getKey()) && _isValidAttributeValue(entry.getValue())) {
        return true;
      }
      return false;
    }
  }
;
  return MapUtil.filter(map,predicateFilter);
}
