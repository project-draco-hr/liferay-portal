{
  Group group=GroupLocalServiceUtil.fetchGroup(_folder.getGroupId());
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(liferayPortletRequest,group,JournalPortletKeys.JOURNAL,0,0,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/edit_folder.jsp");
  portletURL.setParameter("folderId",String.valueOf(_folder.getFolderId()));
  return portletURL;
}
