{
  try {
    if (isGoogleDocs(fileVersion)) {
      return new GoogleDocsDLFileVersionActionsDisplayContext(parentDLFileEntryActionsDisplayContext,request,response,fileVersion);
    }
  }
 catch (  PortalException pe) {
    throw new SystemException(pe);
  }
  return parentDLFileEntryActionsDisplayContext;
}
