{
  try {
    Object returnValue=PortalClassInvoker.invoke(false,_CLASS_NAME,"getLockKey",new String[]{String.class.getName()},contextPath);
    return String.valueOf(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new IllegalStateException("Unable to get lock key",e);
  }
}
