{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Site Map Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Site Map Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("//a[contains(text(),'Plugins')]"));
  assertTrue(selenium.isElementPresent("//a[contains(text(),'Site Map Test Page')]"));
  assertTrue(selenium.isElementPresent("link=Site Map Child Page"));
}
