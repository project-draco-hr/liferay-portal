{
  ExtendedAttributeDefinition[] extendedAttributeDefinitions=_extendedObjectClassDefinition.getAttributeDefinitions(ObjectClassDefinition.ALL);
  for (  ExtendedAttributeDefinition extendedAttributeDefinition : extendedAttributeDefinitions) {
    if (id.equals(extendedAttributeDefinition.getID())) {
      return extendedAttributeDefinition;
    }
  }
  return null;
}
