{
  _readLock.lock();
  try {
    ValueWrapper<V> valueWrapper=_cache.get(key);
    if (valueWrapper != null) {
      valueWrapper._hitCount.getAndIncrement();
      _hitCount.getAndIncrement();
      return valueWrapper._value;
    }
  }
  finally {
    _readLock.unlock();
  }
  _missCount.getAndIncrement();
  return null;
}
