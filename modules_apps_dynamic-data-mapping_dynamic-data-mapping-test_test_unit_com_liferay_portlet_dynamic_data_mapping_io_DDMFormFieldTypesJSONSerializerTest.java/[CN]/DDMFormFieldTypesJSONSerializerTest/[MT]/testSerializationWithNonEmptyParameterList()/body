{
  List<DDMFormFieldType> ddmFormFieldTypes=new ArrayList<>();
  DDMFormFieldType ddmFormFieldType=mock(DDMFormFieldType.class);
  whenDDMFormFieldTypeGetDDMFormFieldTypeSettings(ddmFormFieldType,DDMFormFieldTypeSettings.class);
  whenDDMFormFieldTypeGetIcon(ddmFormFieldType,"my-icon");
  whenDDMFormFieldTypeGetJavaScriptClass(ddmFormFieldType,"myJavaScriptClass");
  whenDDMFormFieldTypeGetJavaScriptModule(ddmFormFieldType,"myJavaScriptModule");
  whenDDMFormFieldTypeGetName(ddmFormFieldType,"Text");
  DDMFormFieldRenderer ddmFormFieldRenderer=mock(DDMFormFieldRenderer.class);
  whenDDMFormFieldTypeGetDDMFormFieldRenderer(ddmFormFieldType,ddmFormFieldRenderer);
  whenDDMFormFieldRendererGetTemplateNamespace(ddmFormFieldRenderer,"_templateNamespace_");
  ddmFormFieldTypes.add(ddmFormFieldType);
  String actualJSON=DDMFormFieldTypesJSONSerializerUtil.serialize(ddmFormFieldTypes);
  JSONAssert.assertEquals(createExpectedJSON(),actualJSON,false);
}
