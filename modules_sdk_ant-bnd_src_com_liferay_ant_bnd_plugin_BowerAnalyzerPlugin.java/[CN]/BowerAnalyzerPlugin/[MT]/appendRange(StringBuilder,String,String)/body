{
  sb.append("(&");
  Matcher matcher=VERSION_PREFIX_RANGE_PATTERN.matcher(group1);
  matcher.matches();
  appendPrefixRange(sb,matcher.group(1),matcher.group(2));
  matcher=VERSION_PREFIX_RANGE_PATTERN.matcher(group2);
  matcher.matches();
  appendPrefixRange(sb,matcher.group(1),matcher.group(2));
  sb.append(")");
}
