{
  if (_persistence == null) {
    _persistence=(KaleoTaskAssignmentInstancePersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoTaskAssignmentInstancePersistence.class.getName());
