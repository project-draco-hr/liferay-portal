{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select companyId from Layout where plid = ?");
    ps.setLong(1,plid);
    rs=ps.executeQuery();
    while (rs.next()) {
      long companyId=rs.getLong("companyId");
      return companyId;
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return 0;
}
