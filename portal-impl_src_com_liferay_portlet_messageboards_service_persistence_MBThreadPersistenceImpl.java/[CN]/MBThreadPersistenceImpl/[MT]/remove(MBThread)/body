{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(mbThread);
    }
  }
  mbThread=removeImpl(mbThread);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(mbThread);
    }
  }
  return mbThread;
}
