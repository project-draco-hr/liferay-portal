{
  MBThread mbThread=(MBThread)EntityCacheUtil.getResult(MBThreadModelImpl.ENTITY_CACHE_ENABLED,MBThreadImpl.class,primaryKey);
  if (mbThread == _nullMBThread) {
    return null;
  }
  if (mbThread == null) {
    Session session=null;
    try {
      session=openSession();
      mbThread=(MBThread)session.get(MBThreadImpl.class,primaryKey);
      if (mbThread != null) {
        cacheResult(mbThread);
      }
 else {
        EntityCacheUtil.putResult(MBThreadModelImpl.ENTITY_CACHE_ENABLED,MBThreadImpl.class,primaryKey,_nullMBThread);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(MBThreadModelImpl.ENTITY_CACHE_ENABLED,MBThreadImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return mbThread;
}
