{
  EntityCacheUtil.removeResult(MBThreadModelImpl.ENTITY_CACHE_ENABLED,MBThreadImpl.class,mbThread.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_ROOTMESSAGEID,new Object[]{Long.valueOf(mbThread.getRootMessageId())});
}
