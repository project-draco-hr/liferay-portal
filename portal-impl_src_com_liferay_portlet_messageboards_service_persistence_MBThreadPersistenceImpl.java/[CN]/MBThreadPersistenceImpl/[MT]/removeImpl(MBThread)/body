{
  mbThread=toUnwrappedModel(mbThread);
  Session session=null;
  try {
    session=openSession();
    if (mbThread.isCachedModel()) {
      mbThread=(MBThread)session.get(MBThreadImpl.class,mbThread.getPrimaryKeyObj());
    }
    if (mbThread != null) {
      session.delete(mbThread);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (mbThread != null) {
    clearCache(mbThread);
  }
  return mbThread;
}
