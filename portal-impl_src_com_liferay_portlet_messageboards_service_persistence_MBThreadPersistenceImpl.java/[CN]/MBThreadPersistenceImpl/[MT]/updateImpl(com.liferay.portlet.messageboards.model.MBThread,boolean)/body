{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,mbThread,merge);
    mbThread.setNew(false);
    return mbThread;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(MBThread.class.getName());
  }
}
