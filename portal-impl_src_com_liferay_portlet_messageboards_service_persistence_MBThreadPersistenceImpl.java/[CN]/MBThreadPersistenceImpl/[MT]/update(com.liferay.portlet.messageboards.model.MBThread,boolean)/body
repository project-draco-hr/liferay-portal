{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(mbThread);
    }
 else {
      if (mbThread.isNew()) {
        session.save(mbThread);
      }
    }
    session.flush();
    mbThread.setNew(false);
    return mbThread;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
