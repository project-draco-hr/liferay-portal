{
  if (mbThread.isNew()) {
    Object[] args=new Object[]{Long.valueOf(mbThread.getRootMessageId())};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_ROOTMESSAGEID,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_ROOTMESSAGEID,args,mbThread);
  }
 else {
    MBThreadModelImpl mbThreadModelImpl=(MBThreadModelImpl)mbThread;
    if ((mbThreadModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_ROOTMESSAGEID.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{Long.valueOf(mbThread.getRootMessageId())};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_ROOTMESSAGEID,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_ROOTMESSAGEID,args,mbThread);
    }
  }
}
