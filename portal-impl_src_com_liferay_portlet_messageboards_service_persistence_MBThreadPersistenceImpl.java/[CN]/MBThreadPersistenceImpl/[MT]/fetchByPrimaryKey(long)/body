{
  MBThread result=(MBThread)EntityCacheUtil.getResult(MBThreadModelImpl.ENTITY_CACHE_ENABLED,MBThread.class,threadId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      MBThread mbThread=(MBThread)session.get(MBThreadImpl.class,new Long(threadId));
      if (mbThread != null) {
        cacheResult(mbThread);
      }
      return mbThread;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (MBThread)result;
  }
}
