{
  MBThread mbThread=(MBThread)EntityCacheUtil.getResult(MBThreadModelImpl.ENTITY_CACHE_ENABLED,MBThreadImpl.class,threadId,this);
  if (mbThread == null) {
    Session session=null;
    try {
      session=openSession();
      mbThread=(MBThread)session.get(MBThreadImpl.class,Long.valueOf(threadId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (mbThread != null) {
        cacheResult(mbThread);
      }
      closeSession(session);
    }
  }
  return mbThread;
}
