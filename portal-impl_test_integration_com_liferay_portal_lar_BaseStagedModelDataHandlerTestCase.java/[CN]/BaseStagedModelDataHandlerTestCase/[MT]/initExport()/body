{
  zipWriter=ZipWriterFactoryUtil.getZipWriter();
  portletDataContext=PortletDataContextFactoryUtil.createExportPortletDataContext(stagingGroup.getCompanyId(),stagingGroup.getGroupId(),getParameterMap(),getStartDate(),getEndDate(),zipWriter);
  rootElement=SAXReaderUtil.createElement("root");
  portletDataContext.setExportDataRootElement(rootElement);
}
