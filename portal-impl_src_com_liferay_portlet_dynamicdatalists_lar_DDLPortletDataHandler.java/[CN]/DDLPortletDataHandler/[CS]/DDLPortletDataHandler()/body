{
  setDataLocalized(true);
  setDeletionSystemEventStagedModelTypes(new StagedModelType(DDLRecord.class),new StagedModelType(DDLRecordSet.class));
  setExportControls(new PortletDataHandlerBoolean(NAMESPACE,"record-sets",true,false,null,DDLRecordSet.class.getName()),new PortletDataHandlerBoolean(NAMESPACE,"records",true,false,null,DDLRecord.class.getName()),new PortletDataHandlerBoolean(NAMESPACE,"data-definitions",true,false,null,DDMStructure.class.getName(),DDLRecordSet.class.getName()));
  XStreamAliasRegistryUtil.register(DDLRecordImpl.class,"DDLRecord");
  XStreamAliasRegistryUtil.register(DDLRecordSetImpl.class,"DDLRecordSet");
  XStreamAliasRegistryUtil.register(DDMStructureImpl.class,"DDMStructure");
