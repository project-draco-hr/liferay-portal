{
  portletDataContext.addPermissions("com.liferay.portlet.dynamicdatalist",portletDataContext.getScopeGroupId());
  Element rootElement=addExportDataRootElement(portletDataContext);
  ActionableDynamicQuery actionableDynamicQuery=new DDLRecordSetActionableDynamicQuery(){
    @Override protected void addCriteria(    DynamicQuery dynamicQuery){
      portletDataContext.addDateRangeCriteria(dynamicQuery,"modifiedDate");
    }
    @Override protected void performAction(    Object object) throws PortalException {
      DDLRecordSet recordSet=(DDLRecordSet)object;
      StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,recordSet);
    }
  }
;
  actionableDynamicQuery.setGroupId(portletDataContext.getScopeGroupId());
  actionableDynamicQuery.performActions();
  return getExportDataRootElementString(rootElement);
}
