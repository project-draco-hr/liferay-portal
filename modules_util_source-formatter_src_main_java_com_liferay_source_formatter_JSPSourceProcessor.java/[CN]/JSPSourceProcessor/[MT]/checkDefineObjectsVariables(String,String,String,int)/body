{
  for (  String[] defineObject : _LIFERAY_THEME_DEFINE_OBJECTS) {
    checkDefineObjectsVariable(line,fileName,lineCount,defineObject[0],defineObject[1],defineObject[2],"liferay-theme");
  }
  for (  String[] defineObject : _PORTLET_DEFINE_OBJECTS) {
    checkDefineObjectsVariable(line,fileName,lineCount,defineObject[0],defineObject[1],defineObject[2],"portlet");
  }
  if (!portalSource) {
    return;
  }
  try {
    for (    String directoryName : getPluginsInsideModulesDirectoryNames()) {
      if (absolutePath.contains(directoryName)) {
        return;
      }
    }
  }
 catch (  Exception e) {
  }
  for (  String[] defineObject : _LIFERAY_FRONTEND_DEFINE_OBJECTS) {
    checkDefineObjectsVariable(line,fileName,lineCount,defineObject[0],defineObject[1],defineObject[2],"liferay-frontend");
  }
}
