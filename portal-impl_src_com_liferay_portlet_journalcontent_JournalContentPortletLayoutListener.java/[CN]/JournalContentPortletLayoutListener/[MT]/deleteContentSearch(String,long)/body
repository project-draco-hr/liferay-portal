{
  MockHttpServletRequest request=new MockHttpServletRequest();
  Layout layout=LayoutLocalServiceUtil.getLayout(plid);
  request.setAttribute(WebKeys.LAYOUT,layout);
  PortletPreferences preferences=PortletPreferencesFactoryUtil.getPortletSetup(request,portletId,StringPool.BLANK);
  String articleId=preferences.getValue("article-id",null);
  if (Validator.isNull(articleId)) {
    return;
  }
  try {
    JournalContentSearchLocalServiceUtil.deleteArticleContentSearch(layout.getGroupId(),layout.isPrivateLayout(),layout.getLayoutId(),portletId,articleId);
  }
 catch (  NoSuchContentSearchException nscse) {
  }
}
