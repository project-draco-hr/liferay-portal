{
  ExecutorService executorService=Executors.newFixedThreadPool(hostNames.size());
  for (  String targetHostName : hostNames) {
    FutureTask<Integer> futureTask=new FutureTask<>(new AvailableSlaveCallable(getTotalRecentBatchSize(targetHostName),properties.getProperty("jenkins.local.url[" + targetHostName + "]")));
    executorService.execute(futureTask);
    futureTasks.add(futureTask);
  }
  executorService.shutdown();
}
