{
  ExecutorService executorService=Executors.newFixedThreadPool(masters.size());
  for (  String targetMaster : masters) {
    FutureTask<Integer> futureTask=new FutureTask<>(new AvailableSlaveCallable(getRecentBatchSizesTotal(targetMaster),properties.getProperty("jenkins.local.url[" + targetMaster + "]")));
    executorService.execute(futureTask);
    futureTasks.add(futureTask);
  }
  executorService.shutdown();
}
