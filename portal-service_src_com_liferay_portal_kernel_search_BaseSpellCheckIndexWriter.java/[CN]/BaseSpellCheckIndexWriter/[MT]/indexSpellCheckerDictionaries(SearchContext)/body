{
  try {
    for (    String languageId : _SUPPORTED_LOCALES) {
      indexKeywords(searchContext.getCompanyId(),languageId,PropsKeys.INDEX_SEARCH_SPELL_CHECKER_DICTIONARY,Field.SPELL_CHECK_WORD,SuggestionConstants.TYPE_SPELL_CHECKER,0);
    }
  }
 catch (  Exception e) {
    throw new SearchException(e);
  }
}
