{
  String sql=CustomSQLUtil.get(id,queryDefinition,DLFileVersionImpl.TABLE_NAME);
  if (inlineSQLHelper) {
    sql=InlineSQLHelperUtil.replacePermissionCheck(sql,DLFileEntry.class.getName(),"DLFileVersion.fileEntryId",groupId);
  }
  if (ArrayUtil.isNotEmpty(mimeTypes)) {
    StringBundler sb=new StringBundler(5);
    sb.append(sql);
    sb.append(WHERE_AND);
    sb.append(StringPool.OPEN_PARENTHESIS);
    sb.append(getMimeTypes(mimeTypes,DLFileVersionImpl.TABLE_NAME));
    sb.append(StringPool.CLOSE_PARENTHESIS);
    sql=sb.toString();
  }
  return sql;
}
