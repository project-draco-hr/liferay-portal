{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select * from JournalStructure");
    rs=ps.executeQuery();
    while (rs.next()) {
      updateStructure(rs);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  runSQL("drop table JournalStructure");
}
