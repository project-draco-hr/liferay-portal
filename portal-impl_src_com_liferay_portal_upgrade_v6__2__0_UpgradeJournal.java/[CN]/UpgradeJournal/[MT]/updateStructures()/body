{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select * from JournalStructure");
    rs=ps.executeQuery();
    while (rs.next()) {
      updateStructure(rs);
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
  runSQL("drop table JournalStructure");
}
