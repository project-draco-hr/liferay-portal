{
  long parentDDMStructureId=0;
  if (Validator.isNotNull(parentStructureId)) {
    parentDDMStructureId=updateStructure(parentStructureId);
  }
  addDDMStructure(uuid_,ddmStructureId,groupId,companyId,userId,userName,createDate,modifiedDate,parentDDMStructureId,PortalUtil.getClassNameId(JournalArticle.class.getName()),ddmStructureKey,name,description,xsd,PropsValues.JOURNAL_ARTICLE_STORAGE_TYPE,DDMStructureConstants.TYPE_DEFAULT);
}
