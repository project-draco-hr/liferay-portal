{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select companyId, groupId, resourcePrimKey, structureId " + "from JournalArticle where structureId != ''");
    rs=ps.executeQuery();
    while (rs.next()) {
      long companyId=rs.getLong("companyId");
      long groupId=rs.getLong("groupId");
      long resourcePrimKey=rs.getLong("resourcePrimKey");
      String structureId=rs.getString("structureId");
      long ddmStructureId=getDDMStructureId(groupId,getCompanyGroupId(companyId),structureId);
      runSQL("update AssetEntry set classTypeId = " + ddmStructureId + " where classPK = "+ resourcePrimKey);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
