{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps1=connection.prepareStatement("select companyId, groupId, resourcePrimKey, structureId " + "from JournalArticle where structureId != ''");ResultSet rs=ps1.executeQuery()){
    try (PreparedStatement ps2=AutoBatchPreparedStatementUtil.concurrentAutoBatch(connection,"update AssetEntry set classTypeId = ? where classPK " + "= ?")){
      while (rs.next()) {
        long groupId=rs.getLong("groupId");
        long companyId=rs.getLong("companyId");
        long resourcePrimKey=rs.getLong("resourcePrimKey");
        String structureId=rs.getString("structureId");
        long ddmStructureId=getDDMStructureId(groupId,getCompanyGroupId(companyId),structureId);
        ps2.setLong(1,ddmStructureId);
        ps2.setLong(2,resourcePrimKey);
        ps2.addBatch();
      }
      ps2.executeBatch();
    }
   }
 }
