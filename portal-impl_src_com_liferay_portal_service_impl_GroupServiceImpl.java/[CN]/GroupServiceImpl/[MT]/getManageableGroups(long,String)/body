{
  PermissionChecker permissionChecker=getPermissionChecker();
  List<Group> manageableGroups=groupLocalService.getManageableGroups(userId);
  if (Validator.isNull(actionId) || permissionChecker.isCompanyAdmin()) {
    return manageableGroups;
  }
  for (  Group group : manageableGroups) {
    if (!GroupPermissionUtil.contains(permissionChecker,group.getGroupId(),actionId)) {
      manageableGroups.remove(group);
    }
  }
  return manageableGroups;
}
