{
  if (((messageId == null) && (_messageId != null)) || ((messageId != null) && (_messageId == null)) || ((messageId != null) && (_messageId != null) && !messageId.equals(_messageId))) {
    if (!XSS_ALLOW_MESSAGEID) {
      messageId=XSSUtil.strip(messageId);
    }
    _messageId=messageId;
    setModified(true);
  }
}
