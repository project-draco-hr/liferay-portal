{
  if (((parentMessageId == null) && (_parentMessageId != null)) || ((parentMessageId != null) && (_parentMessageId == null)) || ((parentMessageId != null) && (_parentMessageId != null) && !parentMessageId.equals(_parentMessageId))) {
    if (!XSS_ALLOW_PARENTMESSAGEID) {
      parentMessageId=XSSUtil.strip(parentMessageId);
    }
    _parentMessageId=parentMessageId;
    setModified(true);
  }
}
