{
  if (((categoryId == null) && (_categoryId != null)) || ((categoryId != null) && (_categoryId == null)) || ((categoryId != null) && (_categoryId != null) && !categoryId.equals(_categoryId))) {
    if (!XSS_ALLOW_CATEGORYID) {
      categoryId=XSSUtil.strip(categoryId);
    }
    _categoryId=categoryId;
    setModified(true);
  }
}
