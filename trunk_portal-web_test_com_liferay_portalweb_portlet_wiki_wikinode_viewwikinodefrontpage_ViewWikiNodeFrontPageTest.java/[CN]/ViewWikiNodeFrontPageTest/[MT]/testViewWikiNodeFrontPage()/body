{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Wiki Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace("Wiki Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("This is a wiki frontpage test."),selenium.getText("//div[@class='wiki-body']"));
  selenium.clickAt("link=Wiki Node Name",RuntimeVariables.replace("Wiki Node Name"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("This is a wiki node frontpage test."),selenium.getText("//div[@class='wiki-body']"));
  selenium.clickAt("link=Main",RuntimeVariables.replace("Main"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("This is a wiki frontpage test."),selenium.getText("//div[@class='wiki-body']"));
}
