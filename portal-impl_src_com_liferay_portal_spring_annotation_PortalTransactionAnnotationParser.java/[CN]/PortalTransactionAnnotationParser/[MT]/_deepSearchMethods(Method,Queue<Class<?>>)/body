{
  Transactional annotation=method.getAnnotation(Transactional.class);
  if (annotation != null) {
    return annotation;
  }
  if (candidateQueue.isEmpty()) {
    return null;
  }
  Class<?> clazz=candidateQueue.poll();
  if (method.getDeclaringClass() != clazz) {
    try {
      Method specificMethod=clazz.getDeclaredMethod(method.getName(),method.getParameterTypes());
      annotation=specificMethod.getAnnotation(Transactional.class);
      if (annotation != null) {
        return annotation;
      }
    }
 catch (    Exception e) {
    }
  }
  annotation=clazz.getAnnotation(Transactional.class);
  if (annotation == null) {
    _queueSuperTypes(clazz,candidateQueue);
    return _deepSearchMethods(method,candidateQueue);
  }
 else {
    return annotation;
  }
}
