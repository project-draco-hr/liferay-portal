{
  if (candidateQueue.isEmpty()) {
    return null;
  }
  Class<?> clazz=candidateQueue.poll();
  Transactional annotation=clazz.getAnnotation(Transactional.class);
  if (annotation == null) {
    _queueSuperTypes(clazz,candidateQueue);
    return _deepSearchTypes(candidateQueue);
  }
 else {
    return annotation;
  }
}
