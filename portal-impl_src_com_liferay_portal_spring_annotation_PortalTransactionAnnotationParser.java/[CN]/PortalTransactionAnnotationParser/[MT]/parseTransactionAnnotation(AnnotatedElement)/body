{
  Transactional transactional=null;
  if (annotatedElement instanceof Method) {
    Method method=(Method)annotatedElement;
    transactional=AnnotationLocator.locate(method,method.getDeclaringClass(),Transactional.class);
  }
 else {
    transactional=AnnotationLocator.locate((Class<?>)annotatedElement,Transactional.class);
  }
  return TransactionAttributeBuilder.build(transactional);
}
