{
  long organizationId1=organization1.getOrganizationId();
  long organizationId2=organization2.getOrganizationId();
  int value=0;
  if (organizationId1 < organizationId2) {
    value=-1;
  }
 else   if (organizationId1 > organizationId2) {
    value=1;
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
