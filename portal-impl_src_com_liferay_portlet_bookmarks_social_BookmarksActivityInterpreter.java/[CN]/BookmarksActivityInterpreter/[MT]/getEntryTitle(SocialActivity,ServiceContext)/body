{
  if (activity.isClassName(BookmarksEntry.class.getName())) {
    BookmarksEntry entry=BookmarksEntryLocalServiceUtil.getEntry(activity.getClassPK());
    return entry.getName();
  }
 else   if (activity.isClassName(BookmarksFolder.class.getName())) {
    BookmarksFolder folder=BookmarksFolderLocalServiceUtil.getFolder(activity.getClassPK());
    return folder.getName();
  }
  return StringPool.BLANK;
}
