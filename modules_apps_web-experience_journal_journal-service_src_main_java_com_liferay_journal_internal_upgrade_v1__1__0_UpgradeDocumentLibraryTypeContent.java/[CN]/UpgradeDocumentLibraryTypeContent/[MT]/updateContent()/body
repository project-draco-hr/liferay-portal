{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps1=connection.prepareStatement("select content, id_ from JournalArticle where content like " + "?")){
    ps1.setString(1,"%type=\"document_library\"%");
    ResultSet rs1=ps1.executeQuery();
    while (rs1.next()) {
      String content=rs1.getString(1);
      long id=rs1.getLong(2);
      try (PreparedStatement ps2=connection.prepareStatement("update JournalArticle set content = ? where id_ = " + "?")){
        ps2.setString(1,convertContent(content));
        ps2.setLong(2,id);
        ps2.executeUpdate();
      }
     }
  }
 }
