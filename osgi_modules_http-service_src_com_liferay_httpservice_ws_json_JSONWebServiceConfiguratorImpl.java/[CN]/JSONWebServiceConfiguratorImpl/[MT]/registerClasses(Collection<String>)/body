{
  if ((classPaths == null) || classPaths.isEmpty()) {
    return;
  }
  ClassLoader classLoader=getClassLoader();
  for (  String classPath : classPaths) {
    URL url=classLoader.getResource(classPath);
    String className=StringUtil.replace(url.getPath(),new String[]{StringPool.SLASH,".class"},new String[]{StringPool.PERIOD,StringPool.BLANK});
    if (className.startsWith(StringPool.PERIOD)) {
      className=className.substring(1);
    }
    try {
      registerClass(className,url.openStream());
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
}
