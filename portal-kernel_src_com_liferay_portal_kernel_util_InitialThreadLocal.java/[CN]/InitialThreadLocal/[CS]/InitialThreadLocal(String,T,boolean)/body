{
  super(shortLived);
  _name=name;
  _initialValue=initialValue;
  Method cloneMethod=null;
  if (_initialValue instanceof Cloneable) {
    try {
      Class<?> clazz=_initialValue.getClass();
      cloneMethod=clazz.getMethod(_METHOD_CLONE);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
  _cloneMethod=cloneMethod;
}
