{
  boolean containsBaseModelPermission=BaseModelPermissionCheckerUtil.containsBaseModelPermission(permissionChecker,groupId,WikiPage.class.getName(),_classPK,ActionKeys.UPDATE);
  if (!containsBaseModelPermission) {
    throw new PrincipalException.MustHavePermission(permissionChecker,WikiPage.class.getName(),_classPK,ActionKeys.UPDATE);
  }
}
