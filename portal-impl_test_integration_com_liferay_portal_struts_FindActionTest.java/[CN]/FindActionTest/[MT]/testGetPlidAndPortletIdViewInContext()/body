{
  addLayouts(true,false);
  Object[] plidAndPorltetId=BaseFindActionHelper.getPlidAndPortletId(getThemeDisplay(),_blogsEntry.getGroupId(),_assetLayout.getPlid(),_portletIds);
  Assert.assertEquals(_blogLayout.getPlid(),plidAndPorltetId[0]);
  String portletId=PortletProviderUtil.getPortletId(BlogsEntry.class.getName(),PortletProvider.Action.VIEW);
  Assert.assertEquals(portletId,plidAndPorltetId[1]);
}
