{
  addLayouts(true,false);
  Object[] plidAndPorltetId=BaseFindActionDelegate.getPlidAndPortletId(getThemeDisplay(),_blogsEntry.getGroupId(),_assetLayout.getPlid(),_PORTLET_IDS);
  Assert.assertEquals(_blogLayout.getPlid(),plidAndPorltetId[0]);
  Assert.assertEquals(PortletKeys.BLOGS,plidAndPorltetId[1]);
}
