{
  addLayouts(true,false);
  PortletLayoutFinder.Result result=_portletLayoutFinder.find(getThemeDisplay(),_blogsEntryGroupId);
  Assert.assertEquals(_blogLayout.getPlid(),result.getPlid());
  String portletId=PortletProviderUtil.getPortletId(BlogsEntry.class.getName(),PortletProvider.Action.VIEW);
  Assert.assertEquals(portletId,result.getPortletId());
}
