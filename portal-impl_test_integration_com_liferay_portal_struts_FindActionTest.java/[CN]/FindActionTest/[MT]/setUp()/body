{
  _portletIds=new String[]{PortletProviderUtil.getPortletId(BlogsEntry.class.getName(),PortletProvider.Action.MANAGE),PortletProviderUtil.getPortletId(BlogsEntry.class.getName(),PortletProvider.Action.VIEW)};
  _portletLayoutFinder=new BasePortletLayoutFinder(){
    @Override protected String[] getPortletIds(){
      return _portletIds;
    }
  }
;
}
