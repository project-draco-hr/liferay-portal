{
  attachProcess("Attached " + toString());
  String[] aspectClassNames=StringUtil.split(System.getProperty("aspectClasses"));
  Class<?>[] aspectClasses=new Class<?>[aspectClassNames.length];
  Thread currentThread=Thread.currentThread();
  ClassLoader contextClassLoader=currentThread.getContextClassLoader();
  try {
    for (int i=0; i < aspectClassNames.length; i++) {
      aspectClasses[i]=contextClassLoader.loadClass(aspectClassNames[i]);
    }
    WeavingClassLoader weavingClassLoader=new WeavingClassLoader(ClassPathUtil.getClassPathURLs(ClassPathUtil.getJVMClassPath(true)),aspectClasses,_dumpDir);
    currentThread.setContextClassLoader(weavingClassLoader);
    return ReflectionTestUtil.invoke(SerializableUtil.deserialize(_encodedProcessCallable,weavingClassLoader),"call",new Class<?>[0]);
  }
 catch (  Exception e) {
    throw new ProcessException(e);
  }
 finally {
    currentThread.setContextClassLoader(contextClassLoader);
  }
}
