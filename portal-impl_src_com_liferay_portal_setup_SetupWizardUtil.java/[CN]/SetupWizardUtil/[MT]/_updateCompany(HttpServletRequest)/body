{
  Company company=CompanyLocalServiceUtil.getCompanyById(PortalInstances.getDefaultCompanyId());
  String languageId=ParamUtil.getString(request,"companyLocale",getDefaultLanguageId());
  String companyName=ParamUtil.getString(request,"companyName",PropsValues.COMPANY_DEFAULT_NAME);
  SetupWizardSampleDataUtil.updateCompany(company,companyName,languageId);
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  themeDisplay.setCompany(company);
}
