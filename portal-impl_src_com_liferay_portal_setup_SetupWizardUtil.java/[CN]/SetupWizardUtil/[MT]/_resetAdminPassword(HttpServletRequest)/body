{
  String defaultAdminEmailAddress=_getParameter(request,PropsKeys.ADMIN_EMAIL_FROM_ADDRESS,"test@liferay.com");
  User user=UserLocalServiceUtil.getUserByEmailAddress(PortalUtil.getDefaultCompanyId(),defaultAdminEmailAddress);
  UserLocalServiceUtil.updatePasswordReset(user.getUserId(),true);
  HttpSession session=request.getSession();
  session.setAttribute(WebKeys.SETUP_WIZARD_PASSWORD_UPDATED,true);
}
