{
  String defaultAdminEmailAddress=_getParameter(request,PropsKeys.ADMIN_EMAIL_FROM_ADDRESS,"test@liferay.com");
  try {
    User adminUser=UserLocalServiceUtil.getUserByEmailAddress(PortalUtil.getDefaultCompanyId(),defaultAdminEmailAddress);
    UserLocalServiceUtil.updatePasswordReset(adminUser.getUserId(),true);
    request.getSession().setAttribute(WebKeys.SETUP_WIZARD_PASSWORD_UPDATED,String.valueOf(true));
  }
 catch (  Exception e) {
  }
}
