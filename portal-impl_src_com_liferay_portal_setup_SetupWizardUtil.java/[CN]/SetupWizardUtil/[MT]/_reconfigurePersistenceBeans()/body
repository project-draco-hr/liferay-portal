{
  @SuppressWarnings("rawtypes") Map<String,BasePersistenceImpl> beanPersistenceImpls=PortalBeanLocatorUtil.locate(BasePersistenceImpl.class);
  SessionFactory sessionFactory=(SessionFactory)PortalBeanLocatorUtil.locate("liferaySessionFactory");
  for (  String name : beanPersistenceImpls.keySet()) {
    BasePersistenceImpl<?> beanPersistenceImpl=beanPersistenceImpls.get(name);
    beanPersistenceImpl.setSessionFactory(sessionFactory);
  }
}
