  String userName=_getParameter(request,PropsKeys.JDBC_DEFAULT_USERNAME,null);
  String password=_getParameter(request,PropsKeys.JDBC_DEFAULT_PASSWORD,null);
  String jndiName=StringPool.BLANK;
