{
  Map<String,Serializable> taskContextMap=btEntry.getTaskContextMap();
  long groupId=(Long)taskContextMap.get("groupId");
  boolean privateLayout=(Boolean)taskContextMap.get("privateLayout");
  Map<String,String[]> parameterMap=(Map<String,String[]>)taskContextMap.get("parameterMap");
  List<FileEntry> btEntryAttachments=btEntry.getAttachmentsFileEntries();
  for (  FileEntry btEntryAttachment : btEntryAttachments) {
    LayoutServiceUtil.importLayouts(groupId,privateLayout,parameterMap,btEntryAttachment.getContentStream());
  }
}
