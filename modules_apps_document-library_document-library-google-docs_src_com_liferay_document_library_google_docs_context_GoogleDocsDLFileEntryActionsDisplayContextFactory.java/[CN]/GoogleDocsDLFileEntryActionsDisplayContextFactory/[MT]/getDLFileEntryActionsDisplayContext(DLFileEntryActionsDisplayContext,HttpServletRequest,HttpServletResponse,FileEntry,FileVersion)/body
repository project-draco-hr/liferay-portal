{
  try {
    if (isGoogleDocs(fileVersion)) {
      return new GoogleDocsDLFileEntryActionsDisplayContext(parentDLFileEntryActionsDisplayContext,request,response,fileEntry,fileVersion);
    }
  }
 catch (  PortalException pe) {
    throw new SystemException(pe);
  }
  return parentDLFileEntryActionsDisplayContext;
}
