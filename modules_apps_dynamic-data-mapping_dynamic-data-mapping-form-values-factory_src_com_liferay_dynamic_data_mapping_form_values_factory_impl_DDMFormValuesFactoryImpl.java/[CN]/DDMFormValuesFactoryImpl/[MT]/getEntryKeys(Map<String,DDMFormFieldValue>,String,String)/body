{
  Set<String> entryKeys=new HashSet<>();
  String entryKeyPrefix=getEntryKeyPrefix(parentEntryKey,fieldNameFilter);
  for (  Map.Entry<String,DDMFormFieldValue> entry : ddmFormFieldValuesMap.entrySet()) {
    String key=entry.getKey();
    DDMFormFieldValue ddmFormFieldValue=entry.getValue();
    if (key.startsWith(entryKeyPrefix) && Validator.equals(ddmFormFieldValue.getName(),fieldNameFilter)) {
      entryKeys.add(key);
    }
  }
  return entryKeys;
}
