{
  Set<String> ddmFormFieldParameterNames=new TreeSet<>();
  ddmFormFieldParameterName=extractPrefix(ddmFormFieldParameterName);
  ddmFormFieldParameterName=extractSuffix(ddmFormFieldParameterName);
  ddmFormFieldParameterNames.add(ddmFormFieldParameterName);
  int pos=ddmFormFieldParameterName.indexOf(DDMFormRendererConstants.DDM_FORM_FIELDS_SEPARATOR);
  while (pos != -1) {
    ddmFormFieldParameterNames.add(ddmFormFieldParameterName.substring(0,pos));
    pos=ddmFormFieldParameterName.indexOf(DDMFormRendererConstants.DDM_FORM_FIELDS_SEPARATOR,pos + 1);
  }
  return ddmFormFieldParameterNames;
}
