{
  _configurationScopeMappings.put(JavaPlugin.COMPILE_CONFIGURATION_NAME,Conf2ScopeMappingContainer.COMPILE);
  _configurationScopeMappings.put("provided",Conf2ScopeMappingContainer.PROVIDED);
  if (OSDetector.isWindows()) {
    _mavenExecutable="mvn.cmd";
  }
 else {
    _mavenExecutable="mvn";
  }
}
