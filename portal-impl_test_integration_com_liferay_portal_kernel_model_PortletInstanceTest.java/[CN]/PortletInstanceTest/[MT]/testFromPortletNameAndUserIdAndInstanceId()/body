{
  PortletInstance portletInstance=PortletInstance.fromPortletNameAndUserIdAndInstanceId(PortletKeys.TEST,"1234_xyz");
  Assert.assertEquals("1234_xyz",portletInstance.getUserIdAndInstanceId());
  Assert.assertEquals("xyz",portletInstance.getInstanceId());
  Assert.assertEquals(PortletKeys.TEST,portletInstance.getPortletName());
  Assert.assertEquals(1234,portletInstance.getUserId());
}
