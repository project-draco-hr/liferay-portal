{
  AssetTagStats assetTagStats=(AssetTagStats)EntityCacheUtil.getResult(AssetTagStatsModelImpl.ENTITY_CACHE_ENABLED,AssetTagStatsImpl.class,tagStatsId);
  if (assetTagStats == _nullAssetTagStats) {
    return null;
  }
  if (assetTagStats == null) {
    Session session=null;
    try {
      session=openSession();
      assetTagStats=(AssetTagStats)session.get(AssetTagStatsImpl.class,Long.valueOf(tagStatsId));
      if (assetTagStats != null) {
        cacheResult(assetTagStats);
      }
 else {
        EntityCacheUtil.putResult(AssetTagStatsModelImpl.ENTITY_CACHE_ENABLED,AssetTagStatsImpl.class,tagStatsId,_nullAssetTagStats);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(AssetTagStatsModelImpl.ENTITY_CACHE_ENABLED,AssetTagStatsImpl.class,tagStatsId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return assetTagStats;
}
