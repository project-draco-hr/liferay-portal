{
  AssetTagStats assetTagStats=(AssetTagStats)EntityCacheUtil.getResult(AssetTagStatsModelImpl.ENTITY_CACHE_ENABLED,AssetTagStatsImpl.class,tagStatsId,this);
  if (assetTagStats == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (assetTagStats == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      assetTagStats=(AssetTagStats)session.get(AssetTagStatsImpl.class,Long.valueOf(tagStatsId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (assetTagStats == null)) {
        EntityCacheUtil.putResult(AssetTagStatsModelImpl.ENTITY_CACHE_ENABLED,AssetTagStatsImpl.class,tagStatsId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(assetTagStats);
      }
      closeSession(session);
    }
  }
  return assetTagStats;
}
