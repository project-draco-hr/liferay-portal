{
  AssetTagStats assetTagStats=fetchByPrimaryKey(tagStatsId);
  if (assetTagStats == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No AssetTagStats exists with the primary key " + tagStatsId);
    }
    throw new NoSuchTagStatsException("No AssetTagStats exists with the primary key " + tagStatsId);
  }
  return assetTagStats;
}
