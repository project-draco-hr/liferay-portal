{
  Map<String,Map<String,Map<String,String>>> fieldsMap=new HashMap<String,Map<String,Map<String,String>>>();
  Map<String,Map<String,String>> defaultLocaleFields=new HashMap<String,Map<String,String>>();
  fieldsMap.put(LocaleUtil.getSiteDefault().toString(),defaultLocaleFields);
  if (fieldNames != null) {
    for (    String fieldName : fieldNames) {
      defaultLocaleFields.put(fieldName,null);
    }
  }
  return fieldsMap;
}
