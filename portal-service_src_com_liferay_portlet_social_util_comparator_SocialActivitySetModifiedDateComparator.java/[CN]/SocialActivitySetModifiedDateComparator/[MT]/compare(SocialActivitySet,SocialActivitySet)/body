{
  long modifiedDate1=activitySet1.getModifiedDate();
  long modifiedDate2=activitySet2.getModifiedDate();
  int value=0;
  if (modifiedDate1 == modifiedDate2) {
    value=0;
  }
 else   if (modifiedDate1 < modifiedDate2) {
    value=-1;
  }
 else {
    value=1;
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
