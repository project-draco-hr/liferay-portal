        Thread.sleep(1000);
      }
    selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
boolean PortletPresent=selenium.isElementPresent("//span[3]/a/img");
if (!PortletPresent) {
label=3;
continue;
}
