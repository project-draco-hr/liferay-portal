{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      for (int second=0; ; second++) {
        if (second >= 60) {
          fail("timeout");
        }
        try {
          if (selenium.isElementPresent("link=Wiki Test Page")) {
            break;
          }
        }
 catch (        Exception e) {
