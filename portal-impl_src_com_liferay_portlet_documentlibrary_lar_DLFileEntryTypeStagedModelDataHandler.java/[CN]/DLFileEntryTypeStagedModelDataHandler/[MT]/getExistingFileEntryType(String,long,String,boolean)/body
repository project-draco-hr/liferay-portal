{
  DLFileEntryType existingDLFileEntryType=null;
  if (!preloaded) {
    existingDLFileEntryType=DLFileEntryTypeLocalServiceUtil.fetchDLFileEntryTypeByUuidAndGroupId(uuid,groupId);
  }
 else {
    existingDLFileEntryType=DLFileEntryTypeLocalServiceUtil.fetchFileEntryType(groupId,fileEntryTypeKey);
  }
  return existingDLFileEntryType;
}
