{
  List<DLFileEntryType> fileEntryTypes=DLFileEntryTypeLocalServiceUtil.getDLFileEntryTypesByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<DLFileEntryType>());
  if (ListUtil.isEmpty(fileEntryTypes)) {
    return null;
  }
  return fileEntryTypes.get(0);
}
