{
  String uuid=referenceElement.attributeValue("uuid");
  long groupId=GetterUtil.getLong(referenceElement.attributeValue("live-group-id"));
  String fileEntryTypeKey=referenceElement.attributeValue("file-entry-type-key");
  boolean preloaded=GetterUtil.getBoolean(referenceElement.attributeValue("preloaded"));
  if (!validateMissingGroupReference(portletDataContext,referenceElement)) {
    return false;
  }
  Map<Long,Long> groupIds=(Map<Long,Long>)portletDataContext.getNewPrimaryKeysMap(Group.class);
  groupId=MapUtil.getLong(groupIds,groupId,groupId);
  try {
    DLFileEntryType existingFileEntryType=fetchExistingFileEntryType(uuid,groupId,fileEntryTypeKey,preloaded);
    if (existingFileEntryType == null) {
      return false;
    }
    return true;
  }
 catch (  SystemException se) {
    return false;
  }
}
