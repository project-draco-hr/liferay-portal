{
  List<Long> folderIds=ListUtil.toList(new long[]{_folder.getFolderId()});
  String[] mimeTypes=null;
  if (mimeType != null) {
    mimeTypes=new String[]{mimeType};
  }
  long userId=0;
  if (withUserId) {
    userId=_folder.getUserId();
  }
  return DLFileEntryFinderUtil.findByG_U_F_M(_group.getGroupId(),userId,folderIds,mimeTypes,queryDefinition);
}
