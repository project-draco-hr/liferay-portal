{
  List<Long> repositoryIds=ListUtil.toList(new long[]{_defaultRepositoryFolder.getRepositoryId(),_newRepositoryFolder.getRepositoryId()});
  List<Long> folderIds=ListUtil.toList(new long[]{_defaultRepositoryFolder.getFolderId(),_newRepositoryFolder.getFolderId()});
  return doCountBy_G_U_R_F_M(userId,repositoryIds,folderIds,mimeType,queryDefinition);
}
