{
  QueryDefinition queryDefinition=new QueryDefinition();
  queryDefinition.setStatus(WorkflowConstants.STATUS_ANY);
  Assert.assertEquals(3,doCountBy_G_U_F_M(0,null,queryDefinition));
  Assert.assertEquals(2,doCountBy_G_U_F_M(_folder.getUserId(),null,queryDefinition));
  Assert.assertEquals(2,doCountBy_G_U_F_M(0,ContentTypes.TEXT_PLAIN,queryDefinition));
  Assert.assertEquals(1,doCountBy_G_U_F_M(_folder.getUserId(),ContentTypes.TEXT_PLAIN,queryDefinition));
  queryDefinition.setStatus(WorkflowConstants.STATUS_APPROVED);
  Assert.assertEquals(2,doCountBy_G_U_F_M(0,null,queryDefinition));
  Assert.assertEquals(1,doCountBy_G_U_F_M(_folder.getUserId(),null,queryDefinition));
  Assert.assertEquals(1,doCountBy_G_U_F_M(0,ContentTypes.TEXT_PLAIN,queryDefinition));
  Assert.assertEquals(0,doCountBy_G_U_F_M(_folder.getUserId(),ContentTypes.TEXT_PLAIN,queryDefinition));
  queryDefinition.setStatus(WorkflowConstants.STATUS_IN_TRASH,true);
  Assert.assertEquals(2,doCountBy_G_U_F_M(0,null,queryDefinition));
  Assert.assertEquals(1,doCountBy_G_U_F_M(_folder.getUserId(),null,queryDefinition));
  Assert.assertEquals(1,doCountBy_G_U_F_M(0,ContentTypes.TEXT_PLAIN,queryDefinition));
  Assert.assertEquals(0,doCountBy_G_U_F_M(_folder.getUserId(),ContentTypes.TEXT_PLAIN,queryDefinition));
}
