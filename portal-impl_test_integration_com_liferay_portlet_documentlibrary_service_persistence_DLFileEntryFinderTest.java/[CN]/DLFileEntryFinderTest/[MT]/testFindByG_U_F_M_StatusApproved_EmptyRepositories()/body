{
  QueryDefinition queryDefinition=new QueryDefinition();
  queryDefinition.setStatus(WorkflowConstants.STATUS_APPROVED);
  List<DLFileEntry> dlFileEntries=doFindBy_G_U_R_F_M_EmptyRepositories(_defaultRepositoryFolder.getUserId(),null,queryDefinition);
