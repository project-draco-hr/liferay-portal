{
  List<Long> repositoryIds=ListUtil.toList(new long[]{_folderDefaultRepository.getRepositoryId(),_folderNewRepository.getRepositoryId()});
  List<Long> folderIds=ListUtil.toList(new long[]{_folderDefaultRepository.getFolderId(),_folderNewRepository.getFolderId()});
  return doFindBy_G_U_R_F_M(userId,repositoryIds,folderIds,mimeType,queryDefinition);
}
