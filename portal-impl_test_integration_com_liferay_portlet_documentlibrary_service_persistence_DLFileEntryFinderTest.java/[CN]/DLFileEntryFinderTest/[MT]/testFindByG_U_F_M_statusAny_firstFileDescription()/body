{
  QueryDefinition queryDefinition=getQueryDefinition(WorkflowConstants.STATUS_ANY);
  List<DLFileEntry> dlFileEntries=doFindBy_G_U_F_M(_folder.getUserId(),ContentTypes.TEXT_PLAIN,queryDefinition);
  DLFileEntry dlFileEntry=dlFileEntries.get(0);
  Assert.assertEquals("FE3.txt",dlFileEntry.getDescription());
}
