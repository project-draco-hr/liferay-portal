{
  if (Validator.isNotNull(_arrayableOperator)) {
    if (!_type.equals("char") && !_type.equals("int") && !_type.equals("long")&& !_type.equals("short")&& !_type.equals("String")) {
      throw new IllegalArgumentException("Type " + _type + " cannot be arrayable");
    }
  }
  String comparator=_comparator;
  if (comparator == null) {
    comparator=StringPool.EQUAL;
  }
  if (_arrayableOperator.equals("AND") && !comparator.equals(StringPool.NOT_EQUAL)) {
    throw new IllegalArgumentException("Illegal combination of arrayable \"AND\" and comparator \"" + comparator + "\"");
  }
  if (_arrayableOperator.equals("OR") && !comparator.equals(StringPool.EQUAL) && !comparator.equals(StringPool.LIKE)) {
    throw new IllegalArgumentException("Illegal combination of arrayable \"OR\" and comparator \"" + comparator + "\"");
  }
}
