{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Sample Icefaces IPC Ajax Push Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Sample Icefaces IPC Ajax Push Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Sample ICEfaces IPC - Bookings"));
  selenium.click("//img[@alt='Remove']");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if ("Are you sure you want to remove this component?".equals(selenium.getConfirmation())) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertFalse(selenium.isTextPresent("Sample ICEfaces IPC - Bookings"));
}
