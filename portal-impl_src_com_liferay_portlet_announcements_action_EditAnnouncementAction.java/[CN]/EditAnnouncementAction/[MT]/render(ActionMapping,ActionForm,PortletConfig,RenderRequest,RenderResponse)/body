{
  try {
    ActionUtil.getAnnouncement(req);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchAnnouncementException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.announcements.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward(getForward(req,"portlet.announcements.edit_announcement"));
}
