{
  _bundleContext=bundleContext;
  Filter filter=bundleContext.createFilter("(&(objectClass=" + PanelApp.class.getName() + ")"+ "(panel.category.key="+ PanelCategoryKeys.SITE_ADMINISTRATION+ "*))");
  _serviceTracker=new ServiceTracker<>(bundleContext,filter,new PanelAppServiceTrackerCustomizer());
  _serviceTracker.open();
}
