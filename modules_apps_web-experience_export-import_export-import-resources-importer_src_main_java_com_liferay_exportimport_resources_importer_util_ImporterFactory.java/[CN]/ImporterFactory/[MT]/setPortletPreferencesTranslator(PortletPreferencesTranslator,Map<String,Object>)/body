{
  String rootPortletId=GetterUtil.getString(properties.get("translator.root.portlet.id"));
  if (Validator.isNull(rootPortletId)) {
    if (_log.isWarnEnabled()) {
      _log.warn("The property \"translator.root.portlet.id\" is null");
    }
    return;
  }
  _portletPreferencesTranslators.put(rootPortletId,portletPreferencesTranslator);
}
