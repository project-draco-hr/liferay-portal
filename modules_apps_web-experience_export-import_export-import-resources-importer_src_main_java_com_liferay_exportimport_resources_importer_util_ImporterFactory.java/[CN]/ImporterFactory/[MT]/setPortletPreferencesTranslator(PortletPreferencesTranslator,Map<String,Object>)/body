{
  String rootPortletId=GetterUtil.getString(properties.get("rootPortletId"));
  if (Validator.isNull(rootPortletId)) {
    if (_log.isWarnEnabled()) {
      _log.warn("The property \"rootPortletId\" is null");
    }
    return;
  }
  _portletPreferencesTranslators.put(rootPortletId,portletPreferencesTranslator);
}
