{
  List<User> users=UserLocalServiceUtil.getOrganizationUsers(WOLLoginPostAction.ORGANIZATION_LIFERAY_INC_ID);
  for (  User user1 : users) {
    WOLLoginPostAction.addAdvancedUserPages(user1);
    for (    User user2 : users) {
      if (user1.getUserId() != user2.getUserId()) {
        SocialRelationLocalServiceUtil.addRelation(user1.getUserId(),user2.getUserId(),SocialRelationConstants.TYPE_BI_FRIEND);
      }
    }
  }
}
