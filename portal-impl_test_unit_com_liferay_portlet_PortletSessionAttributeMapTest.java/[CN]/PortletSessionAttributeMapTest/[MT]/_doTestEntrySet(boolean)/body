{
  PortletSessionAttributeMap portletSessionAttributeMap=new PortletSessionAttributeMap(_session,_getScopePrefix(portletScope));
  Set<Entry<String,Object>> entrySet=portletSessionAttributeMap.entrySet();
  Assert.assertEquals(3,entrySet.size());
  Assert.assertTrue(entrySet.contains(new SimpleEntry<>(_encodeKey(portletScope,_KEY_1),_value1)));
  Assert.assertTrue(entrySet.contains(new SimpleEntry<>(_encodeKey(portletScope,_KEY_2),_value2)));
  Assert.assertTrue(entrySet.contains(new SimpleEntry<>(_encodeKey(portletScope,_KEY_3),_value3)));
}
