{
  PortletSessionAttributeMap portletSessionAttributeMap=new PortletSessionAttributeMap(_session);
  Assert.assertSame(_session,portletSessionAttributeMap.session);
  Assert.assertNull(portletSessionAttributeMap.scopePrefix);
  portletSessionAttributeMap=new PortletSessionAttributeMap(_session,null);
  Assert.assertSame(_session,portletSessionAttributeMap.session);
  Assert.assertNull(portletSessionAttributeMap.scopePrefix);
  String scopePrefix="scopePrefix";
  portletSessionAttributeMap=new PortletSessionAttributeMap(_session,scopePrefix);
  Assert.assertSame(_session,portletSessionAttributeMap.session);
  Assert.assertSame(scopePrefix,portletSessionAttributeMap.scopePrefix);
}
