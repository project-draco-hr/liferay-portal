{
  PortletSessionAttributeMap portletSessionAttributeMap=new PortletSessionAttributeMap(_session,getScopePrefix(portletScope));
  Assert.assertFalse(portletSessionAttributeMap.containsKey(null));
  Assert.assertTrue(portletSessionAttributeMap.containsKey(encodeKey(portletScope,_KEY_1)));
  Assert.assertTrue(portletSessionAttributeMap.containsKey(encodeKey(portletScope,_KEY_2)));
  Assert.assertTrue(portletSessionAttributeMap.containsKey(encodeKey(portletScope,_KEY_3)));
  Assert.assertFalse(portletSessionAttributeMap.containsKey(encodeKey(portletScope,_KEY_4)));
  Assert.assertFalse(portletSessionAttributeMap.containsKey(encodeKey(portletScope,_KEY_5)));
}
