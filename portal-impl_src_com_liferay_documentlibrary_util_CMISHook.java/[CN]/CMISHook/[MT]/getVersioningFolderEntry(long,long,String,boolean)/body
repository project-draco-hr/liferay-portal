{
  Entry repositoryFolderEntry=getRepositoryFolderEntry(companyId,repositoryId);
  Link link=repositoryFolderEntry.getLink(_cmisConstants.LINK_DESCENDANTS);
  String url=link.getHref().toString();
  Entry versioningFolderEntry=CMISUtil.getEntry(url,fileName,_cmisConstants.BASE_TYPE_FOLDER);
  if (create && (versioningFolderEntry == null)) {
    versioningFolderEntry=CMISUtil.createFolder(url,fileName);
  }
  return versioningFolderEntry;
}
