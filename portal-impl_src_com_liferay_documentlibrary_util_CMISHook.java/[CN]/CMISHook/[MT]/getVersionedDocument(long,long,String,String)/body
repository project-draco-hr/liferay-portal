{
  Folder versioningFolder=getVersioningFolder(companyId,repositoryId,fileName,false);
  if (versioningFolder == null) {
    throw new NoSuchFileException();
  }
  Document document=getDocument(versioningFolder,versionNumber);
  if (document == null) {
    throw new NoSuchFileException();
  }
  return document;
}
