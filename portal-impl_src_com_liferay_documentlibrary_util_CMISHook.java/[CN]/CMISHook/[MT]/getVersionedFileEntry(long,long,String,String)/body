{
  Entry versioningFolderEntry=getVersioningFolderEntry(companyId,repositoryId,fileName,false);
  if (versioningFolderEntry == null) {
    throw new NoSuchFileException();
  }
  Link link=versioningFolderEntry.getLink(_cmisConstants.LINK_DESCENDANTS);
  String url=link.getHref().toString();
  Entry fileEntry=CMISUtil.getEntry(url,String.valueOf(versionNumber),_cmisConstants.BASE_TYPE_DOCUMENT);
  if (fileEntry == null) {
    throw new NoSuchFileException();
  }
  return fileEntry;
}
