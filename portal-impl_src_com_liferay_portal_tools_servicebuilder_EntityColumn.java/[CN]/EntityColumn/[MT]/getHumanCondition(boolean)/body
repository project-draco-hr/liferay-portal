{
  StringBundler sb=new StringBundler();
  sb.append(_name);
  sb.append(" ");
  sb.append(convertComparatorToHtml(_comparator));
  sb.append(" ");
  if (arrayable && hasArrayableOperator()) {
    if (isArrayableAndOperator()) {
      sb.append("all ");
    }
 else {
      sb.append("any ");
    }
  }
  sb.append("&#63;");
  return sb.toString();
}
