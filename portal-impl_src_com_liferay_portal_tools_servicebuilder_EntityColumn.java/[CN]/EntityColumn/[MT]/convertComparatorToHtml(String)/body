{
  if (comparator.equals(">")) {
    return "&gt;";
  }
  if (comparator.equals("<")) {
    return "&lt;";
  }
  if (comparator.equals(">=")) {
    return "&ge;";
  }
  if (comparator.equals("<=")) {
    return "&le;";
  }
  if (comparator.equals("!=")) {
    return "&ne;";
  }
  return comparator;
}
