{
  return new EntityColumn(getName(),getDBName(),getType(),isPrimary(),isAccessor(),isFilterPrimary(),getEJBName(),getMappingKey(),getMappingTable(),isCaseSensitive(),isOrderByAscending(),isOrderColumn(),getComparator(),getArrayableOperator(),getIdType(),getIdParam(),isConvertNull(),isLazy(),isLocalized(),isJsonEnabled());
}
