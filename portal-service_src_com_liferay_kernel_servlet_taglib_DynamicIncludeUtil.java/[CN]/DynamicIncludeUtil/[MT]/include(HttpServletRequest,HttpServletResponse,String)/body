{
  List<DynamicInclude> dynamicIncludes=getDynamicIncludes(key);
  if ((dynamicIncludes != null) && !dynamicIncludes.isEmpty()) {
    for (    DynamicInclude dynamicInclude : dynamicIncludes) {
      try {
        dynamicInclude.include(request,response);
      }
 catch (      Exception e) {
        _log.error(e,e);
      }
    }
  }
}
