{
  String orderByCol=ParamUtil.getString(_workflowDefinitionLinkRequestHelper.getRequest(),"orderByCol","resource");
  String orderByType=ParamUtil.getString(_workflowDefinitionLinkRequestHelper.getRequest(),"orderByType","asc");
  return WorkflowDefinitionLinkPortletUtil.getWorkflowDefinitionLinkOrderByComparator(orderByCol,orderByType);
}
