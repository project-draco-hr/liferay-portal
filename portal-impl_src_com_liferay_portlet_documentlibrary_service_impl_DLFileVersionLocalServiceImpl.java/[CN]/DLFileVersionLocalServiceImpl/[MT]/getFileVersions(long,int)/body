{
  List<DLFileVersion> dlFileVersions=null;
  if (status == WorkflowConstants.STATUS_ANY) {
    dlFileVersions=dlFileVersionPersistence.findByFileEntryId(fileEntryId);
  }
 else {
    dlFileVersions=dlFileVersionPersistence.findByF_S(fileEntryId,status);
  }
  dlFileVersions=ListUtil.copy(dlFileVersions);
  Collections.sort(dlFileVersions,new FileVersionVersionComparator());
  return dlFileVersions;
}
