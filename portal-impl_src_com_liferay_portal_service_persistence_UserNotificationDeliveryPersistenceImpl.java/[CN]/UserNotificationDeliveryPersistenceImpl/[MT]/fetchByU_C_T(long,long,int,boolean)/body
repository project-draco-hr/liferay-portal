{
  Object[] finderArgs=new Object[]{userId,classNameId,type};
  Object result=null;
  if (retrieveFromCache) {
    result=FinderCacheUtil.getResult(FINDER_PATH_FETCH_BY_U_C_T,finderArgs,this);
  }
  if (result instanceof UserNotificationDelivery) {
    UserNotificationDelivery userNotificationDelivery=(UserNotificationDelivery)result;
    if ((userId != userNotificationDelivery.getUserId()) || (classNameId != userNotificationDelivery.getClassNameId()) || (type != userNotificationDelivery.getType())) {
      result=null;
    }
  }
  if (result == null) {
    StringBundler query=new StringBundler(5);
    query.append(_SQL_SELECT_USERNOTIFICATIONDELIVERY_WHERE);
    query.append(_FINDER_COLUMN_U_C_T_USERID_2);
    query.append(_FINDER_COLUMN_U_C_T_CLASSNAMEID_2);
    query.append(_FINDER_COLUMN_U_C_T_TYPE_2);
    String sql=query.toString();
    Session session=null;
    try {
      session=openSession();
      Query q=session.createQuery(sql);
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(userId);
      qPos.add(classNameId);
      qPos.add(type);
      List<UserNotificationDelivery> list=q.list();
      if (list.isEmpty()) {
        FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_U_C_T,finderArgs,list);
      }
 else {
        UserNotificationDelivery userNotificationDelivery=list.get(0);
        result=userNotificationDelivery;
        cacheResult(userNotificationDelivery);
        if ((userNotificationDelivery.getUserId() != userId) || (userNotificationDelivery.getClassNameId() != classNameId) || (userNotificationDelivery.getType() != type)) {
          FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_U_C_T,finderArgs,userNotificationDelivery);
        }
      }
    }
 catch (    Exception e) {
      FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_U_C_T,finderArgs);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  if (result instanceof List<?>) {
    return null;
  }
 else {
    return (UserNotificationDelivery)result;
  }
}
