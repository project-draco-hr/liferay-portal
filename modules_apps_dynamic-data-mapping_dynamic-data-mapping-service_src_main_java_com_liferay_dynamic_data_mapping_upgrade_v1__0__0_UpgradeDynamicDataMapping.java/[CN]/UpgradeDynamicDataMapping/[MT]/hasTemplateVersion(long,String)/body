{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select * from DDMTemplateVersion where templateId = ? and " + "version = ?");
    ps.setLong(1,templateId);
    ps.setString(2,version);
    rs=ps.executeQuery();
    return rs.next();
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
