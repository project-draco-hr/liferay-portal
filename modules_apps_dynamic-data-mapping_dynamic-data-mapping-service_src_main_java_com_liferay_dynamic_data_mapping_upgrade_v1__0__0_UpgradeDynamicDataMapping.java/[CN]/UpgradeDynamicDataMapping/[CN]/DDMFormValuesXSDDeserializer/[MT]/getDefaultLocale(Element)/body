{
  if (dynamicElementElement == null) {
    String locale=null;
    try {
      locale=UpgradeProcessUtil.getDefaultLanguageId(_companyId);
    }
 catch (    SQLException sqle) {
      _log.error("Unable to get default locale for company " + _companyId,sqle);
      throw new RuntimeException(sqle);
    }
    return LocaleUtil.fromLanguageId(locale);
  }
  String defaultLanguageId=dynamicElementElement.attributeValue("default-language-id");
  return LocaleUtil.fromLanguageId(defaultLanguageId);
}
