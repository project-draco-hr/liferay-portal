{
  Set<Class<? extends Capability>> supportedCapabilitiesSet=supportedCapabilities.keySet();
  if (!supportedCapabilitiesSet.containsAll(exportedCapabilityClasses)) {
    throw new IllegalArgumentException(String.format("Unable to export unsupported capability %s. Supported " + "capabilities are %s.",exportedCapabilityClasses,supportedCapabilities.keySet()));
  }
  _supportedCapabilities=supportedCapabilities;
  _exportedCapabilityClasses=exportedCapabilityClasses;
}
