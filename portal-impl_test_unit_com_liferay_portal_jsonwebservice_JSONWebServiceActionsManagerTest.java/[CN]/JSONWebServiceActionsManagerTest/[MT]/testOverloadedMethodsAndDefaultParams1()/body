{
  MockHttpServletRequest mockHttpServletRequest=createHttpRequest("/abc/hello");
  mockHttpServletRequest.setParameter("a","123");
  mockHttpServletRequest.setAttribute("c","qwe");
  JSONWebServiceAction jsonWebServiceAction=JSONWebServiceActionsManagerUtil.getJSONWebServiceAction(mockHttpServletRequest);
  String string=(String)jsonWebServiceAction.invoke();
  Assert.assertEquals("hello:123",string);
}
