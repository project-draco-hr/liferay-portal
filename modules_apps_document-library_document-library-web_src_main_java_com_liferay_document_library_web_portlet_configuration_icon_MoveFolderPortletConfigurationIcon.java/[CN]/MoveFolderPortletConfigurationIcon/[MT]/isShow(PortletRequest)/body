{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    if (DLFolderPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),_folder.getFolderId(),ActionKeys.UPDATE) && !_folder.isMountPoint()) {
      return true;
    }
  }
 catch (  PortalException pe) {
  }
  return false;
}
