{
  _serviceTrackerMap=ServiceTrackerMapFactory.openMultiValueMap(_bundleContext,TrackedOne.class,null,new PropertyServiceReferenceMapper<String,TrackedOne>("target"),new Comparator<ServiceReference<TrackedOne>>(){
    @Override public int compare(    ServiceReference<TrackedOne> serviceReference1,    ServiceReference<TrackedOne> serviceReference2){
      return 0;
    }
  }
);
  registerService(new TrackedOne());
  registerService(new TrackedOne());
  List<TrackedOne> services=_serviceTrackerMap.getService("aTarget");
  Assert.assertEquals(2,services.size());
}
