{
  _color=color;
  if (_calendarRemoteModel != null) {
    try {
      Class<?> clazz=_calendarRemoteModel.getClass();
      Method method=clazz.getMethod("setColor",int.class);
      method.invoke(_calendarRemoteModel,color);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
