{
  super.init(portletConfig);
  PortletContext portletContext=portletConfig.getPortletContext();
  _portletContextName=portletContext.getPortletContextName();
  _actionTemplateId=getVelocityTemplateId(getInitParameter("action-template"));
  _editTemplateId=getVelocityTemplateId(getInitParameter("edit-template"));
  _helpTemplateId=getVelocityTemplateId(getInitParameter("help-template"));
  _resourceTemplateId=getVelocityTemplateId(getInitParameter("resource-template"));
  _viewTemplateId=getVelocityTemplateId(getInitParameter("view-template"));
}
