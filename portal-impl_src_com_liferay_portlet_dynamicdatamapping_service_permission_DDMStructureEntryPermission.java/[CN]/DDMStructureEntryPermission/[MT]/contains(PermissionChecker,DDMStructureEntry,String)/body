{
  if (permissionChecker.hasOwnerPermission(structureEntry.getCompanyId(),DDMStructureEntry.class.getName(),structureEntry.getStructureEntryId(),structureEntry.getUserId(),actionId)) {
    return true;
  }
  return permissionChecker.hasPermission(structureEntry.getGroupId(),DDMStructureEntry.class.getName(),structureEntry.getStructureEntryId(),actionId);
}
