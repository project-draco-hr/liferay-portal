{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Blogs Permissions Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Blogs Permissions Page"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("link=Permissions Blogs Test Entry"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("CA Permissions Blogs Test Comment "));
  assertTrue(selenium.isTextPresent("Member Permissions Blogs Test Comment"));
}
