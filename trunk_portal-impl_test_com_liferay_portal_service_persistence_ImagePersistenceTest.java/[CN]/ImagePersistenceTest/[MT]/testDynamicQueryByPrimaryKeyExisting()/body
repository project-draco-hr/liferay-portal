{
  Image newImage=addImage();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Image.class,Image.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("imageId",newImage.getImageId()));
  List<Image> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Image existingImage=result.get(0);
  assertEquals(existingImage,newImage);
}
