{
  String typeName=proceedingJoinPoint.getTarget().getClass().getName();
  Log log=getLog(typeName);
  if (log.isInfoEnabled()) {
    log.info("Before " + typeName);
  }
  Object returnVal=proceedingJoinPoint.proceed();
  if (log.isInfoEnabled()) {
    log.info("After " + typeName);
  }
  return returnVal;
}
