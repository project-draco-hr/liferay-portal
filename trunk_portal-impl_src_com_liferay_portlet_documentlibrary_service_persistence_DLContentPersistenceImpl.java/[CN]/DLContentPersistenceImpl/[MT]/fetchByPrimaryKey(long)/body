{
  DLContent dlContent=(DLContent)EntityCacheUtil.getResult(DLContentModelImpl.ENTITY_CACHE_ENABLED,DLContentImpl.class,contentId,this);
  if (dlContent == null) {
    Session session=null;
    try {
      session=openSession();
      dlContent=(DLContent)session.get(DLContentImpl.class,Long.valueOf(contentId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (dlContent != null) {
        cacheResult(dlContent);
      }
      closeSession(session);
    }
  }
  return dlContent;
}
