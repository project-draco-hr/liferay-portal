{
  if (requestDispatcher != null) {
    if (PropsValues.PORTLET_CONTAINER_IMPL_SUN) {
      requestDispatcher=new StrutsBridgeRequestDispatcher(requestDispatcher,path);
    }
 else {
      requestDispatcher=new LiferayRequestDispatcher(requestDispatcher,path);
    }
  }
  return requestDispatcher;
}
