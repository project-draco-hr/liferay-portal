{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),SyncDeviceLocalService.class.getName());
    if (invokableLocalService instanceof SyncDeviceLocalService) {
      _service=(SyncDeviceLocalService)invokableLocalService;
    }
 else {
      _service=new SyncDeviceLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(SyncDeviceLocalServiceUtil.class,"_service");
  }
  return _service;
}
