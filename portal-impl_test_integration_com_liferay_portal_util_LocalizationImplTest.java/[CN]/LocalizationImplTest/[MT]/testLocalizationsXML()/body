{
  String xml=StringPool.BLANK;
  xml=LocalizationUtil.updateLocalization(xml,"greeting",_englishHello,_englishId,_englishId);
  xml=LocalizationUtil.updateLocalization(xml,"greeting",_germanHello,_germanId,_englishId);
  Assert.assertEquals(_englishHello,LocalizationUtil.getLocalization(xml,_englishId));
  Assert.assertEquals(_germanHello,LocalizationUtil.getLocalization(xml,_germanId));
}
