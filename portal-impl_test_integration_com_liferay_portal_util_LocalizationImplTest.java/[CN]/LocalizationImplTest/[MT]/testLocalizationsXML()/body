{
  String xml=StringPool.BLANK;
  xml=LocalizationUtil.updateLocalization(xml,"greeting",_englishHello,_englishLanguageId,_englishLanguageId);
  xml=LocalizationUtil.updateLocalization(xml,"greeting",_germanHello,_germanLanguageId,_englishLanguageId);
  Assert.assertEquals(_englishHello,LocalizationUtil.getLocalization(xml,_englishLanguageId));
  Assert.assertEquals(_germanHello,LocalizationUtil.getLocalization(xml,_germanLanguageId));
}
