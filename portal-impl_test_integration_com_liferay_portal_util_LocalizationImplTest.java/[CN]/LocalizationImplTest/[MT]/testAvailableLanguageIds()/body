{
  Document document=SAXReaderUtil.read(_xml);
  String[] languageIdsFromDoc=LocalizationUtil.getAvailableLanguageIds(document);
  String[] languageIdsFromXml=LocalizationUtil.getAvailableLanguageIds(_xml);
  Assert.assertEquals(languageIdsFromDoc.length,2);
  Assert.assertEquals(languageIdsFromXml.length,2);
  Arrays.sort(languageIdsFromDoc);
  Arrays.sort(languageIdsFromXml);
  Assert.assertTrue("Available language ids from Document and XML don't match",Arrays.equals(languageIdsFromDoc,languageIdsFromXml));
}
