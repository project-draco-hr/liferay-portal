{
  Document document=SAXReaderUtil.read(_xml);
  String languageIdsFromDoc=LocalizationUtil.getDefaultLanguageId(document);
  String languageIdsFromXml=LocalizationUtil.getDefaultLanguageId(_xml);
  Assert.assertEquals("The default language ids from Document and XML don't match",languageIdsFromDoc,languageIdsFromXml);
}
