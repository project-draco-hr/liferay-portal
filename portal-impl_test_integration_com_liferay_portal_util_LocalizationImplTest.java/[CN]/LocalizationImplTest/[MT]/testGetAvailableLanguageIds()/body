{
  Document document=SAXReaderUtil.read(_xml);
  String[] documentAvailableLanguageIds=LocalizationUtil.getAvailableLanguageIds(document);
  Assert.assertEquals(documentAvailableLanguageIds.length,2);
  String[] xmlAvailableLanguageIds=LocalizationUtil.getAvailableLanguageIds(_xml);
  Assert.assertEquals(xmlAvailableLanguageIds.length,2);
  Arrays.sort(documentAvailableLanguageIds);
  Arrays.sort(xmlAvailableLanguageIds);
  Assert.assertTrue("Available language IDs between the document and XML do not match",Arrays.equals(documentAvailableLanguageIds,xmlAvailableLanguageIds));
}
