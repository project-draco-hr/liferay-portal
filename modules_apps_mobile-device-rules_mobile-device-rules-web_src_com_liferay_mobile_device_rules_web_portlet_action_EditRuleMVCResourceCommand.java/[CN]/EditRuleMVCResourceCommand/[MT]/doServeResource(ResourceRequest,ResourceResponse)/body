{
  long ruleId=ParamUtil.getLong(resourceRequest,"ruleId");
  if (ruleId > 0) {
    MDRRule rule=MDRRuleServiceUtil.fetchRule(ruleId);
    resourceRequest.setAttribute(MobileDeviceRulesWebKeys.MOBILE_DEVICE_RULES_RULE,rule);
  }
  String type=ParamUtil.getString(resourceRequest,"type");
  ActionUtil.includeEditorJSP(getPortletConfig(resourceRequest),resourceRequest,resourceResponse,type,ActionUtil.getRuleEditorJSPMethod);
}
