{
  List<Long> hitLayoutIds=JournalContentSearchLocalServiceUtil.getLayoutIds(_article.getGroupId(),privateLayout,_article.getArticleId());
  for (  Long hitLayoutId : hitLayoutIds) {
    Layout hitLayout=LayoutLocalServiceUtil.getLayout(_article.getGroupId(),privateLayout,hitLayoutId.longValue());
    if (LayoutPermissionUtil.contains(themeDisplay.getPermissionChecker(),hitLayout,ActionKeys.VIEW)) {
      return PortalUtil.getLayoutURL(hitLayout,themeDisplay);
    }
  }
  return noSuchEntryRedirect;
}
