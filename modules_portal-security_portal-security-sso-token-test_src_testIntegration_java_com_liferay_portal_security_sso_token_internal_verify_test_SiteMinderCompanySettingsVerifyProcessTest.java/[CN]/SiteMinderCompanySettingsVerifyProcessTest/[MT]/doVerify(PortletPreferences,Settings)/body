{
  for (  String key : LegacyTokenPropsKeys.SHIBBOLETH_KEYS) {
    Assert.assertTrue(Validator.isNull(portletPreferences.getValue(key,StringPool.BLANK)));
  }
  Assert.assertTrue(GetterUtil.getBoolean(settings.getValue(TokenConstants.AUTH_ENABLED,StringPool.FALSE)));
  Assert.assertFalse(GetterUtil.getBoolean(settings.getValue(TokenConstants.IMPORT_FROM_LDAP,StringPool.TRUE)));
  Assert.assertEquals("testSiteminder",settings.getValue(TokenConstants.USER_HEADER,StringPool.BLANK));
}
