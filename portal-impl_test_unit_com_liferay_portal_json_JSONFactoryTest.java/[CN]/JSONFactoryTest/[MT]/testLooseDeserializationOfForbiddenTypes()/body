{
  try {
    JSONFactoryUtil.looseDeserialize("{\"class\":" + "\"com.liferay.portal.kernel.dao.orm.EntityCacheUtil\"}}");
    fail();
  }
 catch (  Exception e) {
  }
  try {
    Object object=JSONFactoryUtil.looseDeserialize("{\"class\":\"java.lang.Thread\"}}");
    assertEquals(Thread.class,object.getClass());
  }
 catch (  Exception e) {
    fail(e.toString());
  }
}
