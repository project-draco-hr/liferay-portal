{
  Connection con=null;
  PreparedStatement ps=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("delete from ResourcePermission where name = ?");
    ps.setString(1,AssetTag.class.getName());
    ps.execute();
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
}
