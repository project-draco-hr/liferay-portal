{
  PortalIncludeUtil.include(pageContext,new PortalIncludeUtil.HTMLRenderer(){
    @Override public void renderHTML(    HttpServletRequest request,    HttpServletResponse response) throws IOException, ServletException {
      _itemSelectorView.renderHTML(request,response,_itemSelectorCriterion,_portletURL,_itemSelectedEventName);
    }
  }
);
}
