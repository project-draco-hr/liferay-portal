{
  long workflowTaskId=ParamUtil.getLong(portletRequest,WorkflowTaskConstants.WORKFLOW_TASK_ID);
  long assigneeUserId=ParamUtil.getLong(portletRequest,WorkflowTaskConstants.ASSIGNEE_USER_ID);
  String comment=ParamUtil.getString(portletRequest,WorkflowTaskConstants.COMMENT);
  WorkflowTaskManagerUtil.assignWorkflowTaskToUser(getCompanyId(portletRequest),getUserId(portletRequest),workflowTaskId,assigneeUserId,comment,null,null);
  super.doProcessCommand(portletRequest,portletResponse);
}
