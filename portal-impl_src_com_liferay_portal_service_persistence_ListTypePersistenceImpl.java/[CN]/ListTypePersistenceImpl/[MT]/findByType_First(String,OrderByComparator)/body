{
  List<ListType> list=findByType(type,0,1,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(4);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("type=");
    msg.append(type);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchListTypeException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
