{
  Session session=null;
  try {
    session=openSession();
    ListType listType=(ListType)session.get(ListTypeImpl.class,new Integer(listTypeId));
    if (listType == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + listTypeId);
      }
      throw new NoSuchListTypeException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + listTypeId);
    }
    return remove(listType);
  }
 catch (  NoSuchListTypeException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
