{
  ListType listType=findByPrimaryKey(listTypeId);
  Session session=null;
  try {
    session=openSession();
    ListType[] array=new ListTypeImpl[3];
    array[0]=getByType_PrevAndNext(session,listType,type,orderByComparator,true);
    array[1]=listType;
    array[2]=getByType_PrevAndNext(session,listType,type,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
