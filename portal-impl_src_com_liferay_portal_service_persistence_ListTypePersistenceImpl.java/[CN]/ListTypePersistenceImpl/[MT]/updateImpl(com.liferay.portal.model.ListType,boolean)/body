{
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(listType);
    }
 else {
      if (listType.isNew()) {
        session.save(listType);
      }
    }
    session.flush();
    listType.setNew(false);
    return listType;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ListType.class.getName());
  }
}
