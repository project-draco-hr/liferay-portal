{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,listType,merge);
    listType.setNew(false);
    return listType;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ListType.class.getName());
  }
}
