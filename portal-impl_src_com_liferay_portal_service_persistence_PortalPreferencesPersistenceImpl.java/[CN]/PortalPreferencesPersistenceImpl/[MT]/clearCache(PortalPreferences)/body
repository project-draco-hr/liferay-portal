{
  EntityCacheUtil.removeResult(PortalPreferencesModelImpl.ENTITY_CACHE_ENABLED,PortalPreferencesImpl.class,portalPreferences.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_O_O,new Object[]{Long.valueOf(portalPreferences.getOwnerId()),Integer.valueOf(portalPreferences.getOwnerType())});
}
