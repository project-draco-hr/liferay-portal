{
  EntityCacheUtil.putResult(PortalPreferencesModelImpl.ENTITY_CACHE_ENABLED,PortalPreferencesImpl.class,portalPreferences.getPrimaryKey(),portalPreferences);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_O_O,new Object[]{portalPreferences.getOwnerId(),portalPreferences.getOwnerType()},portalPreferences);
  portalPreferences.resetOriginalValues();
}
