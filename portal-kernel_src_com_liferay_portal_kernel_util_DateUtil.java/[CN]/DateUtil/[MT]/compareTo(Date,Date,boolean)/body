{
  if (!ignoreMilliseconds) {
    return compareTo(date1,date2);
  }
  if ((date1 != null) && (date2 == null)) {
    return -1;
  }
 else   if ((date1 == null) && (date2 != null)) {
    return 1;
  }
 else   if ((date1 == null) && (date2 == null)) {
    return 0;
  }
  long time1=date1.getTime() / Time.SECOND;
  long time2=date2.getTime() / Time.SECOND;
  if (time1 == time2) {
    return 0;
  }
 else   if (time1 < time2) {
    return -1;
  }
 else {
    return 1;
  }
}
