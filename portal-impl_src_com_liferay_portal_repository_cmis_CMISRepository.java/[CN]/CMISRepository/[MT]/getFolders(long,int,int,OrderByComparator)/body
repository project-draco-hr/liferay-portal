{
  List<Folder> folders=getFolders(parentFolderId);
  if (obc != null) {
    if (obc instanceof FolderCreateDateComparator || obc instanceof FolderModifiedDateComparator) {
      folders=ListUtil.sort(folders,obc);
    }
 else     if (obc instanceof FolderNameComparator) {
      if (!obc.isAscending()) {
        folders=ListUtil.sort(folders,obc);
      }
    }
  }
  if ((start == QueryUtil.ALL_POS) && (end == QueryUtil.ALL_POS)) {
    return folders;
  }
 else {
    return ListUtil.subList(folders,start,end);
  }
}
