{
  if (object instanceof org.apache.chemistry.opencmis.client.api.Folder) {
    return new CMISFolder(this,getRepositoryId(),toEntryId(object.getId()),(org.apache.chemistry.opencmis.client.api.Folder)object);
  }
 else {
    throw new NoSuchFolderException("CMIS object is not a folder " + object);
  }
}
