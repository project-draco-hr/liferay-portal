{
  if (cmisObject instanceof org.apache.chemistry.opencmis.client.api.Folder) {
    return new CMISFolder(this,getRepositoryId(),toRepositoryEntryId(cmisObject.getId()),(org.apache.chemistry.opencmis.client.api.Folder)cmisObject);
  }
 else {
    throw new NoSuchFolderException("CMIS object is not a folder " + cmisObject);
  }
}
