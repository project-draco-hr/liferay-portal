{
  Session session=getSession();
  org.apache.chemistry.opencmis.client.api.Folder cmisFolder=getCmisFolder(session,folderId);
  OperationContext operationContext=getOperationContext(fileEntriesFirst);
  ItemIterable<CmisObject> cmisObjects=cmisFolder.getChildren(operationContext);
  return cmisObjects.iterator();
}
