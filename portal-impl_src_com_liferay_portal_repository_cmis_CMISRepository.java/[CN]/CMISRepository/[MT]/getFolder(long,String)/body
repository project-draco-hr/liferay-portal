{
  try {
    Session session=getSession();
    String objectId=getObjectId(session,parentFolderId,false,title);
    if (objectId != null) {
      CmisObject cmisObject=session.getObject(objectId);
      return toFolder((org.apache.chemistry.opencmis.client.api.Folder)cmisObject);
    }
  }
 catch (  CmisObjectNotFoundException confe) {
    throw new NoSuchFolderException("No CMIS folder with {parentFolderId=" + parentFolderId + ", title="+ title+ "}",confe);
  }
catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    processException(e);
    throw new RepositoryException(e);
  }
  throw new NoSuchFolderException("No CMIS folder with {parentFolderId=" + parentFolderId + ", title="+ title+ "}");
}
