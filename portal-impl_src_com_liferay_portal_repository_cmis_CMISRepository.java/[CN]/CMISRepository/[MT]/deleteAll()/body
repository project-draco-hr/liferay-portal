{
  try {
    Session session=getSession();
    org.apache.chemistry.opencmis.client.api.Folder rootCMISFolder=session.getRootFolder();
    rootCMISFolder.deleteTree(true,UnfileObject.DELETE,true);
  }
 catch (  Exception e) {
    UnicodeProperties typeSettingsProperties=getTypeSettingsProperties();
    String repositoryId=typeSettingsProperties.getProperty(REPOSITORY_ID);
    _log.error("Unexpected error deleting CMIS repository " + repositoryId,e);
  }
}
