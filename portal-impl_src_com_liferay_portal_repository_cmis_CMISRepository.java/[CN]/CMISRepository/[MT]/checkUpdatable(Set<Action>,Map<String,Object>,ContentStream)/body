{
  if (properties != null) {
    if (!allowableActionsSet.contains(Action.CAN_UPDATE_PROPERTIES)) {
      throw new PrincipalException();
    }
  }
  if (contentStream != null) {
    if (!allowableActionsSet.contains(Action.CAN_SET_CONTENT_STREAM)) {
      throw new PrincipalException();
    }
  }
}
