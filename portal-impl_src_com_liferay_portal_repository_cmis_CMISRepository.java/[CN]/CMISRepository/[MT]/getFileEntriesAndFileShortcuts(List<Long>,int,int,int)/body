{
  List<Object> fileEntriesAndFileShortcuts=new ArrayList<Object>();
  for (  long folderId : folderIds) {
    List<FileEntry> fileEntries=getFileEntries(folderId,start,end,null);
    fileEntriesAndFileShortcuts.addAll(fileEntries);
  }
  fileEntriesAndFileShortcuts.addAll(dlAppHelperLocalService.getFileShortcuts(getGroupId(),folderIds,status));
  return fileEntriesAndFileShortcuts;
}
