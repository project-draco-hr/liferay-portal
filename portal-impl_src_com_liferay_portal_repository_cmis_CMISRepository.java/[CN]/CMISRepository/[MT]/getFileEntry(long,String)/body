{
  try {
    Session session=getSession();
    String objectId=getId(session,folderId,true,title);
    if (objectId != null) {
      CmisObject cmisObject=session.getObject(new ObjectIdImpl(objectId));
      Document document=(Document)cmisObject;
      return toFileEntry(document);
    }
  }
 catch (  PortalException pe) {
    throw pe;
  }
catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    processException(e);
    throw new RepositoryException(e);
  }
  throw new NoSuchFileEntryException("No CMIS file entry with {folderId=" + folderId + ", title="+ title+ "}");
}
