{
  try {
    Session session=getSession();
    ObjectId versionSeriesId=toFileEntryId(fileEntryId);
    Document document=(Document)session.getObject(versionSeriesId);
    document.refresh();
    document.checkOut();
    document=(Document)session.getObject(versionSeriesId);
    document.refresh();
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
