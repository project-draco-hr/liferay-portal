{
  try {
    Session session=getSession();
    ObjectId objectId=new ObjectIdImpl(uuid);
    return toFileEntry(session.getObject(objectId));
  }
 catch (  PortalException pe) {
    throw pe;
  }
catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new RepositoryException(e);
  }
}
