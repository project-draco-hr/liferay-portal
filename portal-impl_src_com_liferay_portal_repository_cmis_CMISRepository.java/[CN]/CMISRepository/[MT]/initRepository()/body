{
  try {
    Session session=getSession();
    session.getRepositoryInfo();
  }
 catch (  PortalException pe) {
    throw pe;
  }
catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    processException(e);
    throw new RepositoryException("Unable to initialize CMIS session for repository " + getRepositoryId(),e);
  }
}
