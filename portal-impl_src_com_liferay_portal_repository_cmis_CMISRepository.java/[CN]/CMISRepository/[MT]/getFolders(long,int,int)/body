{
  try {
    List<Folder> folders=new ArrayList<Folder>();
    Iterator<CmisObject> itr=getIterator(parentFolderId,false,start,end,null);
    boolean foldersSection=false;
    while (itr.hasNext()) {
      CmisObject cmisObject=itr.next();
      if (cmisObject instanceof org.apache.chemistry.opencmis.client.api.Folder) {
        Folder folder=toFolder(cmisObject);
        folders.add(folder);
        foldersSection=true;
      }
 else       if (foldersSection) {
        break;
      }
    }
    return folders;
  }
 catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new RepositoryException(e);
  }
}
