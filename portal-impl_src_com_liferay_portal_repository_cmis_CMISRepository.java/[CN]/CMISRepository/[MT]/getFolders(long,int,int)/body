{
  try {
    Session session=getSession();
    List<String> ids=getIds(session,parentFolderId,false,start,end,null);
    List<Folder> folders=new ArrayList<Folder>(ids.size());
    for (    String id : ids) {
      org.apache.chemistry.opencmis.client.api.Folder folder=(org.apache.chemistry.opencmis.client.api.Folder)session.getObject(new ObjectIdImpl(id));
      folders.add(toFolder(folder));
    }
    return folders;
  }
 catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new RepositoryException(e);
  }
}
