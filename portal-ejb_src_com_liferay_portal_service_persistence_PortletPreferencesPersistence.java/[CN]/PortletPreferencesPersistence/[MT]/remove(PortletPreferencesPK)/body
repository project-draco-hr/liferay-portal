{
  Session session=null;
  try {
    session=openSession();
    PortletPreferences portletPreferences=(PortletPreferences)session.get(PortletPreferencesImpl.class,portletPreferencesPK);
    if (portletPreferences == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No PortletPreferences exists with the primary key " + portletPreferencesPK);
      }
      throw new NoSuchPortletPreferencesException("No PortletPreferences exists with the primary key " + portletPreferencesPK);
    }
    return remove(portletPreferences);
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
