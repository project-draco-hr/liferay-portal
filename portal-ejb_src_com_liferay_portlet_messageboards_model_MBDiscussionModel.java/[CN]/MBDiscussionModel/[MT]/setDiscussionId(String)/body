{
  if (((discussionId == null) && (_discussionId != null)) || ((discussionId != null) && (_discussionId == null)) || ((discussionId != null) && (_discussionId != null) && !discussionId.equals(_discussionId))) {
    if (!XSS_ALLOW_DISCUSSIONID) {
      discussionId=XSSUtil.strip(discussionId);
    }
    _discussionId=discussionId;
  }
}
