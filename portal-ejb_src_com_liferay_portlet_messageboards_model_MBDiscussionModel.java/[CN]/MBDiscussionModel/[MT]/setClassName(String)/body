{
  if (((className == null) && (_className != null)) || ((className != null) && (_className == null)) || ((className != null) && (_className != null) && !className.equals(_className))) {
    if (!XSS_ALLOW_CLASSNAME) {
      className=XSSUtil.strip(className);
    }
    _className=className;
    setModified(true);
  }
}
