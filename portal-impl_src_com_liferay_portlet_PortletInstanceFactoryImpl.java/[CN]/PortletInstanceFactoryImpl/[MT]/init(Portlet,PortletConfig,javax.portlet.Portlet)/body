{
  PortletContext portletContext=portletConfig.getPortletContext();
  InvokerFilterContainer invokerFilterContainer=new InvokerFilterContainerImpl(portlet,portletContext);
  InvokerPortlet invokerPortlet=_invokerPortletFactory.create(portlet,portletInstance,portletContext,invokerFilterContainer);
  invokerPortlet.init(portletConfig);
  return invokerPortlet;
}
