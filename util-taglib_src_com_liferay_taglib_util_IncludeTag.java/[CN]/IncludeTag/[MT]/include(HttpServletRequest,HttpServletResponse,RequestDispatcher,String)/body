{
  ClassLoader classLoader=PortalClassLoaderUtil.getClassLoader();
  Class<?> classObj=classLoader.loadClass(_LIFERAY_REQUEST_DISPATCHER);
  Constructor<?> constructor=classObj.getConstructor(RequestDispatcher.class,String.class);
  Object obj=constructor.newInstance(requestDispatcher,page);
  Method method=classObj.getMethod("include",ServletRequest.class,ServletResponse.class,boolean.class);
  method.invoke(obj,request,response,true);
}
