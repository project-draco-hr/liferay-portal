{
  try {
    include(page);
  }
 catch (  Exception e) {
    String currentURL=(String)request.getAttribute(WebKeys.CURRENT_URL);
    String message="Current URL " + currentURL + " generates exception: "+ e.getMessage();
    LogUtil.log(_log,e,message);
    if (e instanceof JspException) {
      throw (JspException)e;
    }
  }
}
