{
  GoogleDocsMetadataHelper googleDocsMetadataHelper=new GoogleDocsMetadataHelper(dlFileEntryType);
  if (googleDocsMetadataHelper.isGoogleDocs()) {
    return new GoogleDocsDLEditFileEntryDisplayContext(parentDLEditFileEntryDisplayContext,request,response,dlFileEntryType);
  }
  return parentDLEditFileEntryDisplayContext;
}
