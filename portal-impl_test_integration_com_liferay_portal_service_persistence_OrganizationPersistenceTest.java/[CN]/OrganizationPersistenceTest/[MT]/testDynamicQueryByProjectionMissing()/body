{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Organization.class,Organization.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("organizationId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("organizationId",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
