{
  selenium.open("/web/guest/home/");
  loadRequiredJavaScriptModules();
  selenium.clickAt("_145_addApplication",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("layout_configuration_content")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.type("layout_configuration_content",RuntimeVariables.replace(""));
  selenium.typeKeys("layout_configuration_content",RuntimeVariables.replace("a"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@title='Activities']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertTrue(selenium.isVisible("//div[@title='Activities']"));
}
