{
  selenium.select("_26_id","label=Spanish to English");
  selenium.type("_26_text","Mi nombre es traductor de Liferay, fluido adentro sobre 6 millones de formas de comunicaci\u00f3n.");
  selenium.click("//input[@value='Translate']");
  selenium.waitForPageToLoad("30000");
  verifyTrue(selenium.isTextPresent("My name is translator of Liferay, fluid inside on 6 million communication forms."));
}
