{
  if (status == ANY) {
    return LABEL_ANY;
  }
 else   if (status == APPROVED) {
    return LABEL_APPROVED;
  }
 else   if (status == DENIED) {
    return LABEL_DENIED;
  }
 else   if (status == DRAFT) {
    return LABEL_DRAFT;
  }
 else   if (status == EXPIRED) {
    return LABEL_EXPIRED;
  }
 else   if (status == PENDING) {
    return LABEL_PENDING;
  }
 else {
    return LABEL_ANY;
  }
}
