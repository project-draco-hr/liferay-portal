{
  if (status.equals(LABEL_ANY)) {
    return ANY;
  }
 else   if (status.equals(LABEL_APPROVED)) {
    return APPROVED;
  }
 else   if (status.equals(LABEL_DENIED)) {
    return DENIED;
  }
 else   if (status.equals(LABEL_DRAFT)) {
    return DRAFT;
  }
 else   if (status.equals(LABEL_EXPIRED)) {
    return EXPIRED;
  }
 else   if (status.equals(LABEL_PENDING)) {
    return PENDING;
  }
 else {
    return ANY;
  }
}
