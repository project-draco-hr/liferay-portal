{
  DLFileEntry dlFileEntry=DLFileEntryLocalServiceUtil.getDLFileEntry(primaryKey);
  String content="Content: Enterprise. Open Source. For Life.";
  FileEntry fileEntry=DLAppServiceUtil.updateFileEntry(primaryKey,ServiceTestUtil.randomString() + ".txt",ContentTypes.TEXT_PLAIN,dlFileEntry.getTitle(),StringPool.BLANK,StringPool.BLANK,false,content.getBytes(),serviceContext);
  LiferayFileEntry liferayFileEntry=(LiferayFileEntry)fileEntry;
  return liferayFileEntry.getDLFileEntry();
}
