{
  if (!InlineSQLHelperUtil.isEnabled()) {
    return findByUuid_C_PrevAndNext(sacpEntryId,uuid,companyId,orderByComparator);
  }
  SACPEntry sacpEntry=findByPrimaryKey(sacpEntryId);
  Session session=null;
  try {
    session=openSession();
    SACPEntry[] array=new SACPEntryImpl[3];
    array[0]=filterGetByUuid_C_PrevAndNext(session,sacpEntry,uuid,companyId,orderByComparator,true);
    array[1]=sacpEntry;
    array[2]=filterGetByUuid_C_PrevAndNext(session,sacpEntry,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
