{
  IdempotentRetryAssert.retryAssert(3,TimeUnit.SECONDS,new Callable<Void>(){
    @Override public Void call() throws Exception {
      long groupEntriesCount=searchGroupEntriesCount(group.getGroupId(),userId);
      Assert.assertEquals(expected,groupEntriesCount);
      return null;
    }
  }
);
}
