{
  StringMaker sm=new StringMaker();
  if (Validator.isNull(middleName)) {
    sm.append(firstName);
    sm.append(StringPool.SPACE);
    sm.append(lastName);
  }
 else {
    sm.append(firstName);
    sm.append(StringPool.SPACE);
    sm.append(middleName);
    sm.append(StringPool.SPACE);
    sm.append(lastName);
  }
  return sm.toString();
}
