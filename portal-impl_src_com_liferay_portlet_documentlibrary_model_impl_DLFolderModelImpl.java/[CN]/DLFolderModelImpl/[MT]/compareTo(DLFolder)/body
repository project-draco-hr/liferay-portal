{
  int value=0;
  if (getParentFolderId() < dlFolder.getParentFolderId()) {
    value=-1;
  }
 else   if (getParentFolderId() > dlFolder.getParentFolderId()) {
    value=1;
  }
 else {
    value=0;
  }
  if (value != 0) {
    return value;
  }
  value=getName().toLowerCase().compareTo(dlFolder.getName().toLowerCase());
  if (value != 0) {
    return value;
  }
  return 0;
}
