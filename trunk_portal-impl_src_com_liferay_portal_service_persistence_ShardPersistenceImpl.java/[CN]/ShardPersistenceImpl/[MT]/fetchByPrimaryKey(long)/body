{
  Shard shard=(Shard)EntityCacheUtil.getResult(ShardModelImpl.ENTITY_CACHE_ENABLED,ShardImpl.class,shardId,this);
  if (shard == null) {
    Session session=null;
    try {
      session=openSession();
      shard=(Shard)session.get(ShardImpl.class,Long.valueOf(shardId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (shard != null) {
        cacheResult(shard);
      }
      closeSession(session);
    }
  }
  return shard;
}
