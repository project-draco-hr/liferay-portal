{
  EntityCacheUtil.removeResult(ShardModelImpl.ENTITY_CACHE_ENABLED,ShardImpl.class,shard.getPrimaryKey());
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_NAME,new Object[]{shard.getName()});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_C,new Object[]{Long.valueOf(shard.getClassNameId()),Long.valueOf(shard.getClassPK())});
}
