{
  assertEquals("Hello World (2)",StringUtil.appendParentheticalSuffix("Hello World",2));
  assertEquals("Hello (World) (2)",StringUtil.appendParentheticalSuffix("Hello (World)",2));
  assertEquals("Hello World (3)",StringUtil.appendParentheticalSuffix("Hello World (2)",3));
  assertEquals("Hello World (2) (4)",StringUtil.appendParentheticalSuffix("Hello World (2)",4));
}
