{
  Map<String,String> map=new HashMap<>();
  map.put("Hallo","Hello");
  map.put("Wirld","World");
  Assert.assertEquals("Hello World",StringUtil.replace("AB Hallo CD AB Wirld CD","AB "," CD",map));
  Assert.assertEquals("Hello World",StringUtil.replace("Hello World",StringPool.BLANK,StringPool.BLANK,map));
}
