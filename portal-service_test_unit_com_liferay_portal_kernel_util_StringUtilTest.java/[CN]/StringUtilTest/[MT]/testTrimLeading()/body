{
  Assert.assertNull(StringUtil.trimLeading(null));
  Assert.assertSame(StringPool.BLANK,StringUtil.trimLeading(StringPool.BLANK));
  String spacesString=" \t\r\n";
  Assert.assertSame(StringPool.BLANK,StringUtil.trimLeading(spacesString));
  String testString="a";
  Assert.assertSame(testString,StringUtil.trimLeading(testString));
  testString="ab";
  Assert.assertSame(testString,StringUtil.trimLeading(testString));
  String leadingSpacesString=" \t\r\n" + testString;
  Assert.assertEquals(testString,StringUtil.trimLeading(leadingSpacesString));
  String trailingSpacesString=testString + " \t\r\n";
  Assert.assertSame(trailingSpacesString,StringUtil.trimLeading(trailingSpacesString));
  String surroundingSpacesString=" \t\r\n" + testString + " \t\r\n";
  Assert.assertEquals(testString + " \t\r\n",StringUtil.trimLeading(surroundingSpacesString));
}
