{
  Assert.assertNull(StringUtil.trimTrailing(null));
  Assert.assertSame(StringPool.BLANK,StringUtil.trimTrailing(StringPool.BLANK));
  String spacesString=" \t\r\n";
  Assert.assertSame(StringPool.BLANK,StringUtil.trimTrailing(spacesString));
  String testString="a";
  Assert.assertSame(testString,StringUtil.trimTrailing(testString));
  testString="ab";
  Assert.assertSame(testString,StringUtil.trimTrailing(testString));
  String leadingSpacesString=" \t\r\n" + testString;
  Assert.assertSame(leadingSpacesString,StringUtil.trimTrailing(leadingSpacesString));
  String trailingSpacesString=testString + " \t\r\n";
  Assert.assertEquals(testString,StringUtil.trimTrailing(trailingSpacesString));
  String surroundingSpacesString=" \t\r\n" + testString + " \t\r\n";
  Assert.assertEquals(" \t\r\n" + testString,StringUtil.trimTrailing(surroundingSpacesString));
}
