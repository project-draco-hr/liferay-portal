{
  Assert.assertNull(StringUtil.trimTrailing(null,null));
  Assert.assertSame(StringPool.BLANK,StringUtil.trimTrailing(" ",null));
  Assert.assertSame(StringPool.BLANK,StringUtil.trimTrailing(" ",new char[0]));
  char[] exceptions={'\t','\r'};
  Assert.assertSame(StringPool.BLANK,StringUtil.trimTrailing(StringPool.BLANK,exceptions));
  String spacesString=" \t\r\n";
  Assert.assertEquals(" \t\r",StringUtil.trimTrailing(spacesString,exceptions));
  String testString="\t";
  Assert.assertSame(testString,StringUtil.trimTrailing(testString,exceptions));
  testString="\t\r";
  Assert.assertSame(testString,StringUtil.trimTrailing(testString,exceptions));
  Assert.assertSame(StringPool.BLANK,StringUtil.trimTrailing(" \n",exceptions));
  String leadingSpacesString=" \t\r\n" + testString;
  Assert.assertSame(leadingSpacesString,StringUtil.trimTrailing(leadingSpacesString,exceptions));
  String trailingSpacesString=testString + " \t\r\n";
  Assert.assertEquals(testString + " \t\r",StringUtil.trimTrailing(trailingSpacesString,exceptions));
  String surroundingSpacesString=" \t\r\n" + testString + " \t\r\n";
  Assert.assertEquals(" \t\r\n" + testString + " \t\r",StringUtil.trimTrailing(surroundingSpacesString,exceptions));
}
