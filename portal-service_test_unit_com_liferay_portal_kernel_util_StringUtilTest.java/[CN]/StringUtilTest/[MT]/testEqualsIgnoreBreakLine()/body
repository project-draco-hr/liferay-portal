{
  Assert.assertTrue(StringUtil.equalsIgnoreBreakLine("Hello\n World","Hello World"));
  Assert.assertTrue(StringUtil.equalsIgnoreBreakLine("Hello\r\n World","Hello World"));
  Assert.assertTrue(StringUtil.equalsIgnoreBreakLine("\nHello World","Hello World"));
  Assert.assertTrue(StringUtil.equalsIgnoreBreakLine("Hello World\n","Hello World"));
  Assert.assertFalse(StringUtil.equalsIgnoreBreakLine("Hello World",""));
  Assert.assertFalse(StringUtil.equalsIgnoreBreakLine("Hello World\n",null));
}
