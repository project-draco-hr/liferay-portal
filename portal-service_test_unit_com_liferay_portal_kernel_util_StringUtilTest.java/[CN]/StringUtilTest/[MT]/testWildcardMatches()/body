{
  String s="abc";
  String wildcard="abc";
  Assert.assertTrue(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  s="aBc";
  wildcard="abc";
  Assert.assertTrue(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false));
  s="abc";
  wildcard="%c";
  Assert.assertTrue(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  s="abc";
  wildcard="__c";
  Assert.assertTrue(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  s="abc";
  wildcard="ab";
  Assert.assertFalse(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  s="abc";
  wildcard="a_Z";
  Assert.assertFalse(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  s="abc";
  wildcard="a%";
  Assert.assertTrue(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  s="abcd";
  wildcard="a%__d";
  Assert.assertTrue(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  s="abc";
  wildcard="%c";
  Assert.assertTrue(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  s="abc";
  wildcard="abc%";
  Assert.assertTrue(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  s="abc";
  wildcard="abc%z";
  Assert.assertFalse(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  s="a_b%c";
  wildcard="a\\_b\\%c";
  Assert.assertTrue(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  s=new String(new char[]{(char)0,'_','a',(char)2,'%','c','d','e'});
  wildcard=new String(new char[]{(char)0,'\\','_','_',(char)2,'\\','%','c','%'});
  Assert.assertTrue(s,StringUtil.wildcardMatches(s,wildcard,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
}
