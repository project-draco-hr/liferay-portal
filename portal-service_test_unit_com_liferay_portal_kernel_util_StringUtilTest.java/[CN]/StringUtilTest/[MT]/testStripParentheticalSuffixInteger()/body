{
  Assert.assertEquals("Hello World",StringUtil.stripParentheticalSuffix("Hello World (2)"));
  Assert.assertEquals("Hello World(2)",StringUtil.stripParentheticalSuffix("Hello World(2)"));
  Assert.assertEquals("Hello (World)",StringUtil.stripParentheticalSuffix("Hello (World) (2)"));
  Assert.assertEquals("Hello World (2)",StringUtil.stripParentheticalSuffix("Hello World (2) (3)"));
}
