{
  Assert.assertEquals("Hello (World)",StringUtil.appendParentheticalSuffix("Hello","World"));
  Assert.assertEquals("Hello (World) (Liferay)",StringUtil.appendParentheticalSuffix("Hello (World)","Liferay"));
  Assert.assertEquals("Hello (World) (Liferay Portal)",StringUtil.appendParentheticalSuffix("Hello (World)","Liferay Portal"));
}
