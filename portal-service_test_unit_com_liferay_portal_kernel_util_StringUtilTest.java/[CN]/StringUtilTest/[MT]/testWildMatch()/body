{
  String wild="abc";
  String s="abc";
  Assert.assertTrue(s,StringUtil.wildMatch(wild,s,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false));
  wild="abc";
  s="aBc";
  Assert.assertTrue(s,StringUtil.wildMatch(wild,s,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,true));
  wild="%c";
  s="abc";
  Assert.assertTrue(s,StringUtil.wildMatch(wild,s,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false));
  wild="__c";
  s="abc";
  Assert.assertTrue(s,StringUtil.wildMatch(wild,s,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false));
  wild="a_Z";
  s="abc";
  Assert.assertFalse(s,StringUtil.wildMatch(wild,s,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false));
  wild="a%";
  s="abc";
  Assert.assertTrue(s,StringUtil.wildMatch(wild,s,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false));
  wild="a%__d";
  s="abcd";
  Assert.assertTrue(s,StringUtil.wildMatch(wild,s,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false));
  wild="abc%";
  s="abc";
  Assert.assertTrue(s,StringUtil.wildMatch(wild,s,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false));
  wild="abc%z";
  s="abc";
  Assert.assertFalse(s,StringUtil.wildMatch(wild,s,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false));
  wild="a\\_b\\%c";
  s="a_b%c";
  Assert.assertTrue(s,StringUtil.wildMatch(wild,s,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false));
  wild=new String(new char[]{(char)0,'\\','_','_',(char)2,'\\','%','c','%'});
  s=new String(new char[]{(char)0,'_','a',(char)2,'%','c','d','e'});
  Assert.assertTrue(s,StringUtil.wildMatch(wild,s,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false));
}
