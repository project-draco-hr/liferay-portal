{
  int pos=-1;
  while (true) {
    pos=parameters.indexOf(CharPool.SPACE,pos + 1);
    if (pos == -1) {
      return parameters;
    }
    String annotation=parameters.substring(0,pos);
    if ((_javaSourceProcessor.getLevel(annotation) == 0) && (_javaSourceProcessor.getLevel(annotation,"<",">") == 0)) {
      return parameters.substring(pos + 1);
    }
  }
}
