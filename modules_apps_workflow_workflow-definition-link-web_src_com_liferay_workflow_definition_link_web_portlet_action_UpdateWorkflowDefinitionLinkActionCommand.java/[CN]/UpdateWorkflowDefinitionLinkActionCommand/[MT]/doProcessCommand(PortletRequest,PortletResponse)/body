{
  long groupId=ParamUtil.getLong(portletRequest,"groupId");
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  Enumeration<String> enu=portletRequest.getParameterNames();
  while (enu.hasMoreElements()) {
    String name=enu.nextElement();
    if (!name.startsWith(_PREFIX)) {
      continue;
    }
    String className=name.substring(_PREFIX.length());
    String workflowDefinition=ParamUtil.getString(portletRequest,name);
    WorkflowDefinitionLinkLocalServiceUtil.updateWorkflowDefinitionLink(themeDisplay.getUserId(),themeDisplay.getCompanyId(),groupId,className,0,0,workflowDefinition);
  }
}
