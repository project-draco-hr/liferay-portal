{
  try {
    updateWorkflowDefinitionLinks(portletRequest);
  }
 catch (  Exception e) {
    SessionErrors.add(portletRequest,e.getClass());
    PortletSession portletSession=portletRequest.getPortletSession();
    PortletContext portletContext=portletSession.getPortletContext();
    PortletRequestDispatcher portletRequestDispatcher=portletContext.getRequestDispatcher(portletResponse.encodeURL("/error.jsp"));
    portletRequestDispatcher.include(portletRequest,portletResponse);
  }
}
