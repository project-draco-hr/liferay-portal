{
  StringBundler sb=new StringBundler();
  sb.append(getField());
  sb.append(':');
  sb.append(includeLower() ? '[' : '{');
  sb.append(getLowerTerm() != null ? getLowerTerm() : '*');
  sb.append(" TO ");
  sb.append(getUpperTerm() != null ? getUpperTerm() : '*');
  sb.append(includeUpper() ? ']' : '}');
  return sb.toString();
}
