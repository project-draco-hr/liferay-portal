{
  if (_showSelectArticleIcon != null) {
    return _showSelectArticleIcon;
  }
  ThemeDisplay themeDisplay=(ThemeDisplay)_request.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  _showSelectArticleIcon=PortletPermissionUtil.contains(themeDisplay.getPermissionChecker(),themeDisplay.getLayout(),portletDisplay.getId(),ActionKeys.CONFIGURATION);
  return _showSelectArticleIcon;
}
