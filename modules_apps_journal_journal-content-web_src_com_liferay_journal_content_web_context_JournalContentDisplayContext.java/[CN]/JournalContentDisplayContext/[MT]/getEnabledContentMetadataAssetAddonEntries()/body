{
  List<ContentMetadataAssetAddonEntry> contentMetadataAssetAddonEntries=ListUtil.filter(ContentMetadataAssetAddonEntryTracker.getContentMetadataAssetAddonEntries(),new PredicateFilter<ContentMetadataAssetAddonEntry>(){
    @Override public boolean filter(    ContentMetadataAssetAddonEntry contentMetadataEntry){
      return contentMetadataEntry.isEnabled();
    }
  }
);
  return ListUtil.sort(contentMetadataAssetAddonEntries,new PropertyComparator("weight",true,false));
}
