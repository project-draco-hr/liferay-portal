{
  List<UserToolAssetAddonEntry> userToolEntries=ListUtil.filter(UserToolAssetAddonEntryTracker.getUserToolAddonEntries(),new PredicateFilter<UserToolAssetAddonEntry>(){
    @Override public boolean filter(    UserToolAssetAddonEntry userToolEntry){
      return userToolEntry.isEnabled();
    }
  }
);
  return ListUtil.sort(userToolEntries,new PropertyComparator("weight",true,false));
}
