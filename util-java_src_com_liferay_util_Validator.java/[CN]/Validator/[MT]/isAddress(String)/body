{
  if (isNull(address)) {
    return false;
  }
  String[] tokens=address.split(StringPool.AT);
  if (tokens.length != 2) {
    return false;
  }
  for (int i=0; i < tokens.length; i++) {
    char[] c=tokens[i].toCharArray();
    for (int j=0; j < c.length; j++) {
      if (Character.isWhitespace(c[j])) {
        return false;
      }
    }
  }
  return true;
}
