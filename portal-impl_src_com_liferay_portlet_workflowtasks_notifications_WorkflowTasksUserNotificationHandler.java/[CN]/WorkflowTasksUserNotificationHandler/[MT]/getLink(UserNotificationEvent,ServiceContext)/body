{
  JSONObject payload=JSONFactoryUtil.createJSONObject(userNotificationEvent.getPayload());
  String className=payload.getString("entryClassName");
  WorkflowHandler wh=WorkflowHandlerRegistryUtil.getWorkflowHandler(className);
  if (wh == null) {
    return null;
  }
  return wh.getNotificationLink(payload,serviceContext);
}
