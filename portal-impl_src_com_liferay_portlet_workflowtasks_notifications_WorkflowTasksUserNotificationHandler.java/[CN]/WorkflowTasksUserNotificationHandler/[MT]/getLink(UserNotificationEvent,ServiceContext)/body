{
  JSONObject jsonObject=JSONFactoryUtil.createJSONObject(userNotificationEvent.getPayload());
  String entryClassName=jsonObject.getString("entryClassName");
  WorkflowHandler workflowHandler=WorkflowHandlerRegistryUtil.getWorkflowHandler(entryClassName);
  if (workflowHandler == null) {
    return null;
  }
  long workflowTaskId=jsonObject.getLong("workflowTaskId");
  return workflowHandler.getEditWorkflowTaskURL(workflowTaskId,serviceContext);
}
