{
  Website result=(Website)EntityCacheUtil.getResult(WebsiteModelImpl.ENTITY_CACHE_ENABLED,Website.class,websiteId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      Website website=(Website)session.get(WebsiteImpl.class,new Long(websiteId));
      cacheResult(website);
      return website;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (Website)result;
  }
}
