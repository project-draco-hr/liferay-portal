{
  Website website=(Website)EntityCacheUtil.getResult(WebsiteModelImpl.ENTITY_CACHE_ENABLED,WebsiteImpl.class,websiteId,this);
  if (website == null) {
    Session session=null;
    try {
      session=openSession();
      website=(Website)session.get(WebsiteImpl.class,new Long(websiteId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (website != null) {
        cacheResult(website);
      }
      closeSession(session);
    }
  }
  return website;
}
