{
  Website website=(Website)EntityCacheUtil.getResult(WebsiteModelImpl.ENTITY_CACHE_ENABLED,WebsiteImpl.class,websiteId);
  if (website == _nullWebsite) {
    return null;
  }
  if (website == null) {
    Session session=null;
    try {
      session=openSession();
      website=(Website)session.get(WebsiteImpl.class,Long.valueOf(websiteId));
      if (website != null) {
        cacheResult(website);
      }
 else {
        EntityCacheUtil.putResult(WebsiteModelImpl.ENTITY_CACHE_ENABLED,WebsiteImpl.class,websiteId,_nullWebsite);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(WebsiteModelImpl.ENTITY_CACHE_ENABLED,WebsiteImpl.class,websiteId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return website;
}
