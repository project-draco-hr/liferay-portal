{
  Website website=findByPrimaryKey(websiteId);
  Session session=null;
  try {
    session=openSession();
    Website[] array=new WebsiteImpl[3];
    array[0]=getByCompanyId_PrevAndNext(session,website,companyId,orderByComparator,true);
    array[1]=website;
    array[2]=getByCompanyId_PrevAndNext(session,website,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
