{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(website);
    }
  }
  website=removeImpl(website);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(website);
    }
  }
  return website;
}
