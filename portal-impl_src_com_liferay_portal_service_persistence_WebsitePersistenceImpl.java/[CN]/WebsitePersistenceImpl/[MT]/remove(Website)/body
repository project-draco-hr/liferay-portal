{
  Session session=null;
  try {
    session=openSession();
    website=(Website)session.merge(website);
    session.delete(website);
    session.flush();
    return website;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Website.class.getName());
  }
}
