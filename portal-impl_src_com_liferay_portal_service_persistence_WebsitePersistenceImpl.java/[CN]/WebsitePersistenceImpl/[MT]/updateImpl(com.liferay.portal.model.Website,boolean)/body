{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,website,merge);
    website.setNew(false);
    return website;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Website.class.getName());
  }
}
