{
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(website);
    }
 else {
      if (website.isNew()) {
        session.save(website);
      }
    }
    session.flush();
    website.setNew(false);
    return website;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Website.class.getName());
  }
}
