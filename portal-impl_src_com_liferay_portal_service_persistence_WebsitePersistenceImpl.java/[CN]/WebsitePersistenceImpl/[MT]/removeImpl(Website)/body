{
  website=toUnwrappedModel(website);
  Session session=null;
  try {
    session=openSession();
    if (website.isCachedModel()) {
      website=(Website)session.get(WebsiteImpl.class,website.getPrimaryKeyObj());
    }
    if (website != null) {
      session.delete(website);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (website != null) {
    clearCache(website);
  }
  return website;
}
