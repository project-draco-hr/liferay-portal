{
  setUpDDMFormFieldRenderer();
  setUpDDMFormFieldType();
  when(_ddmFormFieldTypeRegistry.getDDMFormFieldType(Matchers.anyString())).thenReturn(_ddmFormFieldType);
  DDMFormFieldTypeRegistryUtil ddmFormFieldTypeRegistryUtil=new DDMFormFieldTypeRegistryUtil();
  ddmFormFieldTypeRegistryUtil.setDDMFormFieldTypeRegistry(_ddmFormFieldTypeRegistry);
}
