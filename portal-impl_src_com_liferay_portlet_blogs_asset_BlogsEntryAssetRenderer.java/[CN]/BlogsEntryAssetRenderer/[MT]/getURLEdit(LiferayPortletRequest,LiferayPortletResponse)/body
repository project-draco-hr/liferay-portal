{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(PortalUtil.getControlPanelPlid(liferayPortletRequest),PortletKeys.BLOGS,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/blogs/edit_entry");
  portletURL.setParameter("entryId",String.valueOf(_entry.getEntryId()));
  return portletURL;
}
