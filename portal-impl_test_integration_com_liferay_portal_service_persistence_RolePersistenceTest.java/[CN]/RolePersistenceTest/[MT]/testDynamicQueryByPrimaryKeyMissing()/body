{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Role.class,Role.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("roleId",ServiceTestUtil.nextLong()));
  List<Role> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
