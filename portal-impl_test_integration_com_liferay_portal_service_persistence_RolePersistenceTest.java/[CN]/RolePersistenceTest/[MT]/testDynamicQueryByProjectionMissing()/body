{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Role.class,Role.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("roleId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("roleId",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
