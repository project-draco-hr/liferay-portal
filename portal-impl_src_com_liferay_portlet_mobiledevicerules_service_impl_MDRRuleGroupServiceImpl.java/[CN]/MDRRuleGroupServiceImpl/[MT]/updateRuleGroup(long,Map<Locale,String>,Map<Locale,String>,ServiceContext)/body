{
  MDRRuleGroup ruleGroup=mdrRuleGroupLocalService.getMDRRuleGroup(ruleGroupId);
  MDRRuleGroupPermissionUtil.check(getPermissionChecker(),ruleGroup.getGroupId(),ruleGroup.getRuleGroupId(),ActionKeys.UPDATE);
  return mdrRuleGroupLocalService.updateRuleGroup(ruleGroupId,nameMap,descriptionMap,serviceContext);
}
