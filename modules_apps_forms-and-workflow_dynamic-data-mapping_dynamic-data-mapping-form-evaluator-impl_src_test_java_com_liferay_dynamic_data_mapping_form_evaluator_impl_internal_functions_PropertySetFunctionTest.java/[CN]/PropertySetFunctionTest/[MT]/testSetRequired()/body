{
  DDMFormFieldEvaluationResult ddmFormFieldEvaluationResult=new DDMFormFieldEvaluationResult("field",StringUtil.randomString());
  evaluatePropertySetFunction(ddmFormFieldEvaluationResult,"required",false);
  Assert.assertFalse(ddmFormFieldEvaluationResult.isRequired());
  evaluatePropertySetFunction(ddmFormFieldEvaluationResult,"required",true);
  Assert.assertTrue(ddmFormFieldEvaluationResult.isRequired());
}
