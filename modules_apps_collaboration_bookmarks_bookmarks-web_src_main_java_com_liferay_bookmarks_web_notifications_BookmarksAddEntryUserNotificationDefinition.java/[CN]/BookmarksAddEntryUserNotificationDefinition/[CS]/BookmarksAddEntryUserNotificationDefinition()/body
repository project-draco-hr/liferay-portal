{
  super(BookmarksPortletKeys.BOOKMARKS,0,UserNotificationDefinition.NOTIFICATION_TYPE_ADD_ENTRY,"receive-a-notification-when-someone-adds-a-new-bookmark-in-a-" + "folder-you-are-subscribed-to");
  addUserNotificationDeliveryType(new UserNotificationDeliveryType("email",UserNotificationDeliveryConstants.TYPE_EMAIL,true,true));
  addUserNotificationDeliveryType(new UserNotificationDeliveryType("website",UserNotificationDeliveryConstants.TYPE_WEBSITE,true,true));
}
