{
  JarResource jarResource=new JarResource("META-INF/spring/context.dependencies","dependencies/META-INF/spring/context.dependencies");
  Jar jar=analyze(Arrays.asList(_TEST_PACKAGE),jarResource);
  Resource resource=jar.getResource("OSGI-INF/context/context.dependencies");
  String collect=IO.collect(resource.openInputStream());
  Assert.assertEquals("bar.foo.Dependency\njava.lang.String\n",collect);
}
