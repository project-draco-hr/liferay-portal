{
  super(portletRequest,new StructureDisplayTerms(portletRequest),new StructureSearchTerms(portletRequest),DEFAULT_CUR_PARAM,DEFAULT_DELTA,iteratorURL,null,EMPTY_RESULTS_MESSAGE);
  StructureDisplayTerms displayTerms=(StructureDisplayTerms)getDisplayTerms();
  iteratorURL.setParameter(StructureDisplayTerms.DESCRIPTION,displayTerms.getDescription());
  iteratorURL.setParameter(StructureDisplayTerms.NAME,displayTerms.getName());
  iteratorURL.setParameter(StructureDisplayTerms.STORAGE_TYPE,displayTerms.getStorageType());
}
