{
  if (_persistence == null) {
    _persistence=(CalendarBookingPersistence)PortletBeanLocatorUtil.locate(com.liferay.calendar.service.ClpSerializer.getServletContextName(),CalendarBookingPersistence.class.getName());
    ReferenceRegistry.registerReference(CalendarBookingUtil.class,"_persistence");
  }
  return _persistence;
}
