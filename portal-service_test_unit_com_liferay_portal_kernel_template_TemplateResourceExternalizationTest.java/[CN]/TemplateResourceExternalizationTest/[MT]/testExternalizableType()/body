{
  Assert.assertTrue(Externalizable.class.isAssignableFrom(TemplateResource.class));
  Assert.assertTrue(TemplateResource.class.isAssignableFrom(CacheTemplateResource.class));
  Assert.assertTrue(TemplateResource.class.isAssignableFrom(JournalTemplateResource.class));
  Assert.assertTrue(TemplateResource.class.isAssignableFrom(StringTemplateResource.class));
  Assert.assertTrue(TemplateResource.class.isAssignableFrom(URLTemplateResource.class));
}
