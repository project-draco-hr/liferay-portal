{
  if (groupIds.length == 0) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(groupIds.length + 2);
  sb.append(" AND (AssetEntry.groupId = ? ");
  for (int i=1; i < groupIds.length; i++) {
    sb.append(" OR AssetEntry.groupId = ? ");
  }
  sb.append(")");
  return sb.toString();
}
