{
  if (classNameIds.length == 0) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(classNameIds.length + 2);
  sb.append(" AND (classNameId = ?");
  for (int i=1; i < classNameIds.length; i++) {
    sb.append(" OR classNameId = ? ");
  }
  sb.append(") ");
  return sb.toString();
}
