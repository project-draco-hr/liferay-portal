{
  long[] restoreFolderIds=StringUtil.split(ParamUtil.getString(actionRequest,"restoreFolderIds"),0L);
  for (  long restoreFolderId : restoreFolderIds) {
    BookmarksFolderServiceUtil.restoreFolderFromTrash(restoreFolderId);
  }
  long[] restoreEntryIds=StringUtil.split(ParamUtil.getString(actionRequest,"restoreEntryIds"),0L);
  for (  long restoreEntryId : restoreEntryIds) {
    BookmarksEntryServiceUtil.restoreEntryFromTrash(restoreEntryId);
  }
}
