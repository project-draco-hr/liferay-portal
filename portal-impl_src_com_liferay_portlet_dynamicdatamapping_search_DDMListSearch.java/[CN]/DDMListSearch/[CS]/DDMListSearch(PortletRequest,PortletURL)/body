{
  super(portletRequest,new DDMListDisplayTerms(portletRequest),new DDMListSearchTerms(portletRequest),DEFAULT_CUR_PARAM,DEFAULT_DELTA,iteratorURL,headerNames,EMPTY_RESULTS_MESSAGE);
  DDMListDisplayTerms displayTerms=(DDMListDisplayTerms)getDisplayTerms();
  iteratorURL.setParameter(DDMListDisplayTerms.DESCRIPTION,displayTerms.getDescription());
  iteratorURL.setParameter(DDMListDisplayTerms.LIST_KEY,displayTerms.getListKey());
  iteratorURL.setParameter(DDMListDisplayTerms.NAME,String.valueOf(displayTerms.getName()));
}
