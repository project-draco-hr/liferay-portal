  ExpressionTestData expressionTestData=ExpressionTestData.newExpressionTestData("variable01 + variable02 + variable03").usingIntegerVariable("variable01",null,5).usingIntegerVariable("variable02","variable01 + 3",5).usingIntegerVariable("variable03","variable02 + variable01",5).withReturnType(Integer.class).withExpectedResult(5 + (5 + 3) + ((5 + 3) + 5));
