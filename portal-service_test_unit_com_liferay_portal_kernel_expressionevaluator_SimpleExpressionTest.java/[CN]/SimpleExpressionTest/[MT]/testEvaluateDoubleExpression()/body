{
  double variable01Value=5.5;
  double variable02value=variable01Value + 3;
  double variable03Value=variable01Value + variable02value;
  double expectedResult=variable01Value + variable02value + variable03Value;
  ExpressionTestData expressionTestData=ExpressionTestData.newExpressionTestData("variable01 + variable02 + variable03").usingDoubleVariable("variable01",null,variable01Value).usingDoubleVariable("variable02","variable01 + 3",null).usingDoubleVariable("variable03","variable02 + variable01",null).withReturnType(Double.class).withExpectedResult(expectedResult);
  testExpression(expressionTestData);
}
