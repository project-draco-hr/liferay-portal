{
  float variable01Value=5.5F;
  float variable02value=variable01Value + 3;
  float variable03Value=variable01Value + variable02value;
  float expectedResult=variable01Value + variable02value + variable03Value;
  ExpressionTestData expressionTestData=ExpressionTestData.newExpressionTestData("variable01 + variable02 + variable03").usingFloatVariable("variable01",null,variable01Value).usingFloatVariable("variable02","variable01 + 3",null).usingFloatVariable("variable03","variable02 + variable01",null).withReturnType(Float.class).withExpectedResult(expectedResult);
  testExpression(expressionTestData);
}
