{
  List<Organization> organizations=OrganizationLocalServiceUtil.getManageableOrganizations(permissionChecker.getUserId());
  for (  Organization organization : organizations) {
    if (permissionChecker.isCommunityAdmin(organization.getGroup().getGroupId())) {
      return true;
    }
    if (OrganizationPermissionUtil.contains(permissionChecker,organization.getOrganizationId(),ActionKeys.MANAGE_USERS)) {
      return true;
    }
    if (OrganizationPermissionUtil.contains(permissionChecker,organization.getOrganizationId(),ActionKeys.MANAGE_SUBORGANIZATIONS)) {
      return true;
    }
  }
  return false;
}
