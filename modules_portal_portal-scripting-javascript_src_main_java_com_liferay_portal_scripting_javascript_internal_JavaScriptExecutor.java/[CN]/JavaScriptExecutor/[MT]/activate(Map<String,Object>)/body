{
  initScriptingExecutorClassLoader();
  JavaScriptExecutorConfiguration javaScriptExecutorConfiguration=Configurable.createConfigurable(JavaScriptExecutorConfiguration.class,properties);
  String[] forbiddenClassNames=StringUtil.split(javaScriptExecutorConfiguration.forbiddenClassNames(),StringPool.COMMA);
  _forbiddenClassNames=new HashSet<>(Arrays.asList(forbiddenClassNames));
}
