{
  List<WikiPage> list=findByU_N_S(userId,nodeId,status,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(8);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("userId=");
    msg.append(userId);
    msg.append(", nodeId=");
    msg.append(nodeId);
    msg.append(", status=");
    msg.append(status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchPageException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
