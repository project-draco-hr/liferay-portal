{
  List<WikiPage> list=findByN_T_H(nodeId,title,head,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(8);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("nodeId=");
    msg.append(nodeId);
    msg.append(", title=");
    msg.append(title);
    msg.append(", head=");
    msg.append(head);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchPageException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
