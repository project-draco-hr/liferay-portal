{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("delete from QUARTZ_TRIGGERS where TRIGGER_NAME = " + "? and TRIGGER_GROUP = ?");
    ps.setString(1,jobName);
    ps.setString(2,jobGroup);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
