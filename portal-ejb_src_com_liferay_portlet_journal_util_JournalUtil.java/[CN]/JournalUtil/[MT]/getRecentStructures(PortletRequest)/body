{
  PortletSession ses=req.getPortletSession();
  Stack recentStructures=(Stack)ses.getAttribute(WebKeys.JOURNAL_RECENT_STRUCTURES);
  if (recentStructures == null) {
    recentStructures=new FiniteUniqueStack(MAX_STACK_SIZE);
    ses.setAttribute(WebKeys.JOURNAL_RECENT_STRUCTURES,recentStructures);
  }
  return recentStructures;
}
