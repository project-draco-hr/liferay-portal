{
  try {
    SAXReader reader=new SAXReader();
    Document doc=reader.read(new StringReader(content));
    Element root=doc.getRootElement();
    root.setAttributeValue("available-locales",StringUtil.remove(root.attributeValue("available-locales"),"de_DE"));
    removeArticleLocale(root,languageId);
    content=formatXML(doc);
  }
 catch (  Exception e) {
    _log.error(e);
  }
  return content;
}
