{
  String emailArticleReviewEnabled=prefs.getValue("email-article-review-enabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleReviewEnabled)) {
    return GetterUtil.getBoolean(emailArticleReviewEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsUtil.JOURNAL_EMAIL_ARTICLE_REVIEW_ENABLED));
  }
}
