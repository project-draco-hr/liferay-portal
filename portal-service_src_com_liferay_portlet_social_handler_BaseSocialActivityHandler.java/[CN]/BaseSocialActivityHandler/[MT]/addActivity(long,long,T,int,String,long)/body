{
  String className=getClassName(classedModel);
  long primaryKey=getPrimaryKey(classedModel);
  if (type == SocialActivityConstants.TYPE_SUBSCRIBE) {
    if (primaryKey != groupId) {
      getSocialActivityLocalService().addActivity(userId,groupId,className,primaryKey,SocialActivityConstants.TYPE_SUBSCRIBE,extraData,0);
    }
  }
 else {
    getSocialActivityLocalService().addActivity(userId,groupId,className,primaryKey,type,extraData,receiverUserId);
  }
}
