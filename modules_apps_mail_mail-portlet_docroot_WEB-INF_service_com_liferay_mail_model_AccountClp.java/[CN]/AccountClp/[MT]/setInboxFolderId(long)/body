{
  _inboxFolderId=inboxFolderId;
  if (_accountRemoteModel != null) {
    try {
      Class<?> clazz=_accountRemoteModel.getClass();
      Method method=clazz.getMethod("setInboxFolderId",long.class);
      method.invoke(_accountRemoteModel,inboxFolderId);
