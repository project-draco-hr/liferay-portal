{
  _login=login;
  if (_accountRemoteModel != null) {
    try {
      Class<?> clazz=_accountRemoteModel.getClass();
      Method method=clazz.getMethod("setLogin",String.class);
      method.invoke(_accountRemoteModel,login);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
