{
  for (int i=0; i < _DEFAULT_JOB_NUMBER; i++) {
    Trigger trigger=_quartzTriggerFactory.createTrigger(_TEST_JOB_NAME_PREFIX + i,defaultGroupName,null,null,_DEFAULT_INTERVAL,TimeUnit.SECOND);
    addJob(_TEST_JOB_NAME_PREFIX + i,defaultGroupName,storageType,(org.quartz.Trigger)trigger.getWrappedTrigger());
  }
}
