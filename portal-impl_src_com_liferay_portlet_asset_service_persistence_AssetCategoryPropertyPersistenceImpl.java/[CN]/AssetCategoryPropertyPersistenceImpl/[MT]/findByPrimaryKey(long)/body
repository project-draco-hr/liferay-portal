{
  AssetCategoryProperty assetCategoryProperty=fetchByPrimaryKey(categoryPropertyId);
  if (assetCategoryProperty == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + categoryPropertyId);
    }
    throw new NoSuchCategoryPropertyException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + categoryPropertyId);
  }
  return assetCategoryProperty;
}
