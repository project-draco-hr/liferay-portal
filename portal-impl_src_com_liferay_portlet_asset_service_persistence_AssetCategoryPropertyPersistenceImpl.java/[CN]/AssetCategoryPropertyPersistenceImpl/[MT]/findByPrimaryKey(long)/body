{
  AssetCategoryProperty assetCategoryProperty=fetchByPrimaryKey(categoryPropertyId);
  if (assetCategoryProperty == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No AssetCategoryProperty exists with the primary key " + categoryPropertyId);
    }
    throw new NoSuchCategoryPropertyException("No AssetCategoryProperty exists with the primary key " + categoryPropertyId);
  }
  return assetCategoryProperty;
}
