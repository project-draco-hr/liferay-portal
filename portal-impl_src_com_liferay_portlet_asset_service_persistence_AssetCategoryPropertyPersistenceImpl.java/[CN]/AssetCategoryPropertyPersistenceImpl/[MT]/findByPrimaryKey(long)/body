{
  AssetCategoryProperty assetCategoryProperty=fetchByPrimaryKey(propertyId);
  if (assetCategoryProperty == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No AssetCategoryProperty exists with the primary key " + propertyId);
    }
    throw new NoSuchCategoryPropertyException("No AssetCategoryProperty exists with the primary key " + propertyId);
  }
  return assetCategoryProperty;
}
