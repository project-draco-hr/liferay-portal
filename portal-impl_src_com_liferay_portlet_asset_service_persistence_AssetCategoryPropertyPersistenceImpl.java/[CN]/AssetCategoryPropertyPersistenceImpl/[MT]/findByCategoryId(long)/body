{
  Object[] finderArgs=new Object[]{new Long(categoryId)};
  List<AssetCategoryProperty> list=(List<AssetCategoryProperty>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_CATEGORYID,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portlet.asset.model.AssetCategoryProperty WHERE ");
      query.append("categoryId = ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("key_ ASC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(categoryId);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<AssetCategoryProperty>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_CATEGORYID,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
