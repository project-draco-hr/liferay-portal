{
  SetupTestableTomcatTask setupTestableTomcatTask=(SetupTestableTomcatTask)GradleUtil.getTask(project,TestIntegrationPlugin.SETUP_TESTABLE_TOMCAT_TASK_NAME);
  final TomcatAppServer tomcatAppServer=(TomcatAppServer)liferayExtension.getAppServer("tomcat");
  setupTestableTomcatTask.setZipUrl(new Callable<String>(){
    @Override public String call() throws Exception {
      return tomcatAppServer.getZipUrl();
    }
  }
);
}
