{
  FileTree fileTree=formatXMLTask.getSource();
  if (!fileTree.isEmpty()) {
    return;
  }
  BuildWSDLTask buildWSDLTask=(BuildWSDLTask)GradleUtil.getTask(formatXMLTask.getProject(),WSDLBuilderPlugin.BUILD_WSDL_TASK_NAME);
  formatXMLTask.setSource(buildWSDLTask.getInputDir());
  formatXMLTask.include("**/*.wsdl");
}
