{
  PollsVote pollsVote=findByPrimaryKey(voteId);
  Session session=null;
  try {
    session=openSession();
    PollsVote[] array=new PollsVoteImpl[3];
    array[0]=getByQuestionId_PrevAndNext(session,pollsVote,questionId,orderByComparator,true);
    array[1]=pollsVote;
    array[2]=getByQuestionId_PrevAndNext(session,pollsVote,questionId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
