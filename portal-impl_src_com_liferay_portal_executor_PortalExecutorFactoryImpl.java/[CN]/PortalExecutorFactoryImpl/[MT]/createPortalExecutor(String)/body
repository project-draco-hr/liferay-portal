{
  ThreadFactory threadFactory=new NamedThreadFactory(executorName,Thread.NORM_PRIORITY,PACLClassLoaderUtil.getPortalClassLoader());
  return new ThreadPoolExecutor(_corePoolSize,_maxPoolSize,_keepAliveTime,_timeUnit,_allowCoreThreadTimeout,_maxQueueSize,_rejectedExecutionHandler,threadFactory,_threadPoolHandler);
}
