{
  try {
    ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
    UserGroup userGroup=ActionUtil.getUserGroup(portletRequest);
    return UserGroupPermissionUtil.contains(themeDisplay.getPermissionChecker(),userGroup.getUserGroupId(),ActionKeys.UPDATE) && UserGroupPermissionUtil.contains(themeDisplay.getPermissionChecker(),userGroup.getUserGroupId(),ActionKeys.VIEW);
  }
 catch (  Exception e) {
  }
  return false;
}
