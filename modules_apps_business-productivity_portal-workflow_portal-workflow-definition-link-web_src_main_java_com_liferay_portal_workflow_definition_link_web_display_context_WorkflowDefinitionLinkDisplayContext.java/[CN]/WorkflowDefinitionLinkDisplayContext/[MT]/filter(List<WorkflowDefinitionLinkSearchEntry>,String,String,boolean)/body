{
  if (Validator.isNull(resource) && Validator.isNull(workflowDefinitionLabel)) {
    return workflowDefinitionLinkSearchEntries;
  }
  PredicateFilter<WorkflowDefinitionLinkSearchEntry> predicateFilter=createPredicateFilter(resource,workflowDefinitionLabel,andOperator);
  return ListUtil.filter(workflowDefinitionLinkSearchEntries,predicateFilter);
}
