{
  WorkflowDefinitionLink workflowDefinitionLink=getWorkflowDefinitionLink(className);
  if (workflowDefinitionLink == null) {
    return false;
  }
  if (workflowDefinitionLink.getWorkflowDefinitionName().equals(workflowDefinition.getName()) && (workflowDefinitionLink.getWorkflowDefinitionVersion() == workflowDefinition.getVersion())) {
    return true;
  }
  return false;
}
