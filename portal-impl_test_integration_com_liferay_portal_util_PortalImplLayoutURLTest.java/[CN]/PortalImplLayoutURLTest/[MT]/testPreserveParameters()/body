{
  ThemeDisplay themeDisplay=initThemeDisplay(company,group,publicLayout,VIRTUAL_HOSTNAME);
  themeDisplay.setDoAsUserId("impersonated");
  String virtualHostnameFriendlyURL=PortalUtil.getLayoutURL(publicLayout,themeDisplay,true);
  Assert.assertEquals("impersonated",HttpUtil.getParameter(virtualHostnameFriendlyURL,"doAsUserId"));
}
