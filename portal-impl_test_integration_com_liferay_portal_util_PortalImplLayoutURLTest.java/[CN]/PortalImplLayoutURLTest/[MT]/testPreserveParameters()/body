{
  ThemeDisplay themeDisplay=initThemeDisplay(company,group,layout,VIRTUAL_HOSTNAME);
  themeDisplay.setDoAsUserId("impersonated");
  String virtualHostnameFriendlyURL=PortalUtil.getLayoutURL(layout,themeDisplay,true);
  Assert.assertEquals("impersonated",HttpUtil.getParameter(virtualHostnameFriendlyURL,"doAsUserId"));
}
