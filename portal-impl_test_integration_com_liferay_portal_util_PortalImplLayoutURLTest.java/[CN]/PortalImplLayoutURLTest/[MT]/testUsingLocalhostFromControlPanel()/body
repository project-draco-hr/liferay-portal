{
  ThemeDisplay themeDisplay=initThemeDisplay(company,group,controlPanelLayout,_VIRTUAL_HOSTNAME);
  String virtualHostnameFriendlyURL=PortalUtil.getLayoutURL(layout,themeDisplay,false);
  themeDisplay.setServerName(_LOCALHOST);
  String localhostFriendlyURL=PortalUtil.getLayoutURL(layout,themeDisplay,false);
  Assert.assertEquals(localhostFriendlyURL,virtualHostnameFriendlyURL);
}
