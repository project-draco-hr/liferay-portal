{
  ThemeDisplay themeDisplay=initThemeDisplay(company,group,controlPanelLayout,_VIRTUAL_HOSTNAME);
  themeDisplay.setDoAsUserId("impersonated");
  LayoutSet layoutSet=LayoutSetLocalServiceUtil.getLayoutSet(group.getGroupId(),false);
  String layoutSetFriendlyURL=PortalUtil.getLayoutSetFriendlyURL(layoutSet,themeDisplay);
  Assert.assertTrue(layoutSetFriendlyURL.contains("doAsUserId=impersonated"));
}
