{
  ThemeDisplay themeDisplay=initThemeDisplay(company,group,layout,VIRTUAL_HOSTNAME);
  String virtualHostnameFriendlyURL=PortalUtil.getLayoutURL(layout,themeDisplay,true);
  themeDisplay.setServerName(LOCALHOST);
  String localhostFriendlyURL=PortalUtil.getLayoutURL(layout,themeDisplay,true);
  Assert.assertEquals(localhostFriendlyURL,virtualHostnameFriendlyURL);
}
