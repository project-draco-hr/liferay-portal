{
  ThemeDisplay themeDisplay=initThemeDisplay(company,group,layout,VIRTUAL_HOSTNAME);
  String virtualHostnameFriendlyURL=PortalUtil.getLayoutURL(layout,themeDisplay,true);
  themeDisplay=initThemeDisplay(company,group,controlPanelLayout,VIRTUAL_HOSTNAME,LOCALHOST);
  String controlPanelFriendlyURL=PortalUtil.getLayoutURL(layout,themeDisplay,true);
  Assert.assertEquals(virtualHostnameFriendlyURL,controlPanelFriendlyURL);
}
