{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//img[@alt='\u4e2d\u6587 (\u4e2d\u56fd)']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("//img[@alt='\u4e2d\u6587 (\u4e2d\u56fd)']");
  selenium.click("//div[@id='navigation']/ul/li[3]/a/span");
  selenium.typeKeys("new_page",RuntimeVariables.replace("\u8bed\u8a00\u6d4b\u8bd5\u9875"));
  selenium.type("new_page",RuntimeVariables.replace("\u8bed\u8a00\u6d4b\u8bd5\u9875"));
  selenium.click("link=\u50a8\u5b58");
  selenium.click(RuntimeVariables.replace("//img[@alt='English (United States)']"));
