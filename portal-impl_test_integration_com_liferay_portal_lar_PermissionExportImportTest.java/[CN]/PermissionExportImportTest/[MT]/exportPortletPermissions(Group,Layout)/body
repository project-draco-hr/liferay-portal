{
  PortletDataContext portletDataContext=mock(PortletDataContext.class);
  when(portletDataContext.getCompanyId()).thenReturn(TestPropsValues.getCompanyId());
  when(portletDataContext.getGroupId()).thenReturn(exportGroup.getGroupId());
  Element portletElement=SAXReaderUtil.createElement("portlet");
  PermissionExporter permissionExporter=new PermissionExporter();
  permissionExporter.exportPortletPermissions(portletDataContext,new LayoutCache(),_PORTLET_ID,exportLayout,portletElement);
  return portletElement;
}
