{
  List<String> actions=ResourceActionsUtil.getResourceActions(portletId,null);
  Resource resource=ResourceLocalServiceUtil.getResource(TestPropsValues.getCompanyId(),portletId,ResourceConstants.SCOPE_INDIVIDUAL,importResourcePrimKey);
  List<String> currentIndividualActions=new ArrayList<String>();
  ResourcePermissionUtil.populateResourcePermissionActionIds(importGroup.getGroupId(),role,resource,actions,currentIndividualActions,new ArrayList<String>(),new ArrayList<String>(),new ArrayList<String>());
  Assert.assertEquals(actionIds.length,currentIndividualActions.size());
  for (  String action : currentIndividualActions) {
    boolean foundActionId=false;
    for (    String actionId : actionIds) {
      if (action.equals(actionId)) {
        foundActionId=true;
        break;
      }
    }
    if (!foundActionId) {
      Assert.fail("Failed to import permissions.");
    }
  }
}
