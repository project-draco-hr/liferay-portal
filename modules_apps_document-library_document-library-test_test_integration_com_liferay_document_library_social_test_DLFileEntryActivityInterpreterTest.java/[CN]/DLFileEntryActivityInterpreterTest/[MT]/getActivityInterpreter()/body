{
  try {
    Registry registry=RegistryUtil.getRegistry();
    Collection<SocialActivityInterpreter> socialActivityInterpreters=registry.getServices(SocialActivityInterpreter.class,"(javax.portlet.name=" + DLPortletKeys.DOCUMENT_LIBRARY + ")");
    for (    SocialActivityInterpreter socialActivityInterpreter : socialActivityInterpreters) {
      if (ArrayUtil.contains(socialActivityInterpreter.getClassNames(),DLFileEntry.class.getName())) {
        return socialActivityInterpreter;
      }
    }
    throw new IllegalStateException("No activity interpreter found for class " + DLFileEntry.class.getName());
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
