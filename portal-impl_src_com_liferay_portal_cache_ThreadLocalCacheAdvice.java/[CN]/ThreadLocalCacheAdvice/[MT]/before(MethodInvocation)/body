{
  String cacheName=_buildCacheName(methodInvocation);
  ThreadLocalCachable threadLocalCachable=_findThreadLocalCachableAnnotation(methodInvocation,cacheName);
  if (threadLocalCachable == _NULL_THREADLOCALCACHE) {
    return null;
  }
  ThreadLocalCache<?> threadLocalCache=ThreadLocalCacheManager.getThreadLocalCache(threadLocalCachable.scope(),cacheName);
  String cacheKey=_buildCacheKey(methodInvocation.getArguments());
  Object cacheValue=threadLocalCache.get(cacheKey);
  if (cacheValue == _NULL_RESULT) {
    return null;
  }
  return cacheValue;
}
