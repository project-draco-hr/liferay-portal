{
  finderCache.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  finderCache.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  MDRRuleGroup mdrRuleGroup : mdrRuleGroups) {
    entityCache.removeResult(MDRRuleGroupModelImpl.ENTITY_CACHE_ENABLED,MDRRuleGroupImpl.class,mdrRuleGroup.getPrimaryKey());
    clearUniqueFindersCache((MDRRuleGroupModelImpl)mdrRuleGroup);
  }
}
