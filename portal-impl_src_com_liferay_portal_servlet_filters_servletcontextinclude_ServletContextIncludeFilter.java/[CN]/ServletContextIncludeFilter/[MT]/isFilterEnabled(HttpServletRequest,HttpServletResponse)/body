{
  try {
    if (request.getAttribute(WebKeys.SERVLET_CONTEXT_INCLUDE_FILTER_THEME) != null) {
      return true;
    }
    Theme theme=getTheme(request);
    if (theme != null) {
      request.setAttribute(WebKeys.SERVLET_CONTEXT_INCLUDE_FILTER_THEME,theme);
      return true;
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return false;
}
