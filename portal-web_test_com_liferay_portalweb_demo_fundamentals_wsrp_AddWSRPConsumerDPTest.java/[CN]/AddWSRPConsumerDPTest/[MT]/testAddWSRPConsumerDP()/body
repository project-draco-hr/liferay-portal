{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=WSRP",RuntimeVariables.replace("WSRP"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Consumers",RuntimeVariables.replace("Consumers"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Consumer']",RuntimeVariables.replace("Add Consumer"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_1_WAR_wsrpportlet_name']",RuntimeVariables.replace("consumer demo"));
  selenium.type("//input[@id='_1_WAR_wsrpportlet_url']",RuntimeVariables.replace(RuntimeVariables.getValue("producerURL")));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals(RuntimeVariables.replace("consumer demo"),selenium.getText("//td[1]/a"));
}
