{
  super(req);
  _strutsAttributes=(Map<String,Object>)req.getAttribute(WebKeys.STRUTS_BRIDGES_ATTRIBUTES);
  if (_strutsAttributes == null) {
    _strutsAttributes=new HashMap<String,Object>();
    req.setAttribute(WebKeys.STRUTS_BRIDGES_ATTRIBUTES,_strutsAttributes);
  }
  UploadServletRequest uploadReq=UploadRequestUtil.getUploadServletRequest(req);
  if (uploadReq != null) {
    _multipartParams=uploadReq.getMultipartParameterMap();
  }
}
