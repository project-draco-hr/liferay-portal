{
  String referringPortletResource=ParamUtil.getString(req,"referringPortletResource");
  if (Validator.isNull(referringPortletResource)) {
    return;
  }
  TagsAsset asset=TagsAssetLocalServiceUtil.getAsset(className,classPK);
  PortletPreferences prefs=PortletPreferencesFactoryUtil.getPortletSetup(req,referringPortletResource,true,true);
  addSelection(className,assetOrder,asset.getAssetId(),prefs);
  prefs.store();
}
