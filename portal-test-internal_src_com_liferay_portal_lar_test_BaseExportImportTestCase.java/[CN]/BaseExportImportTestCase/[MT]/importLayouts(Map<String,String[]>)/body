{
  User user=TestPropsValues.getUser();
  Map<String,Serializable> importLayoutSettingsMap=ExportImportConfigurationSettingsMapFactory.buildImportLayoutSettingsMap(user,importedGroup.getGroupId(),false,null,parameterMap);
  ExportImportConfiguration importConfiguration=ExportImportConfigurationLocalServiceUtil.addExportImportConfiguration(user.getUserId(),importedGroup.getGroupId(),StringPool.BLANK,StringPool.BLANK,ExportImportConfigurationConstants.TYPE_IMPORT_LAYOUT,importLayoutSettingsMap,WorkflowConstants.STATUS_DRAFT,new ServiceContext());
  ExportImportServiceUtil.importLayouts(importConfiguration,larFile);
}
