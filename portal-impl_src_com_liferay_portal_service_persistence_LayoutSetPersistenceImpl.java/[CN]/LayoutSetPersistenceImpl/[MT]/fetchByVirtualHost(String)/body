{
  String finderClassName=LayoutSet.class.getName();
  String finderMethodName="fetchByVirtualHost";
  Object[] finderArgs=new Object[]{virtualHost};
  Object result=FinderCache.getResult(finderClassName,finderMethodName,finderArgs);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringMaker query=new StringMaker();
      query.append("FROM com.liferay.portal.model.LayoutSet WHERE ");
      if (virtualHost == null) {
        query.append("virtualHost IS NULL");
      }
 else {
        query.append("virtualHost = ?");
      }
      query.append(" ");
      Query q=session.createQuery(query.toString());
      int queryPos=0;
      if (virtualHost != null) {
        q.setString(queryPos++,virtualHost);
      }
      List list=q.list();
      if (list.size() == 0) {
        return null;
      }
      LayoutSet layoutSet=(LayoutSet)list.get(0);
      FinderCache.putResult(finderClassName,finderMethodName,finderArgs,layoutSet);
      return layoutSet;
    }
 catch (    Exception e) {
      throw HibernateUtil.processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (LayoutSet)result;
  }
}
