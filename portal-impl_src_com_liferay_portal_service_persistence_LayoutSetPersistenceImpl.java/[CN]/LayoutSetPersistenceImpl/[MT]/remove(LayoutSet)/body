{
  Session session=null;
  try {
    session=openSession();
    layoutSet=(LayoutSet)session.merge(layoutSet);
    session.delete(layoutSet);
    session.flush();
    return layoutSet;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(LayoutSet.class.getName());
  }
}
