{
  Session session=null;
  try {
    session=openSession();
    session.delete(layoutSet);
    session.flush();
    return layoutSet;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(LayoutSet.class.getName());
  }
}
