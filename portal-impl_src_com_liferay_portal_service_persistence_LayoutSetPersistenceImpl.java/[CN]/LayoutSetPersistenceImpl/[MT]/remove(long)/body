{
  Session session=null;
  try {
    session=openSession();
    LayoutSet layoutSet=(LayoutSet)session.get(LayoutSetImpl.class,new Long(layoutSetId));
    if (layoutSet == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No LayoutSet exists with the primary key " + layoutSetId);
      }
      throw new NoSuchLayoutSetException("No LayoutSet exists with the primary key " + layoutSetId);
    }
    return remove(layoutSet);
  }
 catch (  NoSuchLayoutSetException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
