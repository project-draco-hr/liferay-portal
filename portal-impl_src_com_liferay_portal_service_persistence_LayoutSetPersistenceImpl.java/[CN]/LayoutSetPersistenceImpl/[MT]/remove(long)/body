{
  Session session=null;
  try {
    session=openSession();
    LayoutSet layoutSet=(LayoutSet)session.get(LayoutSetImpl.class,new Long(layoutSetId));
    if (layoutSet == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + layoutSetId);
      }
      throw new NoSuchLayoutSetException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + layoutSetId);
    }
    return layoutSetPersistence.remove(layoutSet);
  }
 catch (  NoSuchLayoutSetException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
