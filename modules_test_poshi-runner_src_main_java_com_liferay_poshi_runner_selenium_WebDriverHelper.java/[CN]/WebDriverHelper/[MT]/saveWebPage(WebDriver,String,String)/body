{
  if (!PropsValues.SAVE_WEB_PAGE) {
    return;
  }
  String css=getCssSource(html);
  html=html.replace("<\\html>","");
  StringBuilder sb=new StringBuilder(4);
  sb.append(html);
  sb.append("<style>");
  sb.append(css);
  sb.append("</style></html>");
  FileUtil.write(fileName,sb.toString());
}
