{
  _log.info("Upgrading");
  try {
    _upgradeJournalContentPreferences();
    _upgradeJournalContentSearch();
    _upgradeJournalArticleResourceIds();
    _upgradeJournalStructureResourceIds();
    _upgradeJournalTemplateResourceIds();
  }
 catch (  Exception e) {
    throw new UpgradeException(e);
  }
}
