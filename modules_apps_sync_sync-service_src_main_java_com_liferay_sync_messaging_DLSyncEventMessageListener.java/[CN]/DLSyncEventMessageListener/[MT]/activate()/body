{
  ActionableDynamicQuery actionableDynamicQuery=_dlSyncEventLocalService.getActionableDynamicQuery();
  actionableDynamicQuery.setPerformActionMethod(new ActionableDynamicQuery.PerformActionMethod<DLSyncEvent>(){
    @Override public void performAction(    DLSyncEvent dlSyncEvent) throws PortalException {
      try {
        processDLSyncEvent(dlSyncEvent.getModifiedTime(),dlSyncEvent.getEvent(),dlSyncEvent.getType(),dlSyncEvent.getTypePK());
        deleteDLSyncEvent(dlSyncEvent.getModifiedTime(),dlSyncEvent.getSyncEventId(),dlSyncEvent.getTypePK());
      }
 catch (      Exception e) {
        if (_log.isDebugEnabled()) {
          _log.debug(e,e);
        }
      }
    }
  }
);
  try {
    actionableDynamicQuery.performActions();
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
