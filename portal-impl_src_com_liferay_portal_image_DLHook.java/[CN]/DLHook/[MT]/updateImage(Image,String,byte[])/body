{
  String fileName=getFileName(image.getImageId(),image.getType());
  InputStream is=new UnsyncByteArrayInputStream(bytes);
  if (DLStoreUtil.hasFile(_COMPANY_ID,_REPOSITORY_ID,fileName,_VERSION_NUMBER)) {
    DLStoreUtil.deleteFile(_COMPANY_ID,_REPOSITORY_ID,fileName);
  }
  DLStoreUtil.addFile(_COMPANY_ID,_REPOSITORY_ID,fileName,true,new ServiceContext(),is);
}
