{
  String fileName=getFileName(image.getImageId(),image.getType());
  Date now=new Date();
  InputStream is=new ByteArrayInputStream(bytes);
  if (DLLocalServiceUtil.hasFile(_COMPANY_ID,_REPOSITORY_ID,fileName,_VERSION_NUMBER)) {
    DLServiceUtil.deleteFile(_COMPANY_ID,_PORTLET_ID,_REPOSITORY_ID,fileName);
  }
  DLLocalServiceUtil.addFile(_COMPANY_ID,_PORTLET_ID,_GROUP_ID,_REPOSITORY_ID,fileName,_FILE_ENTRY_ID,_PROPERTIES,now,_TAGS_CATEGORIES,_TAGS_ENTRIES,is);
}
