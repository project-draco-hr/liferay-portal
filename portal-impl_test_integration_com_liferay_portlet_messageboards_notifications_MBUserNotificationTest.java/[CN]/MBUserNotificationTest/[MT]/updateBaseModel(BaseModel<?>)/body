{
  MBMessage message=(MBMessage)baseModel;
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(message.getGroupId(),TestPropsValues.getUserId());
  MBTestUtil.populateNotificationsServiceContext(serviceContext,Constants.UPDATE);
  message=MBMessageLocalServiceUtil.updateMessage(TestPropsValues.getUserId(),message.getMessageId(),RandomTestUtil.randomString(),serviceContext);
  return message;
}
