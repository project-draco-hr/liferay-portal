{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId(),TestPropsValues.getUserId());
  serviceContext.setCommand(Constants.ADD);
  serviceContext.setLayoutFullURL("http://localhost");
  MBMessage message=MBMessageLocalServiceUtil.addMessage(TestPropsValues.getUserId(),RandomTestUtil.randomString(),group.getGroupId(),_category.getCategoryId(),RandomTestUtil.randomString(),RandomTestUtil.randomString(),serviceContext);
  return message;
}
