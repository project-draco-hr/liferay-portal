{
  if (!ServerDetector.isWebLogic()) {
    ServletContext servletContext=request.getServletContext();
    InvokerFilterHelper invokerFilterHelper=(InvokerFilterHelper)servletContext.getAttribute(InvokerFilterHelper.class.getName());
    FilterConfig filterConfig=getFilterConfig();
    invokerFilterHelper.unregisterFilter(filterConfig.getFilterName());
  }
  if (isWrap(response)) {
    return new WebLogicIncludeServletResponse(response);
  }
  return response;
}
