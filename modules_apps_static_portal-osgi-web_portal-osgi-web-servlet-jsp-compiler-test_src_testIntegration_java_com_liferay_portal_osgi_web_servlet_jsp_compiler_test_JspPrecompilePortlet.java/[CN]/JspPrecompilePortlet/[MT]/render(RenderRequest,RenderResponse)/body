{
  PortletContext portletContext=getPortletContext();
  String jspFileName=renderRequest.getParameter(_JSP_FILE_NAME_PARAMETER_NAME);
  if (jspFileName == null) {
    throw new IllegalArgumentException(_JSP_FILE_NAME_PARAMETER_NAME + " query must not be null");
  }
  PortletRequestDispatcher portletRequestDispatcher=portletContext.getRequestDispatcher(jspFileName);
  portletRequestDispatcher.include(renderRequest,renderResponse);
}
