{
  String jsp=renderRequest.getParameter(_JSP_PARAMETER);
  if (jsp == null) {
    throw new IllegalArgumentException(_JSP_PARAMETER + " query must not be null");
  }
  PortletContext portletContext=getPortletContext();
  PortletRequestDispatcher portletRequestDispatcher=portletContext.getRequestDispatcher(jsp);
  portletRequestDispatcher.include(renderRequest,renderResponse);
}
