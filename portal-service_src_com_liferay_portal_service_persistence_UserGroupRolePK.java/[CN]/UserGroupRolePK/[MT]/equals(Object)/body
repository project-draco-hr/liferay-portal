{
  if (obj == null) {
    return false;
  }
  UserGroupRolePK pk=null;
  try {
    pk=(UserGroupRolePK)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  if ((userId.equals(pk.userId)) && (roleId.equals(pk.roleId)) && (groupId == pk.groupId)) {
    return true;
  }
 else {
    return false;
  }
}
