{
  if (obj == null) {
    return false;
  }
  UserGroupRolePK pk=null;
  try {
    pk=(UserGroupRolePK)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  if ((userId == pk.userId) && (groupId == pk.groupId) && (roleId == pk.roleId)) {
    return true;
  }
 else {
    return false;
  }
}
