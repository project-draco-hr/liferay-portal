{
  PortletURL portletURL=PortletURLFactoryUtil.create(portletRequest,NotificationsPortletKeys.NOTIFICATIONS,themeDisplay.getPlid(),PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/notifications/configuration.jsp");
  try {
    portletURL.setWindowState(LiferayWindowState.POP_UP);
  }
 catch (  WindowStateException wse) {
  }
  return portletURL;
}
