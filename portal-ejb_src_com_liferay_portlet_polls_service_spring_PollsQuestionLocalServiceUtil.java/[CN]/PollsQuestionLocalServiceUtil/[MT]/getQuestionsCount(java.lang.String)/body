{
  try {
    PollsQuestionLocalService pollsQuestionLocalService=PollsQuestionLocalServiceFactory.getService();
    return pollsQuestionLocalService.getQuestionsCount(groupId);
  }
 catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
