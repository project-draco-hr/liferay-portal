{
  ResultRow row=(ResultRow)_renderRequest.getAttribute(WebKeys.SEARCH_CONTAINER_RESULT_ROW);
  WorkflowTask workflowTask=null;
  if (Validator.isNotNull(row)) {
    workflowTask=(WorkflowTask)row.getParameter(WorkflowTaskConstants.WORKFLOW_TASK);
  }
 else {
    workflowTask=(WorkflowTask)_renderRequest.getAttribute(WebKeys.WORKFLOW_TASK);
  }
  return workflowTask;
}
