{
  long[] pooledActorsIds=getPooledActorsIds(workflowTask);
  if (pooledActorsIds.length == 0) {
    return false;
  }
  if (workflowTask.isCompleted()) {
    return false;
  }
  if ((pooledActorsIds.length == 1) && (pooledActorsIds[0] == _workflowTaskRequestHelper.getUserId())) {
    return false;
  }
  return true;
}
