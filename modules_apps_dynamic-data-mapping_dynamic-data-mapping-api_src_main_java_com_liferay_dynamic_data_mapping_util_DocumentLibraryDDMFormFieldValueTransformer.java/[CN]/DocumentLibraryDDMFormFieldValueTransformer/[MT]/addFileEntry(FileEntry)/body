{
  String originalTempFileName=TempFileEntryUtil.getOriginalTempFileName(tempFileEntry.getFileName());
  String fileName=DLUtil.getUniqueFileName(tempFileEntry.getGroupId(),tempFileEntry.getFolderId(),originalTempFileName);
  return DLAppServiceUtil.addFileEntry(tempFileEntry.getGroupId(),0,fileName,tempFileEntry.getMimeType(),fileName,StringPool.BLANK,StringPool.BLANK,tempFileEntry.getContentStream(),tempFileEntry.getSize(),new ServiceContext());
}
