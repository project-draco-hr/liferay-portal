{
  if (portletDataContext.addPrimaryKey(JournalPortletDataHandler.class,"deleteData")) {
    return portletPreferences;
  }
  journalArticleLocalService.deleteArticles(portletDataContext.getScopeGroupId());
  journalFolderLocalService.deleteFolders(portletDataContext.getGroupId());
  ddmTemplateLocalService.deleteTemplates(portletDataContext.getScopeGroupId(),PortalUtil.getClassNameId(DDMStructure.class));
  ddmStructureLocalService.deleteStructures(portletDataContext.getScopeGroupId(),PortalUtil.getClassNameId(JournalArticle.class));
  return portletPreferences;
}
