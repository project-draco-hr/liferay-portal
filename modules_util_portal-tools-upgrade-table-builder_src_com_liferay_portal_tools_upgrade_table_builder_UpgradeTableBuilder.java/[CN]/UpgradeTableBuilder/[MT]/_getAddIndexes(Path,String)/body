{
  List<String> addIndexes=new ArrayList<>();
  try (BufferedReader bufferedReader=Files.newBufferedReader(indexesFilePath,StandardCharsets.UTF_8)){
    String line=null;
    while ((line=bufferedReader.readLine()) != null) {
      if (line.contains(" on " + tableName + " (") || line.contains(" on " + tableName + "_ (")) {
        String sql=line.trim();
        if (sql.endsWith(";")) {
          sql=sql.substring(0,sql.length() - 1);
        }
        addIndexes.add(sql);
      }
    }
  }
   return addIndexes.toArray(new String[addIndexes.size()]);
}
