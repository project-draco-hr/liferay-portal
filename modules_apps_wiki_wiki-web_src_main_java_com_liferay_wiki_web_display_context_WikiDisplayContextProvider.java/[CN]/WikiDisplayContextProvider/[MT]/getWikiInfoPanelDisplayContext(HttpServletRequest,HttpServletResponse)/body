{
  Collection<WikiDisplayContextFactory> wikiDisplayContextFactories=_wikiDisplayContextFactories.values();
  WikiInfoPanelDisplayContext wikiInfoPanelDisplayContext=new DefaultWikiInfoPanelDisplayContext(request,response);
  for (  WikiDisplayContextFactory wikiDisplayContextFactory : wikiDisplayContextFactories) {
    wikiInfoPanelDisplayContext=wikiDisplayContextFactory.getWikiInfoPanelDisplayContext(wikiInfoPanelDisplayContext,request,response);
  }
  return wikiInfoPanelDisplayContext;
}
