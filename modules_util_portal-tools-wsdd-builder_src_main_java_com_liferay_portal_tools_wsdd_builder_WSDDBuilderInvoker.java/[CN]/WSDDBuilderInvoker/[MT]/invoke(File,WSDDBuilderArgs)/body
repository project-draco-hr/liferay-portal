{
  WSDDBuilder wsddBuilder=new WSDDBuilder();
  wsddBuilder.setClassPath(wsddBuilderArgs.getClassPath());
  wsddBuilder.setFileName(_getAbsolutePath(baseDir,wsddBuilderArgs.getFileName()));
  wsddBuilder.setOutputPath(_getAbsolutePath(baseDir,wsddBuilderArgs.getOutputPath()) + "/");
  wsddBuilder.setServerConfigFileName(_getAbsolutePath(baseDir,wsddBuilderArgs.getServerConfigFileName()));
  wsddBuilder.setServiceNamespace(wsddBuilderArgs.getServiceNamespace());
  wsddBuilder.build();
  return wsddBuilder;
}
