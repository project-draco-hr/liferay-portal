{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select choiceId, questionId from PollsChoice");
    rs=ps.executeQuery();
    while (rs.next()) {
      long choiceId=rs.getLong("choiceId");
      long questionId=rs.getLong("questionId");
      long groupId=getGroupId(questionId);
      if (groupId > 0) {
        runSQL("update PollsChoice set groupId = " + groupId + " where choiceId = "+ choiceId);
      }
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
