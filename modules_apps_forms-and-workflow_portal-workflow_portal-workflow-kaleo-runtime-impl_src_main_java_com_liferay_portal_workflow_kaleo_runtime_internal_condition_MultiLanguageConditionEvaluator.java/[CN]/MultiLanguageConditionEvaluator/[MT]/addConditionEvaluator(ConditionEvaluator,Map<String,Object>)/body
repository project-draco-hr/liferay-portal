{
  String[] scriptingLanguages=getScriptingLanguages(conditionEvaluator,properties);
  for (  String scriptingLanguage : scriptingLanguages) {
    String conditionEvaluatorKey=getConditionEvaluatorKey(scriptingLanguage,ClassUtil.getClassName(conditionEvaluator));
    _conditionEvaluators.put(conditionEvaluatorKey,conditionEvaluator);
  }
}
