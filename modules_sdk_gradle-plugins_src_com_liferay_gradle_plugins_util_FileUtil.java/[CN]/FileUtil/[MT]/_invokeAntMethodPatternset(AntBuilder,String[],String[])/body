{
  Closure<Void> closure=new Closure<Void>(null){
    @SuppressWarnings("unused") public void doCall(){
      if (ArrayUtil.isNotEmpty(excludes)) {
        for (        String exclude : excludes) {
          _invokeAntMethod(antBuilder,"exclude","name",exclude);
        }
      }
      if (ArrayUtil.isNotEmpty(includes)) {
        for (        String include : includes) {
          _invokeAntMethod(antBuilder,"include","name",include);
        }
      }
    }
  }
;
  antBuilder.invokeMethod("patternset",closure);
}
