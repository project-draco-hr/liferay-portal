{
  AssetEntryQuery assetEntryQuery=AssetEntryQueryTestUtil.createAssetEntryQuery(_group.getGroupId(),JournalArticle.class.getName(),null,null,new long[]{},null);
  SearchContext searchContext=SearchContextTestUtil.getSearchContext(_group.getGroupId());
  searchContext.setGroupIds(assetEntryQuery.getGroupIds());
  int initialEntries=0;
  assertCount(initialEntries,assetEntryQuery,searchContext);
  JournalArticle article=JournalTestUtil.addArticle(_group.getGroupId(),RandomTestUtil.randomString(),RandomTestUtil.randomString());
  Assert.assertTrue(article.isIndexable());
  assertCount(initialEntries + 1,assetEntryQuery,searchContext);
}
