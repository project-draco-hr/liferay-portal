{
  List<DataSampleProcessor<DataSample>> dataSampleProcessors=_dataSampleProcessors.get(namespace);
  if (dataSampleProcessors == null) {
    dataSampleProcessors=new ArrayList<DataSampleProcessor<DataSample>>();
    _dataSampleProcessors.put(namespace,dataSampleProcessors);
  }
  dataSampleProcessors.add(dataSampleProcessor);
}
