{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Breadcrumb Test Page");
  selenium.click(RuntimeVariables.replace("link=Breadcrumb Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Liferay"),selenium.getText("//div/ul[@class='breadcrumbs breadcrumbs-vertical lfr-component']/li[1]/span/a"));
  assertEquals(RuntimeVariables.replace("Breadcrumb Test Page"),selenium.getText("//div/ul[@class='breadcrumbs breadcrumbs-vertical lfr-component']/li[2]/span/a"));
}
