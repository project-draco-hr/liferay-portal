{
  BlogsEntryLocalServiceUtil.updateStatus(getUserId(),blogsEntry.getEntryId(),WorkflowConstants.STATUS_APPROVED,getServiceContext(blogsEntry));
  BlogsEntryLocalServiceUtil.updateStatus(getUserId(),blogsEntry.getEntryId(),WorkflowConstants.STATUS_DRAFT,getServiceContext(blogsEntry));
  BlogsEntryLocalServiceUtil.updateStatus(getUserId(),blogsEntry.getEntryId(),WorkflowConstants.STATUS_APPROVED,getServiceContext(blogsEntry));
  Assert.assertTrue(isAssetEntryVisible(blogsEntry.getEntryId()));
  Assert.assertEquals(1,searchBlogsEntriesCount(group.getGroupId()));
  checkSocialActivity(BlogsActivityKeys.UPDATE_ENTRY,1);
}
