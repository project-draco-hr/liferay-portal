{
  Thread.sleep(500 * TestPropsValues.JUNIT_DELAY_FACTOR);
  List<SocialActivity> activities=SocialActivityLocalServiceUtil.getGroupActivities(group.getGroupId(),QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  int count=0;
  for (  SocialActivity activity : activities) {
    if ((activityType == ACTIVITY_KEY_ANY) || (activity.getType() == activityType)) {
      count=count + 1;
    }
  }
  Assert.assertEquals(expectedCount,count);
}
