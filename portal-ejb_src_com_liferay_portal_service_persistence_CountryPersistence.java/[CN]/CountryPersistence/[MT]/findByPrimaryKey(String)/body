{
  Session session=null;
  try {
    session=openSession();
    CountryHBM countryHBM=(CountryHBM)session.get(CountryHBM.class,countryId);
    if (countryHBM == null) {
      _log.warn("No Country exists with the primary key " + countryId.toString());
      throw new NoSuchCountryException("No Country exists with the primary key " + countryId.toString());
    }
    return CountryHBMUtil.model(countryHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
