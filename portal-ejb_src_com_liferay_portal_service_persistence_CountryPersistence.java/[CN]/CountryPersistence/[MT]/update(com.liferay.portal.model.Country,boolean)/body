{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(country);
    }
 else {
      if (country.isNew()) {
        session.save(country);
      }
    }
    session.flush();
    country.setNew(false);
    return country;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
