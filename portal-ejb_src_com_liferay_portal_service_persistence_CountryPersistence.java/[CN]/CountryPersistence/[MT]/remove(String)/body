{
  Session session=null;
  try {
    session=openSession();
    Country country=(Country)session.get(CountryImpl.class,countryId);
    if (country == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No Country exists with the primary key " + countryId);
      }
      throw new NoSuchCountryException("No Country exists with the primary key " + countryId);
    }
    return remove(country);
  }
 catch (  NoSuchCountryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
