{
  Assert.assertEquals("aaa,bbb,ccc",StringUtil.merge(filterAttributes(_attributes,"some-attribute")));
  Assert.assertEquals("aaa,bbb,ccc",StringUtil.merge(filterAttributes(_attributes,"some-attribute,some-other-attribute,another-attribute")));
  Assert.assertEquals("aaa,bbb,ccc",StringUtil.merge(filterAttributes(_attributes,"attribute-one,attribute-two,nonexisting-attribute")));
}
