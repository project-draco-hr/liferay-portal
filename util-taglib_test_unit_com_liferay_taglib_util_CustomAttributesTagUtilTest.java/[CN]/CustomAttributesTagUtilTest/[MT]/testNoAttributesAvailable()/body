{
  Assert.assertEquals(StringPool.BLANK,StringUtil.merge(filterAttributes(Collections.<String>emptyList(),"some-attribute")));
  Assert.assertEquals(StringPool.BLANK,StringUtil.merge(filterAttributes(Collections.<String>emptyList(),"some-attribute,some-other-attribute,another-attribute")));
  Assert.assertEquals(StringPool.BLANK,StringUtil.merge(filterAttributes(Collections.<String>emptyList(),StringPool.BLANK)));
  Assert.assertEquals(StringPool.BLANK,StringUtil.merge(filterAttributes(Collections.<String>emptyList(),null)));
}
