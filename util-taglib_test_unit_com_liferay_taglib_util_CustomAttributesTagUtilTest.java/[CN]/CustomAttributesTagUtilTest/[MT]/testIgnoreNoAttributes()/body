{
  Assert.assertEquals("aaa,bbb,ccc",StringUtil.merge(filterAttributes(_attributes,StringPool.BLANK)));
  Assert.assertEquals("aaa,bbb,ccc",StringUtil.merge(filterAttributes(_attributes,null)));
  Assert.assertEquals("aaa,bbb,ccc",StringUtil.merge(filterAttributes(_attributes,StringPool.BLANK)));
  Assert.assertEquals("aaa,bbb,ccc",StringUtil.merge(filterAttributes(_attributes,StringPool.SPACE)));
}
