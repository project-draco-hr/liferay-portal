{
  assertEquals("aaa,bbb,ccc",concat(getUnignoredAttributes(attributes.elements(),"")));
  assertEquals("aaa,bbb,ccc",concat(getUnignoredAttributes(attributes.elements(),null)));
  assertEquals("aaa,bbb,ccc",concat(getUnignoredAttributes(attributes.elements(),",")));
  assertEquals("aaa,bbb,ccc",concat(getUnignoredAttributes(attributes.elements()," ")));
}
