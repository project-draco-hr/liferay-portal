{
  long[] userIds=addUsers();
  User user=UserLocalServiceUtil.getUser(userIds[0]);
  UserGroupRoleServiceUtil.addUserGroupRoles(user.getUserId(),_group.getGroupId(),addStandardRoles());
  MembershipPolicyTestUtil.updateUser(user,null,null,null,null,Collections.<UserGroupRole>emptyList());
  Assert.assertTrue(getPropagateRolesMethodFlag());
}
