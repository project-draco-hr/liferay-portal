{
  String s1=(String)obj1;
  String s2=(String)obj2;
  if (Validator.isNull(s1)) {
    s1="";
  }
  if (Validator.isNull(s2)) {
    s2="";
  }
  if (!_asc) {
    String temp=s1;
    s1=s2;
    s2=temp;
  }
  if (_caseSensitive) {
    return s1.compareTo(s2);
  }
 else {
    return s1.compareToIgnoreCase(s2);
  }
}
