{
  try {
    com.liferay.portlet.dynamicdatamapping.model.DDMStructureLink returnValue=DDMStructureLinkServiceUtil.getStructureLink(classNameId,classPK,structureId);
    return com.liferay.portlet.dynamicdatamapping.model.DDMStructureLinkSoap.toSoapModel(returnValue);
