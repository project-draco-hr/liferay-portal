{
  if (!_shouldProceed(destinationName,messageListener)) {
    return;
  }
  MessageBusUtil.registerMessageListener(DestinationNames.SEARCH_READER,_defaultSearchReaderMessageListener);
  MessageBusUtil.registerMessageListener(DestinationNames.SEARCH_WRITER,_defaultSearchWriterMessageListener);
  BaseSearchEngineMessageListener searchEngineMessageListener=(BaseSearchEngineMessageListener)messageListener;
  if (!searchEngineMessageListener.getSearchEngineName().contains(_LUCENE_SEARCH_ENGINE_STRING)) {
    _setBooleanQueryFactory(new com.liferay.portal.search.lucene.BooleanQueryFactoryImpl());
    _setTermQueryFactory(new com.liferay.portal.search.lucene.TermQueryFactoryImpl());
  }
}
