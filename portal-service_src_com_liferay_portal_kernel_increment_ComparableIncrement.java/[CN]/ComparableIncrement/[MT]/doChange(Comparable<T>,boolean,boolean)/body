{
  ComparableIncrement<T> increment=this;
  if (forNew) {
    increment=createComparableIncrement();
  }
  if (delta == null) {
    return increment;
  }
  T value=(T)this.value;
  if ((doIncrease && increasing && (delta.compareTo(value) > 0)) || (!doIncrease && !increasing && (delta.compareTo(value) < 0))) {
    increment.setValue(delta);
  }
  return increment;
}
