{
  String[] valuesArray=StringUtil.split(value);
  if (ArrayUtil.isEmpty(valuesArray)) {
    return false;
  }
  if (!ArrayUtil.contains(valuesArray,LanguageConstants.VALUE_FIRST_NAME) || !ArrayUtil.contains(valuesArray,LanguageConstants.VALUE_LAST_NAME)) {
    return false;
  }
  if (ArrayUtil.contains(valuesArray,LanguageConstants.VALUE_PREFIX) && !valuesArray[0].equals(LanguageConstants.VALUE_PREFIX)) {
    return false;
  }
  int lastIndex=valuesArray.length - 1;
  if (ArrayUtil.contains(valuesArray,LanguageConstants.VALUE_SUFFIX) && !valuesArray[lastIndex].equals(LanguageConstants.VALUE_SUFFIX)) {
    return false;
  }
  for (  String curValue : valuesArray) {
    if (!ArrayUtil.contains(LanguageConstants.VALUES_USER_NAME_FIELD_NAMES,curValue)) {
      return false;
    }
  }
  return true;
}
