{
  DDMDataProviderInstance newDDMDataProviderInstance=addDDMDataProviderInstance();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMDataProviderInstance.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("dataProviderInstanceId"));
  Object newDataProviderInstanceId=newDDMDataProviderInstance.getDataProviderInstanceId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("dataProviderInstanceId",new Object[]{newDataProviderInstanceId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Object existingDataProviderInstanceId=result.get(0);
  Assert.assertEquals(existingDataProviderInstanceId,newDataProviderInstanceId);
}
