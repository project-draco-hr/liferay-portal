{
  Message msg1=(Message)obj1;
  Message msg2=(Message)obj2;
  try {
    if (msg1.getReceivedDate().before(msg2.getReceivedDate())) {
      return 1;
    }
 else     if (msg1.getReceivedDate().after(msg2.getReceivedDate())) {
      return -1;
    }
  }
 catch (  MessagingException me) {
    me.printStackTrace();
  }
  return 0;
}
