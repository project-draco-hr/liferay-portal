{
  if (((repoGroupId == null) && (_repoGroupId != null)) || ((repoGroupId != null) && (_repoGroupId == null)) || ((repoGroupId != null) && (_repoGroupId != null) && !repoGroupId.equals(_repoGroupId))) {
    if (!XSS_ALLOW_REPOGROUPID) {
      repoGroupId=XSSUtil.strip(repoGroupId);
    }
    _repoGroupId=repoGroupId;
  }
}
