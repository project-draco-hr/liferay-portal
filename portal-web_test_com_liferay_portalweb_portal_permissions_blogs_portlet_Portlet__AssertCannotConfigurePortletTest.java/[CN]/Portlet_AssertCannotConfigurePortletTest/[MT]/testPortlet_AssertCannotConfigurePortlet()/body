{
  selenium.open("/web/guest/home/");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Blogs Permissions Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Blogs Permissions Page",RuntimeVariables.replace("Blogs Permissions Page"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertTrue(selenium.isElementNotPresent("//span[@title='Options']/ul/li/strong/a"));
  assertFalse(selenium.isTextPresent("Options"));
  assertTrue(selenium.isElementNotPresent("link=Configuration"));
}
