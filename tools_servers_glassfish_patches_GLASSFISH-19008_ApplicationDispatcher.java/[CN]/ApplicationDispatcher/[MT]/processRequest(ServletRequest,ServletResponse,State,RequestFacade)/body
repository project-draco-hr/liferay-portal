{
  if (request != null) {
    if (state.dispatcherType != DispatcherType.ERROR) {
      state.outerRequest.setAttribute(Globals.DISPATCHER_REQUEST_PATH_ATTR,getCombinedPath());
      invoke(state.outerRequest,response,state,requestFacade);
    }
 else {
      invoke(state.outerRequest,response,state,requestFacade);
    }
  }
}
