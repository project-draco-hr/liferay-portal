{
  org.apache.catalina.Logger logger=context.getLogger();
  if (logger != null) {
    logger.log("ApplicationDispatcher[" + context.getPath() + "] "+ message,t,org.apache.catalina.Logger.WARNING);
  }
 else {
    log.log(Level.WARNING,"ApplicationDispatcher[" + context.getPath() + "]: "+ message,t);
  }
}
