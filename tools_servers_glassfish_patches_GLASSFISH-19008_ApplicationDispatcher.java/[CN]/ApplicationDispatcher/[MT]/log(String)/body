{
  org.apache.catalina.Logger logger=context.getLogger();
  if (logger != null) {
    logger.log("ApplicationDispatcher[" + context.getPath() + "]: "+ message);
  }
 else {
    if (log.isLoggable(Level.INFO)) {
      log.info("ApplicationDispatcher[" + context.getPath() + "]: "+ message);
    }
  }
}
