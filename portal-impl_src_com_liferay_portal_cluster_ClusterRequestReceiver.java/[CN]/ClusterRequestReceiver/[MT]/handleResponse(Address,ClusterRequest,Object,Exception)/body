{
  ClusterNodeResponse clusterNodeResponse=_clusterExecutorImpl.generateClusterNodeResponse(clusterRequest,returnValue,exception);
  Channel controlChannel=_clusterExecutorImpl.getControlChannel();
  try {
    controlChannel.send((org.jgroups.Address)address.getRealAddress(),clusterNodeResponse);
  }
 catch (  Exception e) {
    _log.error("Unable to send response message " + clusterNodeResponse,e);
  }
catch (  Throwable t) {
    _log.error(t,t);
  }
}
