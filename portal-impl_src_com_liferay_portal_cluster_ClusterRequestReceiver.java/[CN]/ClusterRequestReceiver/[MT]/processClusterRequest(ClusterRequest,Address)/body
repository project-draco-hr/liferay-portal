{
  Serializable responsePayload=_clusterExecutorImpl.handleReceivedClusterRequest(clusterRequest,sourceAddress);
  if (responsePayload == null) {
    return;
  }
  ClusterChannel clusterChannel=_clusterExecutorImpl.getClusterChannel();
  try {
    clusterChannel.sendUnicastMessage(responsePayload,sourceAddress);
  }
 catch (  Throwable t) {
    _log.error("Unable to send message " + responsePayload,t);
  }
}
