{
  String className=ClusterRequestReceiver.class.getName();
  _parallelThreadPoolExecutor=PortalExecutorManagerUtil.getPortalExecutor(className + "_parallel");
  _serialThreadPoolExecutor=new ThreadPoolExecutor(1,1,60L,TimeUnit.SECONDS,false,Integer.MAX_VALUE,_parallelThreadPoolExecutor.getRejectedExecutionHandler(),new NamedThreadFactory(className + "_serial",Thread.NORM_PRIORITY,PortalClassLoaderUtil.getClassLoader()),_parallelThreadPoolExecutor.getThreadPoolHandler());
}
