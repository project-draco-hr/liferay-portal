{
  if (_log.isDebugEnabled()) {
    _log.debug("Accepted view " + view);
  }
  if (_lastView == null) {
    _lastView=view;
    return;
  }
  List<Address> departAddresses=getDepartAddresses(view);
  _lastView=view;
  if (departAddresses.isEmpty()) {
    return;
  }
  _clusterExecutorImpl.memberRemoved(departAddresses);
}
