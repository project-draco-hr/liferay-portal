{
  ClusterExecutorImpl clusterExecutorImpl=null;
  try {
    clusterExecutorImpl=getClusterExecutorImpl(false,false);
    Address destAddress=clusterExecutorImpl.getLocalClusterNodeAddress();
    ClusterRequest clusterRequest=ClusterRequest.createUnicastRequest(null,destAddress);
    FutureClusterResponses futureClusterResponses=clusterExecutorImpl.execute(clusterRequest);
    assertFutureClusterResponsesWithException(futureClusterResponses,clusterRequest.getUuid(),destAddress,"Payload is not of type " + MethodHandler.class.getName());
  }
  finally {
    if (clusterExecutorImpl != null) {
      clusterExecutorImpl.destroy();
    }
  }
}
