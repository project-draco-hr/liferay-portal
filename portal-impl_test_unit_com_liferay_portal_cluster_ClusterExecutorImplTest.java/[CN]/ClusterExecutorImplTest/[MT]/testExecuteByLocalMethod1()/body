{
  ClusterExecutorImpl clusterExecutorImpl=null;
  try {
    clusterExecutorImpl=getClusterExecutorImpl(false,false);
    MethodHandler methodHandler=new MethodHandler(testMethod1MethodKey,StringPool.BLANK);
    ClusterNode clusterNode=clusterExecutorImpl.getLocalClusterNode();
    ClusterRequest clusterRequest=ClusterRequest.createUnicastRequest(methodHandler,clusterNode.getClusterNodeId());
    FutureClusterResponses futureClusterResponses=clusterExecutorImpl.execute(clusterRequest);
    Address address=clusterExecutorImpl.getLocalClusterNodeAddress();
    assertFutureClusterResponsesWithoutException(futureClusterResponses.get(),clusterRequest.getUuid(),null,address);
  }
  finally {
    if (clusterExecutorImpl != null) {
      clusterExecutorImpl.destroy();
    }
  }
}
