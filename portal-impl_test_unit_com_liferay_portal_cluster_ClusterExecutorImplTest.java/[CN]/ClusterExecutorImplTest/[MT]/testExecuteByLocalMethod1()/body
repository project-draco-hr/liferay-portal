{
  ClusterExecutorImpl clusterExecutorImpl=null;
  try {
    clusterExecutorImpl=getClusterExecutorImpl(false,false);
    String destClusterId=clusterExecutorImpl.getLocalClusterNode().getClusterNodeId();
    Address destAddress=clusterExecutorImpl.getLocalClusterNodeAddress();
    MethodHandler methodHandler=new MethodHandler(_TEST_METHOD_KEY_1,"");
    ClusterRequest clusterRequest=ClusterRequest.createUnicastRequest(methodHandler,destClusterId);
    FutureClusterResponses futureClusterResponses=clusterExecutorImpl.execute(clusterRequest);
    assertFutureClusterResponsesWithoutException(futureClusterResponses.get(),clusterRequest.getUuid(),null,destAddress);
  }
  finally {
    if (clusterExecutorImpl != null) {
      clusterExecutorImpl.destroy();
    }
  }
}
