{
  ClusterExecutorImpl clusterExecutorImpl=null;
  try {
    clusterExecutorImpl=getClusterExecutorImpl(false,false);
    clusterExecutorImpl.portalServerAddressConfigured(new InetSocketAddress(80));
    clusterExecutorImpl.portalLocalAddressConfigured(new InetSocketAddress(80));
  }
  finally {
    if (clusterExecutorImpl != null) {
      clusterExecutorImpl.destroy();
    }
  }
}
