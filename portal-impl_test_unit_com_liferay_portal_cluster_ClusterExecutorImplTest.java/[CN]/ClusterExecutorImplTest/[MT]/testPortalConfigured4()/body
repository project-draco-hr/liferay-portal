{
  ClusterExecutorImpl clusterExecutorImpl=null;
  try {
    clusterExecutorImpl=getClusterExecutorImpl(false,false);
    ClusterNode clusterNode=clusterExecutorImpl.getLocalClusterNode();
    Assert.assertNull(clusterNode.getPortalInetSocketAddress());
    clusterExecutorImpl.portalLocalInetSockAddressConfigured(new InetSocketAddress(InetAddress.getByName(SetPortalInetSocketAddressAdvice.SECURE_PORTAL_ADDRESS),SetPortalInetSocketAddressAdvice.SECURE_PORTAL_PORT));
    Assert.assertEquals(new InetSocketAddress(InetAddress.getByName(SetPortalInetSocketAddressAdvice.SECURE_PORTAL_ADDRESS),SetPortalInetSocketAddressAdvice.SECURE_PORTAL_PORT),clusterNode.getPortalInetSocketAddress());
    clusterExecutorImpl.portalLocalInetSockAddressConfigured(new InetSocketAddress(InetAddress.getByName(SetPortalInetSocketAddressAdvice.SECURE_PORTAL_ADDRESS),SetPortalInetSocketAddressAdvice.SECURE_PORTAL_PORT));
    Assert.assertEquals(new InetSocketAddress(InetAddress.getByName(SetPortalInetSocketAddressAdvice.SECURE_PORTAL_ADDRESS),SetPortalInetSocketAddressAdvice.SECURE_PORTAL_PORT),clusterNode.getPortalInetSocketAddress());
  }
  finally {
    if (clusterExecutorImpl != null) {
      clusterExecutorImpl.destroy();
    }
  }
}
