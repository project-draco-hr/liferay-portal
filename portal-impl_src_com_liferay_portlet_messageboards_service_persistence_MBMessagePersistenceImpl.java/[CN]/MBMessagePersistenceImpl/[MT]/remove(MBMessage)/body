{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(mbMessage);
    }
  }
  mbMessage=removeImpl(mbMessage);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(mbMessage);
    }
  }
  return mbMessage;
}
