{
  FinderCache.clearCache(MBMessage.class.getName());
  Session session=null;
  try {
    session=openSession();
    session.delete(mbMessage);
    session.flush();
    return mbMessage;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
