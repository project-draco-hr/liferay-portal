{
  MBMessage mbMessage=(MBMessage)EntityCacheUtil.getResult(MBMessageModelImpl.ENTITY_CACHE_ENABLED,MBMessageImpl.class,messageId,this);
  if (mbMessage == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (mbMessage == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      mbMessage=(MBMessage)session.get(MBMessageImpl.class,Long.valueOf(messageId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (mbMessage == null)) {
        EntityCacheUtil.putResult(MBMessageModelImpl.ENTITY_CACHE_ENABLED,MBMessageImpl.class,messageId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(mbMessage);
      }
      closeSession(session);
    }
  }
  return mbMessage;
}
