{
  MBMessage result=(MBMessage)EntityCacheUtil.getResult(MBMessageModelImpl.ENTITY_CACHE_ENABLED,MBMessage.class,messageId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      MBMessage mbMessage=(MBMessage)session.get(MBMessageImpl.class,new Long(messageId));
      cacheResult(mbMessage);
      return mbMessage;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (MBMessage)result;
  }
}
