{
  EntityCacheUtil.removeResult(MBMessageModelImpl.ENTITY_CACHE_ENABLED,MBMessageImpl.class,mbMessage.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{mbMessage.getUuid(),Long.valueOf(mbMessage.getGroupId())});
}
