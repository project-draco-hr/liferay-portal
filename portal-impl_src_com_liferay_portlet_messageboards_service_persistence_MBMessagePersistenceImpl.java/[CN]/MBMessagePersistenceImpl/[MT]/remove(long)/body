{
  Session session=null;
  try {
    session=openSession();
    MBMessage mbMessage=(MBMessage)session.get(MBMessageImpl.class,new Long(messageId));
    if (mbMessage == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + messageId);
      }
      throw new NoSuchMessageException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + messageId);
    }
    return remove(mbMessage);
  }
 catch (  NoSuchMessageException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
