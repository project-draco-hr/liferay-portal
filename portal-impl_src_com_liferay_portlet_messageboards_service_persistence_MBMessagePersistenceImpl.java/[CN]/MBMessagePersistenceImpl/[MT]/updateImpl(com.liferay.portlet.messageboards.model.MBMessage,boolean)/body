{
  mbMessage=toUnwrappedModel(mbMessage);
  boolean isNew=mbMessage.isNew();
  MBMessageModelImpl mbMessageModelImpl=(MBMessageModelImpl)mbMessage;
  if (Validator.isNull(mbMessage.getUuid())) {
    String uuid=PortalUUIDUtil.generate();
    mbMessage.setUuid(uuid);
  }
  long userId=GetterUtil.getLong(PrincipalThreadLocal.getName());
  if (userId > 0) {
    long companyId=mbMessage.getCompanyId();
    long groupId=mbMessage.getGroupId();
    long messageId=0;
    if (!isNew) {
      messageId=mbMessage.getPrimaryKey();
    }
    try {
      mbMessage.setSubject(SanitizerUtil.sanitize(companyId,groupId,userId,com.liferay.portlet.messageboards.model.MBMessage.class.getName(),messageId,ContentTypes.TEXT_PLAIN,Sanitizer.MODE_ALL,mbMessage.getSubject(),null));
    }
 catch (    SanitizerException se) {
      throw new SystemException(se);
    }
  }
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,mbMessage,merge);
    mbMessage.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  if (isNew) {
    FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  }
 else {
    if (!Validator.equals(mbMessage.getUuid(),mbMessageModelImpl.getOriginalUuid())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_UUID,new Object[]{mbMessageModelImpl.getOriginalUuid()});
    }
    if (mbMessage.getGroupId() != mbMessageModelImpl.getOriginalGroupId()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_GROUPID,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalGroupId())});
    }
    if (mbMessage.getCompanyId() != mbMessageModelImpl.getOriginalCompanyId()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_COMPANYID,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalCompanyId())});
    }
    if (mbMessage.getThreadId() != mbMessageModelImpl.getOriginalThreadId()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_THREADID,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalThreadId())});
    }
    if (mbMessage.getThreadId() != mbMessageModelImpl.getOriginalThreadId()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_THREADREPLIES,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalThreadId())});
    }
    if (mbMessage.getUserId() != mbMessageModelImpl.getOriginalUserId()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_USERID,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalUserId())});
    }
    if ((mbMessage.getGroupId() != mbMessageModelImpl.getOriginalGroupId()) || (mbMessage.getUserId() != mbMessageModelImpl.getOriginalUserId())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_G_U,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalGroupId()),Long.valueOf(mbMessageModelImpl.getOriginalUserId())});
    }
    if ((mbMessage.getGroupId() != mbMessageModelImpl.getOriginalGroupId()) || (mbMessage.getCategoryId() != mbMessageModelImpl.getOriginalCategoryId())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_G_C,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalGroupId()),Long.valueOf(mbMessageModelImpl.getOriginalCategoryId())});
    }
    if ((mbMessage.getGroupId() != mbMessageModelImpl.getOriginalGroupId()) || (mbMessage.getStatus() != mbMessageModelImpl.getOriginalStatus())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_G_S,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalGroupId()),Integer.valueOf(mbMessageModelImpl.getOriginalStatus())});
    }
    if ((mbMessage.getCompanyId() != mbMessageModelImpl.getOriginalCompanyId()) || (mbMessage.getStatus() != mbMessageModelImpl.getOriginalStatus())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_C_S,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalCompanyId()),Integer.valueOf(mbMessageModelImpl.getOriginalStatus())});
    }
    if ((mbMessage.getUserId() != mbMessageModelImpl.getOriginalUserId()) || (mbMessage.getClassNameId() != mbMessageModelImpl.getOriginalClassNameId())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_U_C,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalUserId()),Long.valueOf(mbMessageModelImpl.getOriginalClassNameId())});
    }
    if ((mbMessage.getClassNameId() != mbMessageModelImpl.getOriginalClassNameId()) || (mbMessage.getClassPK() != mbMessageModelImpl.getOriginalClassPK())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_C_C,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalClassNameId()),Long.valueOf(mbMessageModelImpl.getOriginalClassPK())});
    }
    if ((mbMessage.getThreadId() != mbMessageModelImpl.getOriginalThreadId()) || (mbMessage.getParentMessageId() != mbMessageModelImpl.getOriginalParentMessageId())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_T_P,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalThreadId()),Long.valueOf(mbMessageModelImpl.getOriginalParentMessageId())});
    }
    if ((mbMessage.getThreadId() != mbMessageModelImpl.getOriginalThreadId()) || (mbMessage.getAnswer() != mbMessageModelImpl.getOriginalAnswer())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_T_A,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalThreadId()),Boolean.valueOf(mbMessageModelImpl.getOriginalAnswer())});
    }
    if ((mbMessage.getThreadId() != mbMessageModelImpl.getOriginalThreadId()) || (mbMessage.getStatus() != mbMessageModelImpl.getOriginalStatus())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_T_S,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalThreadId()),Integer.valueOf(mbMessageModelImpl.getOriginalStatus())});
    }
    if ((mbMessage.getThreadId() != mbMessageModelImpl.getOriginalThreadId()) || (mbMessage.getStatus() != mbMessageModelImpl.getOriginalStatus())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_TR_S,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalThreadId()),Integer.valueOf(mbMessageModelImpl.getOriginalStatus())});
    }
    if ((mbMessage.getGroupId() != mbMessageModelImpl.getOriginalGroupId()) || (mbMessage.getUserId() != mbMessageModelImpl.getOriginalUserId()) || (mbMessage.getStatus() != mbMessageModelImpl.getOriginalStatus())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_G_U_S,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalGroupId()),Long.valueOf(mbMessageModelImpl.getOriginalUserId()),Integer.valueOf(mbMessageModelImpl.getOriginalStatus())});
    }
    if ((mbMessage.getGroupId() != mbMessageModelImpl.getOriginalGroupId()) || (mbMessage.getCategoryId() != mbMessageModelImpl.getOriginalCategoryId()) || (mbMessage.getThreadId() != mbMessageModelImpl.getOriginalThreadId())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_G_C_T,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalGroupId()),Long.valueOf(mbMessageModelImpl.getOriginalCategoryId()),Long.valueOf(mbMessageModelImpl.getOriginalThreadId())});
    }
    if ((mbMessage.getGroupId() != mbMessageModelImpl.getOriginalGroupId()) || (mbMessage.getCategoryId() != mbMessageModelImpl.getOriginalCategoryId()) || (mbMessage.getStatus() != mbMessageModelImpl.getOriginalStatus())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_G_C_S,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalGroupId()),Long.valueOf(mbMessageModelImpl.getOriginalCategoryId()),Integer.valueOf(mbMessageModelImpl.getOriginalStatus())});
    }
    if ((mbMessage.getUserId() != mbMessageModelImpl.getOriginalUserId()) || (mbMessage.getClassNameId() != mbMessageModelImpl.getOriginalClassNameId()) || (mbMessage.getClassPK() != mbMessageModelImpl.getOriginalClassPK())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_U_C_C,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalUserId()),Long.valueOf(mbMessageModelImpl.getOriginalClassNameId()),Long.valueOf(mbMessageModelImpl.getOriginalClassPK())});
    }
    if ((mbMessage.getUserId() != mbMessageModelImpl.getOriginalUserId()) || (mbMessage.getClassNameId() != mbMessageModelImpl.getOriginalClassNameId()) || (mbMessage.getStatus() != mbMessageModelImpl.getOriginalStatus())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_U_C_S,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalUserId()),Long.valueOf(mbMessageModelImpl.getOriginalClassNameId()),Integer.valueOf(mbMessageModelImpl.getOriginalStatus())});
    }
    if ((mbMessage.getClassNameId() != mbMessageModelImpl.getOriginalClassNameId()) || (mbMessage.getClassPK() != mbMessageModelImpl.getOriginalClassPK()) || (mbMessage.getStatus() != mbMessageModelImpl.getOriginalStatus())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_C_C_S,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalClassNameId()),Long.valueOf(mbMessageModelImpl.getOriginalClassPK()),Integer.valueOf(mbMessageModelImpl.getOriginalStatus())});
    }
    if ((mbMessage.getGroupId() != mbMessageModelImpl.getOriginalGroupId()) || (mbMessage.getCategoryId() != mbMessageModelImpl.getOriginalCategoryId()) || (mbMessage.getThreadId() != mbMessageModelImpl.getOriginalThreadId())|| (mbMessage.getStatus() != mbMessageModelImpl.getOriginalStatus())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_G_C_T_S,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalGroupId()),Long.valueOf(mbMessageModelImpl.getOriginalCategoryId()),Long.valueOf(mbMessageModelImpl.getOriginalThreadId()),Integer.valueOf(mbMessageModelImpl.getOriginalStatus())});
    }
    if ((mbMessage.getUserId() != mbMessageModelImpl.getOriginalUserId()) || (mbMessage.getClassNameId() != mbMessageModelImpl.getOriginalClassNameId()) || (mbMessage.getClassPK() != mbMessageModelImpl.getOriginalClassPK())|| (mbMessage.getStatus() != mbMessageModelImpl.getOriginalStatus())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_U_C_C_S,new Object[]{Long.valueOf(mbMessageModelImpl.getOriginalUserId()),Long.valueOf(mbMessageModelImpl.getOriginalClassNameId()),Long.valueOf(mbMessageModelImpl.getOriginalClassPK()),Integer.valueOf(mbMessageModelImpl.getOriginalStatus())});
    }
  }
  EntityCacheUtil.putResult(MBMessageModelImpl.ENTITY_CACHE_ENABLED,MBMessageImpl.class,mbMessage.getPrimaryKey(),mbMessage);
  if (isNew) {
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{mbMessage.getUuid(),Long.valueOf(mbMessage.getGroupId())},mbMessage);
  }
 else {
    if (!Validator.equals(mbMessage.getUuid(),mbMessageModelImpl.getOriginalUuid()) || (mbMessage.getGroupId() != mbMessageModelImpl.getOriginalGroupId())) {
      FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{mbMessageModelImpl.getOriginalUuid(),Long.valueOf(mbMessageModelImpl.getOriginalGroupId())});
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{mbMessage.getUuid(),Long.valueOf(mbMessage.getGroupId())},mbMessage);
    }
  }
  return mbMessage;
}
