{
  List<WikiPage> pages=WikiPageLocalServiceUtil.getPages(pageResource.getNodeId(),pageResource.getTitle(),QueryUtil.ALL_POS,QueryUtil.ALL_POS,new PageVersionComparator(true));
  if (pages.size() <= 1) {
    return;
  }
  WikiPage firstPage=pages.get(0);
  Date createDate=firstPage.getCreateDate();
  for (  WikiPage page : pages) {
    if (!createDate.equals(page.getCreateDate())) {
      page.setCreateDate(createDate);
      WikiPageLocalServiceUtil.updateWikiPage(page);
    }
  }
}
