{
  List<WikiPage> pages=WikiPageLocalServiceUtil.getNoAssetPages();
  if (_log.isDebugEnabled()) {
    _log.debug("Processing " + pages.size() + " pages with no tags assets");
  }
  for (  WikiPage page : pages) {
    try {
      WikiPageLocalServiceUtil.updateTagsAsset(page.getUserId(),page,new String[0]);
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to update tags asset for page " + page.getPageId() + ": "+ e.getMessage());
      }
    }
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Tags assets verified for pages");
  }
}
