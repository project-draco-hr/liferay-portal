{
  WikiNode node=(WikiNode)obj;
  Document document=getDocument(obj);
  if (!node.isInTrash()) {
    SearchEngineUtil.deleteDocument(getSearchEngineId(),node.getCompanyId(),document.get(Field.UID),isCommitImmediately());
    return;
  }
  SearchEngineUtil.updateDocument(getSearchEngineId(),node.getCompanyId(),document,isCommitImmediately());
}
