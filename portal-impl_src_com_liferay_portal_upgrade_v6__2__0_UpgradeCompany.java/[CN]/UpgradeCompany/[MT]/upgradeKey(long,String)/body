{
  Key keyObj=null;
  if (Validator.isNotNull(key)) {
    keyObj=(Key)Base64.stringToObjectSilent(key);
  }
  if (keyObj != null) {
    String algorithm=keyObj.getAlgorithm();
    if (!algorithm.equals("DES")) {
      return;
    }
  }
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("update Company set key_ = ? where companyId = ?");
    ps.setString(1,Base64.objectToString(Encryptor.generateKey()));
    ps.setLong(2,companyId);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
