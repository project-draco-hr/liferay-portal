{
  String keyAlgorithm=Encryptor.KEY_ALGORITHM;
  if (keyAlgorithm.equals("DES")) {
    return;
  }
  try (PreparedStatement ps=connection.prepareStatement("select companyId, key_ from Company");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long companyId=rs.getLong("companyId");
      String key=rs.getString("key_");
      upgradeKey(companyId,key);
    }
  }
 }
