{
  MBCategory category=null;
  try {
    category=getParentCategory();
  }
 catch (  Exception e) {
    return null;
  }
  while (category != null) {
    if (category.isInTrash()) {
      return category;
    }
    try {
      category=category.getParentCategory();
    }
 catch (    Exception e) {
      return null;
    }
  }
  return null;
}
