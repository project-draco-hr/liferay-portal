{
  if (actionId.equals(ActionKeys.VIEW)) {
    User user=UserLocalServiceUtil.getUserById(permissionChecker.getUserId());
    return isViewableGroup(user,layout.getGroupId(),layout,controlPanelCategory,checkResourcePermission,permissionChecker);
  }
  return containsWithoutViewableGroup(permissionChecker,layout,controlPanelCategory,checkResourcePermission,actionId);
}
