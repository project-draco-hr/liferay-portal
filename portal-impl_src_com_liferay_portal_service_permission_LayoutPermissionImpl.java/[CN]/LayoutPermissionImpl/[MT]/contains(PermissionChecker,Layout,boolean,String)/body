{
  if (layout.isTypeControlPanel()) {
    if (!permissionChecker.isSignedIn()) {
      return false;
    }
    return true;
  }
  if (isAttemptToModifyLockedLayout(layout,actionId)) {
    return false;
  }
  Boolean hasPermission=StagingPermissionUtil.hasPermission(permissionChecker,layout.getGroup(),Layout.class.getName(),layout.getGroupId(),null,actionId);
  if (hasPermission != null) {
    return hasPermission.booleanValue();
  }
  return containsWithViewableGroup(permissionChecker,layout,checkViewableGroup,actionId);
}
