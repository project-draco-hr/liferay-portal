{
  Configuration configuration=EhcacheConfigurationUtil.getConfiguration(configurationURL,_clusterAware,_usingDefault);
  if (!_name.equals(configuration.getName())) {
    return;
  }
  Map<String,CacheConfiguration> cacheConfigurations=configuration.getCacheConfigurations();
  for (  CacheConfiguration cacheConfiguration : cacheConfigurations.values()) {
    replaceCache(new Cache(cacheConfiguration));
  }
}
