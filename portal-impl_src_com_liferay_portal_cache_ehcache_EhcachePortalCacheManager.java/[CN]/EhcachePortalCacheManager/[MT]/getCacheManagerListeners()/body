{
  CacheManagerEventListenerRegistry cacheManagerEventListenerRegistry=_cacheManager.getCacheManagerEventListenerRegistry();
  Set<PortalCacheManagerEventListener> portalCacheManagerEventListeners=cacheManagerEventListenerRegistry.getRegisteredListeners();
  Set<CacheManagerListener> cacheManagerListeners=new HashSet<CacheManagerListener>();
  for (  PortalCacheManagerEventListener portalCacheManagerEventListener : portalCacheManagerEventListeners) {
    cacheManagerListeners.add(portalCacheManagerEventListener.getCacheManagerListener());
  }
  return cacheManagerListeners;
}
