{
  super((HttpServletResponse)pageContext.getResponse());
  if (trimNewLines && (pageContext instanceof PageContextWrapper)) {
    PageContextWrapper pageContextWrapper=(PageContextWrapper)pageContext;
    PageContext wrappedPageContext=pageContextWrapper.getWrappedPageContext();
    _printWriter=UnsyncPrintWriterPool.borrow(new TrimNewLinesJspWriter(wrappedPageContext.getOut()));
  }
 else {
    _printWriter=UnsyncPrintWriterPool.borrow(pageContext.getOut());
  }
}
