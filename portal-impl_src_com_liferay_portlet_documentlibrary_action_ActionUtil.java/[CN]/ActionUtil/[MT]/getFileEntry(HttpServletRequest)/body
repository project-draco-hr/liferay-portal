{
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  long groupId=themeDisplay.getScopeGroupId();
  long folderId=ParamUtil.getLong(request,"folderId");
  long newFolderId=ParamUtil.getLong(request,"newFolderId");
  String name=ParamUtil.getString(request,"name");
  DLFileEntry fileEntry=null;
  if (Validator.isNotNull(name)) {
    try {
      fileEntry=DLFileEntryServiceUtil.getFileEntry(groupId,folderId,name);
    }
 catch (    NoSuchFileEntryException nsfe) {
      fileEntry=DLFileEntryServiceUtil.getFileEntry(groupId,newFolderId,name);
    }
  }
  request.setAttribute(WebKeys.DOCUMENT_LIBRARY_FILE_ENTRY,fileEntry);
}
