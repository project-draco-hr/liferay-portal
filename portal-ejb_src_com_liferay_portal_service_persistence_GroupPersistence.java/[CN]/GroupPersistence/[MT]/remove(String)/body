{
  Session session=null;
  try {
    session=openSession();
    GroupHBM groupHBM=(GroupHBM)session.get(GroupHBM.class,groupId);
    if (groupHBM == null) {
      _log.warn("No Group exists with the primary key " + groupId.toString());
      throw new NoSuchGroupException("No Group exists with the primary key " + groupId.toString());
    }
    session.delete(groupHBM);
    session.flush();
    return GroupHBMUtil.model(groupHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
