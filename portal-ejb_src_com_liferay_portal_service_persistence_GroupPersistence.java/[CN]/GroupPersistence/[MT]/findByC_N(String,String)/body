{
  Group group=fetchByC_N(companyId,name);
  if (group == null) {
    String msg="No Group exists with the key ";
    msg+=StringPool.OPEN_CURLY_BRACE;
    msg+="companyId=";
    msg+=companyId;
    msg+=", ";
    msg+="name=";
    msg+=name;
    msg+=StringPool.CLOSE_CURLY_BRACE;
    if (_log.isWarnEnabled()) {
      _log.warn(msg);
    }
    throw new NoSuchGroupException(msg);
  }
  return group;
}
