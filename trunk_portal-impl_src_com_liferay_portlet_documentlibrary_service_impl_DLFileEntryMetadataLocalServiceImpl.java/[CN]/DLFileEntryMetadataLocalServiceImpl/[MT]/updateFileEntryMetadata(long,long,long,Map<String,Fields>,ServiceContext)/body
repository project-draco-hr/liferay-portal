{
  DLFileEntryType fileEntryType=dlFileEntryTypeLocalService.getFileEntryType(fileEntryTypeId);
  List<DDMStructure> ddmStructures=fileEntryType.getDDMStructures();
  updateFileEntryMetadata(fileEntryType.getCompanyId(),ddmStructures,fileEntryTypeId,fileEntryId,fileVersionId,fieldsMap,serviceContext);
}
