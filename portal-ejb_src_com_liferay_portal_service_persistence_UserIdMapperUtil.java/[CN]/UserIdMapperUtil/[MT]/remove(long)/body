{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(userIdMapperId));
  }
  com.liferay.portal.model.UserIdMapper userIdMapper=getPersistence().remove(userIdMapperId);
  if (listener != null) {
    listener.onAfterRemove(userIdMapper);
  }
  return userIdMapper;
}
