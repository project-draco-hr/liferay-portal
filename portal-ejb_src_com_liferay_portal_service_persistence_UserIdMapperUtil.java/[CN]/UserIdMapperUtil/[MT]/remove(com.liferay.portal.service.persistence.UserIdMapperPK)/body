{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(userIdMapperPK));
  }
  com.liferay.portal.model.UserIdMapper userIdMapper=getPersistence().remove(userIdMapperPK);
  if (listener != null) {
    listener.onAfterRemove(userIdMapper);
  }
  return userIdMapper;
}
