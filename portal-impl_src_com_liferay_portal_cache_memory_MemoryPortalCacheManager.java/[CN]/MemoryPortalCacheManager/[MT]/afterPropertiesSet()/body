{
  if (_name == null) {
    throw new NullPointerException("Name is null");
  }
  _memoryPortalCaches=new ConcurrentHashMap<String,MemoryPortalCache<K,V>>(_cacheManagerInitialCapacity);
  for (  CacheManagerListener cacheManagerListener : _cacheManagerListeners) {
    cacheManagerListener.init();
  }
  PortalCacheProvider.registerPortalCacheManager(this);
}
