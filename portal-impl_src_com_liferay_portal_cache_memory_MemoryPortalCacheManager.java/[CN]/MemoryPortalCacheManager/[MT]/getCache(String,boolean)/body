{
  MemoryPortalCache<K,V> portalCache=_memoryPortalCaches.get(name);
  if (portalCache == null) {
    portalCache=new MemoryPortalCache<K,V>(this,name,_cacheInitialCapacity);
    _memoryPortalCaches.put(name,portalCache);
    for (    CacheManagerListener cacheManagerListener : _cacheManagerListeners) {
      cacheManagerListener.notifyCacheAdded(name);
    }
  }
  return portalCache;
}
