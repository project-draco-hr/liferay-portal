{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),WSRPConsumerLocalService.class.getName());
    if (invokableLocalService instanceof WSRPConsumerLocalService) {
      _service=(WSRPConsumerLocalService)invokableLocalService;
    }
 else {
      _service=new WSRPConsumerLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(WSRPConsumerLocalServiceUtil.class,"_service");
  }
  return _service;
}
