{
  try {
    Layout layout=getLayout();
    if (layout == null) {
      return false;
    }
    Group group=layout.getGroup();
    if (group.isLayoutPrototype()) {
      return false;
    }
    if (LayoutPermissionUtil.contains(themeDisplay.getPermissionChecker(),layout,ActionKeys.UPDATE)) {
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
