{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(LayoutRevision.class,LayoutRevision.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("layoutRevisionId",nextLong()));
  List<LayoutRevision> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
