{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  LayoutRevision newLayoutRevision=addLayoutRevision();
  _persistence.clearCache();
  LayoutRevisionModelImpl existingLayoutRevisionModelImpl=(LayoutRevisionModelImpl)_persistence.findByPrimaryKey(newLayoutRevision.getPrimaryKey());
  Assert.assertEquals(existingLayoutRevisionModelImpl.getLayoutSetBranchId(),existingLayoutRevisionModelImpl.getOriginalLayoutSetBranchId());
  Assert.assertEquals(existingLayoutRevisionModelImpl.getHead(),existingLayoutRevisionModelImpl.getOriginalHead());
  Assert.assertEquals(existingLayoutRevisionModelImpl.getPlid(),existingLayoutRevisionModelImpl.getOriginalPlid());
}
