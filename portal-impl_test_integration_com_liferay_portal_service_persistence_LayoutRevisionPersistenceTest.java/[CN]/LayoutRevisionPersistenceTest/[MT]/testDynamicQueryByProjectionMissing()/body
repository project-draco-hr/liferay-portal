{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(LayoutRevision.class,LayoutRevision.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("layoutRevisionId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("layoutRevisionId",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
