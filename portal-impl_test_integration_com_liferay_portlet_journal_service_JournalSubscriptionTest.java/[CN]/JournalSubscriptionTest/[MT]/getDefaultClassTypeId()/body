{
  Company company=CompanyLocalServiceUtil.getCompany(group.getCompanyId());
  DDMStructure defaultStructure=DDMStructureLocalServiceUtil.getStructure(company.getGroupId(),PortalUtil.getClassNameId(JournalArticle.class),"BASIC-WEB-CONTENT");
  Assert.assertNotNull(defaultStructure);
  return defaultStructure.getStructureId();
}
