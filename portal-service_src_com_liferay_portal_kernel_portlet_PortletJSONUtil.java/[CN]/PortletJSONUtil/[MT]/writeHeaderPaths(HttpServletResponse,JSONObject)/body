{
  JSONArray headerCssPathsJSONArray=jsonObject.getJSONArray("headerCssPaths");
  JSONArray headerJavaScriptPathsJSONArray=jsonObject.getJSONArray("headerJavaScriptPaths");
  if ((headerCssPathsJSONArray.length() == 0) && (headerJavaScriptPathsJSONArray.length() == 0)) {
    return;
  }
  writePaths(response,headerCssPathsJSONArray,headerJavaScriptPathsJSONArray);
}
