{
  JSONArray footerCssPathsJSONArray=jsonObject.getJSONArray("footerCssPaths");
  JSONArray footerJavaScriptPathsJSONArray=jsonObject.getJSONArray("footerJavaScriptPaths");
  if ((footerCssPathsJSONArray.length() == 0) && (footerJavaScriptPathsJSONArray.length() == 0)) {
    return;
  }
  writePaths(response,footerCssPathsJSONArray,footerJavaScriptPathsJSONArray);
}
