{
  E element=null;
  long nanos=timeUnit.toNanos(timeout);
  _takeLock.lockInterruptibly();
  try {
    while (_count.get() == 0) {
      if (nanos <= 0) {
        return null;
      }
      nanos=_notEmptyCondition.awaitNanos(nanos);
    }
    element=_dequeue();
    if (_count.getAndDecrement() > 1) {
      _notEmptyCondition.signal();
    }
  }
  finally {
    _takeLock.unlock();
  }
  return element;
}
