{
  final Portlet portlet=createPortletWithContextPathAndResources("/pluginContextPath","/css/main.css");
  portlet.setTimestamp(10000L);
  ComboServletURLGenerator comboServletURLGenerator=new ComboServletURLGenerator();
  comboServletURLGenerator.setPortletResourcesAccessors(PortletResourcesAccessor.headerPortalCss);
  comboServletURLGenerator.setVisited(new HashSet<String>());
  comboServletURLGenerator.setComboURLPrefix(COMBO_URL_PREFIX);
  Collection<String> urls=comboServletURLGenerator.generateURLsFor(portlets(portlet));
  assertURLs(urls,COMBO_URL_PREFIX + "&/css/main.css&t=10000");
}
