{
  List<Role> rolesWithPermission=ResourceBlockLocalServiceUtil.getRoles(BookmarksEntry.class.getName(),_bookmarksEntry1.getEntryId(),ActionKeys.VIEW);
  Assert.assertTrue(rolesWithPermission.contains(_role1));
  Assert.assertTrue(rolesWithPermission.contains(_role2));
  rolesWithPermission=ResourceBlockLocalServiceUtil.getRoles(BookmarksEntry.class.getName(),_bookmarksEntry2.getEntryId(),ActionKeys.VIEW);
  Assert.assertTrue(rolesWithPermission.contains(_role1));
  Assert.assertFalse(rolesWithPermission.contains(_role2));
}
