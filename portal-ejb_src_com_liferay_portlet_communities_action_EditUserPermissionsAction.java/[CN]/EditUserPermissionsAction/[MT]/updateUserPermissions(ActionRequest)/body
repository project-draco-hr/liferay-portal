{
  long groupId=ParamUtil.getLong(req,"groupId");
  long resourceId=ParamUtil.getLong(req,"resourceId");
  long userId=ParamUtil.getLong(req,"userIdsPosValue");
  String[] actionIds=StringUtil.split(ParamUtil.getString(req,"userIdActionIds"));
  PermissionServiceUtil.setUserPermissions(userId,groupId,actionIds,resourceId);
}
