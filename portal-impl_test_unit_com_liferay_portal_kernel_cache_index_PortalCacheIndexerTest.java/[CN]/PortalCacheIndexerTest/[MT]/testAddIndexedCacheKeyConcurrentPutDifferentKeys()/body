{
  _mappedMethodNameCallableInvocationHandler.putBeforeCallable(ConcurrentMap.class.getMethod("putIfAbsent",Object.class,Object.class),new Callable<Void>(){
    @Override public Void call(){
      _portalCache.put(_INDEX_1_KEY_1,_VALUE);
      return null;
    }
  }
);
  _portalCache.put(_INDEX_1_KEY_2,_VALUE);
  assertIndexCacheSynchronization();
}
