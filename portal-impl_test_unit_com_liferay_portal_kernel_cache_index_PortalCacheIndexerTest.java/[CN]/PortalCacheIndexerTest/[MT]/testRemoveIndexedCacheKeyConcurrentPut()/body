{
  _portalCache.put(_INDEX_1_KEY_1,_VALUE);
  _mappedMethodCallableInvocationHandler.putBeforeCallable(ConcurrentMap.class.getMethod("remove",Object.class,Object.class),new Callable<Void>(){
    @Override public Void call() throws Exception {
      _portalCache.put(_INDEX_1_KEY_2,_VALUE);
      return null;
    }
  }
);
  _portalCache.remove(_INDEX_1_KEY_1);
  assertIndexCacheSynchronization();
}
