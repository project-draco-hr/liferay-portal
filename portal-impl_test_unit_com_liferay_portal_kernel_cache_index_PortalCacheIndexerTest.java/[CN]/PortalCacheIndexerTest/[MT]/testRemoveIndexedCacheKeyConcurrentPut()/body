{
  _portalCache.put(_INDEX_1_KEY_1,_VALUE);
  Callable<?> callable=new Callable<Object>(){
    @Override public Object call() throws Exception {
      _portalCache.put(_INDEX_1_KEY_2,_VALUE);
      return null;
    }
  }
;
  _mappedMethodNameCallableInvocationHandler.putBeforeCallable(ConcurrentMap.class.getMethod("remove",Object.class,Object.class),callable);
  _portalCache.remove(_INDEX_1_KEY_1);
  assertIndexCacheSynchronization();
}
