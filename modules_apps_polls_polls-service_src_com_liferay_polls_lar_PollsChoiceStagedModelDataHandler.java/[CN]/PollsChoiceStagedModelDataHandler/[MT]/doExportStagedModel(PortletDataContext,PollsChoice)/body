{
  PollsQuestion question=_pollsQuestionLocalService.getQuestion(choice.getQuestionId());
  StagedModelDataHandlerUtil.exportReferenceStagedModel(portletDataContext,choice,question,PortletDataContext.REFERENCE_TYPE_STRONG);
  Element choiceElement=portletDataContext.getExportDataElement(choice);
  portletDataContext.addClassedModel(choiceElement,ExportImportPathUtil.getModelPath(choice),choice);
}
