{
  Configuration configuration=_configurationModel.getConfiguration();
  if (configuration != null) {
    addDDMFormFieldValues(attributeDefinition.getID(),attributeDefinition.getCardinality(),configuration,ddmFormValues);
  }
 else {
    addDDMFormFieldValues(attributeDefinition.getID(),attributeDefinition.getDefaultValue(),ddmFormValues);
  }
}
