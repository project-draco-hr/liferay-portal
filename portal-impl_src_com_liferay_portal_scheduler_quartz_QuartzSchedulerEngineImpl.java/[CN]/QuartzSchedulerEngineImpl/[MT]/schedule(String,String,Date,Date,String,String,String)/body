{
  if (!PropsValues.SCHEDULER_ENABLED) {
    return;
  }
  try {
    CronTrigger cronTrigger=new CronTrigger(groupName,groupName,cronText);
    if (startDate != null) {
      cronTrigger.setStartTime(startDate);
    }
    if (endDate != null) {
      cronTrigger.setEndTime(endDate);
    }
    _schdule(groupName,cronTrigger,description,destination,messageBody);
  }
 catch (  ParseException pe) {
    throw new SchedulerException("Unable to parse cron text",pe);
  }
}
