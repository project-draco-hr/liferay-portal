{
  try {
    String[] jobNames=_scheduler.getJobNames(groupName);
    List<SchedulerRequest> requests=new ArrayList<SchedulerRequest>();
    for (    String jobName : jobNames) {
      JobDetail jobDetail=_scheduler.getJobDetail(jobName,groupName);
      JobDataMap jobDataMap=jobDetail.getJobDataMap();
      String description=jobDataMap.getString(DESCRIPTION);
      String messageBody=jobDataMap.getString(MESSAGE_BODY);
      CronTrigger cronTrigger=(CronTrigger)_scheduler.getTrigger(jobName,groupName);
      SchedulerRequest schedulerRequest=new SchedulerRequest(null,jobName,groupName,cronTrigger.getCronExpression(),cronTrigger.getStartTime(),cronTrigger.getEndTime(),description,null,messageBody);
      requests.add(schedulerRequest);
    }
    return requests;
  }
 catch (  org.quartz.SchedulerException se) {
    throw new SchedulerException("Unable to retrieve job",se);
  }
}
