{
  try {
    SchedulerContext schedulerContext=_scheduler.getContext();
    ObjectValuePair<TriggerState,Exception> jobState=(ObjectValuePair<TriggerState,Exception>)schedulerContext.get(jobFullName);
    return jobState;
  }
 catch (  org.quartz.SchedulerException se) {
    throw new SchedulerException("Unable to get job state",se);
  }
}
