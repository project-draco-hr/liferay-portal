{
  if (!PropsValues.SCHEDULER_ENABLED) {
    return;
  }
  try {
    String[] jobNames=_scheduler.getJobNames(groupName);
    for (    String jobName : jobNames) {
      unschedule(jobName,groupName);
    }
  }
 catch (  Exception e) {
    throw new SchedulerException("Unable to unschedule jobs",e);
  }
}
