{
  long classNameId=PortalUtil.getClassNameId(DDLRecordSet.class);
  String structureKey=StringPool.BLANK;
  boolean autoStructureKey=true;
  String storageType="xml";
  String xml=ContentUtil.get("com/liferay/portal/dependencies/" + "liferay-documentlibrary-structures.xml");
  Document document=SAXReaderUtil.read(xml);
  Element rootElement=document.getRootElement();
  List<Element> structureElements=rootElement.elements("structure");
  for (  Element structureElement : structureElements) {
    Element nameElement=structureElement.element("name");
    Element descriptionElement=structureElement.element("description");
    String name=nameElement.getText();
    String description=descriptionElement.getText();
    List<DDMStructure> structures=DDMStructureLocalServiceUtil.getStructure(groupId,name,description);
    if (structures.size() > 0) {
      continue;
    }
    Element structureDefinition=structureElement.element("root");
    String xsd=structureDefinition.asXML();
    DDMStructureLocalServiceUtil.addStructure(userId,groupId,classNameId,structureKey,autoStructureKey,name,description,xsd,storageType,serviceContext);
  }
}
