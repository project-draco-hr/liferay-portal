{
  String name1=syncFile1.getName();
  String name2=syncFile2.getName();
  if ((syncFile1.getParentFolderId() == syncFile2.getParentFolderId()) && name1.equals(name2)) {
    return Long.compare(syncFile1.getModifiedTime(),syncFile2.getModifiedTime());
  }
 else {
    return Long.compare(syncFile1.getSize(),syncFile2.getSize());
  }
}
