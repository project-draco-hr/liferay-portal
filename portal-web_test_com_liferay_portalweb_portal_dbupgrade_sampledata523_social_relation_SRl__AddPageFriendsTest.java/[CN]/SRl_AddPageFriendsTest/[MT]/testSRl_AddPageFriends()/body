{
  selenium.open("/web/socialrelationsn1/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@id='add-page']/a/span")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Add Page"),selenium.getText("//div[@id='add-page']/a/span"));
  selenium.clickAt("//div[@id='add-page']/a/span",RuntimeVariables.replace("Add Page"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//input[@name='new_page']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.type("//input[@name='new_page']",RuntimeVariables.replace("Friends Test Page"));
  selenium.clickAt("link=Save",RuntimeVariables.replace("Save"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Friends Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Friends Test Page",RuntimeVariables.replace("Friends Test Page"));
  selenium.waitForPageToLoad("30000");
}
