{
  CalendarResource calendarResource=findByPrimaryKey(calendarResourceId);
  Session session=null;
  try {
    session=openSession();
    CalendarResource[] array=new CalendarResourceImpl[3];
    array[0]=getByC_C_A_PrevAndNext(session,calendarResource,companyId,code,active,orderByComparator,true);
    array[1]=calendarResource;
    array[2]=getByC_C_A_PrevAndNext(session,calendarResource,companyId,code,active,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
