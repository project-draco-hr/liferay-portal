{
  LinkedHashMap groupParams=new LinkedHashMap();
  groupParams.put("usersGroups",new Long(permissionChecker.getUserId()));
  int count=GroupLocalServiceUtil.searchCount(permissionChecker.getCompanyId(),null,null,groupParams);
  if (count > 0) {
    return true;
  }
 else {
    return false;
  }
}
