{
  String className=TestAssetRendererFactory.class.getName();
  PortalImpl portalImpl=new PortalImpl();
  Long classNameId=portalImpl.getClassNameId(TestAssetRendererFactory.class);
  AssetRendererFactory assetRendererFactory=AssetRendererFactoryRegistryUtil.getAssetRendererFactoryByClassNameId(classNameId);
  Class<?> clazz=assetRendererFactory.getClass();
  Assert.assertEquals(className,clazz.getName());
}
