{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(liferayPortletRequest,BookmarksPortletKeys.BOOKMARKS_ADMIN,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcRenderCommandName","/bookmarks/edit_folder");
  portletURL.setParameter("folderId",String.valueOf(_folder.getFolderId()));
  return portletURL;
}
