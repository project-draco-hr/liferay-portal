{
  selenium.open("/user/socialofficefriendsn/home");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div/div[1]/div/div/div/ul[1]/li[1]/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("//div/div[1]/div/div/div/ul[1]/li[1]/a",RuntimeVariables.replace("Home"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Activities"),selenium.getText("//div[2]/section/header/h1/span"));
  assertEquals(RuntimeVariables.replace("Friends"),selenium.getText("//li[1]/span/span/a"));
  selenium.clickAt("//li[1]/span/span/a",RuntimeVariables.replace("Friends"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertTrue(selenium.isVisible("//div[1]/span[1]/img"));
  assertTrue(selenium.isVisible("//span[2]/span/img"));
  assertEquals(RuntimeVariables.replace("Joe Bloggs wrote a new blog entry, Blogs Entry Title, in Open Site Name."),selenium.getText("//section/div/div/div/div/div/div[2]/div[1]"));
  assertEquals(RuntimeVariables.replace("Joe Bloggs"),selenium.getText("//div[2]/div[1]/a[1]"));
  assertEquals(RuntimeVariables.replace("Blogs Entry Title"),selenium.getText("//div[1]/a[2]"));
  assertEquals(RuntimeVariables.replace("Open Site Name"),selenium.getText("//a[3]"));
}
