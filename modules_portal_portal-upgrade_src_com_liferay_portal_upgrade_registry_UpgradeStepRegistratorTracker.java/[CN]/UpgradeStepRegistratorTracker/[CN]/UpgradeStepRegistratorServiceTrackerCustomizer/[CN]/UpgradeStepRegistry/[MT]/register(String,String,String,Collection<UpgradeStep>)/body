{
  List<UpgradeInfo> upgradeInfos=buildUpgradeInfos(fromSchemaVersionString,toSchemaVersionString,upgradeSteps);
  for (  UpgradeInfo upgradeInfo : upgradeInfos) {
    ServiceRegistration<UpgradeStep> serviceRegistration=_register(_bundleContext,bundleSymbolicName,upgradeInfo);
    _serviceRegistrations.add(serviceRegistration);
  }
}
