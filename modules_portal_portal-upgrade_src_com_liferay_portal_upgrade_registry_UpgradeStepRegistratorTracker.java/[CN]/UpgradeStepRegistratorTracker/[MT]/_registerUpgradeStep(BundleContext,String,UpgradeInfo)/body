{
  Dictionary<String,Object> properties=new Hashtable<>();
  properties.put("upgrade.db.type","any");
  properties.put("upgrade.from.version",upgradeInfo.getFromSchemaVersionString());
  properties.put("upgrade.to.version",upgradeInfo.getToSchemaVersionString());
  properties.put("upgrade.bundle.symbolic.name",upgradeBundleSymbolicName);
  return bundleContext.registerService(UpgradeStep.class,upgradeInfo.getUpgradeStep(),properties);
}
