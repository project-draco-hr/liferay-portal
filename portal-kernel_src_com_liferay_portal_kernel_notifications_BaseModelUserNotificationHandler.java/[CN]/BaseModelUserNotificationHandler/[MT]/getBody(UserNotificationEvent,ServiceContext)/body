{
  JSONObject jsonObject=JSONFactoryUtil.createJSONObject(userNotificationEvent.getPayload());
  AssetRenderer<?> assetRenderer=getAssetRenderer(jsonObject);
  if (assetRenderer == null) {
    UserNotificationEventLocalServiceUtil.deleteUserNotificationEvent(userNotificationEvent.getUserNotificationEventId());
    return null;
  }
  return StringUtil.replace(getBodyTemplate(),new String[]{"[$BODY$]","[$TITLE$]"},new String[]{HtmlUtil.escape(StringUtil.shorten(getBodyContent(jsonObject),70)),getTitle(jsonObject,assetRenderer,serviceContext)});
}
