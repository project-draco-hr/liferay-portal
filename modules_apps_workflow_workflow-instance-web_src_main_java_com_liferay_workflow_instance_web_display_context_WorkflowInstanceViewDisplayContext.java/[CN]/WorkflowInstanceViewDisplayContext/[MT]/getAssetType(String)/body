{
  for (  WorkflowHandler<?> workflowHandler : getSearchableAssetsWorkflowHandlers()) {
    String assetType=workflowHandler.getType(workflowInstanceRequestHelper.getLocale());
    if (StringUtil.equalsIgnoreCase(keywords,assetType)) {
      return workflowHandler.getClassName();
    }
  }
  return StringPool.BLANK;
}
