{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SocialActivitySetting.class,SocialActivitySetting.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("activitySettingId",nextLong()));
  List<SocialActivitySetting> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
