{
  mockStatic(RoleLocalServiceUtil.class);
  Role siteMemberRole=Mockito.mock(Role.class);
  Mockito.when(siteMemberRole.getName()).thenReturn(RoleConstants.SITE_MEMBER);
  when(RoleLocalServiceUtil.getDefaultGroupRole(Mockito.anyLong())).thenReturn(siteMemberRole);
  Role guestRole=Mockito.mock(Role.class);
  Mockito.when(guestRole.getName()).thenReturn(RoleConstants.GUEST);
  when(RoleLocalServiceUtil.getRole(Mockito.anyLong(),Mockito.eq(RoleConstants.GUEST))).thenReturn(guestRole);
}
