{
  aQute.bnd.build.Project bndProject=getBndProject();
  try {
    if (_file.isFile() && _file.getName().endsWith(".jar")) {
      if (_deployRepo != null) {
        bndProject.deploy(_deployRepo,_file);
      }
 else {
        bndProject.deploy(_file);
      }
    }
 else {
      project.log("Not a JAR file " + _file.getAbsolutePath(),Project.MSG_ERR);
    }
  }
 catch (  Exception e) {
    throw new BuildException(e.getMessage(),e,new org.apache.tools.ant.Location(_file.getAbsolutePath()));
  }
  report(bndProject);
  if (bndProject.getErrors().size() > 0) {
    throw new BuildException("Unable to deploy",new org.apache.tools.ant.Location(_file.getAbsolutePath()));
  }
}
