{
  if (_downstreamJobs == null) {
    setStatus("running");
    return;
  }
  for (  DownstreamJob downstreamJob : _downstreamJobs) {
    downstreamJob.update();
  }
  if (_downstreamJobs.size() == getDownstreamJobCount("completed")) {
    setStatus("completed");
    return;
  }
  setStatus("running");
}
