{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext();
  if (group.isLayout()) {
    Company company=CompanyLocalServiceUtil.getCompany(layout.getCompanyId());
    serviceContext.setAttribute("ddmGroupId",company.getGroupId());
  }
  for (int i=0; i < count; i++) {
    JournalArticle journalArticle=JournalTestUtil.addArticle(group.getGroupId(),RandomTestUtil.randomString(),RandomTestUtil.randomString(100),serviceContext);
    assetEntries.add(getAssetEntry(journalArticle));
  }
  return assetEntries;
}
