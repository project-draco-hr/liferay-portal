{
  StringBundler sb=new StringBundler(9);
  sb.append("{discussionId=");
  sb.append(getDiscussionId());
  sb.append(", classNameId=");
  sb.append(getClassNameId());
  sb.append(", classPK=");
  sb.append(getClassPK());
  sb.append(", threadId=");
  sb.append(getThreadId());
  sb.append("}");
  return sb.toString();
}
