{
  for (  long companyId : PortalInstances.getCompanyIds()) {
    String fileName=SystemProperties.get(SystemProperties.TMP_DIR) + "/temp-lucene-" + companyId+ "-"+ System.currentTimeMillis();
    try {
      LuceneHelperUtil.dumpIndex(TestPropsValues.getCompanyId(),new FileOutputStream(fileName));
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
 finally {
      _luceneFileNames.put(companyId,fileName);
    }
  }
}
