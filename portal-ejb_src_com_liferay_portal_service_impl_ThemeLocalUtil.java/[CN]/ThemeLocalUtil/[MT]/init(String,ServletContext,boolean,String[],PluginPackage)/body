{
  List themeIds=new ArrayList();
  try {
    for (int i=0; i < xmls.length; i++) {
      Set themes=_readThemes(servletContextName,ctx,loadFromServletContext,xmls[i],pluginPackage);
      Iterator itr=themes.iterator();
      while (itr.hasNext()) {
        String themeId=(String)itr.next();
        if (!themeIds.contains(themeId)) {
          themeIds.add(themeId);
        }
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  _themesPool.clear();
  return themeIds;
}
