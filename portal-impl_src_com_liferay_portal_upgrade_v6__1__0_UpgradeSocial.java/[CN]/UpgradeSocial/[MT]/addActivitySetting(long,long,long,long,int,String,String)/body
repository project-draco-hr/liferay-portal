{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("insert into SocialActivitySetting (activitySettingId, " + "groupId, companyId, classNameId, activityType, name, " + "value) values (?, ?, ?, ?, ?, ?, ?)");
    ps.setLong(1,activitySettingId);
    ps.setLong(2,groupId);
    ps.setLong(3,companyId);
    ps.setLong(4,classNameId);
    ps.setInt(5,activityType);
    ps.setString(6,name);
    ps.setString(7,value);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
