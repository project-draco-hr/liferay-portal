{
  try (PreparedStatement ps=connection.prepareStatement("select groupId, companyId, userId, classNameId, classPK " + "from AssetEntry where entryId = " + assetEntryId);ResultSet rs=ps.executeQuery()){
    if (rs.next()) {
      long groupId=rs.getLong("groupId");
      long companyId=rs.getLong("companyId");
      long userId=rs.getLong("userId");
      long classNameId=rs.getLong("classNameId");
      long classPK=rs.getLong("classPK");
      return new long[]{groupId,companyId,userId,classNameId,classPK};
    }
    return null;
  }
 }
