{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select groupId, companyId, userId, classNameId, classPK " + "from AssetEntry where entryId = " + assetEntryId);
    rs=ps.executeQuery();
    if (rs.next()) {
      long groupId=rs.getLong("groupId");
      long companyId=rs.getLong("companyId");
      long userId=rs.getLong("userId");
      long classNameId=rs.getLong("classNameId");
      long classPK=rs.getLong("classPK");
      return new long[]{groupId,companyId,userId,classNameId,classPK};
    }
    return null;
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
