{
  Assert.assertEquals("red,green,yellow,",StringUtil.removeFromList("red,blue,green,yellow","blue"));
  Assert.assertEquals("",StringUtil.removeFromList("blue","blue"));
  Assert.assertEquals("",StringUtil.removeFromList("blue,","blue"));
  Assert.assertEquals("red;green;yellow;",StringUtil.removeFromList("red;blue;green;yellow","blue",";"));
  Assert.assertEquals("",StringUtil.removeFromList("blue","blue",";"));
  Assert.assertEquals("",StringUtil.removeFromList("blue;","blue",";"));
}
