{
  Assert.assertEquals("HELLO WORLD",StringUtil.toUpperCase("hello world"));
  Assert.assertEquals("HELLO WORLD",StringUtil.toUpperCase("HELLO WORLD"));
  Assert.assertEquals("HELLO WORLD",StringUtil.toUpperCase("hElLo WoRlD"));
  Assert.assertEquals("HELLO-WORLD-1",StringUtil.toUpperCase("hello-world-1"));
}
