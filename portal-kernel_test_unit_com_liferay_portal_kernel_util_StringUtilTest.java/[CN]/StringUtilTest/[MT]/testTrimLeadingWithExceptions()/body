{
  Assert.assertNull(StringUtil.trimLeading(null,null));
  Assert.assertSame(StringPool.BLANK,StringUtil.trimLeading(" ",null));
  Assert.assertSame(StringPool.BLANK,StringUtil.trimLeading(" ",new char[0]));
  char[] exceptions={'\r','\t'};
  Assert.assertSame(StringPool.BLANK,StringUtil.trimLeading(StringPool.BLANK,exceptions));
  Assert.assertEquals("\t\r\n",StringUtil.trimLeading(" \t\r\n",exceptions));
  Assert.assertSame("\t",StringUtil.trimLeading("\t",exceptions));
  Assert.assertSame("\t\r",StringUtil.trimLeading("\t\r",exceptions));
  Assert.assertSame(StringPool.BLANK,StringUtil.trimLeading(" \n",exceptions));
  Assert.assertEquals("\t\r\n\t\r",StringUtil.trimLeading(" \t\r\n\t\r",exceptions));
  Assert.assertSame("\t\r \t\r\n",StringUtil.trimLeading("\t\r \t\r\n",exceptions));
  Assert.assertEquals("\t\r\n\t\r \t\r\n",StringUtil.trimLeading(" \t\r\n\t\r \t\r\n",exceptions));
}
