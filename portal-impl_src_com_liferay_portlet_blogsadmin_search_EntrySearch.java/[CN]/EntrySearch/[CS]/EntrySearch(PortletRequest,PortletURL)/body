{
  super(portletRequest,new EntryDisplayTerms(portletRequest),new EntrySearchTerms(portletRequest),DEFAULT_CUR_PARAM,DEFAULT_DELTA,iteratorURL,headerNames,EMPTY_RESULTS_MESSAGE);
  EntryDisplayTerms displayTerms=(EntryDisplayTerms)getDisplayTerms();
  iteratorURL.setParameter(EntryDisplayTerms.AUTHOR,displayTerms.getAuthor());
  iteratorURL.setParameter(EntryDisplayTerms.STATUS,String.valueOf(displayTerms.getStatus()));
  iteratorURL.setParameter(EntryDisplayTerms.TITLE,displayTerms.getTitle());
}
