{
  if (!isEnabled()) {
    throw new IllegalStateException("Cannot call \"" + getCommandPath() + "\" when GhostScript is disabled. Enable ImageMagick "+ "along with GhostScript.");
  }
  LinkedList<String> commands=new LinkedList<String>();
  commands.add(_commandPath);
  commands.add("-dBATCH");
  commands.add("-dSAFER");
  commands.add("-dNOPAUSE");
  commands.add("-dNOPROMPT");
  commands.add("-sFONTPATH" + _globalSearchPath);
  commands.addAll(arguments);
  return ProcessUtil.execute(ProcessUtil.LOGGING_OUTPUT_PROCESSOR,commands);
}
