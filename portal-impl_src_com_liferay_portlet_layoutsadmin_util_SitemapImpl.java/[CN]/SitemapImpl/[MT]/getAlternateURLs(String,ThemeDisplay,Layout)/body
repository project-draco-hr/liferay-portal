{
  Map<Locale,String> alternateURLs=new HashMap<>();
  for (  Locale availableLocale : LanguageUtil.getAvailableLocales(layout.getGroupId())) {
    String alternateURL=PortalUtil.getAlternateURL(canonicalURL,themeDisplay,availableLocale,layout);
    alternateURLs.put(availableLocale,alternateURL);
  }
  return alternateURLs;
}
