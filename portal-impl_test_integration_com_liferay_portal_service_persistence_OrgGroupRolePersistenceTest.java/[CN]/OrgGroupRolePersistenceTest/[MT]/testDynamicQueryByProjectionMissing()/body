{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(OrgGroupRole.class,OrgGroupRole.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("id.organizationId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("id.organizationId",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
