{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Bookmarks Test Page",RuntimeVariables.replace("Bookmarks Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Recent",RuntimeVariables.replace("Recent"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Test Folder Entry"),selenium.getText("//td[1]/a"));
  assertEquals(RuntimeVariables.replace("http://www.liferay.com"),selenium.getText("//td[2]/a"));
}
