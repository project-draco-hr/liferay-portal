{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  MBMailingList mbMailingList : mbMailingLists) {
    EntityCacheUtil.removeResult(MBMailingListModelImpl.ENTITY_CACHE_ENABLED,MBMailingListImpl.class,mbMailingList.getPrimaryKey());
    clearUniqueFindersCache((MBMailingListModelImpl)mbMailingList);
  }
}
