{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),PortletKeys.CALENDAR,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/calendar/edit_event");
  portletURL.setParameter("eventId",String.valueOf(_event.getEventId()));
  return portletURL;
}
