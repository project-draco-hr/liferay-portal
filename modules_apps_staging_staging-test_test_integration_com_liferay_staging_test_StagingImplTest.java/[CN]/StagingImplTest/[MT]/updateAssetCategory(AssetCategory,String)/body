{
  Map<Locale,String> titleMap=new HashMap<>();
  for (  Locale locale : _locales) {
    titleMap.put(locale,name.concat(LocaleUtil.toLanguageId(locale)));
  }
  return AssetCategoryLocalServiceUtil.updateCategory(TestPropsValues.getUserId(),category.getCategoryId(),category.getParentCategoryId(),titleMap,category.getDescriptionMap(),category.getVocabularyId(),null,ServiceContextTestUtil.getServiceContext());
}
