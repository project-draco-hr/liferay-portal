{
  PortletPreferences portletPreferences=portletRequest.getPreferences();
  String[] symbols=StringUtil.split(StringUtil.toUpperCase(ParamUtil.getString(portletRequest,"symbols")));
  if (symbols.length > 1) {
    portletPreferences.setValues("symbols",symbols);
  }
 else {
    portletPreferences.setValues("symbols",_CurrencyConverterConfiguration.symbols());
  }
}
