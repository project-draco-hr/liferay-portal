{
  Map<String,String> renderedDDMFormFieldsMap=getRenderedDDMFormFieldsMap(ddmForm,ddmFormRenderingContext);
  DDMFormLayoutTransformer ddmFormLayoutTransformer=new DDMFormLayoutTransformer(ddmFormLayout,renderedDDMFormFieldsMap,ddmFormRenderingContext.getLocale());
  return ddmFormLayoutTransformer.getPages();
}
