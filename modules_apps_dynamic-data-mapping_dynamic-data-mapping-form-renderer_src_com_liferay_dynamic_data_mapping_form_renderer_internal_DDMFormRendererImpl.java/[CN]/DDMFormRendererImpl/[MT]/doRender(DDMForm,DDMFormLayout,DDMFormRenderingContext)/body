{
  Template template=TemplateManagerUtil.getTemplate(TemplateConstants.LANG_TYPE_SOY,_templateResource,false);
  populateCommonContext(template,ddmForm,ddmFormLayout,ddmFormRenderingContext);
  String html=render(template,getTemplateNamespace(ddmFormLayout));
  String javaScript=render(template,"ddm.form_renderer_js");
  return html.concat(javaScript);
}
