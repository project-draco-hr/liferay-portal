{
  for (  PortletCategory portletCategory : _portletCategories.values()) {
    portletCategory.separate(portletIds);
  }
  Iterator<String> itr=_portletIds.iterator();
  while (itr.hasNext()) {
    String portletId=itr.next();
    if (portletIds.contains(portletId)) {
      itr.remove();
    }
  }
}
