{
  PortalPreferences newPortalPreferences=addPortalPreferences();
  _persistence.clearCache();
  PortalPreferences existingPortalPreferences=_persistence.findByPrimaryKey(newPortalPreferences.getPrimaryKey());
  Assert.assertEquals(existingPortalPreferences.getOwnerId(),ReflectionTestUtil.invoke(existingPortalPreferences,"getOriginalOwnerId",new Class<?>[0]));
  Assert.assertEquals(existingPortalPreferences.getOwnerType(),ReflectionTestUtil.invoke(existingPortalPreferences,"getOriginalOwnerType",new Class<?>[0]));
}
