{
  document.addKeyword(_PREFIX_SETTING + "privateLayout",MapUtil.getBoolean(settingsMap,"privateLayout"));
  document.addKeyword(_PREFIX_SETTING + "sourceGroupId",MapUtil.getLong(settingsMap,"sourceGroupId"));
  document.addKeyword(_PREFIX_SETTING + "targetGroupId",MapUtil.getLong(settingsMap,"targetGroupId"));
}
