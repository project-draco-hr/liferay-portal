{
  try {
    Map<String,Preference> preferences=getPreferences();
    for (    Map.Entry<String,Preference> entry : preferences.entrySet()) {
      String key=entry.getKey();
      if (key.startsWith(namespace) && !isReadOnly(key)) {
        reset(key);
      }
    }
    store();
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
