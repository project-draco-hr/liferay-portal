{
  TransactionStatus transactionStatus=start(platformTransactionManager,transactionAttribute);
  Object returnValue=null;
  try {
    returnValue=methodInvocation.proceed();
  }
 catch (  Throwable throwable) {
    rollback(platformTransactionManager,throwable,transactionAttribute,transactionStatus);
  }
  commit(platformTransactionManager,transactionAttribute,transactionStatus);
  return returnValue;
}
