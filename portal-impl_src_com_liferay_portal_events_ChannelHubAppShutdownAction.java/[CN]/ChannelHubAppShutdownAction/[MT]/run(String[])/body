{
  if (ids.length > 0) {
    long companyId=GetterUtil.get(ids[0],0L);
    if (companyId > 0) {
      try {
        ChannelHubManagerUtil.destroyChannelHub(companyId);
      }
 catch (      ChannelException e) {
        if (_log.isErrorEnabled()) {
          _log.error("Unable to destroy ChannelHub for companyId " + companyId,e);
        }
      }
    }
  }
}
