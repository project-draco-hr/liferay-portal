{
  List<String> unverifiedModelNames=new ArrayList<String>();
  for (  VerifiableAuditedModel verifiableAuditedModel : _verifiableAuditedModels) {
    unverifiedModelNames.add(verifiableAuditedModel.getModelName());
  }
  while (!unverifiedModelNames.isEmpty()) {
    int count=unverifiedModelNames.size();
    for (    VerifiableAuditedModel verifiableAuditedModel : _verifiableAuditedModels) {
      if (unverifiedModelNames.contains(verifiableAuditedModel.getJoinByColumnName()) || !unverifiedModelNames.contains(verifiableAuditedModel.getModelName())) {
        continue;
      }
      verifyModel(verifiableAuditedModel);
      unverifiedModelNames.remove(verifiableAuditedModel.getModelName());
    }
    if (unverifiedModelNames.size() == count) {
      throw new VerifyException("Circular dependency detected " + unverifiedModelNames);
    }
  }
}
