{
  Module module=findByPrimaryKey(moduleId);
  Session session=null;
  try {
    session=openSession();
    Module[] array=new ModuleImpl[3];
    array[0]=getByAppId_PrevAndNext(session,module,appId,orderByComparator,true);
    array[1]=module;
    array[2]=getByAppId_PrevAndNext(session,module,appId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
