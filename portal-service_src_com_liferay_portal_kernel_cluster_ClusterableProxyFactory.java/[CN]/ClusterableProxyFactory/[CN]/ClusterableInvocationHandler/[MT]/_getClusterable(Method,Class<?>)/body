{
  MethodKey methodKey=new MethodKey(targetClass,method.getName(),method.getParameterTypes());
  Clusterable clusterable=_clusterables.get(methodKey);
  if (clusterable != null) {
    if (clusterable == NullClusterable.NULL_CLUSTERABLE) {
      return null;
    }
    return clusterable;
  }
  clusterable=AnnotationLocator.locate(method,_targetObject.getClass(),Clusterable.class);
  if (clusterable == null) {
    _clusterables.put(methodKey,NullClusterable.NULL_CLUSTERABLE);
  }
 else {
    _clusterables.put(methodKey,clusterable);
  }
  return clusterable;
}
