{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(statsId));
  }
  com.liferay.portlet.ratings.model.RatingsStats ratingsStats=getPersistence().remove(statsId);
  if (listener != null) {
    listener.onAfterRemove(ratingsStats);
  }
  return ratingsStats;
}
