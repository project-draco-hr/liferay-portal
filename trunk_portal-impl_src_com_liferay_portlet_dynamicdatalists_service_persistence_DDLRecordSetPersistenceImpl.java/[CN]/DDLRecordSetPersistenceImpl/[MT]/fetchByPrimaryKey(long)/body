{
  DDLRecordSet ddlRecordSet=(DDLRecordSet)EntityCacheUtil.getResult(DDLRecordSetModelImpl.ENTITY_CACHE_ENABLED,DDLRecordSetImpl.class,recordSetId,this);
  if (ddlRecordSet == null) {
    Session session=null;
    try {
      session=openSession();
      ddlRecordSet=(DDLRecordSet)session.get(DDLRecordSetImpl.class,Long.valueOf(recordSetId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (ddlRecordSet != null) {
        cacheResult(ddlRecordSet);
      }
      closeSession(session);
    }
  }
  return ddlRecordSet;
}
