{
  OrgLabor orgLabor=OrgLaborUtil.findByPrimaryKey(orgLaborId);
  checkPermission(orgLabor.getOrganizationId(),ActionKeys.UPDATE);
  return OrgLaborLocalServiceUtil.updateOrgLabor(orgLaborId,sunOpen,sunClose,monOpen,monClose,tueOpen,tueClose,wedOpen,wedClose,thuOpen,thuClose,friOpen,friClose,satOpen,satClose);
}
