{
  try {
    List<CmisExtensionElement> extensions=cmisObject.getExtensions(ExtensionLevel.PROPERTIES).get(0).getChildren();
    for (    CmisExtensionElement extension : extensions) {
      if (extension.getName().equals("properties")) {
        for (        CmisExtensionElement property : extension.getChildren()) {
          String id=property.getAttributes().get("propertyDefinitionId");
          if (id.equals("cm:description")) {
            for (            CmisExtensionElement propertyValues : property.getChildren()) {
              return propertyValues.getValue();
            }
          }
        }
      }
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return StringPool.BLANK;
}
