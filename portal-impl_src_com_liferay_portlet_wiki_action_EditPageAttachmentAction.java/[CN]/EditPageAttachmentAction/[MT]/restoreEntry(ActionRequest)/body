{
  String[] restoreEntryId=StringUtil.split(ParamUtil.getString(actionRequest,"restoreEntryId"));
  long nodeId=GetterUtil.getLong(restoreEntryId[0],0L);
  String title=GetterUtil.getString(restoreEntryId[1],StringPool.BLANK);
  String attachment=GetterUtil.getString(restoreEntryId[2],StringPool.BLANK);
  moveAttachmentFromTrash(nodeId,title,attachment);
}
