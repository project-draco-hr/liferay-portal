{
  List<MBThreadModel> mbThreadModels=new ArrayList<MBThreadModel>(_maxMBThreadCount);
  for (int i=0; i < _maxMBThreadCount; i++) {
    mbThreadModels.add(newMBThreadModel(_counter.get(),mbCategoryModel.getGroupId(),mbCategoryModel.getCategoryId(),_counter.get(),_maxMBMessageCount));
  }
  return mbThreadModels;
}
