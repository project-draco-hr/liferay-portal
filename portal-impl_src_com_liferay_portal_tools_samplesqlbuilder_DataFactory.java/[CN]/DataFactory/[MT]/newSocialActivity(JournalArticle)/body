{
  int type=JournalActivityKeys.UPDATE_ARTICLE;
  if (journalArticle.getVersion() == JournalArticleConstants.VERSION_DEFAULT) {
    type=JournalActivityKeys.ADD_ARTICLE;
  }
  return newSocialActivity(journalArticle.getGroupId(),getJournalArticleClassNameId(),journalArticle.getResourcePrimKey(),type,journalArticle.getUrlTitle());
}
