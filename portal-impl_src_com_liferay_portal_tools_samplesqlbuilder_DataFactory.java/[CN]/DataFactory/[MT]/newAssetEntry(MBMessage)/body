{
  long classNameId=0;
  boolean visible=false;
  if (mbMessage.isDiscussion()) {
    classNameId=getMBDiscussionClassNameId();
  }
 else {
    classNameId=getMBMessageClassNameId();
    visible=true;
  }
  return newAssetEntry(mbMessage.getGroupId(),mbMessage.getCreateDate(),mbMessage.getModifiedDate(),classNameId,mbMessage.getMessageId(),mbMessage.getUuid(),0,visible,ContentTypes.TEXT_HTML,mbMessage.getSubject());
}
