{
  List<ResourcePermissionModel> resourcePermissions=new ArrayList<ResourcePermissionModel>(3);
  resourcePermissions.add(newResourcePermission(name,primKey,_guestRole.getRoleId(),0));
  resourcePermissions.add(newResourcePermission(name,primKey,_ownerRole.getRoleId(),ownerId));
  resourcePermissions.add(newResourcePermission(name,primKey,_siteMemberRole.getRoleId(),0));
  return resourcePermissions;
}
