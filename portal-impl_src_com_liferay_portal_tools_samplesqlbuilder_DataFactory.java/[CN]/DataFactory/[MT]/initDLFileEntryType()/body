{
  _defaultDLFileEntryType=new DLFileEntryTypeImpl();
  Date date=new Date();
  _defaultDLFileEntryType.setUuid(PortalUUIDUtil.generate());
  _defaultDLFileEntryType.setFileEntryTypeId(DLFileEntryTypeConstants.FILE_ENTRY_TYPE_ID_BASIC_DOCUMENT);
  _defaultDLFileEntryType.setCreateDate(date);
  _defaultDLFileEntryType.setModifiedDate(date);
  _defaultDLFileEntryType.setName(DLFileEntryTypeConstants.NAME_BASIC_DOCUMENT);
}
