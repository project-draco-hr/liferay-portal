{
  _defaultDLFileEntryType=new DLFileEntryTypeImpl();
  _defaultDLFileEntryType.setUuid(SequentialUUID.generate());
  _defaultDLFileEntryType.setFileEntryTypeId(DLFileEntryTypeConstants.FILE_ENTRY_TYPE_ID_BASIC_DOCUMENT);
  _defaultDLFileEntryType.setCreateDate(nextFutureDate());
  _defaultDLFileEntryType.setModifiedDate(nextFutureDate());
  _defaultDLFileEntryType.setName(DLFileEntryTypeConstants.NAME_BASIC_DOCUMENT,LocaleUtil.getDefault());
  _defaultDLDDMStructure=newDDMStructure(_guestGroupId,getDLFileEntryClassNameId(),"TIKARAWMETADATA",_dlDDMStructureContent);
}
