{
  List<ResourcePermission> resourcePermissions=new ArrayList<ResourcePermission>(3);
  resourcePermissions.add(newResourcePermission(name,primKey,_ownerRole.getRoleId()));
  resourcePermissions.add(newResourcePermission(name,primKey,_guestRole.getRoleId()));
  resourcePermissions.add(newResourcePermission(name,primKey,_siteMemberRole.getRoleId()));
  return resourcePermissions;
}
