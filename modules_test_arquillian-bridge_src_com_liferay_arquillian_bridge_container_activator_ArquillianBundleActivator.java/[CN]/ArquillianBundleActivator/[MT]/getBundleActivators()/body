{
  if (_bundleActivators != null) {
    return _bundleActivators;
  }
  _bundleActivators=new ArrayList<>();
  Class<?> clazz=getClass();
  String[] classNames=StringUtil.splitLines(StringUtil.read(clazz.getClassLoader(),"/META-INF/services/" + BundleActivator.class.getName(),true));
  for (  String className : classNames) {
    _bundleActivators.add((BundleActivator)InstanceFactory.newInstance(clazz.getClassLoader(),className));
  }
  return _bundleActivators;
}
