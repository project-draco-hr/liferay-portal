{
  Session session=null;
  try {
    session=openSession();
    Calendar calendar=(Calendar)session.get(CalendarImpl.class,primaryKey);
    if (calendar == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchCalendarException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(calendar);
  }
 catch (  NoSuchCalendarException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
