{
  if (sacpEntry.isDefaultSACPEntry() && !CompanyThreadLocal.isDeleteInProcess()) {
    throw new RequiredSACPEntryException();
  }
  sacpEntry=sacpEntryPersistence.remove(sacpEntry);
  resourceLocalService.deleteResource(sacpEntry.getCompanyId(),SACPEntry.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,sacpEntry.getSacpEntryId());
  return sacpEntry;
}
