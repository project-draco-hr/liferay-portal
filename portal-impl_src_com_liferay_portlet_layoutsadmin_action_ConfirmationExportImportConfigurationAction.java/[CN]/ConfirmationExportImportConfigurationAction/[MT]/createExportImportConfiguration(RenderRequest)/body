{
  ExportImportConfiguration exportImportConfiguration=null;
  boolean localPublishing=ParamUtil.getBoolean(renderRequest,"localPublishing");
  if (localPublishing) {
    exportImportConfiguration=ExportImportConfigurationFactory.buildDefaultLocalPublishingExportImportConfiguration(renderRequest);
  }
 else {
    exportImportConfiguration=ExportImportConfigurationFactory.buildDefaultRemotePublishingExportImportConfiguration(renderRequest);
  }
  renderRequest.setAttribute("exportImportConfigurationId",exportImportConfiguration.getExportImportConfigurationId());
}
