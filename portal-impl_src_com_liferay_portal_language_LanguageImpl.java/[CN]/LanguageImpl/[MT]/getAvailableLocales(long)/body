{
  if (groupId <= 0) {
    return getAvailableLocales();
  }
  try {
    if (isInheritLocales(groupId)) {
      return getAvailableLocales();
    }
  }
 catch (  Exception e) {
  }
  Map<String,Locale> groupLanguageIdLocalesMap=_getGroupLanguageIdLocalesMap(groupId);
  return new HashSet<>(groupLanguageIdLocalesMap.values());
}
