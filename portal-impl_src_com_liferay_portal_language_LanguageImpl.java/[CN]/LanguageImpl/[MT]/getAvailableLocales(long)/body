{
  if (groupId <= 0) {
    return getAvailableLocales();
  }
  try {
    if (hasInheritedLocales(groupId)) {
      return getAvailableLocales();
    }
  }
 catch (  Exception e) {
  }
  if (_groupLocalesMap.get(groupId) == null) {
    _initGroupLocales(groupId);
  }
  return _groupLocalesMap.get(groupId);
}
