{
  Registry registry=RegistryUtil.getRegistry();
  MultiVMPool multiVMPool=registry.getService(MultiVMPool.class);
  PortalCache<Long,V> cache=(PortalCache<Long,V>)multiVMPool.getPortalCache(cacheName);
  PortalCacheMapSynchronizeUtil.synchronize(cache,syncMap,new Synchronizer<Long,V>(){
    @Override public void onSynchronize(    Map<? extends Long,? extends V> map,    Long key,    V value,    int timeToLive){
      synchronizeHook.apply(key);
    }
  }
);
  return cache;
}
