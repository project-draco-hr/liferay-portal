{
  try {
    JournalTemplateLocalService journalTemplateLocalService=JournalTemplateLocalServiceFactory.getService();
    return journalTemplateLocalService.search(companyId,templateId,groupId,structureId,structureIdComparator,name,description,andOperator,begin,end,obc);
  }
 catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
