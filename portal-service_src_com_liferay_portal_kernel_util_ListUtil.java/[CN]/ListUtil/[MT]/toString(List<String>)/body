{
  if ((list == null) || list.isEmpty()) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(2 * list.size() - 1);
  for (int i=0; i < list.size(); i++) {
    String value=list.get(i);
    if (value != null) {
      sb.append(value);
    }
    if ((i + 1) != list.size()) {
      sb.append(StringPool.COMMA_AND_SPACE);
    }
  }
  return sb.toString();
}
