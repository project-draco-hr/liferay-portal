{
  if ((list == null) || list.isEmpty()) {
    return;
  }
  Set<E> set=new HashSet<E>();
  Iterator<? extends E> itr=list.iterator();
  while (itr.hasNext()) {
    E obj=itr.next();
    if (!set.add(obj)) {
      itr.remove();
    }
  }
  if (comparator != null) {
    Collections.sort(list,comparator);
  }
}
