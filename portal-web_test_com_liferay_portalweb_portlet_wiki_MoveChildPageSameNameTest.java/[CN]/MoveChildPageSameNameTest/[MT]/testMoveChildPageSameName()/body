{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Test Second Child Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Details",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Move",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_36_newTitle",RuntimeVariables.replace("Test Second Child Page"));
  selenium.clickAt("//input[@value='Rename']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
