{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/socialoffice01/so/profile");
  selenium.waitForVisible("//div[@class='lfr-contact-name']/a");
  assertEquals(RuntimeVariables.replace("Social01 Office01 User01"),selenium.getText("//div[@class='lfr-contact-name']/a"));
  assertEquals(RuntimeVariables.replace("tag1"),selenium.getText("//div[@data-title='Tags']"));
  selenium.clickAt("//div[@data-title='Tags']",RuntimeVariables.replace("tag1"));
  selenium.waitForVisible("//input[contains(@id,'TagNames')]");
  selenium.click("//span[contains(@class,'textboxlistentry-close')]");
  Thread.sleep(1000);
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPartialText("//div[contains(@class,'user-information-column-2-content')]","Social01 Office01 User01 does not have any tags.");
  assertTrue(selenium.isElementNotPresent("//ul[@class='user-tags']/li/a"));
  assertTrue(selenium.isPartialText("//div[contains(@class,'user-information-column-2-content')]","Social01 Office01 User01 does not have any tags."));
  assertFalse(selenium.isTextPresent("tag1"));
}
