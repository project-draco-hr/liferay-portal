{
  for (  Map.Entry<String,Set<Header>> entry : headers.entrySet()) {
    String key=entry.getKey();
    boolean first=true;
    for (    Header header : entry.getValue()) {
      if (first) {
        header.setToResponse(key,response);
        first=false;
      }
 else {
        header.addToResponse(key,response);
      }
    }
  }
}
