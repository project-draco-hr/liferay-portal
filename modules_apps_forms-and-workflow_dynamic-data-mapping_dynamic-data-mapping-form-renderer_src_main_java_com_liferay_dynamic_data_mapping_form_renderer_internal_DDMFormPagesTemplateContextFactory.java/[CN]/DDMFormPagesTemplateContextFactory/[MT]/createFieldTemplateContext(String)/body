{
  if (_ddmFormEvaluationResult == null) {
    _ddmFormEvaluationResult=_createDDMFormEvaluationResult();
  }
  DDMFormFieldTemplateContextFactory ddmFormFieldTemplateContextFactory=new DDMFormFieldTemplateContextFactory(_ddmFormFieldsMap,_ddmFormEvaluationResult,_ddmFormFieldValuesMap.get(ddmFormFieldName),_ddmFormRenderingContext);
  ddmFormFieldTemplateContextFactory.setDDMFormFieldTypeServicesTracker(_ddmFormFieldTypeServicesTracker);
  return ddmFormFieldTemplateContextFactory.create();
}
