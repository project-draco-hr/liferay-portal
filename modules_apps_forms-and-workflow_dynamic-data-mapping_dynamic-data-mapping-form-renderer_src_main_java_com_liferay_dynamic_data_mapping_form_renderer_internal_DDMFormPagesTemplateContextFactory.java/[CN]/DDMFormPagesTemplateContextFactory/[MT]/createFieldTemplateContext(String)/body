{
  if (_ddmFormFieldEvaluationResultMap == null) {
    _ddmFormFieldEvaluationResultMap=createDDMFormFieldEvaluationResultsMap();
  }
  DDMFormFieldTemplateContextFactory ddmFormFieldTemplateContextFactory=new DDMFormFieldTemplateContextFactory(_ddmFormFieldsMap,_ddmFormFieldEvaluationResultMap.get(ddmFormFieldName),_ddmFormFieldValuesMap.get(ddmFormFieldName),_ddmFormRenderingContext);
  ddmFormFieldTemplateContextFactory.setDDMFormFieldTypeServicesTracker(_ddmFormFieldTypeServicesTracker);
  return ddmFormFieldTemplateContextFactory.create();
}
