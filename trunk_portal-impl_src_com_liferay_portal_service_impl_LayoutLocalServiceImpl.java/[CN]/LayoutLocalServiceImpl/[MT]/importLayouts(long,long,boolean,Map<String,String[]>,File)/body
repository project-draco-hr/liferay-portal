{
  try {
    LayoutImporter layoutImporter=new LayoutImporter();
    layoutImporter.importLayouts(userId,groupId,privateLayout,parameterMap,file);
  }
 catch (  PortalException pe) {
    throw pe;
  }
catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new SystemException(e);
  }
}
