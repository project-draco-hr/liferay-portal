{
  Set<String> availableParameters=new HashSet<>();
  StringBuilder sb=new StringBuilder();
  sb.append(jobURL);
  sb.append("/api/json");
  sb.append("?tree=actions[parameterDefinitions[name,type,value]]");
  JSONObject jsonObject=JenkinsResultsParserUtil.toJSONObject(sb.toString());
  JSONArray parameterDefinitions=jsonObject.getJSONArray("actions").getJSONObject(0).getJSONArray("parameterDefinitions");
  for (int i=0; i < parameterDefinitions.length(); i++) {
    JSONObject parameterDefinition=parameterDefinitions.getJSONObject(i);
    if (parameterDefinition.getString("type").equals("StringParameterDefinition")) {
      availableParameters.add(parameterDefinition.getString("name"));
    }
  }
  return availableParameters;
}
