{
  JSONArray actionsJSONArray=buildJSONObject.getJSONArray("actions");
  if ((actionsJSONArray.length() > 0) && actionsJSONArray.getJSONObject(0).has("parameters")) {
    JSONArray parametersJSONArray=actionsJSONArray.getJSONObject(0).getJSONArray("parameters");
    return getJobParameters(parametersJSONArray);
  }
 else {
    return new HashMap<>();
  }
}
