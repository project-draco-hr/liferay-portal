{
  AuthTokenIgnoreActionsRegistry.register("property1");
  AuthTokenIgnoreActionsRegistry.register("property2");
  AuthTokenIgnoreActionsRegistry.register("property3");
  AuthTokenIgnoreActionsRegistry.unregister("property1");
  Set<String> portletCSRFWhitelistActions=AuthTokenIgnoreActionsRegistry.getAuthTokenIgnoreActions();
  Assert.assertFalse("The property has not being unregistered",portletCSRFWhitelistActions.contains("property1"));
  Assert.assertTrue("The property has not being registered",portletCSRFWhitelistActions.contains("property2"));
  Assert.assertTrue("The property has not being registered",portletCSRFWhitelistActions.contains("property3"));
}
