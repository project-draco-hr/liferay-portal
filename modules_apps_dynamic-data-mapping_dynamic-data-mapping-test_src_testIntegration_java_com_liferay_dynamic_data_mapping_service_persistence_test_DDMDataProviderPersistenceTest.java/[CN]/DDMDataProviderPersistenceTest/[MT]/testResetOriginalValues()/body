{
  DDMDataProvider newDDMDataProvider=addDDMDataProvider();
  _persistence.clearCache();
  DDMDataProvider existingDDMDataProvider=_persistence.findByPrimaryKey(newDDMDataProvider.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingDDMDataProvider.getUuid(),ReflectionTestUtil.invoke(existingDDMDataProvider,"getOriginalUuid",new Class<?>[0])));
  Assert.assertEquals(Long.valueOf(existingDDMDataProvider.getGroupId()),ReflectionTestUtil.<Long>invoke(existingDDMDataProvider,"getOriginalGroupId",new Class<?>[0]));
}
