{
  DDMDataProvider newDDMDataProvider=addDDMDataProvider();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMDataProvider.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("dataProviderId"));
  Object newDataProviderId=newDDMDataProvider.getDataProviderId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("dataProviderId",new Object[]{newDataProviderId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Object existingDataProviderId=result.get(0);
  Assert.assertEquals(existingDataProviderId,newDataProviderId);
}
