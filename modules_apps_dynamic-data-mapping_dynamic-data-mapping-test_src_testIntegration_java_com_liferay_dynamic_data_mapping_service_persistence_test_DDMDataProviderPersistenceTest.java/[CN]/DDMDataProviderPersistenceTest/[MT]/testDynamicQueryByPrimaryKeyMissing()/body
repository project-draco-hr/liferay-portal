{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMDataProvider.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("dataProviderId",RandomTestUtil.nextLong()));
  List<DDMDataProvider> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
