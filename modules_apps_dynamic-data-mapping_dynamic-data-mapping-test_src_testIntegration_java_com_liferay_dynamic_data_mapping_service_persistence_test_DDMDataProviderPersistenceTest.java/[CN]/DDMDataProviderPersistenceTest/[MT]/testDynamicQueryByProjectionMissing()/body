{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMDataProvider.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("dataProviderId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("dataProviderId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
