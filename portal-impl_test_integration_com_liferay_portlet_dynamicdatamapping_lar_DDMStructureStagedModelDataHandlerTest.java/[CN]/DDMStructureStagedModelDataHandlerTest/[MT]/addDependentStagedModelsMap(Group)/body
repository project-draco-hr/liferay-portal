{
  Map<String,List<StagedModel>> dependentStagedModelsMap=new HashMap<>();
  DDMStructure ddmStructure=DDMStructureTestUtil.addStructure(group.getGroupId(),DDLRecordSet.class.getName(),DDMStructureTestUtil.getSampleDDMForm(RandomTestUtil.randomString()));
  addDependentStagedModel(dependentStagedModelsMap,DDMStructure.class,ddmStructure);
  return dependentStagedModelsMap;
}
