{
  Map<String,List<StagedModel>> dependentStagedModelsMap=new HashMap<String,List<StagedModel>>();
  DDMStructure ddmStructure=DDMStructureTestUtil.addStructure(group.getGroupId(),DDLRecordSet.class.getName(),DDMStructureTestUtil.getSampleStructureXSD(RandomTestUtil.randomString()));
  addDependentStagedModel(dependentStagedModelsMap,DDMStructure.class,ddmStructure);
  return dependentStagedModelsMap;
}
