{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Announcements Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Announcements Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Important Priority Announcement"),selenium.getText("//div[1]/h3/a"));
  assertEquals(RuntimeVariables.replace("Normal Priority Announcement"),selenium.getText("//div[2]/h3/a"));
}
