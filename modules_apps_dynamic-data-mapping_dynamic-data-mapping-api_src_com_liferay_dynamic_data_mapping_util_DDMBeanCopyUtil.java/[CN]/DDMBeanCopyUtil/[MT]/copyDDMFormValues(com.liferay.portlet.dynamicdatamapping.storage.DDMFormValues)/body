{
  if (ddmFormValues == null) {
    return null;
  }
  DDMFormValues formValues=new DDMFormValues(copyDDMForm(ddmFormValues.getDDMForm()));
  formValues.setAvailableLocales(ddmFormValues.getAvailableLocales());
  formValues.setDefaultLocale(ddmFormValues.getDefaultLocale());
  for (  com.liferay.portlet.dynamicdatamapping.storage.DDMFormFieldValue ddmFormFieldValue : ddmFormValues.getDDMFormFieldValues()) {
    formValues.addDDMFormFieldValue(copyDDMFormFieldValue(ddmFormFieldValue));
  }
  return formValues;
}
