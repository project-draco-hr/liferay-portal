{
  if (value == null) {
    return null;
  }
  com.liferay.portlet.dynamicdatamapping.model.UnlocalizedValue unlocalizedValue=new com.liferay.portlet.dynamicdatamapping.model.UnlocalizedValue(value.getString(value.getDefaultLocale()));
  Map<Locale,String> values=value.getValues();
  for (  Locale locale : values.keySet()) {
    unlocalizedValue.addString(locale,values.get(locale));
  }
  return unlocalizedValue;
}
