{
  if (ddmFormLayoutRow == null) {
    return null;
  }
  com.liferay.portlet.dynamicdatamapping.model.DDMFormLayoutRow formLayoutRow=new com.liferay.portlet.dynamicdatamapping.model.DDMFormLayoutRow();
  for (  DDMFormLayoutColumn ddmFormLayoutPageColumn : ddmFormLayoutRow.getDDMFormLayoutColumns()) {
    formLayoutRow.addDDMFormLayoutColumn(copyDDMFormLayoutColumn(ddmFormLayoutPageColumn));
  }
  return formLayoutRow;
}
