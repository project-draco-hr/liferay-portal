{
  if (ddmFormLayoutRow == null) {
    return null;
  }
  com.liferay.portlet.dynamicdatamapping.model.DDMFormLayoutRow copyDDMFormLayoutRow=new com.liferay.portlet.dynamicdatamapping.model.DDMFormLayoutRow();
  for (  DDMFormLayoutColumn ddmFormLayoutPageColumn : ddmFormLayoutRow.getDDMFormLayoutColumns()) {
    copyDDMFormLayoutRow.addDDMFormLayoutColumn(copyDDMFormLayoutColumn(ddmFormLayoutPageColumn));
  }
  return copyDDMFormLayoutRow;
}
