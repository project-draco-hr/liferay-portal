{
  if (ddmFormLayout == null) {
    return null;
  }
  DDMFormLayout formLayout=new DDMFormLayout();
  formLayout.setDefaultLocale(ddmFormLayout.getDefaultLocale());
  for (  com.liferay.portlet.dynamicdatamapping.model.DDMFormLayoutPage ddmFormLayoutPage : ddmFormLayout.getDDMFormLayoutPages()) {
    formLayout.addDDMFormLayoutPage(copyDDMFormLayoutPage(ddmFormLayoutPage));
  }
  return formLayout;
}
