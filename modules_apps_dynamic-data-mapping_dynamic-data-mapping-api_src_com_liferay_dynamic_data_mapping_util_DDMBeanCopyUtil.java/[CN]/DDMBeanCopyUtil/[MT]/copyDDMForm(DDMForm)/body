{
  if (ddmForm == null) {
    return null;
  }
  com.liferay.portlet.dynamicdatamapping.model.DDMForm form=new com.liferay.portlet.dynamicdatamapping.model.DDMForm();
  form.setAvailableLocales(ddmForm.getAvailableLocales());
  form.setDefaultLocale(ddmForm.getDefaultLocale());
  for (  DDMFormField ddmFormField : ddmForm.getDDMFormFields()) {
    com.liferay.portlet.dynamicdatamapping.model.DDMFormField formField=copyDDMFormField(ddmFormField,form);
    form.addDDMFormField(formField);
  }
  return form;
}
