{
  if (ddmFormLayoutPage == null) {
    return null;
  }
  DDMFormLayoutPage copyDDMFormLayoutPage=new DDMFormLayoutPage();
  copyDDMFormLayoutPage.setTitle(copyLocalizedValue(ddmFormLayoutPage.getTitle()));
  copyDDMFormLayoutPage.setDescription(copyLocalizedValue(ddmFormLayoutPage.getDescription()));
  for (  com.liferay.portlet.dynamicdatamapping.model.DDMFormLayoutRow ddmFormLayoutPageRow : ddmFormLayoutPage.getDDMFormLayoutRows()) {
    copyDDMFormLayoutPage.addDDMFormLayoutRow(copyDDMFormLayoutRow(ddmFormLayoutPageRow));
  }
  return copyDDMFormLayoutPage;
}
