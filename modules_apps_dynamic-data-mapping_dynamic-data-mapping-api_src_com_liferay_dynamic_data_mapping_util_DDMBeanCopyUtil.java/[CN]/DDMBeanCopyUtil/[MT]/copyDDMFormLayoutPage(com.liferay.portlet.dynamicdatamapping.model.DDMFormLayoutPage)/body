{
  if (ddmFormLayoutPage == null) {
    return null;
  }
  DDMFormLayoutPage formLayoutPage=new DDMFormLayoutPage();
  formLayoutPage.setTitle(copyLocalizedValue(ddmFormLayoutPage.getTitle()));
  formLayoutPage.setDescription(copyLocalizedValue(ddmFormLayoutPage.getDescription()));
  for (  com.liferay.portlet.dynamicdatamapping.model.DDMFormLayoutRow ddmFormLayoutPageRow : ddmFormLayoutPage.getDDMFormLayoutRows()) {
    formLayoutPage.addDDMFormLayoutRow(copyDDMFormLayoutRow(ddmFormLayoutPageRow));
  }
  return formLayoutPage;
}
