{
  if (ddmFormLayoutRow == null) {
    return null;
  }
  DDMFormLayoutRow copyDDMFormLayoutRow=new DDMFormLayoutRow();
  for (  com.liferay.portlet.dynamicdatamapping.model.DDMFormLayoutColumn ddmFormLayoutPageColumn : ddmFormLayoutRow.getDDMFormLayoutColumns()) {
    copyDDMFormLayoutRow.addDDMFormLayoutColumn(copyDDMFormLayoutColumn(ddmFormLayoutPageColumn));
  }
  return copyDDMFormLayoutRow;
}
