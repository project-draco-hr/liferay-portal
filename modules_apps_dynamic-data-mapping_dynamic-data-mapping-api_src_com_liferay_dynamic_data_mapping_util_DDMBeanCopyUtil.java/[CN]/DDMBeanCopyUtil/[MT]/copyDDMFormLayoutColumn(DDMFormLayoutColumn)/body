{
  if (ddmFormLayoutColumn == null) {
    return null;
  }
  com.liferay.portlet.dynamicdatamapping.model.DDMFormLayoutColumn formLayoutColumn=new com.liferay.portlet.dynamicdatamapping.model.DDMFormLayoutColumn();
  formLayoutColumn.setDDMFormFieldNames(ddmFormLayoutColumn.getDDMFormFieldNames());
  formLayoutColumn.setSize(ddmFormLayoutColumn.getSize());
  return formLayoutColumn;
}
