{
  if (ddmFormValues == null) {
    return null;
  }
  com.liferay.portlet.dynamicdatamapping.storage.DDMFormValues formValues=new com.liferay.portlet.dynamicdatamapping.storage.DDMFormValues(copyDDMForm(ddmFormValues.getDDMForm()));
  formValues.setAvailableLocales(ddmFormValues.getAvailableLocales());
  formValues.setDefaultLocale(ddmFormValues.getDefaultLocale());
  for (  DDMFormFieldValue ddmFormFieldValue : ddmFormValues.getDDMFormFieldValues()) {
    formValues.addDDMFormFieldValue(copyDDMFormFieldValue(ddmFormFieldValue));
  }
  return formValues;
}
