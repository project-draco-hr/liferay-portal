{
  if (ddmFormLayout == null) {
    return null;
  }
  com.liferay.portlet.dynamicdatamapping.model.DDMFormLayout copyDDMFormLayout=new com.liferay.portlet.dynamicdatamapping.model.DDMFormLayout();
  copyDDMFormLayout.setDefaultLocale(ddmFormLayout.getDefaultLocale());
  for (  DDMFormLayoutPage ddmFormLayoutPage : ddmFormLayout.getDDMFormLayoutPages()) {
    copyDDMFormLayout.addDDMFormLayoutPage(copyDDMFormLayoutPage(ddmFormLayoutPage));
  }
  return copyDDMFormLayout;
}
