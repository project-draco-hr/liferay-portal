{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Image Gallery Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Image Gallery Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//tr[4]/td[1]/a[1]/strong",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//a/strong",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Add Subfolder",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(5000);
  selenium.type("_31_name",RuntimeVariables.replace("Test1 Image1.jpg"));
  selenium.type("_31_description",RuntimeVariables.replace("Image Name Title Folder Test"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have entered invalid data. Please try again."));
  assertTrue(selenium.isTextPresent("Please enter a unique folder name."));
}
