{
  if (format.equals("rss")) {
    addSearchResultRSS(root,groupId,scopeGroupId,entryClassName,entryClassPK,title,link,updated,summary,tags,ratings,score);
  }
 else {
    addSearchResultAtom(root,groupId,scopeGroupId,entryClassName,entryClassPK,title,link,updated,summary,tags,ratings,score);
  }
}
