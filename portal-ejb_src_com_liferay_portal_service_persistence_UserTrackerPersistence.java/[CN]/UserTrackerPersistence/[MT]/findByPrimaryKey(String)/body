{
  Session session=null;
  try {
    session=openSession();
    UserTracker userTracker=(UserTracker)session.get(UserTracker.class,userTrackerId);
    if (userTracker == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No UserTracker exists with the primary key " + userTrackerId.toString());
        throw new NoSuchUserTrackerException("No UserTracker exists with the primary key " + userTrackerId.toString());
      }
    }
    return userTracker;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
