{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(userTracker);
    }
 else {
      if (userTracker.isNew()) {
        session.save(userTracker);
      }
    }
    session.flush();
    userTracker.setNew(false);
    return userTracker;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
