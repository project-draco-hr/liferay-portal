{
  if (!PropsValues.SCHEDULER_ENABLED) {
    return false;
  }
  Scheduler scheduler=getScheduler(storageType);
  try {
    if (scheduler.isShutdown() || scheduler.isInStandbyMode()) {
      return false;
    }
  }
 catch (  org.quartz.SchedulerException e) {
    throw new SchedulerException(e);
  }
  return true;
}
