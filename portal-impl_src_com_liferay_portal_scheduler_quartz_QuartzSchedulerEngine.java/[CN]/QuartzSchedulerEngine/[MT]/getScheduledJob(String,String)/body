{
  if (!PropsValues.SCHEDULER_ENABLED) {
    return null;
  }
  try {
    Scheduler scheduler=getScheduler(groupName);
    jobName=fixMaxLength(jobName,JOB_NAME_MAX_LENGTH);
    groupName=fixMaxLength(getOriginalGroupName(groupName),GROUP_NAME_MAX_LENGTH);
    JobKey jobKey=new JobKey(jobName,groupName);
    return getScheduledJob(scheduler,jobKey);
  }
 catch (  Exception e) {
    throw new SchedulerException("Unable to get job {jobName=" + jobName + ", groupName="+ groupName+ "}",e);
  }
}
