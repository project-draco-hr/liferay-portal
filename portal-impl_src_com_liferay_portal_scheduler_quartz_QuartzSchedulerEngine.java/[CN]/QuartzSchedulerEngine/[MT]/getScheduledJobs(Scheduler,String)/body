{
  groupName=fixMaxLength(getOriginalGroupName(groupName),GROUP_NAME_MAX_LENGTH);
  List<SchedulerResponse> schedulerResponses=new ArrayList<SchedulerResponse>();
  String[] jobNames=scheduler.getJobNames(groupName);
  for (  String jobName : jobNames) {
    SchedulerResponse schedulerResponse=getScheduledJob(scheduler,jobName,groupName);
    if (schedulerResponse != null) {
      schedulerResponses.add(schedulerResponse);
    }
  }
  return schedulerResponses;
}
