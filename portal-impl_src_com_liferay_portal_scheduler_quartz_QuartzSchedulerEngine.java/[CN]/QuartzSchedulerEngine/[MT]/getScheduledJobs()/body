{
  if (!PropsValues.SCHEDULER_ENABLED) {
    return null;
  }
  try {
    String[] groupNames=_permanentScheduler.getJobGroupNames();
    List<SchedulerResponse> schedulerResponses=new ArrayList<SchedulerResponse>();
    for (    String groupName : groupNames) {
      schedulerResponses.addAll(getScheduledJobs(groupName,_permanentScheduler));
    }
    groupNames=_memoryScheduler.getJobGroupNames();
    for (    String groupName : groupNames) {
      schedulerResponses.addAll(getScheduledJobs(groupName,_memoryScheduler));
    }
    return schedulerResponses;
  }
 catch (  Exception e) {
    throw new SchedulerException("Unable to get jobs",e);
  }
}
