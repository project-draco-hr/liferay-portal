{
  WorkflowLink workflowLink=fetchByPrimaryKey(workflowLinkId);
  if (workflowLink == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No WorkflowLink exists with the primary key " + workflowLinkId);
    }
    throw new NoSuchWorkflowLinkException("No WorkflowLink exists with the primary key " + workflowLinkId);
  }
  return workflowLink;
}
