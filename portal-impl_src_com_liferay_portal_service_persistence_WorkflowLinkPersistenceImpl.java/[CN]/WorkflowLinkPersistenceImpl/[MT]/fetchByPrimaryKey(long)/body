{
  WorkflowLink workflowLink=(WorkflowLink)EntityCacheUtil.getResult(WorkflowLinkModelImpl.ENTITY_CACHE_ENABLED,WorkflowLinkImpl.class,workflowLinkId,this);
  if (workflowLink == null) {
    Session session=null;
    try {
      session=openSession();
      workflowLink=(WorkflowLink)session.get(WorkflowLinkImpl.class,new Long(workflowLinkId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (workflowLink != null) {
        cacheResult(workflowLink);
      }
      closeSession(session);
    }
  }
  return workflowLink;
}
