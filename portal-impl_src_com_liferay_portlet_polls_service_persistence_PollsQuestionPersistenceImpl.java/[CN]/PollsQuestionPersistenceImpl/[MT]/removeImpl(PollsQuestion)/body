{
  Session session=null;
  try {
    session=openSession();
    session.delete(pollsQuestion);
    session.flush();
    return pollsQuestion;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(PollsQuestion.class.getName());
  }
}
