{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(pollsQuestion);
    }
  }
  pollsQuestion=removeImpl(pollsQuestion);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(pollsQuestion);
    }
  }
  return pollsQuestion;
}
