{
  PollsQuestion pollsQuestion=(PollsQuestion)EntityCacheUtil.getResult(PollsQuestionModelImpl.ENTITY_CACHE_ENABLED,PollsQuestionImpl.class,questionId,this);
  if (pollsQuestion == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (pollsQuestion == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      pollsQuestion=(PollsQuestion)session.get(PollsQuestionImpl.class,Long.valueOf(questionId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (pollsQuestion == null)) {
        EntityCacheUtil.putResult(PollsQuestionModelImpl.ENTITY_CACHE_ENABLED,PollsQuestionImpl.class,questionId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(pollsQuestion);
      }
      closeSession(session);
    }
  }
  return pollsQuestion;
}
