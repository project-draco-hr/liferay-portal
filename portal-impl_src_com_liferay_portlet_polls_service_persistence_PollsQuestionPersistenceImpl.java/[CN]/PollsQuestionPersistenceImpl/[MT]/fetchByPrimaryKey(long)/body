{
  PollsQuestion result=(PollsQuestion)EntityCacheUtil.getResult(PollsQuestionModelImpl.ENTITY_CACHE_ENABLED,PollsQuestion.class,questionId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      PollsQuestion pollsQuestion=(PollsQuestion)session.get(PollsQuestionImpl.class,new Long(questionId));
      cacheResult(pollsQuestion);
      return pollsQuestion;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (PollsQuestion)result;
  }
}
