{
  PollsQuestion pollsQuestion=(PollsQuestion)EntityCacheUtil.getResult(PollsQuestionModelImpl.ENTITY_CACHE_ENABLED,PollsQuestionImpl.class,questionId,this);
  if (pollsQuestion == null) {
    Session session=null;
    try {
      session=openSession();
      pollsQuestion=(PollsQuestion)session.get(PollsQuestionImpl.class,new Long(questionId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (pollsQuestion != null) {
        cacheResult(pollsQuestion);
      }
      closeSession(session);
    }
  }
  return pollsQuestion;
}
