{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(User.class,User.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("userId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("userId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
