{
  JournalFolder folder=getJournalFolder(classPK);
  List<JournalFolder> folders=JournalFolderLocalServiceUtil.getFolders(folder.getGroupId(),parentContainerModelId,start,end);
  List<ContainerModel> containerModels=new ArrayList<ContainerModel>(folders.size());
  for (  JournalFolder curFolder : folders) {
    containerModels.add(curFolder);
  }
  return containerModels;
}
