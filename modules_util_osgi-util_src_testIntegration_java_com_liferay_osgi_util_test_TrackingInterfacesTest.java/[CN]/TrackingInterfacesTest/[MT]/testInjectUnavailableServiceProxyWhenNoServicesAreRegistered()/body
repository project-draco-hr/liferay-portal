{
  TestInterface testInterface=new TestInterface();
  try (ReflectionServiceTracker reflectionServiceTracker=new ReflectionServiceTracker(testInterface)){
    Assert.assertNotNull(testInterface.getTrackedOne());
    Assert.assertNotNull(testInterface.getTrackedTwo());
    try {
      InterfaceOne interfaceOne=testInterface.getTrackedOne();
      interfaceOne.noop1();
      Assert.fail();
    }
 catch (    UnavailableServiceException use) {
      Assert.assertEquals(InterfaceOne.class,use.getUnavailableServiceClass());
    }
    try {
      InterfaceTwo interfaceTwo=testInterface.getTrackedTwo();
      interfaceTwo.noop2();
      Assert.fail();
    }
 catch (    UnavailableServiceException use) {
      Assert.assertEquals(InterfaceTwo.class,use.getUnavailableServiceClass());
    }
  }
 }
