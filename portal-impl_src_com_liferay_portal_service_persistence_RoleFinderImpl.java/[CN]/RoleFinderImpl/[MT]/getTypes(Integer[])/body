{
  if (types.length == 0) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(types.length * 2);
  sb.append(" AND ");
  for (int i=0; i < types.length; i++) {
    sb.append("Role_.type_ = ?");
    if ((i + 1) < types.length) {
      sb.append(" OR ");
    }
  }
  return sb.toString();
}
