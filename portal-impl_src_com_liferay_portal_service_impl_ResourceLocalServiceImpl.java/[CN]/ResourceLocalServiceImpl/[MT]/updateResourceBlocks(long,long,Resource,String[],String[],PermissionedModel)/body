{
  if (permissionedModel == null) {
    throw new IllegalArgumentException("Permissioned model is null");
  }
  Role role=roleLocalService.getDefaultGroupRole(groupId);
  resourceBlockLocalService.setIndividualScopePermissions(companyId,groupId,resource.getName(),permissionedModel,role.getRoleId(),Arrays.asList(groupPermissions));
  role=roleLocalService.getRole(companyId,RoleConstants.GUEST);
  resourceBlockLocalService.setIndividualScopePermissions(companyId,groupId,resource.getName(),permissionedModel,role.getRoleId(),Arrays.asList(guestPermissions));
}
