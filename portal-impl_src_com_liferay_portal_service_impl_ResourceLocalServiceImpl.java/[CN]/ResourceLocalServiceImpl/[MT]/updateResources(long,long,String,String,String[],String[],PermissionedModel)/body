{
  Resource resource=getResource(companyId,name,ResourceConstants.SCOPE_INDIVIDUAL,primKey);
  if (groupPermissions == null) {
    groupPermissions=new String[0];
  }
  if (guestPermissions == null) {
    guestPermissions=new String[0];
  }
  if (resourceBlockLocalService.isSupported(name)) {
    updateResources_6Blocks(companyId,groupId,resource,groupPermissions,guestPermissions,permissionedModel);
  }
 else {
    updateResources_6(companyId,groupId,resource,groupPermissions,guestPermissions);
  }
}
