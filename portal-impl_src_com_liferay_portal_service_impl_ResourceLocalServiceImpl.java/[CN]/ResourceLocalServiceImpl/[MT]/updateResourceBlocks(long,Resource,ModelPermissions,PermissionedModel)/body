{
  if (permissionedModel == null) {
    throw new IllegalArgumentException("Permissioned model is null");
  }
  for (  Role role : modelPermissions.getRoles()) {
    resourceBlockLocalService.setIndividualScopePermissions(role.getCompanyId(),groupId,resource.getName(),permissionedModel,role.getRoleId(),modelPermissions.getActionIds(role));
  }
}
