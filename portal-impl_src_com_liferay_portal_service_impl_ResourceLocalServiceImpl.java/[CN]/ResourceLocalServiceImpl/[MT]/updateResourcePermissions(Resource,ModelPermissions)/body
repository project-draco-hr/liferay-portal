{
  for (  Role role : modelPermissions.getRoles()) {
    List<String> actionIds=modelPermissions.getActionIds(role);
    String[] actionIdsArray=actionIds.toArray(new String[actionIds.size()]);
    resourcePermissionLocalService.setResourcePermissions(resource.getCompanyId(),resource.getName(),resource.getScope(),resource.getPrimKey(),role.getRoleId(),actionIdsArray);
  }
}
