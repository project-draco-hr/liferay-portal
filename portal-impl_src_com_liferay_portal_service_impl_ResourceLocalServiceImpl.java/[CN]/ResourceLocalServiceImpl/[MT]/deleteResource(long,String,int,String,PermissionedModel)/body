{
  if (PropsValues.PERMISSIONS_USER_CHECK_ALGORITHM == 6) {
    if (resourceBlockLocalService.isSupported(name)) {
      if (permissionedModel == null) {
        throw new IllegalArgumentException("Permissioned model is null");
      }
      resourceBlockLocalService.releasePermissionedModelResourceBlock(permissionedModel);
      return;
    }
 else {
      resourcePermissionLocalService.deleteResourcePermissions(companyId,name,scope,primKey);
      return;
    }
  }
  try {
    Resource resource=getResource(companyId,name,scope,primKey);
    deleteResource(resource.getResourceId());
  }
 catch (  NoSuchResourceException nsre) {
    if (_log.isWarnEnabled()) {
      _log.warn(nsre);
    }
  }
}
