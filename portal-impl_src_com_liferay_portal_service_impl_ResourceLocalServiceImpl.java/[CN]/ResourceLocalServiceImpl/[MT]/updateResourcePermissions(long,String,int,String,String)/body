{
  List<ResourcePermission> resourcePermissions=resourcePermissionLocalService.getResourcePermissions(companyId,name,scope,primKey);
  for (  ResourcePermission resourcePermission : resourcePermissions) {
    resourcePermission.setPrimKey(newPrimKey);
    resourcePermissionPersistence.update(resourcePermission);
  }
  PermissionCacheUtil.clearResourcePermissionCache(name,primKey);
}
