{
  try {
    if (_folder == null) {
      return false;
    }
    ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
    if (BookmarksFolderPermissionChecker.contains(themeDisplay.getPermissionChecker(),_folder,ActionKeys.DELETE)) {
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
