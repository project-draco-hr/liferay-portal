{
  entityCache.putResult(KaleoDefinitionModelImpl.ENTITY_CACHE_ENABLED,KaleoDefinitionImpl.class,kaleoDefinition.getPrimaryKey(),kaleoDefinition);
  finderCache.putResult(FINDER_PATH_FETCH_BY_C_N_V,new Object[]{kaleoDefinition.getCompanyId(),kaleoDefinition.getName(),kaleoDefinition.getVersion()},kaleoDefinition);
  kaleoDefinition.resetOriginalValues();
}
