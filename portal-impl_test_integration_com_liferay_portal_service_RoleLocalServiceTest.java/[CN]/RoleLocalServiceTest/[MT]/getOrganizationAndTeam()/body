{
  User user=TestPropsValues.getUser();
  _organization=OrganizationLocalServiceUtil.addOrganization(user.getUserId(),OrganizationConstants.DEFAULT_PARENT_ORGANIZATION_ID,RandomTestUtil.randomString(),false);
  Team team=TeamLocalServiceUtil.addTeam(user.getUserId(),_organization.getGroupId(),RandomTestUtil.randomString(),null,new ServiceContext());
  return new Object[]{_organization,team};
}
