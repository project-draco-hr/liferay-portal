{
  User user=TestPropsValues.getUser();
  Organization organization=OrganizationLocalServiceUtil.addOrganization(user.getUserId(),OrganizationConstants.DEFAULT_PARENT_ORGANIZATION_ID,RandomTestUtil.randomString(),false);
  Team team=TeamLocalServiceUtil.addTeam(user.getUserId(),organization.getGroupId(),RandomTestUtil.randomString(),null);
  return new Object[]{organization,team};
}
