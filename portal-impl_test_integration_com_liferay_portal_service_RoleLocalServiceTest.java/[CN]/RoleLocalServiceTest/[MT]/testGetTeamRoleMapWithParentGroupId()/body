{
  Object[] objects=prepareOrganization();
  Organization organization=(Organization)objects[0];
  Team team=(Team)objects[1];
  Layout layout=LayoutTestUtil.addLayout(organization.getGroupId(),ServiceTestUtil.randomString());
  Group group=GroupTestUtil.addGroup(TestPropsValues.getUserId(),organization.getGroupId(),layout);
  Map<Team,Role> teamRoleMap=RoleLocalServiceUtil.getTeamRoleMap(group.getGroupId());
  doTestGetTeamRoleMap(teamRoleMap,team,true);
}
