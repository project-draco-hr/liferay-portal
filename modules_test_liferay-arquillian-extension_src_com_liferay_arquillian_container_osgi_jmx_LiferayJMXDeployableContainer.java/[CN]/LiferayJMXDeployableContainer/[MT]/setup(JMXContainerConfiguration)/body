{
  config.setAutostartBundle(true);
  config.setJmxPassword(GetterUtil.getString(PropsUtil.get("jmx.password")));
  config.setJmxServiceURL(GetterUtil.getString(PropsUtil.get("jmx.url"),DEFAULT_JMX_SERVICE_URL));
  config.setJmxUsername(GetterUtil.getString(PropsUtil.get("jmx.username")));
  super.setup(config);
}
