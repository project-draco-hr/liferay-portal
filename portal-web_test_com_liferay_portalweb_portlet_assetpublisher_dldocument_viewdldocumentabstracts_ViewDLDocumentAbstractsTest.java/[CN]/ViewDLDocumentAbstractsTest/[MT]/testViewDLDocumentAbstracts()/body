{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("AP DL Document Title"),selenium.getText("//div[1]/h3/a"));
  assertEquals(RuntimeVariables.replace("AP DL Document Title"),selenium.getText("//div[2]/div[1]/div/a"));
  assertTrue(selenium.isPartialText("//div[2]/a","Read More"));
  selenium.clickAt("//div[2]/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("AP DL Document Title"),selenium.getText("//div/h3"));
  assertEquals(RuntimeVariables.replace("AP DL Document Title"),selenium.getText("//div[2]/div[1]/a"));
  assertEquals(RuntimeVariables.replace("View in Context \u00bb"),selenium.getText("//div[2]/a"));
}
