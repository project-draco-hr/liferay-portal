{
  PortletURL portletURL=PortletURLFactoryUtil.create(portletRequest,PortletKeys.BLOGS,themeDisplay.getPlid(),PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/blogs/edit_entry");
  String currentURL=PortalUtil.getCurrentURL(portletRequest);
  portletURL.setParameter("redirect",currentURL);
  portletURL.setParameter("backURL",currentURL);
  URLMenuItem urlMenuItem=new URLMenuItem();
  urlMenuItem.setURL(portletURL.toString());
  urlMenuItem.setLabel(LanguageUtil.get(themeDisplay.getLocale(),"add-entry"));
  return urlMenuItem;
}
