{
  if (valid) {
    for (    String hostName : hostNames) {
      if (!Validator.isHostName(hostName)) {
        Assert.fail(hostName);
      }
    }
  }
 else {
    for (    String hostName : hostNames) {
      if (Validator.isHostName(hostName)) {
        Assert.fail(hostName);
      }
    }
  }
}
