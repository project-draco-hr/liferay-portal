{
  if (valid) {
    for (    String emailAddress : emailAddresses) {
      if (!Validator.isEmailAddress(emailAddress)) {
        Assert.fail(emailAddress);
      }
    }
  }
 else {
    for (    String emailAddress : emailAddresses) {
      if (Validator.isEmailAddress(emailAddress)) {
        Assert.fail(emailAddress);
      }
    }
  }
}
