{
  if (valid) {
    for (    String iPv4Address : iPv4Addresses) {
      if (!Validator.isIPv4Address(iPv4Address)) {
        Assert.fail(iPv4Address);
      }
    }
  }
 else {
    for (    String iPv4Address : iPv4Addresses) {
      if (Validator.isIPv4Address(iPv4Address)) {
        Assert.fail(iPv4Address);
      }
    }
  }
}
