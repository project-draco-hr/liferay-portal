{
  if (valid) {
    for (    String filePath : filePaths) {
      if (!Validator.isFilePath(filePath,isParentDirAllowed)) {
        Assert.fail(filePath);
      }
    }
  }
 else {
    for (    String filePath : filePaths) {
      if (Validator.isFilePath(filePath,isParentDirAllowed)) {
        Assert.fail(filePath);
      }
    }
  }
}
