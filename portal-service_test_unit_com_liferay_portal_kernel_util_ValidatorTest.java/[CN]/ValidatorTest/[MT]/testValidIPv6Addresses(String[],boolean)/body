{
  if (valid) {
    for (    String iPv6Address : iPv6Addresses) {
      if (!Validator.isIPv6Address(iPv6Address)) {
        Assert.fail(iPv6Address);
      }
    }
  }
 else {
    for (    String iPv6Address : iPv6Addresses) {
      if (Validator.isIPv6Address(iPv6Address)) {
        Assert.fail(iPv6Address);
      }
    }
  }
}
