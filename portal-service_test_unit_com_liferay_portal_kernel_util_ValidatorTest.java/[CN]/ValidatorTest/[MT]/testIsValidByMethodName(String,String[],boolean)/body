{
  Method method=ReflectionUtil.getDeclaredMethod(Validator.class,methodName,String.class);
  if (valid) {
    for (    String param : params) {
      if (!(Boolean)method.invoke(null,param)) {
        Assert.fail(param);
      }
    }
  }
 else {
    for (    String param : params) {
      if ((Boolean)method.invoke(null,param)) {
        Assert.fail(param);
      }
    }
  }
}
