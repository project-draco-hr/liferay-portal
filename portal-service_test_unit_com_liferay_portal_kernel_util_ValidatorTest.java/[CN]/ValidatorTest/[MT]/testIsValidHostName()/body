{
  String[] validHostNames={"localhost","127.0.0.1","10.10.10.1","abc.com","9to5.net","liferay.com","www.liferay.com","www.liferay.co.uk","::1","[abcd:1234:ef01:2345:6789:0123:4567]"};
  for (  String validHostName : validHostNames) {
    if (!Validator.isHostName(validHostName)) {
      Assert.fail(validHostName);
    }
  }
  String[] invalidHostNames={"(999.999.999)","123_456_789_012","www.$dollar$.com","{abcd:1234:ef01:2345:6789:0123:4567}"};
  for (  String invalidHostName : invalidHostNames) {
    if (Validator.isHostName(invalidHostName)) {
      Assert.fail(invalidHostName);
    }
  }
}
