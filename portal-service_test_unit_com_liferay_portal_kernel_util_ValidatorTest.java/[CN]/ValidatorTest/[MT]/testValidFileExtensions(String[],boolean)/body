{
  if (valid) {
    for (    String fileExtension : fileExtensions) {
      if (!Validator.isFileExtension(fileExtension)) {
        Assert.fail(fileExtension);
      }
    }
  }
 else {
    for (    String fileExtension : fileExtensions) {
      if (Validator.isFileExtension(fileExtension)) {
        Assert.fail(fileExtension);
      }
    }
  }
}
