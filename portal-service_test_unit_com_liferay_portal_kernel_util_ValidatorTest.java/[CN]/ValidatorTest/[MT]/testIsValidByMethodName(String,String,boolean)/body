{
  Method method=ReflectionUtil.getDeclaredMethod(Validator.class,methodName,String.class);
  Boolean result=(Boolean)method.invoke(null,param);
  if (valid) {
    if (!result.booleanValue()) {
      Assert.fail(param);
    }
  }
 else {
    if (result.booleanValue()) {
      Assert.fail(param);
    }
  }
}
