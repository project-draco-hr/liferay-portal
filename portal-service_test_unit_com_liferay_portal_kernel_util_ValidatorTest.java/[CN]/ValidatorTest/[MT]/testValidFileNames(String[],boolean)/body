{
  if (valid) {
    for (    String fileName : fileNames) {
      if (!Validator.isFileName(fileName)) {
        Assert.fail(fileName);
      }
    }
  }
 else {
    for (    String fileName : fileNames) {
      if (Validator.isFileName(fileName)) {
        Assert.fail(fileName);
      }
    }
  }
}
