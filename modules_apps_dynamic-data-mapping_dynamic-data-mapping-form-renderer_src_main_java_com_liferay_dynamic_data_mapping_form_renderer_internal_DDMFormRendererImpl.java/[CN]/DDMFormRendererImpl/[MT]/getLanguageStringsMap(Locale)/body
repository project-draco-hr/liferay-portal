{
  Map<String,String> stringsMap=new HashMap<>();
  ResourceBundle portalResourceBundle=ResourceBundleUtil.getBundle("content.Language",locale,PortalClassLoaderUtil.getClassLoader());
  ResourceBundle resourceBundle=new AggregateResourceBundle(portalResourceBundle,ResourceBundleUtil.getBundle("content.Language",locale,getClass()));
  stringsMap.put("next",LanguageUtil.get(resourceBundle,"next"));
  stringsMap.put("previous",LanguageUtil.get(resourceBundle,"previous"));
  stringsMap.put("submit",LanguageUtil.get(resourceBundle,"submit"));
  return stringsMap;
}
