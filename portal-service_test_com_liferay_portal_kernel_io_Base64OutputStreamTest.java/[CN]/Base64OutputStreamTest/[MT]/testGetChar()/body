{
  try {
    File testFile=new File(_filePath);
    int sixbit=0;
    Base64OutputStream instance=new Base64OutputStream(new FileOutputStream(testFile));
    char expResult='A';
    char result=instance.getChar(sixbit);
    assertEquals(expResult,result);
    sixbit=64;
    expResult='?';
    result=instance.getChar(sixbit);
    assertEquals(expResult,result);
    testFile.delete();
  }
 catch (  Exception e) {
    fail("Test getChar failed");
  }
}
