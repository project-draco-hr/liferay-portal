{
  try {
    File testFile=new File(_TEST_FILE_NAME);
    Base64OutputStream base64OutputStream=new Base64OutputStream(new FileOutputStream(testFile));
    assertEquals('A',base64OutputStream.getChar(0));
    assertEquals('?',base64OutputStream.getChar(64));
    testFile.delete();
  }
 catch (  Exception e) {
    fail(e.getMessage());
  }
}
