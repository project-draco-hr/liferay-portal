{
  try {
    if (PropsValues.CLUSTER_LINK_ENABLED) {
      ClusterExecutorUtil.removeClusterEventListener(_memorySchedulerClusterEventListener);
      LockLocalServiceUtil.unlock(SchedulerEngine.class.getName(),SchedulerEngine.class.getName(),_localClusterNodeId,PropsValues.MEMORY_CLUSTER_SCHEDULER_LOCK_CACHE_ENABLED);
    }
  }
 catch (  Exception e) {
    throw new SchedulerException("Unable to shutdown scheduler",e);
  }
  _schedulerEngine.shutdown();
}
