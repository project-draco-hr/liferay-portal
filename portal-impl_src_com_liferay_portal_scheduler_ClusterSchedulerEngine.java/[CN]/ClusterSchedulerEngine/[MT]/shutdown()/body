{
  _portalReady=false;
  try {
    ClusterExecutorUtil.removeClusterEventListener(_clusterEventListener);
    LockLocalServiceUtil.unlock(_LOCK_CLASS_NAME,_LOCK_CLASS_NAME,_localClusterNodeAddress,PropsValues.MEMORY_CLUSTER_SCHEDULER_LOCK_CACHE_ENABLED);
  }
 catch (  Exception e) {
    throw new SchedulerException("Unable to shutdown scheduler",e);
  }
  _schedulerEngine.shutdown();
}
