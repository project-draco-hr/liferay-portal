{
  try {
    if (isMemorySchedulerSlave(groupName)) {
      removeMemoryClusteredJobs(groupName);
      return;
    }
  }
 catch (  Exception e) {
    throw new SchedulerException("Unable to unschedule jobs in group " + groupName,e);
  }
  _readLock.lock();
  try {
    _schedulerEngine.unschedule(groupName);
  }
  finally {
    _readLock.unlock();
  }
  skipClusterInvoking(groupName);
}
