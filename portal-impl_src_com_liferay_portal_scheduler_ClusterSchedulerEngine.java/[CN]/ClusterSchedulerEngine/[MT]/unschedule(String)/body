{
  boolean isMemoryClusteredSlaveJob=isMemoryClusteredSlaveJob(groupName);
  _readLock.lock();
  try {
    if (isMemoryClusteredSlaveJob) {
      removeMemoryClusteredJobs(groupName);
      return;
    }
    _schedulerEngine.unschedule(groupName);
  }
  finally {
    _readLock.unlock();
  }
  skipClusterInvoking(groupName);
}
