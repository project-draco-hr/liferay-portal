{
  if (_localClusterNodeId == null) {
    ClusterNode clusterNode=ClusterExecutorUtil.getLocalClusterNode();
    _localClusterNodeId=clusterNode.getClusterNodeId();
  }
  Lock lock=LockLocalServiceUtil.lock(SchedulerEngine.class.getName(),SchedulerEngine.class.getName(),oldOwner,_localClusterNodeId,PropsValues.MEMORY_CLUSTER_SCHEDULER_LOCK_CACHE_ENABLED,replaceOldLock);
  return lock;
}
