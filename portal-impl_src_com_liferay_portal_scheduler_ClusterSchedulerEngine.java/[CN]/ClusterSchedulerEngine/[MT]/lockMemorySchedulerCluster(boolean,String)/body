{
  if (_localClusterNodeAddress == null) {
    _localClusterNodeAddress=getSerializedString(ClusterExecutorUtil.getLocalClusterNodeAddress());
  }
  Lock lock=LockLocalServiceUtil.lock(SchedulerEngine.class.getName(),SchedulerEngine.class.getName(),oldOwner,_localClusterNodeAddress,PropsValues.MEMORY_CLUSTER_SCHEDULER_LOCK_CACHE_ENABLED,replaceOldLock);
  return lock;
}
