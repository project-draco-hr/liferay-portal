{
  if (storageType.equals(StorageType.MEMORY_CLUSTERED)) {
    String masterAddressString=getMasterAddressString(false);
    if (!_localClusterNodeAddress.equals(masterAddressString)) {
      return callMaster(masterAddressString,_getScheduledJobMethodKey,jobName,groupName,storageType);
    }
  }
  _readLock.lock();
  try {
    return _schedulerEngine.getScheduledJob(jobName,groupName,storageType);
  }
  finally {
    _readLock.unlock();
  }
}
