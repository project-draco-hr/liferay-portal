{
  Set<Map.Entry<String,ObjectValuePair<SchedulerResponse,TriggerState>>> memorySingleInstanceJobs=_memorySingleInstanceJobs.entrySet();
  Iterator<Map.Entry<String,ObjectValuePair<SchedulerResponse,TriggerState>>> itr=memorySingleInstanceJobs.iterator();
  while (itr.hasNext()) {
    Map.Entry<String,ObjectValuePair<SchedulerResponse,TriggerState>> entry=itr.next();
    ObjectValuePair<SchedulerResponse,TriggerState> memorySingleInstanceJob=entry.getValue();
    SchedulerResponse schedulerResponse=memorySingleInstanceJob.getKey();
    if (groupName.equals(schedulerResponse.getGroupName())) {
      itr.remove();
    }
  }
}
