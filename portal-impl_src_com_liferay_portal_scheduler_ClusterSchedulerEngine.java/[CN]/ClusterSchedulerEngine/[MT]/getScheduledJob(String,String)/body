{
  if (!PropsValues.SCHEDULER_ENABLED) {
    return null;
  }
  try {
    if (isMemorySchedulerSlave(groupName)) {
      return (SchedulerResponse)callMaster(_getScheduledJobMethodKey,jobName,groupName);
    }
  }
 catch (  Exception e) {
    throw new SchedulerException("Unable to get job {jobName=" + jobName + ", groupName="+ groupName+ "}",e);
  }
  _readLock.lock();
  try {
    return _schedulerEngine.getScheduledJob(jobName,groupName);
  }
  finally {
    _readLock.unlock();
  }
}
