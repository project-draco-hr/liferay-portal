{
  if (!PropsValues.SCHEDULER_ENABLED) {
    return;
  }
  try {
    if (isMemorySchedulerSlave(groupName)) {
      return;
    }
  }
 catch (  Exception e) {
    throw new SchedulerException("Unable to suppress error for job {jobName=" + jobName + ", groupName="+ groupName+ "}",e);
  }
  _readLock.lock();
  try {
    _schedulerEngine.suppressError(jobName,groupName);
  }
  finally {
    _readLock.unlock();
  }
  skipClusterInvoking(groupName);
}
