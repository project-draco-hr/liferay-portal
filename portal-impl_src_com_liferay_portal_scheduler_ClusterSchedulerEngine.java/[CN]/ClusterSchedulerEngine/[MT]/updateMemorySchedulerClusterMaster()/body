{
  try {
    Lock lock=lockMemorySchedulerCluster(false,null);
    Address masterAddress=(Address)getDeserializedObject(lock.getOwner());
    if (ClusterExecutorUtil.isClusterNodeAlive(masterAddress)) {
      return;
    }
    lockMemorySchedulerCluster(true,lock.getOwner());
  }
 catch (  Exception e) {
    throw new SchedulerException("Unable to update memory scheduler cluster master",e);
  }
}
