{
  if (_localClusterNodeAddress == null) {
    _localClusterNodeAddress=getSerializedString(ClusterExecutorUtil.getLocalClusterNodeAddress());
  }
  Lock lock=null;
  if (expectOwner == null) {
    lock=LockLocalServiceUtil.lock(_classNameForLock,_classNameForLock,_localClusterNodeAddress,PropsValues.MEMORY_CLUSTER_SCHEDULER_LOCK_CACHE_ENABLED);
  }
 else {
    lock=LockLocalServiceUtil.lock(_classNameForLock,_classNameForLock,expectOwner,_localClusterNodeAddress,PropsValues.MEMORY_CLUSTER_SCHEDULER_LOCK_CACHE_ENABLED);
  }
  return lock;
}
