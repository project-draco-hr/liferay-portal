{
  String groupName=trigger.getGroupName();
  String jobName=trigger.getJobName();
  boolean isMemoryClusteredSlaveJob=isMemoryClusteredSlaveJob(groupName);
  _readLock.lock();
  try {
    if (isMemoryClusteredSlaveJob) {
      SchedulerResponse schedulerResponse=new SchedulerResponse();
      schedulerResponse.setDescription(description);
      schedulerResponse.setDestinationName(destinationName);
      schedulerResponse.setGroupName(groupName);
      schedulerResponse.setJobName(jobName);
      schedulerResponse.setMessage(message);
      schedulerResponse.setTrigger(trigger);
      _memoryClusteredJobs.put(getFullName(jobName,groupName),new ObjectValuePair<SchedulerResponse,TriggerState>(schedulerResponse,TriggerState.NORMAL));
      return;
    }
    _schedulerEngine.schedule(trigger,description,destinationName,message);
  }
  finally {
    _readLock.unlock();
  }
  skipClusterInvoking(groupName);
}
