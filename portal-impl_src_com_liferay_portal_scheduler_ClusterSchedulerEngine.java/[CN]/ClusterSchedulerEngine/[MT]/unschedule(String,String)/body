{
  boolean isMemoryClusteredSlaveJob=isMemoryClusteredSlaveJob(groupName);
  _readLock.lock();
  try {
    if (isMemoryClusteredSlaveJob) {
      _memoryClusteredJobs.remove(getFullName(jobName,groupName));
      return;
    }
    _schedulerEngine.unschedule(jobName,groupName);
  }
  finally {
    _readLock.unlock();
  }
  skipClusterInvoking(groupName);
}
