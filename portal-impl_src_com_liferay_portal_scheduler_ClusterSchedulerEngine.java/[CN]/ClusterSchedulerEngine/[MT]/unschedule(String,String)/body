{
  try {
    if (isMemorySchedulerSlave(groupName)) {
      _memorySingleInstanceJobs.remove(getFullName(jobName,groupName));
      return;
    }
  }
 catch (  Exception e) {
    throw new SchedulerException("Unable to unschedule job {jobName=" + jobName + ", groupName="+ groupName+ "}",e);
  }
  _readLock.lock();
  try {
    _schedulerEngine.unschedule(jobName,groupName);
  }
  finally {
    _readLock.unlock();
  }
  skipClusterInvoking(groupName);
}
