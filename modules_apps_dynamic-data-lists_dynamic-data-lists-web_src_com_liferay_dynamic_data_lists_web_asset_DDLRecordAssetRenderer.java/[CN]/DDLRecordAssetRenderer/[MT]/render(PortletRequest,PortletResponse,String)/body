{
  if (template.equals(TEMPLATE_ABSTRACT) || template.equals(TEMPLATE_FULL_CONTENT)) {
    portletRequest.setAttribute(DDLWebKeys.DYNAMIC_DATA_LISTS_RECORD,_record);
    portletRequest.setAttribute(DDLWebKeys.DYNAMIC_DATA_LISTS_RECORD_VERSION,_recordVersion);
    return "/asset/full_content.jsp";
  }
 else {
    return null;
  }
}
