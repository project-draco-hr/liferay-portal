{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),PortletKeys.DYNAMIC_DATA_LISTS,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/dynamic_data_lists/edit_record");
  portletURL.setParameter("recordId",String.valueOf(_record.getRecordId()));
  portletURL.setParameter("version",_recordVersion.getVersion());
  return portletURL;
}
