  if (effectiveNamePtr > 0) {
    decodeUnicodeString(buffer,_effectiveName);
  }
  if (fullNamePtr > 0) {
    decodeUnicodeString(buffer,_fullName);
  }
  if (logonScriptPtr > 0) {
