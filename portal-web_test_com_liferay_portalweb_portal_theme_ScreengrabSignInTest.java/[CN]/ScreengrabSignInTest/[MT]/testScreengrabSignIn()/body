{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.waitForElementPresent("link=Sign Out");
  selenium.click(RuntimeVariables.replace("link=Sign Out"));
  selenium.waitForPageToLoad("30000");
  selenium.open("web/themetesting/testpage01");
  selenium.type("_58_login",RuntimeVariables.replace("test@liferay.com"));
  selenium.type("_58_password",RuntimeVariables.replace("test"));
  selenium.click("//input[@type='checkbox']");
  FileUtil.mkdirs(RuntimeVariables.replace("L:\\portal\\build\\portal-web\\test-output\\brochure\\"));
  selenium.captureEntirePageScreenshot(RuntimeVariables.replace("L:\\portal\\build\\portal-web\\test-output\\brochure\\ScreengrabTest01.jpg"),"");
  selenium.click(RuntimeVariables.replace("//input[@value='Sign In']"));
  selenium.waitForPageToLoad("30000");
}
