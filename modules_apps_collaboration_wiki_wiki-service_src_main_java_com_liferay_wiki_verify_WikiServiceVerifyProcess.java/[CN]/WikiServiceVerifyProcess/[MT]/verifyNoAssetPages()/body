{
  try (LoggingTimer loggingTimer=new LoggingTimer()){
    List<WikiPage> pages=_wikiPageLocalService.getNoAssetPages();
    if (_log.isDebugEnabled()) {
      _log.debug("Processing " + pages.size() + " pages with no asset");
    }
    for (    WikiPage page : pages) {
      try {
        _wikiPageLocalService.updateAsset(page.getUserId(),page,null,null,null,null);
      }
 catch (      Exception e) {
        if (_log.isWarnEnabled()) {
          _log.warn("Unable to update asset for page " + page.getPageId() + ": "+ e.getMessage());
        }
      }
    }
    if (_log.isDebugEnabled()) {
      _log.debug("Assets verified for pages");
    }
  }
 }
