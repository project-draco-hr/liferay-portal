{
  SassCompiler sassCompiler=new RubySassCompiler();
  String expectedOutput="@charset \"UTF-8\";.foo { content: '\\f105'; }";
  String actualOutput=sassCompiler.compileString(".foo { $icon-var: '\\f105'; content: '#{$icon-var}'; }","");
  Assert.assertEquals(stripNewLines(expectedOutput),stripNewLines(actualOutput));
}
