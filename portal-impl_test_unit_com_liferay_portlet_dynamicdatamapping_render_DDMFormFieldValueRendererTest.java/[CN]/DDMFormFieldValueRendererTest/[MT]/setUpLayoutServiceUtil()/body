{
  mockStatic(LayoutServiceUtil.class);
  when(LayoutServiceUtil.getLayoutName(Matchers.anyLong(),Matchers.anyBoolean(),Matchers.anyLong(),Matchers.eq("en_US"))).thenReturn("Layout Name");
  when(LayoutServiceUtil.getLayoutName(Matchers.anyLong(),Matchers.anyBoolean(),Matchers.anyLong(),Matchers.eq("pt_BR"))).thenReturn("Nome da Pagina");
}
