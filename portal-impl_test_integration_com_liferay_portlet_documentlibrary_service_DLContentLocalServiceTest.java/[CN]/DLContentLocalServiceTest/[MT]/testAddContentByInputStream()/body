{
  long companyId=RandomTestUtil.nextLong();
  long repositoryId=RandomTestUtil.nextLong();
  String path=RandomTestUtil.randomString();
  DLContent addDLContent=_dlContentLocalService.addContent(companyId,repositoryId,path,Store.VERSION_DEFAULT,new ByteArrayInputStream(_DATA_VERSION_1),1024);
  DLContent getDLContent=_dlContentLocalService.getContent(companyId,repositoryId,path);
  Assert.assertEquals(addDLContent,getDLContent);
}
