{
  long companyId=nextLong();
  long repositoryId=nextLong();
  String path=randomString();
  _dlContentLocalService.addContent(companyId,repositoryId,path,Store.VERSION_DEFAULT,_DATA_VERSION_1);
  assertTrue(_dlContentLocalService.hasContent(companyId,repositoryId,path,Store.VERSION_DEFAULT));
  _dlContentLocalService.deleteContent(companyId,repositoryId,path,Store.VERSION_DEFAULT);
  assertFalse(_dlContentLocalService.hasContent(companyId,repositoryId,path,Store.VERSION_DEFAULT));
}
