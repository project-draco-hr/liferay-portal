{
  String path=RandomTestUtil.randomString();
  DLContent addDLContent1=_dlContentLocalService.addContent(_companyId,_repositoryId,path,Store.VERSION_DEFAULT,_DATA_VERSION_1);
  DLContent getDLContent1=_dlContentLocalService.getContent(_companyId,_repositoryId,path);
  Assert.assertEquals(addDLContent1,getDLContent1);
  DLContent addDLContent2=_dlContentLocalService.addContent(_companyId,_repositoryId,path,"1.1",_DATA_VERSION_2);
  DLContent getDLContent2=_dlContentLocalService.getContent(_companyId,_repositoryId,path);
  Assert.assertEquals(addDLContent2,getDLContent2);
}
