{
  long companyId=nextLong();
  long repositoryId=nextLong();
  String path=randomString();
  DLContent addDLContent1=_dlContentLocalService.addContent(companyId,repositoryId,path,Store.VERSION_DEFAULT,_DATA_VERSION_1);
  DLContent getDLContent1=_dlContentLocalService.getContent(companyId,repositoryId,path);
  assertEquals(addDLContent1,getDLContent1);
  DLContent addDLContent2=_dlContentLocalService.addContent(companyId,repositoryId,path,"1.1",_DATA_VERSION_2);
  DLContent getDLContent2=_dlContentLocalService.getContent(companyId,repositoryId,path);
  assertEquals(addDLContent2,getDLContent2);
}
