{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select id_, content from JournalArticle where companyId = " + companyId + " and structureId is NULL or structureId "+ "LIKE ''");
    String name=addBasicWebContentStructureAndTemplate(companyId);
    rs=ps.executeQuery();
    while (rs.next()) {
      long id_=rs.getLong("id_");
      String content=rs.getString("content");
      updateJournalArticle(id_,name,name,content);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
