{
  try {
    if (_writer != null) {
      _writer.close();
    }
 else     if (_stream != null) {
      _stream.close();
    }
  }
 catch (  IOException e) {
  }
  if (_unsyncByteArrayOutputStream != null) {
    _response.setContentLength(_unsyncByteArrayOutputStream.size());
    _unsyncByteArrayOutputStream.writeTo(_response.getOutputStream());
  }
}
