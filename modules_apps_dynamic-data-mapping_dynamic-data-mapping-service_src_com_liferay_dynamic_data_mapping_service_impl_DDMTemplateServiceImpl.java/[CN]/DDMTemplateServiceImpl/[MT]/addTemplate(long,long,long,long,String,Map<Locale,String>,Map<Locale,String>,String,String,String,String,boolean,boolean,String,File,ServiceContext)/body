{
  DDMTemplatePermission.checkAddTemplatePermission(getPermissionChecker(),groupId,classNameId,resourceClassNameId);
  return ddmTemplateLocalService.addTemplate(getUserId(),groupId,classNameId,classPK,resourceClassNameId,templateKey,nameMap,descriptionMap,type,mode,language,script,cacheable,smallImage,smallImageURL,smallImageFile,serviceContext);
}
