{
  String rootDir=WebDirDetector.getRootDir(PortalClassLoaderUtil.getClassLoader());
  _rubyScriptFile=new File(rootDir + "WEB-INF/sass/main.rb");
  try {
    _rubyExecutor.eval(null,new HashMap<String,Object>(),null,"require 'rubygems'\nrequire 'sass'");
  }
 catch (  ScriptingException se) {
    _log.error(se,se);
  }
}
