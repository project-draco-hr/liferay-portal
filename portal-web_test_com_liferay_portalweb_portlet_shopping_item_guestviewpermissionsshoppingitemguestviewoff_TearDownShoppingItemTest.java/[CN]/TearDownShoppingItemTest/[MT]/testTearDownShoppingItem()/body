case 1:
      selenium.open("/web/guest/home/");
    for (int second=0; ; second++) {
      if (second >= 90) {
        fail("timeout");
      }
      try {
        if (selenium.isVisible("link=Shopping Test Page")) {
          break;
        }
      }
 catch (      Exception e) {
      }
      Thread.sleep(1000);
    }
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace("Shopping Test Page"));
selenium.waitForPageToLoad("30000");
boolean shoppingItem1Present=selenium.isElementPresent("//td[2]/a");
if (!shoppingItem1Present) {
label=2;
continue;
}
assertEquals(RuntimeVariables.replace("Actions"),selenium.getText("//td[5]/span/ul/li/strong/a/span"));
selenium.clickAt("//td[5]/span/ul/li/strong/a/span",RuntimeVariables.replace("Actions"));
for (int second=0; ; second++) {
if (second >= 90) {
fail("timeout");
}
