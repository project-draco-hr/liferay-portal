{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=All Pages",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Page']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_36_title",RuntimeVariables.replace("Wiki Page Test"));
  selenium.type("_36_content",RuntimeVariables.replace("This is a wiki page test."));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//span[@class='aui-icon-search aui-icon']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have entered invalid data. Please try again."));
  assertTrue(selenium.isTextPresent("There is already a page with the specified title."));
}
