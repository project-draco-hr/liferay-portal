{
  try {
    return ResourceBundle.getBundle(getResourceBundleBaseName(_clazz),locale,_clazz.getClassLoader(),UTF8Control.INSTANCE);
  }
 catch (  Exception e) {
    return ResourceBundle.getBundle(getResourceBundleBaseName(_clazz),locale,PortalClassLoaderUtil.getClassLoader(),UTF8Control.INSTANCE);
  }
}
