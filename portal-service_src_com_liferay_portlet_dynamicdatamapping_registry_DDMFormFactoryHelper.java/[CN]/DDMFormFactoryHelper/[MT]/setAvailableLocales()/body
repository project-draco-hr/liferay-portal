{
  if (Validator.isNull(_ddmForm.availableLanguageIds())) {
    _availableLocales=LanguageUtil.getAvailableLocales();
    return;
  }
  _availableLocales=new TreeSet<>();
  for (  String availableLanguageId : StringUtil.split(_ddmForm.availableLanguageIds())) {
    _availableLocales.add(LocaleUtil.fromLanguageId(availableLanguageId));
  }
}
