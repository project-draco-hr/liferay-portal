{
  DDMListItem ddmListItem=(DDMListItem)EntityCacheUtil.getResult(DDMListItemModelImpl.ENTITY_CACHE_ENABLED,DDMListItemImpl.class,listItemId,this);
  if (ddmListItem == null) {
    Session session=null;
    try {
      session=openSession();
      ddmListItem=(DDMListItem)session.get(DDMListItemImpl.class,Long.valueOf(listItemId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (ddmListItem != null) {
        cacheResult(ddmListItem);
      }
      closeSession(session);
    }
  }
  return ddmListItem;
}
