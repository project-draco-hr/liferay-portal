{
  long[] folderIds=null;
  long folderId=ParamUtil.getLong(actionRequest,"folderId");
  if (folderId > 0) {
    folderIds=new long[]{folderId};
  }
 else {
    folderIds=StringUtil.split(ParamUtil.getString(actionRequest,"folderIds"),0L);
  }
  long parentFolderId=ParamUtil.getLong(actionRequest,"parentFolderId");
  ServiceContext serviceContext=ServiceContextFactory.getInstance(DLFileEntry.class.getName(),actionRequest);
  for (  long moveFolderId : folderIds) {
    if (moveFromTrash) {
      DLAppServiceUtil.moveFolderFromTrash(moveFolderId,parentFolderId,serviceContext);
    }
 else {
      DLAppServiceUtil.moveFolder(moveFolderId,parentFolderId,serviceContext);
    }
  }
}
