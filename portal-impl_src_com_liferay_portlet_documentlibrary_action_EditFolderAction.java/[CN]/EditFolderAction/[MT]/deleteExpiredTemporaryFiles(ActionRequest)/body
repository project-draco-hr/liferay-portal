{
  long repositoryId=ParamUtil.getLong(actionRequest,"repositoryId");
  LocalRepository localRepository=RepositoryLocalServiceUtil.getLocalRepositoryImpl(repositoryId);
  if (localRepository.isCapabilityProvided(TemporaryFilesCapability.class)) {
    TemporaryFilesCapability temporaryFilesCapability=localRepository.getCapability(TemporaryFilesCapability.class);
    temporaryFilesCapability.deleteExpiredTemporaryFiles();
  }
}
