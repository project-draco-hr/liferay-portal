{
  selenium.open("/web/socialrequestsn1/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Requests Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Requests Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("socialrequestfn2 socialrequestmn2 socialrequestln2 wants to be your friend."),selenium.getText("//div/div/div/div[2]/div/div"));
  assertEquals(RuntimeVariables.replace("Confirm"),selenium.getText("//div[2]/div/ul/li/a/span"));
  assertEquals(RuntimeVariables.replace("Ignore"),selenium.getText("//div[2]/div/ul/li[2]/a/span"));
}
