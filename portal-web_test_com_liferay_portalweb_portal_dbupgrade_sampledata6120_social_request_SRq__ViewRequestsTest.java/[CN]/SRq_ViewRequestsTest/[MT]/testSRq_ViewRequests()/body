{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/socialrequestsn1/home/");
  selenium.click(RuntimeVariables.replace("link=Requests Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("socialrequestfn2 socialrequestmn2 socialrequestln2 wants to be your friend."),selenium.getText("//div[@class='request-title']"));
  assertEquals(RuntimeVariables.replace("Confirm"),selenium.getText("//a/span[.='Confirm']"));
  assertEquals(RuntimeVariables.replace("Ignore"),selenium.getText("//a/span[.='Ignore']"));
}
