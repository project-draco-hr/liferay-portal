{
  _paclPolicies.put(classLoader,paclPolicy);
  if (!paclPolicy.isActive()) {
    return;
  }
  _activeCount++;
  if (_activeCount == 1) {
    if (_log.isInfoEnabled()) {
      _log.info("Activating PACL policy manager");
    }
    _overridePortalSecurityManager();
    ServiceBeanAopCacheManagerUtil.reset();
  }
}
