{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(release);
    }
 else {
      if (release.isNew()) {
        session.save(release);
      }
    }
    session.flush();
    release.setNew(false);
    return release;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
