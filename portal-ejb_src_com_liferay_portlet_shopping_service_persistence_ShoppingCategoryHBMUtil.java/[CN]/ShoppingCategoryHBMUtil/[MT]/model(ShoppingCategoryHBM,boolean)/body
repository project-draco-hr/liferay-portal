{
  com.liferay.portlet.shopping.model.ShoppingCategory shoppingCategory=null;
  if (checkPool) {
    shoppingCategory=ShoppingCategoryPool.get(shoppingCategoryHBM.getPrimaryKey());
  }
  if (shoppingCategory == null) {
    shoppingCategory=new com.liferay.portlet.shopping.model.ShoppingCategory(shoppingCategoryHBM.getCategoryId(),shoppingCategoryHBM.getCompanyId(),shoppingCategoryHBM.getCreateDate(),shoppingCategoryHBM.getModifiedDate(),shoppingCategoryHBM.getParentCategoryId(),shoppingCategoryHBM.getName());
    ShoppingCategoryPool.put(shoppingCategory.getPrimaryKey(),shoppingCategory);
  }
  return shoppingCategory;
}
