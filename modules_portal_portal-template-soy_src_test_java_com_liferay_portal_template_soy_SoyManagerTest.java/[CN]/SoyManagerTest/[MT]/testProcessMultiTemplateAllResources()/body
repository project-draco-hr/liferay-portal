{
  Template template=_soyManagerTestHelper.getTemplate(Arrays.asList("multi.soy","simple.soy","context.soy","multi-context.soy"));
  template.put("namespace","soy.multiTest.simple");
  UnsyncStringWriter unsyncStringWriter=new UnsyncStringWriter();
  template.processTemplate(unsyncStringWriter);
  Assert.assertEquals("Hello.",unsyncStringWriter.toString());
}
