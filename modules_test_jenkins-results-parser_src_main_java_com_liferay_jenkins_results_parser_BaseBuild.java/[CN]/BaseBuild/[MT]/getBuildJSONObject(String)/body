{
  if (getBuildURL() == null) {
    return null;
  }
  StringBuffer sb=new StringBuffer();
  sb.append(JenkinsResultsParserUtil.getLocalURL(getBuildURL()));
  sb.append("/api/json?pretty");
  if (tree != null) {
    sb.append("&tree=");
    sb.append(tree);
  }
  return JenkinsResultsParserUtil.toJSONObject(sb.toString(),false);
}
