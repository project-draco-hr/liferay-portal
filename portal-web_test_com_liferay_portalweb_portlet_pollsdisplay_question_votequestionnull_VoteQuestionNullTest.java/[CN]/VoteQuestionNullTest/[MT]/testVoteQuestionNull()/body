{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Polls Display Test Page");
  selenium.clickAt("link=Polls Display Test Page",RuntimeVariables.replace("Polls Display Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Vote']",RuntimeVariables.replace("Vote"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Please select an option."),selenium.getText("//div[@class='portlet-msg-error']"));
}
