{
  validate(nameMap);
  CalendarResource calendarResource=calendarResourcePersistence.findByPrimaryKey(calendarResourceId);
  calendarResource.setModifiedDate(serviceContext.getModifiedDate(null));
  calendarResource.setNameMap(nameMap);
  calendarResource.setDescriptionMap(descriptionMap);
  calendarResource.setActive(active);
  calendarResourcePersistence.update(calendarResource);
  updateAsset(calendarResource.getUserId(),calendarResource,serviceContext.getAssetCategoryIds(),serviceContext.getAssetTagNames(),serviceContext.getAssetPriority());
  return calendarResource;
}
