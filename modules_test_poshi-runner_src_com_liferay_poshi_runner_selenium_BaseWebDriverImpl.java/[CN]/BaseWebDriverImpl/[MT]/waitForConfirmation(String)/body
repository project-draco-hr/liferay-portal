{
  int timeout=TestPropsValues.TIMEOUT_EXPLICIT_WAIT / TestPropsValues.TIMEOUT_IMPLICIT_WAIT;
  for (int second=0; ; second++) {
    if (second >= timeout) {
      assertConfirmation(pattern);
    }
    try {
      if (isConfirmation(pattern)) {
        break;
      }
    }
 catch (    Exception e) {
    }
  }
}
