{
  Session session=null;
  try {
    session=openSession();
    DLFileEntryMetadata dlFileEntryMetadata=(DLFileEntryMetadata)session.get(DLFileEntryMetadataImpl.class,primaryKey);
    if (dlFileEntryMetadata == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchFileEntryMetadataException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(dlFileEntryMetadata);
  }
 catch (  NoSuchFileEntryMetadataException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
