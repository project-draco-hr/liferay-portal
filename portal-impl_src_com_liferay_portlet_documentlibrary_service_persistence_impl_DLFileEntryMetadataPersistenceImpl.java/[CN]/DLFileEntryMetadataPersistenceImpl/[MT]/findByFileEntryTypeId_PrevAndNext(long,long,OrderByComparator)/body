{
  DLFileEntryMetadata dlFileEntryMetadata=findByPrimaryKey(fileEntryMetadataId);
  Session session=null;
  try {
    session=openSession();
    DLFileEntryMetadata[] array=new DLFileEntryMetadataImpl[3];
    array[0]=getByFileEntryTypeId_PrevAndNext(session,dlFileEntryMetadata,fileEntryTypeId,orderByComparator,true);
    array[1]=dlFileEntryMetadata;
    array[2]=getByFileEntryTypeId_PrevAndNext(session,dlFileEntryMetadata,fileEntryTypeId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
