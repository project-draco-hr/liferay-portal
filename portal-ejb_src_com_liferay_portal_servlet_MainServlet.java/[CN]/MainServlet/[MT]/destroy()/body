{
  try {
    Iterator itr=PortletServiceUtil.getPortlets(_companyId).iterator();
    while (itr.hasNext()) {
      Portlet portlet=(Portlet)itr.next();
      PortletInstanceFactory.destroy(portlet);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  JobScheduler.shutdown();
  super.destroy();
}
