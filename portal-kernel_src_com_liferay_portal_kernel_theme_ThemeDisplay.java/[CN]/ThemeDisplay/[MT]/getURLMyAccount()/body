{
  if (_urlMyAccount == null) {
    String portletId=PortletProviderUtil.getPortletId(PortalMyAccountApplicationType.MyAccount.CLASS_NAME,PortletProvider.Action.VIEW);
    _urlMyAccount=PortalUtil.getControlPanelPortletURL(getRequest(),portletId,PortletRequest.RENDER_PHASE);
  }
  return _urlMyAccount;
}
