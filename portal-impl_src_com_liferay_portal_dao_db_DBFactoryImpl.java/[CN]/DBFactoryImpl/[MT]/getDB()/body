{
  if (_db == null) {
    try {
      if (_log.isInfoEnabled()) {
        _log.info("Using dialect " + PropsValues.HIBERNATE_DIALECT);
      }
      Dialect dialect=(Dialect)Class.forName(PropsValues.HIBERNATE_DIALECT).newInstance();
      DBFactoryUtil.setDB(dialect);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
  return _db;
}
