{
  if (_db != null) {
    return;
  }
  _db=getDB(type);
  if (_db == null) {
    _log.error("No DB implementation exists for " + type);
  }
 else {
    if (_log.isDebugEnabled()) {
      Class<?> clazz=_db.getClass();
      _log.debug("Using DB implementation " + clazz.getName() + " for "+ type);
    }
  }
}
