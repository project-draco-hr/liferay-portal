{
  List<Website> websites=new ArrayList<Website>();
  for (int i=0; ; i++) {
    long websiteId=ParamUtil.getLong(actionRequest,"websiteId" + i,-1);
    if (websiteId < 0) {
      break;
    }
    String url=ParamUtil.getString(actionRequest,"websiteUrl" + i);
    int typeId=ParamUtil.getInteger(actionRequest,"websiteTypeId" + i);
    boolean primary=ParamUtil.getBoolean(actionRequest,"websitePrimary" + i);
    Website website=new WebsiteImpl();
    website.setWebsiteId(websiteId);
    website.setUrl(url);
    website.setTypeId(typeId);
    website.setPrimary(primary);
    websites.add(website);
  }
  return websites;
}
