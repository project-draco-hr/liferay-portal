{
  long websiteId=ParamUtil.getLong(actionRequest,"websiteId" + websiteSuffix);
  String url=ParamUtil.getString(actionRequest,"url" + websiteSuffix);
  int typeId=ParamUtil.getInteger(actionRequest,"typeId" + websiteSuffix);
  boolean primary=ParamUtil.getBoolean(actionRequest,"primary" + websiteSuffix);
  Website website=null;
  if (websiteId <= 0) {
    if (Validator.isNull(url)) {
      return 0;
    }
    website=WebsiteServiceUtil.addWebsite(className,classPK,url,typeId,primary);
  }
 else {
    website=WebsiteServiceUtil.updateWebsite(websiteId,url,typeId,primary);
  }
  return website.getWebsiteId();
}
