{
  if (permissionChecker.isCompanyAdmin()) {
    return;
  }
  Iterator<Role> itr=roles.iterator();
  while (itr.hasNext()) {
    Role role=itr.next();
    String name=role.getName();
    if (name.equals(RoleConstants.GUEST) || name.equals(RoleConstants.OWNER) || name.equals(RoleConstants.USER)|| !RolePermissionUtil.contains(permissionChecker,role.getRoleId(),ActionKeys.ASSIGN_MEMBERS)) {
      itr.remove();
    }
  }
}
