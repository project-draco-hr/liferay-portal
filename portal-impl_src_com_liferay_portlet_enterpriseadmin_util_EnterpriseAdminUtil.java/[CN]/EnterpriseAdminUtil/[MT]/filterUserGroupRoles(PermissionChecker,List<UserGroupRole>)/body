{
  Iterator<UserGroupRole> itr=userGroupRoles.iterator();
  while (itr.hasNext()) {
    UserGroupRole userGroupRole=itr.next();
    Role role=userGroupRole.getRole();
    if (role.getName().equals(RoleConstants.ORGANIZATION_MEMBER) || role.getName().equals(RoleConstants.COMMUNITY_MEMBER)) {
      itr.remove();
    }
  }
  if (permissionChecker.isCompanyAdmin()) {
    return;
  }
  itr=userGroupRoles.iterator();
  while (itr.hasNext()) {
    UserGroupRole userGroupRole=itr.next();
    if (!RolePermissionUtil.contains(permissionChecker,userGroupRole.getRoleId(),ActionKeys.ASSIGN_MEMBERS)) {
      itr.remove();
    }
  }
}
