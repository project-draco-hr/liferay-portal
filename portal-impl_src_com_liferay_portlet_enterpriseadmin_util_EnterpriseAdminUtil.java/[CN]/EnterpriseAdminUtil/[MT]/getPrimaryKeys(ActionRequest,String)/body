{
  long[] primKeys=StringUtil.split(ParamUtil.getString(actionRequest,name + "PrimaryKeys"),0L);
  List<Long> filteredKeys=new ArrayList<Long>();
  for (  long primKey : primKeys) {
    if (primKey > 0) {
      filteredKeys.add(primKey);
    }
  }
  primKeys=ArrayUtil.toArray(filteredKeys.toArray(new Long[filteredKeys.size()]));
  return primKeys;
}
