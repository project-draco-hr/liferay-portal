{
  if (permissionChecker.isCompanyAdmin()) {
    return;
  }
  if (organizations instanceof UnmodifiableList) {
    organizations=ListUtil.copy(organizations);
  }
  Iterator<Organization> itr=organizations.iterator();
  while (itr.hasNext()) {
    Organization organization=itr.next();
    if (!OrganizationPermissionUtil.contains(permissionChecker,organization.getOrganizationId(),ActionKeys.ASSIGN_MEMBERS)) {
      itr.remove();
    }
  }
}
