{
  try {
    com.liferay.portlet.shopping.model.ShoppingOrder returnValue=ShoppingOrderServiceUtil.updateOrder(plid,orderId,ppTxnId,ppPaymentStatus,ppPaymentGross,ppReceiverEmail,ppPayerEmail);
    return com.liferay.portlet.shopping.model.ShoppingOrderSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
