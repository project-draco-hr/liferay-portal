{
  SyncFilePersistence syncFilePersistence=SyncFileService.getSyncFilePersistence();
  syncFilePersistence.executeRaw("ALTER TABLE `SyncAccount` ADD COLUMN oAuthEnabled" + " VARCHAR(16777216) BEFORE password;");
  syncFilePersistence.executeRaw("ALTER TABLE `SyncAccount` ADD COLUMN oAuthConsumerSecret" + " VARCHAR(16777216) BEFORE oAuthEnabled;");
  syncFilePersistence.executeRaw("ALTER TABLE `SyncAccount` ADD COLUMN oAuthConsumerKey" + " VARCHAR(16777216) BEFORE oAuthConsumerSecret;");
}
