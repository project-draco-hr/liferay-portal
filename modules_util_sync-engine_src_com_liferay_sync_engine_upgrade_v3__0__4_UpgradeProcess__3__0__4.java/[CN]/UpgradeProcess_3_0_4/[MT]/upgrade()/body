{
  SyncAccountPersistence syncAccountPersistence=SyncAccountService.getSyncAccountPersistence();
  syncAccountPersistence.executeRaw("ALTER TABLE `SyncAccount` ADD COLUMN oAuthEnabled " + "VARCHAR(16777216) BEFORE password;");
  syncAccountPersistence.executeRaw("ALTER TABLE `SyncAccount` ADD COLUMN oAuthConsumerSecret " + "VARCHAR(16777216) BEFORE oAuthEnabled;");
  syncAccountPersistence.executeRaw("ALTER TABLE `SyncAccount` ADD COLUMN oAuthConsumerKey " + "VARCHAR(16777216) BEFORE oAuthConsumerSecret;");
}
