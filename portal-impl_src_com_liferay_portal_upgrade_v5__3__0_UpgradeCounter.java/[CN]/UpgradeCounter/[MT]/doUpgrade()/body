{
  Connection con=null;
  Statement st=null;
  ResultSet rs=null;
  try {
    long maxLayoutId=100;
    con=DataAccess.getConnection();
    st=con.createStatement();
    rs=st.executeQuery("select max(layoutId) from layout");
    if (rs.next()) {
      maxLayoutId=rs.getLong(1) + 1;
    }
    CounterLocalServiceUtil.reset(Layout.class.getName(),maxLayoutId);
  }
  finally {
    DataAccess.cleanUp(con,st,rs);
  }
}
