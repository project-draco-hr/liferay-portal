{
  try {
    Organization organization=ActionUtil.getOrganization(portletRequest);
    Group organizationGroup=organization.getGroup();
    PortletURL portletURL=PortletProviderUtil.getPortletURL(portletRequest,organizationGroup,Group.class.getName(),PortletProvider.Action.EDIT);
    portletURL.setParameter("viewOrganizationsRedirect",PortalUtil.getCurrentURL(portletRequest));
    return portletURL.toString();
  }
 catch (  Exception e) {
  }
  return StringPool.BLANK;
}
