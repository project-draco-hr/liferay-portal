{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(groupId,TestPropsValues.getUserId());
  DLAppTestUtil.populateNotificationsServiceContext(serviceContext,Constants.ADD);
  return DLAppLocalServiceUtil.addFileEntry(TestPropsValues.getUserId(),groupId,folderId,sourceFileName,mimeType,RandomTestUtil.randomBytes(),serviceContext);
