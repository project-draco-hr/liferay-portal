{
  QueryDefinition queryDefinition=new QueryDefinition();
  queryDefinition.setStatus(WorkflowConstants.STATUS_ANY);
  Assert.assertEquals(2,JournalArticleFinderUtil.countByG_U_F_C(_group.getGroupId(),TestPropsValues.getUserId(),Collections.EMPTY_LIST,JournalArticleConstants.CLASSNAME_ID_DEFAULT,queryDefinition));
  queryDefinition.setStatus(WorkflowConstants.STATUS_IN_TRASH);
  Assert.assertEquals(1,JournalArticleFinderUtil.countByG_U_F_C(_group.getGroupId(),_USER_ID,Collections.EMPTY_LIST,JournalArticleConstants.CLASSNAME_ID_DEFAULT,queryDefinition));
  queryDefinition.setStatus(WorkflowConstants.STATUS_IN_TRASH,true);
  Assert.assertEquals(0,JournalArticleFinderUtil.countByG_U_F_C(_group.getGroupId(),_USER_ID,Collections.EMPTY_LIST,JournalArticleConstants.CLASSNAME_ID_DEFAULT,queryDefinition));
}
