{
  QueryDefinition queryDefinition=new QueryDefinition();
  queryDefinition.setStatus(WorkflowConstants.STATUS_ANY);
  Assert.assertEquals(1,JournalArticleFinderUtil.countByG_C_S(_group.getGroupId(),JournalArticleConstants.CLASSNAME_ID_DEFAULT,_ddmStructure.getStructureKey(),queryDefinition));
  Assert.assertEquals(2,JournalArticleFinderUtil.countByG_C_S(_group.getGroupId(),JournalArticleConstants.CLASSNAME_ID_DEFAULT,"0",queryDefinition));
  queryDefinition.setStatus(WorkflowConstants.STATUS_IN_TRASH);
  Assert.assertEquals(0,JournalArticleFinderUtil.countByG_C_S(_group.getGroupId(),JournalArticleConstants.CLASSNAME_ID_DEFAULT,_ddmStructure.getStructureKey(),queryDefinition));
  Assert.assertEquals(1,JournalArticleFinderUtil.countByG_C_S(_group.getGroupId(),JournalArticleConstants.CLASSNAME_ID_DEFAULT,"0",queryDefinition));
  queryDefinition.setStatus(WorkflowConstants.STATUS_IN_TRASH,true);
  Assert.assertEquals(1,JournalArticleFinderUtil.countByG_C_S(_group.getGroupId(),JournalArticleConstants.CLASSNAME_ID_DEFAULT,_ddmStructure.getStructureKey(),queryDefinition));
  Assert.assertEquals(1,JournalArticleFinderUtil.countByG_C_S(_group.getGroupId(),JournalArticleConstants.CLASSNAME_ID_DEFAULT,"0",queryDefinition));
}
