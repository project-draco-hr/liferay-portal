{
  int actualCount=JournalArticleFinderUtil.countByG_C_S(groupId,classNameId,ddmStructureKey,queryDefinition);
  Assert.assertEquals(expectedCount,actualCount);
  List<JournalArticle> articles=JournalArticleFinderUtil.findByG_C_S(groupId,classNameId,ddmStructureKey,queryDefinition);
  actualCount=articles.size();
  Assert.assertEquals(expectedCount,actualCount);
}
