{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(activityTracker);
    }
  }
  activityTracker=removeImpl(activityTracker);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(activityTracker);
    }
  }
  return activityTracker;
}
