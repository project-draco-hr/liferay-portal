{
  Session session=null;
  try {
    session=openSession();
    ActivityTracker activityTracker=(ActivityTracker)session.get(ActivityTrackerImpl.class,new Long(activityTrackerId));
    if (activityTracker == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No ActivityTracker exists with the primary key " + activityTrackerId);
      }
      throw new NoSuchActivityTrackerException("No ActivityTracker exists with the primary key " + activityTrackerId);
    }
    return remove(activityTracker);
  }
 catch (  NoSuchActivityTrackerException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
