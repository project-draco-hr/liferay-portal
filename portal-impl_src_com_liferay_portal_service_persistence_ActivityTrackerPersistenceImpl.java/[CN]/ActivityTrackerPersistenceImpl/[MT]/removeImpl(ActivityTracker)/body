{
  Session session=null;
  try {
    session=openSession();
    session.delete(activityTracker);
    session.flush();
    return activityTracker;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(ActivityTracker.class.getName());
  }
}
