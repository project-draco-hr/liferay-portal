{
  CompanyStatistics companyStatistics=_ServerStatistics.getPortletContainerStatistics(companyId);
  long numSuccesses=0;
  Collection<RequestStatistics> portletsStatistics=companyStatistics.getRenderStatistics();
  for (  RequestStatistics portletStatistics : portletsStatistics) {
    numSuccesses+=portletStatistics.getNumSuccesses();
  }
  return numSuccesses;
}
