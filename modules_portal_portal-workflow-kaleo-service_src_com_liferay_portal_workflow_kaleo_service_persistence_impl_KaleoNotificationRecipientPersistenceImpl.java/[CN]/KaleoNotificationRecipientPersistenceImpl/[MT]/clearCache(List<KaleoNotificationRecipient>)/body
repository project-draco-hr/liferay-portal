{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  KaleoNotificationRecipient kaleoNotificationRecipient : kaleoNotificationRecipients) {
    EntityCacheUtil.removeResult(KaleoNotificationRecipientModelImpl.ENTITY_CACHE_ENABLED,KaleoNotificationRecipientImpl.class,kaleoNotificationRecipient.getPrimaryKey());
  }
}
