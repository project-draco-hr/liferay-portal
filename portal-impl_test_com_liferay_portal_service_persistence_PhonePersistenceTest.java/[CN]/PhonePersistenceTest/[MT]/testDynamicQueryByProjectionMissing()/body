{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Phone.class,Phone.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("phoneId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("phoneId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
