{
  JournalArticle latestIndexableArticle=_journalArticleLocalService.fetchLatestArticle(resourcePrimKey,new int[]{WorkflowConstants.STATUS_APPROVED,WorkflowConstants.STATUS_IN_TRASH});
  if (latestIndexableArticle == null) {
    latestIndexableArticle=_journalArticleLocalService.fetchLatestArticle(resourcePrimKey);
  }
  return latestIndexableArticle;
}
