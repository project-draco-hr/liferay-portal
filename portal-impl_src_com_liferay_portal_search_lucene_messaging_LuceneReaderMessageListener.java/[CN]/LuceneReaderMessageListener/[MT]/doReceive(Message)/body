{
  String responseDestination=message.getResponseDestination();
  String responseId=message.getResponseId();
  SearchRequest searchRequest=(SearchRequest)message.getRequestValue();
  String command=searchRequest.getCommand();
  if (command.equals(SearchRequest.COMMAND_INDEX_ONLY) && Validator.isNotNull(responseDestination) && Validator.isNotNull(responseId)) {
    doCommandIndexOnly(message);
  }
 else   if (command.equals(SearchRequest.COMMAND_SEARCH) && Validator.isNotNull(responseDestination) && Validator.isNotNull(responseId)) {
    doCommandSearch(message,searchRequest);
  }
}
