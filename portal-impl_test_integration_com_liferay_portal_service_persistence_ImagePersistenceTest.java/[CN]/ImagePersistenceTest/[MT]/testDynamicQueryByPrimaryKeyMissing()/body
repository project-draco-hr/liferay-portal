{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Image.class,Image.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("imageId",ServiceTestUtil.nextLong()));
  List<Image> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
