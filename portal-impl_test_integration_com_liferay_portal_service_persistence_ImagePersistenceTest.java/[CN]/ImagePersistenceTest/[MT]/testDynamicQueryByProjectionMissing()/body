{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Image.class,Image.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("imageId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("imageId",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
