{
  selenium.open("/web/guest/home/");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=User Statistics Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=User Statistics Test Page",RuntimeVariables.replace("User Statistics Test Page"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertEquals(RuntimeVariables.replace("Joe Bloggs"),selenium.getText("xPath=(//span[@class='user-name'])[1]"));
  assertEquals(RuntimeVariables.replace("Contribution Score: 0 (Total: 0)"),selenium.getText("xPath=(//div[@class='contribution-score'])[1]"));
  assertEquals(RuntimeVariables.replace("Participation Score: 5 (Total: 5)"),selenium.getText("xPath=(//div[@class='participation-score'])[1]"));
}
