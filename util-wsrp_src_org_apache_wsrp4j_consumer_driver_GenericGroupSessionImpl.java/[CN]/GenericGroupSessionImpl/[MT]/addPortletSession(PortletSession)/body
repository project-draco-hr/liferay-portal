{
  final String MN="addPortletSession";
  if (portletSession != null) {
    this.portletSessions.put(portletSession.getPortletHandle(),portletSession);
    if (logger.isLogging(Logger.TRACE_HIGH)) {
      logger.text(Logger.TRACE_HIGH,MN,"added PortletSession with handle: " + portletSession.getPortletHandle() + " to GroupSession with ID: "+ groupID);
    }
  }
}
