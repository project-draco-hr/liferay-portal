{
  PropsUtil.set(PropsKeys.EHCACHE_PORTAL_CACHE_MANAGER_JMX_ENABLED,Boolean.FALSE.toString());
  PropsUtil.set(PropsKeys.RESOURCE_ACTIONS_CONFIGS,StringPool.BLANK);
  PropsUtil.set(PropsKeys.RESOURCE_ACTIONS_READ_PORTLET_RESOURCES,Boolean.FALSE.toString());
  PropsUtil.set(PropsKeys.SPRING_CONFIGS,StringUtil.merge(_SPRING_CONFIGS));
  InitUtil.initWithSpring();
  BooleanQueryFactoryUtil booleanQueryFactoryUtil=new BooleanQueryFactoryUtil();
  Class<?> clazz=booleanQueryFactoryUtil.getClass();
  for (  Field field : clazz.getDeclaredFields()) {
    BeanReference beanReference=field.getAnnotation(BeanReference.class);
    if (beanReference == null) {
      continue;
    }
    field.setAccessible(true);
    try {
      field.set(booleanQueryFactoryUtil,booleanQueryFactoryUtil);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  booleanQueryFactoryUtil.setGenericQueryFactory(new BooleanQueryFactoryImpl());
}
