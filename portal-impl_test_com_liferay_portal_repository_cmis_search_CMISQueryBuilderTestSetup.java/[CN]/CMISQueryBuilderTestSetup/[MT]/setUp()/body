{
  PropsUtil.set("spring.configs","META-INF/management-spring.xml,META-INF/util-spring.xml," + "META-INF/messaging-core-spring.xml," + "META-INF/messaging-misc-spring.xml,META-INF/search-spring.xml,"+ "META-INF/service-builder-spring.xml");
  PropsUtil.set(PropsKeys.RESOURCE_ACTIONS_READ_PORTLET_RESOURCES,"false");
  PropsUtil.set(PropsKeys.EHCACHE_PORTAL_CACHE_MANAGER_JMX_ENABLED,"false");
  PropsUtil.set(PropsKeys.RESOURCE_ACTIONS_CONFIGS,"");
  InitUtil.initWithSpring();
  BooleanQueryFactoryUtil util=new BooleanQueryFactoryUtil();
  for (  java.lang.reflect.Field field : util.getClass().getDeclaredFields()) {
    BeanReference ref=field.getAnnotation(BeanReference.class);
    if (ref != null) {
      field.setAccessible(true);
      try {
        field.set(util,util);
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
  }
  util.setGenericQueryFactory(new BooleanQueryFactoryImpl());
}
