{
  ResourceCode resourceCode=fetchByPrimaryKey(resourceCodePK);
  if (resourceCode == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ResourceCode exists with the primary key " + resourceCodePK);
    }
    throw new NoSuchResourceCodeException("No ResourceCode exists with the primary key " + resourceCodePK);
  }
  return resourceCode;
}
