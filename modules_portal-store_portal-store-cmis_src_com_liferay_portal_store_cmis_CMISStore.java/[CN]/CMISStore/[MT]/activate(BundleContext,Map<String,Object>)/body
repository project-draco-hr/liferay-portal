{
  _cmisStoreConfiguration=Configurable.createConfigurable(CMISStoreConfiguration.class,properties);
  _operationContext=createOperationContext();
  _sessionFactory=SessionFactoryImpl.newInstance();
  _session=createSession(bundleContext.getBundle(),_cmisStoreConfiguration,_operationContext,_sessionFactory);
  _systemRootDir=getFolder(_session.getRootFolder(),_cmisStoreConfiguration.systemRootDir());
  if (_systemRootDir == null) {
    _systemRootDir=createFolder(_session.getRootFolder(),_cmisStoreConfiguration.systemRootDir());
  }
}
