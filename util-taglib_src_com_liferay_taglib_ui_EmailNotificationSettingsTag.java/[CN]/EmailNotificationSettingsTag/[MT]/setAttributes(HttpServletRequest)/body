{
  request.setAttribute("liferay-ui:email-notification-settings:emailDefinitionTerms",_emailDefinitionTerms);
  request.setAttribute("liferay-ui:email-notification-settings:emailBody",_emailBody);
  request.setAttribute("liferay-ui:email-notification-settings:emailEnabled",String.valueOf(_emailEnabled));
  request.setAttribute("liferay-ui:email-notification-settings:emailParam",_emailParam);
  request.setAttribute("liferay-ui:email-notification-settings:emailSubject",_emailSubject);
}
