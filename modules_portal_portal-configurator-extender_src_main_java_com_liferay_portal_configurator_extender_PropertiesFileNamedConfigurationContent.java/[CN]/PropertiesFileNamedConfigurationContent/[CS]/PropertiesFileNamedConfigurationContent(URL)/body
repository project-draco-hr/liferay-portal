{
  String name=url.getFile();
  if (name.startsWith("/")) {
    name=name.substring(1);
  }
  int lastIndexOfSlash=name.lastIndexOf('/');
  if (lastIndexOfSlash > 0) {
    name=name.substring(lastIndexOfSlash + 1);
  }
  _name=name;
  try {
    _inputStream=url.openStream();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
