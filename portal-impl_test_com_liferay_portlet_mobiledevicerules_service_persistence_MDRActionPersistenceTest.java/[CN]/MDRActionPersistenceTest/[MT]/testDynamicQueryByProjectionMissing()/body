{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MDRAction.class,MDRAction.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("actionId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("actionId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
