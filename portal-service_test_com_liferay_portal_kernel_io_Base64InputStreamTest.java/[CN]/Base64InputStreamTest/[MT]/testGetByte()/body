{
  try {
    File testFile=new File(_filePath);
    FileOutputStream fos=new FileOutputStream(testFile);
    fos.write((byte)'a');
    fos.close();
    Base64InputStream instance=new Base64InputStream(new FileInputStream(testFile));
    char c='A';
    int expResult=0;
    int result=instance.getByte(c);
    assertEquals(expResult,result);
    c='=';
    expResult=0;
    result=instance.getByte(c);
    assertEquals(expResult,result);
    c='\n';
    expResult=-1;
    result=instance.getByte(c);
    assertEquals(expResult,result);
    c='+';
    expResult=62;
    result=instance.getByte(c);
    assertEquals(expResult,result);
    testFile.delete();
  }
 catch (  Exception ex) {
    fail("Test getByte failed");
  }
}
