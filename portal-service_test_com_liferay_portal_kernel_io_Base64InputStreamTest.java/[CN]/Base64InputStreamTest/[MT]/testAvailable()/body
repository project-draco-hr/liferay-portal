{
  File testFile=new File(_filePath);
  FileOutputStream fos=new FileOutputStream(testFile);
  byte[] b={'a','b','c','d'};
  fos.write(b);
  fos.close();
  Base64InputStream instance=new Base64InputStream(new FileInputStream(testFile));
  int result=instance.available();
  assertEquals(3,result);
  instance.close();
  testFile.delete();
}
