{
  Dictionary<String,Object> dictionary=new HashMapDictionary<>();
  dictionary.put(OpenIdConstants.AUTH_ENABLED,_prefsProps.getBoolean(companyId,LegacyOpenIdPropsKeys.OPENID_AUTH_ENABLED,true));
  if (_log.isInfoEnabled()) {
    _log.info("Adding OpenID auth configuration for company " + companyId + " with properties: "+ dictionary);
  }
  storeSettings(companyId,dictionary);
}
