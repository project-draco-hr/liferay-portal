{
  if (inputStream == null) {
    return null;
  }
  StringBuilder sb=new StringBuilder();
  char[] chars=new char[0x10000];
  Reader reader=new InputStreamReader(inputStream,"UTF-8");
  int read=0;
  do {
    read=reader.read(chars,0,chars.length);
    if (read > 0) {
      sb.append(chars,0,read);
    }
  }
 while (read >= 0);
  inputStream.close();
  return sb.toString();
}
