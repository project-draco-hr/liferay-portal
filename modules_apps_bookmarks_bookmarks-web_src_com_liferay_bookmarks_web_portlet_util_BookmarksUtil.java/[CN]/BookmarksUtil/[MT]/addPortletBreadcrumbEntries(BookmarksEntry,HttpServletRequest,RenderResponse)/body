{
  BookmarksFolder folder=entry.getFolder();
  if (folder.getFolderId() != BookmarksFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    addPortletBreadcrumbEntries(folder,request,renderResponse);
  }
  BookmarksEntry unescapedEntry=entry.toUnescapedModel();
  PortletURL portletURL=renderResponse.createRenderURL();
  portletURL.setParameter("struts_action","/bookmarks/view_entry");
  portletURL.setParameter("entryId",String.valueOf(entry.getEntryId()));
  PortalUtil.addPortletBreadcrumbEntry(request,unescapedEntry.getName(),portletURL.toString());
}
