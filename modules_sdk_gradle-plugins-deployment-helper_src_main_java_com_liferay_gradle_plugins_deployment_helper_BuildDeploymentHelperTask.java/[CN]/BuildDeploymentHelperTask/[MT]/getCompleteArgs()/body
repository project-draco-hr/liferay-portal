{
  List<String> completeArgs=new ArrayList<>();
  GUtil.addToCollection(completeArgs,getArgs());
  completeArgs.add("--fileNames=" + getDeploymentFileNames());
  completeArgs.add("--outputFile=" + FileUtil.getAbsolutePath(getOutputFile()));
  File deploymentPath=getDeploymentPath();
  if (deploymentPath != null) {
    completeArgs.add("--path=" + FileUtil.getAbsolutePath(deploymentPath));
  }
  return completeArgs;
}
