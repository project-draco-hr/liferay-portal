{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  Region newRegion=addRegion();
  _persistence.clearCache();
  Region existingRegion=_persistence.findByPrimaryKey(newRegion.getPrimaryKey());
  Assert.assertEquals(existingRegion.getCountryId(),ReflectionTestUtil.invoke(existingRegion,"getOriginalCountryId",new Class<?>[0]));
  Assert.assertTrue(Validator.equals(existingRegion.getRegionCode(),ReflectionTestUtil.invoke(existingRegion,"getOriginalRegionCode",new Class<?>[0])));
}
