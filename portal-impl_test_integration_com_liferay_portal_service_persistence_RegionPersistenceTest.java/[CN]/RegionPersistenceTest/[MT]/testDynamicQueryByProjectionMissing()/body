{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Region.class,Region.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("regionId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("regionId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
