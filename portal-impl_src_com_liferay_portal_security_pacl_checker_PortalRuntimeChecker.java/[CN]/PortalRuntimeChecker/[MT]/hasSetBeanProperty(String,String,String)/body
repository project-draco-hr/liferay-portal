{
  if (servletContextName.equals(getServletContextName())) {
    return true;
  }
  Set<String> setBeanPropertyClassNames=_setBeanPropertyClassNames.get(servletContextName);
  if (setBeanPropertyClassNames == null) {
    return false;
  }
  if (setBeanPropertyClassNames.contains(className)) {
    return true;
  }
  if (Validator.isNotNull(property)) {
    if (setBeanPropertyClassNames.contains(className.concat(StringPool.POUND).concat(property))) {
      return true;
    }
  }
  return false;
}
