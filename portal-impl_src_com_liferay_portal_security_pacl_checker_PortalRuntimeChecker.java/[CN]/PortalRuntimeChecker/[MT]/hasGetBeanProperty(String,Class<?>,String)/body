{
  String className=clazz.getName();
  if (servletContextName.equals(getServletContextName())) {
    return true;
  }
  Set<String> _getBeanPropertyClassNames=_getBeanPropertyClassNamesMap.get(servletContextName);
  if (_getBeanPropertyClassNames == null) {
    return false;
  }
  if (_getBeanPropertyClassNames.contains(className)) {
    return true;
  }
  if (Validator.isNotNull(property)) {
    if (_getBeanPropertyClassNames.contains(className.concat(StringPool.POUND).concat(property))) {
      return true;
    }
  }
  return false;
}
