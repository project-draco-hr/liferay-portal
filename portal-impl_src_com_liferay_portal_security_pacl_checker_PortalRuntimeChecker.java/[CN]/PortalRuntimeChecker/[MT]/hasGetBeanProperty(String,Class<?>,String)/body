{
  if (servletContextName.equals(getServletContextName())) {
    return true;
  }
  Set<String> getBeanPropertyClassNames=_getBeanPropertyClassNames.get(servletContextName);
  if (getBeanPropertyClassNames == null) {
    return false;
  }
  String className=clazz.getName();
  if (getBeanPropertyClassNames.contains(className)) {
    return true;
  }
  if (Validator.isNotNull(property)) {
    if (getBeanPropertyClassNames.contains(className.concat(StringPool.POUND).concat(property))) {
      return true;
    }
  }
  return false;
}
