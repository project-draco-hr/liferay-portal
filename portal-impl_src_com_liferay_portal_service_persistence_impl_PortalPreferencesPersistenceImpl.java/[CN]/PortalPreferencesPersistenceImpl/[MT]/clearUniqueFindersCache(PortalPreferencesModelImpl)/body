{
  Object[] args=new Object[]{portalPreferencesModelImpl.getOwnerId(),portalPreferencesModelImpl.getOwnerType()};
  FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_O_O,args);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_O_O,args);
  if ((portalPreferencesModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_O_O.getColumnBitmask()) != 0) {
    args=new Object[]{portalPreferencesModelImpl.getOriginalOwnerId(),portalPreferencesModelImpl.getOriginalOwnerType()};
    FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_O_O,args);
    FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_O_O,args);
  }
}
