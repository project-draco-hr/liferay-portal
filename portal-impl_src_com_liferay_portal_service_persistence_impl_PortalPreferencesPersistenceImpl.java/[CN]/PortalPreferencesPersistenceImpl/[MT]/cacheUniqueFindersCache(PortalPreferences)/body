{
  if (portalPreferences.isNew()) {
    Object[] args=new Object[]{portalPreferences.getOwnerId(),portalPreferences.getOwnerType()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_O_O,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_O_O,args,portalPreferences);
  }
 else {
    PortalPreferencesModelImpl portalPreferencesModelImpl=(PortalPreferencesModelImpl)portalPreferences;
    if ((portalPreferencesModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_O_O.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{portalPreferences.getOwnerId(),portalPreferences.getOwnerType()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_O_O,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_O_O,args,portalPreferences);
    }
  }
}
