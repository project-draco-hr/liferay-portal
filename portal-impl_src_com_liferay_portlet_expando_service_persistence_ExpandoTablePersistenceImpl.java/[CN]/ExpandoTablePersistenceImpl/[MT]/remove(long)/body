{
  Session session=null;
  try {
    session=openSession();
    ExpandoTable expandoTable=(ExpandoTable)session.get(ExpandoTableImpl.class,Long.valueOf(tableId));
    if (expandoTable == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + tableId);
      }
      throw new NoSuchTableException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + tableId);
    }
    return expandoTablePersistence.remove(expandoTable);
  }
 catch (  NoSuchTableException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
