{
  Session session=null;
  try {
    session=openSession();
    ExpandoTable expandoTable=(ExpandoTable)session.get(ExpandoTableImpl.class,new Long(tableId));
    if (expandoTable == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No ExpandoTable exists with the primary key " + tableId);
      }
      throw new NoSuchTableException("No ExpandoTable exists with the primary key " + tableId);
    }
    return remove(expandoTable);
  }
 catch (  NoSuchTableException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
