{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(expandoTable);
    }
  }
  expandoTable=removeImpl(expandoTable);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(expandoTable);
    }
  }
  return expandoTable;
}
