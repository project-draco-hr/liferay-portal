{
  expandoTable=toUnwrappedModel(expandoTable);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,expandoTable);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(expandoTable);
  return expandoTable;
}
