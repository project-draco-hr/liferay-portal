{
  Session session=null;
  try {
    session=openSession();
    session.delete(expandoTable);
    session.flush();
    return expandoTable;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ExpandoTable.class.getName());
  }
}
