{
  Locale defaultLocale=getDefaultLocale(httpServletRequest);
  String ddmFormFieldParameterValue=getDDMFormFieldParameterValue(httpServletRequest,ddmFormFieldParameterName,defaultLocale,predefinedValue.getString(defaultLocale));
  Value value=new UnlocalizedValue(ddmFormFieldParameterValue);
  ddmFormFieldValue.setValue(value);
}
