{
  Locale defaultLocale=getDefaultLocale(httpServletRequest);
  String ddmFormFieldParameterValue=getDDMFormFieldParameterValue(httpServletRequest,fieldType,ddmFormFieldParameterName,predefinedValue.getString(defaultLocale),defaultLocale);
  Value value=new UnlocalizedValue(ddmFormFieldParameterValue);
  ddmFormFieldValue.setValue(value);
}
