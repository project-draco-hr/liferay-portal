{
  String p=getGroupKey(name);
  current=groups.get(p);
  if (current == null) {
    current=new HashMap<String,Integer>();
    groups.put(p,current);
  }
  if (signature == null) {
    if (superName != null) {
      addInternalName(superName);
    }
    addInternalNames(interfaces);
  }
 else {
    addSignature(signature);
  }
}
