{
  List<LoggingEvent> loggingEvents=_captureAppender.getLoggingEvents();
  for (  LoggingEvent loggingEvent : loggingEvents) {
    String eventMessage=loggingEvent.getRenderedMessage();
    Assert.assertTrue(eventMessage.startsWith("Duplicate entry") || eventMessage.equals("Deadlock found when trying to get lock; " + "try restarting transaction"));
  }
  _captureAppender.close();
}
