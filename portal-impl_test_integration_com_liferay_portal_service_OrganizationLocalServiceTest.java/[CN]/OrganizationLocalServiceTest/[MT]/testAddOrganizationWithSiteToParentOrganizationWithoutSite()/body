{
  Organization organizationA=OrganizationTestUtil.addOrganization(OrganizationConstants.DEFAULT_PARENT_ORGANIZATION_ID,"organizationA",false);
  Organization organizationB=OrganizationTestUtil.addOrganization(organizationA.getOrganizationId(),"organizationB",true);
  Assert.assertEquals(organizationA.getOrganizationId(),organizationB.getParentOrganizationId());
  Group groupB=organizationB.getGroup();
  Assert.assertEquals(GroupConstants.DEFAULT_PARENT_GROUP_ID,groupB.getParentGroupId());
}
