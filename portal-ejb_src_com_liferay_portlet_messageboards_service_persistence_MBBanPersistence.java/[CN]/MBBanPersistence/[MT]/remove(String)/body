{
  Session session=null;
  try {
    session=openSession();
    MBBan mbBan=(MBBan)session.get(MBBanImpl.class,banId);
    if (mbBan == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No MBBan exists with the primary key " + banId);
      }
      throw new NoSuchBanException("No MBBan exists with the primary key " + banId);
    }
    return remove(mbBan);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
