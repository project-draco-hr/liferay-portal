{
  WSRPConsumerPortlet wsrpConsumerPortlet=findByPrimaryKey(wsrpConsumerPortletId);
  Session session=null;
  try {
    session=openSession();
    WSRPConsumerPortlet[] array=new WSRPConsumerPortletImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,wsrpConsumerPortlet,uuid,companyId,orderByComparator,true);
    array[1]=wsrpConsumerPortlet;
    array[2]=getByUuid_C_PrevAndNext(session,wsrpConsumerPortlet,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
