{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Blogs Permissions Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Blogs Permissions Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Permissions Blogs Test Entry",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Add Comment",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("_33_postReplyBody0")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.type("_33_postReplyBody0",RuntimeVariables.replace("Member Permissions Blogs Test Comment"));
  selenium.keyPress("_33_postReplyBody0",RuntimeVariables.replace("\\48"));
  selenium.keyPress("_33_postReplyBody0",RuntimeVariables.replace("\\8"));
  selenium.clickAt("//input[@value='Reply']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertTrue(selenium.isTextPresent("Member Permissions Blogs Test Comment"));
}
