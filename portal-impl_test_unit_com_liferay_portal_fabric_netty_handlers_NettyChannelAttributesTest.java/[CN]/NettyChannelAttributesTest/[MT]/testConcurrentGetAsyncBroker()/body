{
  AsyncBroker<Long,Serializable> asyncBroker=new AsyncBroker<Long,Serializable>();
  AttributeAdvice.setConcurrentValue(asyncBroker);
  Assert.assertSame(asyncBroker,NettyChannelAttributes.getAsyncBroker(_embeddedChannel));
  Assert.assertSame(asyncBroker,NettyChannelAttributes.getAsyncBroker(_embeddedChannel));
}
