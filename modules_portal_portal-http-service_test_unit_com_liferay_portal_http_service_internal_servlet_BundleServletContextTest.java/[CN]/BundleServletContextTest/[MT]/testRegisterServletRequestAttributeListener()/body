{
  List<ServletRequestAttributeListener> servletRequestAttributeListeners=_bundleServletContext.getServletRequestAttributeListeners();
  int initialSize=servletRequestAttributeListeners.size();
  registerListener(_servletRequestAttributeListener);
  servletRequestAttributeListeners=_bundleServletContext.getServletRequestAttributeListeners();
  Assert.assertEquals(initialSize + 1,servletRequestAttributeListeners.size());
}
