{
  URL url=getClass().getResource("/mysql.properties");
  Assume.assumeNotNull(url);
  File readOnlyFolder=temporaryFolder.newFolder();
  readOnlyFolder.setReadable(false);
  readOnlyFolder.setWritable(false);
  String[] args={"-P",url.getFile(),"-S",_DEFAULT_SCHEMA_NAME,"-C",_DEFAULT_COMPANY_ID,"-O",readOnlyFolder.getAbsolutePath()};
  try {
    Main.main(args);
    Assert.fail();
  }
 catch (  ParameterException pe) {
    Assert.assertEquals("File parameter -O is read-only",pe.getMessage());
  }
}
