{
  URL url=getClass().getResource("/mysql.properties");
  Assume.assumeNotNull(url);
  File readOnlyFolder=temporaryFolder.newFolder();
  readOnlyFolder.setReadable(false);
  readOnlyFolder.setWritable(false);
  String[] args={url.getFile(),_DEFAULT_SCHEMA_NAME,_DEFAULT_COMPANY_ID,readOnlyFolder.getAbsolutePath()};
  try {
    Main.main(args);
    Assert.fail();
  }
 catch (  IllegalArgumentException iae) {
    Assert.assertEquals("Output directory is read-only",iae.getMessage());
  }
}
