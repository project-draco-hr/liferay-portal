{
  URL url=getClass().getResource("/mysql.properties");
  String[] args={"-P",url.getFile(),"-S",_DEFAULT_SCHEMA_NAME,"-C","foo","-O","neverMindPath"};
  try {
    Main.main(args);
    Assert.fail();
  }
 catch (  ParameterException pe) {
    Assert.assertEquals("Parameter -C with value foo is not a valid number",pe.getMessage());
  }
}
