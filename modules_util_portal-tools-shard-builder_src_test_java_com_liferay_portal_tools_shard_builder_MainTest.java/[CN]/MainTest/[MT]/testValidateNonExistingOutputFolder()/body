{
  URL url=getClass().getResource("/mysql.properties");
  String[] args={"-P",url.getFile(),"-S",_DEFAULT_SCHEMA_NAME,"-C",_DEFAULT_COMPANY_ID,"-O","foo"};
  try {
    Main.main(args);
    Assert.fail();
  }
 catch (  ParameterException pe) {
    Assert.assertEquals("File parameter -O does not exist",pe.getMessage());
  }
}
