{
  StopWatch stopWatch=new StopWatch();
  stopWatch.start();
  try {
    invokeEvent(eventRequest,eventResponse);
  }
 catch (  PortletException pe) {
    StateAwareResponseImpl stateAwareResponseImpl=(StateAwareResponseImpl)eventResponse;
    stateAwareResponseImpl.reset();
    eventRequest.setAttribute(_portletId + PortletException.class.getName(),pe);
  }
  if (_log.isDebugEnabled()) {
    _log.debug("processEvent for " + _portletId + " takes "+ stopWatch.getTime()+ " ms");
  }
}
