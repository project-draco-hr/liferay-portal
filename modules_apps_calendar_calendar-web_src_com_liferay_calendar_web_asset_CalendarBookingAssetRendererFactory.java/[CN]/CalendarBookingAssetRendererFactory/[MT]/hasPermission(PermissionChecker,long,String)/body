{
  CalendarBooking calendarBooking=CalendarBookingLocalServiceUtil.getCalendarBooking(classPK);
  if (actionId.equals(CalendarActionKeys.DELETE) || actionId.equals(CalendarActionKeys.UPDATE)) {
    actionId=CalendarActionKeys.MANAGE_BOOKINGS;
  }
  return CalendarPermission.contains(permissionChecker,calendarBooking.getCalendarId(),actionId);
}
