{
  SyncAccount syncAccount=SyncAccountService.fetchSyncAccount(getSyncAccountId());
  SyncAccountService.synchronizeSyncAccount(getSyncAccountId(),RetryUtil.incrementRetryDelay(getSyncAccountId()));
  if (_logger.isDebugEnabled()) {
    _logger.debug("Attempting to reconnect to {}. Retry #{}.",syncAccount.getUrl(),RetryUtil.getRetryCount(getSyncAccountId()));
  }
}
