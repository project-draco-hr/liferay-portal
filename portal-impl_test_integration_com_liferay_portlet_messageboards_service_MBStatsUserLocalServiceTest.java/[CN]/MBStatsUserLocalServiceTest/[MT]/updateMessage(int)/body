{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId(),TestPropsValues.getUserId());
  serviceContext.setLayoutFullURL("http://localhost");
  serviceContext.setWorkflowAction(workflowAction);
  _message=MBMessageLocalServiceUtil.updateMessage(TestPropsValues.getUserId(),_message.getMessageId(),_message.getSubject(),_message.getBody(),Collections.<ObjectValuePair<String,InputStream>>emptyList(),Collections.<String>emptyList(),_message.getPriority(),_message.getAllowPingbacks(),serviceContext);
}
