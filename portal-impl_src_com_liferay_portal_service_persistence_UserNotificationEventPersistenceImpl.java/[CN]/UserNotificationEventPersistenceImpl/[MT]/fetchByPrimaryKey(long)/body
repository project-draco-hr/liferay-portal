{
  UserNotificationEvent userNotificationEvent=(UserNotificationEvent)EntityCacheUtil.getResult(UserNotificationEventModelImpl.ENTITY_CACHE_ENABLED,UserNotificationEventImpl.class,userNotificationEventId,this);
  if (userNotificationEvent == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (userNotificationEvent == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      userNotificationEvent=(UserNotificationEvent)session.get(UserNotificationEventImpl.class,Long.valueOf(userNotificationEventId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (userNotificationEvent == null)) {
        EntityCacheUtil.putResult(UserNotificationEventModelImpl.ENTITY_CACHE_ENABLED,UserNotificationEventImpl.class,userNotificationEventId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(userNotificationEvent);
      }
      closeSession(session);
    }
  }
  return userNotificationEvent;
}
