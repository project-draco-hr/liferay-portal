{
  Session session=null;
  try {
    session=openSession();
    UserNotificationEvent userNotificationEvent=(UserNotificationEvent)session.get(UserNotificationEventImpl.class,Long.valueOf(userNotificationEventId));
    if (userNotificationEvent == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + userNotificationEventId);
      }
      throw new NoSuchUserNotificationEventException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + userNotificationEventId);
    }
    return userNotificationEventPersistence.remove(userNotificationEvent);
  }
 catch (  NoSuchUserNotificationEventException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
