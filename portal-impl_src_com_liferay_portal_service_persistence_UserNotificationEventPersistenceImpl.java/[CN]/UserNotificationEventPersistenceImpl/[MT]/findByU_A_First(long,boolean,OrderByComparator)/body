{
  List<UserNotificationEvent> list=findByU_A(userId,archived,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(6);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("userId=");
    msg.append(userId);
    msg.append(", archived=");
    msg.append(archived);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchUserNotificationEventException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
