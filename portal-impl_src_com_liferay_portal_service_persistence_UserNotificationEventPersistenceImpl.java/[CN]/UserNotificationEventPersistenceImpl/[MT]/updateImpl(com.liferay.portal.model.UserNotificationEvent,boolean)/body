{
  userNotificationEvent=toUnwrappedModel(userNotificationEvent);
  boolean isNew=userNotificationEvent.isNew();
  UserNotificationEventModelImpl userNotificationEventModelImpl=(UserNotificationEventModelImpl)userNotificationEvent;
  if (Validator.isNull(userNotificationEvent.getUuid())) {
    String uuid=PortalUUIDUtil.generate();
    userNotificationEvent.setUuid(uuid);
  }
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,userNotificationEvent,merge);
    userNotificationEvent.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  if (isNew) {
    FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  }
 else {
    if (!Validator.equals(userNotificationEvent.getUuid(),userNotificationEventModelImpl.getOriginalUuid())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_UUID,new Object[]{userNotificationEventModelImpl.getOriginalUuid()});
    }
    if (userNotificationEvent.getUserId() != userNotificationEventModelImpl.getOriginalUserId()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_USERID,new Object[]{Long.valueOf(userNotificationEventModelImpl.getOriginalUserId())});
    }
    if ((userNotificationEvent.getUserId() != userNotificationEventModelImpl.getOriginalUserId()) || (userNotificationEvent.getArchived() != userNotificationEventModelImpl.getOriginalArchived())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_U_A,new Object[]{Long.valueOf(userNotificationEventModelImpl.getOriginalUserId()),Boolean.valueOf(userNotificationEventModelImpl.getOriginalArchived())});
    }
  }
  EntityCacheUtil.putResult(UserNotificationEventModelImpl.ENTITY_CACHE_ENABLED,UserNotificationEventImpl.class,userNotificationEvent.getPrimaryKey(),userNotificationEvent);
  return userNotificationEvent;
}
