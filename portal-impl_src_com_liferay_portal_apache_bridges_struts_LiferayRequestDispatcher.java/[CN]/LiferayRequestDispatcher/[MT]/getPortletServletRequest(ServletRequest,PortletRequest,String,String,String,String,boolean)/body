{
  HttpServletRequest request=(HttpServletRequest)servletRequest;
  boolean named=false;
  if (PropsValues.PORTLET_CONTAINER_IMPL_SUN) {
    String lifecyclePhase=(String)portletRequest.getAttribute(PortletRequest.LIFECYCLE_PHASE);
    return new RDRequestWrapper(null,request,portletRequest,requestURI,servletPath,pathInfo,queryString,lifecyclePhase,named,include);
  }
 else {
    PortletRequestImpl portletRequestImpl=(PortletRequestImpl)portletRequest;
    return new PortletServletRequest(request,portletRequestImpl,pathInfo,queryString,requestURI,servletPath,named,include);
  }
}
