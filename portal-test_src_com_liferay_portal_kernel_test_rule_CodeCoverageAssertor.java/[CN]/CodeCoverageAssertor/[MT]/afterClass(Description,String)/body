{
  List<Class<?>> assertClasses=new ArrayList<>();
  if (className != null) {
    ClassLoader classLoader=getClassLoader();
    Class<?> clazz=classLoader.loadClass(className);
    assertClasses.add(clazz);
  }
  appendAssertClasses(assertClasses);
  try {
    _ASSERT_COVERAGE_METHOD.invoke(null,_includeInnerClasses,assertClasses.toArray(new Class<?>[assertClasses.size()]));
  }
 catch (  InvocationTargetException ite) {
    throw ite.getCause();
  }
}
