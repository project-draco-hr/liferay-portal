{
  if (((changeLog == null) && (_changeLog != null)) || ((changeLog != null) && (_changeLog == null)) || ((changeLog != null) && (_changeLog != null) && !changeLog.equals(_changeLog))) {
    if (!XSS_ALLOW_CHANGELOG) {
      changeLog=XSSUtil.strip(changeLog);
    }
    _changeLog=changeLog;
  }
}
