{
  if (((downloadPageURL == null) && (_downloadPageURL != null)) || ((downloadPageURL != null) && (_downloadPageURL == null)) || ((downloadPageURL != null) && (_downloadPageURL != null) && !downloadPageURL.equals(_downloadPageURL))) {
    if (!XSS_ALLOW_DOWNLOADPAGEURL) {
      downloadPageURL=XSSUtil.strip(downloadPageURL);
    }
    _downloadPageURL=downloadPageURL;
  }
