{
  List<Throwable> throwables=new ArrayList<Throwable>();
  if (_statement != null) {
    try {
      _statement.evaluate();
    }
 catch (    Throwable t) {
      throwables.add(t);
    }
  }
  try {
    _testContextHandler.runAfterTestMethod(_instance,_method);
  }
 catch (  Exception e) {
    throwables.add(e);
  }
  if (!throwables.isEmpty()) {
    throw new MultipleFailureException(throwables);
  }
}
