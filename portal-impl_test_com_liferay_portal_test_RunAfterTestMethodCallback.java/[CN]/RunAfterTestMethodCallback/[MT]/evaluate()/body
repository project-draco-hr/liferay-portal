{
  List<Throwable> errors=new ArrayList<Throwable>();
  if (_next != null) {
    try {
      _next.evaluate();
    }
 catch (    Throwable t) {
      errors.add(t);
    }
  }
  try {
    _testContextHandler.runAfterTestMethod(_testInstance,_method);
  }
 catch (  Exception e) {
    errors.add(e);
  }
  if (!errors.isEmpty()) {
    throw new MultipleFailureException(errors);
  }
}
