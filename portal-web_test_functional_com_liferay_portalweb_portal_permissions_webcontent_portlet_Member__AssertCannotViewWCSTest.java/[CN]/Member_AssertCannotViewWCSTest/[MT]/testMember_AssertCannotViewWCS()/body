{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Web Content Search Test Page",RuntimeVariables.replace("Web Content Search Test Page"));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(5000);
  assertTrue(selenium.isVisible("//section"));
  assertTrue(selenium.isElementNotPresent("//input[@value='Search...']"));
  assertTrue(selenium.isElementNotPresent("//div[@class='portlet-body']/form/span[2]/span/span/input"));
  assertEquals(RuntimeVariables.replace("You do not have the roles required to access this portlet."),selenium.getText("//div[@class='portlet-msg-error']"));
}
