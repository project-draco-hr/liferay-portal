{
  ExpandoColumn expandoColumn=ExpandoColumnLocalServiceUtil.getDefaultTableColumn(_companyId,_modelResource,(String)obj);
  PermissionChecker permissionChecker=PermissionThreadLocal.getPermissionChecker();
  if (!ExpandoColumnPermissionUtil.contains(permissionChecker,expandoColumn,ActionKeys.DELETE)) {
    return true;
  }
  return super.isDisabled(obj);
}
