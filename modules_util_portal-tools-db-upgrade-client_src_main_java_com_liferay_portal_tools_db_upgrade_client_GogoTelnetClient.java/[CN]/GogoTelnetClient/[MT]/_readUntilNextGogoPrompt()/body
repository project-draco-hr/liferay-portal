{
  StringBuilder sb=new StringBuilder();
  int c=_dataInputStream.read();
  while (c != -1) {
    sb.append((char)c);
    String s=sb.toString();
    if (s.endsWith("g! ")) {
      break;
    }
    c=_dataInputStream.read();
  }
  String output=sb.substring(0,sb.length() - 3);
  return output.trim();
}
