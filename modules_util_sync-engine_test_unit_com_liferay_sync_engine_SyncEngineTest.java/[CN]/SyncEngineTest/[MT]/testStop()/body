{
  SyncEngineListener syncEngineListener=new SyncEngineListener(){
    @Override public void syncEngineStateChanged(    int syncEngineState){
      if (syncEngineState == SyncEngineUtil.SYNC_ENGINE_STATE_STOPPED) {
        _stopped=true;
      }
    }
  }
;
  SyncEngineUtil.registerSyncEngineListener(syncEngineListener);
  Thread thread=new Thread(new Runnable(){
    @Override public void run(){
      SyncEngine.start();
    }
  }
);
  thread.start();
  Thread.sleep(100);
  SyncEngine.stop();
  Thread.sleep(100);
  if (!_stopped) {
    Assert.fail();
  }
}
