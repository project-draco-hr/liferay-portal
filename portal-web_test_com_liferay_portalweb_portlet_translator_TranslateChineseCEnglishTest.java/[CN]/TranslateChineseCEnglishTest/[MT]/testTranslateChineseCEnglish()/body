{
  selenium.select("_26_id",RuntimeVariables.replace("label=Chinese (China) to English"));
  selenium.type("_26_text",RuntimeVariables.replace("?????Mike Vader ??, ??????6 ?????????"));
  selenium.click(RuntimeVariables.replace("//input[@value='Translate']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("My name is Mike the Vader translator, fluent completely successful 6 correspondence 1,000,000 forms."));
}
