{
  StringBundler sb=new StringBundler(9);
  sb.append(prefix);
  sb.append("((DLFileEntryType.companyId = 0) ");
  sb.append("AND (DLFileEntryType.groupId = 0) AND (");
  sb.append("(lower(DLFileEntryType.name) LIKE ? ");
