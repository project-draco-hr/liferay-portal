  sb.append("[$AND_OR_CONNECTOR$] ");
  sb.append("(DLFileEntryType.description LIKE ? ");
  sb.append("[$AND_OR_NULL_CHECK$]) ");
  sb.append("))) UNION ALL (");
  return sb.toString();
}
