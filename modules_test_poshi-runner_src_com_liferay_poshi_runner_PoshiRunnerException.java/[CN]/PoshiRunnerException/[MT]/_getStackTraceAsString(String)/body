{
  StringBuilder sb=new StringBuilder();
  if (msg == null) {
    sb.append("\nBUILD FAILED:");
  }
 else {
    sb.append("\n");
    sb.append(msg);
  }
  sb.append("\n");
  Stack<String> stack=PoshiRunnerStackTraceUtil.getStackTrace();
  while (!stack.isEmpty()) {
    sb.append(PoshiRunnerStackTraceUtil.popStackTrace());
  }
  return sb.toString();
}
