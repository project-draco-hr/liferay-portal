{
  Map<String,Facet> facetsMap=searchContext.getFacets();
  for (  Facet facet : facetsMap.values()) {
    if (facet.isStatic()) {
      continue;
    }
    Facets facets=searchResponse.getFacets();
    org.elasticsearch.search.facet.Facet elasticsearchFacet=facets.facet(facet.getFieldName());
    FacetCollector facetCollector=new ElasticsearchFacetFieldCollector(elasticsearchFacet);
    facet.setFacetCollector(facetCollector);
  }
}
