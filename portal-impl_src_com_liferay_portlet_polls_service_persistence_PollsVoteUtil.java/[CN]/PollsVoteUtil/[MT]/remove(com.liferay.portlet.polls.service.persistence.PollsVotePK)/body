{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(pollsVotePK));
  }
  com.liferay.portlet.polls.model.PollsVote pollsVote=getPersistence().remove(pollsVotePK);
  if (listener != null) {
    listener.onAfterRemove(pollsVote);
  }
  return pollsVote;
}
