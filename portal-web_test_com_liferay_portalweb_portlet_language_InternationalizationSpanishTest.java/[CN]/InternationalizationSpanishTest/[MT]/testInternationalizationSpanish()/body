    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//img[@alt='espa\u00f1ol (Espa\u00f1a)']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//div[@id='navigation']/ul/li[3]/a/span",RuntimeVariables.replace(""));
  selenium.typeKeys("new_page",RuntimeVariables.replace("P\u00e1gina de la prueba de lengua"));
  selenium.type("new_page",RuntimeVariables.replace("P\u00e1gina de la prueba de lengua"));
