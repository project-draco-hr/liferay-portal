{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//img[@alt='espa\u00f1ol (Espa\u00f1a)']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("//img[@alt='espa\u00f1ol (Espa\u00f1a)']"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//div[@id='navigation']/ul/li[3]/a/span");
  selenium.typeKeys("new_page",RuntimeVariables.replace("P\u00e1gina de la prueba de lengua"));
  selenium.type("new_page",RuntimeVariables.replace("P\u00e1gina de la prueba de lengua"));
  selenium.click("link=Guardar");
  selenium.click(RuntimeVariables.replace("//img[@alt='English (United States)']"));
  selenium.waitForPageToLoad("30000");
}
