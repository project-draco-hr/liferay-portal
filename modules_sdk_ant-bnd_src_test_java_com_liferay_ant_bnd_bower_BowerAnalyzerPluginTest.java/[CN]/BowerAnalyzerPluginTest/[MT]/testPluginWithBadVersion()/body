{
  Analyzer analyzer=new Analyzer();
  Jar jar=new Jar("test");
  jar.putResource("bower.json",new URLResource(getResource("dependencies/bower.bad.version.json")));
  analyzer.setJar(jar);
  BowerAnalyzerPlugin bowerAnalyzerPlugin=new BowerAnalyzerPlugin();
  bowerAnalyzerPlugin.analyzeJar(analyzer);
  Assert.assertEquals("0.0.0.1word-cha_rs",analyzer.getBundleVersion());
  Assert.assertEquals("/liferay-0.0.0.1word-cha_rs",analyzer.getProperty(BowerAnalyzerPlugin.WEB_CONTEXT_PATH));
  String property=analyzer.getProperty(Constants.PROVIDE_CAPABILITY);
  Assert.assertEquals("osgi.webresource;osgi.webresource=liferay;" + "version:Version=\"0.0.0.1word-cha_rs\"",property);
}
