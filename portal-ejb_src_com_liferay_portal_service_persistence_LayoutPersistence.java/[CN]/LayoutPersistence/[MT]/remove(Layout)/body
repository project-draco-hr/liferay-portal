{
  Session session=null;
  try {
    session=openSession();
    session.delete(layout);
    session.flush();
    LayoutPool.removeByO_F(layout.getOwnerId(),layout.getFriendlyURL());
    return layout;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
