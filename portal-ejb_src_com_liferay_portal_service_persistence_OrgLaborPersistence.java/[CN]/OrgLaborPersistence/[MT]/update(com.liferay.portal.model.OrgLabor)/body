{
  Session session=null;
  try {
    session=openSession();
    if (orgLabor.isNew()) {
      session.save(orgLabor);
    }
    session.flush();
    orgLabor.setNew(false);
    return orgLabor;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
