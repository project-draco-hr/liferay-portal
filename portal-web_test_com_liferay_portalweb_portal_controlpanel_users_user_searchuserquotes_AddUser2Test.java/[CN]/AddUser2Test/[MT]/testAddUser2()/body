{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Control Panel");
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Users and Organizations",RuntimeVariables.replace("Users and Organizations"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Add",RuntimeVariables.replace("Add"));
  selenium.waitForVisible("//div[@class='lfr-component lfr-menu-list']/ul/li/a");
  assertEquals(RuntimeVariables.replace("User"),selenium.getText("//div[@class='lfr-component lfr-menu-list']/ul/li/a"));
  selenium.click(RuntimeVariables.replace("//div[@class='lfr-component lfr-menu-list']/ul/li/a"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_125_screenName']",RuntimeVariables.replace("johndoe"));
  selenium.type("//input[@id='_125_emailAddress']",RuntimeVariables.replace("johndoe@liferay.com"));
  selenium.type("//input[@id='_125_firstName']",RuntimeVariables.replace("John"));
  selenium.type("//input[@id='_125_lastName']",RuntimeVariables.replace("Doe"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForVisible("//div[@class='portlet-msg-success']");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals("johndoe",selenium.getValue("//input[@id='_125_screenName']"));
  assertEquals("johndoe@liferay.com",selenium.getValue("//input[@id='_125_emailAddress']"));
  assertEquals("John",selenium.getValue("//input[@id='_125_firstName']"));
  assertEquals("Doe",selenium.getValue("//input[@id='_125_lastName']"));
  selenium.waitForVisible("//a[@id='_125_addressesLink']");
  assertTrue(selenium.isPartialText("//a[@id='_125_addressesLink']","Addresses"));
  selenium.clickAt("//a[@id='_125_addressesLink']",RuntimeVariables.replace("Addresses"));
  selenium.type("//input[@id='_125_addressStreet1_0']",RuntimeVariables.replace("Second Street"));
  selenium.type("//input[@id='_125_addressZip0']",RuntimeVariables.replace("22222"));
  selenium.type("//input[@id='_125_addressCity0']",RuntimeVariables.replace("Chicago"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForVisible("//div[@class='portlet-msg-success']");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals("Second Street",selenium.getValue("//input[@id='_125_addressStreet1_0']"));
  assertEquals("22222",selenium.getValue("//input[@id='_125_addressZip0']"));
  assertEquals("Chicago",selenium.getValue("//input[@id='_125_addressCity0']"));
}
