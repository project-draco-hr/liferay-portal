{
  final MethodNode methodNode=new MethodNode();
  headMethodNode.accept(new AdviceAdapter(Opcodes.ASM5,methodNode,headMethodNode.access,headMethodNode.name,headMethodNode.desc){
    @Override protected void onMethodExit(    int opcode){
      mv=_emptyMethodVisitor;
    }
  }
);
  tailMethodNode.accept(new AdviceAdapter(Opcodes.ASM5,_emptyMethodVisitor,tailMethodNode.access,tailMethodNode.name,tailMethodNode.desc){
    @Override protected void onMethodEnter(){
      mv=methodNode;
    }
  }
);
  containerMethodNode.instructions=methodNode.instructions;
}
