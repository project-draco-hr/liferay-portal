{
  KaleoTaskInstanceToken kaleoTaskInstanceToken=kaleoTaskInstanceTokenPersistence.findByPrimaryKey(kaleoTaskInstanceTokenId);
  int kaleoTaskFormsCount=kaleoTaskFormPersistence.countByKaleoTaskId(kaleoTaskInstanceToken.getKaleoTaskId());
  int kaleoTaskFormInstancesCount=kaleoTaskFormInstancePersistence.countByKaleoTaskInstanceTokenId(kaleoTaskInstanceTokenId);
  if (kaleoTaskFormsCount > kaleoTaskFormInstancesCount) {
    return true;
  }
  return false;
}
