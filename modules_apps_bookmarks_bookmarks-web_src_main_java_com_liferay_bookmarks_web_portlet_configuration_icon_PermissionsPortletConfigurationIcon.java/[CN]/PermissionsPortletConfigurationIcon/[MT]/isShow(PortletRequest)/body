{
  User user=themeDisplay.getUser();
  if (user.isDefaultUser()) {
    return false;
  }
  PermissionChecker permissionChecker=themeDisplay.getPermissionChecker();
  try {
    if (!GroupPermissionUtil.contains(permissionChecker,themeDisplay.getScopeGroupId(),ActionKeys.PERMISSIONS)) {
      return false;
    }
  }
 catch (  PortalException pe) {
    return false;
  }
  return true;
}
