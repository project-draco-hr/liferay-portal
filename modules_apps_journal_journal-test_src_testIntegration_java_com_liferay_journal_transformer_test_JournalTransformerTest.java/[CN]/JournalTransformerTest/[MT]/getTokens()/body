{
  Map<String,String> tokens=JournalUtil.getTokens(TestPropsValues.getGroupId(),(PortletRequestModel)null,null);
  tokens.put(TemplateConstants.CLASS_NAME_ID,String.valueOf(ClassNameLocalServiceUtil.getClassNameId(DDMStructure.class.getName())));
  tokens.put("article_group_id",String.valueOf(TestPropsValues.getGroupId()));
  tokens.put("ddm_structure_id",String.valueOf(_ddmStructure.getStructureId()));
  tokens.put("company_id",String.valueOf(TestPropsValues.getCompanyId()));
  return tokens;
}
