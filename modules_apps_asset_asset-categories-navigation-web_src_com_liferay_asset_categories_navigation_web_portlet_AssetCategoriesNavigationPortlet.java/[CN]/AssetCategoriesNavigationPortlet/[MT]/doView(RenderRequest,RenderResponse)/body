{
  ThemeDisplay themeDisplay=(ThemeDisplay)renderRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  try {
    AssetCategoriesNavigationPortletInstanceConfiguration assetCategoriesNavigationPortletInstanceConfiguration=_settingsFactory.getSettings(AssetCategoriesNavigationPortletInstanceConfiguration.class,new PortletInstanceSettingsLocator(themeDisplay.getLayout(),portletDisplay.getId()));
    renderRequest.setAttribute(AssetCategoriesNavigationPortletInstanceConfiguration.class.getName(),assetCategoriesNavigationPortletInstanceConfiguration);
  }
 catch (  PortalException pe) {
    throw new SystemException(pe);
  }
  super.doView(renderRequest,renderResponse);
}
