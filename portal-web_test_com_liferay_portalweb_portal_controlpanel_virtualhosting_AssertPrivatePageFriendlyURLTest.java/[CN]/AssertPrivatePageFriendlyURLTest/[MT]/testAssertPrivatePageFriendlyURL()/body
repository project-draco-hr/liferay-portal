{
  selenium.open("/group/alpha/");
  assertEquals(RuntimeVariables.replace("http://localhost:8080/group/alpha/"),selenium.getLocation());
  assertEquals(RuntimeVariables.replace("Virtual Hosting Community"),selenium.getText("//li[2]/span/a"));
  assertTrue(selenium.isElementPresent("link=Private Page"));
  selenium.clickAt("link=Private Page",RuntimeVariables.replace("Private Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("http://localhost:8080/group/alpha/private-page"),selenium.getLocation());
}
