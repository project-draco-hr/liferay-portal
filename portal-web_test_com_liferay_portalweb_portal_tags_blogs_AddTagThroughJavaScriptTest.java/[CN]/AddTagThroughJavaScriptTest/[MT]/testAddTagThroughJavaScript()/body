    Thread.sleep(1000);
  }
  selenium.clickAt("link=Tags1 Blogs1 Test1 Entry1",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
