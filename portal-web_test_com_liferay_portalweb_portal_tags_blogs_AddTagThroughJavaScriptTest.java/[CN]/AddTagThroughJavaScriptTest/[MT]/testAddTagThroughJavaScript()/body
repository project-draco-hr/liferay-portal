    try {
      if (selenium.isElementPresent("//li[5]/span/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Edit",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(5000);
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//div[7]/div/input[2]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.typeKeys("//div[7]/div/input[2]",RuntimeVariables.replace("selenium2 lifera2"));
  selenium.type("//div[7]/div/input[2]",RuntimeVariables.replace("selenium2 liferay2"));
  selenium.clickAt("//div[7]/div/input[3]",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//span/span[2]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("selenium2 liferay2x"),selenium.getText("//span/span[2]"));
