{
  if (isAdvancedSearch()) {
    if (Validator.isNotNull(city) || countryId > 0 || Validator.isNotNull(name) || regionId > 0 || Validator.isNotNull(street) || Validator.isNotNull(type) || Validator.isNotNull(zip)) {
      return true;
    }
  }
 else {
    if (Validator.isNotNull(keywords)) {
      return true;
    }
  }
  return false;
}
