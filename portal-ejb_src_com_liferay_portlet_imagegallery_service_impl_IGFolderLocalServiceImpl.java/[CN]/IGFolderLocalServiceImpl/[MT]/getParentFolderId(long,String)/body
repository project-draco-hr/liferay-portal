{
  if (!parentFolderId.equals(IGFolderImpl.DEFAULT_PARENT_FOLDER_ID)) {
    IGFolder parentFolder=IGFolderUtil.fetchByPrimaryKey(parentFolderId);
    if ((parentFolder == null) || (groupId != parentFolder.getGroupId())) {
      parentFolderId=IGFolderImpl.DEFAULT_PARENT_FOLDER_ID;
    }
  }
  return parentFolderId;
}
