{
  ModelListener listener=null;
  if (Validator.isNotNull(_LISTENER)) {
    try {
      listener=(ModelListener)Class.forName(_LISTENER).newInstance();
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(journalArticlePK));
  }
  com.liferay.portlet.journal.model.JournalArticle journalArticle=getPersistence().remove(journalArticlePK);
  if (listener != null) {
    listener.onAfterRemove(journalArticle);
  }
  return journalArticle;
}
