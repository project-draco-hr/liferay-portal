{
  while (url.endsWith("/")) {
    int x=url.length() - 1;
    url=url.substring(0,x);
  }
  if (path.equals("")) {
    return url;
  }
  while (path.startsWith("/")) {
    if (path.equals("/")) {
      return url;
    }
 else {
      path=path.substring(1);
    }
  }
  while (path.endsWith("/")) {
    int x=path.length() - 1;
    path=path.substring(0,x);
  }
  StringBuilder sb=new StringBuilder();
  sb.append(url);
  sb.append("/");
  sb.append(path);
  return sb.toString();
}
