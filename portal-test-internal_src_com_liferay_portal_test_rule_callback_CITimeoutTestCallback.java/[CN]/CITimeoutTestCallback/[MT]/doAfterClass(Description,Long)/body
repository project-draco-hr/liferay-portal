{
  long testTime=System.currentTimeMillis() - startTime;
  if (testTime <= TestPropsValues.CI_TEST_TIMEOUT_TIME) {
    return;
  }
  String message=description.getClassName() + " spent " + testTime+ "ms, passed the timeout threshold "+ TestPropsValues.CI_TEST_TIMEOUT_TIME+ "ms.";
  System.setProperty(_TIMEOUT_TEST_CLASS_MESSAGE,message);
  Assert.fail(message + " Marked it as failed and aborting " + "all following tests");
}
