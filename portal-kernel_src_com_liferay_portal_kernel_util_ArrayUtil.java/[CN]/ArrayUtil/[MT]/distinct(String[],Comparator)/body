{
  if ((array == null) || (array.length == 0)) {
    return array;
  }
  Set set=null;
  if (comparator == null) {
    set=new TreeSet();
  }
 else {
    set=new TreeSet(comparator);
  }
  for (int i=0; i < array.length; i++) {
    Object obj=array[i];
    if (!set.contains(obj)) {
      set.add(obj);
    }
  }
  return (String[])set.toArray(new String[0]);
}
