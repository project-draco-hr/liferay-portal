{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLFileShortcut.class,DLFileShortcut.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("fileShortcutId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("fileShortcutId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
