{
  Object value=properties.get("notification.type");
  String[] notificationTypes=GetterUtil.getStringValues(value,new String[]{String.valueOf(value)});
  if (ArrayUtil.isEmpty(notificationTypes)) {
    throw new IllegalArgumentException("Must have a notification.type property for: " + ClassUtil.getClassName(notificationSender));
  }
  return notificationTypes;
}
