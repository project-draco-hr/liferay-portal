{
  if (_latestArticle != null) {
    return _latestArticle;
  }
  JournalArticleDisplay articleDisplay=getArticleDisplay();
  if (articleDisplay == null) {
    return null;
  }
  _latestArticle=JournalArticleLocalServiceUtil.fetchLatestArticle(articleDisplay.getGroupId(),articleDisplay.getArticleId(),WorkflowConstants.STATUS_ANY);
  return _latestArticle;
}
