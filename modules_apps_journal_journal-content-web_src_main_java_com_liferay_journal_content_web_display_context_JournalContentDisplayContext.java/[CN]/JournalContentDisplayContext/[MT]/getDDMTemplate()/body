{
  if (_ddmTemplate != null) {
    return _ddmTemplate;
  }
  JournalArticleDisplay articleDisplay=getArticleDisplay();
  if ((articleDisplay == null) || Validator.isNull(articleDisplay.getDDMTemplateKey())) {
    return null;
  }
  _ddmTemplate=DDMTemplateLocalServiceUtil.fetchTemplate(articleDisplay.getGroupId(),PortalUtil.getClassNameId(DDMStructure.class),getDDMTemplateKey(),true);
  return _ddmTemplate;
}
