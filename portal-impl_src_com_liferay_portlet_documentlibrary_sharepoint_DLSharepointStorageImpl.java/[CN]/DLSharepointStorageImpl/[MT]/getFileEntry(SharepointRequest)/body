{
  String parentFolderPath=getParentFolderPath(sharepointRequest);
  long groupId=getGroupId(parentFolderPath);
  long parentFolderId=getLastFolderId(groupId,parentFolderPath,DLFolderImpl.DEFAULT_PARENT_FOLDER_ID);
  String title=getResourceName(sharepointRequest);
  return DLFileEntryServiceUtil.getFileEntryByTitle(parentFolderId,title);
}
