{
  try {
    MethodKey methodKey=new MethodKey(_className,methodName,parameterTypes);
    RequestStatistics requestStatistics=_methodRequestStatistics.get(methodKey);
    if (requestStatistics != null) {
      return requestStatistics.getErrorCount();
    }
  }
 catch (  ClassNotFoundException cnfe) {
    throw new SystemException(cnfe);
  }
  return -1;
}
