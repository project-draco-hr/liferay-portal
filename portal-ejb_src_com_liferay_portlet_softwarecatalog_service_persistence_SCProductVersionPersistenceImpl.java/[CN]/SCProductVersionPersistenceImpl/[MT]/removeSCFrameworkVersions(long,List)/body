{
  try {
    for (int i=0; i < scFrameworkVersions.size(); i++) {
      com.liferay.portlet.softwarecatalog.model.SCFrameworkVersion scFrameworkVersion=(com.liferay.portlet.softwarecatalog.model.SCFrameworkVersion)scFrameworkVersions.get(i);
      removeSCFrameworkVersion.remove(pk,scFrameworkVersion.getPrimaryKey());
    }
  }
 catch (  DataAccessException dae) {
    throw new SystemException(dae);
  }
}
