{
  if (!_localTransportAddresses.contains(message.getSrc()) || (message.getDest() != null)) {
    _clusterForwardMessageListener.receive((com.liferay.portal.kernel.messaging.Message)message.getObject());
  }
 else {
    if (_log.isDebugEnabled()) {
      _log.debug("Block received message " + message);
    }
  }
}
