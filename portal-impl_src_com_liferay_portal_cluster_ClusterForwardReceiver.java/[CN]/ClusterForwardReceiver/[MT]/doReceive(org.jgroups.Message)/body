{
  List<Address> localTransportAddresses=_clusterLinkImpl.getLocalTransportAddresses();
  if (localTransportAddresses.contains(jGroupsMessage.getSrc())) {
    if (_log.isDebugEnabled()) {
      _log.debug("Block received message " + jGroupsMessage);
    }
  }
 else {
    _clusterLinkImpl.sendLocalMessage((Message)jGroupsMessage.getObject());
  }
}
