{
  KaleoLog kaleoLog=findByPrimaryKey(kaleoLogId);
  Session session=null;
  try {
    session=openSession();
    KaleoLog[] array=new KaleoLogImpl[3];
    array[0]=getByKaleoInstanceId_PrevAndNext(session,kaleoLog,kaleoInstanceId,orderByComparator,true);
    array[1]=kaleoLog;
    array[2]=getByKaleoInstanceId_PrevAndNext(session,kaleoLog,kaleoInstanceId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
