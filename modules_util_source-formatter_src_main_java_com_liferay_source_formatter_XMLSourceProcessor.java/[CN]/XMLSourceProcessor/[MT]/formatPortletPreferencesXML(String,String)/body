{
  Document document=readXML(content);
  checkOrder(fileName,document.getRootElement(),"preference",null,new PortletPreferenceElementComparator());
  Matcher matcher=_incorrectDefaultPreferencesFileName.matcher(fileName);
  if (matcher.find()) {
    String correctFileName=matcher.group(1) + "-default-portlet-preferences.xml";
    processMessage(fileName,"Rename file to " + correctFileName);
  }
}
