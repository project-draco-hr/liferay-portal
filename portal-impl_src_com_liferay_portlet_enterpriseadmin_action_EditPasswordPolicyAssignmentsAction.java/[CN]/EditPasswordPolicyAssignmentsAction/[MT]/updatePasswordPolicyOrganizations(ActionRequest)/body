{
  long passwordPolicyId=ParamUtil.getLong(req,"passwordPolicyId");
  long[] addOrganizationIds=StringUtil.split(ParamUtil.getString(req,"addOrganizationIds"),0L);
  long[] removeOrganizationIds=StringUtil.split(ParamUtil.getString(req,"removeOrganizationIds"),0L);
  OrganizationServiceUtil.addPasswordPolicyOrganizations(passwordPolicyId,addOrganizationIds);
  OrganizationServiceUtil.unsetPasswordPolicyOrganizations(passwordPolicyId,removeOrganizationIds);
}
