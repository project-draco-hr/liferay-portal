{
  long passwordPolicyId=ParamUtil.getLong(actionRequest,"passwordPolicyId");
  long[] addOrganizationIds=StringUtil.split(ParamUtil.getString(actionRequest,"addOrganizationIds"),0L);
  long[] removeOrganizationIds=StringUtil.split(ParamUtil.getString(actionRequest,"removeOrganizationIds"),0L);
  OrganizationServiceUtil.addPasswordPolicyOrganizations(passwordPolicyId,addOrganizationIds);
  OrganizationServiceUtil.unsetPasswordPolicyOrganizations(passwordPolicyId,removeOrganizationIds);
}
