{
  if (isEscapedModel()) {
    return (ExpandoRow)this;
  }
 else {
    ExpandoRow model=new ExpandoRowImpl();
    model.setNew(isNew());
    model.setEscapedModel(true);
    model.setRowId(getRowId());
    model.setTableId(getTableId());
    model.setClassPK(getClassPK());
    model=(ExpandoRow)Proxy.newProxyInstance(ExpandoRow.class.getClassLoader(),new Class[]{ExpandoRow.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
