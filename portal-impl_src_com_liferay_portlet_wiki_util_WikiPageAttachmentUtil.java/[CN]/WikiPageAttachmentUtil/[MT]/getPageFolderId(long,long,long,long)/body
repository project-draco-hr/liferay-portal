{
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  long repositoryId=PortletFileRepositoryUtil.getPortletRepository(groupId,PortletKeys.WIKI,serviceContext);
  long nodeFolderId=PortletFileRepositoryUtil.getFolder(userId,repositoryId,DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,String.valueOf(nodeId),serviceContext);
  return PortletFileRepositoryUtil.getFolder(userId,repositoryId,nodeFolderId,String.valueOf(pageId),serviceContext);
}
