{
  int c=get();
  if (c == '/') {
switch (peek()) {
case '/':
      for (; ; ) {
        c=get();
        if (c <= '\n') {
          return c;
        }
      }
case '*':
    get();
  for (; ; ) {
switch (get()) {
case '*':
      if (peek() == '/') {
        get();
        return ' ';
      }
    break;
case EOF:
  throw new UnterminatedCommentException();
}
}
default :
return c;
}
}
return c;
}
