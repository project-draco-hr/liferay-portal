{
  try {
    String key=ACTION_NAME_PREFIX + action;
    String value=LanguageUtil.get(pageContext,key,null);
    if (value == null) {
      value=PortletResourceBundles.getString(pageContext,key);
    }
    if (value == null) {
      value=key;
    }
    return value;
  }
 catch (  LanguageException le) {
    return action;
  }
}
