{
  Class<?> clazz=this.getValue();
  if (this == BASE64) {
    return new ObjectValuePair<>(clazz.getName(),StringPool.BLANK);
  }
 else   if (this == FILE_ENTRY) {
    return new ObjectValuePair<>(clazz.getName(),String.valueOf(fileEntry.getFileEntryId()));
  }
 else {
    return new ObjectValuePair<>(clazz.getName(),DLUtil.getImagePreviewURL(fileEntry,themeDisplay));
  }
}
