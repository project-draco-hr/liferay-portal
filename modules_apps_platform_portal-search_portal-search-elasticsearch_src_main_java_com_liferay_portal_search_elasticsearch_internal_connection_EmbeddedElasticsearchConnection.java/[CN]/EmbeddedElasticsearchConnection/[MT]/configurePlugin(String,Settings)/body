{
  String version=Version.CURRENT.toString();
  String zip="/plugins/" + name + "-"+ version+ ".zip";
  try {
    EmbeddedElasticsearchPluginManager.installPlugin(name,zip,getClass(),settings);
  }
 catch (  IOException ioe) {
    throw new RuntimeException("Unable to install " + name + " plugin from "+ zip,ioe);
  }
}
