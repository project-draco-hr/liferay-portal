{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId());
  serviceContext.setAddGroupPermissions(false);
  serviceContext.setAddGuestPermissions(false);
  AssetVocabulary assetVocabulary=AssetVocabularyLocalServiceUtil.addVocabulary(TestPropsValues.getUserId(),RandomTestUtil.randomString(),ServiceContextTestUtil.getServiceContext(_group.getGroupId()));
  addAssetCategories(assetVocabulary.getVocabularyId());
}
