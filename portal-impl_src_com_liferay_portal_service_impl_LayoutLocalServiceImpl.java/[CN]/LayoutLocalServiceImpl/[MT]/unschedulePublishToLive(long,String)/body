{
  try {
    SchedulerRequest schedulerRequest=new SchedulerRequest(SchedulerRequest.COMMAND_UNREGISTER,jobName,getSchedulerGroupName(liveGroupId));
    MessageBusUtil.sendMessage(DestinationNames.SCHEDULER,JSONUtil.serialize(schedulerRequest));
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
