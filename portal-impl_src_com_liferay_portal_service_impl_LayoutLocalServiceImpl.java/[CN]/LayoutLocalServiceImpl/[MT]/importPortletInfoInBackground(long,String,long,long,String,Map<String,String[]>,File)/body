{
  Map<String,Serializable> taskContextMap=createTaskContextMap(plid,groupId,portletId,parameterMap,null,null);
  BTEntry btEntry=btEntryLocalService.addEntry(userId,groupId,taskName,null,PortletImportBackgroundTaskExecutor.class,taskContextMap,new ServiceContext());
  btEntryLocalService.addEntryAttachment(userId,btEntry.getBtEntryId(),file.getName(),file);
  return btEntry.getBtEntryId();
}
