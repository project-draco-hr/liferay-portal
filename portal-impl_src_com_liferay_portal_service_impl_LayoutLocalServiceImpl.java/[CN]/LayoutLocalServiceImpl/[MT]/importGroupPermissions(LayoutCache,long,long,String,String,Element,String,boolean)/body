{
  Element actionEl=parentEl.element(elName);
  if (actionEl == null) {
    return;
  }
  List actions=getActions(actionEl);
  Resource resource=layoutCache.getResource(companyId,groupId,resourceName,ResourceImpl.SCOPE_INDIVIDUAL,resourcePrimKey,portletActions);
  permissionLocalService.setGroupPermissions(groupId,(String[])actions.toArray(new String[0]),resource.getResourceId());
}
