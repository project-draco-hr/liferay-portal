{
  Map<String,Serializable> taskContextMap=createTaskContextMap(plid,groupId,portletId,parameterMap,startDate,endDate);
  BTEntry btEntry=btEntryLocalService.addEntry(userId,groupId,taskName,null,PortletExportBackgroundTaskExecutor.class,taskContextMap,new ServiceContext());
  return btEntry.getBtEntryId();
}
