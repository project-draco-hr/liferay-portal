{
  List<Layout> layouts=layoutPersistence.findByG_P(groupId,privateLayout);
  for (  Layout layout : layouts) {
    int nextPriority=helper.getNextPriority(layout.getGroupId(),layout.isPrivateLayout(),layout.getParentLayoutId(),layout.getSourcePrototypeLayoutUuid(),layout.getPriority());
    layout.setPriority(nextPriority);
    layoutPersistence.update(layout,false);
  }
}
