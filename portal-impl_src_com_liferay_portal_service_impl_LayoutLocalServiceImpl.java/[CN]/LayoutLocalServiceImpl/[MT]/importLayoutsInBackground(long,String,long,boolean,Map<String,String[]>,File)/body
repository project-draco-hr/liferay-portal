{
  Map<String,Serializable> taskContextMap=buildTaskContextMap(userId,groupId,privateLayout,null,parameterMap,null,null);
  BackgroundTask backgroundTask=backgroundTaskLocalService.addBackgroundTask(userId,groupId,taskName,null,LayoutImportBackgroundTaskExecutor.class,taskContextMap,new ServiceContext());
  backgroundTaskLocalService.addBackgroundTaskAttachment(userId,backgroundTask.getBackgroundTaskId(),file.getName(),file);
  return backgroundTask.getBackgroundTaskId();
}
