{
  Map<String,Serializable> settingsMap=ExportImportConfigurationSettingsMapFactory.buildSettingsMap(userId,groupId,privateLayout,null,parameterMap,Constants.IMPORT,null,null,file.getName());
  ServiceContext serviceContext=new ServiceContext();
  ExportImportConfiguration exportImportConfiguration=exportImportConfigurationLocalService.addExportImportConfiguration(userId,groupId,taskName,StringPool.BLANK,ExportImportConfigurationConstants.TYPE_IMPORT_LAYOUT,settingsMap,WorkflowConstants.STATUS_DRAFT,serviceContext);
  return importLayoutsInBackground(userId,exportImportConfiguration,file);
}
