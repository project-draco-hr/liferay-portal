{
  Map<String,Serializable> taskContextMap=buildTaskContextMap(groupId,privateLayout,null,parameterMap,null,null);
  BTEntry btEntry=btEntryLocalService.addEntry(userId,groupId,taskName,null,LayoutImportBackgroundTaskExecutor.class,taskContextMap,new ServiceContext());
  btEntryLocalService.addEntryAttachment(userId,btEntry.getBtEntryId(),file.getName(),file);
  return btEntry.getBtEntryId();
}
