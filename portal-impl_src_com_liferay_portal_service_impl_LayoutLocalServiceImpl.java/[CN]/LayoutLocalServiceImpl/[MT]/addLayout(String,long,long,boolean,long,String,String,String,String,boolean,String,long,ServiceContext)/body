{
  Map<Locale,String> localeNamesMap=new HashMap<Locale,String>();
  Locale defaultLocale=LocaleUtil.getDefault();
  localeNamesMap.put(defaultLocale,name);
  return addLayout(uuid,userId,groupId,privateLayout,parentLayoutId,localeNamesMap,null,description,type,hidden,friendlyURL,dlFolderId,serviceContext);
}
