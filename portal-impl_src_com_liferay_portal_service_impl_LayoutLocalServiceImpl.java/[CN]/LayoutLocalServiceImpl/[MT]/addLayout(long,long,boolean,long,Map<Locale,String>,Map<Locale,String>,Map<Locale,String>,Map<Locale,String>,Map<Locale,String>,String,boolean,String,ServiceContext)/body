{
  Map<Locale,String> friendlyURLMap=new HashMap<Locale,String>();
  friendlyURLMap.put(LocaleUtil.getSiteDefault(),friendlyURL);
  return addLayout(userId,groupId,privateLayout,parentLayoutId,nameMap,titleMap,descriptionMap,keywordsMap,robotsMap,type,StringPool.BLANK,hidden,friendlyURLMap,serviceContext);
}
