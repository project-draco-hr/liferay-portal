{
  if (layout.getLayoutId() == layoutId) {
    return true;
  }
 else {
    Iterator itr=layout.getChildren().iterator();
    while (itr.hasNext()) {
      Layout childLayout=(Layout)itr.next();
      if (isDescendant(childLayout,layoutId)) {
        return true;
      }
    }
    return false;
  }
}
