{
  Map<String,Serializable> taskContextMap=buildTaskContextMap(groupId,privateLayout,layoutIds,parameterMap,startDate,endDate);
  BTEntry btEntry=btEntryLocalService.addEntry(userId,groupId,taskName,null,LayoutExportBackgroundTaskExecutor.class,taskContextMap,new ServiceContext());
  return btEntry.getBtEntryId();
}
