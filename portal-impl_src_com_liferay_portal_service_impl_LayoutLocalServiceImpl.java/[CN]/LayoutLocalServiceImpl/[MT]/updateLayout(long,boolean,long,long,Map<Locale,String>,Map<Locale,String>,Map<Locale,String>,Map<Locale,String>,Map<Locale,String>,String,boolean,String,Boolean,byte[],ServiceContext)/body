{
  Map<Locale,String> friendlyURLMap=new HashMap<Locale,String>();
  friendlyURLMap.put(LocaleUtil.getSiteDefault(),friendlyURL);
  return updateLayout(groupId,privateLayout,layoutId,parentLayoutId,nameMap,titleMap,descriptionMap,keywordsMap,robotsMap,type,hidden,friendlyURLMap,iconImage,iconBytes,serviceContext);
}
