{
  StringMaker sm=new StringMaker();
  sm.append(context.getRootPath());
  sm.append("/ratings/");
  sm.append(PortalUtil.getClassNameId(className));
  sm.append(CharPool.FORWARD_SLASH);
  sm.append(classPK);
  sm.append(CharPool.FORWARD_SLASH);
  sm.append(rating.getEntryId());
  sm.append(".xml");
  return sm.toString();
}
