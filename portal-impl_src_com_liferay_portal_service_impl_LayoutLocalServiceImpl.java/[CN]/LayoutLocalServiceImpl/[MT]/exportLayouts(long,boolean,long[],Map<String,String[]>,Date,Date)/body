{
  FileCacheOutputStream fcos=exportLayoutsAsStream(groupId,privateLayout,layoutIds,parameterMap,startDate,endDate);
  byte[] bytes=null;
  try {
    bytes=fcos.getBytes();
  }
 catch (  IOException ioe) {
    throw new SystemException(ioe);
  }
 finally {
    fcos.cleanUp();
  }
  return bytes;
}
