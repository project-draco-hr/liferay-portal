{
  String articleId=getArticleId(actionRequest);
  setPreference(actionRequest,"articleId",articleId);
  long articleGroupId=getArticleGroupId(actionRequest);
  setPreference(actionRequest,"articleGroupId",String.valueOf(articleGroupId));
  ServiceBeanMethodInvocationFactoryUtil.proceed(this,ConfigurationActionImpl.class,_doProcessActionMethod,new Object[]{portletConfig,actionRequest,actionResponse},new String[]{"transactionAdvice"});
}
