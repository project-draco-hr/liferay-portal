{
  if (_methodInterceptors != null) {
    return _methodInterceptors;
  }
  List<MethodInterceptor> methodInterceptors=new ArrayList<MethodInterceptor>();
  MethodInterceptor methodInterceptor=(MethodInterceptor)PortalBeanLocatorUtil.locate("transactionAdvice");
  methodInterceptors.add(methodInterceptor);
  _methodInterceptors=methodInterceptors;
  return _methodInterceptors;
}
