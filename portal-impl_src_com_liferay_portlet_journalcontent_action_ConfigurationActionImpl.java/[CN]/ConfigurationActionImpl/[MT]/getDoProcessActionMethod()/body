{
  if (_doProcessActionMethod != null) {
    return _doProcessActionMethod;
  }
  Class<?> clazz=getClass();
  try {
    _doProcessActionMethod=clazz.getMethod("doProcessAction",new Class<?>[]{PortletConfig.class,ActionRequest.class,ActionResponse.class});
  }
 catch (  Exception e) {
    throw new java.lang.IllegalStateException(e);
  }
  return _doProcessActionMethod;
}
