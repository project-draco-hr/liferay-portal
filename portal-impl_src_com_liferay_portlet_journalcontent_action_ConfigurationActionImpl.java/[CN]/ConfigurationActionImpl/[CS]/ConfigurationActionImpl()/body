{
  Method doProcessActionMethod=null;
  try {
    Class<?> clazz=getClass();
    doProcessActionMethod=clazz.getDeclaredMethod("doProcessAction",new Class<?>[]{PortletConfig.class,ActionRequest.class,ActionResponse.class});
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  _doProcessActionMethod=doProcessActionMethod;
}
