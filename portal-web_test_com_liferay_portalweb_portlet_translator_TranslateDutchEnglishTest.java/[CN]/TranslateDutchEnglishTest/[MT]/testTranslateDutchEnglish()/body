{
  selenium.select("_26_id","label=Dutch to English");
  selenium.type("_26_text","Mijn naam is Vertaler Liferay, vloeiend in meer dan 6 miljoen vormen van mededeling.");
  selenium.click("//input[@value='Translate']");
  selenium.waitForPageToLoad("30000");
  verifyTrue(selenium.isTextPresent("My name is translator Liferay, fluently in more than 6 millions forms of communication."));
}
