{
  selenium.select("_26_id",RuntimeVariables.replace("label=Dutch to English"));
  selenium.type("_26_text",RuntimeVariables.replace("Mijn naam is Vertaler Liferay, vloeiend in meer dan 6 miljoen vormen van mededeling."));
  selenium.click(RuntimeVariables.replace("//input[@value='Translate']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("My name is translator Liferay, fluently in more than 6 millions forms of communication."));
}
