{
  String columnId=(String)attributes.get("id");
  sb.append("<div id=\"layout-column_");
  sb.append(columnId);
  sb.append("\">");
  LayoutTypePortlet layoutTypePortlet=_themeDisplay.getLayoutTypePortlet();
  List portlets=layoutTypePortlet.getAllPortlets(columnId);
  for (int i=0; i < portlets.size(); i++) {
    Portlet portlet=(Portlet)portlets.get(i);
    String rootPortletId=portlet.getRootPortletId();
    String instanceId=portlet.getInstanceId();
    RuntimePortletUtil.processPortlet(sb,_ctx,_req,_res,null,null,rootPortletId,instanceId,columnId,new Integer(i),new Integer(portlets.size()));
  }
  sb.append("<div class=\"layout-blank-portlet\"></div>");
  sb.append("</div>");
}
