{
  ServletContext ctx=(ServletContext)req.getAttribute(WebKeys.CTX);
  String companyId=PortalUtil.getCompanyId(req);
  String portletId=ParamUtil.getString(req,"p_p_id");
  Portlet portlet=PortletServiceUtil.getPortletById(companyId,portletId);
  String columnId=ParamUtil.getString(req,"p_p_col_id");
  int columnPos=ParamUtil.getInteger(req,"p_p_col_pos");
  int columnCount=ParamUtil.getInteger(req,"p_p_col_count");
  PortalUtil.renderPortlet(null,ctx,req,res,portlet,columnId,new Integer(columnPos),new Integer(columnCount));
  return null;
}
