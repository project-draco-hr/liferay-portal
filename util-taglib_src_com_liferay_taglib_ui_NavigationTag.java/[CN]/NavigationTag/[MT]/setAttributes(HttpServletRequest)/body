{
  request.setAttribute("liferay-ui:navigation:bulletStyle",_bulletStyle);
  request.setAttribute("liferay-ui:navigation:displayStyle",getDisplayStyle());
  request.setAttribute("liferay-ui:navigation:displayStyleGroupId",String.valueOf(_ddmTemplateGroupId));
  request.setAttribute("liferay-ui:navigation:headerType",_headerType);
  request.setAttribute("liferay-ui:navigation:includedLayouts",_includedLayouts);
  request.setAttribute("liferay-ui:navigation:nestedChildren",String.valueOf(_nestedChildren));
  request.setAttribute("liferay-ui:navigation:preview",String.valueOf(_preview));
  request.setAttribute("liferay-ui:navigation:rootLayoutLevel",String.valueOf(_rootLayoutLevel));
  request.setAttribute("liferay-ui:navigation:rootLayoutType",_rootLayoutType);
  try {
    List<NavItem> branchNavItems=getBranchNavItems(request);
    request.setAttribute("liferay-ui:navigation:navItems",branchNavItems);
  }
 catch (  PortalException e) {
    _log.error(e);
  }
}
