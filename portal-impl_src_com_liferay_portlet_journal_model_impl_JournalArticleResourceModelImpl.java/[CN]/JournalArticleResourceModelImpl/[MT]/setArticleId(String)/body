{
  if (((articleId == null) && (_articleId != null)) || ((articleId != null) && (_articleId == null)) || ((articleId != null) && (_articleId != null) && !articleId.equals(_articleId))) {
    if (!XSS_ALLOW_ARTICLEID) {
      articleId=XSSUtil.strip(articleId);
