{
  FileCollection fileCollection=compileThemeTask.getFrontendThemeFiles();
  if ((fileCollection != null) && !fileCollection.isEmpty()) {
    return;
  }
  Configuration configuration=GradleUtil.getConfiguration(compileThemeTask.getProject(),FRONTEND_THEMES_CONFIGURATION_NAME);
  compileThemeTask.setFrontendThemeFiles(configuration);
}
