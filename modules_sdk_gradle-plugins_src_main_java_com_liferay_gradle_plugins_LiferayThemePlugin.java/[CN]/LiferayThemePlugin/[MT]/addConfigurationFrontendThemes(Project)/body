{
  Configuration configuration=GradleUtil.addConfiguration(project,FRONTEND_THEMES_CONFIGURATION_NAME);
  configuration.setDescription("Configures com.liferay.frontend.theme.* for compiling themes.");
  configuration.setVisible(false);
  GradleUtil.executeIfEmpty(configuration,new Action<Configuration>(){
    @Override public void execute(    Configuration configuration){
      addDependenciesFrontendThemes(project);
    }
  }
);
  return configuration;
}
