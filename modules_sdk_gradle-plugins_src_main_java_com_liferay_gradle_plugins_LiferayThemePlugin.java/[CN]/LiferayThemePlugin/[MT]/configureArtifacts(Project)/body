{
  ArtifactHandler artifacts=project.getArtifacts();
  File warFile=getWarFile(project);
  artifacts.add(Dependency.ARCHIVES_CONFIGURATION,warFile,new Closure<Void>(project){
    @SuppressWarnings("unused") public void doCall(    ConfigurablePublishArtifact configurablePublishArtifact){
      Task gulpBuildTask=GradleUtil.getTask(project,GULP_BUILD_TASK_NAME);
      configurablePublishArtifact.builtBy(gulpBuildTask);
    }
  }
);
}
