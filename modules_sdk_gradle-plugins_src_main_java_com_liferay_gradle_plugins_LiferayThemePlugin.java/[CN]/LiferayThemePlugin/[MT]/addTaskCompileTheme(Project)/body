{
  CompileThemeTask compileThemeTask=GradleUtil.addTask(project,COMPILE_THEME_TASK_NAME,CompileThemeTask.class);
  compileThemeTask.dependsOn(BUILD_THUMBNAILS_TASK_NAME);
  compileThemeTask.setDescription("Compiles the theme by merging the \"diffs\" directory with the " + "parent theme.");
  compileThemeTask.setGroup(BasePlugin.BUILD_GROUP);
  return compileThemeTask;
}
