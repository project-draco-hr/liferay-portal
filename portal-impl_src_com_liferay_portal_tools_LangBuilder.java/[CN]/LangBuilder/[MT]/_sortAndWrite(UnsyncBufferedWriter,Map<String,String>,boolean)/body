{
  boolean firstEntry=true;
  for (  Map.Entry<String,String> entry : messagesMap.entrySet()) {
    if (!firstLine || !firstEntry) {
      unsyncBufferedWriter.newLine();
    }
    firstEntry=false;
    unsyncBufferedWriter.write(entry.getKey() + "=" + entry.getValue());
  }
  messagesMap.clear();
}
