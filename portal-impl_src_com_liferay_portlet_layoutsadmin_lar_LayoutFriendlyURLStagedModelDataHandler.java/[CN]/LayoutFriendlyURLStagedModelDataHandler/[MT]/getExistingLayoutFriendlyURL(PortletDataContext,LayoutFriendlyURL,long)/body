{
  LayoutFriendlyURL existingLayoutFriendlyURL=LayoutFriendlyURLLocalServiceUtil.fetchLayoutFriendlyURLByUuidAndGroupId(layoutFriendlyURL.getUuid(),portletDataContext.getScopeGroupId());
  if (existingLayoutFriendlyURL == null) {
    existingLayoutFriendlyURL=LayoutFriendlyURLLocalServiceUtil.fetchLayoutFriendlyURL(plid,layoutFriendlyURL.getLanguageId(),false);
  }
  return existingLayoutFriendlyURL;
}
