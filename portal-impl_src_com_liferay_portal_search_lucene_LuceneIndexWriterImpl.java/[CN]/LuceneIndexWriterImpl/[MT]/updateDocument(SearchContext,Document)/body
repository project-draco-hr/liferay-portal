{
  try {
    LuceneHelperUtil.updateDocument(searchContext.getCompanyId(),new Term(Field.UID,document.getUID()),_getLuceneDocument(document));
    if (_log.isDebugEnabled()) {
      _log.debug("Updated document " + document.get(Field.UID));
    }
  }
 catch (  IOException ioe) {
    throw new SearchException(ioe);
  }
}
