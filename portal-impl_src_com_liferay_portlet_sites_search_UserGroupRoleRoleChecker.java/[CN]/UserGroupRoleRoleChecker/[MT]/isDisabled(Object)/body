{
  Role role=(Role)obj;
  try {
    if ((isChecked(role) && SiteMembershipPolicyUtil.isRoleRequired(_user.getUserId(),_group.getGroupId(),role.getRoleId())) || (!isChecked(role) && !SiteMembershipPolicyUtil.isRoleAllowed(_user.getUserId(),_group.getGroupId(),role.getRoleId()))) {
      return true;
    }
  }
  finally {
    return super.isDisabled(obj);
  }
}
