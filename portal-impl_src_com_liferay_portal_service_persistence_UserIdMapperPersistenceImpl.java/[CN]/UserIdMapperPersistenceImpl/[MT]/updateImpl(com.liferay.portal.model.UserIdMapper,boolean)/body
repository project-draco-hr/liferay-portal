{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,userIdMapper,merge);
    userIdMapper.setNew(false);
    return userIdMapper;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(UserIdMapper.class.getName());
  }
}
