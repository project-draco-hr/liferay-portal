{
  UserIdMapper userIdMapper=(UserIdMapper)EntityCacheUtil.getResult(UserIdMapperModelImpl.ENTITY_CACHE_ENABLED,UserIdMapperImpl.class,userIdMapperId);
  if (userIdMapper == _nullUserIdMapper) {
    return null;
  }
  if (userIdMapper == null) {
    Session session=null;
    try {
      session=openSession();
      userIdMapper=(UserIdMapper)session.get(UserIdMapperImpl.class,Long.valueOf(userIdMapperId));
      if (userIdMapper != null) {
        cacheResult(userIdMapper);
      }
 else {
        EntityCacheUtil.putResult(UserIdMapperModelImpl.ENTITY_CACHE_ENABLED,UserIdMapperImpl.class,userIdMapperId,_nullUserIdMapper);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(UserIdMapperModelImpl.ENTITY_CACHE_ENABLED,UserIdMapperImpl.class,userIdMapperId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return userIdMapper;
}
