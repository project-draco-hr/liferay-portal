{
  UserIdMapper result=(UserIdMapper)EntityCacheUtil.getResult(UserIdMapperModelImpl.ENTITY_CACHE_ENABLED,UserIdMapper.class,userIdMapperId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      UserIdMapper userIdMapper=(UserIdMapper)session.get(UserIdMapperImpl.class,new Long(userIdMapperId));
      if (userIdMapper != null) {
        cacheResult(userIdMapper);
      }
      return userIdMapper;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (UserIdMapper)result;
  }
}
