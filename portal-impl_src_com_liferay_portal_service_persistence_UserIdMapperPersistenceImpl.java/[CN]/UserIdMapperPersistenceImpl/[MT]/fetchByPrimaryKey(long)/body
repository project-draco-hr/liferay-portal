{
  UserIdMapper userIdMapper=(UserIdMapper)EntityCacheUtil.getResult(UserIdMapperModelImpl.ENTITY_CACHE_ENABLED,UserIdMapperImpl.class,userIdMapperId,this);
  if (userIdMapper == null) {
    Session session=null;
    try {
      session=openSession();
      userIdMapper=(UserIdMapper)session.get(UserIdMapperImpl.class,Long.valueOf(userIdMapperId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (userIdMapper != null) {
        cacheResult(userIdMapper);
      }
      closeSession(session);
    }
  }
  return userIdMapper;
}
