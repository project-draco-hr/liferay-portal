{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(userIdMapper);
    }
  }
  userIdMapper=removeImpl(userIdMapper);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(userIdMapper);
    }
  }
  return userIdMapper;
}
