{
  if (obj == null) {
    return false;
  }
  PortletItem portletItem=null;
  try {
    portletItem=(PortletItem)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=portletItem.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
