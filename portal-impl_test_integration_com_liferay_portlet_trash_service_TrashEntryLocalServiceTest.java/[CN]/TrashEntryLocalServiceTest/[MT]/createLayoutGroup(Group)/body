{
  Layout layout=LayoutTestUtil.addLayout(group);
  String name=String.valueOf(layout.getPlid());
  User user=UserTestUtil.getAdminUser(group.getCompanyId());
  return GroupLocalServiceUtil.addGroup(user.getUserId(),GroupConstants.DEFAULT_PARENT_GROUP_ID,Layout.class.getName(),layout.getPlid(),GroupConstants.DEFAULT_LIVE_GROUP_ID,name,null,0,true,GroupConstants.DEFAULT_MEMBERSHIP_RESTRICTION,null,false,true,null);
}
