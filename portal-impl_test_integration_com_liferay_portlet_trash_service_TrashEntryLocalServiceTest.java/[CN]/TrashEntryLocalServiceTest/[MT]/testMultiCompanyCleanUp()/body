{
  int actual=0;
  for (int i=0; i < 4; i++) {
    Group group=setTrashEntriesMaxAge(createGroup(createCompany()),2);
    actual+=createTrashEntriesForTwoDaysExpiryTest(group);
  }
  TrashEntryLocalServiceUtil.checkEntries();
  Assert.assertEquals(actual,TrashEntryLocalServiceUtil.getTrashEntriesCount());
}
