{
  for (int i=0; i < _companyCount; i++) {
    Group group=setTrashEntriesMaxAge(createGroup(createCompany()),_maxAgeDay);
    createTrashEntries(group);
  }
  TrashEntryLocalServiceUtil.checkEntries();
  Assert.assertEquals(_companyCount * _notExpiredTrashEntryCount,TrashEntryLocalServiceUtil.getTrashEntriesCount());
}
