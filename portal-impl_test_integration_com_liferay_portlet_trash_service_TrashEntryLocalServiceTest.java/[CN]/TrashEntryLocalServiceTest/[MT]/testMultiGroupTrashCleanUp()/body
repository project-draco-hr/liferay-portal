{
  for (int i=0; i < _groupCount; i++) {
    Group group=setTrashEntriesMaxAge(createGroup(TestPropsValues.getCompanyId()),_maxAgeDay);
    createTrashEntries(group);
  }
  TrashEntryLocalServiceUtil.checkEntries();
  Assert.assertEquals(_groupCount * _notExpiredTrashEntryCount,TrashEntryLocalServiceUtil.getTrashEntriesCount());
}
