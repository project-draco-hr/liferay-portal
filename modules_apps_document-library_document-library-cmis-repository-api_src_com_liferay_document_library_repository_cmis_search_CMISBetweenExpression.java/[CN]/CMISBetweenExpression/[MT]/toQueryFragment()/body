{
  StringBundler sb=new StringBundler(7);
  sb.append(_field);
  if (_includesLower) {
    sb.append(" >= ");
  }
 else {
    sb.append(" > ");
  }
  sb.append(_lowerTerm);
  sb.append(" AND ");
  sb.append(_field);
  if (_includesUpper) {
    sb.append(" <= ");
  }
 else {
    sb.append(" < ");
  }
  sb.append(_upperTerm);
  return sb.toString();
}
