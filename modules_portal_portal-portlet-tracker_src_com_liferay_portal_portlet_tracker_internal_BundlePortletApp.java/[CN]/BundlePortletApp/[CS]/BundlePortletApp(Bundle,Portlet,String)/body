{
  _portalPortletModel=portalPortletModel;
  _servletContextName=bundle.getSymbolicName().replaceAll("[^a-zA-Z0-9]","");
  if ((httpServiceEndpoint.length() > 0) && httpServiceEndpoint.endsWith("/")) {
    httpServiceEndpoint=httpServiceEndpoint.substring(0,httpServiceEndpoint.length() - 1);
  }
  _servletContextPath=httpServiceEndpoint.concat("/").concat(_servletContextName);
  _pluginPackage=new BundlePluginPackage(bundle,this);
  _portletApp=portalPortletModel.getPortletApp();
}
