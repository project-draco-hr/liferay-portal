{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(UserGroup.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("userGroupId",RandomTestUtil.nextLong()));
  List<UserGroup> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
