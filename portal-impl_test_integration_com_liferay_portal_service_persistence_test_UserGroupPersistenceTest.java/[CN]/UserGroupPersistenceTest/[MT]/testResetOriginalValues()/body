{
  UserGroup newUserGroup=addUserGroup();
  _persistence.clearCache();
  UserGroup existingUserGroup=_persistence.findByPrimaryKey(newUserGroup.getPrimaryKey());
  Assert.assertEquals(Long.valueOf(existingUserGroup.getCompanyId()),ReflectionTestUtil.<Long>invoke(existingUserGroup,"getOriginalCompanyId",new Class<?>[0]));
  Assert.assertTrue(Validator.equals(existingUserGroup.getName(),ReflectionTestUtil.invoke(existingUserGroup,"getOriginalName",new Class<?>[0])));
}
