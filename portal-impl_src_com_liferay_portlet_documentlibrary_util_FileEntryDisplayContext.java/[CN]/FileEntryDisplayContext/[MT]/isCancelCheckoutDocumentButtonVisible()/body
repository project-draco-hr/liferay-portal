{
  if ((_fileEntryDisplayContextHelper.hasUpdatePermission() && _fileEntryDisplayContextHelper.isCheckedOut() && _fileEntryDisplayContextHelper.isFileEntryLockedByMe()) || (_fileEntryDisplayContextHelper.isCheckedOut() && _fileEntryDisplayContextHelper.hasOverrideCheckoutPermission())) {
    return true;
  }
  return false;
}
