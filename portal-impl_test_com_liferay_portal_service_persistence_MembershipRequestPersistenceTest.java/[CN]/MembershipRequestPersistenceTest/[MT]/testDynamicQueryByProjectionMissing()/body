{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MembershipRequest.class,MembershipRequest.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("membershipRequestId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("membershipRequestId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
