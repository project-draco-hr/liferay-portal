{
  if (name.startsWith("net.sf.ehcache.") || name.startsWith("org.aopalliance.") || name.startsWith("org.hibernate.")|| name.startsWith("org.springframework.")) {
    ClassLoader portalClassLoader=PortalClassLoaderUtil.getClassLoader();
    return portalClassLoader.loadClass(name);
  }
 else {
    return super.loadClass(name);
  }
}
