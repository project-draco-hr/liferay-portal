{
  DDMStructureLink ddmStructureLink=(DDMStructureLink)EntityCacheUtil.getResult(DDMStructureLinkModelImpl.ENTITY_CACHE_ENABLED,DDMStructureLinkImpl.class,structureLinkId,this);
  if (ddmStructureLink == null) {
    Session session=null;
    try {
      session=openSession();
      ddmStructureLink=(DDMStructureLink)session.get(DDMStructureLinkImpl.class,Long.valueOf(structureLinkId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (ddmStructureLink != null) {
        cacheResult(ddmStructureLink);
      }
      closeSession(session);
    }
  }
  return ddmStructureLink;
}
