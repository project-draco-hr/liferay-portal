{
  DDMStructureLink ddmStructureLink=(DDMStructureLink)EntityCacheUtil.getResult(DDMStructureLinkModelImpl.ENTITY_CACHE_ENABLED,DDMStructureLinkImpl.class,structureLinkId,this);
  if (ddmStructureLink == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (ddmStructureLink == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      ddmStructureLink=(DDMStructureLink)session.get(DDMStructureLinkImpl.class,Long.valueOf(structureLinkId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (ddmStructureLink == null)) {
        EntityCacheUtil.putResult(DDMStructureLinkModelImpl.ENTITY_CACHE_ENABLED,DDMStructureLinkImpl.class,structureLinkId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(ddmStructureLink);
      }
      closeSession(session);
    }
  }
  return ddmStructureLink;
}
