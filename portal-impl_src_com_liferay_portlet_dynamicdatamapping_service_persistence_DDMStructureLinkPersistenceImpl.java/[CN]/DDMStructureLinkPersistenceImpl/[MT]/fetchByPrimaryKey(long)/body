{
  DDMStructureLink ddmStructureLink=(DDMStructureLink)EntityCacheUtil.getResult(DDMStructureLinkModelImpl.ENTITY_CACHE_ENABLED,DDMStructureLinkImpl.class,structureLinkId);
  if (ddmStructureLink == _nullDDMStructureLink) {
    return null;
  }
  if (ddmStructureLink == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      ddmStructureLink=(DDMStructureLink)session.get(DDMStructureLinkImpl.class,Long.valueOf(structureLinkId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (ddmStructureLink != null) {
        cacheResult(ddmStructureLink);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(DDMStructureLinkModelImpl.ENTITY_CACHE_ENABLED,DDMStructureLinkImpl.class,structureLinkId,_nullDDMStructureLink);
      }
      closeSession(session);
    }
  }
  return ddmStructureLink;
}
