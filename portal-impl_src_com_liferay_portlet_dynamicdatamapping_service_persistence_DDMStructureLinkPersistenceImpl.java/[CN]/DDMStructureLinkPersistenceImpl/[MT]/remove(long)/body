{
  Session session=null;
  try {
    session=openSession();
    DDMStructureLink ddmStructureLink=(DDMStructureLink)session.get(DDMStructureLinkImpl.class,Long.valueOf(structureLinkId));
    if (ddmStructureLink == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + structureLinkId);
      }
      throw new NoSuchStructureLinkException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + structureLinkId);
    }
    return ddmStructureLinkPersistence.remove(ddmStructureLink);
  }
 catch (  NoSuchStructureLinkException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
