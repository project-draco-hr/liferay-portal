{
  Object[] args=new Object[]{userThreadModelImpl.getUserId(),userThreadModelImpl.getMbThreadId()};
  finderCache.removeResult(FINDER_PATH_COUNT_BY_U_M,args);
  finderCache.removeResult(FINDER_PATH_FETCH_BY_U_M,args);
  if ((userThreadModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_U_M.getColumnBitmask()) != 0) {
    args=new Object[]{userThreadModelImpl.getOriginalUserId(),userThreadModelImpl.getOriginalMbThreadId()};
    finderCache.removeResult(FINDER_PATH_COUNT_BY_U_M,args);
    finderCache.removeResult(FINDER_PATH_FETCH_BY_U_M,args);
  }
}
