{
  Collection<Method> methods=DDMFormFactory.getDDMFormFieldMethods(clazz);
  for (  Method method : methods) {
    DDMFormFactoryHelper ddmFormFactoryHelper=new DDMFormFactoryHelper(clazz,method);
    _ddmFormFieldMethodNameMap.put(ddmFormFactoryHelper.getDDMFormFieldName(),method.getName());
  }
}
