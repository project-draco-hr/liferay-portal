{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Test Transaction Test Page",RuntimeVariables.replace("Test Transaction Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//p[1]","BarLocalServiceUtil"));
  assertTrue(selenium.isPartialText("//p[1]","addBar_Success=PASSED"));
  assertTrue(selenium.isPartialText("//p[1]","addBarAndClassName_PortalRollback=PASSED"));
  assertTrue(selenium.isPartialText("//p[1]","addBarAndClassName_PortletRollback=PASSED"));
  assertTrue(selenium.isPartialText("//p[2]","PortalServiceUtil"));
  assertTrue(selenium.isPartialText("//p[2]","testAddClassNameAndTestTransactionPortletBar_Success=PASSED"));
  assertTrue(selenium.isPartialText("//p[2]","testAddClassNameAndTestTransactionPortletBar_PortalRollback=PASSED"));
  assertTrue(selenium.isPartialText("//p[2]","testAddClassNameAndTestTransactionPortletBar_PortletRollback=PASSED"));
}
