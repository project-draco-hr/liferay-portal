{
  if (PropsValues.PERMISSIONS_USER_CHECK_ALGORITHM == 5) {
    updateAccessInControlPanelPermission_5(PortletKeys.BLOGS,PortletKeys.BLOGS_ADMIN);
    updateAccessInControlPanelPermission_5(PortletKeys.MESSAGE_BOARDS,PortletKeys.MESSAGE_BOARDS_ADMIN);
  }
 else   if (PropsValues.PERMISSIONS_USER_CHECK_ALGORITHM == 6) {
    updateAccessInControlPanelPermission_6(PortletKeys.BLOGS,PortletKeys.BLOGS_ADMIN);
    updateAccessInControlPanelPermission_6(PortletKeys.MESSAGE_BOARDS,PortletKeys.MESSAGE_BOARDS_ADMIN);
  }
}
