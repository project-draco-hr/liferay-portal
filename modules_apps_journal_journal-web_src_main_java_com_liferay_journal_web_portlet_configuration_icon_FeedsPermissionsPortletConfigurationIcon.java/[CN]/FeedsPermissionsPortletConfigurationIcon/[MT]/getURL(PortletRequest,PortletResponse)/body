    url=PermissionsURLTag.doTag(StringPool.BLANK,"com.liferay.journal",HtmlUtil.escape(themeDisplay.getScopeGroupName()),null,String.valueOf(themeDisplay.getScopeGroupId()),LiferayWindowState.POP_UP.toString(),null,themeDisplay.getRequest());
  }
 catch (  Exception e) {
  }
  return url;
}
