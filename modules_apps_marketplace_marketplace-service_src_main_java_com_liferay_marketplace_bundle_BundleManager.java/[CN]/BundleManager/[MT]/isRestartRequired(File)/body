{
  try (ZipFile zipFile=new ZipFile(file)){
    ZipEntry zipEntry=zipFile.getEntry("liferay-marketplace.properties");
    if (zipEntry == null) {
      return false;
    }
    Properties properties=new Properties();
    properties.load(zipFile.getInputStream(zipEntry));
    return GetterUtil.getBoolean(properties.getProperty("restart-required"),true);
  }
 catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      _log.debug("Unable to read liferay-marketplace.properties from " + file.getName());
    }
  }
  return false;
}
