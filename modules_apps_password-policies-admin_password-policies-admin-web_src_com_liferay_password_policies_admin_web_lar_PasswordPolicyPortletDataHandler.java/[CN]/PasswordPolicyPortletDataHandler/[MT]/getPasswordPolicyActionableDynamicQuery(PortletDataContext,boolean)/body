{
  ActionableDynamicQuery actionableDynamicQuery=_passwordPolicyLocalService.getExportActionableDynamicQuery(portletDataContext);
  actionableDynamicQuery.setPerformActionMethod(new ActionableDynamicQuery.PerformActionMethod(){
    @Override public void performAction(    Object object) throws PortalException {
      if (!export) {
        return;
      }
      PasswordPolicy passwordPolicy=(PasswordPolicy)object;
      StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,passwordPolicy);
    }
  }
);
  return actionableDynamicQuery;
}
