{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(journalContentSearchPK));
  }
  com.liferay.portlet.journal.model.JournalContentSearch journalContentSearch=getPersistence().remove(journalContentSearchPK);
  if (listener != null) {
    listener.onAfterRemove(journalContentSearch);
  }
  return journalContentSearch;
}
