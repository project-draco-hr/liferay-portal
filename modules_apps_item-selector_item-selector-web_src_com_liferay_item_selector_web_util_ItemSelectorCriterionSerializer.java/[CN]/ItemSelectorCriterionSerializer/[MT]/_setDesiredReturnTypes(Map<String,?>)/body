{
  Set<ItemSelectorReturnType> desiredItemSelectorReturnTypes=new LinkedHashSet<>();
  List<String> desiredItemSelectorReturnTypeNames=(List<String>)map.get("desiredReturnTypes");
  for (  String desiredItemSelectoReturnTypeName : desiredItemSelectorReturnTypeNames) {
    Set<ItemSelectorReturnType> availableItemSelectorReturnTypes=_itemSelectorCriterion.getItemSelectorAvailableReturnTypes();
    for (    ItemSelectorReturnType availableItemSelectorReturnType : availableItemSelectorReturnTypes) {
      String availableItemSelectorReturnTypeName=availableItemSelectorReturnType.getName();
      if (availableItemSelectorReturnTypeName.equals(desiredItemSelectoReturnTypeName)) {
        desiredItemSelectorReturnTypes.add(availableItemSelectorReturnType);
        break;
      }
    }
  }
  if (desiredItemSelectorReturnTypes.isEmpty()) {
    if (_log.isWarnEnabled()) {
      _log.warn("No valid item selector return types found");
    }
  }
  _itemSelectorCriterion.setItemSelectorDesiredReturnTypes(desiredItemSelectorReturnTypes);
}
