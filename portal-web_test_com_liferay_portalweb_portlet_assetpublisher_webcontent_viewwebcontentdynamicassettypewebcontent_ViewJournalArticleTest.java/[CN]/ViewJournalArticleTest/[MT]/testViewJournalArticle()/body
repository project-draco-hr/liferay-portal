{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=AP Web Content Name"));
  assertEquals(RuntimeVariables.replace("AP Web Content Body"),selenium.getText("//p"));
  assertTrue(selenium.isElementPresent("link=Read More \u00bb"));
}
