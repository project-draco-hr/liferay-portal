{
  String uuid=referenceElement.attributeValue("uuid");
  long groupId=GetterUtil.getLong(referenceElement.attributeValue("live-group-id"));
  if (!validateMissingGroupReference(portletDataContext,referenceElement)) {
    return false;
  }
  Map<Long,Long> groupIds=(Map<Long,Long>)portletDataContext.getNewPrimaryKeysMap(Group.class);
  groupId=MapUtil.getLong(groupIds,groupId,groupId);
  try {
    return validateMissingReference(uuid,portletDataContext.getCompanyId(),groupId);
  }
 catch (  Exception e) {
    return false;
  }
}
