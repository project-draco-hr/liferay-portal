{
  String elementName=referenceElement.getName();
  if (elementName.equals("missing-reference")) {
    String uuid=referenceElement.attributeValue("uuid");
    try {
      return validateMissingReference(uuid,portletDataContext.getCompanyId(),portletDataContext.getScopeGroupId());
    }
 catch (    Exception e) {
      return false;
    }
  }
  return true;
}
