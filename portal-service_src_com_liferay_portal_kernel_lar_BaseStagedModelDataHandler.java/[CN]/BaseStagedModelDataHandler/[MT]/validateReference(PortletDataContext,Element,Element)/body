{
  String elementName=referenceElement.getName();
  if (elementName.equals("missing-reference")) {
    String uuid=referenceElement.attributeValue("uuid");
    try {
      boolean referenceExist=validateMissingReference(uuid,portletDataContext.getCompanyId(),portletDataContext.getScopeGroupId());
      if (!referenceExist) {
        referenceExist=validateMissingReference(uuid,portletDataContext.getCompanyId(),portletDataContext.getCompanyGroupId());
      }
      return referenceExist;
    }
 catch (    Exception e) {
      return false;
    }
  }
  return true;
}
