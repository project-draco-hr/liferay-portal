{
  if (ExportImportThreadLocal.isLayoutExportInProcess()) {
    return ExportImportLifecycleConstants.PROCESS_FLAG_LAYOUT_EXPORT_IN_PROCESS;
  }
 else   if (ExportImportThreadLocal.isLayoutImportInProcess()) {
    return ExportImportLifecycleConstants.PROCESS_FLAG_LAYOUT_IMPORT_IN_PROCESS;
  }
 else   if (ExportImportThreadLocal.isPortletExportInProcess()) {
    return ExportImportLifecycleConstants.PROCESS_FLAG_PORTLET_EXPORT_IN_PROCESS;
  }
 else   if (ExportImportThreadLocal.isPortletImportInProcess()) {
    return ExportImportLifecycleConstants.PROCESS_FLAG_PORTLET_IMPORT_IN_PROCESS;
  }
  return 0;
}
