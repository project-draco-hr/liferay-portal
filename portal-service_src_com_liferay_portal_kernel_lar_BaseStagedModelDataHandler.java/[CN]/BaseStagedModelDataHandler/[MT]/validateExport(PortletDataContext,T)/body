{
  if (stagedModel instanceof WorkflowedModel) {
    WorkflowedModel workflowedModel=(WorkflowedModel)stagedModel;
    if (!ArrayUtil.contains(getExportableStatuses(),workflowedModel.getStatus())) {
      throw new PortletDataException(PortletDataException.STATUS_UNAVAILABLE);
    }
  }
  TrashHandler trashHandler=TrashHandlerRegistryUtil.getTrashHandler(stagedModel.getStagedModelType().getClassName());
  if (trashHandler != null) {
    try {
      long classPK=(Long)stagedModel.getPrimaryKeyObj();
      if (trashHandler.isInTrash(classPK) || trashHandler.isInTrashContainer(classPK)) {
        throw new PortletDataException(PortletDataException.STATUS_IN_TRASH);
      }
    }
 catch (    Exception e) {
      if (e instanceof PortletDataException) {
        throw (PortletDataException)e;
      }
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to check trash status for " + stagedModel.getModelClassName());
      }
    }
  }
}
