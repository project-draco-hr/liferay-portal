{
  Assert.assertTrue(isValidFilter("(object=value)"));
  Assert.assertFalse(isValidFilter("(=value)"));
  Assert.assertFalse(isValidFilter("(<=value)"));
  Assert.assertFalse(isValidFilter("(>=value)"));
  Assert.assertFalse(isValidFilter("(~=value)"));
  Assert.assertFalse(isValidFilter("(~=value)(object=value)"));
}
