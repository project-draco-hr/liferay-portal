{
  if (_adaptor != null) {
    return _adaptor;
  }
  try {
    List<AdaptorFactory> services=ServiceLoader.load(AdaptorFactory.class);
    for (    AdaptorFactory adaptorFactory : services) {
      _adaptor=adaptorFactory.newAdaptor();
    }
  }
 catch (  Exception e) {
    throw new AdaptorException(e);
  }
  return _adaptor;
}
