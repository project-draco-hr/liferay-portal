{
  AtomicLongArray hitCounterArray=switchData._hitCounterArray;
  if (!_className.equals(switchData._className) || (_lineNumber != switchData._lineNumber) || (_switchNumber != switchData._switchNumber)|| (_hitCounterArray.length() != hitCounterArray.length())) {
    throw new IllegalArgumentException("Switch data mismatch, left : " + toString() + ", right : "+ switchData);
  }
  for (int i=0; i < _hitCounterArray.length(); i++) {
    _hitCounterArray.addAndGet(i,hitCounterArray.get(i));
  }
}
