{
  StringBuilder sb=new StringBuilder();
  sb.append("{className=");
  sb.append(_className);
  sb.append(", lineNumber=");
  sb.append(_lineNumber);
  sb.append(", switchNumber=");
  sb.append(_switchNumber);
  sb.append(", caseNumber=");
  sb.append(_hitCounterArray.length());
  sb.append("}");
  return sb.toString();
}
