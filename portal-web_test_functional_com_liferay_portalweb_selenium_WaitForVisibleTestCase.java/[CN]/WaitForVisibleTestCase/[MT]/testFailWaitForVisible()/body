{
  String expectedException="Element is not present at \"//Does/Not/Exists\"";
  if (SELENIUM_LOGGER_ENABLED) {
    expectedException="Command failure \"waitForVisible\" with parameter " + "\"//Does/Not/Exists\" : " + expectedException;
  }
  try {
    selenium.waitForVisible("//Does/Not/Exists");
  }
 catch (  Throwable t) {
    assertEquals(t.getMessage(),expectedException);
  }
}
