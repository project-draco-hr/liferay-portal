{
  String expectedException="Element is not present at \"//Does/Not/Exists\"";
  if (TestPropsValues.SELENIUM_LOGGER_ENABLED) {
    expectedException="Command failure \"waitForVisible\" with parameter " + "\"//Does/Not/Exists\" : Element is not present at " + "\"//Does/Not/Exists\"";
  }
  try {
    selenium.waitForVisible("//Does/Not/Exists");
  }
 catch (  Throwable e) {
    assertEquals(e.getMessage(),expectedException);
  }
}
