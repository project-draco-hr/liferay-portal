{
  sql=PortalUtil.transformCustomSQL(sql);
  StringBuilder sb=new StringBuilder();
  try {
    UnsyncBufferedReader br=new UnsyncBufferedReader(new UnsyncStringReader(sql));
    String line=null;
    while ((line=br.readLine()) != null) {
      sb.append(line.trim());
      sb.append(StringPool.SPACE);
    }
    br.close();
  }
 catch (  IOException ioe) {
    return sql;
  }
  return sb.toString();
}
