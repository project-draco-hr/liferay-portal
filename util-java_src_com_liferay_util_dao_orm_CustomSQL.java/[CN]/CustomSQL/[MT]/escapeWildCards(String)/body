{
  if (!isVendorMySQL() && !isVendorOracle()) {
    return keywords;
  }
  StringBuilder sb=new StringBuilder(keywords);
  for (int i=0; i < sb.length(); ++i) {
    if (sb.charAt(i) == '\\') {
      i++;
      continue;
    }
    if (sb.charAt(i) == '_') {
      sb.insert(i,'\\');
      i++;
      continue;
    }
    if (sb.charAt(i) == '%') {
      sb.insert(i,'\\');
      i++;
      continue;
    }
  }
  return sb.toString();
}
