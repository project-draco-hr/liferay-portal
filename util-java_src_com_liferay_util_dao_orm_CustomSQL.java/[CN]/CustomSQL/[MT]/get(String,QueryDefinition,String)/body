{
  String sql=get(id);
  if (!Validator.isBlank(table) && !table.endsWith(StringPool.PERIOD)) {
    table=table.concat(StringPool.PERIOD);
  }
  if (queryDefinition.getStatus() == WorkflowConstants.STATUS_ANY) {
    sql=sql.replace(_STATUS_KEYWORD,_STATUS_CONDITION_EMPTY);
  }
 else {
    if (queryDefinition.isExcludeStatus()) {
      sql=sql.replace(_STATUS_KEYWORD,table.concat(_STATUS_CONDITION_INVERSE));
    }
 else {
      sql=sql.replace(_STATUS_KEYWORD,table.concat(_STATUS_CONDITION_DEFAULT));
    }
  }
  return sql;
}
