{
  if (!parentFolderId.equals(BookmarksFolderImpl.DEFAULT_PARENT_FOLDER_ID)) {
    BookmarksFolder parentFolder=BookmarksFolderUtil.fetchByPrimaryKey(parentFolderId);
    if ((parentFolder == null) || (!groupId.equals(parentFolder.getGroupId()))) {
      parentFolderId=BookmarksFolderImpl.DEFAULT_PARENT_FOLDER_ID;
    }
  }
  return parentFolderId;
}
