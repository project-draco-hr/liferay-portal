{
  try {
    com.liferay.portlet.asset.model.AssetCategory returnValue=AssetCategoryServiceUtil.updateCategory(categoryId,parentCategoryId,name,vocabularyId,categoryProperties,serviceContext);
    return com.liferay.portlet.asset.model.AssetCategorySoap.toSoapModel(returnValue);
