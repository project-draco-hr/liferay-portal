{
  RatingsStats ratingsStats=(RatingsStats)EntityCacheUtil.getResult(RatingsStatsModelImpl.ENTITY_CACHE_ENABLED,RatingsStatsImpl.class,statsId,this);
  if (ratingsStats == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (ratingsStats == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      ratingsStats=(RatingsStats)session.get(RatingsStatsImpl.class,Long.valueOf(statsId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (ratingsStats == null)) {
        EntityCacheUtil.putResult(RatingsStatsModelImpl.ENTITY_CACHE_ENABLED,RatingsStatsImpl.class,statsId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(ratingsStats);
      }
      closeSession(session);
    }
  }
  return ratingsStats;
}
