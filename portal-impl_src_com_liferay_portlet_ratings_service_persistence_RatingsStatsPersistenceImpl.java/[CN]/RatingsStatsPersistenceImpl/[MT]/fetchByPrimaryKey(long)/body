{
  RatingsStats result=(RatingsStats)EntityCacheUtil.getResult(RatingsStatsModelImpl.ENTITY_CACHE_ENABLED,RatingsStatsImpl.class,statsId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      RatingsStats ratingsStats=(RatingsStats)session.get(RatingsStatsImpl.class,new Long(statsId));
      if (ratingsStats != null) {
        cacheResult(ratingsStats);
      }
      return ratingsStats;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (RatingsStats)result;
  }
}
