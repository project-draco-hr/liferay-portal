{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,ratingsStats,merge);
    ratingsStats.setNew(false);
    return ratingsStats;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(RatingsStats.class.getName());
  }
}
