{
  StringBundler sb=new StringBundler(7);
  sb.append("delete from ");
  sb.append(tableName);
  sb.append(" where classPK not in (select ");
  sb.append(orphanedColumnName);
  sb.append(" from ");
  sb.append(orphanedTableName);
  sb.append(StringPool.CLOSE_PARENTHESIS);
  runSQL(sb.toString());
}
