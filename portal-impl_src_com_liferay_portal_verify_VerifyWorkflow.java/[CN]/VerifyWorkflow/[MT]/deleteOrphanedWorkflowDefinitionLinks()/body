{
  String sql="select distinct classNameId from WorkflowDefinitionLink";
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement(sql);
    rs=ps.executeQuery();
    while (rs.next()) {
      long classNameId=rs.getLong("classNameId");
      ClassName className=ClassNameLocalServiceUtil.fetchClassName(classNameId);
      if (className == null) {
        continue;
      }
      String classNameValue=className.getValue();
      for (      String[] orphanedAttachedModel : getOrphanedAttachedModels()) {
        String orphanedClassName=orphanedAttachedModel[0];
        if (classNameValue.equals(orphanedClassName)) {
          String orphanedTableName=orphanedAttachedModel[1];
          String orphanedColumnName=orphanedAttachedModel[2];
          deleteOrphanedWorkflowDefinitionLinks(con,orphanedTableName,orphanedColumnName);
        }
      }
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
