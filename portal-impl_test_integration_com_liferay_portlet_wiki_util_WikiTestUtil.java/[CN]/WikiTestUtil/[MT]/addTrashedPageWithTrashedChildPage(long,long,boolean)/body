{
  WikiPage page=WikiTestUtil.addPage(TestPropsValues.getUserId(),groupId,nodeId,"TestPage",true);
  WikiPage childPage=WikiTestUtil.addPage(TestPropsValues.getUserId(),nodeId,"TestChildPage",ServiceTestUtil.randomString(),"TestPage",true,ServiceTestUtil.getServiceContext(groupId));
  if (explicitlyRemoveChildPage) {
    WikiPageLocalServiceUtil.movePageToTrash(TestPropsValues.getUserId(),childPage);
  }
  WikiPageLocalServiceUtil.movePageToTrash(TestPropsValues.getUserId(),page);
  page=WikiPageLocalServiceUtil.getPageByPageId(page.getPageId());
  childPage=WikiPageLocalServiceUtil.getPageByPageId(childPage.getPageId());
  return new WikiPage[]{page,childPage};
}
