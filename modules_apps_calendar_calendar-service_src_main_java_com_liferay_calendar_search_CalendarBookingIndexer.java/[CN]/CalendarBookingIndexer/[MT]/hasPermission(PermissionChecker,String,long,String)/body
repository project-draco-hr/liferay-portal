{
  if (actionId.equals(ActionKeys.VIEW)) {
    CalendarBooking calendarBooking=_calendarBookingLocalService.getCalendarBooking(entryClassPK);
    return CalendarPermission.contains(permissionChecker,calendarBooking.getCalendar(),CalendarActionKeys.VIEW_BOOKING_DETAILS);
  }
 else {
    return super.hasPermission(permissionChecker,entryClassName,entryClassPK,actionId);
  }
}
