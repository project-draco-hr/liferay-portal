{
  try {
    long rawInterval=PrefsPropsUtil.getLong(PropsUtil.MESSAGE_BOARDS_EXPIRE_BAN_JOB_INTERVAL,PropsValues.MESSAGE_BOARDS_EXPIRE_BAN_JOB_INTERVAL);
    if (_log.isDebugEnabled()) {
      _log.debug("Interval " + rawInterval + " minutes");
    }
    _interval=rawInterval * Time.MINUTE;
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
