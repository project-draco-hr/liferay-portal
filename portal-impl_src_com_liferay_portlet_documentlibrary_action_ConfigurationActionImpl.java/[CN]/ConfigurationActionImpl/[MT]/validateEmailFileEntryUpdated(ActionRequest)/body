{
  boolean emailFileEntryUpdatedEnabled=GetterUtil.getBoolean(getParameter(actionRequest,"emailFileEntryUpdatedEnabled"));
  String emailFileEntryUpdatedSubject=getLocalizedParameter(actionRequest,"emailFileEntryUpdatedSubject");
  String emailFileEntryUpdatedBody=getLocalizedParameter(actionRequest,"emailFileEntryUpdatedBody");
  if (emailFileEntryUpdatedEnabled) {
    if (Validator.isNull(emailFileEntryUpdatedSubject)) {
      SessionErrors.add(actionRequest,"emailFileEntryUpdatedSubject");
    }
 else     if (Validator.isNull(emailFileEntryUpdatedBody)) {
      SessionErrors.add(actionRequest,"emailFileEntryUpdatedBody");
    }
  }
}
