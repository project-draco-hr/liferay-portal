{
  RoleTestUtil.removeResourcePermission(RoleConstants.GUEST,BookmarksFolder.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,String.valueOf(_folder.getFolderId()),ActionKeys.VIEW);
  RoleTestUtil.removeResourcePermission(RoleConstants.SITE_MEMBER,BookmarksFolder.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,String.valueOf(_folder.getFolderId()),ActionKeys.VIEW);
}
