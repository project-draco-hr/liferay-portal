{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Document Library Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Document Library Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("//b"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("//b"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//strong");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Edit")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Edit"));
  selenium.waitForPageToLoad("30000");
  selenium.typeKeys("_20_title",RuntimeVariables.replace("Edited Test Document"));
  selenium.type("_20_title",RuntimeVariables.replace("Edited Test Document"));
  selenium.type("_20_description",RuntimeVariables.replace("This is an edited test document!"));
  selenium.click(RuntimeVariables.replace("//input[@value='Save']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertTrue(selenium.isTextPresent("Edited Test Document.txt"));
  assertTrue(selenium.isTextPresent("This is an edited test document!"));
}
