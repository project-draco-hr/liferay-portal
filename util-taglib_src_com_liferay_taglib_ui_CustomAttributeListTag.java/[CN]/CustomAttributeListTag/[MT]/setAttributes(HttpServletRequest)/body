{
  request.setAttribute("liferay-ui:custom-attribute-list:className",_className);
  request.setAttribute("liferay-ui:custom-attribute-list:classPK",String.valueOf(_classPK));
  request.setAttribute("liferay-ui:custom-attribute-list:editable",String.valueOf(_editable));
  request.setAttribute("liferay-ui:custom-attribute-list:label",String.valueOf(_label));
}
