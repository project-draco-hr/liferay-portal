{
  _clusterSchedulerEngine=_getClusterSchedulerEngine(true,2,2);
  SchedulerResponse schedulerResponse=_clusterSchedulerEngine.getScheduledJob(_TEST_JOB_0,_MEMORY_CLUSTER_TEST_GROUP);
  Assert.assertNotNull(schedulerResponse);
  List<SchedulerResponse> schedulerResponses=_clusterSchedulerEngine.getScheduledJobs(_MEMORY_CLUSTER_TEST_GROUP);
  Assert.assertEquals(2,schedulerResponses.size());
  schedulerResponses=_clusterSchedulerEngine.getScheduledJobs();
  Assert.assertEquals(4,schedulerResponses.size());
  try {
    _clusterSchedulerEngine.getScheduledJob(_WRONG_JOB_NAME,_WRONG_PERSISTENT_GROUP_NAME);
    Assert.fail();
  }
 catch (  SchedulerException se) {
  }
  try {
    _clusterSchedulerEngine.getScheduledJobs(_WRONG_PERSISTENT_GROUP_NAME);
    Assert.fail();
  }
 catch (  SchedulerException se) {
  }
  _clusterSchedulerEngine.delete(_MEMORY_CLUSTER_TEST_GROUP);
  try {
    _clusterSchedulerEngine.delete(_PERSISTENT_TEST_GROUP);
    Assert.fail();
  }
 catch (  Exception e) {
  }
  try {
    _clusterSchedulerEngine.getScheduledJobs();
    Assert.fail();
  }
 catch (  SchedulerException se) {
  }
}
