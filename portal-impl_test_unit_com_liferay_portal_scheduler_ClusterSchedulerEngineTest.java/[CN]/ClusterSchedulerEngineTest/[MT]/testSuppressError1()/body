{
  _clusterSchedulerEngine=_getClusterSchedulerEngine(true,1,0);
  SchedulerResponse schedulerResponse=_clusterSchedulerEngine.getScheduledJob(_TEST_JOB_NAME_0,_MEMORY_CLUSTER_TEST_GROUP_NAME);
  _assertSuppressErrorValue(schedulerResponse,null);
  _clusterSchedulerEngine.suppressError(_TEST_JOB_NAME_0,_MEMORY_CLUSTER_TEST_GROUP_NAME);
  schedulerResponse=_clusterSchedulerEngine.getScheduledJob(_TEST_JOB_NAME_0,_MEMORY_CLUSTER_TEST_GROUP_NAME);
  _assertSuppressErrorValue(schedulerResponse,Boolean.TRUE);
  try {
    _clusterSchedulerEngine.suppressError(_INVALID_JOB_NAME,_MEMORY_CLUSTER_TEST_GROUP_NAME);
    Assert.fail();
  }
 catch (  SchedulerException se) {
  }
}
