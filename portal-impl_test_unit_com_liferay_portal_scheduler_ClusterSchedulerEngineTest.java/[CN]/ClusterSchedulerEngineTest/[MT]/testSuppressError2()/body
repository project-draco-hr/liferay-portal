{
  _clusterSchedulerEngine=_getClusterSchedulerEngine(false,1,0);
  SchedulerResponse schedulerResponse=_clusterSchedulerEngine.getScheduledJob(_TEST_JOB_0,_MEMORY_CLUSTER_TEST_GROUP);
  _assertSuppressErrorFlag(schedulerResponse,null);
  _clusterSchedulerEngine.suppressError(_TEST_JOB_0,_MEMORY_CLUSTER_TEST_GROUP);
  schedulerResponse=_clusterSchedulerEngine.getScheduledJob(_TEST_JOB_0,_MEMORY_CLUSTER_TEST_GROUP);
  _assertSuppressErrorFlag(schedulerResponse,null);
}
