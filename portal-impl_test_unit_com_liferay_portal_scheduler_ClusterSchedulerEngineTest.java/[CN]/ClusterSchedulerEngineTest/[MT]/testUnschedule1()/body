{
  _clusterSchedulerEngine=_getClusterSchedulerEngine(true,2,0);
  SchedulerResponse schedulerResponse=_clusterSchedulerEngine.getScheduledJob(_TEST_JOB_0,_MEMORY_CLUSTER_TEST_GROUP);
  _assertTriggerState(schedulerResponse,TriggerState.NORMAL);
  _clusterSchedulerEngine.unschedule(_TEST_JOB_0,_MEMORY_CLUSTER_TEST_GROUP);
  schedulerResponse=_clusterSchedulerEngine.getScheduledJob(_TEST_JOB_0,_MEMORY_CLUSTER_TEST_GROUP);
  _assertTriggerState(schedulerResponse,TriggerState.UNSCHEDULED);
  List<SchedulerResponse> schedulerResponses=_clusterSchedulerEngine.getScheduledJobs(_MEMORY_CLUSTER_TEST_GROUP);
  for (  SchedulerResponse response : schedulerResponses) {
    if (response.getJobName().equals(_TEST_JOB_0)) {
      _assertTriggerState(response,TriggerState.UNSCHEDULED);
    }
 else {
      _assertTriggerState(response,TriggerState.NORMAL);
    }
  }
  _clusterSchedulerEngine.unschedule(_MEMORY_CLUSTER_TEST_GROUP);
  schedulerResponses=_clusterSchedulerEngine.getScheduledJobs(_MEMORY_CLUSTER_TEST_GROUP);
  for (  SchedulerResponse response : schedulerResponses) {
    _assertTriggerState(response,TriggerState.UNSCHEDULED);
  }
  try {
    _clusterSchedulerEngine.unschedule(_WRONG_JOB_NAME,_WRONG_MEMORY_CLUSTER_GROUP_NAME);
    Assert.fail();
  }
 catch (  SchedulerException se) {
  }
  try {
    _clusterSchedulerEngine.unschedule(_WRONG_MEMORY_CLUSTER_GROUP_NAME);
    Assert.fail();
  }
 catch (  SchedulerException se) {
  }
}
