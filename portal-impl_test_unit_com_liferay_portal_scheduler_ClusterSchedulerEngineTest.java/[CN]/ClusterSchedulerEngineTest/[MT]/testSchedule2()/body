{
  _clusterSchedulerEngine=_getClusterSchedulerEngine(false,1,0);
  Map<String,SchedulerResponse> schedulerResponses=_getMemoryClusteredJobs(_clusterSchedulerEngine);
  Assert.assertEquals(1,schedulerResponses.size());
  Trigger trigger=TriggerFactoryUtil.buildTrigger(TriggerType.SIMPLE,_TEST_JOB_NAME_PREFIX + "new",_MEMORY_CLUSTER_TEST_GROUP_NAME,null,null,_DEFAULT_INTERVAL);
  _clusterSchedulerEngine.schedule(trigger,StringPool.BLANK,StringPool.BLANK,new Message());
  schedulerResponses=_getMemoryClusteredJobs(_clusterSchedulerEngine);
  Assert.assertEquals(2,schedulerResponses.size());
}
