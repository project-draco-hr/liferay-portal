{
  _clusterSchedulerEngine=_getClusterSchedulerEngine(false,4,0);
  SchedulerResponse schedulerResponse=_getMemoryClusteredJob(_clusterSchedulerEngine,_TEST_JOB_0,_MEMORY_CLUSTER_TEST_GROUP);
  _assertTriggerState(schedulerResponse,TriggerState.NORMAL);
  _clusterSchedulerEngine.unschedule(_TEST_JOB_0,_MEMORY_CLUSTER_TEST_GROUP);
  schedulerResponse=_getMemoryClusteredJob(_clusterSchedulerEngine,_TEST_JOB_0,_MEMORY_CLUSTER_TEST_GROUP);
  Assert.assertNull(schedulerResponse);
  List<SchedulerResponse> schedulerResponses=_getMemoryClusteredJobs(_clusterSchedulerEngine,_MEMORY_CLUSTER_TEST_GROUP);
  for (  SchedulerResponse response : schedulerResponses) {
    _assertTriggerState(response,TriggerState.NORMAL);
  }
  _clusterSchedulerEngine.unschedule(_MEMORY_CLUSTER_TEST_GROUP);
  schedulerResponses=_getMemoryClusteredJobs(_clusterSchedulerEngine,_MEMORY_CLUSTER_TEST_GROUP);
  for (  SchedulerResponse response : schedulerResponses) {
    _assertTriggerState(response,TriggerState.UNSCHEDULED);
  }
}
