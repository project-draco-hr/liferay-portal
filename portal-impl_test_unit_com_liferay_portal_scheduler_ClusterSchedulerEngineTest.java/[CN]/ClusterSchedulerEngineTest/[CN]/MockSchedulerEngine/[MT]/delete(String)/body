{
  boolean removed=false;
  Set<Map.Entry<String,SchedulerResponse>> set=_defaultJobs.entrySet();
  Iterator<Map.Entry<String,SchedulerResponse>> iterator=set.iterator();
  while (iterator.hasNext()) {
    Map.Entry<String,SchedulerResponse> entry=iterator.next();
    String key=entry.getKey();
    if (key.contains(groupName)) {
      iterator.remove();
      removed=true;
    }
  }
  if (removed) {
    return;
  }
  throw new SchedulerException("No jobs in group " + groupName);
}
