{
  Iterator<Map.Entry<String,SchedulerResponse>> itr=_defaultJobs.entrySet().iterator();
  boolean isRemoved=false;
  while (itr.hasNext()) {
    String key=itr.next().getKey();
    if (key.contains(groupName)) {
      itr.remove();
      isRemoved=true;
    }
  }
  if (isRemoved) {
    return;
  }
  throw new SchedulerException("No job in group " + groupName);
}
