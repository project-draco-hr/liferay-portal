{
  _defaultJobs=new HashMap<String,SchedulerResponse>();
  for (int i=0; i < memoryClusterJobs; i++) {
    _addJobs(_TEST_JOB_PREFIX.concat(String.valueOf(i)),_MEMORY_CLUSTER_TEST_GROUP,StorageType.MEMORY_CLUSTERED,null,null);
  }
  for (int i=0; i < persistentJobs; i++) {
    _addJobs(_TEST_JOB_PREFIX.concat(String.valueOf(i)),_PERSISTENT_TEST_GROUP,StorageType.PERSISTED,null,null);
  }
}
