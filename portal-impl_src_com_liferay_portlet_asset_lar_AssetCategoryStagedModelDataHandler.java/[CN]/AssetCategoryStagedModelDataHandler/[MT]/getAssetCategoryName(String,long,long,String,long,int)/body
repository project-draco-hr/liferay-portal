{
  AssetCategory assetCategory=AssetCategoryUtil.fetchByG_P_N_V_First(groupId,parentCategoryId,name,vocabularyId,null);
  if ((assetCategory == null) || (Validator.isNotNull(uuid) && uuid.equals(assetCategory.getUuid()))) {
    return name;
  }
  name=StringUtil.appendParentheticalSuffix(name,count);
  return getAssetCategoryName(uuid,groupId,parentCategoryId,name,vocabularyId,++count);
}
