{
  if (category.getParentCategoryId() != AssetCategoryConstants.DEFAULT_PARENT_CATEGORY_ID) {
    StagedModelDataHandlerUtil.importReferenceStagedModel(portletDataContext,category,AssetCategory.class,category.getParentCategoryId());
  }
 else {
    StagedModelDataHandlerUtil.importReferenceStagedModel(portletDataContext,category,AssetVocabulary.class,category.getVocabularyId());
  }
}
