{
  _templateId=objectInput.readUTF();
  long journalTemplateId=objectInput.readLong();
  try {
    _journalTemplate=JournalTemplateLocalServiceUtil.getJournalTemplate(journalTemplateId);
  }
 catch (  Exception e) {
    throw new IOException("Unable to retrieve JournalTemplate with id " + journalTemplateId,e);
  }
}
