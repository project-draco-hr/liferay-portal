{
  ThemeDisplay themeDisplay=(ThemeDisplay)renderRequest.getAttribute(WebKeys.THEME_DISPLAY);
  Map<String,List<SocialEquityActionMapping>> equityActionMappingsMap=new HashMap<String,List<SocialEquityActionMapping>>();
  String[] classNames=PortalUtil.getSocialEquityClassNames();
  for (  String className : classNames) {
    List<SocialEquityActionMapping> mergedEquityActionMappings=getMergedEquityActionMappings(themeDisplay.getScopeGroupId(),className);
    equityActionMappingsMap.put(className,mergedEquityActionMappings);
  }
  renderRequest.setAttribute(WebKeys.SOCIAL_EQUITY_ACTION_MAPPINGS_MAP,equityActionMappingsMap);
  return mapping.findForward("portlet.social_equity_admin.view");
}
