{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long scopeGroupId=themeDisplay.getScopeGroupId();
  boolean featureEnabled=SocialEquityGroupSettingLocalServiceUtil.isSocialEquityEnabled(scopeGroupId,Group.class.getName(),type);
  SocialEquityGroupSettingLocalServiceUtil.updateSetting(scopeGroupId,Group.class.getName(),type,!featureEnabled);
}
