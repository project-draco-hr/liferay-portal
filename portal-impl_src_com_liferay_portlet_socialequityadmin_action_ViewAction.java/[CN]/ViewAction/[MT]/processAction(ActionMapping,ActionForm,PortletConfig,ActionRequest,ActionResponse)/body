{
  String cmd=ParamUtil.getString(actionRequest,Constants.CMD);
  if (cmd.equals("updateRanks")) {
    updateRanks(actionRequest);
  }
 else   if (cmd.equals("toggleInformationEnabled")) {
    toggleFeature(actionRequest,SocialEquitySettingConstants.TYPE_INFORMATION);
  }
 else   if (cmd.equals("toggleParticipationEnabled")) {
    toggleFeature(actionRequest,SocialEquitySettingConstants.TYPE_PARTICIPATION);
  }
 else {
    updateSocialEquitySettings(actionRequest);
  }
  sendRedirect(actionRequest,actionResponse);
}
