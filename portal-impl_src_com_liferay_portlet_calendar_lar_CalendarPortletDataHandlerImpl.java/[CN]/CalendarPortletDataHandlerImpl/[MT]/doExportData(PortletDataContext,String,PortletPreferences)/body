{
  context.addPermissions("com.liferay.portlet.calendar",context.getScopeGroupId());
  Document document=SAXReaderUtil.createDocument();
  Element rootElement=document.addElement("calendar-data");
  rootElement.addAttribute("group-id",String.valueOf(context.getScopeGroupId()));
  List<CalEvent> events=CalEventUtil.findByGroupId(context.getScopeGroupId());
  for (  CalEvent event : events) {
    exportEvent(context,rootElement,event);
  }
  return document.formattedString();
}
