{
  List<String> paths=portletDataContext.getDeletionEntries(CalEvent.class.getName());
  for (  String path : paths) {
    if (portletDataContext.isPathNotProcessed(path)) {
      DeletionEntry deletionEntry=(DeletionEntry)portletDataContext.getZipEntryAsObject(path);
      CalEvent calEvent=CalEventUtil.fetchByUUID_G(deletionEntry.getClassUuid(),portletDataContext.getScopeGroupId());
      if (calEvent != null) {
        CalEventLocalServiceUtil.deleteEvent(calEvent.getEventId());
      }
    }
  }
}
