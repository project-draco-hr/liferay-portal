{
  String className=ParamUtil.getString(request,"serviceClassName");
  String methodName=ParamUtil.getString(request,"serviceMethodName");
  if (className.contains(".service.") && className.endsWith("ServiceUtil") && !className.endsWith("LocalServiceUtil")&& !_invalidClassNames.contains(className)&& !_invalidMethodNames.contains(methodName)) {
    return true;
  }
 else {
    return false;
  }
}
