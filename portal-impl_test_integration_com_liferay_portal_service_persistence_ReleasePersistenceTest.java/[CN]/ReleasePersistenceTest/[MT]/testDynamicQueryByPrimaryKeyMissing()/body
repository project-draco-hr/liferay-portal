{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Release.class,Release.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("releaseId",ServiceTestUtil.nextLong()));
  List<Release> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
