{
  UserGroup newUserGroup=addUserGroup();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(UserGroup.class,UserGroup.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("userGroupId",nextLong()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
