{
  Class<?> settingsClass=settingsDefinition.getSettingsClass();
  if (!GroupServiceSettings.class.isAssignableFrom(settingsClass)) {
    throw new IllegalArgumentException("Settings class " + settingsClass.getName() + " does not "+ "implement GroupServiceSettings");
  }
  String[] settingsIds=settingsDefinition.getSettingsIds();
  if (settingsIds.length != 1) {
    throw new IllegalArgumentException("GroupServiceSettings definitions must return a single " + "settingsId");
  }
}
