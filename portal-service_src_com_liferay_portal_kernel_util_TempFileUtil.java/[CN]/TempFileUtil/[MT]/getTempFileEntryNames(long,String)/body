{
  try {
    String tempFolderName=getTempFolderName(userId,tempPathName);
    String[] fileNames=DLStoreUtil.getFileNames(_COMPANY_ID,_REPOSITORY_ID,tempFolderName);
    for (int i=0; i < fileNames.length; i++) {
      String fileName=StringUtil.extractLast(fileNames[i],StringPool.SLASH);
      fileName=StringUtil.replace(fileName,SUFFIX_TEMP_FILENAME,StringPool.BLANK);
      fileNames[i]=fileName;
    }
    return fileNames;
  }
 catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      _log.debug("Unable to list temporary file names for " + userId + " in "+ tempPathName,e);
    }
    return new String[0];
  }
}
