{
  TemporaryFilesCapability temporaryFilesCapability=_getTemporaryFilesCapability(groupId);
  List<FileEntry> fileEntries=temporaryFilesCapability.getTemporaryFileEntries(_UUID,_getFolderPath(userId,folderName));
  List<String> fileNames=new ArrayList<>();
  for (  FileEntry fileEntry : fileEntries) {
    fileNames.add(fileEntry.getFileName());
  }
  return ArrayUtil.toStringArray(fileNames);
}
