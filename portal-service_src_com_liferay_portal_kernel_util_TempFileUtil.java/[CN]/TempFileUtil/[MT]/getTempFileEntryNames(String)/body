{
  File dir=new File(getTempAbsolutePath(tempPathName));
  File[] files=dir.listFiles(new FileFilter(_SUFFIX_TEMP_FILENAME_REGEX));
  String[] fileNames=new String[files.length];
  for (int i=0; i < files.length; i++) {
    File file=files[i];
    String fileName=StringUtil.replace(file.getName(),_SUFFIX_TEMP_FILENAME,StringPool.BLANK);
    fileNames[i]=fileName;
  }
  return fileNames;
}
