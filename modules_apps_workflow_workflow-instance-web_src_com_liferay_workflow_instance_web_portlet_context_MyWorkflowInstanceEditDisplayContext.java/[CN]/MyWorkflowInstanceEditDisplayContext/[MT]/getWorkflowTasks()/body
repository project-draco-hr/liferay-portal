{
  if (workflowTasks == null) {
    workflowTasks=WorkflowTaskManagerUtil.getWorkflowTasksByWorkflowInstance(workflowInstanceRequestHelper.getCompanyId(),workflowInstanceRequestHelper.getUserId(),getWorkflowInstanceId(),false,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  }
  return workflowTasks;
}
