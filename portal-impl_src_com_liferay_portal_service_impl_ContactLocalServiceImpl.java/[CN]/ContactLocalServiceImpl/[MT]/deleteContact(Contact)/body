{
  addressLocalService.deleteAddresses(contact.getCompanyId(),Contact.class.getName(),contact.getContactId());
  emailAddressLocalService.deleteEmailAddresses(contact.getCompanyId(),Contact.class.getName(),contact.getContactId());
  phoneLocalService.deletePhones(contact.getCompanyId(),Contact.class.getName(),contact.getContactId());
  websiteLocalService.deleteWebsites(contact.getCompanyId(),Contact.class.getName(),contact.getContactId());
  contactPersistence.remove(contact);
}
