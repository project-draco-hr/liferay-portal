{
  try {
    List<LCSRoleImpl> lcsRoles=doGetToList(LCSRoleImpl.class,_URL_LCS_ROLE,"lcsProjectId",String.valueOf(lcsProjectId),"lcsAdministrator","true","lcsEnvironmentManager","false");
