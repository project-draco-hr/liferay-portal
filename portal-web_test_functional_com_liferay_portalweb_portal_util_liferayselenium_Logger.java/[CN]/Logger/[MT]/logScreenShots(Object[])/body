{
  StringBundler sb=new StringBundler();
  _screenshotCount++;
  sb.append("&nbsp;&nbsp;&nbsp;&nbsp;");
  sb.append("&nbsp;&nbsp;&nbsp;&nbsp;");
  sb.append("&nbsp;&nbsp;&nbsp;&nbsp;");
  sb.append("&nbsp;&nbsp;&nbsp;&nbsp;");
  sb.append("<img alt=\"");
  sb.append(_screenshotCount);
  sb.append("\" height=\"450\" src=\"screenshots/");
  sb.append(_screenshotCount);
  sb.append(".jpg\" width=\"630\" />");
  sb.append("<br />");
  log("descriptionLog",sb.toString(),"descriptionLog");
}
