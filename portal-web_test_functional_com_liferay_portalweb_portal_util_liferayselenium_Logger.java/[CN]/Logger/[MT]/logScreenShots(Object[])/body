{
  StringBundler sb=new StringBundler();
  _screenshotCount++;
  sb.append("<br />");
  sb.append("<img alt=\"");
  sb.append(_screenshotCount);
  sb.append("\" height=\"750\" src=\"screenshots/");
  sb.append(_screenshotCount);
  sb.append(".jpg\" width=\"1050\" />");
  sb.append("<br />");
  log("descriptionLog",sb.toString(),"descriptionLog");
}
