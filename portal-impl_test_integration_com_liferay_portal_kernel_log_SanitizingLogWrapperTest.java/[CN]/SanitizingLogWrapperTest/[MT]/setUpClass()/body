{
  char[] _chars=new char[128];
  for (int i=0; i < _chars.length; i++) {
    _chars[i]=(char)i;
  }
  _message=new String(_chars);
  String sanitizedMessageSuffix=" [Sanitized]";
  _expectedMessageChars=new char[_chars.length + sanitizedMessageSuffix.length()];
  for (int i=0; i < _chars.length; i++) {
    if ((i == 9) || ((i >= 32) && (i != 127))) {
      _expectedMessageChars[i]=(char)i;
    }
 else {
      _expectedMessageChars[i]=CharPool.UNDERLINE;
    }
  }
  System.arraycopy(sanitizedMessageSuffix.toCharArray(),0,_expectedMessageChars,_chars.length,sanitizedMessageSuffix.length());
}
