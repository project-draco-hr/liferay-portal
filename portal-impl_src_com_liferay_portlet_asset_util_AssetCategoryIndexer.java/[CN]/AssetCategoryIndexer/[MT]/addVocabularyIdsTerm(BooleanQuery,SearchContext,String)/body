{
  if (Validator.isNull(field)) {
    return;
  }
  long[] vocabularyIds=(long[])searchContext.getAttribute("vocabularyIds");
  if (ArrayUtil.isEmpty(vocabularyIds)) {
    return;
  }
  BooleanQuery vocabularyQuery=BooleanQueryFactoryUtil.create(searchContext);
  for (  long vocabularyId : vocabularyIds) {
    vocabularyQuery.addTerm(VOCABULARY_ID,String.valueOf(vocabularyId),false);
  }
  BooleanClauseOccur booleanClauseOccur=BooleanClauseOccur.MUST;
  searchQuery.add(vocabularyQuery,booleanClauseOccur);
}
