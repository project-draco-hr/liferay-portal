{
  String value=getInitParameter(name);
  if (value != null) {
    return value;
  }
  if (name.equals("template-path")) {
    return getInitParameter("jsp-path");
  }
 else   if (name.endsWith("-template")) {
    name=name.substring(0,name.length() - 9) + "-jsp";
    return getInitParameter(name);
  }
  return null;
}
