{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Image Gallery Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Image Gallery Permissions Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//strong/a",RuntimeVariables.replace("Options"));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Configuration"),selenium.getText("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a"));
  selenium.click("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("15_ACTION_VIEW")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.uncheck("15_ACTION_VIEW");
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request processed successfully."),selenium.getText("//div[@id='p_p_id_86_']/div/div[1]"));
  assertFalse(selenium.isChecked("15_ACTION_VIEW"));
}
