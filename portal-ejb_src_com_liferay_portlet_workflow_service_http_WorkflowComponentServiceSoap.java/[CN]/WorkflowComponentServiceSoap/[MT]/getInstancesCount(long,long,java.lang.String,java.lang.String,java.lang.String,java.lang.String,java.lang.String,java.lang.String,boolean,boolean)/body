{
  try {
    int returnValue=WorkflowComponentServiceUtil.getInstancesCount(definitionId,instanceId,workflowName,workflowVersion,gtStartDate,ltStartDate,gtEndDate,ltEndDate,hideEndedTasks,andOperator);
    return returnValue;
  }
 catch (  Exception e) {
    String stackTrace=StackTraceUtil.getStackTrace(e);
    _log.error(stackTrace);
    throw new RemoteException(stackTrace);
  }
}
