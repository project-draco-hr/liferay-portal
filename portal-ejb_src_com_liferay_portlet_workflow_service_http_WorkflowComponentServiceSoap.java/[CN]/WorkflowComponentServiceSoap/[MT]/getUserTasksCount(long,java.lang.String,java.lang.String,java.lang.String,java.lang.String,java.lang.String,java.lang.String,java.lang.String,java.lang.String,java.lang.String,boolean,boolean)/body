{
  try {
    int returnValue=WorkflowComponentServiceUtil.getUserTasksCount(instanceId,taskName,definitionName,assignedTo,createDateGT,createDateLT,startDateGT,startDateLT,endDateGT,endDateLT,hideEndedTasks,andOperator);
