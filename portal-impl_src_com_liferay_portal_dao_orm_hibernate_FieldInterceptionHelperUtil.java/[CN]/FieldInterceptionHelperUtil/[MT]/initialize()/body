{
  try {
    Field instrumentedCacheField=ReflectionUtil.getDeclaredField(FieldInterceptionHelper.class,"_instrumentedCache");
    instrumentedCacheField.set(null,new ConcurrentReferenceKeyHashMap<Class<?>,Boolean>(FinalizeManager.WEAK_REFERENCE_FACTORY));
  }
 catch (  NoSuchFieldException nsfe) {
    _log.error("Missing Hibernate FieldInterceptionHelper patch from " + "LPS-52218");
  }
catch (  Exception e) {
    _log.error(e,e);
  }
}
