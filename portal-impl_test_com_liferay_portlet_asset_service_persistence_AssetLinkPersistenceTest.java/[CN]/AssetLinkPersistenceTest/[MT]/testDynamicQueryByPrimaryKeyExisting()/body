{
  AssetLink newAssetLink=addAssetLink();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AssetLink.class,AssetLink.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("linkId",newAssetLink.getLinkId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  AssetLink existingAssetLink=(AssetLink)result.get(0);
  assertEquals(existingAssetLink,newAssetLink);
}
