{
  List frameworkVersions=SRProductVersionLocalServiceUtil.getSRFrameworkVersions(getPrimaryKey());
  StringBuffer names=new StringBuffer();
  for (Iterator iterator=frameworkVersions.iterator(); iterator.hasNext(); ) {
    SRFrameworkVersion l=(SRFrameworkVersion)iterator.next();
    names.append(l.getName());
    if (iterator.hasNext()) {
      names.append(StringPool.COMMA + StringPool.SPACE);
    }
  }
  return names.toString();
}
