{
  Set<String> availableLanguageIds=new TreeSet<>();
  List<String> articleAvailableTitleAndDescriptionLanguageIds=JournalArticleLocalServiceUtil.getTitleAndDescriptionLanguageIds(getId());
  for (  String availableLanguageId : articleAvailableTitleAndDescriptionLanguageIds) {
    availableLanguageIds.add(availableLanguageId);
  }
  Document document=getDocument();
  if (document != null) {
    for (    String availableLanguageId : LocalizationUtil.getAvailableLanguageIds(document)) {
      availableLanguageIds.add(availableLanguageId);
    }
  }
  return availableLanguageIds.toArray(new String[availableLanguageIds.size()]);
}
