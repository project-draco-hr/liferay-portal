{
  if ((keywords == null) || ((keywords.length == 1) && (keywords[0] == null))) {
    return true;
  }
  for (  String keyword : keywords) {
    if (StringUtil.wildcardMatches(s,keyword,CharPool.UNDERLINE,CharPool.PERCENT,CharPool.BACK_SLASH,false)) {
      return true;
    }
  }
  return false;
}
