{
  if (start < 0) {
    start=0;
  }
  if ((end < 0) || (end > groups.size())) {
    end=groups.size();
  }
  if (start < end) {
    if (obc == null) {
      obc=new GroupNameComparator(true);
    }
    Collections.sort(groups,obc);
    return groups.subList(start,end);
  }
  return Collections.emptyList();
}
