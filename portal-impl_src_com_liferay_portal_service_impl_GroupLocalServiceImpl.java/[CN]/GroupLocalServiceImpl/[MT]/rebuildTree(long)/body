{
  TreePathUtil.rebuildTree(companyId,GroupConstants.DEFAULT_PARENT_GROUP_ID,new TreeModelFinder<Group>(){
    @Override public List<Group> findTreeModels(    long previousId,    long companyId,    long parentId,    int size) throws SystemException {
      return groupPersistence.findByG_C_P(previousId,companyId,parentId,QueryUtil.ALL_POS,size,new GroupIdComparator());
    }
  }
);
}
