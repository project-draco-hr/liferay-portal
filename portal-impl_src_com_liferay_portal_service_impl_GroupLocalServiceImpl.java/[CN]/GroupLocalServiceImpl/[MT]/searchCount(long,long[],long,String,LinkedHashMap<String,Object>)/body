{
  keywords=getRealName(companyId,keywords);
  String[] keywordsArray=null;
  if (Validator.isNotNull(keywords)) {
    keywordsArray=CustomSQLUtil.keywords(keywords);
  }
  boolean andOperator=false;
  if (Validator.isNull(keywords)) {
    andOperator=true;
  }
  return groupFinder.countByC_C_PG_N_D(companyId,classNameIds,parentGroupId,keywordsArray,keywordsArray,keywordsArray,params,andOperator);
}
