{
  String[] keywordsArray=getSearchNames(companyId,keywords);
  boolean andOperator=false;
  if (Validator.isNull(keywords)) {
    andOperator=true;
  }
  if (isInMemoryFilterable(classNameIds)) {
    List<Group> groups=doSearch(companyId,classNameIds,parentGroupId,keywordsArray,keywordsArray,params,andOperator);
    return groups.size();
  }
  return groupFinder.countByC_C_PG_N_D(companyId,classNameIds,parentGroupId,keywordsArray,keywordsArray,params,andOperator);
}
