{
  long defaultUserId=userLocalService.getDefaultUserId(group.getCompanyId());
  String friendlyURL=getFriendlyURL(PropsValues.USER_PERSONAL_PANEL_LAYOUT_FRIENDLY_URL);
  ServiceContext serviceContext=new ServiceContext();
  layoutLocalService.addLayout(defaultUserId,group.getGroupId(),true,LayoutConstants.DEFAULT_PARENT_LAYOUT_ID,PropsValues.USER_PERSONAL_PANEL_LAYOUT_NAME,StringPool.BLANK,StringPool.BLANK,LayoutConstants.TYPE_USER_PERSONAL_PANEL,false,friendlyURL,serviceContext);
}
