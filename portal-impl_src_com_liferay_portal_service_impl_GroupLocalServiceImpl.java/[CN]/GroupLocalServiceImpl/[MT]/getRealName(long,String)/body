{
  String realName=name;
  try {
    Company company=companyLocalService.getCompany(companyId);
    String companyName=company.getAccount().getName();
    name=StringUtil.replace(name,StringPool.PERCENT,StringPool.BLANK);
    if (companyName.indexOf(name) != -1) {
      realName=StringPool.PERCENT + GroupConstants.GUEST + StringPool.PERCENT;
    }
  }
 catch (  PortalException pe) {
  }
  return realName;
}
