{
  if (obc == null) {
    obc=new GroupNameComparator(true);
  }
  String realName=getRealName(companyId,name);
  String[] names=CustomSQLUtil.keywords(name);
  String[] realNames=CustomSQLUtil.keywords(realName);
  String[] descriptions=CustomSQLUtil.keywords(description);
  return groupFinder.findByC_C_PG_N_D(companyId,classNameIds,GroupConstants.ANY_PARENT_GROUP_ID,StringPool.NOT_EQUAL,names,realNames,descriptions,params,andOperator,start,end,obc);
}
