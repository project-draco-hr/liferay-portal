{
  String token=ParamUtil.getString(request,"ticket");
  if (Validator.isNull(token)) {
    return null;
  }
  try {
    Ticket ticket=TicketLocalServiceUtil.getTicket(token);
    if (!ticket.isExpired()) {
      return ticket;
    }
 else {
      TicketLocalServiceUtil.deleteTicket(ticket);
    }
  }
 catch (  Exception e) {
  }
  return null;
}
