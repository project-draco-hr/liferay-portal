{
  Revision newRevision=addRevision();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Revision.class,Revision.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("revisionId",newRevision.getRevisionId()));
  List<Revision> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Revision existingRevision=result.get(0);
  assertEquals(existingRevision,newRevision);
}
