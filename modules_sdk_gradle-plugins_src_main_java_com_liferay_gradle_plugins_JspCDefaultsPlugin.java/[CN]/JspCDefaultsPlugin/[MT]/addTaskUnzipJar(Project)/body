{
  Copy copy=GradleUtil.addTask(project,UNZIP_JAR_TASK_NAME,Copy.class);
  final Jar jar=(Jar)GradleUtil.getTask(project,JavaPlugin.JAR_TASK_NAME);
  copy.dependsOn(jar);
  copy.from(new Closure<FileTree>(null){
    @SuppressWarnings("unused") public FileTree doCall(){
      return project.zipTree(jar.getArchivePath());
    }
  }
);
  copy.into(new File(project.getBuildDir(),"unzipped-jar"));
  return copy;
}
