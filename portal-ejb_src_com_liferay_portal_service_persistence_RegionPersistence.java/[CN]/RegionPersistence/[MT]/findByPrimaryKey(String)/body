{
  Session session=null;
  try {
    session=openSession();
    RegionHBM regionHBM=(RegionHBM)session.get(RegionHBM.class,regionId);
    if (regionHBM == null) {
      _log.warn("No Region exists with the primary key " + regionId.toString());
      throw new NoSuchRegionException("No Region exists with the primary key " + regionId.toString());
    }
    return RegionHBMUtil.model(regionHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
