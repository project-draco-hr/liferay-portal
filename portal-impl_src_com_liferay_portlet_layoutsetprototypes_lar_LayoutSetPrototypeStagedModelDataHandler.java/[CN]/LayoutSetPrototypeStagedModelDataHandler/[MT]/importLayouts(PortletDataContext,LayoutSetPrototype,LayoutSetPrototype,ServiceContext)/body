{
  InputStream inputStream=null;
  try {
    String path=ExportImportPathUtil.getModelPath(layoutSetPrototype,getLayoutSetPrototypeLarFileName(layoutSetPrototype));
    inputStream=portletDataContext.getZipEntryAsInputStream(path);
    SitesUtil.importLayoutSetPrototype(importedLayoutSetPrototype,inputStream,serviceContext);
  }
  finally {
    StreamUtil.cleanUp(inputStream);
  }
}
