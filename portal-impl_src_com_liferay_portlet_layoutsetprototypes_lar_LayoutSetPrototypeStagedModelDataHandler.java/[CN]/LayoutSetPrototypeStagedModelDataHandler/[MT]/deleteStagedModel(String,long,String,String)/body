{
  Group group=GroupLocalServiceUtil.getGroup(groupId);
  LayoutSetPrototype layoutSetPrototype=LayoutSetPrototypeLocalServiceUtil.fetchLayoutSetPrototypeByUuidAndCompanyId(uuid,group.getCompanyId());
  if (layoutSetPrototype != null) {
    LayoutSetPrototypeLocalServiceUtil.deleteLayoutSetPrototype(layoutSetPrototype);
  }
}
