{
  if (tag.equals(HTML.Tag.A)) {
    String href=(String)attributes.getAttribute(HTML.Attribute.HREF);
    if (href != null) {
      _links.add(href);
    }
  }
 else   if (tag.equals(HTML.Tag.IMG)) {
    String src=(String)attributes.getAttribute(HTML.Attribute.SRC);
    if (src != null) {
      _images.add(src);
    }
  }
}
