{
  request.setAttribute("liferay-ui:ddm-template-selector:classNameId",String.valueOf(_classNameId));
  request.setAttribute("liferay-ui:ddm-template-selector:icon",_icon);
  request.setAttribute("liferay-ui:ddm-template-selector:message",_message);
  request.setAttribute("liferay-ui:ddm-template-selector:refreshURL",_refreshURL);
}
