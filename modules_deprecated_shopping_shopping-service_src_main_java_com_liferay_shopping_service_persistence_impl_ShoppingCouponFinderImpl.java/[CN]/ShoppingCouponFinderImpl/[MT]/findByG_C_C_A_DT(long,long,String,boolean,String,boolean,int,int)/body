{
  code=CustomSQLUtil.keywords(code)[0];
  Session session=null;
  try {
    session=openSession();
    String sql=CustomSQLUtil.get(getClass(),FIND_BY_G_C_C_A_DT);
    sql=CustomSQLUtil.replaceAndOperator(sql,andOperator);
    SQLQuery q=session.createSynchronizedSQLQuery(sql);
    q.addEntity("ShoppingCoupon",ShoppingCouponImpl.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(groupId);
    qPos.add(companyId);
    qPos.add(code);
    qPos.add(code);
    qPos.add(active);
    qPos.add(discountType);
    qPos.add(discountType);
    return (List<ShoppingCoupon>)QueryUtil.list(q,getDialect(),start,end);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
}
