{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLFileEntryMetadata.class,DLFileEntryMetadata.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("fileEntryMetadataId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("fileEntryMetadataId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
