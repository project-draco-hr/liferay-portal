{
  CalendarNotificationTemplate calendarNotificationTemplate=_calendarNotificationTemplateLocalService.fetchCalendarNotificationTemplate(GetterUtil.getLong(primKey));
  if (calendarNotificationTemplate == null) {
    return;
  }
  calendarNotificationTemplate.setModifiedDate(new Date());
  _calendarNotificationTemplateLocalService.updateCalendarNotificationTemplate(calendarNotificationTemplate);
}
