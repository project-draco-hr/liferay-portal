{
  _servletContextHelperRegistrationServiceReference=_bundleContext.getServiceReference(ServletContextHelperRegistration.class);
  ServletContextHelperRegistration servletContextHelperRegistration=_bundleContext.getService(_servletContextHelperRegistrationServiceReference);
  servletContextHelperRegistration.setProperties(contextParameters);
  ServletContext servletContext=servletContextHelperRegistration.getServletContext();
  _contextName=servletContext.getServletContextName();
  servletContext.setAttribute("jsp.taglib.mappings",jspTaglibMappings);
  servletContext.setAttribute("osgi-bundlecontext",_bundleContext);
  servletContext.setAttribute("osgi-runtime-vendor",_VENDOR);
  return servletContextHelperRegistration;
}
