{
  if (length >= buffer.length) {
    if (count > 0) {
      outputStream.write(buffer,0,count);
      count=0;
    }
    outputStream.write(bytes,offset,length);
    return;
  }
  if ((count > 0) && (length > buffer.length - count)) {
    outputStream.write(buffer,0,count);
    count=0;
  }
  System.arraycopy(bytes,offset,buffer,count,length);
  count+=length;
}
