{
  List<AddMenuItem> addMenuItems=(List<AddMenuItem>)request.getAttribute("liferay-frontend:add-menu:addMenuItems");
  request.setAttribute("liferay-frontend:add-menu:addMenuItems",addMenuItems);
  request.setAttribute("liferay-frontend:add-menu:maxItems",_maxItems);
  request.setAttribute("liferay-frontend:add-menu:viewMoreUrl",_viewMoreUrl);
}
