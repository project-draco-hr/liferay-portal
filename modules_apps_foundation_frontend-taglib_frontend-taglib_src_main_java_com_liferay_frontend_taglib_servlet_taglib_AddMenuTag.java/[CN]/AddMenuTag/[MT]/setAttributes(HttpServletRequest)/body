{
  request.setAttribute("liferay-frontend:add-menu:maxItems",_maxItems);
  request.setAttribute("liferay-frontend:add-menu:menuItemGroups",getMenuItemGroups());
  request.setAttribute("liferay-frontend:add-menu:menuItemsCount",getMenuItemsCount());
  request.setAttribute("liferay-frontend:add-menu:viewMoreURL",_viewMoreURL);
}
