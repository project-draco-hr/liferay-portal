{
  request.setAttribute("liferay-frontend:add-menu:addMenuItemsCount",getAddMenuItemsCount());
  request.setAttribute("liferay-frontend:add-menu:maxItems",_maxItems);
  request.setAttribute("liferay-frontend:add-menu:menuItems",getMenuItems());
  request.setAttribute("liferay-frontend:add-menu:viewMoreUrl",_viewMoreUrl);
}
