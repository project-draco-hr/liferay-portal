{
  try {
    if (resultSet != null) {
      resultSet.close();
    }
  }
 catch (  SQLException se) {
    ReflectionUtil.throwException(se);
  }
 finally {
    try {
      if (statement != null) {
        statement.close();
      }
    }
 catch (    SQLException se) {
      ReflectionUtil.throwException(se);
    }
 finally {
      try {
        if (connection != null) {
          connection.close();
        }
      }
 catch (      SQLException se) {
        ReflectionUtil.throwException(se);
      }
    }
  }
}
