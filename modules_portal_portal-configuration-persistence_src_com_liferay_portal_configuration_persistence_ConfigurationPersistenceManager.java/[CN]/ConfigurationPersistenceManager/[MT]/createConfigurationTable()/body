{
  Connection con=null;
  Statement s=null;
  ResultSet rs=null;
  try {
    con=_dataSource.getConnection();
    s=con.createStatement();
    s.executeUpdate(buildSQL(_TABLE_SQL_CREATE));
  }
 catch (  IOException|SQLException se) {
    ReflectionUtil.throwException(se);
  }
 finally {
    cleanUp(con,s,rs);
  }
}
