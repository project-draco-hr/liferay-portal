{
  Connection connection=null;
  PreparedStatement preparedStatement=null;
  ResultSet resultSet=null;
  try {
    connection=_dataSource.getConnection();
    preparedStatement=connection.prepareStatement(buildSQL("select configurationId, dictionary from Configuration_ " + "ORDER BY configurationId ASC"),ResultSet.TYPE_FORWARD_ONLY,ResultSet.CONCUR_READ_ONLY);
    resultSet=preparedStatement.executeQuery();
    while (resultSet.next()) {
      String pid=resultSet.getString(1);
      String dictionaryString=resultSet.getString(2);
      _dictionaries.putIfAbsent(pid,toDictionary(dictionaryString));
    }
  }
 catch (  IOException|SQLException e) {
    ReflectionUtil.throwException(e);
  }
 finally {
    cleanUp(connection,preparedStatement,resultSet);
  }
}
