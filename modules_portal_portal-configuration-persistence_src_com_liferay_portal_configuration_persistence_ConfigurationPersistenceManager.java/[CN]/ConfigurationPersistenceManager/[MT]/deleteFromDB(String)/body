{
  Connection con=null;
  PreparedStatement ps=null;
  try {
    con=_dataSource.getConnection();
    ps=con.prepareStatement(buildSQL(_DELETE_CONFIGURATION_SQL));
    ps.setString(1,pid);
    ps.executeUpdate();
  }
 catch (  SQLException se) {
    throw new IOException(se);
  }
 finally {
    cleanUp(con,ps,null);
  }
}
