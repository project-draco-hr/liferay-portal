{
  selenium.open("/web/expando-web-content-community/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Web Content Display Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Web Content Display Page",RuntimeVariables.replace("Web Content Display Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("First Expando Bank"),selenium.getText("//div[@class='journal-content-article']/h1"));
  assertTrue(selenium.isElementPresent("//input[@value='Create Account']"));
  assertEquals(RuntimeVariables.replace("Michael"),selenium.getText("//td[2]"));
  assertEquals(RuntimeVariables.replace("Hashimoto"),selenium.getText("//td[3]"));
  assertEquals(RuntimeVariables.replace("$100.00"),selenium.getText("//td[4]"));
  assertEquals(RuntimeVariables.replace("JR"),selenium.getText("//tr[3]/td[2]"));
  assertEquals(RuntimeVariables.replace("Houn"),selenium.getText("//tr[3]/td[3]"));
  assertEquals(RuntimeVariables.replace("$1,000,000.00"),selenium.getText("//tr[3]/td[4]"));
  assertEquals(RuntimeVariables.replace("Michael"),selenium.getText("//tr[4]/td[2]"));
  assertEquals(RuntimeVariables.replace("Saechang"),selenium.getText("//tr[4]/td[3]"));
  assertEquals(RuntimeVariables.replace("$1,000,000.00"),selenium.getText("//tr[4]/td[4]"));
  assertTrue(selenium.isTextPresent("# of Accounts: 3"));
}
