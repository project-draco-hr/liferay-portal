{
  if (isMatchingFile(file,"WEB-INF/liferay-look-and-feel.xml") && !isJarFile(file)) {
    return true;
  }
  String fileName=file.getName();
  Matcher matcher=_themePluginPattern.matcher(fileName);
  if (matcher.find() && isMatchingFile(file,"WEB-INF/liferay-plugin-package.properties")) {
    return true;
  }
  return false;
}
