{
  updateUserNotificationDelivery(UserNotificationDefinition.NOTIFICATION_TYPE_ADD_ENTRY,UserNotificationDeliveryConstants.TYPE_WEBSITE,false);
  BlogsEntry blogsEntry=addBlogsEntry();
  Assert.assertEquals(1,_logRecords.size());
  LogRecord logRecord=_logRecords.get(0);
  Assert.assertEquals("Sending email",logRecord.getMessage());
  List<JSONObject> userNotificationEventsJSONObjects=getUserNotificationEventsJSONObjects(_user.getUserId(),blogsEntry.getEntryId());
  Assert.assertEquals(0,userNotificationEventsJSONObjects.size());
}
