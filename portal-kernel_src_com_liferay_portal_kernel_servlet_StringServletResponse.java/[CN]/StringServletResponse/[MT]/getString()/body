{
  if (_string != null) {
    return _string;
  }
 else   if (_callGetOutputStream) {
    try {
      return _bam.toString(StringPool.UTF8);
    }
 catch (    UnsupportedEncodingException e) {
      e.printStackTrace();
      return StringPool.BLANK;
    }
  }
 else   if (_callGetWriter) {
    return _sw.toString();
  }
 else {
    return StringPool.BLANK;
  }
}
