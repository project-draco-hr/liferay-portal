{
  ThreadLocal tl=null;
synchronized (_sessionMap) {
    tl=(ThreadLocal)_sessionMap.get(sessionId);
    if (tl != null) {
      Long count=(Long)tl.get();
      if (count.longValue() == 0L) {
        _sessionMap.remove(sessionId);
      }
 else {
        count=new Long(count.longValue() - 1L);
        tl.set(count);
      }
    }
  }
}
