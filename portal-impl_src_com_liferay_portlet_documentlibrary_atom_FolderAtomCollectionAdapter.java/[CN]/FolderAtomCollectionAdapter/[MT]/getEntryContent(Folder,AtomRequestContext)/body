{
  AtomEntryContent atomEntryContent=new AtomEntryContent(AtomEntryContent.Type.XML);
  String srcLink=AtomUtil.createCollectionLink(atomRequestContext,DLFileEntryAtomCollectionAdapter._COLLECTION_NAME) + "?folderId=" + folder.getFolderId();
  atomEntryContent.setSrcLink(srcLink);
  return atomEntryContent;
}
