{
  AtomEntryContent atomEntryContent=new AtomEntryContent(AtomEntryContent.Type.XML);
  String srcLink=AtomUtil.createCollectionLink(atomRequestContext,FileEntryAtomCollectionAdapter._COLLECTION_NAME) + "?folderId=" + folder.getFolderId();
  atomEntryContent.setSrcLink(srcLink);
  return atomEntryContent;
}
