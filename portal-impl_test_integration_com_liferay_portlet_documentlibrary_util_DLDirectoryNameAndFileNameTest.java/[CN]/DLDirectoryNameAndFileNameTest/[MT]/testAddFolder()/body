{
  String name=StringUtil.randomString(20) + PropsValues.DL_CHAR_BLACKLIST[0];
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId(),TestPropsValues.getUserId());
  DLAppServiceUtil.addFolder(_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,name,RandomTestUtil.randomString(),serviceContext);
}
