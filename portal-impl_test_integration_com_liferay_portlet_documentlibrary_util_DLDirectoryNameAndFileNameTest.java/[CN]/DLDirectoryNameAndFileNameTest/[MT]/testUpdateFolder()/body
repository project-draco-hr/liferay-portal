{
  Folder folder=DLAppTestUtil.addFolder(_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID);
  String name=StringUtil.randomString(20) + PropsValues.DL_CHAR_BLACKLIST[0];
  DLAppServiceUtil.updateFolder(folder.getFolderId(),name,StringPool.BLANK,ServiceContextTestUtil.getServiceContext(_group.getGroupId()));
}
