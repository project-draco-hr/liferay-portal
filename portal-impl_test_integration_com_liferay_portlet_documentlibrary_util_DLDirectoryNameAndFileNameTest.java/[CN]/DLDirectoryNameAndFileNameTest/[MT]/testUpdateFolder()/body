{
  Folder folder=DLAppTestUtil.addFolder(_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,StringUtil.randomString(20));
  String name=StringUtil.randomString(20) + _BLACKLIST_CHARS[0];
  try {
    DLAppServiceUtil.updateFolder(folder.getFolderId(),name,StringPool.BLANK,ServiceTestUtil.getServiceContext(_group.getGroupId()));
    Assert.fail(name + " is an invalid name");
  }
 catch (  Exception e) {
    Assert.assertTrue(e instanceof FolderNameException);
  }
}
