{
  String url=StringPool.BLANK;
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    url=PermissionsURLTag.doTag(StringPool.BLANK,PasswordPolicy.class.getName(),themeDisplay.getScopeGroupName(),null,String.valueOf(getPasswordPolicyId(portletRequest)),LiferayWindowState.POP_UP.toString(),null,themeDisplay.getRequest());
  }
 catch (  Exception e) {
  }
  return url;
}
