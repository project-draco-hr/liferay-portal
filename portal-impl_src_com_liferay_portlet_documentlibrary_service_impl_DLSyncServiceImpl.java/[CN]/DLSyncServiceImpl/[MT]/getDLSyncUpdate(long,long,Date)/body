{
  GroupPermissionUtil.check(getPermissionChecker(),repositoryId,ActionKeys.VIEW);
  Date now=new Date();
  List<DLSync> dlSyncs=null;
  if (lastAccessDate != null) {
    dlSyncs=dlSyncPersistence.findByC_M_R(companyId,lastAccessDate,repositoryId);
  }
  DLSyncUpdate dlSyncUpdate=new DLSyncUpdate(dlSyncs,now);
  return dlSyncUpdate;
}
