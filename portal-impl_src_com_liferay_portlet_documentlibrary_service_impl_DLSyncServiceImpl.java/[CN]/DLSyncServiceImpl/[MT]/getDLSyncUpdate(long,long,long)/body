{
  repositoryService.checkRepository(repositoryId);
  List<DLSync> dlSyncs=dlSyncFinder.filterFindByC_M_R(companyId,lastAccessDate,repositoryId);
  for (  DLSync dlSync : dlSyncs) {
    if (dlSync.getModifiedDate() > lastAccessDate) {
      lastAccessDate=dlSync.getModifiedDate();
    }
  }
  DLSyncUpdate dlSyncUpdate=new DLSyncUpdate(dlSyncs,lastAccessDate);
  return dlSyncUpdate;
}
