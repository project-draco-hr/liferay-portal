{
  if (portletDataContext.addPrimaryKey(AdminPortletDataHandler.class,"deleteData")) {
    return portletPreferences;
  }
  _kbArticleLocalService.deleteGroupKBArticles(portletDataContext.getScopeGroupId());
  _kbTemplateLocalService.deleteGroupKBTemplates(portletDataContext.getScopeGroupId());
  return portletPreferences;
}
