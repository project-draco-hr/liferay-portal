{
  List<FileEntry> fileEntries=new ArrayList<>();
  long attachmentsFolderId=getAttachmentsFolderId();
  if (attachmentsFolderId != DLFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    fileEntries=PortletFileRepositoryUtil.getPortletFileEntries(getGroupId(),attachmentsFolderId,WorkflowConstants.STATUS_APPROVED,start,end,obc);
  }
  return fileEntries;
}
