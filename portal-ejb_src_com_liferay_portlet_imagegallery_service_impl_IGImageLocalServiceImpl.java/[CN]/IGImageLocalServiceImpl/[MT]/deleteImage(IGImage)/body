{
  TagsAssetLocalServiceUtil.deleteAsset(IGImage.class.getName(),image.getPrimaryKey().toString());
  ResourceLocalServiceUtil.deleteResource(image.getCompanyId(),IGImage.class.getName(),ResourceImpl.TYPE_CLASS,ResourceImpl.SCOPE_INDIVIDUAL,image.getPrimaryKey().toString());
  ImageLocalUtil.remove(image.getLargeImageId());
  ImageLocalUtil.remove(image.getSmallImageId());
  IGImageUtil.remove(image.getPrimaryKey());
}
