{
  TagsAssetLocalServiceUtil.deleteAsset(IGImage.class.getName(),String.valueOf(image.getImageId()));
  ResourceLocalServiceUtil.deleteResource(image.getCompanyId(),IGImage.class.getName(),ResourceImpl.SCOPE_INDIVIDUAL,image.getImageId());
  ImageLocalUtil.remove(image.getLargeImageId());
  ImageLocalUtil.remove(image.getSmallImageId());
  IGImageUtil.remove(image.getPrimaryKey());
}
