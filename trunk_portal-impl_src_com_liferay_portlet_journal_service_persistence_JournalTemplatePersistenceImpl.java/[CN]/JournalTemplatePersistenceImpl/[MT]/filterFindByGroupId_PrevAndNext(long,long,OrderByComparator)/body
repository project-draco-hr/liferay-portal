{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByGroupId_PrevAndNext(id,groupId,orderByComparator);
  }
  JournalTemplate journalTemplate=findByPrimaryKey(id);
  Session session=null;
  try {
    session=openSession();
    JournalTemplate[] array=new JournalTemplateImpl[3];
    array[0]=filterGetByGroupId_PrevAndNext(session,journalTemplate,groupId,orderByComparator,true);
    array[1]=journalTemplate;
    array[2]=filterGetByGroupId_PrevAndNext(session,journalTemplate,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
