{
  JournalTemplate journalTemplate=(JournalTemplate)EntityCacheUtil.getResult(JournalTemplateModelImpl.ENTITY_CACHE_ENABLED,JournalTemplateImpl.class,id,this);
  if (journalTemplate == null) {
    Session session=null;
    try {
      session=openSession();
      journalTemplate=(JournalTemplate)session.get(JournalTemplateImpl.class,Long.valueOf(id));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (journalTemplate != null) {
        cacheResult(journalTemplate);
      }
      closeSession(session);
    }
  }
  return journalTemplate;
}
