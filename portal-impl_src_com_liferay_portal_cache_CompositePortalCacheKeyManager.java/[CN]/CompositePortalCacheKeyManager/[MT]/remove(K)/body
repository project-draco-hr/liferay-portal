{
  while (true) {
    Set<K> oldValues=_portalCacheKeys.get(key.getSimpleKey());
    if (oldValues == null) {
      return;
    }
    Set<K> values=new HashSet<>(oldValues);
    if (values.remove(key)) {
      if (_portalCacheKeys.replace(key.getSimpleKey(),oldValues,values)) {
        return;
      }
    }
 else {
      return;
    }
  }
}
