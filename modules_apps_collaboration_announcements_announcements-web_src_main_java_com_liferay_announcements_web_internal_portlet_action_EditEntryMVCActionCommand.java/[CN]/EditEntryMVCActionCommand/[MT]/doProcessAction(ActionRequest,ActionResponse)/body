{
  String cmd=ParamUtil.getString(actionRequest,Constants.CMD);
  try {
    if (cmd.equals(Constants.ADD) || cmd.equals(Constants.UPDATE)) {
      updateEntry(actionRequest);
    }
 else     if (cmd.equals(Constants.DELETE)) {
      deleteEntry(actionRequest);
    }
  }
 catch (  EntryContentException|EntryDisplayDateException|EntryExpirationDateException|EntryTitleException|EntryURLException|NoSuchEntryException|PrincipalException e) {
    SessionErrors.add(actionRequest,e.getClass());
  }
}
