{
  executionContextUtilities.completeKaleoTimerInstances(executionContext);
  doExit(currentKaleoNode,executionContext,remainingPathElements);
  kaleoActionExecutor.executeKaleoActions(KaleoNode.class.getName(),currentKaleoNode.getKaleoNodeId(),ExecutionType.ON_EXIT,executionContext);
  notificationUtilities.sendKaleoNotifications(KaleoNode.class.getName(),currentKaleoNode.getKaleoNodeId(),ExecutionType.ON_EXIT,executionContext);
}
