{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(OrgGroupPermission.class,OrgGroupPermission.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id.organizationId",ServiceTestUtil.nextLong()));
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id.groupId",ServiceTestUtil.nextLong()));
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id.permissionId",ServiceTestUtil.nextLong()));
  List<OrgGroupPermission> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
