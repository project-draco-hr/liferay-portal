{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  String[] restoreEntryIds=StringUtil.split(ParamUtil.getString(actionRequest,"restoreEntryIds"));
  for (  String restoreEntryId : restoreEntryIds) {
    JournalArticleServiceUtil.restoreArticleFromTrash(themeDisplay.getScopeGroupId(),restoreEntryId);
  }
}
