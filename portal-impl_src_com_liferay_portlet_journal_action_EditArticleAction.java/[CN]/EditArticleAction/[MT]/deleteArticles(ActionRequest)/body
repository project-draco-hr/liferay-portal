{
  long groupId=ParamUtil.getLong(actionRequest,"groupId");
  String[] deleteArticleIds=StringUtil.split(ParamUtil.getString(actionRequest,"deleteArticleIds"));
  for (int i=0; i < deleteArticleIds.length; i++) {
    int pos=deleteArticleIds[i].lastIndexOf(VERSION_SEPARATOR);
    String articleId=deleteArticleIds[i].substring(0,pos);
    double version=GetterUtil.getDouble(deleteArticleIds[i].substring(pos + VERSION_SEPARATOR.length()));
    String articleURL=ParamUtil.getString(actionRequest,"articleURL");
    JournalArticleServiceUtil.deleteArticle(groupId,articleId,version,articleURL,actionRequest.getPreferences());
    JournalUtil.removeRecentArticle(actionRequest,deleteArticleIds[i]);
  }
}
