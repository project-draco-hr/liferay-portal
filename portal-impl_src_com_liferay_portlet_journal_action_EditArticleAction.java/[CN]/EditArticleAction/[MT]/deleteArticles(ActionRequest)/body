{
  long groupId=ParamUtil.getLong(req,"groupId");
  String[] deleteArticleIds=StringUtil.split(ParamUtil.getString(req,"deleteArticleIds"));
  for (int i=0; i < deleteArticleIds.length; i++) {
    int pos=deleteArticleIds[i].lastIndexOf(VERSION_SEPARATOR);
    String articleId=deleteArticleIds[i].substring(0,pos);
    double version=GetterUtil.getDouble(deleteArticleIds[i].substring(pos + VERSION_SEPARATOR.length()));
    String articleURL=ParamUtil.getString(req,"articleURL");
    JournalArticleServiceUtil.deleteArticle(groupId,articleId,version,articleURL,req.getPreferences());
    JournalUtil.removeRecentArticle(req,deleteArticleIds[i]);
  }
}
