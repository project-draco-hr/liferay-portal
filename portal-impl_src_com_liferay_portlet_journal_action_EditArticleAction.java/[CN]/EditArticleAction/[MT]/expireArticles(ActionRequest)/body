{
  long companyId=PortalUtil.getCompanyId(req);
  long groupId=ParamUtil.getLong(req,"groupId");
  String[] expireArticleIds=StringUtil.split(ParamUtil.getString(req,"expireArticleIds"));
  for (int i=0; i < expireArticleIds.length; i++) {
    int pos=expireArticleIds[i].lastIndexOf(VERSION_SEPARATOR);
    String articleId=expireArticleIds[i].substring(0,pos);
    double version=GetterUtil.getDouble(expireArticleIds[i].substring(pos + VERSION_SEPARATOR.length()));
    JournalArticleServiceUtil.expireArticle(companyId,groupId,articleId,version,null,null);
  }
}
