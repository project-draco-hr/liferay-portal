{
  if (SearchEngineUtil.isIndexReadOnly()) {
    return;
  }
  WikiPage page=wikiPageFinder.findByResourcePrimKey(resourcePrimKey);
  if (page == null) {
    return;
  }
  WikiNode node=wikiNodePersistence.fetchByPrimaryKey(page.getNodeId());
  long companyId=node.getCompanyId();
  long groupId=node.getGroupId();
  long nodeId=node.getNodeId();
  String title=page.getTitle();
  String content=page.getContent();
  String[] tagsEntries=tagsEntryLocalService.getEntryNames(WikiPage.class.getName(),page.getResourcePrimKey());
  try {
    Indexer.updatePage(companyId,groupId,nodeId,title,content,tagsEntries,page.getExpandoBridge());
  }
 catch (  SearchException se) {
    _log.error("Reindexing " + page.getPrimaryKey(),se);
  }
}
