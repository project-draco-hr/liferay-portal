{
  WikiPageResource wikiPageResource=wikiPageResourceLocalService.getPageResource(resourcePrimKey);
  List<WikiPage> pages=wikiPagePersistence.findByN_T(wikiPageResource.getNodeId(),wikiPageResource.getTitle(),0,1,new PageVersionComparator());
  WikiPage page=null;
  if (!pages.isEmpty()) {
    page=pages.get(0);
  }
 else {
    throw new NoSuchPageException();
  }
  return updateStatus(userId,page,status,serviceContext);
}
