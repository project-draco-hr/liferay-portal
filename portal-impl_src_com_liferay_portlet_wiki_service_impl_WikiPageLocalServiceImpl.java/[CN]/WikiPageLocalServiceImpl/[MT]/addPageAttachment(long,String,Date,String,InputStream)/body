{
  if (inputStream == null) {
    return;
  }
  String portletId=CompanyConstants.SYSTEM_STRING;
  long groupId=GroupConstants.DEFAULT_PARENT_GROUP_ID;
  long repositoryId=CompanyConstants.SYSTEM;
  try {
    dlLocalService.addDirectory(companyId,repositoryId,dirName);
  }
 catch (  DuplicateDirectoryException dde) {
  }
  dlLocalService.addFile(companyId,portletId,groupId,repositoryId,dirName + "/" + fileName,false,0,StringPool.BLANK,modifiedDate,new ServiceContext(),inputStream);
}
