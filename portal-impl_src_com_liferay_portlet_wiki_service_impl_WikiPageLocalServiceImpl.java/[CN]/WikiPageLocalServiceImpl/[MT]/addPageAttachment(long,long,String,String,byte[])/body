{
  if (Validator.isNull(fileName)) {
    return;
  }
  WikiPage page=getPage(nodeId,title);
  if (userId == 0) {
    userId=page.getUserId();
  }
  long companyId=page.getCompanyId();
  String portletId=CompanyConstants.SYSTEM_STRING;
  long groupId=GroupConstants.DEFAULT_PARENT_GROUP_ID;
  long repositoryId=CompanyConstants.SYSTEM;
  String dirName=page.getAttachmentsDir();
  try {
    DLStoreUtil.addDirectory(companyId,repositoryId,dirName);
  }
 catch (  DuplicateDirectoryException dde) {
  }
  socialEquityLogLocalService.addEquityLogs(userId,WikiPage.class.getName(),page.getResourcePrimKey(),ActionKeys.ADD_ATTACHMENT,dirName + "/" + fileName);
  DLStoreUtil.addFile(companyId,portletId,groupId,repositoryId,dirName + "/" + fileName,new ServiceContext(),bytes);
}
