{
  List<WikiPage> childPages=getChildren(node,true,trashTitle,WorkflowConstants.STATUS_IN_TRASH);
  for (  WikiPage curPage : childPages) {
    curPage.setParentTitle(title);
    wikiPagePersistence.update(curPage);
    if (!curPage.isInTrashExplicitly()) {
      restoreDependentFromTrash(curPage,page.getNodeId(),curPage.getParentTitle(),trashEntryId);
    }
  }
}
