{
  String title=TrashUtil.appendTrashNamespace(wikiPage.getTitle());
  wikiPage.setTitle(title);
  wikiPagePersistence.update(wikiPage,false);
  WikiPageResource wikiPageResource=wikiPageResourcePersistence.fetchByPrimaryKey(wikiPage.getResourcePrimKey());
  wikiPageResource.setTitle(title);
  wikiPageResourcePersistence.update(wikiPageResource,false);
  return updateStatus(userId,wikiPage,WorkflowConstants.STATUS_IN_TRASH,new ServiceContext());
}
