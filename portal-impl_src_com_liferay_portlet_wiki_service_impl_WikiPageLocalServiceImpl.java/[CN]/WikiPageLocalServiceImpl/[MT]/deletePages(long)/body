{
  List<WikiPage> pages=wikiPagePersistence.findByN_H_P(nodeId,true,StringPool.BLANK);
  for (  WikiPage page : pages) {
    wikiPageLocalService.deletePage(page);
  }
  pages=wikiPagePersistence.findByN_H_P(nodeId,false,StringPool.BLANK);
  for (  WikiPage page : pages) {
    wikiPageLocalService.deletePage(page);
  }
}
