{
  Iterator<WikiPage> itr=wikiPagePersistence.findByN_H_P(nodeId,true,StringPool.BLANK).iterator();
  while (itr.hasNext()) {
    WikiPage page=itr.next();
    deletePage(page);
  }
  itr=wikiPagePersistence.findByN_H_P(nodeId,false,StringPool.BLANK).iterator();
  while (itr.hasNext()) {
    WikiPage page=itr.next();
    deletePage(page);
  }
}
