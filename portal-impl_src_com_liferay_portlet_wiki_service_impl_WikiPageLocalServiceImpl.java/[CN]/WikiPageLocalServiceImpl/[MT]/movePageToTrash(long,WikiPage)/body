{
  String title=TrashUtil.appendTrashNamespace(page.getTitle());
  page.setTitle(title);
  wikiPagePersistence.update(page,false);
  WikiPageResource pageResource=wikiPageResourcePersistence.fetchByPrimaryKey(page.getResourcePrimKey());
  pageResource.setTitle(title);
  wikiPageResourcePersistence.update(pageResource,false);
  return updateStatus(userId,page,WorkflowConstants.STATUS_IN_TRASH,new ServiceContext());
}
