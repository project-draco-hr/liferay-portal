{
  List<WikiPage> redirectPages=wikiPagePersistence.findByN_H_R(page.getNodeId(),true,title);
  for (  WikiPage curPage : redirectPages) {
    curPage.setRedirectTitle(trashTitle);
    wikiPagePersistence.update(curPage);
    if (!curPage.isInTrashExplicitly()) {
      moveDependentToTrash(curPage,trashEntryId,createTrashEntry);
    }
  }
}
