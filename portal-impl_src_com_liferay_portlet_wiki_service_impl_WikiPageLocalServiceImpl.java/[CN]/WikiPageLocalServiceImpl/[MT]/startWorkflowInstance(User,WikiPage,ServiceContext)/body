{
  Map<String,Serializable> workflowContext=new HashMap<String,Serializable>();
  workflowContext.put(WorkflowConstants.CONTEXT_URL,getPageURL(page,serviceContext));
  WorkflowHandlerRegistryUtil.startWorkflowInstance(user.getCompanyId(),page.getGroupId(),user.getUserId(),WikiPage.class.getName(),page.getPageId(),page,serviceContext,workflowContext);
}
