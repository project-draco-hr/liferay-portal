{
  WikiPage wikiPage=getPage(nodeId,title);
  DLFileEntry dlFileEntry=PortletFileRepositoryUtil.getPortletFileEntry(wikiPage.getGroupId(),wikiPage.getAttachmentsFolderId(),fileName);
  long fileEntryId=dlFileEntry.getFileEntryId();
  PortletFileRepositoryUtil.movePortletFileEntryToTrash(userId,fileEntryId);
  return fileEntryId;
}
