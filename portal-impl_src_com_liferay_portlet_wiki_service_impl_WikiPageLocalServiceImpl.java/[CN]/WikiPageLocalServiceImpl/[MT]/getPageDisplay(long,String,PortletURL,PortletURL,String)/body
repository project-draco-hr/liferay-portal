{
  WikiPage page=getPage(nodeId,title);
  String formattedContent=WikiUtil.convert(page,viewPageURL,editPageURL,attachmentURLPrefix);
  return new WikiPageDisplayImpl(page.getUserId(),page.getNodeId(),page.getTitle(),page.getVersion(),page.getContent(),formattedContent,page.getFormat(),page.getHead(),page.getRedirectTo(),page.getAttachmentsFiles());
}
