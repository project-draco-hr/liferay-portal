{
  WikiPage page=getPage(nodeId,title);
  String htmlContent=WikiUtil.convert(page,viewPageURL,editPageURL,attachmentURLPrefix);
  return new WikiPageDisplayImpl(page.getNodeId(),page.getTitle(),page.getUserId(),page.getVersion(),page.getContent(),htmlContent,page.getFormat(),page.getHead(),page.getRedirectTo(),page.getAttachmentsFiles());
}
