{
  try {
    Indexer.deletePage(page.getCompanyId(),page.getNodeId(),page.getTitle());
  }
 catch (  IOException ioe) {
    _log.error("Deleting index " + page.getPrimaryKey(),ioe);
  }
  TagsAssetLocalServiceUtil.deleteAsset(WikiPage.class.getName(),page.getResourcePrimKey());
  MBMessageLocalServiceUtil.deleteDiscussionMessages(WikiPage.class.getName(),page.getResourcePrimKey());
  ResourceLocalServiceUtil.deleteResource(page.getCompanyId(),WikiPage.class.getName(),ResourceImpl.SCOPE_INDIVIDUAL,page.getResourcePrimKey());
  WikiPageResourceLocalServiceUtil.deletePageResource(page.getNodeId(),page.getTitle());
  WikiPageUtil.removeByN_T(page.getNodeId(),page.getTitle());
}
