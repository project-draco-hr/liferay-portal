{
  List<WikiPage> redirectPages=wikiPagePersistence.findByN_H_R_S(nodeId,true,trashTitle,WorkflowConstants.STATUS_IN_TRASH);
  for (  WikiPage curPage : redirectPages) {
    curPage.setRedirectTitle(title);
    wikiPagePersistence.update(curPage);
    if (!curPage.isInTrashExplicitly()) {
      restoreDependentFromTrash(curPage,page.getNodeId(),curPage.getParentTitle(),trashEntryId);
    }
  }
}
