{
  DDMFormLayoutColumn ddmFormLayoutColumn=createDDMFormLayoutColumn("field",12);
  DDMFormLayoutRow ddmFormLayoutRow=createDDMFormLayoutRow(ddmFormLayoutColumn);
  LocalizedValue pageTitle=createPageTitle(LocaleUtil.US,"Page1");
  DDMFormLayoutPage ddmFormLayoutPage=createDDMFormLayoutPage(ddmFormLayoutRow,pageTitle);
  DDMFormLayout ddmFormLayout=createDDMFormLayout(ddmFormLayoutPage,LocaleUtil.BRAZIL);
  _ddmFormLayoutValidator.validate(ddmFormLayout);
}
