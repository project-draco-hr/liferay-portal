{
  if (((wapThemeId == null) && (_wapThemeId != null)) || ((wapThemeId != null) && (_wapThemeId == null)) || ((wapThemeId != null) && (_wapThemeId != null) && !wapThemeId.equals(_wapThemeId))) {
    if (!XSS_ALLOW_WAPTHEMEID) {
      wapThemeId=XSSUtil.strip(wapThemeId);
    }
    _wapThemeId=wapThemeId;
  }
}
