{
  System.out.println("Now testing LDAP integration...");
  selenium.click(RuntimeVariables.replace("link=Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("link=Users"));
  selenium.waitForPageToLoad("30000");
  selenium.type("toggle_id_enterprise_admin_user_searchkeywords",RuntimeVariables.replace("jane"));
  selenium.click(RuntimeVariables.replace("//input[@value='Search']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("No users were found."));
  selenium.type("toggle_id_enterprise_admin_user_searchkeywords",RuntimeVariables.replace("luke"));
  selenium.click(RuntimeVariables.replace("//input[@value='Search']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("No users were found."));
  selenium.type("toggle_id_enterprise_admin_user_searchkeywords",RuntimeVariables.replace("martin"));
  selenium.click(RuntimeVariables.replace("//input[@value='Search']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("No users were found."));
}
