{
  LDAPConfiguration ldapCompanyServiceSettings=LDAPConfigurationUtil.getLDAPConfiguration(companyId);
  boolean defaultImportUserPasswordAutogenerated=ldapCompanyServiceSettings.importUserPasswordAutogenerated();
  if (isImportEnabled(companyId) && defaultImportUserPasswordAutogenerated) {
    return false;
  }
  boolean defaultLDAPExportEnabled=ldapCompanyServiceSettings.exportEnabled();
  if (PrefsPropsUtil.getBoolean(companyId,PropsKeys.LDAP_EXPORT_ENABLED,defaultLDAPExportEnabled)) {
    return true;
  }
 else {
    return false;
  }
}
