{
  boolean defaultImportUserPasswordAutogenerated=_ldapConfiguration.importUserPasswordAutogenerated();
  if (isImportEnabled(companyId) && defaultImportUserPasswordAutogenerated) {
    return false;
  }
  boolean defaultLDAPExportEnabled=_ldapConfiguration.exportEnabled();
  if (PrefsPropsUtil.getBoolean(companyId,PropsKeys.LDAP_EXPORT_ENABLED,defaultLDAPExportEnabled)) {
    return true;
  }
 else {
    return false;
  }
}
