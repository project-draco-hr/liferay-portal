{
  LDAPConfiguration ldapCompanyServiceSettings=LDAPConfigurationUtil.getLDAPConfiguration(companyId);
  boolean defaultLDAPImportEnabled=ldapCompanyServiceSettings.importEnabled();
  if (PrefsPropsUtil.getBoolean(companyId,PropsKeys.LDAP_IMPORT_ENABLED,defaultLDAPImportEnabled)) {
    return true;
  }
 else {
    return false;
  }
}
