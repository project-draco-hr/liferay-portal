{
  LDAPConfiguration ldapCompanyServiceSettings=LDAPConfigurationUtil.getLDAPConfiguration(companyId);
  boolean defaultLDAPExportGroupEnabled=ldapCompanyServiceSettings.exportGroupEnabled();
  if (PrefsPropsUtil.getBoolean(companyId,PropsKeys.LDAP_EXPORT_GROUP_ENABLED,defaultLDAPExportGroupEnabled)) {
    return true;
  }
 else {
    return false;
  }
}
