{
  DDMStructure ddmStructure=DDMStructureTestUtil.addStructure(JournalArticle.class.getName());
  DDMTemplate ddmTemplate=DDMTemplateTestUtil.addTemplate(ddmStructure.getStructureId(),PortalUtil.getClassNameId(JournalArticle.class));
  String content="<?xml version=\"1.0\"?><root></root>";
  JournalArticle article=JournalTestUtil.addArticleWithXMLContent(content,ddmStructure.getStructureKey(),ddmTemplate.getTemplateKey());
  checkArticleMatchesStructure(article,ddmStructure);
}
