{
  if (!actionId.equals(ActionKeys.ADD_TO_PAGE) && !actionId.equals(ActionKeys.CONFIGURATION) && !actionId.equals(ActionKeys.CUSTOMIZE)&& !actionId.equals(ActionKeys.DELETE)&& !actionId.equals(ActionKeys.VIEW)&& group.hasLocalOrRemoteStagingGroup()&& (Validator.isNull(portletId) || group.isStagedPortlet(portletId))) {
    return false;
  }
 else {
    return null;
  }
}
