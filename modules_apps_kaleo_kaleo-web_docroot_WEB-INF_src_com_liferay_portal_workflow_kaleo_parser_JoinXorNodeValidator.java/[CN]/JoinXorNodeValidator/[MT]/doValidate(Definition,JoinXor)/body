{
  if (joinXor.getIncomingTransitionsCount() == 0) {
    throw new WorkflowException("No incoming transition found for join-xor " + joinXor.getName());
  }
  if (joinXor.getOutgoingTransitionsCount() == 0) {
    throw new WorkflowException("No outgoing transition found for join-xor " + joinXor.getName());
  }
}
