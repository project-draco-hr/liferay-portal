{
  KaleoTimer kaleoTimer=findByPrimaryKey(kaleoTimerId);
  Session session=null;
  try {
    session=openSession();
    KaleoTimer[] array=new KaleoTimerImpl[3];
    array[0]=getByKCN_KCPK_Blocking_PrevAndNext(session,kaleoTimer,kaleoClassName,kaleoClassPK,blocking,orderByComparator,true);
    array[1]=kaleoTimer;
    array[2]=getByKCN_KCPK_Blocking_PrevAndNext(session,kaleoTimer,kaleoClassName,kaleoClassPK,blocking,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
