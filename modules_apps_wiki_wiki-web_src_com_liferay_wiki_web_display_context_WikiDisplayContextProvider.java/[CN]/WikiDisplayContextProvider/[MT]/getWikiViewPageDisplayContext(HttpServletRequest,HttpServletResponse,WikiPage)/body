{
  Collection<WikiDisplayContextFactory> wikiDisplayContextFactories=_wikiDisplayContextFactories.values();
  WikiViewPageDisplayContext wikiViewPageDisplayContext=new DefaultWikiViewPageDisplayContext(request,response,wikiPage);
  for (  WikiDisplayContextFactory wikiDisplayContextFactory : wikiDisplayContextFactories) {
    wikiViewPageDisplayContext=wikiDisplayContextFactory.getWikiViewPageDisplayContext(wikiViewPageDisplayContext,request,response,wikiPage);
  }
  return wikiViewPageDisplayContext;
}
