{
  Collection<WikiDisplayContextFactory> wikiDisplayContextFactories=_wikiDisplayContextFactories.values();
  WikiListPagesDisplayContext wikiListPagesDisplayContext=new DefaultWikiListPagesDisplayContext(request,response,wikiNode);
  for (  WikiDisplayContextFactory wikiDisplayContextFactory : wikiDisplayContextFactories) {
    wikiListPagesDisplayContext=wikiDisplayContextFactory.getWikiListPagesDisplayContext(wikiListPagesDisplayContext,request,response,wikiNode);
  }
  return wikiListPagesDisplayContext;
}
