{
  DDMStructure ddmStructure=DDMStructureTestUtil.addStructure(stagingGroup.getGroupId(),DDLRecordSet.class.getName());
  DDLRecordSet recordSet=DDLTestUtil.addRecordSet(stagingGroup.getGroupId(),ddmStructure.getStructureId());
  DDLTestUtil.addRecord(stagingGroup.getGroupId(),recordSet.getRecordSetId());
}
