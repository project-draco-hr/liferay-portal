{
  DDMStructure ddmStructure=DDMStructureTestUtil.addStructure(stagingGroup.getGroupId(),DDLRecordSet.class.getName());
  DDLRecordSetTestHelper recordSetTestHelper=new DDLRecordSetTestHelper(stagingGroup);
  DDLRecordSet recordSet=recordSetTestHelper.addRecordSet(ddmStructure);
  DDLRecordTestHelper recordTestHelper=new DDLRecordTestHelper(stagingGroup,recordSet);
  recordTestHelper.addRecord();
}
