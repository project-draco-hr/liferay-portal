{
  try {
    MethodWrapper methodWrapper=new MethodWrapper(IndexerImpl.class.getName(),"updateFile",new Object[]{new LongWrapper(companyId),portletId,new LongWrapper(groupId),new LongWrapper(repositoryId),fileName});
    IndexProducer.produce(methodWrapper);
  }
 catch (  Exception e) {
    if (e instanceof IOException) {
      throw (IOException)e;
    }
 else {
      _log.error(e);
    }
  }
}
