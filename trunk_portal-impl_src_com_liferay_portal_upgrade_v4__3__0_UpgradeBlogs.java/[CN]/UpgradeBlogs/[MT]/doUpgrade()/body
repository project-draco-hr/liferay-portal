{
  UpgradeColumn upgradeGroupIdColumn=new SwapUpgradeColumnImpl("groupId",AvailableMappersUtil.getGroupIdMapper());
  UpgradeColumn upgradeUserIdColumn=new SwapUpgradeColumnImpl("userId",new Integer(Types.VARCHAR),AvailableMappersUtil.getUserIdMapper());
  PKUpgradeColumnImpl upgradePKColumn=new PKUpgradeColumnImpl("entryId",true);
  UpgradeTable upgradeTable=UpgradeTableFactoryUtil.getUpgradeTable(BlogsEntryTable.TABLE_NAME,BlogsEntryTable.TABLE_COLUMNS,upgradePKColumn,upgradeGroupIdColumn,upgradeUserIdColumn);
  upgradeTable.setCreateSQL(BlogsEntryTable.TABLE_SQL_CREATE);
  upgradeTable.setIndexesSQL(BlogsEntryTable.TABLE_SQL_ADD_INDEXES);
  upgradeTable.updateTable();
  ValueMapper entryIdMapper=upgradePKColumn.getValueMapper();
  AvailableMappersUtil.setBlogsEntryIdMapper(entryIdMapper);
}
