{
  GeoPolygonQueryBuilder geoPolygonQueryBuilder=QueryBuilders.geoPolygonQuery(geoPolygonFilter.getField());
  for (  GeoLocationPoint geoLocationPoint : geoPolygonFilter.getGeoLocationPoints()) {
    geoPolygonQueryBuilder.addPoint(geoLocationPoint.getLatitude(),geoLocationPoint.getLongitude());
  }
  return geoPolygonQueryBuilder;
}
