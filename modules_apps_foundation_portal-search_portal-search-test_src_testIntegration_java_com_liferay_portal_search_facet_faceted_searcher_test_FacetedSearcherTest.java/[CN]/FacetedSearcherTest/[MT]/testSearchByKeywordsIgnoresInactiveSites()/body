{
  Group group1=userSearchFixture.addGroup();
  userSearchFixture.addUser(group1,"Liferay " + RandomTestUtil.randomString());
  Group group2=userSearchFixture.addGroup();
  userSearchFixture.addUser(group2,"Liferay " + RandomTestUtil.randomString());
  assertSearch("Liferay",2);
  deactivate(group1);
  assertSearch("Liferay",1);
  deactivate(group2);
  assertSearch("Liferay",0);
}
