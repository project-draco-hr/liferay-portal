{
  if ((_documents == null) || _documents.isEmpty()) {
    return;
  }
  if (Validator.isNull(_searchEngineId)) {
    _searchEngineId=SearchEngineUtil.getSearchEngineId(_documents);
  }
  SearchEngineUtil.updateDocuments(_searchEngineId,_companyId,new ArrayList<Document>(_documents),_commitImmediately);
  _documents.clear();
}
