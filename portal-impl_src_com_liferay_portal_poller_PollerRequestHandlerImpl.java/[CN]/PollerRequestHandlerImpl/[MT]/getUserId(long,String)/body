{
  long userId=0;
  try {
    Company company=CompanyLocalServiceUtil.getCompany(companyId);
    userId=GetterUtil.getLong(Encryptor.decrypt(company.getKeyObj(),userIdString));
  }
 catch (  Exception e) {
    _log.error("Invalid credentials for company id " + companyId + " and user id "+ userIdString);
  }
  return userId;
}
