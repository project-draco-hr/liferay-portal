{
  if ((name == null) || name.contains("\\") || name.contains("\\\\")|| name.contains("//")|| name.contains(":")|| name.contains("*")|| name.contains("?")|| name.contains("\"")|| name.contains("<")|| name.contains(">")|| name.contains("|")|| name.contains("[")|| name.contains("]")|| name.contains("../")|| name.contains("/..")) {
    return false;
  }
  return true;
}
