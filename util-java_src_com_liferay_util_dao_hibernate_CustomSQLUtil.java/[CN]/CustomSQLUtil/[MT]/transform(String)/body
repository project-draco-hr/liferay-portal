{
  StringMaker sm=new StringMaker();
  try {
    BufferedReader br=new BufferedReader(new StringReader(sql));
    String line=null;
    while ((line=br.readLine()) != null) {
      sm.append(line.trim());
      sm.append(StringPool.SPACE);
    }
    br.close();
  }
 catch (  IOException ioe) {
    return sql;
  }
  return sm.toString();
}
