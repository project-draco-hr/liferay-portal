{
  Folder ddmFolder=addFolder(userId,groupId,DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,"DDM",StringPool.BLANK);
  Folder primaryKeyFolder=addFolder(userId,groupId,ddmFolder.getFolderId(),String.valueOf(primaryKey),StringPool.BLANK);
  return addFolder(userId,groupId,primaryKeyFolder.getFolderId(),fieldName,StringPool.BLANK);
}
