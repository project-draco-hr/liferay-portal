{
  DLFileEntryMetadata dlFileEntryMetadata=DLFileEntryMetadataLocalServiceUtil.getFileEntryMetadata(dlFileEntryMetadataId);
  FileEntry fileEntry=DLAppLocalServiceUtil.getFileEntry(dlFileEntryMetadata.getFileEntryId());
  FileVersion fileVersion=fileEntry.getFileVersion();
  updateFileUploadReferences(fileEntry.getCompanyId(),dlFileEntryMetadata.getDDMStorageId(),fileEntry.getUserId(),fileEntry.getGroupId(),dlFileEntryMetadata,fileVersion.getStatus());
}
