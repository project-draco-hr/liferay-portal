{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PollsQuestion.class,PollsQuestion.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("questionId",ServiceTestUtil.nextLong()));
  List<PollsQuestion> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
