{
  SCProductScreenshot scProductScreenshot=fetchByPrimaryKey(productScreenshotId);
  if (scProductScreenshot == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + productScreenshotId);
    }
    throw new NoSuchProductScreenshotException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + productScreenshotId);
  }
  return scProductScreenshot;
}
