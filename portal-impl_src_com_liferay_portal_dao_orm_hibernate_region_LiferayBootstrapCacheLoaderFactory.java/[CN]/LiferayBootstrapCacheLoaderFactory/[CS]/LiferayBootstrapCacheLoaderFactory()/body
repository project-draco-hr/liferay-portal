{
  String className=PropsValues.EHCACHE_BOOTSTRAP_CACHE_LOADER_FACTORY;
  if (PropsValues.CLUSTER_LINK_ENABLED && PropsValues.EHCACHE_CLUSTER_LINK_REPLICATION_ENABLED) {
    className=EhcacheStreamBootstrapCacheLoaderFactory.class.getName();
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiating " + className + " "+ hashCode());
  }
  try {
    _bootstrapCacheLoaderFactory=(BootstrapCacheLoaderFactory<T>)InstanceFactory.newInstance(PortalClassLoaderUtil.getClassLoader(),className);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
