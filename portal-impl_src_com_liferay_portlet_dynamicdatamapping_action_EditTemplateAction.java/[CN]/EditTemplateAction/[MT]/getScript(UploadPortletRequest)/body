{
  String script=null;
  try {
    InputStream is=uploadPortletRequest.getFileAsStream("script");
    if (is != null) {
      script=new String(FileUtil.getBytes(is));
      is.close();
    }
  }
 catch (  IOException ioe) {
    if (_log.isWarnEnabled()) {
      _log.warn(ioe,ioe);
    }
  }
  return script;
}
