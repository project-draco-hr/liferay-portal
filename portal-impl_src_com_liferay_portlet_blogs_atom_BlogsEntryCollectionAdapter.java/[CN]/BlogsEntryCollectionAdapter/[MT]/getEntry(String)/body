{
  long blogsEntryId=GetterUtil.getLong(resourceName);
  if (blogsEntryId == 0) {
    throw new AtomException(404);
  }
  BlogsEntry blogsEntry=null;
  try {
    blogsEntry=BlogsEntryServiceUtil.getEntry(blogsEntryId);
  }
 catch (  Exception e) {
    throw new AtomException(500,e);
  }
  return blogsEntry;
}
