{
  if (xsl.indexOf("\\n") != -1) {
    xsl=StringUtil.replace(xsl,new String[]{"\\n","\\r"},new String[]{"\n","\r"});
  }
  String templateId=(String)_templateIdColumn.getOldValue();
  if (templateId.equals("BASIC-BANNER")) {
    xsl=StringUtil.replace(xsl,"background-repeat: no-repeat; width: 520; height: 175;","background-repeat: no-repeat; width: 520px; height: 175px;");
  }
  return xsl;
}
