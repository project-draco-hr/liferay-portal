{
  UpgradeColumn upgradeCompanyIdColumn=new SwapUpgradeColumnImpl("companyId",new Integer(Types.VARCHAR),AvailableMappersUtil.getCompanyIdMapper());
  PKUpgradeColumnImpl pkUpgradeColumn=new PKUpgradeColumnImpl("userGroupId",true);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(UserGroupImpl.TABLE_NAME,UserGroupImpl.TABLE_COLUMNS,pkUpgradeColumn,upgradeCompanyIdColumn);
  upgradeTable.setCreateSQL(UserGroupImpl.TABLE_SQL_CREATE);
  upgradeTable.updateTable();
  ValueMapper userGroupIdMapper=pkUpgradeColumn.getValueMapper();
  AvailableMappersUtil.setUserGroupIdMapper(userGroupIdMapper);
}
