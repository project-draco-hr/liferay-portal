{
  PKUpgradeColumnImpl upgradePKColumn=new PKUpgradeColumnImpl("userGroupId",true);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(UserGroupImpl.TABLE_NAME,UserGroupImpl.TABLE_COLUMNS,upgradePKColumn);
  upgradeTable.setCreateSQL(UserGroupImpl.TABLE_SQL_CREATE);
  upgradeTable.updateTable();
  ValueMapper userGroupIdMapper=upgradePKColumn.getValueMapper();
  AvailableMappersUtil.setUserGroupIdMapper(userGroupIdMapper);
}
