{
  TransactionStatusAdapter transactionStatusAdapter=start(platformTransactionManager,transactionAttributeAdapter);
  Object returnValue=null;
  try {
    returnValue=methodInvocation.proceed();
  }
 catch (  Throwable throwable) {
    rollback(platformTransactionManager,throwable,transactionAttributeAdapter,transactionStatusAdapter);
  }
  commit(platformTransactionManager,transactionAttributeAdapter,transactionStatusAdapter);
  return returnValue;
}
