{
  TransactionStatusAdaptor transactionStatusAdaptor=start(platformTransactionManager,transactionAttribute);
  Object returnValue=null;
  try {
    returnValue=methodInvocation.proceed();
  }
 catch (  Throwable throwable) {
    rollback(platformTransactionManager,throwable,transactionAttribute,transactionStatusAdaptor);
  }
  commit(platformTransactionManager,transactionAttribute,transactionStatusAdaptor);
  return returnValue;
}
