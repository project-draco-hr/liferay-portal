{
  boolean equals=true;
  if (this.getStyle().equals(newTok.getStyle()) && this.getOffset() == newTok.getOffset()) {
    for (int ii=this.getBeg(), jj=newTok.getBeg(); ii < this.getEnd() && jj < newTok.getEnd(); ++ii, ++jj) {
      if (myStr.charAt(ii) != newStr.charAt(jj)) {
        equals=false;
        break;
      }
    }
  }
 else {
    equals=false;
  }
  return equals;
}
