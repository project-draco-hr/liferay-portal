{
  List<AssetCategoryProperty> filteredAssetCategoryProperties=new ArrayList<>(assetCategoryProperties.size());
  for (  AssetCategoryProperty assetCategoryProperty : assetCategoryProperties) {
    try {
      if (AssetCategoryPermission.contains(getPermissionChecker(),assetCategoryProperty.getCategoryId(),ActionKeys.VIEW)) {
        filteredAssetCategoryProperties.add(assetCategoryProperty);
      }
    }
 catch (    PortalException pe) {
      continue;
    }
  }
  return filteredAssetCategoryProperties;
}
