{
  long pk=nextLong();
  ExpandoTableRow newExpandoTableRow=_persistence.create(pk);
  newExpandoTableRow.setTableId(nextLong());
  _persistence.update(newExpandoTableRow);
  ExpandoTableRow existingExpandoTableRow=_persistence.findByPrimaryKey(newExpandoTableRow.getPrimaryKey());
  assertEquals(existingExpandoTableRow.getRowId(),newExpandoTableRow.getRowId());
  assertEquals(existingExpandoTableRow.getTableId(),newExpandoTableRow.getTableId());
}
