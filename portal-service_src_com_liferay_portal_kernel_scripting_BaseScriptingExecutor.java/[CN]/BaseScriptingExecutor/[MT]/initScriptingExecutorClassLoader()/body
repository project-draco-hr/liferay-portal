{
  Class<?> clazz=getClass();
  ClassLoader classLoader=clazz.getClassLoader();
  if (!classLoader.equals(PortalClassLoaderUtil.getClassLoader())) {
    _scriptingExecutorClassLoader=AggregateClassLoader.getAggregateClassLoader(PortalClassLoaderUtil.getClassLoader(),classLoader);
  }
 else {
    _scriptingExecutorClassLoader=classLoader;
  }
}
