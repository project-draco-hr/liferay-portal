{
  if (_imageType == null) {
    try {
      Image largeImage=ImageLocalServiceUtil.getImage(getLargeImageId());
      if (largeImage != null) {
        _imageType=largeImage.getType();
      }
 else {
        _imageType=StringPool.BLANK;
      }
    }
 catch (    Exception e) {
      _imageType=StringPool.BLANK;
      _log.error(e);
    }
  }
  return _imageType;
}
