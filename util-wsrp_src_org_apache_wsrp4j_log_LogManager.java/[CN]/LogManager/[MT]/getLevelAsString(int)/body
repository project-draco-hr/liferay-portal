{
  String sLevel;
  if (level < Logger.WARN) {
    sLevel="ERROR";
  }
 else   if (level < Logger.INFO) {
    sLevel="WARN";
  }
 else   if (level < Logger.TRACE_LOW) {
    sLevel="INFO";
  }
 else   if (level < Logger.TRACE_MEDIUM) {
    sLevel="TRACE_LOW";
  }
 else   if (level < Logger.TRACE_HIGH) {
    sLevel="TRACE_MEDIUM";
  }
 else {
    sLevel="TRACE_HIGH";
  }
  return sLevel;
}
