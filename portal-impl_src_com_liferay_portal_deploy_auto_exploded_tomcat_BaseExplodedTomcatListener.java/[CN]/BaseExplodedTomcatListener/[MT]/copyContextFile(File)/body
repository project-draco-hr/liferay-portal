{
  try {
    String tomcatConfDir=PrefsPropsUtil.getString(PropsUtil.AUTO_DEPLOY_TOMCAT_CONF_DIR);
    if (_log.isInfoEnabled()) {
      _log.info("Copying file " + file.getPath() + " to "+ tomcatConfDir);
    }
    FileUtil.copyFile(file,new File(tomcatConfDir + "/" + file.getName()));
  }
 catch (  Exception e) {
    throw new AutoDeployException(e.getMessage());
  }
}
