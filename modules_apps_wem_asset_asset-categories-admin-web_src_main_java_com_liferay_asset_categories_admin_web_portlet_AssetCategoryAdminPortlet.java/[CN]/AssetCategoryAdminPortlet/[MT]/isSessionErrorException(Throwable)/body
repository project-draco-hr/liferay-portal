{
  if (cause instanceof AssetCategoryNameException || cause instanceof CategoryPropertyKeyException || cause instanceof CategoryPropertyValueException|| cause instanceof DuplicateCategoryException|| cause instanceof DuplicateCategoryPropertyException|| cause instanceof DuplicateVocabularyException|| cause instanceof NoSuchCategoryException|| cause instanceof NoSuchVocabularyException|| cause instanceof PrincipalException|| cause instanceof VocabularyNameException) {
    return true;
  }
  return false;
}
