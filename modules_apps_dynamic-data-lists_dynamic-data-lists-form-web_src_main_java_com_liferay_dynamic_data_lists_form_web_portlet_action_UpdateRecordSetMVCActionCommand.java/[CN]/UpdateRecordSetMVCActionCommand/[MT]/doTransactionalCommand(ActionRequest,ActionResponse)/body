{
  DDMStructure ddmStructure=updateDDMStructure(actionRequest);
  DDLRecordSet recordSet=updateRecordSet(actionRequest,ddmStructure.getStructureId());
  UnicodeProperties settingsProperties=recordSet.getSettingsProperties();
  boolean publish=ParamUtil.getBoolean(actionRequest,"publish");
  settingsProperties.setProperty("published",String.valueOf(publish));
  ddlRecordSetService.updateRecordSet(recordSet.getRecordSetId(),settingsProperties.toString());
}
