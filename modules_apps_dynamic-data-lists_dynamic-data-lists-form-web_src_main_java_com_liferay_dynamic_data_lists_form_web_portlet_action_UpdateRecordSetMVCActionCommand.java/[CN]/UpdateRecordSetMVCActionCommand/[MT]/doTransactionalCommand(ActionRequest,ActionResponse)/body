{
  DDMStructure ddmStructure=updateDDMStructure(actionRequest);
  DDLRecordSet recordSet=updateRecordSet(actionRequest,ddmStructure.getStructureId());
  UnicodeProperties typeSettingsProperties=recordSet.getSettingsProperties();
  boolean publish=ParamUtil.getBoolean(actionRequest,"publish");
  typeSettingsProperties.setProperty("published",String.valueOf(publish));
  ddlRecordSetService.updateRecordSet(recordSet.getRecordSetId(),typeSettingsProperties.toString());
}
