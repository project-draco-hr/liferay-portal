{
  if (expression == null) {
    return new String[0];
  }
  List<String> variableNames=new ArrayList<String>();
  Matcher matcher=VARIABLE_REGEX_PATTERN.matcher(expression);
  while (matcher.find()) {
    variableNames.add(matcher.group(1));
  }
  return variableNames.toArray(new String[variableNames.size()]);
}
