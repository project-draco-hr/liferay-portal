{
  long classNameId=PortalUtil.getClassNameId(className);
  List<RatingsStats> list=ratingsStatsFinder.findByC_C(classNameId,classPKs);
  List<RatingsStats> retval=ListUtil.copy(list);
  int delta=classPKs.size() - list.size();
  for (int i=0; delta > 0 && i < classPKs.size(); i++) {
    long classPK=classPKs.get(i);
    boolean found=false;
    for (    RatingsStats stats : list) {
      if (classPK == stats.getClassPK()) {
        found=true;
        break;
      }
    }
    if (!found) {
      RatingsStats stats=ratingsStatsLocalService.addStats(classNameId,classPK);
      retval.add(stats);
      delta--;
    }
  }
  return retval;
}
