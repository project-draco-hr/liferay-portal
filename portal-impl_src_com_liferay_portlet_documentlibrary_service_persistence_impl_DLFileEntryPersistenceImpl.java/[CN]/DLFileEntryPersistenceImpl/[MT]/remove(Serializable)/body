{
  Session session=null;
  try {
    session=openSession();
    DLFileEntry dlFileEntry=(DLFileEntry)session.get(DLFileEntryImpl.class,primaryKey);
    if (dlFileEntry == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchFileEntryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(dlFileEntry);
  }
 catch (  NoSuchFileEntryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
