{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AssetTagStats.class,AssetTagStats.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("tagStatsId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("tagStatsId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
