{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(productVersionId));
  }
  com.liferay.portlet.softwarecatalog.model.SCProductVersion scProductVersion=getPersistence().remove(productVersionId);
  if (listener != null) {
    listener.onAfterRemove(scProductVersion);
  }
  return scProductVersion;
}
