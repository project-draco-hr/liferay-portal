{
  try {
    AdminConfigService adminConfigService=AdminConfigServiceFactory.getService();
    adminConfigService.updateShoppingConfig(shoppingConfig);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
