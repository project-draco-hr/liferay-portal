{
  String randomString=getRegularRandomString();
  TikaSafeRandomizerBumper tikaSafeRandomizerBumper=new TikaSafeRandomizerBumper(ContentTypes.TEXT_PLAIN);
  Assert.assertTrue(tikaSafeRandomizerBumper.accept(randomString.getBytes()));
  randomString=_EXE_MAGIC_HEADER.concat(RandomTestUtil.randomString(6));
  Assert.assertFalse(tikaSafeRandomizerBumper.accept(randomString.getBytes()));
}
