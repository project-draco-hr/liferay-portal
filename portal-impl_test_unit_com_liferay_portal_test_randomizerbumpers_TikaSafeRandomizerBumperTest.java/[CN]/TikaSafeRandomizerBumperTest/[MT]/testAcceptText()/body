{
  String randomString=getRegularRandomString();
  Assert.assertTrue(TikaSafeRandomizerBumper.TEXT_PLAIN_INSTANCE.accept(randomString.getBytes()));
  randomString=_EXE_MAGIC_HEADER.concat(RandomTestUtil.randomString(6));
  Assert.assertFalse(TikaSafeRandomizerBumper.TEXT_PLAIN_INSTANCE.accept(randomString.getBytes()));
}
