{
  TikaSafeRandomizerBumper tikaSafeRandomizerBumper=new TikaSafeRandomizerBumper("application/x-msdownload");
  Assert.assertTrue(tikaSafeRandomizerBumper.accept(EXE_BYTE_ARRAY));
  String randomString=RandomTestUtil.randomString();
  while (randomString.startsWith("MZ")) {
    randomString=RandomTestUtil.randomString();
  }
  Assert.assertFalse(tikaSafeRandomizerBumper.accept(randomString.getBytes()));
}
