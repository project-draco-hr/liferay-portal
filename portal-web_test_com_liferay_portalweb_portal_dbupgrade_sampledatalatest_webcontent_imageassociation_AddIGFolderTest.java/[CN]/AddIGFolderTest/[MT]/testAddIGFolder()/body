{
  selenium.open("/web/web-content-image-association-community/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Image Gallery Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Image Gallery Page",RuntimeVariables.replace("Image Gallery Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Add Folder"),selenium.getText("//div[@class='lfr-component lfr-menu-list lfr-menu-expanded align-right null']/ul/li[2]/a"));
  selenium.click(RuntimeVariables.replace("//div[@class='lfr-component lfr-menu-list lfr-menu-expanded align-right null']/ul/li[2]/a"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_31_name']",RuntimeVariables.replace("Folder Test"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@class='portlet-msg-success']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals(RuntimeVariables.replace("Folder Test"),selenium.getText("//a/strong"));
}
