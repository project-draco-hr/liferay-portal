{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(WikiPage.class,WikiPage.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("pageId",RandomTestUtil.nextLong()));
  List<WikiPage> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
