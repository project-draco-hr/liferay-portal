{
  UpgradeColumn entryIdColumn=new TempUpgradeColumnImpl("entryId");
  UpgradeColumn titleColumn=new TempUpgradeColumnImpl("title");
  UpgradeColumn urlTitleColumn=new BlogsEntryUrlTitleUpgradeColumnImpl(entryIdColumn,titleColumn);
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(BlogsEntryImpl.TABLE_NAME,BlogsEntryImpl.TABLE_COLUMNS,entryIdColumn,titleColumn,urlTitleColumn);
  upgradeTable.updateTable();
}
