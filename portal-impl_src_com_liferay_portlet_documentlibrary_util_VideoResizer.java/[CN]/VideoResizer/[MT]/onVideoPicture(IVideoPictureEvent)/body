{
  IVideoPicture pic=event.getPicture();
  if (_videoResampler == null) {
    _videoResampler=IVideoResampler.make(_width,_height,pic.getPixelType(),pic.getWidth(),pic.getHeight(),pic.getPixelType());
  }
  IVideoPicture out=IVideoPicture.make(pic.getPixelType(),_width,_height);
  _videoResampler.resample(out,pic);
  IVideoPictureEvent asc=new VideoPictureEvent(event.getSource(),out,event.getStreamIndex());
  super.onVideoPicture(asc);
  out.delete();
}
