{
  setAlwaysExportable(true);
  setExportControls(new PortletDataHandlerBoolean(NAMESPACE,"events",true,true));
  setExportMetadataControls(new PortletDataHandlerBoolean(NAMESPACE,"events",true,new PortletDataHandlerControl[]{new PortletDataHandlerBoolean(NAMESPACE,"categories"),new PortletDataHandlerBoolean(NAMESPACE,"comments"),new PortletDataHandlerBoolean(NAMESPACE,"ratings"),new PortletDataHandlerBoolean(NAMESPACE,"tags")}));
  setPublishToLiveByDefault(true);
}
