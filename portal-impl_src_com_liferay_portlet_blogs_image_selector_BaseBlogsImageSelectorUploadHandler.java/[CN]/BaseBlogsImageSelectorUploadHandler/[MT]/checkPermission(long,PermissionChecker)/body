{
  boolean containsResourcePermission=ResourcePermissionCheckerUtil.containsResourcePermission(permissionChecker,BlogsPermission.RESOURCE_NAME,groupId,ActionKeys.ADD_ENTRY);
  if (!containsResourcePermission) {
    throw new PrincipalException.MustHavePermission(permissionChecker.getUserId(),BlogsPermission.RESOURCE_NAME,groupId,ActionKeys.ADD_ENTRY);
  }
}
