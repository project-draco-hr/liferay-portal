{
  AppJavadocBuilderExtension appJavadocBuilderExtension=GradleUtil.getExtension(project,AppJavadocBuilderExtension.class);
  appJavadocBuilderExtension.setGroupNameClosure(new Closure<String>(project){
    @SuppressWarnings("unused") public String doCall(    Project subproject){
      return getAppJavadocGroupName(subproject);
    }
  }
);
}
