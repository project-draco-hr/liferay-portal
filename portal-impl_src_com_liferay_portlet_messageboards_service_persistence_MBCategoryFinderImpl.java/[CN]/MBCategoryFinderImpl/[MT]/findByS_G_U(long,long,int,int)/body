{
  Session session=null;
  try {
    session=openSession();
    String sql=CustomSQLUtil.get(FIND_BY_S_G_U);
    SQLQuery q=session.createSQLQuery(sql);
    q.addEntity("MBCategory",MBCategoryImpl.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(PortalUtil.getClassNameId(MBCategory.class.getName()));
    qPos.add(groupId);
    qPos.add(userId);
    return Collections.unmodifiableList((List<MBCategory>)QueryUtil.list(q,getDialect(),start,end));
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
}
