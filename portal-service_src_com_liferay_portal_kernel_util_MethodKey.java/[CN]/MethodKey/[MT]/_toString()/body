{
  if (_toString == null) {
    StringBundler sb=new StringBundler();
    sb.append(_className);
    sb.append(_methodName);
    if ((_types != null) && (_types.length > 0)) {
      sb.append(StringPool.DASH);
      for (      Class<?> type : _types) {
        sb.append(type.getName());
      }
    }
    _toString=sb.toString();
  }
  return _toString;
}
