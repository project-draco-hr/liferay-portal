{
  if (_content != null) {
    return _content;
  }
  String xmlUrl=XSLContentUtil.replaceUrlTokens(_themeDisplay,getXMLUrl());
  String xslUrl=XSLContentUtil.replaceUrlTokens(_themeDisplay,getXSLUrl());
  _content=XSLContentUtil.transform(_xslContentConfiguration,new URL(xmlUrl),new URL(xslUrl));
  return _content;
}
