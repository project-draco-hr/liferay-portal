{
  if (isSupportsAlterColumnName()) {
    runSQL("alter_column_type ResourceCode name VARCHAR(255) null");
  }
 else {
    UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(ResourceCodeImpl.TABLE_NAME,ResourceCodeImpl.TABLE_COLUMNS);
    upgradeTable.setCreateSQL(ResourceCodeImpl.TABLE_SQL_CREATE);
    upgradeTable.updateTable();
  }
}
