{
  try {
    runSQL("alter_column_type ResourceCode name VARCHAR(255) null");
  }
 catch (  Exception e) {
    UpgradeTable upgradeTable=UpgradeTableFactoryUtil.getUpgradeTable(ResourceCodeTable.TABLE_NAME,ResourceCodeTable.TABLE_COLUMNS);
