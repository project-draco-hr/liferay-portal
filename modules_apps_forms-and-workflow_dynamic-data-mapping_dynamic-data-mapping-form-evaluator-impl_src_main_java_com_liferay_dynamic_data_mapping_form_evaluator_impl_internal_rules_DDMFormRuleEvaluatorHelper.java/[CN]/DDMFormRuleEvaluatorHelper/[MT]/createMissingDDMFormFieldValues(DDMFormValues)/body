{
  Map<String,DDMFormField> ddmFormFields=_ddmForm.getDDMFormFieldsMap(true);
  for (  Map.Entry<String,DDMFormField> entry : ddmFormFields.entrySet()) {
    if (!_ddmFormFieldValues.containsKey(entry.getKey())) {
      DDMFormFieldValue ddmFormFieldValue=createDefaultDDMFormFieldValue(entry.getValue(),ddmFormValues);
      _ddmFormFieldValues.put(entry.getKey(),Arrays.asList(ddmFormFieldValue));
    }
  }
}
