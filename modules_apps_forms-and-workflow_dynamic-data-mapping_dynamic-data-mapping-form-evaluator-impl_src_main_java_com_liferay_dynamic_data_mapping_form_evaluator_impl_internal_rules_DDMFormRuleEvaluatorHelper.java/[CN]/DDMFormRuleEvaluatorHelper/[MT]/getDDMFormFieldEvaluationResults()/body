{
  List<DDMFormFieldEvaluationResult> ddmFormFieldEvaluationResults=new ArrayList<>();
  for (  List<DDMFormFieldEvaluationResult> ddmFormFieldEvaluationResultInstances : _ddmFormFieldEvaluationResults.values()) {
    ddmFormFieldEvaluationResults.addAll(ddmFormFieldEvaluationResultInstances);
  }
