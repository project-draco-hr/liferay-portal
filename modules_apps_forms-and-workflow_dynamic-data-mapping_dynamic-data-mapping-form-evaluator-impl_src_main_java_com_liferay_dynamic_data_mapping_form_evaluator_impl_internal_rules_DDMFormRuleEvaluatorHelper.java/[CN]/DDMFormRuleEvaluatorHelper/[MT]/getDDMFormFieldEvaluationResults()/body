{
  List<DDMFormFieldEvaluationResult> ddmFormFieldEvaluationResults=new ArrayList<>();
  List<DDMFormField> ddmFormFields=_ddmForm.getDDMFormFields();
  for (  DDMFormField ddmFormField : ddmFormFields) {
    ddmFormFieldEvaluationResults.addAll(_ddmFormFieldEvaluationResults.get(ddmFormField.getName()));
  }
  return ddmFormFieldEvaluationResults;
}
