{
  for (  DDMFormRule ddmFormRule : _ddmForm.getDDMFormRules()) {
    evaluateDDMFormRule(ddmFormRule);
  }
  DDMFormEvaluationResult ddmFormEvaluationResult=new DDMFormEvaluationResult();
  ddmFormEvaluationResult.setDDMFormFieldEvaluationResults(getDDMFormFieldEvaluationResults());
  return ddmFormEvaluationResult;
}
