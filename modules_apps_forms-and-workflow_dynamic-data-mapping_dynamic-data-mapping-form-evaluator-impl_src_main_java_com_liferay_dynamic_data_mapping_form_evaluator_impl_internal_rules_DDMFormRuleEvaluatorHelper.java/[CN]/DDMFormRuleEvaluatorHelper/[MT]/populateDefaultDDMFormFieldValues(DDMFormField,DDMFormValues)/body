{
  DDMFormFieldValue ddmFormFieldValue=createDefaultDDMFormFieldValue(ddmFormField,ddmFormValues);
  ddmFormValues.addDDMFormFieldValue(ddmFormFieldValue);
  for (  DDMFormField nestedDDMFormField : ddmFormField.getNestedDDMFormFields()) {
    populateDefaultDDMFormFieldValues(nestedDDMFormField,ddmFormValues);
  }
}
