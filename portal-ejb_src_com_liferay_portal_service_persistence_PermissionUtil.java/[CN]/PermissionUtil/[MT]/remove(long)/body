{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(permissionId));
  }
  com.liferay.portal.model.Permission permission=getPersistence().remove(permissionId);
  if (listener != null) {
    listener.onAfterRemove(permission);
  }
  return permission;
}
