{
  try {
    StopWatch stopWatch=new StopWatch();
    stopWatch.start();
    InitUtil.initWithSpringAndModuleFramework();
    upgrade();
    verify();
    System.out.println("\nSuccessfully completed upgrade process in " + (stopWatch.getTime() / Time.SECOND) + " seconds.");
    System.exit(0);
  }
 catch (  Exception e) {
    e.printStackTrace();
    System.exit(1);
  }
}
