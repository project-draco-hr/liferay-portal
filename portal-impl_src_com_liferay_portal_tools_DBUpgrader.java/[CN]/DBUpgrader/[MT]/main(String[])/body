{
  try {
    StopWatch stopWatch=new StopWatch();
    stopWatch.start();
    InitUtil.initWithSpring(true);
    upgrade();
    verify();
    System.out.println("\nCompleted upgrade and verify processes in " + (stopWatch.getTime() / Time.SECOND) + " seconds");
    System.exit(0);
  }
 catch (  Exception e) {
    e.printStackTrace();
    System.exit(1);
  }
}
