{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select state_ from Release_ where releaseId = ?");
    ps.setLong(1,ReleaseConstants.DEFAULT_ID);
    rs=ps.executeQuery();
    if (rs.next()) {
      return rs.getInt("state_");
    }
    throw new IllegalArgumentException("No Release exists with the primary key " + ReleaseConstants.DEFAULT_ID);
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
