{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select count(*) from ResourceCode");
    rs=ps.executeQuery();
    if (rs.next()) {
      long count=rs.getLong(1);
      return count;
    }
    return 0;
  }
 catch (  Exception e) {
    return 0;
  }
 finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
