{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  String variationName=ParamUtil.getString(actionRequest,"variationName");
  long layoutSetBranchId=ParamUtil.getLong(actionRequest,"layoutSetBranchId");
  LayoutRevisionServiceUtil.deleteLayoutRevisions(layoutSetBranchId,themeDisplay.getPlid(),variationName);
}
