{
  InputStream inputStream=null;
  try {
    inputStream=uploadPortletRequest.getFileAsStream(iconFileName);
    if (inputStream != null) {
      return FileUtil.getBytes(inputStream);
    }
  }
 catch (  IOException e) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to retrieve icon",e);
    }
  }
  return new byte[0];
}
