{
  if (Validator.isNull(key) || (key.equals(RANDOM_KEY))) {
    return;
  }
  key=_encodeKey(namespace,key);
  try {
    if (value != null) {
      _prefs.setValue(key,value);
    }
 else {
      _prefs.reset(key);
    }
    if (_signedIn) {
      _prefs.store();
    }
  }
 catch (  Exception e) {
    _log.error(StackTraceUtil.getStackTrace(e));
  }
}
