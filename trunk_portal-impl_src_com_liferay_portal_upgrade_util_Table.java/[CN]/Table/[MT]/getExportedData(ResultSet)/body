{
  StringBuilder sb=new StringBuilder();
  Object[][] columns=getColumns();
  for (int i=0; i < columns.length; i++) {
    boolean last=false;
    if ((i + 1) == columns.length) {
      last=true;
    }
    appendColumn(sb,rs,(String)columns[i][0],(Integer)columns[i][1],last);
  }
  return sb.toString();
}
