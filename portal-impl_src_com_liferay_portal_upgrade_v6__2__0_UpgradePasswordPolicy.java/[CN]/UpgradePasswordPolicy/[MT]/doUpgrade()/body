{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select passwordPolicyId from " + "passwordpolicy where uuid_ is null");
    rs=ps.executeQuery();
    while (rs.next()) {
      long passwordPolicyId=rs.getLong("passwordPolicyId");
      runSQL("update PasswordPolicy set uuid_ = '" + PortalUUIDUtil.generate() + "' where passwordPolicyId = "+ passwordPolicyId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
