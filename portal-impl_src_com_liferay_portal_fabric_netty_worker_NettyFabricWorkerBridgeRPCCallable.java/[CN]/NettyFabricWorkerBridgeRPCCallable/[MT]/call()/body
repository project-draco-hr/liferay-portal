{
  Channel channel=ChannelThreadLocal.getChannel();
  FabricWorker<T> fabricWorker=NettyChannelAttributes.getFabricWorker(channel,_id);
  if (fabricWorker == null) {
    throw new ProcessException("Unable to locate fabric worker with ID " + _id);
  }
  NoticeableFuture<T> noticeableFuture=fabricWorker.write(_processCallable);
  NettyUtil.scheduleCancellation(channel,noticeableFuture,_rpcRelayTimeout);
  return noticeableFuture;
}
