{
  Map<String,Object> errors=null;
  try {
    errors=(Map<String,Object>)portletSession.getAttribute(KEY);
    if (errors == null) {
      errors=new LinkedHashMap<String,Object>();
      portletSession.setAttribute(KEY,errors);
    }
  }
 catch (  IllegalStateException ise) {
    errors=new LinkedHashMap<String,Object>();
  }
  return errors;
}
