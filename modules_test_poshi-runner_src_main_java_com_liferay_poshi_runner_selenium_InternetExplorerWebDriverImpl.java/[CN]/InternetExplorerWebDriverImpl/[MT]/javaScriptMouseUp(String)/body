{
  if (PropsValues.BROWSER_VERSION.equals("10.0")) {
    WebDriverHelper.executeJavaScriptEvent(this,locator,"MSPointerEvent","MSPointerUp");
  }
 else   if (PropsValues.BROWSER_VERSION.equals("11.0")) {
    WebDriverHelper.executeJavaScriptEvent(this,locator,"MouseEvent","pointerup");
  }
 else {
    super.javaScriptMouseUp(locator);
  }
}
