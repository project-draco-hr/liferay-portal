{
  if (PropsValues.BROWSER_VERSION.equals("10.0")) {
    WebDriverHelper.executeJavaScriptEvent(this,locator,"MSPointerEvent","MSPointerDown");
  }
 else   if (PropsValues.BROWSER_VERSION.equals("11.0")) {
    WebDriverHelper.executeJavaScriptEvent(this,locator,"MouseEvent","pointerdown");
  }
 else {
    super.javaScriptMouseDown(locator);
  }
}
