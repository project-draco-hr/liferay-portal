{
  if (!autoFeedId) {
    if ((Validator.isNull(feedId)) || (Validator.isNumber(feedId)) || (feedId.indexOf(StringPool.SPACE) != -1)) {
      throw new SyndicatedFeedIdException();
    }
    try {
      journalSyndicatedFeedPersistence.findByG_F(groupId,feedId);
      throw new DuplicateSyndicatedFeedIdException();
    }
 catch (    NoSuchSyndicatedFeedException nssfe) {
    }
  }
  validate(groupId,name,description,structureId,contentField,targetLayoutFriendlyUrl);
}
