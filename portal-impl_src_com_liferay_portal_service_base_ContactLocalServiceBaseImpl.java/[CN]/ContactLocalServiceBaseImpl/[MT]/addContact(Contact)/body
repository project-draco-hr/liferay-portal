{
  contact.setNew(true);
  contact=contactPersistence.update(contact,false);
  Indexer indexer=IndexerRegistryUtil.getIndexer(getModelClassName());
  if (indexer != null) {
    try {
      indexer.reindex(contact);
    }
 catch (    SearchException se) {
      if (_log.isWarnEnabled()) {
        _log.warn(se,se);
      }
    }
  }
  return contact;
}
