{
  try {
    new FabricAgentRegistry(null);
    Assert.fail();
  }
 catch (  NullPointerException npe) {
    Assert.assertEquals("Default fabric agent is null",npe.getMessage());
  }
  FabricAgent fabricAgent=new LocalFabricAgent(new EmbeddedProcessExecutor());
  FabricAgentRegistry fabricAgentRegistry=new FabricAgentRegistry(fabricAgent);
  Assert.assertSame(fabricAgent,fabricAgentRegistry.getDefaultFabricAgent());
}
