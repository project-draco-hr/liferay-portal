{
  String quote=bbCodeItem.getAttribute();
  if ((quote != null) && (quote.length() > 0)) {
    sb.append("<div class=\"quote-title\">");
    sb.append(_escapeQuote(quote));
    sb.append(":</div>");
  }
  sb.append("<div class=\"quote\"><div class=\"quote-content\">");
  tags.push("</div></div>");
}
