{
  StringMaker sm=new StringMaker();
  sm.append(StringPool.OPEN_CURLY_BRACE);
  sm.append("nodeId");
  sm.append(StringPool.EQUAL);
  sm.append(nodeId);
  sm.append(StringPool.COMMA);
  sm.append(StringPool.SPACE);
  sm.append("title");
  sm.append(StringPool.EQUAL);
  sm.append(title);
  sm.append(StringPool.COMMA);
  sm.append(StringPool.SPACE);
  sm.append("version");
  sm.append(StringPool.EQUAL);
  sm.append(version);
  sm.append(StringPool.CLOSE_CURLY_BRACE);
  return sm.toString();
}
