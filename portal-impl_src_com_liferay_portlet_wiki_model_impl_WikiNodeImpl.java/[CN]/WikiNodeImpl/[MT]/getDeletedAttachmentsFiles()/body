{
  List<WikiPage> wikiPages=WikiPageLocalServiceUtil.getPages(getNodeId(),QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  List<Tuple> deletedNodeAttachments=new ArrayList<Tuple>();
  for (  WikiPage wikiPage : wikiPages) {
    String[] deletedPageAttachments=wikiPage.getDeletedAttachmentsFiles();
    for (    String deletedPageAttachment : deletedPageAttachments) {
      deletedNodeAttachments.add(new Tuple(wikiPage.getResourcePrimKey(),deletedPageAttachment));
    }
  }
  return deletedNodeAttachments;
}
