{
synchronized (this) {
    if (_wsrpPortlets == null || !_isDirty) {
      FileInputStream fis=null;
      try {
        File file=new File(_fileStore);
        if (file.length() == 0) {
          return _objectFactory.createWSRPPortlets();
        }
 else {
          fis=new FileInputStream(_fileStore);
          JAXBElement<WSRPPortlets> rootElement=(JAXBElement<WSRPPortlets>)_unmarshaller.unmarshal(fis);
          _wsrpPortlets=rootElement.getValue();
        }
      }
 catch (      Exception e) {
        throw new WSRPConsumerException("Could not initialize " + "wsrp portlets jaxb",e);
      }
 finally {
        if (fis != null) {
          try {
            fis.close();
          }
 catch (          IOException ex) {
          }
        }
      }
      _isDirty=true;
    }
  }
  return _wsrpPortlets;
}
