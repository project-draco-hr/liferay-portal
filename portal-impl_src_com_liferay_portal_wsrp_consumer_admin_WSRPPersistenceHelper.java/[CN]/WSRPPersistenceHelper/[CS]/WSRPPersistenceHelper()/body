{
  try {
    _jaxbContext=JAXBContext.newInstance("com.liferay.portal.wsrp.consumer.admin",this.getClass().getClassLoader());
    _marshaller=_jaxbContext.createMarshaller();
    _marshaller.setProperty(Marshaller.JAXB_FORMATTED_OUTPUT,Boolean.TRUE);
    _unmarshaller=_jaxbContext.createUnmarshaller();
    _objectFactory=new ObjectFactory();
  }
 catch (  Exception je) {
    _logger.log(Level.SEVERE,je.getMessage(),je);
  }
}
