{
  List<DDMStructure> structures=DDMStructureManagerUtil.getClassStructures(_group.getCompanyId(),_classNameId);
  int initialSize=structures.size();
  addStructure();
  structures=DDMStructureManagerUtil.getClassStructures(_group.getCompanyId(),_classNameId);
  Assert.assertEquals(initialSize + 1,structures.size());
}
