{
  DDMStructure structure=addStructure();
  Document document=new DocumentImpl();
  DDMFormValues ddmFormValues=createDDMFormValues();
  DDMStructureManagerUtil.addAttributes(structure.getStructureId(),document,ddmFormValues);
  String attributes=DDMStructureManagerUtil.extractAttributes(structure.getStructureId(),ddmFormValues,LocaleUtil.US);
  Assert.assertNotNull(attributes);
}
