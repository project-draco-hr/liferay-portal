{
  try {
    addStructure();
    List<DDMStructure> structures=DDMStructureManagerUtil.getClassStructures(_group.getCompanyId(),PortalUtil.getClassNameId("com.liferay.portlet.dynamicdatamapping.model.DDMStructure"),0,1);
    Assert.assertEquals(1,structures.size());
  }
 catch (  PortalException e) {
    Assert.fail("Should not throw exception");
  }
}
