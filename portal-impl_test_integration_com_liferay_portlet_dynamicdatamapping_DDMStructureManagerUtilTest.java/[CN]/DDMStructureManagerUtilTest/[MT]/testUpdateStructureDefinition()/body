{
  DDMStructure expectedStructure=addStructure();
  String definition=expectedStructure.getDefinition();
  definition=definition.replaceAll("(?s)<dynamic-element[^>]*>.*?</dynamic-element>","");
  DDMStructureManagerUtil.updateStructureDefinition(expectedStructure.getStructureId(),definition);
  DDMStructure structure=DDMStructureManagerUtil.getStructure(expectedStructure.getStructureId());
  Assert.assertEquals(definition,structure.getDefinition());
}
