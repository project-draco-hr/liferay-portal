{
  try {
    addStructure();
    DDMStructure structure=DDMStructureManagerUtil.getStructure(_group.getGroupId(),PortalUtil.getClassNameId("com.liferay.portlet.dynamicdatamapping.model.DDMStructure"),"KEY");
    assertStructure(structure);
  }
 catch (  PortalException e) {
    Assert.fail("Should not throw exception");
  }
}
