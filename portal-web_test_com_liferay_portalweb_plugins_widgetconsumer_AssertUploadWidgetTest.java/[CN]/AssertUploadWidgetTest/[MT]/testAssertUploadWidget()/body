{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Widget Consumer Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Widget Consumer Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("24-Mar-2009"));
  assertEquals("25-Mar-2009",selenium.getValue("//input[@value='25-Mar-2009']"));
}
