{
  sb.append("(&");
  Matcher matcher=_versionPrefixRangePattern.matcher(group1);
  matcher.matches();
  appendPrefixRange(sb,matcher.group(1),matcher.group(2));
  matcher=_versionPrefixRangePattern.matcher(group2);
  matcher.matches();
  appendPrefixRange(sb,matcher.group(1),matcher.group(2));
  sb.append(")");
}
