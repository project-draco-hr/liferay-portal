{
  if (type.equals("boolean")) {
    return "BOOLEAN";
  }
 else   if (type.equals("double")) {
    return "DOUBLE";
  }
 else   if (type.equals("float")) {
    return "FLOAT";
  }
 else   if (type.equals("int")) {
    return "INTEGER";
  }
 else   if (type.equals("long")) {
    return "BIGINT";
  }
 else   if (type.equals("short")) {
    return "INTEGER";
  }
 else {
    return null;
  }
}
