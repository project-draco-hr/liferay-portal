{
  if (!DLFolderPermission.contains(getPermissionChecker(),groupId,folderId,ActionKeys.VIEW)) {
    return Collections.emptyList();
  }
  QueryDefinition queryDefinition=new QueryDefinition(status,start,end,obc);
  return dlFolderFinder.filterFindF_FE_FS_ByG_F_M_M(groupId,folderId,null,includeMountFolders,queryDefinition);
}
