{
  PermissionChecker permissionChecker=getPermissionChecker();
  DLFolder dlFolder=dlFolderLocalService.getFolder(folderId);
  DLFolderPermission.check(permissionChecker,dlFolder,ActionKeys.UPDATE);
  DLFolderPermission.check(permissionChecker,serviceContext.getScopeGroupId(),parentFolderId,ActionKeys.ADD_FOLDER);
  return dlFolderLocalService.moveFolder(getUserId(),folderId,parentFolderId,serviceContext);
}
