{
  if (!DLFolderPermission.contains(getPermissionChecker(),groupId,folderId,ActionKeys.VIEW)) {
    return Collections.emptyList();
  }
  @SuppressWarnings("unchecked") QueryDefinition<?> queryDefinition=new QueryDefinition<Object>(status,start,end,(OrderByComparator<Object>)obc);
  return dlFolderFinder.filterFindF_FE_FS_ByG_F_M_M(groupId,folderId,mimeTypes,includeMountFolders,queryDefinition);
}
