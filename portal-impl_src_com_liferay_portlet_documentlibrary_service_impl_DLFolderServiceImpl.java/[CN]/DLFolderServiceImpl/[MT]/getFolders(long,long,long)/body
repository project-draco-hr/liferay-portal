{
  DLFolderPermission.check(getPermissionChecker(),plid,parentFolderId,ActionKeys.VIEW);
  List folders=new ArrayList();
  Iterator itr=dlFolderLocalService.getFolders(groupId,parentFolderId).iterator();
  while (itr.hasNext()) {
    DLFolder folder=(DLFolder)itr.next();
    if (DLFolderPermission.contains(getPermissionChecker(),folder,ActionKeys.VIEW)) {
      folders.add(folder);
    }
  }
  return folders;
}
