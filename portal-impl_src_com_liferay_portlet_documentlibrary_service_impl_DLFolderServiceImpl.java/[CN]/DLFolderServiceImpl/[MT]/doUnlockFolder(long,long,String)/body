{
  if (Validator.isNotNull(lockUuid)) {
    try {
      Lock lock=lockLocalService.getLock(DLFolder.class.getName(),folderId);
      if (!lockUuid.equals(lock.getUuid())) {
        throw new InvalidLockException("UUIDs do not match");
      }
    }
 catch (    PortalException pe) {
      if (pe instanceof ExpiredLockException || pe instanceof NoSuchLockException) {
      }
 else {
        throw pe;
      }
    }
  }
  lockLocalService.unlock(DLFolder.class.getName(),folderId);
  try {
    List<DLFileEntry> dlFileEntries=dlFileEntryLocalService.getFileEntries(groupId,folderId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
    for (    DLFileEntry dlFileEntry : dlFileEntries) {
      dlFileEntryService.unlockFileEntry(dlFileEntry.getFileEntryId());
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
