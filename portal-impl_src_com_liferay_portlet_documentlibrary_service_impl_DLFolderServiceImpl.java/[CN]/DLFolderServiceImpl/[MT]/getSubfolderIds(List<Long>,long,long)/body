{
  DLFolderPermission.check(getPermissionChecker(),groupId,folderId,ActionKeys.VIEW);
  List<DLFolder> dlFolders=dlFolderPersistence.filterFindByG_P(groupId,folderId);
  for (  DLFolder dlFolder : dlFolders) {
    folderIds.add(dlFolder.getFolderId());
    getSubfolderIds(folderIds,dlFolder.getGroupId(),dlFolder.getFolderId());
  }
}
