{
  if (!DLFolderPermission.contains(getPermissionChecker(),groupId,parentFolderId,ActionKeys.VIEW)) {
    return Collections.emptyList();
  }
  DLSettings dlSettings=DLSettings.getInstance(groupId);
  boolean showHiddenMountFolders=dlSettings.isShowHiddenMountFolders();
  return dlFolderPersistence.filterFindByG_M_P_H(groupId,true,parentFolderId,showHiddenMountFolders,start,end,obc);
}
