{
  List<DLFolder> folders=dlFolderLocalService.getFolders(groupId,parentFolderId);
  folders=ListUtil.copy(folders);
  Iterator<DLFolder> itr=folders.iterator();
  while (itr.hasNext()) {
    DLFolder folder=itr.next();
    if (!DLFolderPermission.contains(getPermissionChecker(),folder.getFolderId(),ActionKeys.VIEW)) {
      itr.remove();
    }
  }
  return folders;
}
