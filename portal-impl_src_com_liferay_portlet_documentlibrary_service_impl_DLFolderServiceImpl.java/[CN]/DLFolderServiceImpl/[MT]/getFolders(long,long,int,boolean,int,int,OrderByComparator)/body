{
  DLFolderPermission.check(getPermissionChecker(),groupId,parentFolderId,ActionKeys.VIEW);
  if (includeMountfolders) {
    return dlFolderPersistence.filterFindByG_P_S(groupId,parentFolderId,status,start,end,obc);
  }
 else {
    return dlFolderPersistence.filterFindByG_M_P_S(groupId,false,parentFolderId,status,start,end,obc);
  }
}
