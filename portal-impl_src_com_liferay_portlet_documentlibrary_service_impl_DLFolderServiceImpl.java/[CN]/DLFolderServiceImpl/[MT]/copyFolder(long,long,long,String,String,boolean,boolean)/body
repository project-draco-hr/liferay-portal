{
  DLFolderPermission.check(getPermissionChecker(),sourceFolderId,ActionKeys.VIEW);
  DLFolder srcFolder=getFolder(sourceFolderId);
  DLFolderPermission.check(getPermissionChecker(),plid,parentFolderId,ActionKeys.ADD_FOLDER);
  DLFolder destFolder=addFolder(plid,parentFolderId,name,description,addCommunityPermissions,addGuestPermissions);
  copyFolder(srcFolder,destFolder,addCommunityPermissions,addGuestPermissions);
  return destFolder;
}
