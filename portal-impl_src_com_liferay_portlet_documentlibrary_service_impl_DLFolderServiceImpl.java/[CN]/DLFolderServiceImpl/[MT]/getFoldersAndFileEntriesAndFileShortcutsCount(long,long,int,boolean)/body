{
  if (!DLFolderPermission.contains(getPermissionChecker(),groupId,folderId,ActionKeys.VIEW)) {
    return 0;
  }
  QueryDefinition<?> queryDefinition=new QueryDefinition<Object>(status);
  return dlFolderFinder.filterCountF_FE_FS_ByG_F_M_M(groupId,folderId,null,includeMountFolders,queryDefinition);
}
