{
  if (primary) {
    Iterator itr=PhoneUtil.findByC_C_C_P(companyId,className,classPK,primary).iterator();
    while (itr.hasNext()) {
      Phone phone=(Phone)itr.next();
      if ((phoneId == null) || (!phone.getPhoneId().equals(phoneId))) {
        phone.setPrimary(false);
        PhoneUtil.update(phone);
      }
    }
  }
}
