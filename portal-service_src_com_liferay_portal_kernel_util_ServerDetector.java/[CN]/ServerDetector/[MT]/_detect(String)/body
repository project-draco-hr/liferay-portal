{
  try {
    ClassLoader systemClassLoader=ClassLoader.getSystemClassLoader();
    systemClassLoader.loadClass(className);
    return true;
  }
 catch (  ClassNotFoundException cnfe) {
    Class<?> classObj=getClass();
    if (classObj.getResource(className) != null) {
      return true;
    }
 else {
      return false;
    }
  }
}
