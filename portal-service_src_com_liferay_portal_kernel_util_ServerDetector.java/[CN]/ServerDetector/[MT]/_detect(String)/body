{
  try {
    ClassLoader.getSystemClassLoader().loadClass(className);
    return Boolean.TRUE;
  }
 catch (  ClassNotFoundException cnfe) {
    Class<?> classObj=_instance.getClass();
    if (classObj.getResource(className) != null) {
      return Boolean.TRUE;
    }
 else {
      return Boolean.FALSE;
    }
  }
}
