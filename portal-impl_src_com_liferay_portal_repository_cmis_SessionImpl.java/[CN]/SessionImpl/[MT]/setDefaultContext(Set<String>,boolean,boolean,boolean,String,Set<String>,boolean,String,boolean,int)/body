{
  IncludeRelationships includeRelationshipsObj=null;
  if (includeRelationships != null) {
    includeRelationshipsObj=IncludeRelationships.fromValue(includeRelationships);
  }
  _session.setDefaultContext(new OperationContextImpl(filter,includeAcls,includeAllowableActions,includePolicies,includeRelationshipsObj,renditionFilter,includePathSegments,orderBy,cacheEnabled,maxItemsPerPage));
}
