{
  Assert.assertTrue(MBCategoryPermission.contains(permissionChecker,_category,ActionKeys.VIEW));
  Assert.assertTrue(MBCategoryPermission.contains(permissionChecker,_subcategory,ActionKeys.VIEW));
  removePortletModelViewPermission(ResourceConstants.SCOPE_GROUP);
  Assert.assertFalse(MBCategoryPermission.contains(permissionChecker,_category,ActionKeys.VIEW));
  Assert.assertFalse(MBCategoryPermission.contains(permissionChecker,_subcategory,ActionKeys.VIEW));
}
