{
  _group=GroupTestUtil.addGroup();
  _category=MBTestUtil.addCategory(_group.getGroupId());
  _subcategory=MBTestUtil.addCategory(_group.getGroupId(),_category.getCategoryId());
  RoleTestUtil.addResourcePermission(RoleConstants.POWER_USER,MBPermission.RESOURCE_NAME,ResourceConstants.SCOPE_GROUP,String.valueOf(_group.getGroupId()),ActionKeys.VIEW);
}
