{
  StringTokenizer st=new StringTokenizer(version,_SEPARATOR);
  _major=st.nextToken();
  if (st.hasMoreTokens()) {
    _minor=st.nextToken();
  }
  if (st.hasMoreTokens()) {
    _bugFix=st.nextToken();
  }
  StringBuilder sb=new StringBuilder();
  while (st.hasMoreTokens()) {
    sb.append(st.nextToken());
    if (st.hasMoreTokens()) {
      sb.append(_SEPARATOR);
    }
  }
  _buildNumber=sb.toString();
}
