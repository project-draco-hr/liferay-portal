{
  StringMaker sm=new StringMaker();
  sm.append(_major);
  if (Validator.isNotNull(_minor)) {
    sm.append(_SEPARATOR);
    sm.append(_minor);
    if (Validator.isNotNull(_bugFix)) {
      sm.append(_SEPARATOR);
      sm.append(_bugFix);
      if (Validator.isNotNull(_buildNumber)) {
        sm.append(_SEPARATOR);
        sm.append(_buildNumber);
      }
    }
  }
  return sm.toString();
}
