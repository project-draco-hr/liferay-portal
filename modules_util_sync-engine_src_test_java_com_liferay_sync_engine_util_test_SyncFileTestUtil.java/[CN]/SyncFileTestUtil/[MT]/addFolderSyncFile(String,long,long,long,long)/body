{
  Files.createDirectory(Paths.get(filePathName));
  SyncFile syncFile=SyncFileService.addSyncFile(null,null,null,filePathName,null,null,parentFolderId,repositoryId,SyncFile.STATE_SYNCED,0,syncAccountId,SyncFile.TYPE_FOLDER,false);
  if (typePK == 0) {
    syncFile.setTypePK(syncFile.getSyncFileId());
    SyncFileService.update(syncFile);
  }
  return syncFile;
}
