{
  URL url=bundle.getResource("/WEB-INF/liferay-plugin-package.properties");
  InputStream inputStream=url.openStream();
  Properties liferayPluginPackageProperties=new Properties();
  liferayPluginPackageProperties.load(inputStream);
  PluginPackage pluginPackage=PluginPackageUtil.readPluginPackageProperties(bundle.getSymbolicName() + "-web",liferayPluginPackageProperties);
  pluginPackage.setContext(bundle.getSymbolicName());
  return pluginPackage;
}
