{
  List<KaleoTaskAssignmentInstance> kaleoTaskAssignmentInstances=kaleoTaskAssignmentInstancePersistence.findBykaleoTaskInstanceTokenId(kaleoTaskInstanceToken.getKaleoTaskInstanceTokenId());
  for (  KaleoTaskAssignmentInstance kaleoTaskAssignmentInstance : kaleoTaskAssignmentInstances) {
    kaleoTaskAssignmentInstancePersistence.remove(kaleoTaskAssignmentInstance);
  }
}
