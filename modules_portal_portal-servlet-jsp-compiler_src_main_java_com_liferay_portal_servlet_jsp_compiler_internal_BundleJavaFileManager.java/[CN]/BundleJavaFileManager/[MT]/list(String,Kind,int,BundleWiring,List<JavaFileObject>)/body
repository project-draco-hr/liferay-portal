{
  Collection<String> resources=_resourceResolver.resolveResources(bundleWiring,packagePath,StringPool.STAR.concat(kind.extension),options);
  if ((resources == null) || resources.isEmpty()) {
    return;
  }
  for (  String resourceName : resources) {
    URL resourceURL=_resourceResolver.getResource(bundleWiring,resourceName);
    JavaFileObject javaFileObject=getJavaFileObject(resourceURL,resourceName);
    if (javaFileObject == null) {
      if (_verbose) {
        _logger.log(Logger.LOG_INFO,"Could not create JavaFileObject for " + resourceURL);
      }
      continue;
    }
    if (_verbose) {
      _logger.log(Logger.LOG_INFO,"Created " + javaFileObject);
    }
    javaFileObjects.add(javaFileObject);
  }
}
