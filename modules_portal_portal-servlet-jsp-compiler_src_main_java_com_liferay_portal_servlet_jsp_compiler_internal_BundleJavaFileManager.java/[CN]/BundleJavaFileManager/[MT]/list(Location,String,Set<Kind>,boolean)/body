{
  if ((location == StandardLocation.CLASS_PATH) && _verbose) {
    StringBundler sb=new StringBundler(8);
    sb.append("List for {location=");
    sb.append(location);
    sb.append(", packageName=");
    sb.append(packageName);
    sb.append(", kinds=");
    sb.append(kinds);
    sb.append(", recurse=");
    sb.append(recurse);
    sb.append(StringPool.CLOSE_CURLY_BRACE);
    _logger.log(Logger.LOG_INFO,sb.toString());
  }
  String packagePath=packageName.replace(CharPool.PERIOD,CharPool.SLASH);
  if (!packageName.startsWith(JAVA_PACKAGE) && (location == StandardLocation.CLASS_PATH)) {
    List<JavaFileObject> javaFileObjects=listFromDependencies(kinds,recurse,packagePath);
    if (!javaFileObjects.isEmpty() || !_systemPackageNames.contains(packageName)) {
      return javaFileObjects;
    }
  }
  return fileManager.list(location,packagePath,kinds,recurse);
}
