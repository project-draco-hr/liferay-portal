{
  Collection<String> resources=_classResolver.resolveClasses(bundleWiring,packagePath,options);
  if ((resources == null) || resources.isEmpty()) {
    return;
  }
  for (  String resourceName : resources) {
    URL resourceURL=_classResolver.getClassURL(bundleWiring,resourceName);
    JavaFileObject javaFileObject=getJavaFileObject(resourceURL,resourceName);
    if (javaFileObject == null) {
      if (_verbose) {
        _logger.log(Logger.LOG_INFO,"Unable to create Java file object for " + resourceURL);
      }
      continue;
    }
    if (_verbose) {
      _logger.log(Logger.LOG_INFO,"Created " + javaFileObject);
    }
    javaFileObjects.add(javaFileObject);
  }
}
