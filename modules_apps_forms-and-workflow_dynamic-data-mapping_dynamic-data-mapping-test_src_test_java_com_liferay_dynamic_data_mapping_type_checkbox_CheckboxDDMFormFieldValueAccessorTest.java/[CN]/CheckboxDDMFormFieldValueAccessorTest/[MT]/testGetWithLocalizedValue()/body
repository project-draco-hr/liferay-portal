{
  Value value=new LocalizedValue(LocaleUtil.US);
  value.addString(LocaleUtil.BRAZIL,"true");
  value.addString(LocaleUtil.US,"false");
  DDMFormFieldValue ddmFormFieldValue=DDMFormValuesTestUtil.createDDMFormFieldValue("Checkbox",value);
  CheckboxDDMFormFieldValueAccessor checkboxDDMFormFieldValueAccessor=new CheckboxDDMFormFieldValueAccessor();
  Assert.assertEquals(Boolean.TRUE,checkboxDDMFormFieldValueAccessor.getValue(ddmFormFieldValue,LocaleUtil.BRAZIL));
  Assert.assertEquals(Boolean.FALSE,checkboxDDMFormFieldValueAccessor.getValue(ddmFormFieldValue,LocaleUtil.US));
}
