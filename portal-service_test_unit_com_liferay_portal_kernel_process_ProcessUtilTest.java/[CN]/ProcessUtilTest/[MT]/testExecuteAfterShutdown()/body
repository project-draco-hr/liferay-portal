{
  ExecutorService executorService=_invokeGetExecutorService();
  executorService.shutdown();
  try {
    ProcessUtil.execute(ProcessUtil.LOGGING_OUTPUT_PROCESSOR,_buildArguments(Echo.class,"2"));
    fail();
  }
 catch (  ProcessException pe) {
    Throwable throwable=pe.getCause();
    assertEquals(RejectedExecutionException.class,throwable.getClass());
  }
}
