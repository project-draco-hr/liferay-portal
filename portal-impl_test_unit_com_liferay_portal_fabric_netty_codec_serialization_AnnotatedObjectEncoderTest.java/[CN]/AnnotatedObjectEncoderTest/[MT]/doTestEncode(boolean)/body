{
  AnnotatedObjectEncoder annotatedObjectEncoder=AnnotatedObjectEncoder.INSTANCE;
  Date date=new Date();
  ByteBuf byteBuf=Unpooled.buffer();
  if (bridge) {
    ReflectionTestUtil.invokeBridge(annotatedObjectEncoder,"encode",new Class<?>[]{ChannelHandlerContext.class,Object.class,ByteBuf.class},null,date,byteBuf);
  }
 else {
    annotatedObjectEncoder.encode(null,date,byteBuf);
  }
  Assert.assertEquals(byteBuf.readInt(),byteBuf.readableBytes());
  AnnotatedObjectInputStream annotatedObjectInputStream=new AnnotatedObjectInputStream(new ByteBufInputStream(byteBuf));
  Assert.assertEquals(date,annotatedObjectInputStream.readObject());
  Assert.assertFalse(byteBuf.isReadable());
}
