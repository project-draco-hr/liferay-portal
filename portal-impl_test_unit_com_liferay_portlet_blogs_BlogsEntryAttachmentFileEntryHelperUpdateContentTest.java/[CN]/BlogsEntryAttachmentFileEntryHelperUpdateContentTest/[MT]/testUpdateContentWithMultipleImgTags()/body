{
  String content=_blogsEntryAttachmentFileEntryHelper.updateContent(populateContentWithMultipleImgTags(_tempFileEntryImgTag),_blogsEntryAttachmentFileEntryReferences);
  String expectedContent=populateContentWithMultipleImgTags("<img src=\"" + _FILE_ENTRY_IMAGE_URL + "\" />");
