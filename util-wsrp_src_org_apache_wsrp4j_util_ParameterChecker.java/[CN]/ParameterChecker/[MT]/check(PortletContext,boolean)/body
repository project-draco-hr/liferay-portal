{
  if (isCheckEnabled()) {
    if (portletContext != null) {
      if (portletContext.getPortletHandle() == null) {
        throwMissingParametersFault("PortletHandle in PortletContext is null");
      }
    }
 else {
      if (nillable == false) {
        throwMissingParametersFault("PortletContext object is null");
      }
    }
  }
}
