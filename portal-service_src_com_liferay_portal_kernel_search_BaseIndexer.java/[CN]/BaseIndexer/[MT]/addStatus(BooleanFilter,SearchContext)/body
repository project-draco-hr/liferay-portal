{
  int status=GetterUtil.getInteger(searchContext.getAttribute(Field.STATUS),WorkflowConstants.STATUS_APPROVED);
  if (status != WorkflowConstants.STATUS_ANY) {
    contextFilter.addRequiredTerm(Field.STATUS,status);
  }
 else {
    contextFilter.addTerm(Field.STATUS,String.valueOf(WorkflowConstants.STATUS_IN_TRASH),BooleanClauseOccur.MUST_NOT);
  }
}
