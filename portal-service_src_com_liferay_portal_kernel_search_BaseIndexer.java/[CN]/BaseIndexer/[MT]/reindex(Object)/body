{
  try {
    if (SearchEngineUtil.isIndexReadOnly() || !isIndexerEnabled()) {
      return;
    }
    if (obj instanceof List<?>) {
      List<?> objects=(List<?>)obj;
      for (      Object curObj : objects) {
        doReindex(curObj);
      }
    }
 else {
      doReindex(obj);
    }
  }
 catch (  SearchException se) {
    throw se;
  }
catch (  Exception e) {
    throw new SearchException(e);
  }
}
