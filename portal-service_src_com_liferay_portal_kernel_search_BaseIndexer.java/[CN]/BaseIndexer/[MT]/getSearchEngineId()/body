{
  if (_searchEngineId != null) {
    return _searchEngineId;
  }
  Class<?> clazz=getClass();
  String searchEngineId=GetterUtil.getString(PropsUtil.get(PropsKeys.INDEX_SEARCH_ENGINE_ID,new com.liferay.portal.kernel.configuration.Filter(clazz.getName())));
  if (Validator.isNotNull(searchEngineId)) {
    SearchEngine searchEngine=SearchEngineHelperUtil.getSearchEngine(searchEngineId);
    if (searchEngine != null) {
      _searchEngineId=searchEngineId;
    }
  }
  if (_searchEngineId == null) {
    _searchEngineId=SearchEngineHelperUtil.getDefaultSearchEngineId();
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Search engine ID for " + clazz.getName() + " is "+ searchEngineId);
  }
  return _searchEngineId;
}
