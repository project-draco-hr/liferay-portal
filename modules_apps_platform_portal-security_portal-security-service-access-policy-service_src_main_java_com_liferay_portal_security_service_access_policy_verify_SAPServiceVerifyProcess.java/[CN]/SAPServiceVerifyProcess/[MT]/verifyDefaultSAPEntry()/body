{
  List<Company> companies=_companyLocalService.getCompanies();
  for (  Company company : companies) {
    try {
      _sapEntryLocalService.checkSystemSAPEntries(company.getCompanyId());
    }
 catch (    PortalException pe) {
      _log.error("Unable to add default service access policy for company " + company.getCompanyId(),pe);
    }
  }
}
