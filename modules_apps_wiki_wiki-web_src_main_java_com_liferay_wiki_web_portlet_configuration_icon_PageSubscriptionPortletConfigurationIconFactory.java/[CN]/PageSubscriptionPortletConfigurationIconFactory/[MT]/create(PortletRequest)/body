{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    WikiNode node=ActionUtil.getNode(portletRequest);
    WikiPage page=ActionUtil.getPage(portletRequest);
    boolean subscribed=_subscriptionLocalService.isSubscribed(themeDisplay.getCompanyId(),themeDisplay.getUserId(),WikiNode.class.getName(),node.getNodeId());
    return new PageSubscriptionPortletConfigurationIcon(portletRequest,node,page,subscribed);
  }
 catch (  Exception e) {
  }
  return null;
}
