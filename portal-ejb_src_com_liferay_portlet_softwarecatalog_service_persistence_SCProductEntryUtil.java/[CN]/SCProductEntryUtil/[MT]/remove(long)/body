{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(productEntryId));
  }
  com.liferay.portlet.softwarecatalog.model.SCProductEntry scProductEntry=getPersistence().remove(productEntryId);
  if (listener != null) {
    listener.onAfterRemove(scProductEntry);
  }
  return scProductEntry;
}
