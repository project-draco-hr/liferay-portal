{
  String[] classPathStringElements=StringUtil.split(classPath,File.pathSeparatorChar);
  PathHolder[] classPathElements=new PathHolder[classPathStringElements.length];
  for (int i=0; i < classPathStringElements.length; i++) {
    classPathElements[i]=new PathHolder(classPathStringElements[i]);
  }
  return classPathElements;
}
