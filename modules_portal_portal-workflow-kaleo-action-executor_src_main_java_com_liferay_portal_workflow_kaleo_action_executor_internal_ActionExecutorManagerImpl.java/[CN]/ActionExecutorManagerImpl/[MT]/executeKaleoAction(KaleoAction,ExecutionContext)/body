{
  String actionExecutorKey=getActionExecutorKey(kaleoAction.getScriptLanguage(),StringUtil.trim(kaleoAction.getScript()));
  ActionExecutor actionExecutor=_actionExecutors.get(actionExecutorKey);
  if (actionExecutor == null) {
    throw new PortalException("No action executor for " + actionExecutorKey);
  }
  actionExecutor.execute(kaleoAction,executionContext);
}
