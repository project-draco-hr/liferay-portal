{
  Object languageProperty=properties.get("com.liferay.portal.workflow.kaleo.action.executor.language");
  String[] defaultValue=new String[]{String.valueOf(languageProperty)};
  String[] languages=GetterUtil.getStringValues(languageProperty,defaultValue);
  for (  String language : languages) {
    String actionExecutorKey=getActionExecutorKey(language,ClassUtil.getClassName(actionExecutor));
    _actionExecutors.put(actionExecutorKey,actionExecutor);
  }
}
