{
  User user;
  if (obj instanceof User) {
    user=(User)obj;
  }
 else   if (obj instanceof Object[]) {
    user=(User)((Object[])obj)[0];
  }
 else {
    throw new IllegalArgumentException(obj + " is not a User");
  }
  try {
    return UserLocalServiceUtil.hasGroupUser(_group.getGroupId(),user.getUserId());
  }
 catch (  Exception e) {
    _log.error(e);
    return false;
  }
}
