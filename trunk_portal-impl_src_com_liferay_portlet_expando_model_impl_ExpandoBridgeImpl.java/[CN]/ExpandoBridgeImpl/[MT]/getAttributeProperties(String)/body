{
  try {
    ExpandoColumn column=ExpandoColumnLocalServiceUtil.getDefaultTableColumn(_companyId,_className,name);
    return column.getTypeSettingsProperties();
  }
 catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      _log.debug("Properties for " + name,e);
    }
    return new UnicodeProperties(true);
  }
}
