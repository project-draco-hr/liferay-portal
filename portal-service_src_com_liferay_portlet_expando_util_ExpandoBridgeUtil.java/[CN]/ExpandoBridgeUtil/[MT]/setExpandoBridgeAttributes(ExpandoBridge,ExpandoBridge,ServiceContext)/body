{
  Map<String,Serializable> newExpandoBridgeAttributes=oldExpandoBridge.getAttributes(false);
  Map<String,Serializable> newAttributes=serviceContext.getExpandoBridgeAttributes();
  for (  String key : newAttributes.keySet()) {
    newExpandoBridgeAttributes.put(key,newAttributes.get(key));
  }
  newExpandoBridge.setAttributes(newExpandoBridgeAttributes,false);
}
