{
  for (  Runnable runnable : _getShutdownHooks()) {
    runnable.run();
  }
synchronized (ProjectDataUtil.class.getName().intern()) {
    FileLock fileLock=_lockFile(lockFile);
    try {
      if (dataFile.exists()) {
        return _readProjectData(dataFile);
      }
 else {
        return new ProjectData();
      }
    }
  finally {
      _unlockFile(fileLock);
    }
  }
}
