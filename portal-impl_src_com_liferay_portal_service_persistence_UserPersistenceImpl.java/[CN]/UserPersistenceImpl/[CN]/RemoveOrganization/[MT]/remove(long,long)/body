{
  if (_persistenceImpl.containsOrganization.contains(userId,organizationId)) {
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onBeforeRemoveAssociation(userId,com.liferay.portal.model.Organization.class.getName(),organizationId);
      }
    }
    _sqlUpdate.update(new Object[]{new Long(userId),new Long(organizationId)});
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onAfterRemoveAssociation(userId,com.liferay.portal.model.Organization.class.getName(),organizationId);
      }
    }
  }
}
