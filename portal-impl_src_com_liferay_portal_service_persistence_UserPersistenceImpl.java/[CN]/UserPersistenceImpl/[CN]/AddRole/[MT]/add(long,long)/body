{
  if (!_persistenceImpl.containsRole.contains(userId,roleId)) {
    ModelListener<com.liferay.portal.model.Role>[] roleListeners=rolePersistence.getListeners();
    for (    ModelListener<User> listener : listeners) {
      listener.onBeforeAddAssociation(userId,com.liferay.portal.model.Role.class.getName(),roleId);
    }
    for (    ModelListener<com.liferay.portal.model.Role> listener : roleListeners) {
      listener.onBeforeAddAssociation(roleId,User.class.getName(),userId);
    }
    _sqlUpdate.update(new Object[]{new Long(userId),new Long(roleId)});
    for (    ModelListener<User> listener : listeners) {
      listener.onAfterAddAssociation(userId,com.liferay.portal.model.Role.class.getName(),roleId);
    }
    for (    ModelListener<com.liferay.portal.model.Role> listener : roleListeners) {
      listener.onAfterAddAssociation(roleId,User.class.getName(),userId);
    }
  }
}
