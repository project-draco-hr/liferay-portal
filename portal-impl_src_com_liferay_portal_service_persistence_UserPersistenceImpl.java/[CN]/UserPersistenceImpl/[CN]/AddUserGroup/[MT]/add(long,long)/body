{
  if (!_persistenceImpl.containsUserGroup.contains(userId,userGroupId)) {
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onBeforeAddAssociation(userId,com.liferay.portal.model.UserGroup.class.getName(),userGroupId);
      }
    }
    _sqlUpdate.update(new Object[]{new Long(userId),new Long(userGroupId)});
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onAfterAddAssociation(userId,com.liferay.portal.model.UserGroup.class.getName(),userGroupId);
      }
    }
  }
}
