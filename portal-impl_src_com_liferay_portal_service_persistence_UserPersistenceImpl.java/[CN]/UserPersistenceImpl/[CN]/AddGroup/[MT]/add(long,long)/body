{
  if (!_persistenceImpl.containsGroup.contains(userId,groupId)) {
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onBeforeAddAssociation(userId,com.liferay.portal.model.Group.class.getName(),groupId);
      }
    }
    _sqlUpdate.update(new Object[]{new Long(userId),new Long(groupId)});
    if (_listeners.length > 0) {
      for (      ModelListener listener : _listeners) {
        listener.onAfterAddAssociation(userId,com.liferay.portal.model.Group.class.getName(),groupId);
      }
    }
  }
}
