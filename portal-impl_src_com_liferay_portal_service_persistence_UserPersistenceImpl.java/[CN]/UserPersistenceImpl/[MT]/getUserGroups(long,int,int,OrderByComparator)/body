{
  Object[] finderArgs=new Object[]{new Long(pk),String.valueOf(start),String.valueOf(end),String.valueOf(obc)};
  List<com.liferay.portal.model.UserGroup> list=(List<com.liferay.portal.model.UserGroup>)FinderCacheUtil.getResult(FINDER_PATH_GET_USERGROUPS,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBundler query=null;
      String sql=null;
      if (obc != null) {
        query=new StringBundler(3);
        query.append(_SQL_GETUSERGROUPS);
        query.append(ORDER_BY_CLAUSE);
        query.append(obc.getOrderBy());
        sql=query.toString();
      }
 else {
        sql=_SQL_GETUSERGROUPS.concat(com.liferay.portal.model.impl.UserGroupModelImpl.ORDER_BY_SQL);
      }
      SQLQuery q=session.createSQLQuery(sql);
      q.addEntity("UserGroup",com.liferay.portal.model.impl.UserGroupImpl.class);
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(pk);
      list=(List<com.liferay.portal.model.UserGroup>)QueryUtil.list(q,getDialect(),start,end);
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<com.liferay.portal.model.UserGroup>();
      }
      userGroupPersistence.cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_GET_USERGROUPS,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
