{
  User result=(User)EntityCacheUtil.getResult(UserModelImpl.ENTITY_CACHE_ENABLED,User.class,userId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      User user=(User)session.get(UserImpl.class,new Long(userId));
      if (user != null) {
        cacheResult(user);
      }
      return user;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (User)result;
  }
}
