{
  User user=(User)EntityCacheUtil.getResult(UserModelImpl.ENTITY_CACHE_ENABLED,UserImpl.class,userId);
  if (user == _nullUser) {
    return null;
  }
  if (user == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      user=(User)session.get(UserImpl.class,Long.valueOf(userId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (user != null) {
        cacheResult(user);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(UserModelImpl.ENTITY_CACHE_ENABLED,UserImpl.class,userId,_nullUser);
      }
      closeSession(session);
    }
  }
  return user;
}
