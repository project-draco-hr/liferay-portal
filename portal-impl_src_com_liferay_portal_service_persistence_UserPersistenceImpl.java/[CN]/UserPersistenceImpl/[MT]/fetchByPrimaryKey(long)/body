{
  User user=(User)EntityCacheUtil.getResult(UserModelImpl.ENTITY_CACHE_ENABLED,UserImpl.class,userId);
  if (user == _nullUser) {
    return null;
  }
  if (user == null) {
    Session session=null;
    try {
      session=openSession();
      user=(User)session.get(UserImpl.class,Long.valueOf(userId));
      if (user != null) {
        cacheResult(user);
      }
 else {
        EntityCacheUtil.putResult(UserModelImpl.ENTITY_CACHE_ENABLED,UserImpl.class,userId,_nullUser);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(UserModelImpl.ENTITY_CACHE_ENABLED,UserImpl.class,userId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return user;
}
