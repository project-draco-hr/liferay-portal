{
  try {
    for (    com.liferay.portal.model.Organization organization : organizations) {
      addOrganization.add(pk,organization.getPrimaryKey());
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    FinderCacheUtil.clearCache(UserModelImpl.TABLE_USERS_ORGS);
  }
}
