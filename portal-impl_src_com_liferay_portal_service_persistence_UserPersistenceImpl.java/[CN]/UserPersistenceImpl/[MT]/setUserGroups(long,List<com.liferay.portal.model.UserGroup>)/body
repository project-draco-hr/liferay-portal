{
  try {
    clearUserGroups.clear(pk);
    for (    com.liferay.portal.model.UserGroup userGroup : userGroups) {
      addUserGroup.add(pk,userGroup.getPrimaryKey());
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    FinderCacheUtil.clearCache("Users_UserGroups");
  }
}
