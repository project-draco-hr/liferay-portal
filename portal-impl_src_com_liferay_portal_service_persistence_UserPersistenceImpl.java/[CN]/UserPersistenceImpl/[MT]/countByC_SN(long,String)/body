{
  Object[] finderArgs=new Object[]{new Long(companyId),screenName};
  Long count=(Long)FinderCacheUtil.getResult(FINDER_PATH_COUNT_BY_C_SN,finderArgs,this);
  if (count == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT COUNT(user) ");
      query.append("FROM User user WHERE ");
      query.append("user.companyId = ?");
      query.append(" AND ");
      if (screenName == null) {
        query.append("user.screenName IS NULL");
      }
 else {
        if (screenName.equals(StringPool.BLANK)) {
          query.append("(user.screenName IS NULL OR ");
        }
        query.append("user.screenName = ?");
        if (screenName.equals(StringPool.BLANK)) {
          query.append(")");
        }
      }
      query.append(" ");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(companyId);
      if (screenName != null) {
        qPos.add(screenName);
      }
      count=(Long)q.uniqueResult();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (count == null) {
        count=Long.valueOf(0);
      }
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_C_SN,finderArgs,count);
      closeSession(session);
    }
  }
  return count.intValue();
}
