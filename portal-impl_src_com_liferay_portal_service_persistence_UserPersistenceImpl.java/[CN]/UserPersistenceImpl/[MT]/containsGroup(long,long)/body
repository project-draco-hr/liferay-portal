{
  Object[] finderArgs=new Object[]{pk,groupPK};
  Boolean value=(Boolean)FinderCacheUtil.getResult(FINDER_PATH_CONTAINS_GROUP,finderArgs,this);
  if (value == null) {
    try {
      value=Boolean.valueOf(containsGroup.contains(pk,groupPK));
      FinderCacheUtil.putResult(FINDER_PATH_CONTAINS_GROUP,finderArgs,value);
    }
 catch (    Exception e) {
      FinderCacheUtil.removeResult(FINDER_PATH_CONTAINS_GROUP,finderArgs);
      throw processException(e);
    }
  }
  return value.booleanValue();
}
