{
  while (!_appStarted && !_isError) {
    try {
      wait();
    }
 catch (    InterruptedException e) {
      _log.info("Interrupted start of app for " + warContext);
    }
  }
  if (!_isError) {
    TargetModuleID[] ids;
    try {
      ids=getDeploymentManager().getAvailableModules(ModuleType.WAR,getDeploymentManager().getTargets());
      int i=-1;
      for (i=0; i < ids.length; i++) {
        if (ids[i].getModuleID().equals(warContext)) {
          break;
        }
      }
      if (i >= 0) {
        getDeploymentManager().start(new TargetModuleID[]{ids[i]});
      }
    }
 catch (    IllegalStateException ise) {
      _log.warn(ise);
    }
catch (    TargetException te) {
      _log.warn(te);
    }
  }
}
