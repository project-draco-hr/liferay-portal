{
  if (null == _deploymentManager) {
    DeploymentFactoryManager dfm=DeploymentFactoryManager.getInstance();
    try {
      Class dfClass=_loader.loadClass(_getParam(PropsKeys.JSR88_DF_CLASSNAME));
      DeploymentFactory dfInstance;
      dfInstance=(DeploymentFactory)dfClass.newInstance();
      dfm.registerDeploymentFactory(dfInstance);
    }
 catch (    ClassNotFoundException cnfe) {
      _log.warn(cnfe);
    }
catch (    IllegalAccessException iae) {
      _log.warn(iae);
    }
catch (    InstantiationException ie) {
      _log.warn(ie);
    }
    try {
      _deploymentManager=dfm.getDeploymentManager(_getParam(PropsKeys.JSR88_DM_ID),_getParam(PropsKeys.JSR88_DM_USER),_getParam(PropsKeys.JSR88_DM_PASSWORD));
    }
 catch (    DeploymentManagerCreationException dmce) {
      _log.warn(dmce);
    }
  }
  return _deploymentManager;
}
