{
  String mvcAction=ParamUtil.getString(renderRequest,"mvcAction");
  MVCRenderCommand mvcRenderCommand=(MVCRenderCommand)_mvcRenderCommandCache.getMVCCommand(mvcAction);
  if (mvcRenderCommand != MVCRenderCommand.EMPTY) {
    String mvcPath=mvcRenderCommand.render(renderRequest,renderResponse);
    if (Validator.isNotNull(mvcPath)) {
      renderRequest.setAttribute("mvcPath",mvcPath);
    }
  }
  super.render(renderRequest,renderResponse);
}
