{
  try {
    checkPermissions(actionRequest);
  }
 catch (  Exception e) {
    throw new PortletException(e);
  }
  String mvcActionCommandName=ParamUtil.getString(actionRequest,ActionRequest.ACTION_NAME);
  if (!mvcActionCommandName.contains(StringPool.COMMA)) {
    MVCActionCommand mvcActionCommand=_mvcActionCommandCache.getMVCCommand(mvcActionCommandName);
    if (mvcActionCommand != MVCActionCommand.EMPTY) {
      return mvcActionCommand.processAction(actionRequest,actionResponse);
    }
  }
 else {
    List<MVCActionCommand> mvcActionCommands=_mvcActionCommandCache.getMVCCommandChain(mvcActionCommandName);
    if (!mvcActionCommands.isEmpty()) {
      for (      MVCActionCommand mvcActionCommand : mvcActionCommands) {
        if (!mvcActionCommand.processAction(actionRequest,actionResponse)) {
          return false;
        }
      }
      return true;
    }
  }
  return super.callActionMethod(actionRequest,actionResponse);
}
