{
  String mvcPath=portletRequest.getParameter("mvcPath");
  if (mvcPath == null) {
    mvcPath=(String)portletRequest.getAttribute(getMVCPathAttributeName(PortalUtil.getPortletId(portletRequest)));
  }
  if (mvcPath == null) {
    mvcPath=portletRequest.getParameter("jspPage");
  }
  return mvcPath;
}
