{
  super.contextInitialized(event);
  ServletContext servletContext=event.getServletContext();
  ApplicationContext applicationContext=WebApplicationContextUtils.getWebApplicationContext(servletContext);
  try {
    Class<?> beanLocatorUtilClass=Class.forName("com.liferay.util.bean.PortletBeanLocatorUtil",true,PortletClassLoaderUtil.getClassLoader());
    Method setBeanLocatorMethod=beanLocatorUtilClass.getMethod("setBeanLocator",new Class[]{BeanLocator.class});
    setBeanLocatorMethod.invoke(beanLocatorUtilClass,new Object[]{new BeanLocatorImpl(applicationContext)});
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  String[] beanDefinitionNames=applicationContext.getBeanDefinitionNames();
  for (  String beanDefinitionName : beanDefinitionNames) {
    applicationContext.getBean(beanDefinitionName);
  }
}
