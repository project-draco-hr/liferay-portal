{
  if (_log.isInfoEnabled()) {
    if (clusterEvent.getClusterEventType().equals(ClusterEventType.JOIN)) {
      _log.info("---JOIN----");
    }
 else {
      _log.info("-----Depart!----");
    }
    for (    ClusterNode clusterNode : clusterEvent.getClusterNodes()) {
      _log.info("-----clusterNode: ");
      _log.info(clusterNode);
    }
  }
}
