{
  try {
    updateLogo(req);
    sendRedirect(req,res);
  }
 catch (  Exception e) {
    if (e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      setForward(req,"portlet.admin.error");
    }
 else     if (e instanceof UploadException) {
      SessionErrors.add(req,e.getClass().getName());
    }
 else {
      throw e;
    }
  }
}
