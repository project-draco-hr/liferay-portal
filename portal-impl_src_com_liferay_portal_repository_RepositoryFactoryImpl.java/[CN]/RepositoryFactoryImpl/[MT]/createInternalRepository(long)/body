{
  RepositoryDefinition repositoryDefinition=getRepositoryDefinition(getDefaultClassNameId());
  RepositoryCreator repositoryCreator=repositoryDefinition.getRepositoryCreator();
  Repository repository=repositoryCreator.createRepository(repositoryId);
  return new CapabilityRepository(repository,repositoryDefinition.getSupportedCapabilities(),repositoryDefinition.getExportedCapabilities(),repositoryDefinition.getRepositoryEventTrigger());
}
