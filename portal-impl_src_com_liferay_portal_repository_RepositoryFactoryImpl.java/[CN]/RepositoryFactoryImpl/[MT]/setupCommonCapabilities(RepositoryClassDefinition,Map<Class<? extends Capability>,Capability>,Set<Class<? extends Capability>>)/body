{
  if (!supportedCapabilities.containsKey(RepositoryEventTriggerCapability.class)) {
    supportedCapabilities.put(RepositoryEventTriggerCapability.class,new LiferayRepositoryEventTriggerCapability(repositoryClassDefinition.getRepositoryEventTrigger()));
    exportedCapabilityClasses.add(RepositoryEventTriggerCapability.class);
  }
}
