{
  if (repository instanceof BaseRepositoryProxyBean) {
    BaseRepositoryProxyBean baseRepositoryProxyBean=(BaseRepositoryProxyBean)repository;
    ClassLoaderBeanHandler classLoaderBeanHandler=(ClassLoaderBeanHandler)ProxyUtil.getInvocationHandler(baseRepositoryProxyBean.getProxyBean());
    Object bean=classLoaderBeanHandler.getBean();
    if (bean instanceof CMISRepositoryHandler) {
      CMISRepositoryHandler cmisRepositoryHandler=(CMISRepositoryHandler)bean;
      supportedCapabilities.put(CMISRepositoryHandler.class,cmisRepositoryHandler);
      exportedCapabilityClasses.add(CMISRepositoryHandler.class);
    }
  }
}
