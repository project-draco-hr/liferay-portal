{
  List<UserNotificationEvent> userNotificationEvents=userNotificationEventPersistence.findByUuid(uuid);
  if (userNotificationEvents.isEmpty()) {
    return null;
  }
  UserNotificationEvent userNotificationEvent=userNotificationEvents.get(0);
  userNotificationEvent.setArchived(archive);
  userNotificationEventPersistence.update(userNotificationEvent,false);
  return userNotificationEvent;
}
