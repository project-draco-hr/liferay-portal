{
  try {
    DDMFormEvaluatorHelper ddmFormEvaluatorHelper=new DDMFormEvaluatorHelper(ddmForm,ddmFormValues,locale);
    ddmFormEvaluatorHelper.setDDMExpressionFactory(_ddmExpressionFactory);
    ddmFormEvaluatorHelper.setJSONFactory(_jsonFactory);
    return ddmFormEvaluatorHelper.evaluate();
  }
 catch (  PortalException pe) {
    throw new DDMFormEvaluationException(pe);
  }
}
