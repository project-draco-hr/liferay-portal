{
  Session session=null;
  try {
    session=openSession();
    JournalStructureHBM journalStructureHBM=(JournalStructureHBM)session.get(JournalStructureHBM.class,journalStructurePK);
    if (journalStructureHBM == null) {
      _log.warn("No JournalStructure exists with the primary key " + journalStructurePK.toString());
      throw new NoSuchStructureException("No JournalStructure exists with the primary key " + journalStructurePK.toString());
    }
    return JournalStructureHBMUtil.model(journalStructureHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
