{
  List<DDMFormFieldValue> ddmFormFieldValues=record.getDDMFormFieldValues(ddmFormField.getName());
  DDMFormFieldValueRendererAccessor ddmFormFieldValueRendererAccessor=getDDMFormFieldValueRendererAccessor(ddmFormField,locale);
  return ListUtil.toString(ddmFormFieldValues,ddmFormFieldValueRendererAccessor,StringPool.COMMA_AND_SPACE);
}
