{
  if (_hasEditDisplayDDMTemplatePermission != null) {
    return _hasEditDisplayDDMTemplatePermission;
  }
  _hasEditDisplayDDMTemplatePermission=Boolean.FALSE;
  if (getDisplayDDMTemplateId() == 0) {
    return _hasEditDisplayDDMTemplatePermission;
  }
  _hasEditDisplayDDMTemplatePermission=DDMTemplatePermission.contains(getPermissionChecker(),getScopeGroupId(),getDisplayDDMTemplateId(),PortletKeys.DYNAMIC_DATA_LISTS,ActionKeys.UPDATE);
  return _hasEditDisplayDDMTemplatePermission;
}
