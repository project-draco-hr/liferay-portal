{
  StringMaker sm=new StringMaker();
  sm.append("{\"javaClass\":\"");
  sm.append(SearchEngineRequest.class.getName());
  sm.append("\",\"command\":\"");
  sm.append(SearchEngineRequest.COMMAND_INDEX_ONLY);
  sm.append("\"}");
  String json=MessageBusUtil.sendSynchronizedMessage(DestinationNames.SEARCH,sm.toString());
  if (json.indexOf("true") != -1) {
    return true;
  }
  return false;
}
