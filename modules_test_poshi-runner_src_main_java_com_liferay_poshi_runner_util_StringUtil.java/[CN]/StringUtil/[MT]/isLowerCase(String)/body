{
  if (s == null) {
    return false;
  }
  for (int i=0; i < s.length(); i++) {
    char c=s.charAt(i);
    if (c <= 127) {
      if ((c >= CharPool.UPPER_CASE_A) && (c <= CharPool.UPPER_CASE_Z)) {
        return false;
      }
      continue;
    }
    if (Character.isLetter(c) && Character.isUpperCase(c)) {
      return false;
    }
  }
  return true;
}
