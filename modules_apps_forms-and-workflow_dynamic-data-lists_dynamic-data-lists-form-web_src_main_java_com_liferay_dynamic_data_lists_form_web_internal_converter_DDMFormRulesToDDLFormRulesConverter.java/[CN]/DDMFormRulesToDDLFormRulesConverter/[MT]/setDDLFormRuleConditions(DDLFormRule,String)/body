{
  Expression conditionExpression=createExpression(conditionExpressionString);
  ConditionExpressionVisitor conditionExpressionVisitor=new ConditionExpressionVisitor();
  conditionExpression.accept(conditionExpressionVisitor);
  ddlFormRule.setDDLFormRuleConditions(conditionExpressionVisitor.getConditions());
  ddlFormRule.setLogicalOperator(conditionExpressionVisitor.getLogicalOperator());
}
