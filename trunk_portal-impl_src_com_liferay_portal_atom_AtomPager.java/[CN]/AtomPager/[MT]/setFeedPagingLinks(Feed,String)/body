{
  FeedPagingHelper.setFirst(feed,AtomUtil.setPageInUrl(url,1));
  FeedPagingHelper.setLast(feed,AtomUtil.setPageInUrl(url,_lastPage));
  if (_previousPage != 0) {
    FeedPagingHelper.setPrevious(feed,AtomUtil.setPageInUrl(url,_previousPage));
  }
  if (_nextPage != 0) {
    FeedPagingHelper.setNext(feed,AtomUtil.setPageInUrl(url,_nextPage));
  }
}
