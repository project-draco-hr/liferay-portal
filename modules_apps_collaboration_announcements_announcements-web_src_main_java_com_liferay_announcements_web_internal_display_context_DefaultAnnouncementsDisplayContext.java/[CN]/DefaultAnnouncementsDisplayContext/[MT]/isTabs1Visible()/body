{
  String portletName=_announcementsRequestHelper.getPortletName();
  if (!portletName.equals(AnnouncementsPortletKeys.ALERTS) || (portletName.equals(AnnouncementsPortletKeys.ALERTS) && AnnouncementsEntryPermission.contains(_announcementsRequestHelper.getPermissionChecker(),_announcementsRequestHelper.getLayout(),AnnouncementsPortletKeys.ALERTS,ActionKeys.ADD_ENTRY))) {
    return true;
  }
  return false;
}
