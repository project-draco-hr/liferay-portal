{
  String portletName=_announcementsRequestHelper.getPortletName();
  ThemeDisplay themeDisplay=_announcementsRequestHelper.getThemeDisplay();
  if (!portletName.equals(AnnouncementsPortletKeys.ALERTS) || (portletName.equals(AnnouncementsPortletKeys.ALERTS) && PortletPermissionUtil.hasControlPanelAccessPermission(_announcementsRequestHelper.getPermissionChecker(),themeDisplay.getCompanyGroupId(),AnnouncementsPortletKeys.ANNOUNCEMENTS_ADMIN))) {
    return true;
  }
  return false;
}
