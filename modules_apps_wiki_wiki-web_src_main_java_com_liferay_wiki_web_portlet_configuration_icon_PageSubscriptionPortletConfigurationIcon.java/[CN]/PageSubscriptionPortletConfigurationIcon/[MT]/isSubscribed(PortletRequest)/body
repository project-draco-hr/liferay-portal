{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  boolean subscribed=false;
  try {
    WikiPage page=ActionUtil.getPage(portletRequest);
    subscribed=_subscriptionLocalService.isSubscribed(themeDisplay.getCompanyId(),themeDisplay.getUserId(),WikiNode.class.getName(),page.getNodeId());
  }
 catch (  Exception e) {
  }
  return subscribed;
}
