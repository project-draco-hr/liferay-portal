{
  String url=StringPool.BLANK;
  try {
    if (_folder != null) {
      url=PermissionsURLTag.doTag(null,DLFolderConstants.getClassName(),HtmlUtil.unescape(_folder.getName()),null,String.valueOf(_folder.getFolderId()),LiferayWindowState.POP_UP.toString(),null,themeDisplay.getRequest());
    }
 else {
      url=PermissionsURLTag.doTag(null,"com.liferay.document.library.kernel",HtmlUtil.unescape(themeDisplay.getScopeGroupName()),null,String.valueOf(themeDisplay.getScopeGroupId()),LiferayWindowState.POP_UP.toString(),null,themeDisplay.getRequest());
    }
  }
 catch (  Exception e) {
  }
  return url;
}
