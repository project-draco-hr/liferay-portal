{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    if (_folder != null) {
      return DLFolderPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),_folder.getFolderId(),ActionKeys.PERMISSIONS);
    }
  }
 catch (  PortalException pe) {
  }
  return false;
}
