{
  try {
    Resource resource=ResourceUtil.findByC_N_T_S_P(companyId,name,typeId,scope,primKey);
    Permission permission=PermissionUtil.findByA_R(actionId,resource.getResourceId());
    return RoleUtil.containsPermission(roleId,permission.getPermissionId());
  }
 catch (  NoSuchPermissionException nspe) {
  }
catch (  NoSuchResourceException nsre) {
  }
  return false;
}
