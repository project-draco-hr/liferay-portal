{
  List<Attribute> attributes=propertyElement.attributes();
  for (  Attribute attribute : attributes) {
    String attributeName=attribute.getName();
    if (attributeName.equals("line-number") || attributeName.equals("name") || attributeName.equals("value")) {
      continue;
    }
 else {
      throwValidationException(1005,fileName,propertyElement,attributeName);
    }
  }
}
