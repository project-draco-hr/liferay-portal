{
  if ((array == null) || (array.length == 0)) {
    return "()";
  }
  StringBundler sb=new StringBundler();
  sb.append("(");
  for (int i=0; i < array.length; i++) {
    String value=array[i];
    if (value != null) {
      sb.append(value);
    }
    if ((i + 1) != array.length) {
      sb.append("|");
    }
  }
  sb.append(")");
  return sb.toString();
}
