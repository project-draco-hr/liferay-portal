{
  String content=readFile(fileName);
  if (content != null) {
    content=content.trim();
    content=StringUtil.replace(content,"\n","");
    content=StringUtil.replace(content,"\r\n","");
    content=StringUtil.replace(content,"\t"," ");
    content=content.replaceAll(" +"," ");
  }
  return content;
}
