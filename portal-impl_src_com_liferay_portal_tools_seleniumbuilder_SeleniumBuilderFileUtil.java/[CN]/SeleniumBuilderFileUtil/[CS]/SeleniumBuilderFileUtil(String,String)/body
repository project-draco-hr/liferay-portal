{
  _baseDirName=baseDirName;
  try {
    String content=FileUtil.read(projectDirName + "/test.properties");
    InputStream inputStream=new ByteArrayInputStream(content.getBytes());
    _properties.load(inputStream);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  _componentNames=ListUtil.fromArray(StringUtil.split(getProperty("component.names")));
  _testCaseAvailablePropertyNames=ListUtil.fromArray(StringUtil.split(getProperty("test.case.available.property.names")));
  _testrayAvailableComponentNames=ListUtil.fromArray(StringUtil.split(getProperty("testray.available.component.names")));
}
