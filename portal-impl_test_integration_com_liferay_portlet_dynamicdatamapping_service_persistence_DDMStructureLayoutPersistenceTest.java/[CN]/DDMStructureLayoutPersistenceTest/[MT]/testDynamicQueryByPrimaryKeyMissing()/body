{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMStructureLayout.class,DDMStructureLayout.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("structureLayoutId",RandomTestUtil.nextLong()));
  List<DDMStructureLayout> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
