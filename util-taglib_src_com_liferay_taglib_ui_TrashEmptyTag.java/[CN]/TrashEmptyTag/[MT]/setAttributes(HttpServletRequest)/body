{
  request.setAttribute("liferay-ui:trash-empty:confirmMessage",_confirmMessage);
  request.setAttribute("liferay-ui:trash-empty:emptyMessage",_emptyMessage);
  request.setAttribute("liferay-ui:trash-empty:portletURL",_portletURL);
