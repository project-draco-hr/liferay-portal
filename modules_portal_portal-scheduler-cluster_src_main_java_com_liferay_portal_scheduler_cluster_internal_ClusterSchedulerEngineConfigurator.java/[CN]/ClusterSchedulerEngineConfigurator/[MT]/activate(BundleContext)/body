{
  SchedulerEngine schedulerEngine=createSchedulerEngineProxy();
  if (_clusterLink.isEnabled()) {
    ClusterSchedulerEngine clusterSchedulerEngine=new ClusterSchedulerEngine(schedulerEngine);
    clusterSchedulerEngine.setClusterMasterExecutor(_clusterMasterExecutor);
    clusterSchedulerEngine.setProps(_props);
    _serviceRegistration=bundleContext.registerService(IdentifiableOSGiService.class,clusterSchedulerEngine,new HashMapDictionary<String,Object>());
    schedulerEngine=clusterSchedulerEngine;
  }
  _schedulerEngineServiceRegistration=registerSchedulerEngine(bundleContext,schedulerEngine);
}
