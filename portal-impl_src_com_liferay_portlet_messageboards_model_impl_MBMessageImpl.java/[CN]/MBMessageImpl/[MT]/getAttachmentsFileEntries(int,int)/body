{
  List<FileEntry> fileEntries=Collections.EMPTY_LIST;
  long attachmentsFolderId=getAttachmentsFolderId();
  if (attachmentsFolderId != DLFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    fileEntries=PortletFileRepositoryUtil.getPortletFileEntries(getGroupId(),attachmentsFolderId,WorkflowConstants.STATUS_APPROVED,start,end,null);
  }
  return fileEntries;
}
