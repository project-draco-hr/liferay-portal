{
  try {
    for (int i=0; i < srLicenses.size(); i++) {
      com.liferay.portlet.softwarerepository.model.SRLicense srLicense=(com.liferay.portlet.softwarerepository.model.SRLicense)srLicenses.get(i);
      removeSRLicense.remove(pk,srLicense.getPrimaryKey());
    }
  }
 catch (  DataAccessException dae) {
    throw new SystemException(dae);
  }
}
