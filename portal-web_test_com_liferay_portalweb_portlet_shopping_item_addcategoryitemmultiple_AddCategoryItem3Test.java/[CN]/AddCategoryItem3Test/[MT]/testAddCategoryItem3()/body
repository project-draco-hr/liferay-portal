{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Shopping Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Categories",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//td[1]/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Item']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_34_sku",RuntimeVariables.replace("2222"));
  selenium.type("_34_name",RuntimeVariables.replace("The Prodigal Project - J. H."));
  selenium.type("_34_description",RuntimeVariables.replace("For those on a journey, the end of which is faintly out of sight."));
  selenium.type("_34_price0",RuntimeVariables.replace("$2.99"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertTrue(selenium.isTextPresent("The Prodigal Project - J. H.\nFor those on a journey, the end of which is faintly out of sight."));
}
