{
  DDMFormValues ddmFormValues=recordVersion.getDDMFormValues();
  List<DDMFormFieldValue> ddmFormFieldValues=ddmFormValues.getDDMFormFieldValues();
  Map<String,List<DDMFormFieldValue>> ddmFormFieldValuesMap=new LinkedHashMap<>();
  for (  DDMFormFieldValue ddmFormFieldValue : ddmFormFieldValues) {
    loadDDMFormFieldValuesMap(ddmFormFieldValue,ddmFormFieldValuesMap);
  }
  return ddmFormFieldValuesMap;
}
