{
  selenium.click("//img[@title='Configuration']");
  selenium.waitForPageToLoad("30000");
  selenium.select("_86_questionId",RuntimeVariables.replace("label=Test Poll Question"));
  selenium.click("//input[@value='Save']");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Return to Full Page");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//td[7]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
