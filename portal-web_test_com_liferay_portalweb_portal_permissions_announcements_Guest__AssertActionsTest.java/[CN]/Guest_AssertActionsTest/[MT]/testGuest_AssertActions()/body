{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Announcements Permissions Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Announcements Permissions Page"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isElementPresent("link=Edit"));
  assertFalse(selenium.isElementPresent("link=Delete"));
  assertFalse(selenium.isElementPresent("link=Manage Entries"));
  assertFalse(selenium.isElementPresent("link=Mark as Read"));
}
