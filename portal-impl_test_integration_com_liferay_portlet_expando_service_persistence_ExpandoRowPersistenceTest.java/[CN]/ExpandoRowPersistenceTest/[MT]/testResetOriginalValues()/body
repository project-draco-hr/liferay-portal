{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  ExpandoRow newExpandoRow=addExpandoRow();
  _persistence.clearCache();
  ExpandoRowModelImpl existingExpandoRowModelImpl=(ExpandoRowModelImpl)_persistence.findByPrimaryKey(newExpandoRow.getPrimaryKey());
  Assert.assertEquals(existingExpandoRowModelImpl.getTableId(),existingExpandoRowModelImpl.getOriginalTableId());
  Assert.assertEquals(existingExpandoRowModelImpl.getClassPK(),existingExpandoRowModelImpl.getOriginalClassPK());
}
