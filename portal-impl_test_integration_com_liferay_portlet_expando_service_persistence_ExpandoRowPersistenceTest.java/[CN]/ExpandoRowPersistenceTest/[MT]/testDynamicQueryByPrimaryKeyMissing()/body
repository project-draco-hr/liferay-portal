{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ExpandoRow.class,ExpandoRow.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("rowId",ServiceTestUtil.nextLong()));
  List<ExpandoRow> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
