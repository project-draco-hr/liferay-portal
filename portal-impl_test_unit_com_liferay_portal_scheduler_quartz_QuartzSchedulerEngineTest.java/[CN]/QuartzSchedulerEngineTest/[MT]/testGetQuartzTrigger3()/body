{
  List<LogRecord> logRecords=JDKLoggerTestUtil.configureJDKLogger(QuartzSchedulerEngine.class.getName(),Level.FINE);
  IntervalTrigger intervalTrigger=new IntervalTrigger(_TEST_JOB_NAME_0,_MEMORY_TEST_GROUP_NAME,0);
  org.quartz.Trigger trigger=_quartzSchedulerEngine.getQuartzTrigger(intervalTrigger);
  Assert.assertNull(trigger);
  Assert.assertEquals(1,logRecords.size());
  LogRecord logRecord=logRecords.get(0);
  Assert.assertEquals("Not scheduling " + _TEST_JOB_NAME_0 + " because interval is less"+ " than or equal to 0",logRecord.getMessage());
}
