{
  Date startDate=new Date(System.currentTimeMillis() + 10000);
  CronTrigger cronTrigger1=new CronTrigger(_TEST_JOB_NAME_0,_MEMORY_TEST_GROUP_NAME,null,"0/1 * * * * ?");
  CronTrigger cronTrigger2=new CronTrigger(_TEST_JOB_NAME_0,_MEMORY_TEST_GROUP_NAME,startDate,"0/1 * * * * ?");
  org.quartz.Trigger trigger1=_quartzSchedulerEngine.getQuartzTrigger(cronTrigger1);
  org.quartz.Trigger trigger2=_quartzSchedulerEngine.getQuartzTrigger(cronTrigger2);
  Date nextFireDate1=trigger1.getStartTime();
  Date nextFireDate2=trigger2.getStartTime();
  Assert.assertTrue(nextFireDate1.before(nextFireDate2));
}
