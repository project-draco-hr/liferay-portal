{
  SchedulerResponse schedulerResponse=_quartzSchedulerEngine.getScheduledJob(_TEST_JOB_NAME_0,_MEMORY_TEST_GROUP_NAME,StorageType.MEMORY);
  Message message=schedulerResponse.getMessage();
  JobState jobState=(JobState)message.get(SchedulerEngine.JOB_STATE);
  Assert.assertNotNull(jobState.getExceptions());
  _quartzSchedulerEngine.suppressError(_TEST_JOB_NAME_0,_MEMORY_TEST_GROUP_NAME,StorageType.MEMORY);
  schedulerResponse=_quartzSchedulerEngine.getScheduledJob(_TEST_JOB_NAME_0,_MEMORY_TEST_GROUP_NAME,StorageType.MEMORY);
  message=schedulerResponse.getMessage();
  jobState=(JobState)message.get(SchedulerEngine.JOB_STATE);
  Assert.assertNull(jobState.getExceptions());
}
