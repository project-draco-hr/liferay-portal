{
  SchedulerResponse schedulerResponse=_quartzSchedulerEngine.getScheduledJob(_TEST_JOB_NAME_0,_MEMORY_TEST_GROUP_NAME);
  Object triggerContent=schedulerResponse.getTrigger().getTriggerContent();
  Assert.assertEquals(_DEFAULT_INTERVAL,triggerContent);
  String newTriggerContent="0 0 12 * * ?";
  Trigger trigger=new CronTrigger(_TEST_JOB_NAME_0,_MEMORY_TEST_GROUP_NAME,newTriggerContent);
  _quartzSchedulerEngine.update(trigger);
  schedulerResponse=_quartzSchedulerEngine.getScheduledJob(_TEST_JOB_NAME_0,_MEMORY_TEST_GROUP_NAME);
  triggerContent=schedulerResponse.getTrigger().getTriggerContent();
  Assert.assertEquals(newTriggerContent,triggerContent);
}
