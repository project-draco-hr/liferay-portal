{
  MockScheduler mockScheduler=ReflectionTestUtil.getFieldValue(_quartzSchedulerEngine,"_memoryScheduler");
  String jobName=_TEST_JOB_NAME_PREFIX + "memory";
  mockScheduler.addJob(jobName,_TEST_GROUP_NAME,StorageType.MEMORY,null);
  SchedulerResponse schedulerResponse=_quartzSchedulerEngine.getScheduledJob(jobName,_MEMORY_TEST_GROUP_NAME);
  Assert.assertNull(schedulerResponse.getTrigger());
  Trigger trigger=new IntervalTrigger(jobName,_MEMORY_TEST_GROUP_NAME,new Date(),_DEFAULT_INTERVAL);
  _quartzSchedulerEngine.update(trigger);
  schedulerResponse=_quartzSchedulerEngine.getScheduledJob(_TEST_JOB_NAME_0,_MEMORY_TEST_GROUP_NAME);
  Assert.assertNotNull(schedulerResponse.getTrigger());
}
