{
  Group group1=(Group)obj1;
  Group group2=(Group)obj2;
  int value=0;
  if (group1.getType() > group2.getType()) {
    value=1;
  }
 else   if (group1.getType() < group2.getType()) {
    value=-1;
  }
  if (value == 0) {
    value=group1.getName().compareTo(group2.getName());
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
