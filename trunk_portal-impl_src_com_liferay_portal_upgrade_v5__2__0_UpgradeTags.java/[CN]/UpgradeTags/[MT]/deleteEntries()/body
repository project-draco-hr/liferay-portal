{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select entryId from TagsEntry where groupId = 0");
    rs=ps.executeQuery();
    while (rs.next()) {
      long entryId=rs.getLong("entryId");
      runSQL("delete from TagsAssets_TagsEntries where entryId = " + entryId);
      runSQL("delete from TagsProperty where entryId = " + entryId);
    }
    runSQL("delete from TagsEntry where groupId = 0");
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
