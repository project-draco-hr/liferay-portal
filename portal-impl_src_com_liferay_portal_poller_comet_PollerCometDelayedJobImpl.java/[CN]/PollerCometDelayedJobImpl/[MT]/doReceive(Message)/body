{
synchronized (_pollerCometDelayedTasks) {
    for (    PollerCometDelayedTask pollerCometDelayedTask : _pollerCometDelayedTasks) {
      try {
        pollerCometDelayedTask.doTask();
      }
 catch (      Exception e) {
        if (_log.isWarnEnabled()) {
          _log.warn("Error occurred while processing" + " PollerCometDelayedTask",e);
        }
      }
    }
    _pollerCometDelayedTasks.clear();
  }
}
