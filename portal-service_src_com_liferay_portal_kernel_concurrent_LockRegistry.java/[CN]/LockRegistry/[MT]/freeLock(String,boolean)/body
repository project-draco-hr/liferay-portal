{
  Map<String,ReentrantLock> lockGroup=_lockGroupMap.remove(groupName);
  if (lockGroup == null) {
    _prematureLockReleases.put(groupName,_dummyValue);
    return null;
  }
  if (unlock == true) {
    for (    ReentrantLock lock : lockGroup.values()) {
      lock.unlock();
    }
    _prematureLockReleases.remove(groupName);
  }
  return lockGroup;
}
