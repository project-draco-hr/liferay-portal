{
  try {
    com.liferay.portlet.dynamicdatamapping.model.DDMTemplate returnValue=DDMTemplateServiceUtil.updateTemplate(templateId,name,description,type,language,script,serviceContext);
    return com.liferay.portlet.dynamicdatamapping.model.DDMTemplateSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
