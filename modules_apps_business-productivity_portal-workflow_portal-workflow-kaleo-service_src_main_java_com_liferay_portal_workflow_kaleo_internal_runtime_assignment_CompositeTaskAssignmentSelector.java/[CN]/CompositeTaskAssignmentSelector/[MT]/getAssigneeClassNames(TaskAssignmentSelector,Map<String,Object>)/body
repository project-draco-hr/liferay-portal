{
  Object value=properties.get("assignee.class.name");
  String[] assigneeClassNames=GetterUtil.getStringValues(value,new String[]{String.valueOf(value)});
  if (ArrayUtil.isEmpty(assigneeClassNames)) {
    throw new IllegalArgumentException("The property \"assignee.class.name\" is invalid for " + ClassUtil.getClassName(taskAssignmentSelector));
  }
  return assigneeClassNames;
}
