{
  String uuid=null;
  if (classedModel instanceof StagedModel) {
    StagedModel stagedModel=(StagedModel)classedModel;
    uuid=stagedModel.getUuid();
  }
 else {
    Class<?> modelClass=classedModel.getClass();
    Method getUuidMethod=modelClass.getMethod("getUuid",new Class[0]);
    if (getUuidMethod != null) {
      uuid=(String)getUuidMethod.invoke(classedModel,new Object[0]);
    }
 else {
      uuid=StringPool.BLANK;
    }
  }
  return uuid;
}
