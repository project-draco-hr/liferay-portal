{
  userId=StringUtil.lowerCase(userId);
  firstName=StringUtil.lowerCase(firstName);
  middleName=StringUtil.lowerCase(middleName);
  lastName=StringUtil.lowerCase(lastName);
  screenName=StringUtil.lowerCase(screenName);
  emailAddress=StringUtil.lowerCase(emailAddress);
  Session session=null;
  try {
    session=HibernateUtil.openSession();
    String sql=CustomSQLUtil.get(COUNT_BY_C_U_FN_MN_LN_SN_EA_A);
    sql=StringUtil.replace(sql,"[$JOIN$]",_getJoin(params));
    sql=StringUtil.replace(sql,"[$WHERE$]",_getWhere(params));
    sql=CustomSQLUtil.replaceAndOperator(sql,andOperator);
    SQLQuery q=session.createSQLQuery(sql);
    q.setCacheable(false);
    q.addScalar(HibernateUtil.getCountColumnName(),Hibernate.LONG);
    QueryPos qPos=QueryPos.getInstance(q);
    _setJoin(qPos,params);
    qPos.add(companyId);
    qPos.add(userId);
    qPos.add(userId);
    qPos.add(firstName);
    qPos.add(firstName);
    qPos.add(middleName);
    qPos.add(middleName);
    qPos.add(lastName);
    qPos.add(lastName);
    qPos.add(screenName);
    qPos.add(screenName);
    qPos.add(emailAddress);
    qPos.add(emailAddress);
    qPos.add(active);
    Iterator itr=q.list().iterator();
    if (itr.hasNext()) {
      Long count=(Long)itr.next();
      if (count != null) {
        return count.intValue();
      }
    }
    return 0;
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    HibernateUtil.closeSession(session);
  }
}
