{
  if (portletDataContext.addPrimaryKey(JournalPortletDataHandler.class,"deleteData")) {
    return portletPreferences;
  }
  _journalArticleLocalService.deleteArticles(portletDataContext.getScopeGroupId());
  _journalFolderLocalService.deleteFolders(portletDataContext.getGroupId());
  _ddmTemplateLocalService.deleteTemplates(portletDataContext.getScopeGroupId(),PortalUtil.getClassNameId(DDMStructure.class));
  _ddmStructureLocalService.deleteStructures(portletDataContext.getScopeGroupId(),PortalUtil.getClassNameId(JournalArticle.class));
  return portletPreferences;
}
