{
  ListType newListType=addListType();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ListType.class,ListType.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("listTypeId",newListType.getListTypeId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  ListType existingListType=(ListType)result.get(0);
  assertEquals(existingListType,newListType);
}
