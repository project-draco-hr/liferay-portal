{
  App newApp=addApp();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(App.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("appId"));
  Object newAppId=newApp.getAppId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("appId",new Object[]{newAppId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Object existingAppId=result.get(0);
  Assert.assertEquals(existingAppId,newAppId);
}
