{
  try {
    com.liferay.portlet.bookmarks.model.BookmarksFolder returnValue=BookmarksFolderServiceUtil.updateFolder(folderId,parentFolderId,name,description,mergeWithParentFolder);
    return com.liferay.portlet.bookmarks.model.BookmarksFolderSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
