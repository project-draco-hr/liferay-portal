{
  Bundle bundle=FrameworkUtil.getBundle(StagedModelRepositoryRegistryUtil.class);
  _bundleContext=bundle.getBundleContext();
  Class<StagedModelRepository<?>> clazz=(Class)StagedModelRepository.class;
  _serviceTracker=ServiceTrackerFactory.open(_bundleContext,clazz,new StagedModelRepositoryServiceTrackerCustomizer());
}
