{
  String url=StringPool.BLANK;
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    WikiPage page=ActionUtil.getPage(portletRequest);
    url=PermissionsURLTag.doTag(null,WikiPage.class.getName(),page.getTitle(),null,String.valueOf(page.getResourcePrimKey()),LiferayWindowState.POP_UP.toString(),null,themeDisplay.getRequest());
  }
 catch (  Exception e) {
  }
  return url;
}
