{
  Iterator<JavaTerm> itr=javaTerms.iterator();
  while (itr.hasNext()) {
    JavaTerm javaTerm=itr.next();
    if (fileName.contains("/test/") && !fileName.endsWith("TestBean.java") && !fileName.endsWith("TestCase.java")) {
      checkTestAnnotations(javaTerm,fileName);
    }
    String javaTermContent=javaTerm.getContent();
    while (true) {
      String newJavaTermContent=sortAnnotations(javaTermContent,StringPool.TAB);
      if (javaTermContent.equals(newJavaTermContent)) {
        break;
      }
      content=content.replace(javaTermContent,newJavaTermContent);
      javaTermContent=newJavaTermContent;
    }
  }
  return sortAnnotations(content,StringPool.BLANK);
}
