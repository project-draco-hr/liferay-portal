{
  if (cdata == null) {
    return StringPool.BLANK;
  }
  cdata=StringUtil.replace(cdata,"\n"," ");
  while (cdata.contains("> ")) {
    cdata=StringUtil.replace(cdata,"> ",">");
  }
  while (cdata.contains(" <")) {
    cdata=StringUtil.replace(cdata," <","<");
  }
  while (cdata.contains("  ")) {
    cdata=StringUtil.replace(cdata,"  "," ");
  }
  return cdata.trim();
}
