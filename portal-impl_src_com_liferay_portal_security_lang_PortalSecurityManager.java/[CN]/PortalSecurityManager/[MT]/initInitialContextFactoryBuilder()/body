{
  if (!_initInitialContextFactoryBuilder) {
    return;
  }
  _initInitialContextFactoryBuilder=false;
  if (NamingManager.hasInitialContextFactoryBuilder()) {
    if (_log.isInfoEnabled()) {
      _log.info("Unable to override the initial context factory builder " + "because one already exists. JNDI security is not " + "enabled.");
    }
    return;
  }
  NamingManager.setInitialContextFactoryBuilder(new PACLInitialContextFactoryBuilder());
}
