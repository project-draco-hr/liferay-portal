{
  Annotation[] annotations=_annotationCache.get(methodInvocation);
  if (annotations == _emptyAnnotations) {
    return defaultValue;
  }
  if (annotations == null) {
    return null;
  }
  for (  Annotation annotation : annotations) {
    if (annotation.annotationType() == annotationType) {
      return (T)annotation;
    }
  }
  return defaultValue;
}
