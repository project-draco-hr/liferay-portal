{
  long fileEntryTypeId=ParamUtil.getLong(serviceContext,"fileEntryTypeId",-1L);
  Map<String,Fields> fieldsMap=getFieldsMap(serviceContext,fileEntryTypeId);
  DLFileEntry dlFileEntry=dlFileEntryService.addFileEntry(getGroupId(),getRepositoryId(),toFolderId(folderId),sourceFileName,mimeType,title,description,changeLog,fileEntryTypeId,fieldsMap,null,is,size,serviceContext);
  addFileEntryResources(dlFileEntry,serviceContext);
  return new LiferayFileEntry(dlFileEntry);
}
