{
  QueryDefinition<Object> queryDefinition=new QueryDefinition<>(status,PrincipalThreadLocal.getUserId(),true,start,end,(OrderByComparator<Object>)obc);
  List<Object> dlFoldersAndDLFileEntriesAndDLFileShortcuts=dlFolderService.getFoldersAndFileEntriesAndFileShortcuts(getGroupId(),toFolderId(folderId),mimeTypes,includeMountFolders,queryDefinition);
  return RepositoryModelUtil.toRepositoryEntries(dlFoldersAndDLFileEntriesAndDLFileShortcuts);
}
