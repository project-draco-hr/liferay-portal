{
  long fileEntryTypeId=ParamUtil.getLong(serviceContext,"fileEntryTypeId",0L);
  Map<String,Fields> fieldsMap=getFieldsMap(serviceContext,fileEntryTypeId);
  long size=0;
  if (file != null) {
    size=file.length();
  }
  DLFileEntry dlFileEntry=dlFileEntryService.updateFileEntry(fileEntryId,sourceFileName,mimeType,title,description,changeLog,majorVersion,fileEntryTypeId,fieldsMap,file,null,size,serviceContext);
  return new LiferayFileEntry(dlFileEntry);
}
