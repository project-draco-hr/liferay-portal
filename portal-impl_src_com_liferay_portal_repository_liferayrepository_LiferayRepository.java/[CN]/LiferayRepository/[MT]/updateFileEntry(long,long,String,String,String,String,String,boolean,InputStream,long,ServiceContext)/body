{
  long fileEntryTypeId=ParamUtil.getLong(serviceContext,"fileEntryTypeId",-1L);
  Map<String,Fields> fieldsMap=getFieldsMap(serviceContext,fileEntryTypeId);
  DLFileEntry dlFileEntry=dlFileEntryService.updateFileEntry(fileEntryId,sourceFileName,mimeType,title,description,changeLog,majorVersion,fileEntryTypeId,fieldsMap,null,is,size,serviceContext);
  return new LiferayFileEntry(dlFileEntry);
}
