{
  List<Object> dlFoldersAndFileEntriesAndFileShortcuts=dlFolderService.getFoldersAndFileEntriesAndFileShortcuts(getGroupId(),toFolderId(folderId),status,includeMountFolders,start,end,obc);
  return RepositoryModelUtil.toFileEntriesFileShortcutsAndFolders(dlFoldersAndFileEntriesAndFileShortcuts);
}
