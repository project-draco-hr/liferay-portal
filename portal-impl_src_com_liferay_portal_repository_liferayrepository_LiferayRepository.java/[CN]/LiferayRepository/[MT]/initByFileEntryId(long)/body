{
  try {
    DLFileEntry dlFileEntry=dlFileEntryService.getFileEntry(fileEntryId);
    initByRepositoryId(dlFileEntry.getRepositoryId());
  }
 catch (  Exception e) {
    if (_log.isTraceEnabled()) {
      if (e instanceof NoSuchFileEntryException) {
        _log.trace(e.getMessage());
      }
 else {
        _log.trace(e,e);
      }
    }
  }
}
