{
  StringBuilder sb=new StringBuilder("resources/");
  sb.append(Platform.isLinux() ? "linux" : Platform.isMac() ? "darwin" : Platform.isWindows() ? "win32" : "");
  if (!Platform.isMac()) {
    sb.append("-x86");
    if (Platform.is64Bit()) {
      sb.append("-64");
    }
  }
  return sb.toString();
}
