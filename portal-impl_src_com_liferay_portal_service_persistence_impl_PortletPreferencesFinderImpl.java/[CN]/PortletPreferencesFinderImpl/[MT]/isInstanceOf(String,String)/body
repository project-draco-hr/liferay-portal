{
  portletPreferencesPortletId=GetterUtil.getString(portletPreferencesPortletId);
  portletId=GetterUtil.getString(portletId);
  if (portletPreferencesPortletId.equals(portletId)) {
    return true;
  }
  PortletInstance portletInstance=PortletInstance.fromPortletInstanceKey(portletPreferencesPortletId);
  return portletInstance.hasIdenticalPortletName(portletId);
}
