{
  final long classNameId=PortalUtil.getClassNameId(className);
  PredicateFilter<AssetVocabulary> associatedVocabulariesFilter=new PredicateFilter<AssetVocabulary>(){
    @Override public boolean filter(    AssetVocabulary assetVocabulary){
      return assetVocabulary.isAssociatedToClassNameAndTypeId(classNameId,classTypeId);
    }
  }
;
  return ListUtil.filter(vocabularies,associatedVocabulariesFilter);
}
