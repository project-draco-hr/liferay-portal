{
  final long classNameId=PortalUtil.getClassNameId(className);
  PredicateFilter<AssetVocabulary> vocabulariesFilter=new PredicateFilter<AssetVocabulary>(){
    @Override public boolean filter(    AssetVocabulary vocabulary){
      return vocabulary.isAssociatedToAssetRendererFactory(classNameId);
    }
  }
;
  return ListUtil.filter(vocabularies,vocabulariesFilter);
}
