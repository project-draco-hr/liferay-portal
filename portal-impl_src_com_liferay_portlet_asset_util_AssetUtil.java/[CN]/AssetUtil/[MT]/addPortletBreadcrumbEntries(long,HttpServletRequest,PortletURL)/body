{
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  boolean isPortletBreadcrumbEntry=Validator.isNotNull(portletDisplay.getId()) && !portletDisplay.isFocused();
  addPortletBreadcrumbEntries(assetCategoryId,request,portletURL,isPortletBreadcrumbEntry);
}
