{
  String result=s;
  AssetCategory assetCategory=null;
  if (categoryId > 0) {
    try {
      assetCategory=AssetCategoryLocalServiceUtil.getAssetCategory(categoryId);
    }
 catch (    NoSuchCategoryException nsac) {
    }
  }
  if (assetCategory != null) {
    List<AssetCategoryProperty> properties=AssetCategoryPropertyLocalServiceUtil.getCategoryProperties(categoryId);
    for (    AssetCategoryProperty property : properties) {
      result=StringUtil.replace(result,"[$" + property.getKey() + "$]",property.getValue());
    }
  }
  return StringUtil.stripBetween(result,"[$","$]");
}
