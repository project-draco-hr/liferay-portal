{
  String defaultAssetPublisherPortletId=getDefaultAssetPublisherId(layout);
  if (Validator.isNull(defaultAssetPublisherPortletId)) {
    return false;
  }
  if (defaultAssetPublisherPortletId.equals(portletId) || defaultAssetPublisherPortletId.equals(portletResource)) {
    return true;
  }
  return false;
}
