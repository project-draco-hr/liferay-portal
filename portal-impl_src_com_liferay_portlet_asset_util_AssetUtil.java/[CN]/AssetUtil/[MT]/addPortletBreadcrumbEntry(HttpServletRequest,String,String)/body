{
  List<BreadcrumbEntry> portletBreadcrumbs=(List<BreadcrumbEntry>)request.getAttribute(WebKeys.PORTLET_BREADCRUMBS);
  if (portletBreadcrumbs != null) {
    for (    BreadcrumbEntry portletBreadcrumb : portletBreadcrumbs) {
      if (title.equals(portletBreadcrumb.getTitle())) {
        return;
      }
    }
  }
  PortalUtil.addPortletBreadcrumbEntry(request,title,url);
}
