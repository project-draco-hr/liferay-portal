{
  int initialServiceRegistrationsSize=_hotDeployListener.getServiceRegistrationsSize();
  HotDeployEvent hotDeployEvent=getHotDeployEvent("classpath:/com/liferay/application/list/deploy/hot/test" + "/dependencies/control-panel-entry-liferay-portlet.xml");
  _hotDeployListener.invokeDeploy(hotDeployEvent);
  Assert.assertEquals(initialServiceRegistrationsSize + 1,_hotDeployListener.getServiceRegistrationsSize());
}
