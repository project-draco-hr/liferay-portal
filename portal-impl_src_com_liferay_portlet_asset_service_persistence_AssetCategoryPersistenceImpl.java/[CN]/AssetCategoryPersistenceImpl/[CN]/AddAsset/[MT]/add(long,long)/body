{
  if (!_persistenceImpl.containsAsset.contains(categoryId,assetId)) {
    ModelListener<com.liferay.portlet.asset.model.Asset>[] assetListeners=assetPersistence.getListeners();
    for (    ModelListener<AssetCategory> listener : listeners) {
      listener.onBeforeAddAssociation(categoryId,com.liferay.portlet.asset.model.Asset.class.getName(),assetId);
    }
    for (    ModelListener<com.liferay.portlet.asset.model.Asset> listener : assetListeners) {
      listener.onBeforeAddAssociation(assetId,AssetCategory.class.getName(),categoryId);
    }
    _sqlUpdate.update(new Object[]{new Long(categoryId),new Long(assetId)});
    for (    ModelListener<AssetCategory> listener : listeners) {
      listener.onAfterAddAssociation(categoryId,com.liferay.portlet.asset.model.Asset.class.getName(),assetId);
    }
    for (    ModelListener<com.liferay.portlet.asset.model.Asset> listener : assetListeners) {
      listener.onAfterAddAssociation(assetId,AssetCategory.class.getName(),categoryId);
    }
  }
}
