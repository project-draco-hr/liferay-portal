{
  ModelListener<com.liferay.portlet.asset.model.Asset>[] assetListeners=assetPersistence.getListeners();
  List<com.liferay.portlet.asset.model.Asset> assets=null;
  if ((listeners.length > 0) || (assetListeners.length > 0)) {
    assets=getAssets(categoryId);
    for (    com.liferay.portlet.asset.model.Asset asset : assets) {
      for (      ModelListener<AssetCategory> listener : listeners) {
        listener.onBeforeRemoveAssociation(categoryId,com.liferay.portlet.asset.model.Asset.class.getName(),asset.getPrimaryKey());
      }
      for (      ModelListener<com.liferay.portlet.asset.model.Asset> listener : assetListeners) {
        listener.onBeforeRemoveAssociation(asset.getPrimaryKey(),AssetCategory.class.getName(),categoryId);
      }
    }
  }
  _sqlUpdate.update(new Object[]{new Long(categoryId)});
  if ((listeners.length > 0) || (assetListeners.length > 0)) {
    for (    com.liferay.portlet.asset.model.Asset asset : assets) {
      for (      ModelListener<AssetCategory> listener : listeners) {
        listener.onAfterRemoveAssociation(categoryId,com.liferay.portlet.asset.model.Asset.class.getName(),asset.getPrimaryKey());
      }
      for (      ModelListener<com.liferay.portlet.asset.model.Asset> listener : assetListeners) {
        listener.onBeforeRemoveAssociation(asset.getPrimaryKey(),AssetCategory.class.getName(),categoryId);
      }
    }
  }
}
