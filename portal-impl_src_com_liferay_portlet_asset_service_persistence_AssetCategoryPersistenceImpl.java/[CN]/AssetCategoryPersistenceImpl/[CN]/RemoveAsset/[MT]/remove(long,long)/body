{
  if (_persistenceImpl.containsAsset.contains(categoryId,assetId)) {
    ModelListener<com.liferay.portlet.asset.model.Asset>[] assetListeners=assetPersistence.getListeners();
    for (    ModelListener<AssetCategory> listener : listeners) {
      listener.onBeforeRemoveAssociation(categoryId,com.liferay.portlet.asset.model.Asset.class.getName(),assetId);
    }
    for (    ModelListener<com.liferay.portlet.asset.model.Asset> listener : assetListeners) {
      listener.onBeforeRemoveAssociation(assetId,AssetCategory.class.getName(),categoryId);
    }
    _sqlUpdate.update(new Object[]{new Long(categoryId),new Long(assetId)});
    for (    ModelListener<AssetCategory> listener : listeners) {
      listener.onAfterRemoveAssociation(categoryId,com.liferay.portlet.asset.model.Asset.class.getName(),assetId);
    }
    for (    ModelListener<com.liferay.portlet.asset.model.Asset> listener : assetListeners) {
      listener.onAfterRemoveAssociation(assetId,AssetCategory.class.getName(),categoryId);
    }
  }
}
