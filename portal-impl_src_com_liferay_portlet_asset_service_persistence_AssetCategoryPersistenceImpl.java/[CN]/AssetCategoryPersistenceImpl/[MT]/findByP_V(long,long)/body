{
  Object[] finderArgs=new Object[]{new Long(parentCategoryId),new Long(vocabularyId)};
  List<AssetCategory> list=(List<AssetCategory>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_P_V,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portlet.asset.model.AssetCategory WHERE ");
      query.append("parentCategoryId = ?");
      query.append(" AND ");
      query.append("vocabularyId = ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("name ASC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(parentCategoryId);
      qPos.add(vocabularyId);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<AssetCategory>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_P_V,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
