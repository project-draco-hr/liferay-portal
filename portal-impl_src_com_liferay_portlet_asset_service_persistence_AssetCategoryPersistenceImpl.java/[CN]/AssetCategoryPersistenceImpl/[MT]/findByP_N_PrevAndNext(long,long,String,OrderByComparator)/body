{
  AssetCategory assetCategory=findByPrimaryKey(categoryId);
  Session session=null;
  try {
    session=openSession();
    AssetCategory[] array=new AssetCategoryImpl[3];
    array[0]=getByP_N_PrevAndNext(session,assetCategory,parentCategoryId,name,orderByComparator,true);
    array[1]=assetCategory;
    array[2]=getByP_N_PrevAndNext(session,assetCategory,parentCategoryId,name,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
