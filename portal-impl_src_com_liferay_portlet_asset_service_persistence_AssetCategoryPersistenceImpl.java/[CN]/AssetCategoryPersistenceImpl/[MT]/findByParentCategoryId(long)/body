{
  Object[] finderArgs=new Object[]{new Long(parentCategoryId)};
  List<AssetCategory> list=(List<AssetCategory>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_PARENTCATEGORYID,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBundler query=new StringBundler();
      query.append(_SQL_SELECT_ASSETCATEGORY_WHERE);
      query.append("assetCategory.parentCategoryId = ?");
      query.append(" ORDER BY ");
      query.append("assetCategory.name ASC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(parentCategoryId);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<AssetCategory>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_PARENTCATEGORYID,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
