{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return countByG_P_N(groupId,parentCategoryId,name);
  }
  StringBundler query=new StringBundler(4);
  query.append(_FILTER_SQL_COUNT_ASSETCATEGORY_WHERE);
  query.append(_FINDER_COLUMN_G_P_N_GROUPID_2);
  query.append(_FINDER_COLUMN_G_P_N_PARENTCATEGORYID_2);
  if (name == null) {
    query.append(_FINDER_COLUMN_G_P_N_NAME_1);
  }
 else {
    if (name.equals(StringPool.BLANK)) {
      query.append(_FINDER_COLUMN_G_P_N_NAME_3);
    }
 else {
      query.append(_FINDER_COLUMN_G_P_N_NAME_2);
    }
  }
  String sql=InlineSQLHelperUtil.replacePermissionCheck(query.toString(),AssetCategory.class.getName(),_FILTER_ENTITY_TABLE_FILTER_PK_COLUMN,groupId);
  Session session=null;
  try {
    session=openSession();
    SQLQuery q=session.createSQLQuery(sql);
    q.addScalar(COUNT_COLUMN_NAME,com.liferay.portal.kernel.dao.orm.Type.LONG);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(groupId);
    qPos.add(parentCategoryId);
    if (name != null) {
      qPos.add(name);
    }
    Long count=(Long)q.uniqueResult();
    return count.intValue();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
