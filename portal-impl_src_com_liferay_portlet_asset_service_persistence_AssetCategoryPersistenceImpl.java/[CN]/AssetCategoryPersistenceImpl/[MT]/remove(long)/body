{
  Session session=null;
  try {
    session=openSession();
    AssetCategory assetCategory=(AssetCategory)session.get(AssetCategoryImpl.class,new Long(categoryId));
    if (assetCategory == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + categoryId);
      }
      throw new NoSuchCategoryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + categoryId);
    }
    return remove(assetCategory);
  }
 catch (  NoSuchCategoryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
