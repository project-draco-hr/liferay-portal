{
  EntityCacheUtil.removeResult(AssetCategoryModelImpl.ENTITY_CACHE_ENABLED,AssetCategoryImpl.class,assetCategory.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{assetCategory.getUuid(),Long.valueOf(assetCategory.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_P_N_V,new Object[]{Long.valueOf(assetCategory.getParentCategoryId()),assetCategory.getName(),Long.valueOf(assetCategory.getVocabularyId())});
}
