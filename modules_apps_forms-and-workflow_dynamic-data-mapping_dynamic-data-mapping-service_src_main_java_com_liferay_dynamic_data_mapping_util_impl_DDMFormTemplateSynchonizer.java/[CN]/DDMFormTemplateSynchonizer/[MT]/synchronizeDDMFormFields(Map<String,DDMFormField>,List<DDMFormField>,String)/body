{
  Iterator<DDMFormField> itr=templateDDMFormFields.iterator();
  while (itr.hasNext()) {
    DDMFormField templateDDMFormField=itr.next();
    String dataType=templateDDMFormField.getDataType();
    String name=templateDDMFormField.getName();
    if (Validator.isNotNull(dataType) && !structureDDMFormFieldsMap.containsKey(name)) {
      itr.remove();
      continue;
    }
    synchronizeDDMFormFieldOptions(structureDDMFormFieldsMap.get(name),templateDDMFormField);
    synchronizeDDMFormFieldRequiredProperty(structureDDMFormFieldsMap.get(name),templateDDMFormField,templateMode);
    synchronizeDDMFormFields(structureDDMFormFieldsMap,templateDDMFormField.getNestedDDMFormFields(),templateMode);
  }
}
