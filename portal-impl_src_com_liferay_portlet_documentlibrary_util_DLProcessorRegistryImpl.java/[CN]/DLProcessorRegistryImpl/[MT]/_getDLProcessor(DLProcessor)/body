{
  if (ProxyUtil.isProxyClass(dlProcessor.getClass())) {
    InvocationHandler invocationHandler=ProxyUtil.getInvocationHandler(dlProcessor);
    if (invocationHandler instanceof ClassLoaderBeanHandler) {
      ClassLoaderBeanHandler classLoaderBeanHandler=(ClassLoaderBeanHandler)invocationHandler;
      Object bean=classLoaderBeanHandler.getBean();
      if (bean instanceof DLProcessor) {
        dlProcessor=(DLProcessor)bean;
      }
    }
  }
  return dlProcessor;
}
