{
  Registry registry=RegistryUtil.getRegistry();
  ServiceRegistration<DLProcessor> previousServiceRegistration=_serviceRegistrations.remove(dlProcessor.getType());
  if (previousServiceRegistration != null) {
    previousServiceRegistration.unregister();
  }
  ServiceRegistration<DLProcessor> serviceRegistration=registry.registerService(DLProcessor.class,dlProcessor);
  _serviceRegistrations.put(dlProcessor.getType(),serviceRegistration);
}
