{
  boolean previewable=true;
  long fileEntryPreviewableProcessorMaxSize=PropsValues.DL_FILE_ENTRY_PREVIEWABLE_PROCESSOR_MAX_SIZE;
  try {
    fileEntryPreviewableProcessorMaxSize=PrefsPropsUtil.getLong(PropsKeys.DL_FILE_ENTRY_PREVIEWABLE_PROCESSOR_MAX_SIZE);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  if (fileEntryPreviewableProcessorMaxSize == 0) {
    previewable=false;
  }
 else   if ((fileEntryPreviewableProcessorMaxSize > 0) && (fileVersion.getSize() > fileEntryPreviewableProcessorMaxSize)) {
    previewable=false;
  }
  return previewable;
}
