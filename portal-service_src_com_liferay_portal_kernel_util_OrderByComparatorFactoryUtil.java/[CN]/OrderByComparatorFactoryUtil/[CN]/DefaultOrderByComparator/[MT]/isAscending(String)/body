{
  String orderBy=getOrderBy();
  if (orderBy == null) {
    return false;
  }
  int index=orderBy.indexOf(StringPool.PERIOD + field + StringPool.SPACE);
  if (index == -1) {
    return false;
  }
  int x=orderBy.indexOf(_ORDER_BY_ASC,index);
  if (x == -1) {
    return false;
  }
  int y=orderBy.indexOf(_ORDER_BY_DESC,index);
  if ((y >= 0) && (y < x)) {
    return false;
  }
 else {
    return true;
  }
}
