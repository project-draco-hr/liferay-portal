{
  if (Validator.isNull(xml)) {
    return xml;
  }
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < xml.length(); i++) {
    char c=xml.charAt(i);
    if ((c == 0x9) || (c == 0xA) || (c == 0xD)|| ((c >= 0x20) && (c <= 0xD7FF))|| ((c >= 0xE000) && (c <= 0xFFFD))|| ((c >= 0x10000) && (c <= 0x10FFFF))) {
      sb.append(c);
    }
  }
  return sb.toString();
}
