{
  Boolean value=null;
  String key=_encodeKey(userId,groupId,name,primKey,actionId);
  if (_localCacheEnabled) {
    Map<String,Object> localCache=_localCache.get();
    value=(Boolean)localCache.get(key);
  }
  if (value == null) {
    value=(Boolean)MultiVMPoolUtil.get(_cache,key);
  }
  return value;
}
