{
  if (ExportImportThreadLocal.isImportInProcess() || !PermissionThreadLocal.isFlushResourceBlockEnabled(companyId,groupId,name)) {
    return;
  }
  _resourceBlockIdsBagCacheIndexer.removeKeys(ResourceBlockIdsBagKeyIndexEncoder.encode(companyId,groupId,name));
  _sendClearCacheClusterMessage(_clearResourceBlockCacheMethodKey,companyId,groupId,name);
}
