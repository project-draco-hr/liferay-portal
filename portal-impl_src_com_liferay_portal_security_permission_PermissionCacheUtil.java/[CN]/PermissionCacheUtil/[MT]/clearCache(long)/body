{
  if (ExportImportThreadLocal.isImportInProcess()) {
    return;
  }
  _userPermissionCheckerBagPortalCache.remove(userId);
  _userRolePortalCacheKeyManager.removeBySimpleKey(UserRoleKey.getSimpleKey(userId));
  _permissionCheckerBagPortalCacheKeyManager.removeBySimpleKey(BagKey.getSimpleKey(userId));
  _permissionPortalCache.removeAll();
  _resourceBlockIdsBagCache.removeAll();
}
