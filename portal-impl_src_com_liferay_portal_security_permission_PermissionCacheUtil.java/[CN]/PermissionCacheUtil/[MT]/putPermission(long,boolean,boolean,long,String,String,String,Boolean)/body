{
  if (value == null) {
    return null;
  }
  PermissionKey permissionKey=new PermissionKey(userId,signedIn,checkGuest,groupId,name,primKey,actionId);
  if (_localCacheAvailable) {
    Map<Serializable,Object> localCache=_localCache.get();
    localCache.put(permissionKey,value);
  }
  _permissionPortalCache.put(permissionKey,value);
  return value;
}
