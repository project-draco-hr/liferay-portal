{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(RepositoryEntry.class,RepositoryEntry.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("repositoryEntryId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("repositoryEntryId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
