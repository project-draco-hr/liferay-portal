{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_36_keywords",RuntimeVariables.replace("\"This is a wiki page test.\""));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=Wiki Page Test"));
  selenium.type("_36_keywords",RuntimeVariables.replace("\"This1 is1 a1 wiki1 page1 test1.\""));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isElementPresent("link=Wiki Page Test"));
}
