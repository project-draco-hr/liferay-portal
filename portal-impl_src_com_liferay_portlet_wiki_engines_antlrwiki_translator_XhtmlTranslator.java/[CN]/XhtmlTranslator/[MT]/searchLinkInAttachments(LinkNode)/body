{
  try {
    List<DLFileEntry> attachments=_page.getAttachmentsFiles();
    for (    DLFileEntry attachment : attachments) {
      String title=attachment.getTitle();
      if (title.equals(linkNode.getLink())) {
        return title;
      }
    }
  }
 catch (  Exception e) {
  }
  return null;
}
