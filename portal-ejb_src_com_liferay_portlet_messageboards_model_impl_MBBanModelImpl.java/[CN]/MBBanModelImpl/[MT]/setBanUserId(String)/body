{
  if (((banUserId == null) && (_banUserId != null)) || ((banUserId != null) && (_banUserId == null)) || ((banUserId != null) && (_banUserId != null) && !banUserId.equals(_banUserId))) {
    if (!XSS_ALLOW_BANUSERID) {
      banUserId=XSSUtil.strip(banUserId);
    }
    _banUserId=banUserId;
  }
}
