{
  try {
    EventsProcessorUtil.process(PropsKeys.APPLICATION_SHUTDOWN_EVENTS,PropsValues.APPLICATION_SHUTDOWN_EVENTS,new String[]{String.valueOf(companyId)});
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  _companyIds=ArrayUtil.remove(_companyIds,companyId);
  _webIds=null;
  _getWebIds();
  WebAppPool.remove(companyId,WebKeys.PORTLET_CATEGORY);
}
