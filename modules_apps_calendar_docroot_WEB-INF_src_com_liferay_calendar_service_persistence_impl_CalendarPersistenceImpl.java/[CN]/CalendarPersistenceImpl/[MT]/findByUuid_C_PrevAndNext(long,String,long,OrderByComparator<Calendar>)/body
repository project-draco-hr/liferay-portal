{
  Calendar calendar=findByPrimaryKey(calendarId);
  Session session=null;
  try {
    session=openSession();
    Calendar[] array=new CalendarImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,calendar,uuid,companyId,orderByComparator,true);
    array[1]=calendar;
    array[2]=getByUuid_C_PrevAndNext(session,calendar,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
