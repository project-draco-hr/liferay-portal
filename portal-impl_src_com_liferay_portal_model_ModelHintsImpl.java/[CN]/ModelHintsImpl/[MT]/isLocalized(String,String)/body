{
  Map<String,Object> fields=(Map<String,Object>)_modelFields.get(model);
  if (fields == null) {
    return false;
  }
 else {
    Boolean localized=(Boolean)fields.get(field + _LOCALIZATION_SUFFIX);
    if (localized != null) {
      return localized;
    }
 else {
      return false;
    }
  }
}
