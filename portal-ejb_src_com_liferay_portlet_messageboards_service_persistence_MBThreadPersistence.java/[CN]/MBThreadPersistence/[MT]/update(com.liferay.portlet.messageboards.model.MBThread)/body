{
  Session session=null;
  try {
    session=openSession();
    if (mbThread.isNew()) {
      session.save(mbThread);
    }
    session.flush();
    mbThread.setNew(false);
    return mbThread;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
