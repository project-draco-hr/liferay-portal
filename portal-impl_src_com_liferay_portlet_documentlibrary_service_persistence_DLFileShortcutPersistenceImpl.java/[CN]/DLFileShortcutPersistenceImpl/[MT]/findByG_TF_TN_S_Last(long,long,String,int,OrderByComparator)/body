{
  int count=countByG_TF_TN_S(groupId,toFolderId,toName,status);
  List<DLFileShortcut> list=findByG_TF_TN_S(groupId,toFolderId,toName,status,count - 1,count,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(9);
    msg.append("No DLFileShortcut exists with the key {");
    msg.append("groupId=" + groupId);
    msg.append(", ");
    msg.append("toFolderId=" + toFolderId);
    msg.append(", ");
    msg.append("toName=" + toName);
    msg.append(", ");
    msg.append("status=" + status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchFileShortcutException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
