{
  Session session=null;
  try {
    session=openSession();
    DLFileShortcut dlFileShortcut=(DLFileShortcut)session.get(DLFileShortcutImpl.class,new Long(fileShortcutId));
    if (dlFileShortcut == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + fileShortcutId);
      }
      throw new NoSuchFileShortcutException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + fileShortcutId);
    }
    return dlFileShortcutPersistence.remove(dlFileShortcut);
  }
 catch (  NoSuchFileShortcutException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
