{
  List<DLFileShortcut> list=findByG_F_A(groupId,folderId,active,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(8);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("groupId=");
    msg.append(groupId);
    msg.append(", folderId=");
    msg.append(folderId);
    msg.append(", active=");
    msg.append(active);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchFileShortcutException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
