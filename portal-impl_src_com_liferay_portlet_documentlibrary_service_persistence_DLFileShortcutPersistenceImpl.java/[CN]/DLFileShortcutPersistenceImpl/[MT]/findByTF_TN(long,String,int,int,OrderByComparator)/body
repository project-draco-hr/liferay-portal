{
  Object[] finderArgs=new Object[]{new Long(toFolderId),toName,String.valueOf(start),String.valueOf(end),String.valueOf(obc)};
  Object result=FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_OBC_TF_TN,finderArgs,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portlet.documentlibrary.model.DLFileShortcut WHERE ");
      query.append("toFolderId = ?");
      query.append(" AND ");
      if (toName == null) {
        query.append("toName IS NULL");
      }
 else {
        query.append("toName = ?");
      }
      query.append(" ");
      if (obc != null) {
        query.append("ORDER BY ");
        query.append(obc.getOrderBy());
      }
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(toFolderId);
      if (toName != null) {
        qPos.add(toName);
      }
      List<DLFileShortcut> list=(List<DLFileShortcut>)QueryUtil.list(q,getDialect(),start,end);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_OBC_TF_TN,finderArgs,list);
      cacheResult(list);
      return list;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (List<DLFileShortcut>)result;
  }
}
