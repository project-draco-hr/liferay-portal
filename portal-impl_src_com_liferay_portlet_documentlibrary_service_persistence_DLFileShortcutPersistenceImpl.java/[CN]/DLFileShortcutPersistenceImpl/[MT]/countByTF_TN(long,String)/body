{
  Object[] finderArgs=new Object[]{new Long(toFolderId),toName};
  Long count=(Long)FinderCacheUtil.getResult(FINDER_PATH_COUNT_BY_TF_TN,finderArgs,this);
  if (count == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT COUNT(*) ");
      query.append("FROM com.liferay.portlet.documentlibrary.model.DLFileShortcut WHERE ");
      query.append("toFolderId = ?");
      query.append(" AND ");
      if (toName == null) {
        query.append("toName IS NULL");
      }
 else {
        query.append("toName = ?");
      }
      query.append(" ");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(toFolderId);
      if (toName != null) {
        qPos.add(toName);
      }
      count=(Long)q.uniqueResult();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (count == null) {
        count=Long.valueOf(0);
      }
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_TF_TN,finderArgs,count);
      closeSession(session);
    }
  }
  return count.intValue();
}
