{
  User user=userPersistence.findByPrimaryKey(userId);
  long groupId=user.getGroup().getGroupId();
  if (privateLayoutsFile != null) {
    layoutLocalService.importLayouts(userId,groupId,true,parameterMap,privateLayoutsFile);
  }
  if (publicLayoutsFile != null) {
    layoutLocalService.importLayouts(userId,groupId,false,parameterMap,publicLayoutsFile);
  }
}
