{
  selenium.refresh();
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertTrue(selenium.isElementPresent("link=Test Category 2"));
  selenium.refresh();
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertTrue(selenium.isElementPresent("link=Test Category 2"));
  selenium.refresh();
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertTrue(selenium.isElementPresent("link=Test Category 2"));
}
