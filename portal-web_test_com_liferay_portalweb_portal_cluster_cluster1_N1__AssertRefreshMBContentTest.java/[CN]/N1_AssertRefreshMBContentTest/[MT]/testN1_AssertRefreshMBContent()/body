{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.refresh();
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=Test Category 2"));
  selenium.refresh();
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=Test Category 2"));
  selenium.refresh();
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=Test Category 2"));
}
