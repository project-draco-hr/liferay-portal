{
  selenium.refresh();
  selenium.waitForPageToLoad("30000");
  selenium.getEval("window.Liferay.fire('initDockbar');");
  assertTrue(selenium.isElementPresent("link=Test Category 2"));
  selenium.refresh();
  selenium.waitForPageToLoad("30000");
  selenium.getEval("window.Liferay.fire('initDockbar');");
  assertTrue(selenium.isElementPresent("link=Test Category 2"));
  selenium.refresh();
  selenium.waitForPageToLoad("30000");
  selenium.getEval("window.Liferay.fire('initDockbar');");
  assertTrue(selenium.isElementPresent("link=Test Category 2"));
}
