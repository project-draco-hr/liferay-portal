{
  if (((screenName == null) && (_screenName != null)) || ((screenName != null) && (_screenName == null)) || ((screenName != null) && (_screenName != null) && !screenName.equals(_screenName))) {
    if (!XSS_ALLOW_SCREENNAME) {
      screenName=XSSUtil.strip(screenName);
    }
    _screenName=screenName;
  }
}
