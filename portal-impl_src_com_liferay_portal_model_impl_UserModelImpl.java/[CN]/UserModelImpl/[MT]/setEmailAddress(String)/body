{
  if (((emailAddress == null) && (_emailAddress != null)) || ((emailAddress != null) && (_emailAddress == null)) || ((emailAddress != null) && (_emailAddress != null) && !emailAddress.equals(_emailAddress))) {
    if (!XSS_ALLOW_EMAILADDRESS) {
      emailAddress=XSSUtil.strip(emailAddress);
    }
    _emailAddress=emailAddress;
  }
}
