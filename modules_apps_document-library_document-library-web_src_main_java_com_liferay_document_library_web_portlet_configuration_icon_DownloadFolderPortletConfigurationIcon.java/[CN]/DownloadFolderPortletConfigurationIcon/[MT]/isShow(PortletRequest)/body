{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    return DLFolderPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),_folder.getFolderId(),ActionKeys.VIEW);
  }
 catch (  PortalException pe) {
  }
  return false;
}
