{
  this.user=user;
  if (user.isDefaultUser()) {
    this.defaultUserId=user.getUserId();
    this.signedIn=false;
  }
 else {
    try {
      this.defaultUserId=UserLocalServiceUtil.getDefaultUserId(user.getCompanyId());
      this.ownerRole=RoleLocalServiceUtil.getRole(user.getCompanyId(),RoleNames.OWNER);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
    this.signedIn=true;
  }
  this.checkGuest=checkGuest;
}
