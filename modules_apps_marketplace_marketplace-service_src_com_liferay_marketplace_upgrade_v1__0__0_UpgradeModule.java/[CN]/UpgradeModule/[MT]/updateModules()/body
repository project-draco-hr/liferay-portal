{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select moduleId, contextName from Marketplace_Module");
    rs=ps.executeQuery();
    while (rs.next()) {
      long moduleId=rs.getLong("moduleId");
      String contextName=rs.getString("contextName");
      String newContextName=null;
      try {
        newContextName=ContextUtil.getContextName(contextName);
        runSQL("update Marketplace_Module set contextName = '" + newContextName + "' where moduleId = "+ moduleId);
      }
 catch (      IOException ioe) {
        _log.error("Unable to update module + " + moduleId + " with the new context name "+ newContextName,ioe);
      }
    }
  }
 catch (  SQLException sqle) {
    _log.error("Unable to update modules",sqle);
  }
 finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
