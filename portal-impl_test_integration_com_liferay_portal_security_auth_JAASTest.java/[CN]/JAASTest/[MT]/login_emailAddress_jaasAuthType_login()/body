{
  _jaasAuthTypeField.set(null,"login");
  User user=TestPropsValues.getUser();
  String emailAddress=user.getEmailAddress();
  String userIdString=String.valueOf(user.getUserId());
  LoginContext loginContext=new LoginContext(_PORTAL_REALM,new JAASCallbackHandler(emailAddress,user.getPassword()));
  try {
    loginContext.login();
  }
 catch (  Exception e) {
    Assert.fail();
  }
  validateSubject(loginContext.getSubject(),userIdString);
}
