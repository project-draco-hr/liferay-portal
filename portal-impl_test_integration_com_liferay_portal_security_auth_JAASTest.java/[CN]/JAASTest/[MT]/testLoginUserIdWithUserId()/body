{
  _jaasAuthTypeField.set(null,"userId");
  LoginContext loginContext=getLoginContext(String.valueOf(_user.getUserId()),_user.getPassword());
  try {
    loginContext.login();
  }
 catch (  Exception e) {
    Assert.fail();
  }
  validateSubject(loginContext.getSubject(),String.valueOf(_user.getUserId()));
}
