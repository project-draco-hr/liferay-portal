{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Categories Navigation Test Page",RuntimeVariables.replace("Categories Navigation Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Topic"),selenium.getText("xPath=(//div[@class='results-header']/h3)[1]"));
  assertEquals(RuntimeVariables.replace("Category Name"),selenium.getText("//ul[@class='categories']"));
  assertEquals(RuntimeVariables.replace("Topic"),selenium.getText("xPath=(//div[@class='results-header']/h3)[2]"));
}
