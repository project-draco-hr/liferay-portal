{
  List<JournalArticleResource> resources=new ArrayList<JournalArticleResource>(binaryContents.size());
  for (  BinaryContent binaryContent : binaryContents) {
    MirageJournalArticleResource resource=(MirageJournalArticleResource)binaryContent;
    resources.add(resource.getJournalResource());
  }
  return resources;
}
