{
  JournalArticleResource journalResource=new JournalArticleResourceImpl();
  journalResource.setGroupId(groupId);
  journalResource.setArticleId(articleId);
  MirageJournalArticleResource mirageResource=new MirageJournalArticleResource(journalResource);
  BinaryContentService binaryContentService=MirageServiceFactory.getArticleResourceService();
  try {
    binaryContentService.deleteBinaryContent(mirageResource,new JournalArticleResourceCriteria());
  }
 catch (  CMSException cmse) {
    processException(cmse);
  }
}
