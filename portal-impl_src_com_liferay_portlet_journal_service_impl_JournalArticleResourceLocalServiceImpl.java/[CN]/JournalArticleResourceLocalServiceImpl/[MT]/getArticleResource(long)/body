{
  JournalArticleResource journalResource=new JournalArticleResourceImpl();
  journalResource.setResourcePrimKey(articleResourcePrimKey);
  JournalResourceContent resourceContent=new JournalResourceContent(journalResource);
  try {
    BinaryContentService binaryContentService=MirageServiceFactory.getArticleResourceService();
    resourceContent=(JournalResourceContent)binaryContentService.getBinaryContent(resourceContent);
  }
 catch (  CMSException ex) {
    _throwException(ex);
  }
  return resourceContent.getJournalResource();
}
