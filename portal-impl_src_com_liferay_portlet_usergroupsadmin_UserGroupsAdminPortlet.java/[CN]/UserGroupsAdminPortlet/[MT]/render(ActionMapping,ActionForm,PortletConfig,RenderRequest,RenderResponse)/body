{
  try {
    ActionUtil.getUserGroup(renderRequest);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchUserGroupException || e instanceof PrincipalException) {
      SessionErrors.add(renderRequest,e.getClass());
      return actionMapping.findForward("portlet.user_groups_admin.error");
    }
 else {
      throw e;
    }
  }
  return actionMapping.findForward(getForward(renderRequest,"portlet.user_groups_admin.edit_user_group_assignments"));
}
