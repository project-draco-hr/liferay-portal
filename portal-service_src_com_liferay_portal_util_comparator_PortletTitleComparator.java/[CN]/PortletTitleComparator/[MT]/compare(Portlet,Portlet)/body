{
  String portletTitle1=StringPool.BLANK;
  String portletTitle2=StringPool.BLANK;
  if (_ctx != null) {
    portletTitle1=PortalUtil.getPortletTitle(portlet1,_ctx,_locale);
    portletTitle2=PortalUtil.getPortletTitle(portlet2,_ctx,_locale);
  }
 else {
    portletTitle1=PortalUtil.getPortletTitle(portlet1,_companyId,_locale);
    portletTitle2=PortalUtil.getPortletTitle(portlet2,_companyId,_locale);
  }
  return portletTitle1.compareTo(portletTitle2);
}
