{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace("Asset Publisher Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Blogs Entry2 Title"),selenium.getText("//h3[@class='asset-title']/a"));
  selenium.clickAt("//h3[@class='asset-title']/a",RuntimeVariables.replace("Blogs Entry2 Title"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isVisible("//h1[@class='header-title']"));
  selenium.click(RuntimeVariables.replace("//a[@class=' taglib-icon']/span[contains(.,'Move to the Recycle Bin')]"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("The selected item was moved to the Recycle Bin. Undo"),selenium.getText("//div[@class='portlet-msg-success taglib-trash-undo']"));
}
