{
  if (isStagingGroup()) {
    return null;
  }
  try {
    if (_stagingGroup == null) {
      _stagingGroup=GroupLocalServiceUtil.getStagingGroup(getGroupId());
    }
    return _stagingGroup;
  }
 catch (  Exception e) {
    _log.error("Error getting staging group for " + getGroupId(),e);
    return null;
  }
}
