{
  ServiceContext serviceContext=portletDataContext.createServiceContext(stagedGroup);
  Group group=_groupLocalService.updateGroup(stagedGroup.getGroupId(),stagedGroup.getParentGroupId(),stagedGroup.getNameMap(),stagedGroup.getDescriptionMap(),stagedGroup.getType(),stagedGroup.getManualMembership(),stagedGroup.getMembershipRestriction(),stagedGroup.getFriendlyURL(),stagedGroup.isInheritContent(),stagedGroup.isActive(),serviceContext);
  return ModelAdapterUtil.adapt(group,Group.class,StagedGroup.class);
}
