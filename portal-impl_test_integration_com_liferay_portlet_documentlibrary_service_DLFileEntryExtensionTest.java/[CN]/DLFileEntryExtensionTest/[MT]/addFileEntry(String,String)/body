{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId(),TestPropsValues.getUserId());
  DLAppTestUtil.populateNotificationsServiceContext(serviceContext,Constants.ADD);
  return DLAppLocalServiceUtil.addFileEntry(TestPropsValues.getUserId(),group.getGroupId(),parentFolder.getFolderId(),sourceFileName,ContentTypes.TEXT_PLAIN,title,StringPool.BLANK,StringPool.BLANK,RandomTestUtil.randomBytes(),serviceContext);
}
