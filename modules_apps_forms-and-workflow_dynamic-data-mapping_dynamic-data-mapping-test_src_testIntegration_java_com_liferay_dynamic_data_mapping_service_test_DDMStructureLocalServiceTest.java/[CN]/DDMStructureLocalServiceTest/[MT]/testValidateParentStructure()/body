{
  DDMStructure structure1=addStructure(0,_classNameId,null,"Test Structure 1",null,read("ddm-structure-text-field.xsd"),StorageType.JSON.getValue(),DDMStructureConstants.TYPE_DEFAULT);
  DDMStructure structure2=addStructure(structure1.getStructureId(),_classNameId,null,"Test Structure 2",null,read("ddm-structure-radio-field.xsd"),StorageType.JSON.getValue(),DDMStructureConstants.TYPE_DEFAULT);
  DDMStructure structure3=addStructure(structure2.getStructureId(),_classNameId,null,"Test Structure 3",null,read("ddm-structure-select-field.xsd"),StorageType.JSON.getValue(),DDMStructureConstants.TYPE_DEFAULT);
  structure1.setParentStructureId(structure3.getStructureId());
  updateStructure(structure1);
}
