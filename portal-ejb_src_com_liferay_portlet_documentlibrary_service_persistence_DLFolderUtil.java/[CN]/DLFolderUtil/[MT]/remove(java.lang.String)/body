{
  ModelListener listener=null;
  if (Validator.isNotNull(_LISTENER)) {
    try {
      listener=(ModelListener)Class.forName(_LISTENER).newInstance();
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(folderId));
  }
  com.liferay.portlet.documentlibrary.model.DLFolder dlFolder=getPersistence().remove(folderId);
  if (listener != null) {
    listener.onAfterRemove(dlFolder);
  }
  return dlFolder;
}
