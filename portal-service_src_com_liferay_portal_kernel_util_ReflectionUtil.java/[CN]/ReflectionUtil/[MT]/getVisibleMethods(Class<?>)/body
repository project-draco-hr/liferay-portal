{
  Set<Method> visiableMethods=new HashSet<Method>(Arrays.asList(clazz.getMethods()));
  visiableMethods.addAll(Arrays.asList(clazz.getDeclaredMethods()));
  while ((clazz=clazz.getSuperclass()) != null) {
    for (    Method declaredMethod : clazz.getDeclaredMethods()) {
      int modifiers=declaredMethod.getModifiers();
      if (!Modifier.isPublic(modifiers) && !Modifier.isPrivate(modifiers)) {
        visiableMethods.add(declaredMethod);
      }
    }
  }
  return visiableMethods;
}
