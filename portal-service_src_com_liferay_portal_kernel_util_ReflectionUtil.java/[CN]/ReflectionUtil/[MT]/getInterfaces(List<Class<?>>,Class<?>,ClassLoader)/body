{
  for (  Class<?> interfaceClass : clazz.getInterfaces()) {
    try {
      if (classLoader != null) {
        interfaceClasses.add(classLoader.loadClass(interfaceClass.getName()));
      }
 else {
        interfaceClasses.add(interfaceClass);
      }
    }
 catch (    ClassNotFoundException cnfe) {
    }
  }
}
