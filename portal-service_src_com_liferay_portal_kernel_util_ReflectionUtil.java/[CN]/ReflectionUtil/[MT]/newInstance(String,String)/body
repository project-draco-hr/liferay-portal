{
  try {
    Class<?> classObject=Class.forName(className);
    Constructor<?> classConstructor=classObject.getConstructor(new Class[]{String.class});
    Object[] arguments=new Object[]{p1};
    return classConstructor.newInstance(arguments);
  }
 catch (  Exception e) {
    _log.error(e,e);
    return null;
  }
}
