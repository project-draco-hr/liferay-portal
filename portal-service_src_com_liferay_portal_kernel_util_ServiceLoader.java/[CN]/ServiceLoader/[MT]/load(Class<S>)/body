{
  Thread currentThread=Thread.currentThread();
  ClassLoader classLoader=currentThread.getContextClassLoader();
  Enumeration<URL> enu=classLoader.getResources("META-INF/services/" + clazz.getName());
  List<S> services=new ArrayList<S>();
  while (enu.hasMoreElements()) {
    URL url=enu.nextElement();
    try {
      _load(services,classLoader,clazz,url);
    }
 catch (    Exception e) {
      _log.error("An error has ocurred while loading the " + clazz + "using the classloader "+ classLoader);
    }
  }
  return services;
}
