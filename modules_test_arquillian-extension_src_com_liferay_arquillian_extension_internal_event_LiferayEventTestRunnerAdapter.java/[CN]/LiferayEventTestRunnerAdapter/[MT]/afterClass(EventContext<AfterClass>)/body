{
  LogAssertionExecutor logAssertionExecutor=_logAssertionExecutorInstance.get();
  ExpectedLogs expectedLogs=getAnnotation(eventContext.getEvent());
  CaptureAppender captureAppender=logAssertionExecutor.startAssert(expectedLogs);
  eventContext.proceed();
  ClearThreadLocalExecutor clearThreadLocalExecutor=_clearThreadLocalExecutorInstance.get();
  clearThreadLocalExecutor.clearThreadLocal();
  logAssertionExecutor.endAssert(expectedLogs,captureAppender);
}
