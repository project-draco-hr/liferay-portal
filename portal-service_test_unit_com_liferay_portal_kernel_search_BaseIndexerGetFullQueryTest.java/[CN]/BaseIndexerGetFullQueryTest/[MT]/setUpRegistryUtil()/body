{
  Registry registry=mock(Registry.class);
  when(registry.setRegistry(registry)).thenReturn(registry);
  when(registry.getRegistry()).thenReturn(registry);
  ServiceTracker<Object,Object> serviceTracker=mock(ServiceTracker.class);
  when(registry.trackServices((Class<Object>)Matchers.any(),(ServiceTrackerCustomizer<Object,Object>)Matchers.any())).thenReturn(serviceTracker);
  RegistryUtil.setRegistry(registry);
  setUpIndexerRegistryUtil();
}
