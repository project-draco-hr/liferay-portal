{
  String entryId=ParamUtil.getString(req,"entryId");
  String folderId=ParamUtil.getString(req,"folderId");
  String name=ParamUtil.getString(req,"name");
  String url=ParamUtil.getString(req,"url");
  String comments=ParamUtil.getString(req,"comments");
  String[] communityPermissions=req.getParameterValues("communityPermissions");
  String[] guestPermissions=req.getParameterValues("guestPermissions");
  if (Validator.isNull(entryId)) {
    BookmarksEntryServiceUtil.addEntry(folderId,name,url,comments,communityPermissions,guestPermissions);
  }
 else {
    BookmarksEntryServiceUtil.updateEntry(entryId,folderId,name,url,comments);
  }
}
