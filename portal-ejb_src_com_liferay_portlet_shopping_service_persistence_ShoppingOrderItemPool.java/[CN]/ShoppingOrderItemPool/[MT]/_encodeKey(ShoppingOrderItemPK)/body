{
  String shoppingOrderItemPKString=String.valueOf(shoppingOrderItemPK);
  if (Validator.isNull(shoppingOrderItemPKString)) {
    _log.debug("Key is null");
    return null;
  }
 else {
    String key=GROUP_NAME + StringPool.POUND + shoppingOrderItemPKString;
    _log.debug("Key " + key);
    return key;
  }
}
