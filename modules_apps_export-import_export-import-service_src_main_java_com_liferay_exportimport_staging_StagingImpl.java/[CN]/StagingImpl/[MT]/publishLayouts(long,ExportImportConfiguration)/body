{
  Map<String,Serializable> taskContextMap=new HashMap<>();
  taskContextMap.put("exportImportConfigurationId",exportImportConfiguration.getExportImportConfigurationId());
  BackgroundTaskManagerUtil.addBackgroundTask(userId,exportImportConfiguration.getGroupId(),StringPool.BLANK,BackgroundTaskExecutorNames.LAYOUT_STAGING_BACKGROUND_TASK_EXECUTOR,taskContextMap,new ServiceContext());
}
