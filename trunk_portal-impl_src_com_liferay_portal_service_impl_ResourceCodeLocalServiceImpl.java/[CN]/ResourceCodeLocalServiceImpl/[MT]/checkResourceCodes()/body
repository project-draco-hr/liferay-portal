{
  if (PropsValues.PERMISSIONS_USER_CHECK_ALGORITHM == 6) {
    return;
  }
  if (_resourceCodes.isEmpty()) {
    List<ResourceCode> resourceCodes=resourceCodePersistence.findAll();
    for (    ResourceCode resourceCode : resourceCodes) {
      String key=encodeKey(resourceCode.getCompanyId(),resourceCode.getName(),resourceCode.getScope());
      _resourceCodes.put(key,resourceCode);
    }
  }
}
