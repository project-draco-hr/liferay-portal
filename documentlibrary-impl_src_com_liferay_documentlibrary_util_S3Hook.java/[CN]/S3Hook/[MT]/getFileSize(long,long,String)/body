{
  try {
    double version=getHeadVersionNumber(companyId,repositoryId,fileName);
    S3Object objectDetails=_s3Service.getObjectDetails(_s3Bucket,getKey(companyId,repositoryId,fileName,version));
    return objectDetails.getContentLength();
  }
 catch (  S3ServiceException s3se) {
    throw new SystemException(s3se);
  }
}
