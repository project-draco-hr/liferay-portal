{
  StagedLayoutSet stagedLayoutSet=null;
  try {
    LayoutSet layoutSet=_layoutSetLocalService.getLayoutSet(groupId,privateLayout);
    stagedLayoutSet=ModelAdapterUtil.adapt(layoutSet,LayoutSet.class,StagedLayoutSet.class);
  }
 catch (  PortalException pe) {
  }
  return Optional.ofNullable(stagedLayoutSet);
}
