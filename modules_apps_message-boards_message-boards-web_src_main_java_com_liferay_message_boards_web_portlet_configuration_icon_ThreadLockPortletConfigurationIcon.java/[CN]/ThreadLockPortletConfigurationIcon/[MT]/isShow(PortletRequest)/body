{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    return MBCategoryPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),_thread.getCategoryId(),ActionKeys.LOCK_THREAD);
  }
 catch (  PortalException pe) {
  }
  return false;
}
