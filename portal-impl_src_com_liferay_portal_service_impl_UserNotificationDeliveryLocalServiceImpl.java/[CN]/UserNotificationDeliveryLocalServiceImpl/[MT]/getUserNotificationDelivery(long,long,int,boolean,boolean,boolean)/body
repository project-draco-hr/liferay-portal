{
  UserNotificationDelivery userNotificationDelivery=fetchUserNotificationDelivery(userId,classNameId,type);
  if (userNotificationDelivery == null) {
    userNotificationDelivery=userNotificationDeliveryLocalService.addUserNotificationDelivery(userId,classNameId,type,email,sms,website);
  }
  return userNotificationDelivery;
}
