{
  UserNotificationDelivery userNotificationDelivery=userNotificationDeliveryPersistence.fetchByU_C_T(userId,classNameId,type);
  if (userNotificationDelivery == null) {
    return addUserNotificationDelivery(userId,classNameId,type,email,sms,website);
  }
  userNotificationDelivery.setEmail(email);
  userNotificationDelivery.setSms(sms);
  userNotificationDelivery.setWebsite(website);
  return userNotificationDeliveryPersistence.update(userNotificationDelivery);
}
