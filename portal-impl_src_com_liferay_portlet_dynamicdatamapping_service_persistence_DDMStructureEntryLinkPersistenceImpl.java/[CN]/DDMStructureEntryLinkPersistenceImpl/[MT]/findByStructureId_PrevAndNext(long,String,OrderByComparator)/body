{
  DDMStructureEntryLink ddmStructureEntryLink=findByPrimaryKey(structureEntryLinkId);
  Session session=null;
  try {
    session=openSession();
    DDMStructureEntryLink[] array=new DDMStructureEntryLinkImpl[3];
    array[0]=getByStructureId_PrevAndNext(session,ddmStructureEntryLink,structureId,orderByComparator,true);
    array[1]=ddmStructureEntryLink;
    array[2]=getByStructureId_PrevAndNext(session,ddmStructureEntryLink,structureId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
