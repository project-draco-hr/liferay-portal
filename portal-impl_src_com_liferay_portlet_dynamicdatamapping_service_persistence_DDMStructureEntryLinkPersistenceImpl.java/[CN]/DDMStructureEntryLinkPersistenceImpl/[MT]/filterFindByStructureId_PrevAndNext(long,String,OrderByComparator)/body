{
  if (!InlineSQLHelperUtil.isEnabled()) {
    return findByStructureId_PrevAndNext(structureEntryLinkId,structureId,orderByComparator);
  }
  DDMStructureEntryLink ddmStructureEntryLink=findByPrimaryKey(structureEntryLinkId);
  Session session=null;
  try {
    session=openSession();
    DDMStructureEntryLink[] array=new DDMStructureEntryLinkImpl[3];
    array[0]=filterGetByStructureId_PrevAndNext(session,ddmStructureEntryLink,structureId,orderByComparator,true);
    array[1]=ddmStructureEntryLink;
    array[2]=filterGetByStructureId_PrevAndNext(session,ddmStructureEntryLink,structureId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
