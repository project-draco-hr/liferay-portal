{
  Session session=null;
  try {
    session=openSession();
    Website website=(Website)session.get(Website.class,websiteId);
    if (website == null) {
      _log.warn("No Website exists with the primary key " + websiteId.toString());
      throw new NoSuchWebsiteException("No Website exists with the primary key " + websiteId.toString());
    }
    session.delete(website);
    session.flush();
    return website;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
