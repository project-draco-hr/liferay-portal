{
  Session session=null;
  try {
    session=openSession();
    if (website.isNew()) {
      session.save(website);
    }
    session.flush();
    website.setNew(false);
    return website;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
