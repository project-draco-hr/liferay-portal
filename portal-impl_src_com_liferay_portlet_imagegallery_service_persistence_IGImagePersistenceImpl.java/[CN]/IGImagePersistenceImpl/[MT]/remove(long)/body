{
  Session session=null;
  try {
    session=openSession();
    IGImage igImage=(IGImage)session.get(IGImageImpl.class,Long.valueOf(imageId));
    if (igImage == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + imageId);
      }
      throw new NoSuchImageException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + imageId);
    }
    return igImagePersistence.remove(igImage);
  }
 catch (  NoSuchImageException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
