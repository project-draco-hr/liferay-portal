{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(igImage);
    }
  }
  igImage=removeImpl(igImage);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(igImage);
    }
  }
  return igImage;
}
