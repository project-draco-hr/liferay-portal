{
  Session session=null;
  try {
    session=openSession();
    igImage=(IGImage)session.merge(igImage);
    session.delete(igImage);
    session.flush();
    return igImage;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(IGImage.class.getName());
  }
}
