{
  IGImage igImage=(IGImage)EntityCacheUtil.getResult(IGImageModelImpl.ENTITY_CACHE_ENABLED,IGImageImpl.class,imageId,this);
  if (igImage == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (igImage == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      igImage=(IGImage)session.get(IGImageImpl.class,Long.valueOf(imageId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (igImage == null)) {
        EntityCacheUtil.putResult(IGImageModelImpl.ENTITY_CACHE_ENABLED,IGImageImpl.class,imageId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(igImage);
      }
      closeSession(session);
    }
  }
  return igImage;
}
