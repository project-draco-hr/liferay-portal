{
  IGImage igImage=(IGImage)EntityCacheUtil.getResult(IGImageModelImpl.ENTITY_CACHE_ENABLED,IGImageImpl.class,imageId,this);
  if (igImage == _nullIGImage) {
    return null;
  }
  if (igImage == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      igImage=(IGImage)session.get(IGImageImpl.class,Long.valueOf(imageId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (!hasException && (igImage == null)) {
        EntityCacheUtil.putResult(IGImageModelImpl.ENTITY_CACHE_ENABLED,IGImageImpl.class,imageId,_nullIGImage);
      }
 else {
        cacheResult(igImage);
      }
      closeSession(session);
    }
  }
  return igImage;
}
