{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(igImage);
    }
 else {
      if (igImage.isNew()) {
        session.save(igImage);
      }
 else {
        igImage=(IGImage)session.merge(igImage);
      }
    }
    session.flush();
    igImage.setNew(false);
    return igImage;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(IGImage.class.getName());
  }
}
