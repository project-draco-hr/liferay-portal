{
  ServiceContext previousServiceContext=ServiceContextThreadLocal.getServiceContext();
  ServiceContextThreadLocal.setServiceContext(serviceContext);
  try {
    return doUpdateUser(userId,oldPassword,newPassword1,newPassword2,passwordReset,reminderQueryQuestion,reminderQueryAnswer,screenName,emailAddress,openId,languageId,timeZoneId,greeting,comments,firstName,middleName,lastName,prefixId,suffixId,male,birthdayMonth,birthdayDay,birthdayYear,smsSn,aimSn,facebookSn,icqSn,jabberSn,msnSn,mySpaceSn,skypeSn,twitterSn,ymSn,jobTitle,groupIds,organizationIds,roleIds,userGroupRoles,userGroupIds,serviceContext);
  }
  finally {
    ServiceContextThreadLocal.setServiceContext(previousServiceContext);
  }
}
