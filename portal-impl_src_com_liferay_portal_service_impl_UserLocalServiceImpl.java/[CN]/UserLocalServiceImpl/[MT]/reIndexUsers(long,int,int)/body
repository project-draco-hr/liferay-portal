{
  List<User> users=userPersistence.findByCompanyId(companyId,start,end);
  for (  User user : users) {
    if (user.isDefaultUser()) {
      continue;
    }
    try {
      UserIndexer.updateUser(user);
    }
 catch (    SearchException se) {
      _log.error("Reindexing " + user.getUserId(),se);
    }
  }
}
