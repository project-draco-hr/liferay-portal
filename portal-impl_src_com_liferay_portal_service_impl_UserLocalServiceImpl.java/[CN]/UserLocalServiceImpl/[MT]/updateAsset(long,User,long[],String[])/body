{
  User owner=userPersistence.findByPrimaryKey(userId);
  Company company=companyPersistence.findByPrimaryKey(owner.getCompanyId());
  Group companyGroup=company.getGroup();
  assetEntryLocalService.updateEntry(userId,companyGroup.getGroupId(),User.class.getName(),user.getUserId(),user.getUuid(),0,assetCategoryIds,assetTagNames,false,null,null,null,null,null,user.getFullName(),null,null,null,null,0,0,null,false);
}
