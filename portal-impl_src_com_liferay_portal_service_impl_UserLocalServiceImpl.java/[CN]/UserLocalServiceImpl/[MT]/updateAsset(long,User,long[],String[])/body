{
  User owner=userPersistence.findByPrimaryKey(userId);
  Company company=companyPersistence.findByPrimaryKey(owner.getCompanyId());
  Group global=company.getGroup();
  assetEntryLocalService.updateEntry(userId,global.getGroupId(),User.class.getName(),user.getUserId(),assetCategoryIds,assetTagNames,true,null,null,null,null,null,user.getFullName(),null,null,null,0,0,null,false);
}
