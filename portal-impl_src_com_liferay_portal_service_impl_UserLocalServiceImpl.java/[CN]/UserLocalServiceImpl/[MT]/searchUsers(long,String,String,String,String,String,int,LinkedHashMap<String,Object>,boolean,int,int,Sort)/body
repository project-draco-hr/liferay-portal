{
  boolean corruptIndex=false;
  Hits hits=null;
  Tuple tuple=null;
  do {
    hits=search(companyId,firstName,middleName,lastName,screenName,emailAddress,status,params,andSearch,start,end,sort);
    try {
      tuple=UsersAdminUtil.getUsers(hits);
    }
 catch (    PortalException pe) {
      throw new SystemException(pe);
    }
    corruptIndex=(Boolean)tuple.getObject(1);
  }
 while (corruptIndex);
  return new BaseModelSearchResult<User>((List<User>)tuple.getObject(0),hits.getLength());
}
