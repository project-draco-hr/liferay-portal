{
  if (!PropsValues.USERS_INDEXER_ENABLED || !PropsValues.USERS_SEARCH_WITH_INDEX || isUseCustomSQL(params)) {
    return userFinder.findByC_FN_MN_LN_SN_EA_S(companyId,firstName,middleName,lastName,screenName,emailAddress,status,params,andSearch,start,end,obc);
  }
  try {
    return UsersAdminUtil.getUsers(search(companyId,firstName,middleName,lastName,screenName,emailAddress,status,params,andSearch,start,end,getSortsFromOrderByComparator(obc)));
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
