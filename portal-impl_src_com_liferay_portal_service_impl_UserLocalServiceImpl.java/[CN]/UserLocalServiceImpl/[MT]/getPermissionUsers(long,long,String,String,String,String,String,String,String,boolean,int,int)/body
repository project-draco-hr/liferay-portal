{
  int orgGroupPermissionsCount=permissionUserFinder.countByOrgGroupPermissions(companyId,name,primKey,actionId);
  if (orgGroupPermissionsCount > 0) {
    return permissionUserFinder.findByUserAndOrgGroupPermission(companyId,name,primKey,actionId,firstName,middleName,lastName,emailAddress,andOperator,start,end);
  }
 else {
    return permissionUserFinder.findByPermissionAndRole(companyId,groupId,name,primKey,actionId,firstName,middleName,lastName,emailAddress,andOperator,start,end);
  }
}
