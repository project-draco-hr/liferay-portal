{
  if (!PropsValues.USERS_INDEXER_ENABLED || !PropsValues.USERS_SEARCH_WITH_INDEX || isUseCustomSQL(params)) {
    return userFinder.findByKeywords(companyId,keywords,status,params,start,end,obc);
  }
  try {
    return UsersAdminUtil.getUsers(search(companyId,keywords,status,params,start,end,getSortFromComparator(obc)));
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
