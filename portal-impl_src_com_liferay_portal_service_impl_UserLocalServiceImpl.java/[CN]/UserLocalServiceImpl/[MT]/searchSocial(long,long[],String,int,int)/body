{
  LinkedHashMap<String,Object> params=new LinkedHashMap<String,Object>();
  params.put("usersGroups",ArrayUtil.toLongArray(groupIds));
  params.put("wildcardMode",WildcardMode.TRAILING);
  return userFinder.findByKeywords(companyId,keywords,WorkflowConstants.STATUS_APPROVED,params,start,end,null);
}
