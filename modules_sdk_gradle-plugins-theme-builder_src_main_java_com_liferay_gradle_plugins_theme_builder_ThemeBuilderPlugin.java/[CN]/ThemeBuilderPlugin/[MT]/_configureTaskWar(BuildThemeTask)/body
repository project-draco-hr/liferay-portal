{
  War war=(War)GradleUtil.getTask(buildThemeTask.getProject(),WarPlugin.WAR_TASK_NAME);
  war.dependsOn(buildThemeTask);
  war.exclude(new Spec<FileTreeElement>(){
    @Override public boolean isSatisfiedBy(    FileTreeElement fileTreeElement){
      File diffsDir=buildThemeTask.getDiffsDir();
      if ((diffsDir != null) && FileUtil.isChild(fileTreeElement.getFile(),diffsDir)) {
        return true;
      }
      return false;
    }
  }
);
}
