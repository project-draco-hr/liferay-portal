{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.USERS_SCREEN_NAME_GENERATOR);
  }
  ClassLoader classLoader=PACLClassLoaderUtil.getPortalClassLoader();
  _originalScreenNameGenerator=(ScreenNameGenerator)InstanceFactory.newInstance(classLoader,PropsValues.USERS_SCREEN_NAME_GENERATOR);
  _screenNameGenerator=_originalScreenNameGenerator;
}
