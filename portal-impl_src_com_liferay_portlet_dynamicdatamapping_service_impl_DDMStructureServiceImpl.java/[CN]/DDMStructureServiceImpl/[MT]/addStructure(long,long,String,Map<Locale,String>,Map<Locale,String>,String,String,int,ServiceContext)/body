{
  String ddmResource=GetterUtil.getString(serviceContext.getAttribute("ddmResource"));
  DDMPermission.check(getPermissionChecker(),serviceContext.getScopeGroupId(),ddmResource,ActionKeys.ADD_STRUCTURE);
  return ddmStructureLocalService.addStructure(getUserId(),groupId,classNameId,structureKey,nameMap,descriptionMap,xsd,storageType,type,serviceContext);
}
