{
  DDMDisplay ddmDisplay=DDMUtil.getDDMDisplay(classNameId);
  DDMPermission.check(getPermissionChecker(),serviceContext.getScopeGroupId(),ddmDisplay.getResourceName(),ddmDisplay.getAddStructureActionId());
  return ddmStructureLocalService.addStructure(getUserId(),groupId,classNameId,nameMap,descriptionMap,ddmForm,ddmFormLayout,serviceContext);
}
