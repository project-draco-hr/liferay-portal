{
  DDMDisplay ddmDisplay=DDMUtil.getDDMDisplay(serviceContext);
  DDMPermission.check(getPermissionChecker(),serviceContext.getScopeGroupId(),ddmDisplay.getResourceName(),ddmDisplay.getAddStructureActionId());
  return ddmStructureLocalService.addStructure(userId,groupId,parentStructureKey,classNameId,structureKey,nameMap,descriptionMap,ddmForm,storageType,type,serviceContext);
}
