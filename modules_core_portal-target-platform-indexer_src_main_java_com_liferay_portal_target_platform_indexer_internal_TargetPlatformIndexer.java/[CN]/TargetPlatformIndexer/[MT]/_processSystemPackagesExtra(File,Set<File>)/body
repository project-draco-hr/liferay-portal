{
  URL url=_systemBundle.getResource("META-INF/system.packages.extra.mf");
  try (Jar jar=new Jar("system.packages.extra");InputStream inputStream=url.openStream()){
    Manifest manifest=new Manifest(inputStream);
    jar.setManifest(manifest);
    File jarFile=new File(tempDir,"system.packages.extra.jar");
    jar.write(jarFile);
    jarFiles.add(jarFile);
  }
 }
