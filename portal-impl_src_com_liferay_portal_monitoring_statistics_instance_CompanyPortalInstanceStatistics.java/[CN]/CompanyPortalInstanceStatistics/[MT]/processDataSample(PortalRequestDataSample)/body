{
  if (dataSample.getCompanyId() != _companyId) {
    return;
  }
  RequestStatus requestStatus=dataSample.getRequestStatus();
  if (requestStatus.equals(RequestStatus.ERROR)) {
    _requestStatistics.getErrorStatistics().incrementCount();
  }
 else   if (requestStatus.equals(RequestStatus.TIMEOUT)) {
    _requestStatistics.getTimeCountStatistics().incrementCount();
  }
 else   if (requestStatus.equals(RequestStatus.SUCCESS)) {
    _requestStatistics.getSuccessStatistics().addDuration(dataSample.getDuration());
  }
}
