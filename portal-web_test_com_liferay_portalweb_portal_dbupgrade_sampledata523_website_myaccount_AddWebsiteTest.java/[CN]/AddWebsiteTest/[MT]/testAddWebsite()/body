{
  selenium.open("/web/guest/home");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=My Account",RuntimeVariables.replace("My Account"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//a[@id='websitesLink']",RuntimeVariables.replace("Websites"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//input[@id='_2_websiteUrl0']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.type("//input[@id='_2_websiteUrl0']",RuntimeVariables.replace("http://www.liferay.com"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request processed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  selenium.clickAt("//a[@id='websitesLink']",RuntimeVariables.replace("Websites"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//input[@id='_2_websiteUrl0']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals("http://www.liferay.com",selenium.getValue("//input[@id='_2_websiteUrl0']"));
}
