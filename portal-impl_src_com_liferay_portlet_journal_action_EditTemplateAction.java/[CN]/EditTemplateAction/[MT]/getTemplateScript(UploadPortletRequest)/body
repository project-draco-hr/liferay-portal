{
  String xsl=null;
  try {
    InputStream is=uploadPortletRequest.getFileAsStream("xsl");
    if (is != null) {
      xsl=new String(FileUtil.getBytes(is));
      is.close();
    }
  }
 catch (  IOException ioe) {
    if (_log.isWarnEnabled()) {
      _log.warn(ioe,ioe);
    }
  }
  return xsl;
}
