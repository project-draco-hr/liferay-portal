{
  long groupId=ParamUtil.getLong(actionRequest,"groupId");
  String[] deleteTemplateIds=StringUtil.split(ParamUtil.getString(actionRequest,"deleteTemplateIds"));
  for (int i=0; i < deleteTemplateIds.length; i++) {
    JournalTemplateServiceUtil.deleteTemplate(groupId,deleteTemplateIds[i]);
    JournalUtil.removeRecentTemplate(actionRequest,deleteTemplateIds[i]);
  }
}
