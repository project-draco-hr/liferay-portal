{
  if (actionIds.length == 0) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(actionIds.length * 2 - 1);
  for (int i=0; i < actionIds.length; i++) {
    sb.append("Permission_.actionId = ?");
    if ((i + 1) < actionIds.length) {
      sb.append(" OR ");
    }
  }
  return sb.toString();
}
