{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select groupId from Group_ where typeSettings like " + "'%staged=true%'");
    rs=ps.executeQuery();
    List<Long> stagedGroupIds=new ArrayList<>();
    while (rs.next()) {
      long stagedGroupId=rs.getLong("groupId");
      stagedGroupIds.add(stagedGroupId);
    }
    return stagedGroupIds;
  }
  finally {
    DataAccess.cleanUp(null,ps,rs);
  }
}
