{
  if (((timeZoneId == null) && (_timeZoneId != null)) || ((timeZoneId != null) && (_timeZoneId == null)) || ((timeZoneId != null) && (_timeZoneId != null) && !timeZoneId.equals(_timeZoneId))) {
    if (!XSS_ALLOW_TIMEZONEID) {
      timeZoneId=XSSUtil.strip(timeZoneId);
    }
    _timeZoneId=timeZoneId;
  }
}
