{
  if (((lastLoginIP == null) && (_lastLoginIP != null)) || ((lastLoginIP != null) && (_lastLoginIP == null)) || ((lastLoginIP != null) && (_lastLoginIP != null) && !lastLoginIP.equals(_lastLoginIP))) {
    if (!XSS_ALLOW_LASTLOGINIP) {
      lastLoginIP=XSSUtil.strip(lastLoginIP);
    }
    _lastLoginIP=lastLoginIP;
  }
}
