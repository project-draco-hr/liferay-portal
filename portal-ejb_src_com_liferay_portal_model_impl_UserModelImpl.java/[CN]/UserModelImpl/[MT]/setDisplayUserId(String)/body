{
  if (((displayUserId == null) && (_displayUserId != null)) || ((displayUserId != null) && (_displayUserId == null)) || ((displayUserId != null) && (_displayUserId != null) && !displayUserId.equals(_displayUserId))) {
    if (!XSS_ALLOW_DISPLAYUSERID) {
      displayUserId=XSSUtil.strip(displayUserId);
    }
    _displayUserId=displayUserId;
  }
}
