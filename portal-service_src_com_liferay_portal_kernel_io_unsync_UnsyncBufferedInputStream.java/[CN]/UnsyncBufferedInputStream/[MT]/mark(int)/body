{
  if (readLimit <= 0) {
    return;
  }
  markLimitIndex=readLimit;
  if (index == 0) {
    return;
  }
  int available=firstInvalidIndex - index;
  if (available > 0) {
    System.arraycopy(buffer,index,buffer,0,available);
    index=0;
    firstInvalidIndex=available;
  }
 else {
    index=firstInvalidIndex=0;
  }
}
