{
  if (xml.indexOf("<wsdl:definitions") == -1) {
    return xml;
  }
  xml=StringUtil.replace(xml,new String[]{"\r\n","\n","  ","> <",_INCORRECT_LONG_ARRAY,_INCORRECT_STRING_ARRAY},new String[]{StringPool.BLANK,StringPool.BLANK,StringPool.BLANK,"><",_CORRECT_LONG_ARRAY,_CORRECT_STRING_ARRAY});
  Document document=SAXReaderUtil.read(xml);
  return document.formattedString();
}
