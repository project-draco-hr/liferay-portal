{
  selenium.select("_26_id",RuntimeVariables.replace("label=German to French"));
  selenium.click(RuntimeVariables.replace("//input[@value='Translate']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Mon nom est un traducteur coulant des Liferay, sous plus de 6 millions de formes de communication."));
}
