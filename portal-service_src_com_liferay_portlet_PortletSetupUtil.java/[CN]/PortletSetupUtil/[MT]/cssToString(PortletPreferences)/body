{
  String css=portletSetup.getValue("portlet-setup-css",StringPool.BLANK);
  try {
    if (Validator.isNotNull(css)) {
      return _toJSONObject(portletSetup,css).toString();
    }
  }
 catch (  Exception e) {
    css=null;
    if (_log.isWarnEnabled()) {
      _log.warn(e);
    }
  }
  return css;
}
