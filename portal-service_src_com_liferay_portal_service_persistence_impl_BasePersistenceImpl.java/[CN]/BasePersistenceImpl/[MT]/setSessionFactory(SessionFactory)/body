{
  _sessionFactory=sessionFactory;
  _dialect=_sessionFactory.getDialect();
  _db=DBManagerUtil.getDB(_dialect,getDataSource());
  DBType dbType=_db.getDBType();
  _databaseOrderByMaxColumns=GetterUtil.getInteger(PropsUtil.get(PropsKeys.DATABASE_ORDER_BY_MAX_COLUMNS,new Filter(dbType.getName())));
}
