{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Announcements Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Announcements Test Page",RuntimeVariables.replace("Announcements Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Show"),selenium.getText("//td[@class='control-entry']/a"));
  selenium.clickAt("//td[@class='control-entry']/a",RuntimeVariables.replace("Show"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (RuntimeVariables.replace("Hide").equals(selenium.getText("//td[@class='control-entry']/a"))) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Hide"),selenium.getText("//td[@class='control-entry']/a"));
  assertTrue(selenium.isVisible("//p"));
}
