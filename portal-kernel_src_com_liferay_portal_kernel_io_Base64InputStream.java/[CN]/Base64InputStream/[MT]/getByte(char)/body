{
  if ((character >= CharPool.UPPER_CASE_A) && (character <= CharPool.UPPER_CASE_Z)) {
    return character - 65;
  }
  if ((character >= CharPool.LOWER_CASE_A) && (character <= CharPool.LOWER_CASE_Z)) {
    return (character - 97) + 26;
  }
  if ((character >= CharPool.NUMBER_0) && (character <= CharPool.NUMBER_9)) {
    return (character - 48) + 52;
  }
  if (character == CharPool.PLUS) {
    return 62;
  }
  if (character == CharPool.SLASH) {
    return 63;
  }
  if (character != CharPool.EQUAL) {
    return -1;
  }
 else {
    return 0;
  }
}
