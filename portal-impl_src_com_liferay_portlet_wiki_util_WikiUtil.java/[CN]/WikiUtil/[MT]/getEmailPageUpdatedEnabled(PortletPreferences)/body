{
  String emailPageUpdatedEnabled=preferences.getValue("emailPageUpdatedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailPageUpdatedEnabled)) {
    return GetterUtil.getBoolean(emailPageUpdatedEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.WIKI_EMAIL_PAGE_UPDATED_ENABLED));
  }
}
