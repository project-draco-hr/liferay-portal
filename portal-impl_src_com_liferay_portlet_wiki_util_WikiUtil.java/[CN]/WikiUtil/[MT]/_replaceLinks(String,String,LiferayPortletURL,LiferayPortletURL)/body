{
  pageURL.setParameter("title",title,false);
  String pageURLToString=pageURL.toString();
  content=StringUtil.replace(content,"[$BEGIN_PAGE_TITLE$]" + title + "[$END_PAGE_TITLE$]",pageURLToString);
  editURL.setParameter("title",title,false);
  String editURLToString=editURL.toString();
  content=StringUtil.replace(content,"[$BEGIN_PAGE_TITLE_EDIT$]" + title + "[$END_PAGE_TITLE_EDIT$]",editURLToString);
  return content;
}
