{
  viewPageURL.setParameter("title",title,false);
  content=StringUtil.replace(content,"[$BEGIN_PAGE_TITLE$]" + title + "[$END_PAGE_TITLE$]",viewPageURL.toString());
  editPageURL.setParameter("title",title,false);
  content=StringUtil.replace(content,"[$BEGIN_PAGE_TITLE_EDIT$]" + title + "[$END_PAGE_TITLE_EDIT$]",editPageURL.toString());
  return content;
}
