{
  ThemeDisplay themeDisplay=(ThemeDisplay)renderRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long groupId=themeDisplay.getScopeGroupId();
  String allNodes=ListUtil.toString(WikiNodeLocalServiceUtil.getNodes(groupId),"name");
  String[] visibleNodes=StringUtil.split(renderRequest.getPreferences().getValue("visible-nodes",allNodes));
  String[] hiddenNodes=StringUtil.split(renderRequest.getPreferences().getValue("hidden-nodes",StringPool.BLANK));
  return getNodes(groupId,visibleNodes,hiddenNodes,themeDisplay.getPermissionChecker());
}
