{
  List<WikiNode> orderedNodes=new ArrayList<WikiNode>();
  for (  String nodeName : nodeNames) {
    for (    WikiNode node : nodes) {
      if (node.getName().equals(nodeName)) {
        orderedNodes.add(node);
        nodes.remove(node);
        break;
      }
    }
  }
  orderedNodes.addAll(nodes);
  return orderedNodes;
}
