{
  List<WikiNode> nodes=WikiNodeLocalServiceUtil.getNodes(groupId);
  nodes=ListUtil.sort(nodes,new VisibleNodesComparator(visibleNodes));
  Iterator<WikiNode> itr=nodes.iterator();
  while (itr.hasNext()) {
    WikiNode node=itr.next();
    if (ArrayUtil.contains(hiddenNodes,node.getName()) || !WikiNodePermission.contains(permissionChecker,node.getNodeId(),ActionKeys.VIEW)) {
      itr.remove();
    }
  }
  return nodes;
}
