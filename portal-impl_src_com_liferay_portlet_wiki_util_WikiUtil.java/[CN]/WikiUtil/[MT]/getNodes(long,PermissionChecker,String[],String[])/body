{
  List nodes=WikiNodeLocalServiceUtil.getNodes(groupId);
  Iterator itr=nodes.iterator();
  while (itr.hasNext()) {
    WikiNode curNode=(WikiNode)itr.next();
    if (ArrayUtil.contains(hiddenNodes,curNode.getName()) || !WikiNodePermission.contains(permissionChecker,curNode.getNodeId(),ActionKeys.VIEW)) {
      itr.remove();
    }
  }
  Collections.sort(nodes,new NodeListComparator(visibleNodes));
  return nodes;
}
