{
  String emailPageUpdatedSubject=prefs.getValue("email-page-updated-subject-prefix",StringPool.BLANK);
  if (Validator.isNotNull(emailPageUpdatedSubject)) {
    return emailPageUpdatedSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsUtil.WIKI_EMAIL_PAGE_UPDATED_SUBJECT_PREFIX));
  }
}
