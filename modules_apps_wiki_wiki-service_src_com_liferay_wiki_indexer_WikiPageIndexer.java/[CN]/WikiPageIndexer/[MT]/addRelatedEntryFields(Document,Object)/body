{
  long classPK=0;
  if (obj instanceof Comment) {
    Comment comment=(Comment)obj;
    classPK=comment.getClassPK();
  }
 else   if (obj instanceof FileEntry) {
    FileEntry fileEntry=(FileEntry)obj;
    Repository repository=_repositoryProvider.getRepository(fileEntry.getRepositoryId());
    RelatedModelCapability relatedModelCapability=repository.getCapability(RelatedModelCapability.class);
    classPK=relatedModelCapability.getClassPK(fileEntry);
  }
  WikiPage page=null;
  try {
    page=WikiPageLocalServiceUtil.getPage(classPK);
  }
 catch (  Exception e) {
    return;
  }
  document.addKeyword(Field.NODE_ID,page.getNodeId());
}
