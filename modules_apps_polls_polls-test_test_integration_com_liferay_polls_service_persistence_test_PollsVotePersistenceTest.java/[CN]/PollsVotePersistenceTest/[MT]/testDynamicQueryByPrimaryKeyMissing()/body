{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PollsVote.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("voteId",RandomTestUtil.nextLong()));
  List<PollsVote> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
