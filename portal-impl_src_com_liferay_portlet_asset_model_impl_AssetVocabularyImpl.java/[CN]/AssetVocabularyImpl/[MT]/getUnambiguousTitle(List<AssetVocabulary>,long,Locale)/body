{
  if (getGroupId() == groupId) {
    return getTitle(locale);
  }
  boolean hasAmbiguousVocabularies=ListUtil.exists(vocabularies,new PredicateFilter<AssetVocabulary>(){
    @Override public boolean filter(    AssetVocabulary curVocabulary){
      String curVocabularyTitle=curVocabulary.getTitle(locale);
      if (curVocabularyTitle.equals(getTitle(locale)) && (curVocabulary.getVocabularyId() != getVocabularyId())) {
        return true;
      }
      return false;
    }
  }
);
  if (hasAmbiguousVocabularies) {
    Group vocabularyGroup=GroupLocalServiceUtil.getGroup(getGroupId());
    StringBundler sb=new StringBundler(5);
    sb.append(getTitle(locale));
    sb.append(StringPool.SPACE);
    sb.append(StringPool.OPEN_PARENTHESIS);
    sb.append(vocabularyGroup.getDescriptiveName(locale));
    sb.append(StringPool.CLOSE_PARENTHESIS);
    return sb.toString();
  }
  return getTitle(locale);
}
