{
  if (_settingsProperties == null) {
    _settingsProperties=getSettingsProperties();
  }
  String[] settingValueIds=StringUtil.split(_settingsProperties.getProperty(settingName),StringPool.COMMA);
  if (settingValueIds.length == 0) {
    return false;
  }
  return ArrayUtil.contains(settingValueIds,AssetCategoryConstants.ALL_CLASS_NAME_IDS);
}
