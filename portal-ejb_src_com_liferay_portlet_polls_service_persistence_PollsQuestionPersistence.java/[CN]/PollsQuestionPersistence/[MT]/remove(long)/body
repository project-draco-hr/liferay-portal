{
  Session session=null;
  try {
    session=openSession();
    PollsQuestion pollsQuestion=(PollsQuestion)session.get(PollsQuestionImpl.class,new Long(questionId));
    if (pollsQuestion == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No PollsQuestion exists with the primary key " + questionId);
      }
      throw new NoSuchQuestionException("No PollsQuestion exists with the primary key " + questionId);
    }
    return remove(pollsQuestion);
  }
 catch (  NoSuchQuestionException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
