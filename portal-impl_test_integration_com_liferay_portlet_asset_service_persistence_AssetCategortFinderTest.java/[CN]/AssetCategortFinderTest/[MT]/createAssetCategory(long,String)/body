{
  AssetVocabulary assetVocabulary=AssetVocabularyLocalServiceUtil.addVocabulary(TestPropsValues.getUserId(),ServiceTestUtil.randomString(),ServiceTestUtil.getServiceContext(groupId));
  Map<Locale,String> titleMap=new HashMap<Locale,String>();
  titleMap.put(LocaleUtil.getDefault(),name);
  return AssetCategoryLocalServiceUtil.addCategory(TestPropsValues.getUserId(),AssetCategoryConstants.DEFAULT_PARENT_CATEGORY_ID,titleMap,null,assetVocabulary.getVocabularyId(),null,ServiceTestUtil.getServiceContext(groupId));
}
