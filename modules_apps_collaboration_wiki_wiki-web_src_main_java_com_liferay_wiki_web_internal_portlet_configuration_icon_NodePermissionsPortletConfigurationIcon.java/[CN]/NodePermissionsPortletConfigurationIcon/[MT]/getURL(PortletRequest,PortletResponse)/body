{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  String url=StringPool.BLANK;
  try {
    WikiNode node=ActionUtil.getNode(portletRequest);
    url=PermissionsURLTag.doTag(StringPool.BLANK,WikiNode.class.getName(),node.getName(),null,String.valueOf(node.getNodeId()),LiferayWindowState.POP_UP.toString(),null,themeDisplay.getRequest());
  }
 catch (  Exception e) {
  }
  return url;
}
