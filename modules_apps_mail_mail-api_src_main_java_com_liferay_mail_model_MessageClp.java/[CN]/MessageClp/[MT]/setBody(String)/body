{
  _body=body;
  if (_messageRemoteModel != null) {
    try {
      Class<?> clazz=_messageRemoteModel.getClass();
      Method method=clazz.getMethod("setBody",String.class);
      method.invoke(_messageRemoteModel,body);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
