{
  ExpandoRow newExpandoRow=addExpandoRow();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ExpandoRow.class,ExpandoRow.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("rowId",nextLong()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
