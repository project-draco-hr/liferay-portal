{
  EntityCacheUtil.putResult(DDLRecordModelImpl.ENTITY_CACHE_ENABLED,DDLRecordImpl.class,ddlRecord.getPrimaryKey(),ddlRecord);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddlRecord.getUuid(),Long.valueOf(ddlRecord.getGroupId())},ddlRecord);
  ddlRecord.resetOriginalValues();
}
