{
  try {
    if (_initalized) {
      return;
    }
    Field defaultTransformersField=TransformerUtil.class.getDeclaredField("defaultTransformers");
    defaultTransformersField.setAccessible(true);
    TypeTransformerMap oldTransformersMap=TransformerUtil.getDefaultTypeTransformers();
    TypeTransformerMap newTransformersMap=new TypeTransformerMap();
    for (    Map.Entry<Class,Transformer> entry : oldTransformersMap.entrySet()) {
      newTransformersMap.put(entry.getKey(),entry.getValue());
    }
    _registerDefaultTransformers(newTransformersMap);
    defaultTransformersField.set(null,newTransformersMap);
    Field modifiersField=Field.class.getDeclaredField("modifiers");
    modifiersField.setAccessible(true);
    modifiersField.setInt(defaultTransformersField,defaultTransformersField.getModifiers() & ~Modifier.FINAL);
    _initalized=true;
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
