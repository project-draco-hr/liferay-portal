{
  InputStream inputStream=null;
  try {
    URL url=classLoader.getResource("META-INF/javadocs.xml");
    if (url == null) {
      return null;
    }
    inputStream=url.openStream();
    return SAXReaderUtil.read(inputStream,true);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
 finally {
    StreamUtil.cleanUp(inputStream);
  }
  return null;
}
