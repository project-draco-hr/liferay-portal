{
  Repository repository=getRepository(classPK);
  List<Folder> folders=repository.getFolders(classPK,false,start,end,null);
  List<TrashRenderer> trashContainerModels=new ArrayList<TrashRenderer>();
  for (  Folder folder : folders) {
    TrashHandler trashHandler=TrashHandlerRegistryUtil.getTrashHandler(DLFolder.class.getName());
    TrashRenderer trashRenderer=trashHandler.getTrashRenderer(folder.getPrimaryKey());
    trashContainerModels.add(trashRenderer);
  }
  return trashContainerModels;
}
