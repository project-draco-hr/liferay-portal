{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long[] deleteCouponIds=StringUtil.split(ParamUtil.getString(actionRequest,"deleteCouponIds"),0L);
  for (int i=0; i < deleteCouponIds.length; i++) {
    ShoppingCouponServiceUtil.deleteCoupon(themeDisplay.getScopeGroupId(),deleteCouponIds[i]);
  }
}
