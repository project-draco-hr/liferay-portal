{
  InvokerFilter invokerFilter=new InvokerFilter();
  MockHttpServletRequest mockHttpServletRequest=new MockHttpServletRequest(HttpMethods.GET,"/c/portal/login;jsessionid=ae01b0f2af.worker1");
  String originalURI=invokerFilter.getOriginalRequestURI(mockHttpServletRequest);
  Assert.assertEquals("/c/portal/login",invokerFilter.getURI(mockHttpServletRequest,originalURI));
}
