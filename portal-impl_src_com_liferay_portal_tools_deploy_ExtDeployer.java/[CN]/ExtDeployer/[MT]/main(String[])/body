{
  ToolDependencies.wireDeployers();
  List<String> wars=new ArrayList<>();
  List<String> jars=new ArrayList<>();
  for (  String arg : args) {
    if (arg.endsWith(".war")) {
      wars.add(arg);
    }
 else     if (arg.endsWith(".jar")) {
      jars.add(arg);
    }
  }
  new ExtDeployer(wars,jars);
}
