selenium.clickAt("//span[@title='Actions']/ul/li/strong/a/span",RuntimeVariables.replace("Actions"));
selenium.waitForText("//div[@class='lfr-component lfr-menu-list']/ul/li/a[contains(.,'Delete')]","Delete");
assertEquals(RuntimeVariables.replace("Delete"),selenium.getText("//div[@class='lfr-component lfr-menu-list']/ul/li/a[contains(.,'Delete')]"));
selenium.clickAt("//div[@class='lfr-component lfr-menu-list']/ul/li/a[contains(.,'Delete')]",RuntimeVariables.replace("Delete"));
assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete the selected vocabularies[\\s\\S]$"));
selenium.waitForElementNotPresent("//input[@name='vocabulary-item-check']");
assertTrue(selenium.isElementNotPresent("//input[@name='vocabulary-item-check']"));
case 2:
case 100:
