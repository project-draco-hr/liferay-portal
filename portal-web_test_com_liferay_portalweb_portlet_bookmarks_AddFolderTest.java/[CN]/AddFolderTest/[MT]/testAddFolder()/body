{
  selenium.click("//input[@value='Add Folder']");
  selenium.waitForPageToLoad("30000");
  selenium.type("_28_name","Test Folder");
  selenium.typeKeys("_28_description","This is a test folder!");
  selenium.click("//input[@value='Save']");
  selenium.waitForPageToLoad("30000");
  selenium.click("//b");
  selenium.waitForPageToLoad("30000");
}
