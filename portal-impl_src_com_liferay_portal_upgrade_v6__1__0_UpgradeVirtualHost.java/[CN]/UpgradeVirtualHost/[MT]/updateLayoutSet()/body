{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select layoutSetId, companyId, virtualHost from " + "LayoutSet where virtualHost != ?");
    ps.setString(1,StringPool.BLANK);
    rs=ps.executeQuery();
    while (rs.next()) {
      long layoutSetId=rs.getLong("layoutSetId");
      long companyId=rs.getLong("companyId");
      String virtualHostName=rs.getString("virtualHost");
      addVirtualHost(companyId,layoutSetId,virtualHostName);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  runSQL("alter table LayoutSet drop column virtualHost");
}
