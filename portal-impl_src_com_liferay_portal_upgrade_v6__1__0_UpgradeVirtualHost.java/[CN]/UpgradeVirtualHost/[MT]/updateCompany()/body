{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select companyId, virtualHost from " + "Company where virtualHost != ''");
    rs=ps.executeQuery();
    while (rs.next()) {
      long companyId=rs.getLong("companyId");
      String virtualHostName=rs.getString("virtualHost");
      addVirtualHost(companyId,0,virtualHostName);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  runSQL("alter table Company drop column virtualHost");
}
