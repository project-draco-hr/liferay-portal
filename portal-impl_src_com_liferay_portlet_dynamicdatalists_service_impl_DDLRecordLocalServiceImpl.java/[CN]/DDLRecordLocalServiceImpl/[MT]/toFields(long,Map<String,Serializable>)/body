{
  DDMStructure ddmStructure=ddmStructureLocalService.getDDMStructure(ddmStructureId);
  Fields fields=new Fields();
  for (  String name : fieldsMap.keySet()) {
    String value=String.valueOf(fieldsMap.get(name));
    Field field=new Field(name,value,ddmStructure.getFieldDataType(name));
    fields.put(field);
  }
  return fields;
}
