{
  Object[] finderArgs=new Object[]{groupId,userId,active,type};
  Long count=(Long)FinderCacheUtil.getResult(FINDER_PATH_COUNT_BY_G_U_A_T,finderArgs,this);
  if (count == null) {
    Session session=null;
    try {
      session=openSession();
      StringBundler query=new StringBundler(5);
      query.append(_SQL_COUNT_SOCIALEQUITYLOG_WHERE);
      query.append(_FINDER_COLUMN_G_U_A_T_GROUPID_2);
      query.append(_FINDER_COLUMN_G_U_A_T_USERID_2);
      query.append(_FINDER_COLUMN_G_U_A_T_ACTIVE_2);
      query.append(_FINDER_COLUMN_G_U_A_T_TYPE_2);
      String sql=query.toString();
      Query q=session.createQuery(sql);
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      qPos.add(userId);
      qPos.add(active);
      qPos.add(type);
      count=(Long)q.uniqueResult();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (count == null) {
        count=Long.valueOf(0);
      }
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_G_U_A_T,finderArgs,count);
      closeSession(session);
    }
  }
  return count.intValue();
}
