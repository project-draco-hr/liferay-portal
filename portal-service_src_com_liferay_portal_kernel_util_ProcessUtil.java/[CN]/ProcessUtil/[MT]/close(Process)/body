{
  try {
    p.waitFor();
  }
 catch (  InterruptedException ie) {
    ie.printStackTrace();
  }
  try {
    p.getInputStream().close();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
  try {
    p.getOutputStream().close();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
  try {
    p.getErrorStream().close();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
}
