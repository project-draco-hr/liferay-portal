{
  AtomicInteger counter=registerDLSyncEventProcessorMessageListener(DLSyncConstants.EVENT_UPDATE);
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  Folder folder=DLAppServiceUtil.addFolder(group.getGroupId(),parentFolder.getFolderId(),RandomTestUtil.randomString(),RandomTestUtil.randomString(),serviceContext);
  DLAppServiceUtil.updateFolder(folder.getFolderId(),folder.getName(),folder.getDescription(),serviceContext);
  Assert.assertEquals(1,counter.get());
}
