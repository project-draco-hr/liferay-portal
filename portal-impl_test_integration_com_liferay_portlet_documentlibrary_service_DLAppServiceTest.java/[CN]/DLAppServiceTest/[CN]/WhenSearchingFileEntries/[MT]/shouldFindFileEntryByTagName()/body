{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  String[] assetTagNames=new String[]{"hello","world"};
  serviceContext.setAssetTagNames(assetTagNames);
  String name="TestTags.txt";
  FileEntry fileEntry=DLAppServiceUtil.addFileEntry(group.getGroupId(),parentFolder.getFolderId(),name,ContentTypes.TEXT_PLAIN,name,StringPool.BLANK,StringPool.BLANK,CONTENT.getBytes(),serviceContext);
  search(fileEntry,false,"hello",true);
  search(fileEntry,false,"world",true);
  search(fileEntry,false,"liferay",false);
}
