{
  int i=RandomTestUtil.randomInt() % PropsValues.DL_CHAR_BLACKLIST.length;
  String blackListedChar=PropsValues.DL_CHAR_BLACKLIST[i];
  String sourceFileName=RandomTestUtil.randomString() + blackListedChar + RandomTestUtil.randomString();
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  DLAppServiceUtil.addFileEntry(group.getGroupId(),parentFolder.getFolderId(),sourceFileName,ContentTypes.TEXT_PLAIN,sourceFileName,StringPool.BLANK,StringPool.BLANK,RandomTestUtil.randomBytes(),serviceContext);
}
