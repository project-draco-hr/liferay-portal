{
  int i=RandomTestUtil.randomInt() % PropsValues.DL_CHAR_BLACKLIST.length;
  String blackListedChar=PropsValues.DL_CHAR_BLACKLIST[i];
  String sourceFileName=RandomTestUtil.randomString() + blackListedChar + RandomTestUtil.randomString();
  addFileEntry(group.getGroupId(),parentFolder.getFolderId(),sourceFileName);
}
