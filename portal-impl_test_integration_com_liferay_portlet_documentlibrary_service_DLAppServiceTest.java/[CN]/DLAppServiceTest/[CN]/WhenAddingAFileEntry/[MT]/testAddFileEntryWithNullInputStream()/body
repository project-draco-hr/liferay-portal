{
  long folderId=parentFolder.getFolderId();
  String description=StringPool.BLANK;
  String changeLog=StringPool.BLANK;
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  try {
    String name="IS-null.txt";
    InputStream is=null;
    FileEntry fileEntry=DLAppServiceUtil.addFileEntry(group.getGroupId(),folderId,name,ContentTypes.TEXT_PLAIN,name,description,changeLog,is,0,serviceContext);
    DLAppServiceUtil.updateFileEntry(fileEntry.getFileEntryId(),name,ContentTypes.TEXT_PLAIN,name,description,changeLog,true,is,0,serviceContext);
    try {
      String newName="IS-changed.txt";
      is=new ByteArrayInputStream(CONTENT.getBytes());
      DLAppServiceUtil.updateFileEntry(fileEntry.getFileEntryId(),newName,ContentTypes.TEXT_PLAIN,newName,description,changeLog,true,is,0,serviceContext);
    }
  finally {
      if (is != null) {
        is.close();
      }
    }
  }
 catch (  Exception e) {
    Assert.fail("Unable to pass null InputStream " + StackTraceUtil.getStackTrace(e));
  }
}
