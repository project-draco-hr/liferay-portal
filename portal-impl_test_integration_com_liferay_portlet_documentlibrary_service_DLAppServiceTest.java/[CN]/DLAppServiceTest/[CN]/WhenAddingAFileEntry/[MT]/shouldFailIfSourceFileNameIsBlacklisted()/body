{
  int i=RandomTestUtil.randomInt() % PropsValues.DL_NAME_BLACKLIST.length;
  String blackListedName=PropsValues.DL_NAME_BLACKLIST[i];
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  DLAppServiceUtil.addFileEntry(group.getGroupId(),parentFolder.getFolderId(),blackListedName,ContentTypes.TEXT_PLAIN,blackListedName,StringPool.BLANK,StringPool.BLANK,RandomTestUtil.randomBytes(),serviceContext);
}
