{
  try (WorkflowHandlerInvocationCounter<DLFileEntry> invocationCounter=new WorkflowHandlerInvocationCounter<>(DLFileEntryConstants.getClassName())){
    addFileEntry(group.getGroupId(),parentFolder.getFolderId());
    Assert.assertEquals(1,invocationCounter.getCount("updateStatus",int.class,Map.class));
  }
