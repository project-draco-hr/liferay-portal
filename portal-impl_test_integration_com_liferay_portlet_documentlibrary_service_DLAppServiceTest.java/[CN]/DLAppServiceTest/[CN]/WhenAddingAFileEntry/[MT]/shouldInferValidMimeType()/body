{
  String fileName=RandomTestUtil.randomString();
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  FileEntry fileEntry=DLAppServiceUtil.addFileEntry(group.getGroupId(),parentFolder.getFolderId(),fileName,ContentTypes.APPLICATION_OCTET_STREAM,fileName,StringPool.BLANK,StringPool.BLANK,CONTENT.getBytes(),serviceContext);
  Assert.assertEquals(ContentTypes.TEXT_PLAIN,fileEntry.getMimeType());
}
