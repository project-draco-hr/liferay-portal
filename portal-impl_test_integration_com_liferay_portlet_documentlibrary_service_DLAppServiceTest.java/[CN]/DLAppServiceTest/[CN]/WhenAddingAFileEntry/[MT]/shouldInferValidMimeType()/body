{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  String name="InvalidMime.txt";
  FileEntry fileEntry=DLAppServiceUtil.addFileEntry(group.getGroupId(),parentFolder.getFolderId(),name,ContentTypes.APPLICATION_OCTET_STREAM,name,StringPool.BLANK,StringPool.BLANK,CONTENT.getBytes(),serviceContext);
  Assert.assertEquals(ContentTypes.TEXT_PLAIN,fileEntry.getMimeType());
}
