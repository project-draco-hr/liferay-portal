{
  try {
    FileEntry fileEntry=DLAppTestUtil.addFileEntry(group.getGroupId(),parentFolder.getFolderId(),"Test-" + _index + ".txt");
    _fileEntryIds[_index]=fileEntry.getFileEntryId();
    if (_log.isDebugEnabled()) {
      _log.debug("Added file " + _index);
    }
    _success=true;
  }
 catch (  Exception e) {
    _log.error("Unable to add file " + _index,e);
  }
}
