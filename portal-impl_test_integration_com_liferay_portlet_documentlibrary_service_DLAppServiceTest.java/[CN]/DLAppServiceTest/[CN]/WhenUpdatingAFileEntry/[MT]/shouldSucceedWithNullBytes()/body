{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  String name="Bytes-null.txt";
  FileEntry fileEntry=addFileEntry(group.getGroupId(),parentFolder.getFolderId());
  DLAppServiceUtil.updateFileEntry(fileEntry.getFileEntryId(),name,ContentTypes.TEXT_PLAIN,name,StringPool.BLANK,StringPool.BLANK,true,(byte[])null,serviceContext);
}
