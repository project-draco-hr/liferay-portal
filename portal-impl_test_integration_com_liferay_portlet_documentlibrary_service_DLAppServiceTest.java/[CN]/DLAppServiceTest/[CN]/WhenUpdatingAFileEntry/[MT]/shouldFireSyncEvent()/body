{
  AtomicInteger counter=registerDLSyncEventProcessorMessageListener(DLSyncConstants.EVENT_UPDATE);
  FileEntry fileEntry=addFileEntry(group.getGroupId(),parentFolder.getFolderId());
  updateFileEntry(fileEntry.getGroupId(),fileEntry.getFileEntryId(),fileEntry.getTitle(),true);
  Assert.assertEquals(2,counter.get());
}
