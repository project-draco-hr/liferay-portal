{
  String fileName=RandomTestUtil.randomString();
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  FileEntry fileEntry=addFileEntry(group.getGroupId(),parentFolder.getFolderId());
  DLAppServiceUtil.updateFileEntry(fileEntry.getFileEntryId(),fileName,ContentTypes.TEXT_PLAIN,fileName,StringPool.BLANK,StringPool.BLANK,true,null,0,serviceContext);
}
