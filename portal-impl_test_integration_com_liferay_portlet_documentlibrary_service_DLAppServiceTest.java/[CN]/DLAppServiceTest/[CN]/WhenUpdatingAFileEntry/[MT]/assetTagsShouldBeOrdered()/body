{
  String fileName=RandomTestUtil.randomString();
  byte[] bytes=CONTENT.getBytes();
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  String[] assetTagNames=new String[]{"hello","world"};
  serviceContext.setAssetTagNames(assetTagNames);
  FileEntry fileEntry=DLAppServiceUtil.addFileEntry(group.getGroupId(),parentFolder.getFolderId(),fileName,ContentTypes.TEXT_PLAIN,fileName,StringPool.BLANK,StringPool.BLANK,bytes,serviceContext);
  assetTagNames=new String[]{"hello","world","liferay"};
  serviceContext.setAssetTagNames(assetTagNames);
  fileEntry=DLAppServiceUtil.updateFileEntry(fileEntry.getFileEntryId(),fileName,ContentTypes.TEXT_PLAIN,fileName,StringPool.BLANK,StringPool.BLANK,false,bytes,serviceContext);
  AssetEntry assetEntry=AssetEntryLocalServiceUtil.fetchEntry(DLFileEntryConstants.getClassName(),fileEntry.getFileEntryId());
  AssertUtils.assertEqualsSorted(assetTagNames,assetEntry.getTagNames());
}
