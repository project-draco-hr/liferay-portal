{
  AtomicInteger counter=registerDLSyncEventProcessorMessageListener(DLSyncConstants.EVENT_ADD);
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId());
  DLAppServiceUtil.addFolder(group.getGroupId(),parentFolder.getFolderId(),RandomTestUtil.randomString(),StringPool.BLANK,serviceContext);
  Assert.assertEquals(1,counter.get());
}
