{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(groupId,TestPropsValues.getUserId());
  DLAppTestUtil.populateNotificationsServiceContext(serviceContext,Constants.ADD);
  serviceContext.setAssetTagNames(assetTagNames);
  return DLAppServiceUtil.addFileEntry(groupId,folderId,fileName,ContentTypes.TEXT_PLAIN,title,StringPool.BLANK,StringPool.BLANK,CONTENT.getBytes(),serviceContext);
}
