{
  if (_fileEntry != null) {
    DLAppServiceUtil.deleteFileEntry(_fileEntry.getFileEntryId());
  }
  super.tearDown();
  for (int i=0; i < ServiceTestUtil.THREAD_COUNT; i++) {
    UserLocalServiceUtil.deleteUser(_userIds[i]);
  }
  MessageBusUtil.removeDestination(DestinationNames.DOCUMENT_LIBRARY_SYNC_EVENT_PROCESSOR);
  if (_originalSyncDestination != null) {
    MessageBusUtil.addDestination(_originalSyncDestination);
  }
  GroupLocalServiceUtil.deleteGroup(group);
}
