{
  String fileName="TestVersion.txt";
  FileEntry fileEntry=addFileEntry(false,fileName);
  assertEquals("Version label incorrect after add","1.0",fileEntry.getVersion());
  fileEntry=updateFileEntry(fileEntry.getFileEntryId(),fileName,false);
  assertEquals("Version label incorrect after minor update","1.1",fileEntry.getVersion());
  fileEntry=updateFileEntry(fileEntry.getFileEntryId(),fileName,true);
  assertEquals("Version label incorrect after major update","2.0",fileEntry.getVersion());
}
