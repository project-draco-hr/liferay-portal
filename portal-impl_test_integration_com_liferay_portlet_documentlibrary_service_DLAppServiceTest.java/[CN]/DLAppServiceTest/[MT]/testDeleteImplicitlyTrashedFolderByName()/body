{
  int initialFolderCount=DLAppServiceUtil.getFoldersCount(group.getGroupId(),parentFolder.getFolderId());
  Folder folder=DLAppTestUtil.addFolder(group.getGroupId(),parentFolder.getFolderId());
  DLAppTestUtil.addFolder(group.getGroupId(),folder.getFolderId());
  DLAppServiceUtil.moveFolderToTrash(folder.getFolderId());
  folder=DLAppServiceUtil.getFolder(folder.getFolderId());
  DLAppServiceUtil.deleteFolder(folder.getRepositoryId(),folder.getParentFolderId(),folder.getName());
  int afterDeleteFolderCount=DLAppServiceUtil.getFoldersCount(group.getGroupId(),parentFolder.getFolderId());
  Assert.assertEquals(initialFolderCount,afterDeleteFolderCount);
}
