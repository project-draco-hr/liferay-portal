{
  Folder folder=DLAppTestUtil.addFolder(group.getGroupId(),parentFolder.getFolderId());
  Folder subfolder=DLAppTestUtil.addFolder(group.getGroupId(),folder.getFolderId());
  DLAppServiceUtil.moveFolderToTrash(subfolder.getFolderId());
  DLAppServiceUtil.moveFolderToTrash(folder.getFolderId());
  folder=DLAppServiceUtil.getFolder(folder.getFolderId());
  DLAppServiceUtil.deleteFolder(folder.getRepositoryId(),folder.getParentFolderId(),folder.getName());
  DLAppServiceUtil.getFolder(subfolder.getFolderId());
}
