{
  Session session=null;
  try {
    session=openSession();
    if (BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(OrgGroupPermissionImpl.class,orgGroupPermission.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(orgGroupPermission);
    session.flush();
    return orgGroupPermission;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(OrgGroupPermission.class.getName());
  }
}
