{
  Session session=null;
  try {
    session=openSession();
    if (orgGroupPermission.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(OrgGroupPermissionImpl.class,orgGroupPermission.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(orgGroupPermission);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.removeResult(OrgGroupPermissionModelImpl.ENTITY_CACHE_ENABLED,OrgGroupPermissionImpl.class,orgGroupPermission.getPrimaryKey());
  return orgGroupPermission;
}
