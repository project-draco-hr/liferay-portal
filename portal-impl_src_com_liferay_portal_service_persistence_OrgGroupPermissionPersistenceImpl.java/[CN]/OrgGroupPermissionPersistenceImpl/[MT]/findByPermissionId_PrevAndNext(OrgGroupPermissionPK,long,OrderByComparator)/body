{
  OrgGroupPermission orgGroupPermission=findByPrimaryKey(orgGroupPermissionPK);
  Session session=null;
  try {
    session=openSession();
    OrgGroupPermission[] array=new OrgGroupPermissionImpl[3];
    array[0]=getByPermissionId_PrevAndNext(session,orgGroupPermission,permissionId,orderByComparator,true);
    array[1]=orgGroupPermission;
    array[2]=getByPermissionId_PrevAndNext(session,orgGroupPermission,permissionId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
