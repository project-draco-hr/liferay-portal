{
  Session session=null;
  try {
    session=openSession();
    if (contact.isNew()) {
      session.save(contact);
    }
    session.flush();
    contact.setNew(false);
    return contact;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
