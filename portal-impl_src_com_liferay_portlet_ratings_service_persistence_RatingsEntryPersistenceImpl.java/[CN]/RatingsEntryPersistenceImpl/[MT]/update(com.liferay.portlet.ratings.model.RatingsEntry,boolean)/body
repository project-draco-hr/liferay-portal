{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(ratingsEntry);
    }
 else {
      if (ratingsEntry.isNew()) {
        session.save(ratingsEntry);
      }
 else {
        ratingsEntry=(RatingsEntry)session.merge(ratingsEntry);
      }
    }
    session.flush();
    ratingsEntry.setNew(false);
    return ratingsEntry;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(RatingsEntry.class.getName());
  }
}
