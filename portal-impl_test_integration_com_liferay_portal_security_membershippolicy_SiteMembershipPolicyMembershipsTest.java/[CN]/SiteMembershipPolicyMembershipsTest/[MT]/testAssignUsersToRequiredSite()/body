{
  long[] requiredSiteIds=addRequiredSites();
  int initialUserGroupCountInRequiredSite=UserLocalServiceUtil.getGroupUsersCount(requiredSiteIds[0]);
  UserServiceUtil.addGroupUsers(requiredSiteIds[0],addUsers(),ServiceTestUtil.getServiceContext());
  Assert.assertEquals(initialUserGroupCountInRequiredSite + 2,UserLocalServiceUtil.getGroupUsersCount(requiredSiteIds[0]));
  Assert.assertTrue(getPropagateMembershipMethodFlag());
}
