{
  Group group=MembershipPolicyTestUtil.addGroup();
  GroupServiceUtil.updateGroup(group.getGroupId(),group.getParentGroupId(),RandomTestUtil.randomString(),group.getDescription(),group.getType(),group.isManualMembership(),group.getMembershipRestriction(),group.getFriendlyURL(),group.isInheritContent(),group.isActive(),ServiceContextTestUtil.getServiceContext());
  Assert.assertTrue(isVerify());
}
