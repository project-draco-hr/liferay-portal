{
  Group group=MembershipPolicyTestUtil.addGroup();
  GroupServiceUtil.updateGroup(group.getGroupId(),group.getParentGroupId(),RandomTestUtil.randomString(),group.getDescription(),group.getType(),group.isManualMembership(),group.getMembershipRestriction(),group.getFriendlyURL(),group.isActive(),ServiceTestUtil.getServiceContext());
  Assert.assertTrue(isVerify());
}
