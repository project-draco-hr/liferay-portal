{
  Group group=MembershipPolicyTestUtil.addGroup();
  Map<Locale,String> nameMap=new HashMap<Locale,String>();
  nameMap.put(LocaleUtil.getDefault(),RandomTestUtil.randomString());
  GroupServiceUtil.updateGroup(group.getGroupId(),group.getParentGroupId(),nameMap,group.getDescriptionMap(),group.getType(),group.isManualMembership(),group.getMembershipRestriction(),group.getFriendlyURL(),group.isInheritContent(),group.isActive(),ServiceContextTestUtil.getServiceContext());
  Assert.assertTrue(isVerify());
}
