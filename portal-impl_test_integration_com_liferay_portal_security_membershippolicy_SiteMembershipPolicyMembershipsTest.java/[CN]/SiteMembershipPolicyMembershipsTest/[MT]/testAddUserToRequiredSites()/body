{
  long[] requiredSiteIds=addRequiredGroups();
  int initialUserGroupCount=UserLocalServiceUtil.getGroupUsersCount(requiredSiteIds[0]);
  MembershipPolicyTestUtil.addUser(null,null,new long[]{requiredSiteIds[0]},null);
  Assert.assertEquals(initialUserGroupCount + 1,UserLocalServiceUtil.getGroupUsersCount(requiredSiteIds[0]));
}
