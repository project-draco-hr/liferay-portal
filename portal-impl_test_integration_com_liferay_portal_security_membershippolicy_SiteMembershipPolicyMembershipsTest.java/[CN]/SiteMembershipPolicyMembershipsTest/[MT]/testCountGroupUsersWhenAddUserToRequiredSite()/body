{
  addUsers();
  addRequiredSites();
  int expectedGroupUsersInRequiredSite=UserLocalServiceUtil.getGroupUsersCount(getRequiredSiteIds()[0]) + addedUsers(1);
  UserServiceUtil.addGroupUsers(getRequiredSiteIds()[0],new long[]{getUserIds()[0]},_getServiceContext());
  int currentGroupUsersInRequiredSite=UserLocalServiceUtil.getGroupUsersCount(getRequiredSiteIds()[0]);
  Assert.assertEquals(expectedGroupUsersInRequiredSite,currentGroupUsersInRequiredSite);
  Assert.assertTrue(getPropagateMembershipMethodFlag());
}
