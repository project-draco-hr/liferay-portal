{
  String[] configLocations=getConfigLocations();
  if (configLocations == null) {
    return;
  }
  for (  String configLocation : configLocations) {
    try {
      reader.loadBeanDefinitions(configLocation);
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn(e,e);
      }
    }
  }
}
