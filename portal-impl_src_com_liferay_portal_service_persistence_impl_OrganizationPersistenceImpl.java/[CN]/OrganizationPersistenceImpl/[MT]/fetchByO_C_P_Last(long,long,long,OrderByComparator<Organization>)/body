{
  int count=countByO_C_P(organizationId,companyId,parentOrganizationId);
  if (count == 0) {
    return null;
  }
  List<Organization> list=findByO_C_P(organizationId,companyId,parentOrganizationId,count - 1,count,orderByComparator);
  if (!list.isEmpty()) {
    return list.get(0);
  }
  return null;
}
