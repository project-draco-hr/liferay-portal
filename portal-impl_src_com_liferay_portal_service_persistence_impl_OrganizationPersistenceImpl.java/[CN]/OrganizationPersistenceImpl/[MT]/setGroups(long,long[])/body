{
  Set<Long> newGroupPKsSet=SetUtil.fromArray(groupPKs);
  Set<Long> oldGroupPKsSet=SetUtil.fromArray(organizationToGroupTableMapper.getRightPrimaryKeys(pk));
  Set<Long> removeGroupPKsSet=new HashSet<Long>(oldGroupPKsSet);
  removeGroupPKsSet.removeAll(newGroupPKsSet);
  organizationToGroupTableMapper.deleteTableMappings(pk,ArrayUtil.toLongArray(removeGroupPKsSet));
  newGroupPKsSet.removeAll(oldGroupPKsSet);
  long companyId=0;
  Organization organization=fetchByPrimaryKey(pk);
  if (organization == null) {
    companyId=companyProvider.getCompanyId();
  }
 else {
    companyId=organization.getCompanyId();
  }
  organizationToGroupTableMapper.addTableMappings(companyId,pk,ArrayUtil.toLongArray(newGroupPKsSet));
}
