{
  Organization organization=findByPrimaryKey(organizationId);
  Session session=null;
  try {
    session=openSession();
    Organization[] array=new OrganizationImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,organization,uuid,companyId,orderByComparator,true);
    array[1]=organization;
    array[2]=getByUuid_C_PrevAndNext(session,organization,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
