{
  long returnValue=0;
  Map userInfoMap=(Map)portletRequest.getAttribute(RenderRequest.USER_INFO);
  if (userInfoMap != null) {
    try {
      returnValue=Long.parseLong((String)userInfoMap.get("liferay.company.id"));
    }
 catch (    NumberFormatException numberFormatException) {
      returnValue=0;
    }
  }
  return (returnValue);
}
