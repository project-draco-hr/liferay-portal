{
  _bundleContext=bundleContext;
  ServiceRegistration<DestinationConfiguration> backgroundTaskServiceRegistration=registerDestinationConfig(DestinationConfiguration.DESTINATION_TYPE_PARALLEL,"liferay/background_task");
  _serviceRegistrations.add(backgroundTaskServiceRegistration);
  ServiceRegistration<DestinationConfiguration> backgroundTaskStatusServiceRegistration=registerDestinationConfig(DestinationConfiguration.DESTINATION_TYPE_PARALLEL,"liferay/background_task_status");
  _serviceRegistrations.add(backgroundTaskStatusServiceRegistration);
}
