{
  Group scopeGroup=themeDisplay.getScopeGroup();
  if ((scopeGroup != null) && (scopeGroup.hasLocalOrRemoteStagingGroup() || (!scopeGroup.isStaged() && !scopeGroup.isStagingGroup()))) {
    return false;
  }
  User user=themeDisplay.getUser();
  if (user.isDefaultUser()) {
    return false;
  }
  return true;
}
