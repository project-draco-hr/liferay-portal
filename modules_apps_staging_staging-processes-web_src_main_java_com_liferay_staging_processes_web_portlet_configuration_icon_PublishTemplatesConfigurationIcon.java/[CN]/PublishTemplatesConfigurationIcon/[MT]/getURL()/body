{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(portletRequest,StagingProcessesPortletKeys.STAGING_PROCESSES,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/publish_templates/view.jsp");
  try {
    portletURL.setWindowState(LiferayWindowState.POP_UP);
  }
 catch (  WindowStateException wse) {
  }
  return portletURL.toString();
}
