{
  for (int i=0; i < itemSelectorCriteria.length; i++) {
    ItemSelectorCriterion itemSelectorCriterion=itemSelectorCriteria[i];
    ItemSelectorCriterionSerializer<ItemSelectorCriterion> itemSelectorCriterionSerializer=new ItemSelectorCriterionSerializer<>(itemSelectorCriterion,i + "_");
    parameters.putAll(itemSelectorCriterionSerializer.getProperties());
  }
}
