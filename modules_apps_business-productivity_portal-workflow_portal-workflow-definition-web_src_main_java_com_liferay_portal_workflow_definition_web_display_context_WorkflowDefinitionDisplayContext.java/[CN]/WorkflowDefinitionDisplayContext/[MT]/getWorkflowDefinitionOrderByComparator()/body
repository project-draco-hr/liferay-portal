{
  String orderByCol=ParamUtil.getString(_workflowDefinitionRequestHelper.getRequest(),"orderByCol","name");
  String orderByType=ParamUtil.getString(_workflowDefinitionRequestHelper.getRequest(),"orderByType","asc");
  return WorkflowDefinitionPortletUtil.getWorkflowDefitionOrderByComparator(orderByCol,orderByType,_workflowDefinitionRequestHelper.getLocale());
}
