{
  long layoutId=0;
  Layout layout=this;
  while (true) {
    if (!layout.isRootLayout()) {
      layout=LayoutLocalServiceUtil.getLayout(layout.getGroupId(),layout.isPrivateLayout(),layout.getParentLayoutId());
    }
 else {
      layoutId=layout.getLayoutId();
      break;
    }
  }
  return layoutId;
}
