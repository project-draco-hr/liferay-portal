{
  String hookImpl=PropsUtil.get(PropsUtil.MAIL_HOOK_IMPL);
  if (GetterUtil.getBoolean(PropsUtil.get(PropsUtil.MAIL_USERNAME_REPLACE))) {
    userId=StringUtil.replace(userId,".","_");
  }
  MethodWrapper methodWrapper=new MethodWrapper(hookImpl,"addUser",new Object[]{userId,password,firstName,middleName,lastName,emailAddress});
  MailProducer.produce(methodWrapper);
}
