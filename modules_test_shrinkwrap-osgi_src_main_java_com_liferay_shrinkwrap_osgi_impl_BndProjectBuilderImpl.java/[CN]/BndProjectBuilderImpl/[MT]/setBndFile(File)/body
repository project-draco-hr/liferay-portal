{
  _bndFile=bndFile;
  File absoluteBndFile=bndFile.getAbsoluteFile();
  File parentBndDir=absoluteBndFile.getParentFile();
  if (_baseDir == null) {
    setBaseDir(parentBndDir);
  }
  if (_projectDir == null) {
    setProjectDir(parentBndDir);
  }
  if (_workspaceDir == null) {
    setWorkspaceDir(parentBndDir);
  }
  return this;
}
