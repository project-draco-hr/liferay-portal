{
  if (fork.getIncomingTransitionsCount() == 0) {
    throw new WorkflowException("No incoming transition found for fork " + fork.getName());
  }
  if (fork.getOutgoingTransitionsCount() < 2) {
    throw new WorkflowException("Less than 2 outgoing transitions found for fork " + fork.getName());
  }
  traverse(fork);
}
