{
  HttpServletRequest request=(HttpServletRequest)pageContext.getRequest();
  StringBundler sb=(StringBundler)request.getAttribute(WebKeys.JS_TOP);
  Set<String> fileNames=(Set<String>)request.getAttribute(WebKeys.JS_TOP_FILE_NAMES);
  if (sb == null) {
    sb=new StringBundler();
    request.setAttribute(WebKeys.JS_TOP,sb);
  }
  sb.append(getBodyContentAsStringBundler());
  if (fileNames == null) {
    fileNames=new HashSet<String>();
    request.setAttribute(WebKeys.JS_TOP_FILE_NAMES,fileNames);
  }
  if (Validator.isNotNull(_src)) {
    fileNames.add(_src);
  }
  return EVAL_PAGE;
}
