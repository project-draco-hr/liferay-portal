{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SystemEvent.class,SystemEvent.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("systemEventId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("systemEventId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
