{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ShoppingItemPrice.class,ShoppingItemPrice.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("itemPriceId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("itemPriceId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
