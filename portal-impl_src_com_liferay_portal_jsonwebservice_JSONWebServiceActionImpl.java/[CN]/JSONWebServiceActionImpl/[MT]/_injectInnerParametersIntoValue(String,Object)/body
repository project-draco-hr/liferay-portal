{
  if (parameterValue == null) {
    return;
  }
  List<KeyValue<String,Object>> innerParameters=_jsonWebServiceActionParameters.getInnerParameters(parameterName);
  if (innerParameters == null) {
    return;
  }
  for (  KeyValue<String,Object> innerParameter : innerParameters) {
    try {
      BeanUtil.setProperty(parameterValue,innerParameter.getKey(),innerParameter.getValue());
    }
 catch (    Exception e) {
      if (_log.isDebugEnabled()) {
        _log.debug("Unable to set inner parameter: " + parameterName + "."+ innerParameter.getKey()+ ", skipping... ",e);
      }
    }
  }
}
