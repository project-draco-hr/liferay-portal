{
  String[] parameterNames=_jsonWebServiceActionConfig.getParameterNames();
  Class<?>[] parameterTypes=_jsonWebServiceActionConfig.getParameterTypes();
  Object[] parameters=new Object[parameterNames.length];
  for (int i=0; i < parameterNames.length; i++) {
    String parameterName=parameterNames[i];
    Object value=_jsonWebServiceActionParameters.getParameter(parameterName);
    Object parameterValue=null;
    if (value != null) {
      Class<?> parameterType=parameterTypes[i];
      if (value == Void.TYPE) {
        parameterValue=_createDefaultParameterValue(parameterName,parameterType);
      }
 else {
        parameterValue=ReflectUtil.castType(value,parameterType);
      }
    }
    parameters[i]=parameterValue;
  }
  return parameters;
}
