{
  String portletId=PortletProviderUtil.getPortletId(BlogsEntry.class.getName(),PortletProvider.Action.VIEW);
  BlogsEntry entry=BlogsEntryLocalServiceUtil.getEntry(classPK);
  long plid=PortalUtil.getPlidFromPortletId(entry.getGroupId(),portletId);
  if (plid == LayoutConstants.DEFAULT_PLID) {
    portletId=PortletProviderUtil.getPortletId(BlogsEntry.class.getName(),PortletProvider.Action.MANAGE);
    plid=PortalUtil.getControlPanelPlid(portletRequest);
  }
  PortletURL portletURL=PortletURLFactoryUtil.create(portletRequest,portletId,plid,PortletRequest.RENDER_PHASE);
  if (!isContainerModel) {
    portletURL.setParameter("mvcRenderCommandName","/blogs/view_entry");
  }
  return portletURL;
}
