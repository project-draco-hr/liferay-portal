{
  DDMFormRenderingContext ddmFormRenderingContext=new DDMFormRenderingContext();
  ddmFormRenderingContext.setDDMFormValues(ddmFormValues);
  ddmFormRenderingContext.setLocale(_LOCALE);
  ddmFormRenderingContext.setPortletNamespace(_PORTLET_NAMESPACE);
  ddmFormRenderingContext.setReadOnly(ddmFormReadOnly);
  ddmFormRenderingContext.setShowRequiredFieldsWarning(showRequiredFieldsWarning);
  DDMFormPagesTemplateContextFactory ddmFormPagesTemplateContextFactory=new DDMFormPagesTemplateContextFactory(ddmForm,ddmFormLayout,ddmFormRenderingContext);
  ddmFormPagesTemplateContextFactory.setDDMFormEvaluator(getDDMFormEvaluator());
  ddmFormPagesTemplateContextFactory.setDDMFormFieldTypeServicesTracker(getDDMFormFieldTypesServicesTracker());
  return ddmFormPagesTemplateContextFactory;
}
