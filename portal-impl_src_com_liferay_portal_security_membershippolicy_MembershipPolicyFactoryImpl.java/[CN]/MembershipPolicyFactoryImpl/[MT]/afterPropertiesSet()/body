{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.USERS_MEMBERSHIP_POLICY);
  }
  ClassLoader classLoader=ClassLoaderUtil.getPortalClassLoader();
  _originalMembershipPolicy=(MembershipPolicy)InstanceFactory.newInstance(classLoader,PropsValues.USERS_MEMBERSHIP_POLICY);
  _membershipPolicy=_originalMembershipPolicy;
}
