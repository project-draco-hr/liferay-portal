{
  int index=0;
  while (true) {
    index=name.indexOf("By",index);
    if (index == -1) {
      break;
    }
    if (index + 2 < name.length()) {
      if (Character.isUpperCase(name.charAt(index + 2))) {
        name=name.substring(0,index);
        break;
      }
    }
    index++;
  }
  return name;
}
