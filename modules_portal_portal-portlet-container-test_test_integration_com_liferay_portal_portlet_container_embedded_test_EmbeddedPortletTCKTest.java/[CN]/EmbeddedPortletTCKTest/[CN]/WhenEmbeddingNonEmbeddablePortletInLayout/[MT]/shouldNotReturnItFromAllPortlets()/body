{
  PortletPreferencesLocalServiceUtil.addPortletPreferences(TestPropsValues.getCompanyId(),PortletKeys.PREFS_OWNER_ID_DEFAULT,PortletKeys.PREFS_OWNER_TYPE_LAYOUT,layout.getPlid(),_nonEmbeddedPortlet.getPortletId(),_nonEmbeddedPortlet,null);
  List<Portlet> allPortlets=_layoutTypePortlet.getAllPortlets();
  Assert.assertFalse(allPortlets.contains(_nonEmbeddedPortlet));
}
