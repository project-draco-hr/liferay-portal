{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),CalendarLocalService.class.getName());
    if (invokableLocalService instanceof CalendarLocalService) {
      _service=(CalendarLocalService)invokableLocalService;
    }
 else {
      _service=new CalendarLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(CalendarLocalServiceUtil.class,"_service");
  }
  return _service;
}
