{
  try {
    com.liferay.portlet.dynamicdatamapping.model.DDMStructure returnValue=DDMStructureServiceUtil.updateStructure(groupId,structureKey,name,description,xsd,serviceContext);
    return com.liferay.portlet.dynamicdatamapping.model.DDMStructureSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
