{
  try {
    com.liferay.portlet.dynamicdatamapping.model.DDMStructure returnValue=DDMStructureServiceUtil.getStructure(groupId,classNameId,structureKey,includeGlobalStructures);
    return com.liferay.portlet.dynamicdatamapping.model.DDMStructureSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
