{
  int firstIndex=_pathBinarySearch.findFirst(path);
  if (firstIndex == -1) {
    return -1;
  }
  int lastIndex=_pathBinarySearch.findLast(path,firstIndex);
  if (lastIndex == -1) {
    lastIndex=firstIndex;
  }
  int found=-1;
  int max=-1;
  for (int i=firstIndex; i <= lastIndex; i++) {
    RESTActionConfig config=_restActionConfig.get(i);
    if (config.getMethod() != null && !config.getMethod().equals(method)) {
      continue;
    }
    String[] methodParameterNames=config.getParameterNames();
    int matched=_countMatchedElements(parameterNames,methodParameterNames);
    if (matched > max) {
      max=matched;
      found=i;
    }
  }
  return found;
}
