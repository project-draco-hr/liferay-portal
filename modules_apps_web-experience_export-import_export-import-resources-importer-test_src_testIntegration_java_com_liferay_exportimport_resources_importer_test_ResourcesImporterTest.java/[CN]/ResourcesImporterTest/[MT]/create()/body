{
  BndProjectBuilder bndProjectBuilder=ShrinkWrap.create(BndProjectBuilder.class);
  bndProjectBuilder.setBndFile(new File("bnd.bnd"));
  bndProjectBuilder.generateManifest(true);
  JavaArchive javaArchive=bndProjectBuilder.as(JavaArchive.class);
  javaArchive.add(new ArchiveAsset(buildTestWebArchive(),ZipExporter.class),"com/liferay/exportimport/resources/importer/test/dependencies/" + "test.war");
  return javaArchive;
}
