{
  Class<?> clazz=getClass();
  ClassLoader classLoader=clazz.getClassLoader();
  ResourceLoader resourceLoader=new TestURLResourceLoader(url,classLoader);
  ServletContext servletContext=new TestServletContext(StringPool.BLANK,resourceLoader,url);
  Registry registry=RegistryUtil.getRegistry();
  registry.registerService(ServletContext.class,servletContext);
  return servletContext;
}
