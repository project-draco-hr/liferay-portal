{
  AssetEntry assetEntry=assetEntryLocalService.updateEntry(userId,folder.getGroupId(),folder.getCreateDate(),folder.getModifiedDate(),BookmarksFolder.class.getName(),folder.getFolderId(),folder.getUuid(),0,assetCategoryIds,assetTagNames,true,true,null,null,null,ContentTypes.TEXT_PLAIN,folder.getName(),folder.getDescription(),null,null,null,0,0,priority);
  assetLinkLocalService.updateLinks(userId,assetEntry.getEntryId(),assetLinkEntryIds,AssetLinkConstants.TYPE_RELATED);
}
