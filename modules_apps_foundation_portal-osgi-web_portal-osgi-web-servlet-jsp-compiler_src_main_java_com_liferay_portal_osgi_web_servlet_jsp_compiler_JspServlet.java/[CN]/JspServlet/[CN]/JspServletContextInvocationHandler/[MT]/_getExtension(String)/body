{
  Matcher matcher=_originalJspPattern.matcher(path);
  if (matcher.matches()) {
    path=matcher.group("file") + matcher.group("extension");
    return _bundle.getEntry(_RESOURCES_DIR + path);
  }
  Enumeration<URL> enumeration=_bundle.findEntries(_RESOURCES_DIR,path.substring(1),false);
  if (enumeration == null) {
    return null;
  }
  List<URL> urls=Collections.list(enumeration);
  return urls.get(urls.size() - 1);
}
