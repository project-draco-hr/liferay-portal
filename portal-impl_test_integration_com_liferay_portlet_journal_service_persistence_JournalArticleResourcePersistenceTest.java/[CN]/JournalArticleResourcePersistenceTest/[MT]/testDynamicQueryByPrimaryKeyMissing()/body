{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalArticleResource.class,JournalArticleResource.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("resourcePrimKey",nextLong()));
  List<JournalArticleResource> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
