{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(dataTracker);
    }
 else {
      if (dataTracker.isNew()) {
        session.save(dataTracker);
      }
    }
    session.flush();
    dataTracker.setNew(false);
    return dataTracker;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
