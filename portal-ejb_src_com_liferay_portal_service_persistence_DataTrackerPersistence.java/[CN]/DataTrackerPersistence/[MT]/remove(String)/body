{
  Session session=null;
  try {
    session=openSession();
    DataTrackerHBM dataTrackerHBM=(DataTrackerHBM)session.get(DataTrackerHBM.class,dataTrackerId);
    if (dataTrackerHBM == null) {
      _log.warn("No DataTracker exists with the primary key " + dataTrackerId.toString());
      throw new NoSuchDataTrackerException("No DataTracker exists with the primary key " + dataTrackerId.toString());
    }
    session.delete(dataTrackerHBM);
    session.flush();
    return DataTrackerHBMUtil.model(dataTrackerHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
