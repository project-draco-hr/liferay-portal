{
  Session session=null;
  try {
    session=openSession();
    DataTracker dataTracker=(DataTracker)session.get(DataTrackerImpl.class,dataTrackerId);
    if (dataTracker == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No DataTracker exists with the primary key " + dataTrackerId);
      }
      throw new NoSuchDataTrackerException("No DataTracker exists with the primary key " + dataTrackerId);
    }
    return remove(dataTracker);
  }
 catch (  NoSuchDataTrackerException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
