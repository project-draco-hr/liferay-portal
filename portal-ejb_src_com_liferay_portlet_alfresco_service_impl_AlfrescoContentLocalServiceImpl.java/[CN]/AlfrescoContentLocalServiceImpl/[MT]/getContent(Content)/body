{
  StringBuffer sb=new StringBuffer();
  String ticket=AuthenticationUtils.getCurrentTicket();
  URL url=new URL(content.getUrl() + "?ticket=" + ticket);
  URLConnection conn=url.openConnection();
  InputStream is=conn.getInputStream();
  int read=is.read();
  while (read != -1) {
    sb.append((char)read);
    read=is.read();
  }
  return sb.toString();
}
