{
  int count=countByG_U_C_C_T_R(groupId,userId,classNameId,classPK,type,receiverUserId);
  if (count == 0) {
    return null;
  }
  List<SocialActivity> list=findByG_U_C_C_T_R(groupId,userId,classNameId,classPK,type,receiverUserId,count - 1,count,orderByComparator);
  if (!list.isEmpty()) {
    return list.get(0);
  }
  return null;
}
