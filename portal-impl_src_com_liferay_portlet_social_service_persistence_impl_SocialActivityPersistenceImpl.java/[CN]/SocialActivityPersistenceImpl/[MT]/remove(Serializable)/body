{
  Session session=null;
  try {
    session=openSession();
    SocialActivity socialActivity=(SocialActivity)session.get(SocialActivityImpl.class,primaryKey);
    if (socialActivity == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchActivityException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(socialActivity);
  }
 catch (  NoSuchActivityException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
