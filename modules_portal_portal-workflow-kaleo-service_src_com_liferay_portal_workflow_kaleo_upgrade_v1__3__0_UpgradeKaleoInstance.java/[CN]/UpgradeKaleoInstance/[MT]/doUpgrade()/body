{
  for (  String[] tableData : _WORKFLOW_INSTANCE_TABLE_DATA) {
    String tableName=tableData[0];
    String columnName=tableData[1];
    String columnValue=null;
    if (StringUtil.matches(columnName,"classNameId")) {
      columnValue=GetterUtil.getString(PortalUtil.getClassNameId(_KALEO_PROCESS_CLASS_NAME));
    }
 else {
      columnValue=_KALEO_PROCESS_CLASS_NAME;
    }
    if (Validator.isNotNull(columnValue)) {
      deleteOrphanedWorkflowInstanceLinks(tableName,columnName,columnValue);
    }
  }
}
