{
  PortletURL portletURL=liferayPortletResponse.createActionURL(WikiPortletKeys.WIKI);
  portletURL.setParameter(ActionRequest.ACTION_NAME,"/wiki/upload_page_attachment");
  portletURL.setParameter("resourcePrimKey",String.valueOf(_wikiAttachmentItemSelectorCriterion.getWikiPageResourceId()));
  return portletURL;
}
