{
  int endIndex=line.indexOf(">",startIndex);
  if (endIndex == -1) {
    endIndex=line.length();
  }
 else {
    String endType=line.substring(endIndex - 1,endIndex);
    if ("%".equals(endType)) {
      endIndex=_getTagEndIndex(line,endIndex + 1);
    }
 else     if ("/".equals(endType)) {
      endIndex=endIndex - 1;
    }
  }
  return endIndex;
}
