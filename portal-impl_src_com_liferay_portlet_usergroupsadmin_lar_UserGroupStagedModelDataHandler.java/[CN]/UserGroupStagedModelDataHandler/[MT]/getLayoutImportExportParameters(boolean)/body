{
  if (isPrivate) {
    return LAR_PARAMETERS;
  }
 else {
    Map<String,String[]> parameters=new HashMap<String,String[]>(LAR_PARAMETERS);
    LAR_PARAMETERS.put(PortletDataHandlerKeys.PORTLET_DATA,new String[]{Boolean.FALSE.toString()});
    LAR_PARAMETERS.put(PortletDataHandlerKeys.PORTLET_DATA_ALL,new String[]{Boolean.FALSE.toString()});
    return parameters;
  }
}
