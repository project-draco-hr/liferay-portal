{
  IGFolder igFolder=(IGFolder)EntityCacheUtil.getResult(IGFolderModelImpl.ENTITY_CACHE_ENABLED,IGFolderImpl.class,folderId,this);
  if (igFolder == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (igFolder == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      igFolder=(IGFolder)session.get(IGFolderImpl.class,Long.valueOf(folderId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (igFolder == null)) {
        EntityCacheUtil.putResult(IGFolderModelImpl.ENTITY_CACHE_ENABLED,IGFolderImpl.class,folderId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(igFolder);
      }
      closeSession(session);
    }
  }
  return igFolder;
}
