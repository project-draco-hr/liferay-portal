{
  IGFolder igFolder=(IGFolder)EntityCacheUtil.getResult(IGFolderModelImpl.ENTITY_CACHE_ENABLED,IGFolderImpl.class,folderId,this);
  if (igFolder == _nullIGFolder) {
    return null;
  }
  if (igFolder == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      igFolder=(IGFolder)session.get(IGFolderImpl.class,Long.valueOf(folderId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (igFolder != null) {
        cacheResult(igFolder);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(IGFolderModelImpl.ENTITY_CACHE_ENABLED,IGFolderImpl.class,folderId,_nullIGFolder);
      }
      closeSession(session);
    }
  }
  return igFolder;
}
