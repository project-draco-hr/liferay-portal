{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByG_P_PrevAndNext(folderId,groupId,parentFolderId,orderByComparator);
  }
  IGFolder igFolder=findByPrimaryKey(folderId);
  Session session=null;
  try {
    session=openSession();
    IGFolder[] array=new IGFolderImpl[3];
    array[0]=filterGetByG_P_PrevAndNext(session,igFolder,groupId,parentFolderId,orderByComparator,true);
    array[1]=igFolder;
    array[2]=filterGetByG_P_PrevAndNext(session,igFolder,groupId,parentFolderId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
