{
  igFolder=toUnwrappedModel(igFolder);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,igFolder);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  IGFolderModelImpl igFolderModelImpl=(IGFolderModelImpl)igFolder;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{igFolderModelImpl.getUuid(),Long.valueOf(igFolderModelImpl.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_P_N,new Object[]{Long.valueOf(igFolderModelImpl.getGroupId()),Long.valueOf(igFolderModelImpl.getParentFolderId()),igFolderModelImpl.getName()});
  EntityCacheUtil.removeResult(IGFolderModelImpl.ENTITY_CACHE_ENABLED,IGFolderImpl.class,igFolder.getPrimaryKey());
  return igFolder;
}
