{
  Session session=null;
  try {
    session=openSession();
    igFolder=(IGFolder)session.merge(igFolder);
    session.delete(igFolder);
    session.flush();
    return igFolder;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(IGFolder.class.getName());
  }
}
