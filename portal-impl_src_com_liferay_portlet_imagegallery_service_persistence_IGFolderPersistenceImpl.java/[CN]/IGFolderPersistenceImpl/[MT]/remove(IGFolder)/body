{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(igFolder);
    }
  }
  igFolder=removeImpl(igFolder);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(igFolder);
    }
  }
  return igFolder;
}
