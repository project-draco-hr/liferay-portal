{
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(igFolder);
    }
 else {
      if (igFolder.isNew()) {
        session.save(igFolder);
      }
    }
    session.flush();
    igFolder.setNew(false);
    return igFolder;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(IGFolder.class.getName());
  }
}
