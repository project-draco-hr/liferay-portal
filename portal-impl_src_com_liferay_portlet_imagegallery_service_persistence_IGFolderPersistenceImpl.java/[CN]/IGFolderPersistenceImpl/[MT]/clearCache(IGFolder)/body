{
  EntityCacheUtil.removeResult(IGFolderModelImpl.ENTITY_CACHE_ENABLED,IGFolderImpl.class,igFolder.getPrimaryKey());
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{igFolder.getUuid(),Long.valueOf(igFolder.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_P_N,new Object[]{Long.valueOf(igFolder.getGroupId()),Long.valueOf(igFolder.getParentFolderId()),igFolder.getName()});
}
