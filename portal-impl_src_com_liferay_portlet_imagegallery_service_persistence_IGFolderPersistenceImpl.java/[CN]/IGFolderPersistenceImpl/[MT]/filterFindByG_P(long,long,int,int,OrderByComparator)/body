{
  Session session=null;
  try {
    session=openSession();
    StringBundler query=null;
    if (orderByComparator != null) {
      query=new StringBundler(4 + (orderByComparator.getOrderByFields().length * 3));
    }
 else {
      query=new StringBundler(4);
    }
    query.append(_FILTER_SQL_SELECT_IGFOLDER_WHERE);
    query.append(_FINDER_COLUMN_G_P_GROUPID_2);
    query.append(_FINDER_COLUMN_G_P_PARENTFOLDERID_2);
    if (orderByComparator != null) {
      appendOrderByComparator(query,_ORDER_BY_ENTITY_ALIAS,orderByComparator);
    }
 else {
      query.append(IGFolderModelImpl.ORDER_BY_JPQL);
    }
    String sql=InlineSQLHelperUtil.replacePermissionCheck(query.toString(),IGFolder.class.getName(),_FILTER_COLUMN_FOLDERID,_FILTER_COLUMN_USERID,groupId);
    SQLQuery q=session.createSQLQuery(sql);
    q.addEntity(_FILTER_ENTITY_ALIAS,IGFolderImpl.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(groupId);
    qPos.add(parentFolderId);
    return (List<IGFolder>)QueryUtil.list(q,getDialect(),start,end);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
