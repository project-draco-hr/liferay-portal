{
  _ajaxableRenderEnable=GetterUtil.getBoolean(request.getAttribute(WebKeys.PORTLET_AJAXABLE_RENDER));
  _request=request;
  _response=response;
  _portletRenderersMap=new TreeMap<Integer,List<PortletRenderer>>(new Comparator<Integer>(){
    public int compare(    Integer renderWeight1,    Integer renderWeight2){
      return renderWeight2.compareTo(renderWeight1);
    }
  }
);
  if (Validator.isNotNull(portletId)) {
    ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(com.liferay.portal.util.WebKeys.THEME_DISPLAY);
    _portlet=PortletLocalServiceUtil.getPortletById(themeDisplay.getCompanyId(),portletId);
  }
}
