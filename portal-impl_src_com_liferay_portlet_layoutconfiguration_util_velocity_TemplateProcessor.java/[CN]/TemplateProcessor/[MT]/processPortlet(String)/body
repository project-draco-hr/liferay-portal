{
  try {
    _request.setAttribute(WebKeys.RENDER_PORTLET_RESOURCE,Boolean.TRUE);
    StringServletResponse stringServletResponse=new StringServletResponse(_response);
    RuntimePortletUtil.processPortlet(_request,stringServletResponse,_portletId);
    return stringServletResponse.getString();
  }
  finally {
    _request.removeAttribute(WebKeys.RENDER_PORTLET_RESOURCE);
  }
}
