{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),KaleoInstanceLocalService.class.getName());
    if (invokableLocalService instanceof KaleoInstanceLocalService) {
      _service=(KaleoInstanceLocalService)invokableLocalService;
    }
 else {
      _service=new KaleoInstanceLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(KaleoInstanceLocalServiceUtil.class,"_service");
  }
  return _service;
}
