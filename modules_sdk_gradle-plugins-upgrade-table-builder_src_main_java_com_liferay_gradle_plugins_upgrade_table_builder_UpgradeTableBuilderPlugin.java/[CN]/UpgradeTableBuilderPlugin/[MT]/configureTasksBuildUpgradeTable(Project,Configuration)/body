{
  TaskContainer taskContainer=project.getTasks();
  taskContainer.withType(BuildUpgradeTableTask.class,new Action<BuildUpgradeTableTask>(){
    @Override public void execute(    BuildUpgradeTableTask buildUpgradeTableTask){
      configureTaskBuildUpgradeTableClasspath(buildUpgradeTableTask,upgradeTableBuilderConfiguration);
    }
  }
);
}
