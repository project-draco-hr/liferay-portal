{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Document Library Permissions Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Document Library Permissions Page",RuntimeVariables.replace("Document Library Permissions Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Documents and Media"),selenium.getText("//span[@class='portlet-title-text']"));
  assertEquals(RuntimeVariables.replace("You do not have the roles required to access this portlet."),selenium.getText("//div[@class='portlet-msg-error']"));
}
