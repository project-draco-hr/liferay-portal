{
  StringMaker sm=new StringMaker();
  for (int i=0; i < tags.size(); i++) {
    String tag=tags.get(i);
    if (i == 0) {
      sm.append("\r\n");
    }
    sm.append("\t\t<tag>");
    sm.append(tag);
    sm.append("</tag>\r\n");
    if ((i + 1) == tags.size()) {
      sm.append("\t");
    }
  }
  return sm.toString();
}
