{
  List wars=new ArrayList();
  List jars=new ArrayList();
  for (int i=0; i < args.length; i++) {
    String fileName=args[i].toLowerCase();
    if (fileName.endsWith(".war")) {
      wars.add(args[i]);
    }
 else     if (fileName.endsWith(".jar")) {
      jars.add(args[i]);
    }
  }
  new BaseDeployer(wars,jars);
}
