{
  Metadata metadata=null;
  File file=null;
  if (!isSupported(mimeType)) {
    return metadata;
  }
  try {
    file=FileUtil.createTempFile(extension);
    FileUtil.write(file,inputStream);
    metadata=extractMetadata(file);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
 finally {
    FileUtil.delete(file);
  }
  return metadata;
}
