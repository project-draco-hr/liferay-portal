{
  request.setAttribute(DDLWebKeys.DYNAMIC_DATA_LISTS_RECORD,_formRecord);
  DDLFormViewRecordDisplayContext ddlFormViewRecordDisplayContext=new DDLFormViewRecordDisplayContext(request,response,_ddlRecordLocalService,_ddmFormRenderer,_ddmStructureLocalService);
  request.setAttribute(WebKeys.PORTLET_DISPLAY_CONTEXT,ddlFormViewRecordDisplayContext);
  return super.include(request,response,template);
}
