{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBCategory.class,MBCategory.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("categoryId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("categoryId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
