    }
    if (params4 != null) {
      userIds.addAll(countByC_FN_MN_LN_SN_EA_S(session,companyId,firstNames,middleNames,lastNames,screenNames,emailAddresses,status,params4,andOperator));
    }
    return userIds.size();
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
