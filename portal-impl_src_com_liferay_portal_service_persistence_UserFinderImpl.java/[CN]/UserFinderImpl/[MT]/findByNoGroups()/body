{
  Session session=null;
  try {
    session=openSession();
    String sql=CustomSQLUtil.get(FIND_BY_NO_GROUPS);
    SQLQuery q=session.createSQLQuery(sql);
    q.addEntity("User_",UserImpl.class);
    return q.list(true);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
}
