{
  if (isEscapedModel()) {
    return (Region)this;
  }
 else {
    Region model=new RegionImpl();
    model.setNew(isNew());
    model.setEscapedModel(true);
    model.setRegionId(getRegionId());
    model.setCountryId(getCountryId());
    model.setRegionCode(HtmlUtil.escape(getRegionCode()));
    model.setName(HtmlUtil.escape(getName()));
    model.setActive(getActive());
    model=(Region)Proxy.newProxyInstance(Region.class.getClassLoader(),new Class[]{Region.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
