{
  if (folderId == DLFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    if (actionId.equals(ActionKeys.ACCESS) || actionId.equals(ActionKeys.ADD_SUBFOLDER) || actionId.equals(ActionKeys.DELETE)) {
      return false;
    }
    return DLPermission.contains(permissionChecker,groupId,actionId);
  }
 else {
    Folder folder=DLAppLocalServiceUtil.getFolder(folderId);
    return folder.containsPermission(permissionChecker,actionId);
  }
}
