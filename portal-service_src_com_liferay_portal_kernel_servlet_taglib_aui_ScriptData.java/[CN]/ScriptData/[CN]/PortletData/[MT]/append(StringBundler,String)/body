{
  if (Validator.isNull(use)) {
    _rawSB.append(contentSB);
  }
 else {
    _callbackSB.append("(function() {");
    _callbackSB.append(contentSB);
    _callbackSB.append("})();");
    String[] useArray=StringUtil.split(use);
    for (int i=0; i < useArray.length; i++) {
      _useSet.add(useArray[i]);
    }
  }
}
