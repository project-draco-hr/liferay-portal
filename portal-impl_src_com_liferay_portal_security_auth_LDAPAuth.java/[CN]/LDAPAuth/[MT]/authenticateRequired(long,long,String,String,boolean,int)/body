{
  if (allowOmniadmin && (authenticateOmniadmin(companyId,emailAddress,screenName,userId) == SUCCESS)) {
    return SUCCESS;
  }
  if (PrefsPropsUtil.getBoolean(companyId,PropsKeys.LDAP_AUTH_REQUIRED) || !PrefsPropsUtil.getBoolean(companyId,PropsKeys.LDAP_IMPORT_USER_PASSWORD_DEFAULT)) {
    return failureCode;
  }
 else {
    return SUCCESS;
  }
}
