{
  AssetRendererFactory<MBCategory> assetRendererFactory=getAssetRendererFactory();
  PortletURL portletURL=assetRendererFactory.getURLView(liferayPortletResponse,windowState);
  portletURL.setParameter("mvcRenderCommandName","/message_boards/view");
  portletURL.setParameter("mbCategoryId",String.valueOf(_category.getCategoryId()));
  portletURL.setWindowState(windowState);
  return portletURL.toString();
}
