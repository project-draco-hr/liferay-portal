{
  PortletConfig portletConfig=getPortletConfig();
  String portletName=portletConfig.getPortletName();
  String wsrpConsumerPortletUuid=portletName;
  if (portletName.startsWith("WSRP_")) {
    wsrpConsumerPortletUuid=portletName.substring(5,portletName.length());
  }
  wsrpConsumerPortletUuid=PortalUUIDUtil.fromJsSafeUuid(wsrpConsumerPortletUuid);
  WSRPConsumerPortlet wsrpConsumerPortlet=WSRPConsumerPortletLocalServiceUtil.getWSRPConsumerPortlet(wsrpConsumerPortletUuid);
  return wsrpConsumerPortlet;
}
