{
  Map<String,String> params=new HashMap<>();
  _putCriteriaParam(params,itemSelectorCriteria);
  for (int i=0; i < itemSelectorCriteria.length; i++) {
    ItemSelectorCriterion itemSelectorCriterion=itemSelectorCriteria[i];
    String paramPrefix=i + "_";
    _putDesiredReturnTypesParam(params,paramPrefix,itemSelectorCriterion);
    _putItemCriterionSelectionParams(params,paramPrefix,itemSelectorCriterion);
  }
  return new RequestDescriptor(PORTLET_URL,params);
}
