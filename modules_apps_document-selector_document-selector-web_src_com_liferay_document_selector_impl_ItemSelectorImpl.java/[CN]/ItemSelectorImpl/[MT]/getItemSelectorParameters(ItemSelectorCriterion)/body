{
  Map<String,String> params=new HashMap<>();
  _populateCriteria(params,itemSelectorCriteria);
  for (int i=0; i < itemSelectorCriteria.length; i++) {
    ItemSelectorCriterion itemSelectorCriterion=itemSelectorCriteria[i];
    String paramPrefix=i + "_";
    _populateDesiredReturnTypes(params,paramPrefix,itemSelectorCriterion);
    _populateItemSelectorCriteria(params,paramPrefix,itemSelectorCriterion);
  }
  return params;
}
