{
  params.put(PARAM_CRITERIA,_getCommaSeparatedList(Arrays.asList(itemSelectorCriteria),new Function<ItemSelectorCriterion,String>(){
    @Override public String apply(    ItemSelectorCriterion itemSelectorCriterion){
      Class<?> clazz=itemSelectorCriterion.getClass();
      return clazz.getName();
    }
  }
));
}
