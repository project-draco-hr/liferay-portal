{
  Map<String,String> params=getItemSelectorParameters(itemSelectorCriteria);
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletURL portletURL=PortletURLFactoryUtil.create(portletRequest,DocumentSelectorPortletKeys.DOCUMENT_SELECTOR,themeDisplay.getPlid(),PortletRequest.RENDER_PHASE);
  for (  Map.Entry<String,String> entry : params.entrySet()) {
    portletURL.setParameter(entry.getKey(),entry.getValue());
  }
  return portletURL;
}
