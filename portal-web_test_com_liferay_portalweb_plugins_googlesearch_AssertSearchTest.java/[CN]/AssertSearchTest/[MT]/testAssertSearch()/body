{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Google Search Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Google Search Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.typeKeys("//input[1]",RuntimeVariables.replace("lifera"));
  selenium.type("//input[1]",RuntimeVariables.replace("liferay"));
  selenium.click(RuntimeVariables.replace("//input[@type='image']"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isTextPresent("Google Search is temporarily unavailable."));
}
