{
  SessionImpl sessionImpl=(SessionImpl)session;
  org.hibernate.Session hibernateSession=sessionImpl.getSession();
  if (hibernateSession instanceof LiferaySession) {
    hibernateSession=((LiferaySession)hibernateSession).getHibernateSession();
  }
  _criteria=_detachedCriteria.getExecutableCriteria(hibernateSession);
  if ((_start != null) && (_end != null)) {
    _criteria=_criteria.setFirstResult(_start.intValue());
    _criteria=_criteria.setMaxResults(_end.intValue() - _start.intValue());
  }
}
