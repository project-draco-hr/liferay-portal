{
  String expandoPath=getImportCategoryPath(portletDataContext,parentCategoryId,true);
  Map<String,Serializable> parentCategoryAttributes=null;
  if (Validator.isNotNull(expandoPath)) {
    parentCategoryAttributes=(Map<String,Serializable>)portletDataContext.getZipEntryAsObject(expandoPath);
  }
  return parentCategoryAttributes;
}
