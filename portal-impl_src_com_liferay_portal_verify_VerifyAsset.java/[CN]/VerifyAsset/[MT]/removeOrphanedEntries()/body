{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    long classNameId=PortalUtil.getClassNameId(DLFileEntryConstants.getClassName());
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select entryId, classPK from AssetEntry where " + "classNameId =" + classNameId + " and classPK not in"+ "(select fileVersionId from DLFileVersion);");
    rs=ps.executeQuery();
    while (rs.next()) {
      long classPK=rs.getLong("classPK");
      long entryId=rs.getLong("entryId");
      DLFileEntry dlFileEntry=DLFileEntryLocalServiceUtil.fetchDLFileEntry(classPK);
      if (dlFileEntry != null) {
        continue;
      }
      AssetEntryLocalServiceUtil.deleteAssetEntry(entryId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
