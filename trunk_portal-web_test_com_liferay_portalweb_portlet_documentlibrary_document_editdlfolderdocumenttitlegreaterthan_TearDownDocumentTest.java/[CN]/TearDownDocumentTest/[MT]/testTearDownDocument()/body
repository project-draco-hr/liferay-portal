{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      selenium.open("/web/guest/home/");
    for (int second=0; ; second++) {
      if (second >= 60) {
        fail("timeout");
      }
      try {
        if (selenium.isVisible("link=Document Library Test Page")) {
          break;
        }
      }
 catch (      Exception e) {
      }
      Thread.sleep(1000);
    }
  selenium.saveScreenShotAndSource();
selenium.clickAt("link=Document Library Test Page",RuntimeVariables.replace(""));
selenium.waitForPageToLoad("30000");
selenium.saveScreenShotAndSource();
boolean folderPresent=selenium.isElementPresent("//a[2]/strong");
if (!folderPresent) {
label=2;
continue;
}
selenium.clickAt("//a[2]/strong",RuntimeVariables.replace(""));
selenium.waitForPageToLoad("30000");
selenium.saveScreenShotAndSource();
case 2:
boolean document1Present=selenium.isElementPresent("//td[6]/span/ul/li/strong/a");
if (!document1Present) {
label=3;
continue;
}
