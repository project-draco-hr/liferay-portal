{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  UserGroup newUserGroup=addUserGroup();
  _persistence.clearCache();
  UserGroupModelImpl existingUserGroupModelImpl=(UserGroupModelImpl)_persistence.findByPrimaryKey(newUserGroup.getPrimaryKey());
  assertEquals(existingUserGroupModelImpl.getCompanyId(),existingUserGroupModelImpl.getOriginalCompanyId());
  assertTrue(Validator.equals(existingUserGroupModelImpl.getName(),existingUserGroupModelImpl.getOriginalName()));
}
