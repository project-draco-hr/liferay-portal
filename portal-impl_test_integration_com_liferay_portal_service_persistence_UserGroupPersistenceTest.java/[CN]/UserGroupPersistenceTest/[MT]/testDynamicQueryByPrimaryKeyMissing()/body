{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(UserGroup.class,UserGroup.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("userGroupId",ServiceTestUtil.nextLong()));
  List<UserGroup> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
