{
  socialEquityGroupSetting.setNew(false);
  socialEquityGroupSetting=socialEquityGroupSettingPersistence.update(socialEquityGroupSetting,merge);
  Indexer indexer=IndexerRegistryUtil.getIndexer(getModelClassName());
  if (indexer != null) {
    try {
      indexer.reindex(socialEquityGroupSetting);
    }
 catch (    SearchException se) {
      if (_log.isWarnEnabled()) {
        _log.warn(se,se);
      }
    }
  }
  return socialEquityGroupSetting;
}
