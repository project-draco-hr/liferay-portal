{
  return StringUtil.replace(content,new String[]{"[$GROUP_ID_PUBLIC$]","[$GROUP_ID_PRIVATE$]","[$LAYOUT_ID_PUBLIC$]","[$LAYOUT_ID_PRIVATE$]"},new String[]{String.valueOf(publicLayout.getGroupId()),String.valueOf(privateLayout.getGroupId()),String.valueOf(publicLayout.getLayoutId()),String.valueOf(privateLayout.getLayoutId())});
}
