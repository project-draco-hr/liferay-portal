{
  String xml=replaceParameters(getContent("missing_references.txt"),_fileEntry);
  ZipWriter zipWriter=ZipWriterFactoryUtil.getZipWriter();
  zipWriter.addEntry("/manifest.xml",xml);
  Map<String,MissingReference> missingReferences=ExportImportHelperUtil.validateMissingReferences(TestPropsValues.getUserId(),_stagingGroup.getGroupId(),new HashMap<String,String[]>(),zipWriter.getFile());
  Assert.assertEquals(2,missingReferences.size());
  FileUtil.delete(zipWriter.getFile());
}
