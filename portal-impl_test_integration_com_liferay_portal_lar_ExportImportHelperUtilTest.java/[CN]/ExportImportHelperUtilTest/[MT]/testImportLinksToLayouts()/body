{
  Element rootElement=_portletDataContextExport.getExportDataRootElement();
  Element entryElement=rootElement.element("entry");
  String content=replaceParameters(getContent("layout_links.txt"),_fileEntry);
  content=ExportImportHelperUtil.replaceExportContentReferences(_portletDataContextExport,_referrerStagedModel,entryElement,content,true);
  String importedContent=ExportImportHelperUtil.replaceExportContentReferences(_portletDataContextExport,_referrerStagedModel,entryElement,content,true);
  Assert.assertEquals(importedContent,content);
}
