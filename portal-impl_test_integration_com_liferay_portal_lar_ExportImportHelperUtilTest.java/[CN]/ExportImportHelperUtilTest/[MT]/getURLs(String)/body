{
  Pattern pattern=Pattern.compile("href=|\\{|\\[");
  Matcher matcher=pattern.matcher(StringPool.BLANK);
  String[] lines=StringUtil.split(content,StringPool.NEW_LINE);
  List<String> urls=new ArrayList<String>();
  for (  String line : lines) {
    matcher.reset(line);
    if (matcher.find()) {
      urls.add(line);
    }
  }
  return urls;
}
