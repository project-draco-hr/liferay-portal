{
  Layout layout=LayoutTestUtil.addLayout(_stagingGroup.getGroupId(),RandomTestUtil.randomString(FriendlyURLRandomizerBumper.INSTANCE));
  LayoutTestUtil.addLayout(_stagingGroup.getGroupId(),RandomTestUtil.randomString(FriendlyURLRandomizerBumper.INSTANCE),layout.getPlid());
  String selectedLayoutsJSON=ExportImportHelperUtil.getSelectedLayoutsJSON(_stagingGroup.getGroupId(),false,StringUtil.merge(new long[0]));
  JSONArray selectedLayoutsJSONArray=JSONFactoryUtil.createJSONArray(selectedLayoutsJSON);
  Assert.assertEquals(0,selectedLayoutsJSONArray.length());
}
