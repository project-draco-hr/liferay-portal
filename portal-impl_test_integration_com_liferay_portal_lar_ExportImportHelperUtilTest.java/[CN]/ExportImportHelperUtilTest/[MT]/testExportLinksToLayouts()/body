{
  Element rootElement=_portletDataContextExport.getExportDataRootElement();
  String content=replaceLinksToLayoutsParameters(getContent("layout_links.txt"),_stagingPrivateLayout,_stagingPublicLayout);
  content=ExportImportHelperUtil.replaceExportContentReferences(_portletDataContextExport,_referrerStagedModel,rootElement.element("entry"),content,true);
  assertLinksToLayouts(content,_stagingPrivateLayout,false);
  assertLinksToLayouts(content,_stagingPrivateLayout,true);
  assertLinksToLayouts(content,_stagingPublicLayout,false);
  assertLinksToLayouts(content,_stagingPublicLayout,true);
}
