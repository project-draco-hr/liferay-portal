{
  List<Layout> layouts=LayoutLocalServiceUtil.getLayouts(_stagingGroup.getGroupId(),privateLayout);
  User user=TestPropsValues.getUser();
  Map<String,Serializable> settingsMap=ExportImportConfigurationSettingsMapFactory.buildSettingsMap(user.getUserId(),_stagingGroup.getGroupId(),privateLayout,ExportImportHelperUtil.getLayoutIds(layouts),new HashMap<String,String[]>(),user.getLocale(),user.getTimeZone());
  ExportImportConfiguration exportImportConfiguration=ExportImportConfigurationLocalServiceUtil.addExportImportConfiguration(user.getUserId(),_stagingGroup.getGroupId(),StringPool.BLANK,StringPool.BLANK,ExportImportConfigurationConstants.TYPE_EXPORT_LAYOUT,settingsMap,WorkflowConstants.STATUS_DRAFT,new ServiceContext());
  File larFile=LayoutLocalServiceUtil.exportLayoutsAsFile(exportImportConfiguration);
  LayoutLocalServiceUtil.importLayouts(TestPropsValues.getUserId(),_liveGroup.getGroupId(),privateLayout,new HashMap<String,String[]>(),larFile);
}
