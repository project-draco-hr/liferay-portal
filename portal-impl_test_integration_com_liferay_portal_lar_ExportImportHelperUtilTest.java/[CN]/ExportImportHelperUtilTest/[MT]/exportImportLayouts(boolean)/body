{
  List<Layout> layouts=LayoutLocalServiceUtil.getLayouts(_stagingGroup.getGroupId(),privateLayout);
  File larFile=LayoutLocalServiceUtil.exportLayoutsAsFile(_stagingGroup.getGroupId(),privateLayout,ExportImportHelperUtil.getLayoutIds(layouts),new HashMap<String,String[]>(),null,null);
  LayoutLocalServiceUtil.importLayouts(TestPropsValues.getUserId(),_liveGroup.getGroupId(),privateLayout,new HashMap<String,String[]>(),larFile);
}
