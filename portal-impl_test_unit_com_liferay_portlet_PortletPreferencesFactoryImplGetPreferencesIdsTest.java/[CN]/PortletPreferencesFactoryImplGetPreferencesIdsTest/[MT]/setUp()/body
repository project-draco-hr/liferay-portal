  PortletPreferencesFactoryUtil portletPreferencesFactoryUtil=new PortletPreferencesFactoryUtil();
  portletPreferencesFactoryUtil.setPortletPreferencesFactory(new PortletPreferencesFactoryImpl());
  _layout.setCompanyId(RandomTestUtil.randomLong());
  _layout.setPlid(RandomTestUtil.randomLong());
  _layout.setPrivateLayout(true);
  PowerMockito.mockStatic(LayoutPermissionUtil.class);
  Mockito.when(LayoutPermissionUtil.contains(Mockito.any(PermissionChecker.class),Mockito.eq(_layout),Mockito.eq(ActionKeys.UPDATE))).thenReturn(true);
