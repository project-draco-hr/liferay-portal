{
  if (!_workflowTaskPermissionChecker.hasPermission(themeDisplay.getScopeGroupId(),workflowTask,themeDisplay.getPermissionChecker())) {
    throw new PrincipalException(String.format("User %d does not have permissions to view the task %d ",themeDisplay.getUserId(),workflowTask.getWorkflowTaskId()));
  }
}
