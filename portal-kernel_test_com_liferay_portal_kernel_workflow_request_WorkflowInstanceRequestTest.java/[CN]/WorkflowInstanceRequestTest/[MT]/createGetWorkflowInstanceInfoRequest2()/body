{
  final String relationType="";
  final long relationId=1;
  _context.checking(new Expectations(){
{
      oneOf(_factory).createCredential(0);
      will(returnValue(null));
      oneOf(_manager).getWorkflowInstanceInfo(relationType,relationId,false);
    }
  }
);
  WorkflowInstanceRequest request=WorkflowInstanceRequest.createGetWorkflowInstanceInfoRequest(relationType,relationId,false);
  assertTrue(request.hasReturnValue());
  request.execute(_manager);
}
