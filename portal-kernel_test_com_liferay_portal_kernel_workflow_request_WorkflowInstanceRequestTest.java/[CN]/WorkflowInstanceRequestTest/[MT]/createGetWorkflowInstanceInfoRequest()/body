{
  final long workflowInstanceId=1;
  _context.checking(new Expectations(){
{
      oneOf(_factory).createCredential(0);
      will(returnValue(null));
      oneOf(_manager).getWorkflowInstanceInfo(workflowInstanceId,false);
    }
  }
);
  WorkflowInstanceRequest request=WorkflowInstanceRequest.createGetWorkflowInstanceInfoRequest(workflowInstanceId,false);
  assertTrue(request.hasReturnValue());
  request.execute(_manager);
}
