{
  final long workflowInstanceId=1;
  final Map<String,Object> variables=new HashMap<String,Object>();
  _context.checking(new Expectations(){
{
      oneOf(_factory).createCredential(0);
      will(returnValue(null));
      oneOf(_manager).addContextInformation(workflowInstanceId,variables);
    }
  }
);
  WorkflowInstanceRequest request=WorkflowInstanceRequest.createAddContextInformationRequest(workflowInstanceId,variables);
  assertTrue(request.hasReturnValue());
  request.execute(_manager);
}
