{
  final long workflowInstanceId=1;
  final boolean includeChildren=false;
  _context.checking(new Expectations(){
{
      oneOf(_factory).createCredential(0);
      will(returnValue(null));
      oneOf(_manager).getWorkflowInstanceHistory(workflowInstanceId,includeChildren);
    }
  }
);
  WorkflowInstanceRequest request=WorkflowInstanceRequest.createGetWorkflowInstanceHistoryRequest(workflowInstanceId,includeChildren);
  assertTrue(request.hasReturnValue());
  request.execute(_manager);
}
