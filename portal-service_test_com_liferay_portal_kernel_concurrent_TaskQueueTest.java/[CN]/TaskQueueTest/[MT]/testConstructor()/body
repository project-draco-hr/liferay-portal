{
  try {
    new TaskQueue(0);
    fail();
  }
 catch (  IllegalArgumentException iae) {
  }
  try {
    new TaskQueue(-1);
    fail();
  }
 catch (  IllegalArgumentException iae) {
  }
  TaskQueue taskQueue=new TaskQueue(10);
  assertEquals(10,taskQueue.remainingCapacity());
  taskQueue=new TaskQueue();
  assertEquals(Integer.MAX_VALUE,taskQueue.remainingCapacity());
}
