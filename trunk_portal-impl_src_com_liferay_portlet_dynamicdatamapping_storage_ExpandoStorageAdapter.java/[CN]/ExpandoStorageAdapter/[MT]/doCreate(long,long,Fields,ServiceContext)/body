{
  ExpandoTable expandoTable=_getExpandoTable(companyId,ddmStructureId,fields);
  ExpandoRow expandoRow=ExpandoRowLocalServiceUtil.addRow(expandoTable.getTableId(),CounterLocalServiceUtil.increment());
  _updateFields(expandoTable,expandoRow.getClassPK(),fields);
  DDMStorageLinkLocalServiceUtil.addStorageLink(expandoTable.getClassNameId(),expandoRow.getRowId(),ddmStructureId,serviceContext);
  return expandoRow.getRowId();
}
