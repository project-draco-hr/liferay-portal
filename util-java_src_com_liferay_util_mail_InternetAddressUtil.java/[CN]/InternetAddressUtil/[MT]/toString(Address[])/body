{
  StringBuilder sb=new StringBuilder();
  if (addresses != null) {
    for (int i=0; i < addresses.length; i++) {
      sb.append(toString(addresses[i]));
      if (i < addresses.length - 1) {
        sb.append(StringPool.COMMA);
        sb.append(StringPool.NBSP);
      }
    }
  }
  return sb.toString();
}
