{
  doUpdate(t.getNestedSetsTreeNodeScopeId(),-1,t.getNestedSetsTreeNodeLeft(),false,t.getNestedSetsTreeNodeRight(),false,null);
  doUpdate(t.getNestedSetsTreeNodeScopeId(),true,-2,t.getNestedSetsTreeNodeRight(),false);
  doUpdate(t.getNestedSetsTreeNodeScopeId(),false,-2,t.getNestedSetsTreeNodeRight(),false);
}
