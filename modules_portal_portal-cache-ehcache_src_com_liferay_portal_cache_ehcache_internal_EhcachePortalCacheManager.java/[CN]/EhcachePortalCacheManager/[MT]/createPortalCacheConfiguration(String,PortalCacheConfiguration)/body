{
  EhcachePortalCacheConfiguration ehcachePortalCacheConfiguration=(EhcachePortalCacheConfiguration)defaultPortalCacheConfiguration;
  return new EhcachePortalCacheConfiguration(name,ehcachePortalCacheConfiguration.getCacheListenerConfigurations(),ehcachePortalCacheConfiguration.getBootstrapLoaderConfiguration(),ehcachePortalCacheConfiguration.isRequireSerialization());
}
