{
  ObjectValuePair<Configuration,PortalCacheManagerConfiguration> configurationObjectValuePair=EhcacheConfigurationHelperUtil.getConfigurationObjectValuePair(configurationURL,isClusterAware(),_usingDefault,props);
  reconfigEhcache(configurationObjectValuePair.getKey());
  reconfigPortalCache(configurationObjectValuePair.getValue());
}
