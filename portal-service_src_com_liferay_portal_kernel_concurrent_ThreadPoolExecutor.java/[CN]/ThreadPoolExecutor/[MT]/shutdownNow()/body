{
  _mainLock.lock();
  try {
    int state=_runState;
    if (state < _STOP) {
      _runState=_STOP;
    }
    for (    WorkerTask workerTask : _workerTasks) {
      workerTask._thread.interrupt();
    }
    List<Runnable> runnables=new ArrayList<Runnable>();
    _taskQueue.drainTo(runnables);
    _tryTerminate();
    return runnables;
  }
  finally {
    _mainLock.unlock();
  }
}
