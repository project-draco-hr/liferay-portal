{
  _mainLock.lock();
  try {
    long count=_completedTaskCount;
    for (    WorkerTask worker : _workerTaskSet) {
      count+=worker._localCompletedTaskCount;
      if (worker._isLocked()) {
        count++;
      }
    }
    return count + _taskQueue.size();
  }
  finally {
    _mainLock.unlock();
  }
}
