{
  String key=_encodeKey(groupId);
  Map<String,List<CalEvent>> eventsPool=(ConcurrentHashMap<String,List<CalEvent>>)_portalCache.get(key);
  if (eventsPool == null) {
    eventsPool=new ConcurrentHashMap<String,List<CalEvent>>();
    _portalCache.put(key,(Serializable)eventsPool);
  }
  return eventsPool;
}
