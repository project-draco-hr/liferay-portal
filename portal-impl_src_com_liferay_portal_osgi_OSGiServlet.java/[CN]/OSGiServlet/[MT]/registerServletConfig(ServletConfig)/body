{
  Hashtable<String,Object> properties=new Hashtable<String,Object>();
  properties.put(OSGiConstants.PORTAL_SERVICE_BEAN_NAME,ServletConfig.class.getName());
  properties.put(Constants.SERVICE_VENDOR,ReleaseInfo.getVendor());
  _bundleContext.registerService(new String[]{ServletConfig.class.getName()},servletConfig,properties);
}
