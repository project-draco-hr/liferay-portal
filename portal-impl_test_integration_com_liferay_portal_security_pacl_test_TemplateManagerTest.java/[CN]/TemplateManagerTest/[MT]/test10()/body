{
  Template template=TemplateManagerUtil.getTemplate(TemplateConstants.LANG_TYPE_VM,new StringTemplateResource("123.vm","#set($sum = 5 + 6)$sum"),TemplateContextType.STANDARD);
  UnsyncStringWriter unsyncStringWriter=new UnsyncStringWriter();
  template.processTemplate(unsyncStringWriter);
  String result=unsyncStringWriter.toString();
  Assert.assertEquals(11,GetterUtil.getInteger(result));
}
