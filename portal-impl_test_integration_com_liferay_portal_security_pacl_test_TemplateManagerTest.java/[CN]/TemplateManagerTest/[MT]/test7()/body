{
  Template template=TemplateManagerUtil.getTemplate(TemplateConstants.LANG_TYPE_VM,new StringTemplateResource("123.vm","#if ($httpUtil) FAIL #else PASS #end"),TemplateContextType.STANDARD);
  UnsyncStringWriter unsyncStringWriter=new UnsyncStringWriter();
  template.processTemplate(unsyncStringWriter);
  String result=unsyncStringWriter.toString().trim();
  Assert.assertEquals("PASS",result);
}
