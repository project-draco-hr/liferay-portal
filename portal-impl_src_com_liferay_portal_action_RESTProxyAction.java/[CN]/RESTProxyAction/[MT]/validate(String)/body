{
  if (Validator.isNull(url) || !HttpUtil.hasDomain(url)) {
    return false;
  }
  String[] allowedUrlPrefixes=PropsValues.REST_PROXY_URL_PREFIXES_ALLOWED;
  if (allowedUrlPrefixes.length == 0) {
    return true;
  }
  for (  String allowedUrlPrefix : allowedUrlPrefixes) {
    if (StringUtil.startsWith(url,allowedUrlPrefix)) {
      return true;
    }
  }
  if (_log.isDebugEnabled()) {
    _log.debug("URL " + url + " is not allowed");
  }
  return false;
}
