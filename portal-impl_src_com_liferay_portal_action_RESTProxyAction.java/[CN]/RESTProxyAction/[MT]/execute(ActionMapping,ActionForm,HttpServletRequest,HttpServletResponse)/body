{
  String url=ParamUtil.getString(request,"url");
  if (validate(url)) {
    String location=url;
    String content=StringPool.BLANK;
    int pos=location.indexOf(CharPool.QUESTION);
    if (pos != -1) {
      location=url.substring(0,pos);
      content=url.substring(pos + 1);
    }
    Http.Options options=new Http.Options();
    options.setBody(content,ContentTypes.APPLICATION_X_WWW_FORM_URLENCODED,StringPool.UTF8);
    options.setLocation(location);
    options.setPost(true);
    content=HttpUtil.URLtoString(options);
    ServletResponseUtil.write(response,content);
  }
  return null;
}
