{
  Map<String,Serializable> workflowContext=new HashMap<String,Serializable>();
  workflowContext.put(WorkflowConstants.CONTEXT_URL,"http://localhost");
  message=MBMessageLocalServiceUtil.updateStatus(message.getUserId(),message.getMessageId(),WorkflowConstants.STATUS_APPROVED,serviceContext,workflowContext);
  return message;
}
