{
  for (  UserGroupRole userGroupRole : userGroupRoles) {
    if (userGroupRole.getRole().getType() != RoleConstants.TYPE_ORGANIZATION) {
      userGroupRoles.remove(userGroupRole);
    }
  }
  OrganizationMembershipPolicy membershipPolicy=OrganizationMembershipPolicyFactoryUtil.getMembershipPolicy();
  membershipPolicy.propagateAddRoles(userGroupRoles);
}
