{
  DDMFormFieldTypeServicesTracker ddmFormFieldTypeServicesTracker=mock(DDMFormFieldTypeServicesTracker.class);
  Map<String,Object> properties=new HashMap<>();
  properties.put("ddm.form.field.type.icon","my-icon");
  properties.put("ddm.form.field.type.js.class.name","myJavaScriptClass");
  properties.put("ddm.form.field.type.js.module","myJavaScriptModule");
  when(ddmFormFieldTypeServicesTracker.getDDMFormFieldTypeProperties(Matchers.anyString())).thenReturn(properties);
  DDMFormFieldRenderer ddmFormFieldRenderer=mock(DDMFormFieldRenderer.class);
  whenDDMFormFieldRendererGetTemplateNamespace(ddmFormFieldRenderer,"_templateNamespace_");
  when(ddmFormFieldTypeServicesTracker.getDDMFormFieldRenderer(Matchers.anyString())).thenReturn(ddmFormFieldRenderer);
  return ddmFormFieldTypeServicesTracker;
}
