{
  try {
    LayoutSet layoutSet=layout.getLayoutSet();
    if (layout.isLayoutPrototypeLinkEnabled() || layoutSet.isLayoutSetPrototypeLinkEnabled()) {
      LayoutTypePortletImpl layoutTypePortlet=new LayoutTypePortletImpl(layout);
      return isLayoutLocked(layoutTypePortlet);
    }
  }
 catch (  Exception e) {
  }
  return false;
}
