{
  if (layoutSet.isLayoutSetPrototypeLinkEnabled()) {
    try {
      LayoutSetPrototype layoutSetPrototype=LayoutSetPrototypeLocalServiceUtil.getLayoutSetPrototypeByUuid(layoutSet.getLayoutSetPrototypeUuid());
      String allowModifications=layoutSetPrototype.getSettingsProperty("allowModifications");
      if (Validator.isNotNull(allowModifications)) {
        return !GetterUtil.getBoolean(allowModifications);
      }
    }
 catch (    Exception e) {
    }
  }
  return false;
}
