{
  if (!layoutSet.isLayoutSetPrototypeLinkActive()) {
    return true;
  }
  try {
    LayoutSetPrototype layoutSetPrototype=LayoutSetPrototypeLocalServiceUtil.getLayoutSetPrototypeByUuid(layoutSet.getLayoutSetPrototypeUuid());
    String layoutsUpdateable=layoutSetPrototype.getSettingsProperty("layoutsUpdateable");
    if (Validator.isNotNull(layoutsUpdateable)) {
      return GetterUtil.getBoolean(layoutsUpdateable,true);
    }
  }
 catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      _log.debug(e,e);
    }
  }
  return true;
}
