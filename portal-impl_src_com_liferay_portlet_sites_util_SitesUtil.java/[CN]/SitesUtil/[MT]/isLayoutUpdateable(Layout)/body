{
  try {
    LayoutSet layoutSet=layout.getLayoutSet();
    if ((layout.isLayoutPrototypeLinkEnabled() || layoutSet.isLayoutSetPrototypeLinkEnabled()) && (Validator.isNotNull(layout.getSourcePrototypeLayoutUuid()))) {
      LayoutTypePortletImpl layoutTypePortlet=new LayoutTypePortletImpl(layout);
      return isLayoutUpdateable(layoutTypePortlet);
    }
  }
 catch (  Exception e) {
  }
  return true;
}
