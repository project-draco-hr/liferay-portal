{
  request.setAttribute("liferay-ui:ratings:className",_className);
  request.setAttribute("liferay-ui:ratings:classPK",String.valueOf(_classPK));
  request.setAttribute("liferay-ui:ratings:numberOfStars",String.valueOf(_numberOfStars));
  request.setAttribute("liferay-ui:ratings:preSetRatingsEntry",_preSetRatingsEntry);
  if (_preSetRatingsEntry) {
    request.setAttribute("liferay-ui:ratings:ratingsEntry",_ratingsEntry);
  }
  request.setAttribute("liferay-ui:ratings:preSetRatingsStats",_preSetRatingsStats);
  if (_preSetRatingsStats) {
    request.setAttribute("liferay-ui:ratings:ratingsStats",_ratingsStats);
  }
  request.setAttribute("liferay-ui:ratings:type",_type);
  request.setAttribute("liferay-ui:ratings:url",_url);
}
