{
  Boolean hasPermission=StagingPermissionUtil.hasPermission(permissionChecker,classPK,RESOURCE_NAME,classPK,PortletKeys.MESSAGE_BOARDS,actionId);
  if (hasPermission != null) {
    return hasPermission.booleanValue();
  }
  return permissionChecker.hasPermission(classPK,RESOURCE_NAME,classPK,actionId);
}
