{
  String content=null;
  try {
    content=getContent(req,res);
  }
 catch (  Exception e) {
    res.sendError(HttpServletResponse.SC_INTERNAL_SERVER_ERROR,e.getMessage());
    return;
  }
  res.setContentType("text/javascript");
  res.setHeader("Cache-Control","no-cache");
  PrintWriter pw=res.getWriter();
  pw.write(content);
  pw.close();
}
