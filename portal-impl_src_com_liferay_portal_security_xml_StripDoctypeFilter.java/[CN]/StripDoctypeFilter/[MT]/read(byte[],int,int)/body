{
  if (_documentStarted) {
    return _inputStream.read(bytes,offset,length);
  }
  int read=0;
  for (read=0; read < length; read++) {
    int c=read();
    if (c == -1) {
      if (read == 0) {
        return -1;
      }
      return read;
    }
    bytes[offset + read]=(byte)(c & 0xFF);
  }
  return read;
}
