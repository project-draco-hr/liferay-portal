{
  long fileShortcutId=ParamUtil.getLong(actionRequest,"fileShortcutId");
  long newFolderId=ParamUtil.getLong(actionRequest,"newFolderId");
  DLFileShortcut fileShortcut=DLAppServiceUtil.getFileShortcut(fileShortcutId);
  ServiceContext serviceContext=ServiceContextFactory.getInstance(DLFileShortcut.class.getName(),actionRequest);
  DLAppServiceUtil.updateFileShortcut(fileShortcutId,newFolderId,fileShortcut.getToFileEntryId(),serviceContext);
}
