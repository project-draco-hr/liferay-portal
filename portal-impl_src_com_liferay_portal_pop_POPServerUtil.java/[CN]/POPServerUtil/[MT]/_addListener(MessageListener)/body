{
  if (listener == null) {
    if (_log.isDebugEnabled()) {
      _log.debug("Do not add null listener");
    }
    return;
  }
  Registry registry=RegistryUtil.getRegistry();
  ServiceRegistration<MessageListener> serviceRegistration=registry.registerService(MessageListener.class,listener);
  _serviceRegistrations.put(listener,serviceRegistration);
}
