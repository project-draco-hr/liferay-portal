{
  String tagsEntries=ParamUtil.getString(req,"tagsEntries");
  if (_log.isDebugEnabled()) {
    _log.debug("Compile entries " + tagsEntries);
  }
  req.setAttribute(WebKeys.TAGS_COMPILER_ENTRIES,StringUtil.split(tagsEntries));
  HttpServletRequest httpReq=PortalUtil.getHttpServletRequest(req);
  ThemeDisplay themeDisplay=(ThemeDisplay)req.getAttribute(WebKeys.THEME_DISPLAY);
  RenderParametersPool.clear(httpReq,themeDisplay.getPlid(),PortletKeys.TAGS_COMPILER);
}
