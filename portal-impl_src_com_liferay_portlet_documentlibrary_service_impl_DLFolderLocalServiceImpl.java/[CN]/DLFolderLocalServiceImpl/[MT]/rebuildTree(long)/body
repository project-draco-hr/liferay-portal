{
  TreePathUtil.rebuildTree(companyId,DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,new TreeModelFinder<DLFolder>(){
    @Override public List<DLFolder> findTreeModels(    long previousId,    long companyId,    long parentId,    int size) throws SystemException {
      return dlFolderPersistence.findByF_C_P_NotS(previousId,companyId,parentId,WorkflowConstants.STATUS_IN_TRASH,QueryUtil.ALL_POS,size,new FolderIdComparator());
    }
  }
);
}
