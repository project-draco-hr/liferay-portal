{
  if (Validator.isNotNull(lockUuid)) {
    try {
      Lock lock=LockHelperUtil.getLock(DLFolder.class.getName(),folderId);
      if (!lockUuid.equals(lock.getUuid())) {
        throw new InvalidLockException("UUIDs do not match");
      }
    }
 catch (    PortalException pe) {
      if (pe instanceof ExpiredLockException || pe instanceof NoSuchLockException) {
      }
 else {
        throw pe;
      }
    }
  }
  LockHelperUtil.unlock(DLFolder.class.getName(),folderId);
}
