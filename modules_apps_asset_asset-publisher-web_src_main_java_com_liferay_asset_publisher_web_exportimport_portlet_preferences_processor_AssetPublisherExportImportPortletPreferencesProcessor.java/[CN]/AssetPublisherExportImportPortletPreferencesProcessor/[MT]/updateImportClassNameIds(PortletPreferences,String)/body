{
  String[] oldValues=portletPreferences.getValues(key,null);
  if (oldValues == null) {
    return;
  }
  String[] newValues=new String[oldValues.length];
  int i=0;
  for (  String oldValue : oldValues) {
    if (key.equals("anyAssetType") && (oldValue.equals("false") || oldValue.equals("true"))) {
      newValues[i++]=oldValue;
      continue;
    }
    try {
      long classNameId=PortalUtil.getClassNameId(oldValue);
      newValues[i++]=String.valueOf(classNameId);
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to find class name ID for class name " + oldValue);
      }
    }
  }
  portletPreferences.setValues(key,newValues);
}
