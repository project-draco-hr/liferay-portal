{
  if ((Validator.isNull(name)) || (Validator.isNumber(name)) || (name.indexOf(StringPool.COMMA) != -1)|| (name.indexOf(StringPool.STAR) != -1)) {
    throw new RoleNameException();
  }
  try {
    Role role=RoleFinder.findByC_N(companyId,name);
    if ((roleId <= 0) || (role.getRoleId() != roleId)) {
      throw new DuplicateRoleException();
    }
  }
 catch (  NoSuchRoleException nsge) {
  }
}
