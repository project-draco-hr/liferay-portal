{
  if (PropsValues.PERMISSIONS_USER_CHECK_ALGORITHM == 6) {
    resourcePermissionLocalService.setResourcePermissions(companyId,name,ResourceConstants.SCOPE_COMPANY,"0",role.getRoleId(),actionIds);
  }
 else {
    permissionLocalService.setRolePermissions(role.getRoleId(),companyId,name,ResourceConstants.SCOPE_COMPANY,"0",actionIds);
  }
}
