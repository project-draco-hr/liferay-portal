{
  if ((classNameId == PortalUtil.getClassNameId(Role.class)) && ((Validator.isNull(name)) || (Validator.isNumber(name)) || (name.indexOf(CharPool.COMMA) != -1)|| (name.indexOf(CharPool.STAR) != -1))) {
    throw new RoleNameException();
  }
  try {
    Role role=roleFinder.findByC_N(companyId,name);
    if (role.getRoleId() != roleId) {
      throw new DuplicateRoleException();
    }
  }
 catch (  NoSuchRoleException nsge) {
  }
}
