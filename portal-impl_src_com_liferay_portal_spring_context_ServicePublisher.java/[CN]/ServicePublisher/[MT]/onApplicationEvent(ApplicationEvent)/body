{
  if (applicationEvent instanceof ContextRefreshedEvent) {
    ContextRefreshedEvent contextRefreshedEvent=(ContextRefreshedEvent)applicationEvent;
    ModuleFrameworkUtilAdapter.registerContext(contextRefreshedEvent.getApplicationContext());
  }
 else   if (applicationEvent instanceof ContextClosedEvent) {
    ContextClosedEvent contextClosedEvent=(ContextClosedEvent)applicationEvent;
    ModuleFrameworkUtilAdapter.unregisterContext(contextClosedEvent.getApplicationContext());
  }
}
