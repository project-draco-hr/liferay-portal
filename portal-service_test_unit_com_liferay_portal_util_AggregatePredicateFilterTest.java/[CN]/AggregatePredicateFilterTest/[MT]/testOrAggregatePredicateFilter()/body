{
  List<String> list=Arrays.asList("Bob","Clair","James","Joe");
  AggregatePredicateFilter<String> aggregatePredicateFilter=new AggregatePredicateFilter<>(new StartsWithPredicateFilter("J"));
  aggregatePredicateFilter=aggregatePredicateFilter.or(new EndsWithPredicateFilter("R"));
  Assert.assertEquals(Arrays.asList("Clair","James","Joe"),ListUtil.filter(list,aggregatePredicateFilter));
}
