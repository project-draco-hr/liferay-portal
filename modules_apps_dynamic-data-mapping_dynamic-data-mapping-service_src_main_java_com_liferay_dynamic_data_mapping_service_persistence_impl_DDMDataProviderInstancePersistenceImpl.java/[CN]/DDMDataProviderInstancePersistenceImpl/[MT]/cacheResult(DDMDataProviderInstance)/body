{
  entityCache.putResult(DDMDataProviderInstanceModelImpl.ENTITY_CACHE_ENABLED,DDMDataProviderInstanceImpl.class,ddmDataProviderInstance.getPrimaryKey(),ddmDataProviderInstance);
  finderCache.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddmDataProviderInstance.getUuid(),ddmDataProviderInstance.getGroupId()},ddmDataProviderInstance);
  ddmDataProviderInstance.resetOriginalValues();
}
