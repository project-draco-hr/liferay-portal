{
  Session session=null;
  try {
    session=openSession();
    DDMDataProviderInstance ddmDataProviderInstance=(DDMDataProviderInstance)session.get(DDMDataProviderInstanceImpl.class,primaryKey);
    if (ddmDataProviderInstance == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchDataProviderInstanceException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(ddmDataProviderInstance);
  }
 catch (  NoSuchDataProviderInstanceException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
