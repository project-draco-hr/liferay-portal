{
  Assert.assertTrue(LanguageValidator.isValid(LanguageConstants.KEY_LINE_END,"left"));
  Assert.assertTrue(LanguageValidator.isValid(LanguageConstants.KEY_LINE_END,"right"));
  Assert.assertFalse(LanguageValidator.isValid(LanguageConstants.KEY_LINE_END,"up"));
  Assert.assertFalse(LanguageValidator.isValid(LanguageConstants.KEY_LINE_END,"down"));
  Assert.assertFalse(LanguageValidator.isValid(LanguageConstants.KEY_LINE_END,"any-other-value"));
  Assert.assertFalse(LanguageValidator.isValid(LanguageConstants.KEY_LINE_END,"Any other value"));
}
