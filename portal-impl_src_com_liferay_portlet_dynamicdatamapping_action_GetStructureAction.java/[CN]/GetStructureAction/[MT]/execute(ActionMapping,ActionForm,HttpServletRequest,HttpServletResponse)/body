{
  try {
    long structureId=ParamUtil.getLong(request,"structureId");
    DDMStructure structure=DDMStructureServiceUtil.getStructure(structureId);
    String definition=structure.getDefinition();
    ServletResponseUtil.sendFile(request,response,null,definition.getBytes(),ContentTypes.TEXT_XML_UTF8);
    return null;
  }
 catch (  Exception e) {
    PortalUtil.sendError(e,request,response);
    return null;
  }
}
