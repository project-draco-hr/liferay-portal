{
  Primitive primitive=new Primitive();
  ObjectGraphUtil.walkObjectGraph(primitive,_recordVisitor);
  Assert.assertEquals(1,_objectValuePairs.size());
  ObjectValuePair<Field,Object> objectValuePair=_objectValuePairs.get(0);
  Assert.assertEquals(ReflectionUtil.getDeclaredField(Primitive.class,"_int"),objectValuePair.getKey());
  Assert.assertSame(primitive,objectValuePair.getValue());
}
