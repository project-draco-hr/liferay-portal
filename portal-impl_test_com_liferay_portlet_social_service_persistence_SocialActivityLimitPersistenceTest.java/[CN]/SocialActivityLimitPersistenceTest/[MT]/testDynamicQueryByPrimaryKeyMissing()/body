{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SocialActivityLimit.class,SocialActivityLimit.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("activityLimitId",nextLong()));
  List<SocialActivityLimit> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
