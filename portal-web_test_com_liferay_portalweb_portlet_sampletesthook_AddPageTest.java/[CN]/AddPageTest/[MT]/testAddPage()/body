{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("my-community-private-pages")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("my-community-private-pages"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//div[@id='add-page']/a/span");
  selenium.typeKeys("new_page",RuntimeVariables.replace("Sample Test Hook Page"));
  selenium.type("new_page",RuntimeVariables.replace("Sample Test Hook Page"));
  selenium.click("link=Save");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Sample Test Hook Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Sample Test Hook Page"));
  selenium.waitForPageToLoad("30000");
}
