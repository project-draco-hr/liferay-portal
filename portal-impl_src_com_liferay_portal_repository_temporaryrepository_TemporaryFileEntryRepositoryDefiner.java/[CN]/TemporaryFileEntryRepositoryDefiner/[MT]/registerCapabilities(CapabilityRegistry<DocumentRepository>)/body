{
  DocumentRepository documentRepository=capabilityRegistry.getTarget();
  DLFileEntryServiceAdapter dlFileEntryServiceAdapter=DLFileEntryServiceAdapter.create(documentRepository);
  capabilityRegistry.addExportedCapability(BulkOperationCapability.class,new LiferayBulkOperationCapability(documentRepository,dlFileEntryServiceAdapter,DLFolderServiceAdapter.create(documentRepository)));
  capabilityRegistry.addExportedCapability(TemporaryFileEntriesCapability.class,new TemporaryFileEntriesCapabilityImpl(documentRepository));
  capabilityRegistry.addSupportedCapability(WorkflowCapability.class,new MinimalWorkflowCapability(dlFileEntryServiceAdapter));
}
