{
  _serviceTrackerMap=ServiceTrackerMapFactory.multiValueMap(_bundleContext,TrackedOne.class,null,new PropertyServiceReferenceMapper<String,TrackedOne>("target"),new Comparator<ServiceReference<TrackedOne>>(){
    @Override public int compare(    ServiceReference<TrackedOne> o1,    ServiceReference<TrackedOne> o2){
      return 0;
    }
  }
);
  _serviceTrackerMap.open();
  registerService(new TrackedOne());
  registerService(new TrackedOne());
  List<TrackedOne> services=_serviceTrackerMap.getService("aTarget");
  Assert.assertEquals(2,services.size());
}
