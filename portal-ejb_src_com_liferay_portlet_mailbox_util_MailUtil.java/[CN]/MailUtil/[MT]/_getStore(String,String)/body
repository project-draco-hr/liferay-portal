{
  Store store=(Store)_store.get(userId);
  if (store == null) {
    store=_getSession().getStore("imap");
    store.connect(_getIMAPHost(),_getLogin(userId),password);
    _store.put(userId,store);
    if (_folder == null) {
      setCurrentFolder(userId,password,MAIL_INBOX_NAME);
    }
  }
  return store;
}
