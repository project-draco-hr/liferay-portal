{
  AuthVerifier authVerifier=authVerifierConfiguration.getAuthVerifier();
  Properties properties=authVerifierConfiguration.getProperties();
  String[] urlsRejected=StringUtil.split(properties.getProperty("urls.rejected"));
  if ((urlsRejected.length > 0) && (Wildcard.matchOne(requestURI,urlsRejected) > -1)) {
    return false;
  }
  String[] urls=StringUtil.split(properties.getProperty("urls"));
  if (urls.length == 0) {
    Class<?> authVerifierClass=authVerifier.getClass();
    _log.error("Auth verifier " + authVerifierClass.getName() + " does not have any URLs configured");
    return false;
  }
  return Wildcard.matchOne(requestURI,urls) > -1;
}
