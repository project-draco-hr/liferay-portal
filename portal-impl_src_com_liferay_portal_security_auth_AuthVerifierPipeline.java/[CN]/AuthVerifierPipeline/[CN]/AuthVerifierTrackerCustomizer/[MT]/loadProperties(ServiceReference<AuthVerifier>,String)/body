{
  Map<String,Object> serviceReferenceProperties=serviceReference.getProperties();
  Properties result=new Properties();
  String authVerifierSettingsKey=getAuthVerifierPropertyName(authVerifierClassName);
  for (  String settingsKey : serviceReferenceProperties.keySet()) {
    String propertiesKey=settingsKey;
    if (settingsKey.startsWith(authVerifierSettingsKey)) {
      propertiesKey=settingsKey.substring(authVerifierSettingsKey.length());
    }
    Object settingsValue=serviceReferenceProperties.get(settingsKey);
    if (settingsValue instanceof String) {
      result.setProperty(propertiesKey,(String)settingsValue);
    }
  }
  return result;
}
