{
  Properties properties=new Properties();
  String authVerifierPropertyName=getAuthVerifierPropertyName(authVerifierClassName);
  Map<String,Object> serviceReferenceProperties=serviceReference.getProperties();
  for (  String key : serviceReferenceProperties.keySet()) {
    String propertiesKey=key;
    if (key.startsWith(authVerifierPropertyName)) {
      propertiesKey=key.substring(authVerifierPropertyName.length());
    }
    Object value=serviceReferenceProperties.get(key);
    properties.setProperty(propertiesKey,String.valueOf(value));
  }
  return properties;
}
