{
  Properties properties=authVerifierConfiguration.getProperties();
  String[] urlsIncludes=StringUtil.split(properties.getProperty("urls.includes"));
  if ((urlsIncludes.length == 0) && _log.isWarnEnabled()) {
    _log.warn("Auth verifier " + authVerifierConfiguration.getAuthVerifierClassName() + " does not have URLs configured");
    return false;
  }
  return true;
}
