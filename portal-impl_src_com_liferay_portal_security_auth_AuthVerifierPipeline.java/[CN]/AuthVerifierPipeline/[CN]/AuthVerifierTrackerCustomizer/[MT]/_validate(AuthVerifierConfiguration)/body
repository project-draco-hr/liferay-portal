{
  Properties properties=authVerifierConfiguration.getProperties();
  String[] urlsIncludes=StringUtil.split(properties.getProperty("urls.includes"));
  if (urlsIncludes.length == 0) {
    if (_log.isWarnEnabled()) {
      String authVerifierClassName=authVerifierConfiguration.getAuthVerifierClassName();
      _log.warn("Auth verifier " + authVerifierClassName + " does not have URLs configured");
    }
    return false;
  }
  return true;
}
