{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLFileShortcut.class,DLFileShortcut.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("fileShortcutId",ServiceTestUtil.nextLong()));
  List<DLFileShortcut> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
