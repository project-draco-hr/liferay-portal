{
  Session session=null;
  try {
    session=openSession();
    SocialActivityAchievement socialActivityAchievement=(SocialActivityAchievement)session.get(SocialActivityAchievementImpl.class,Long.valueOf(activityAchievementId));
    if (socialActivityAchievement == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + activityAchievementId);
      }
      throw new NoSuchActivityAchievementException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + activityAchievementId);
    }
    return socialActivityAchievementPersistence.remove(socialActivityAchievement);
  }
 catch (  NoSuchActivityAchievementException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
