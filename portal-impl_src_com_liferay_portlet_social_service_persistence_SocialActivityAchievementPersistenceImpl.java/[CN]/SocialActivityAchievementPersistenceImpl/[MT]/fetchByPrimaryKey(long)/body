{
  SocialActivityAchievement socialActivityAchievement=(SocialActivityAchievement)EntityCacheUtil.getResult(SocialActivityAchievementModelImpl.ENTITY_CACHE_ENABLED,SocialActivityAchievementImpl.class,activityAchievementId);
  if (socialActivityAchievement == _nullSocialActivityAchievement) {
    return null;
  }
  if (socialActivityAchievement == null) {
    Session session=null;
    try {
      session=openSession();
      socialActivityAchievement=(SocialActivityAchievement)session.get(SocialActivityAchievementImpl.class,Long.valueOf(activityAchievementId));
      if (socialActivityAchievement != null) {
        cacheResult(socialActivityAchievement);
      }
 else {
        EntityCacheUtil.putResult(SocialActivityAchievementModelImpl.ENTITY_CACHE_ENABLED,SocialActivityAchievementImpl.class,activityAchievementId,_nullSocialActivityAchievement);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(SocialActivityAchievementModelImpl.ENTITY_CACHE_ENABLED,SocialActivityAchievementImpl.class,activityAchievementId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return socialActivityAchievement;
}
