{
  addCanonicalPath(paths,directory.getCanonicalPath() + File.separatorChar);
  for (  File file : directory.listFiles()) {
    if (file.isDirectory()) {
      addCanonicalPaths(paths,file);
    }
 else {
      File canonicalFile=new File(file.getCanonicalPath());
      File parentFile=canonicalFile.getParentFile();
      addCanonicalPath(paths,parentFile.getPath() + File.separatorChar);
    }
  }
}
