{
  for (  Permission deleteFilePermission : _deletePermissions) {
    if (deleteFilePermission.implies(permission)) {
      return true;
    }
  }
  if (isJSPCompiler(permission.getName(),FILE_PERMISSION_ACTION_DELETE)) {
    return true;
  }
  if (isResinVFS()) {
    return true;
  }
  return false;
}
