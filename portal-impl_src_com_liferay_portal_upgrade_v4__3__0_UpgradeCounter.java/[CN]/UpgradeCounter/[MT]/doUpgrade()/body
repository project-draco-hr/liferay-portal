{
  List<String> names=CounterLocalServiceUtil.getNames();
  for (  String name : names) {
    if (name.startsWith("com.liferay.") && !name.equals(CounterModelImpl.class.getName()) && !name.equals(Permission.class.getName())&& !name.equals(Resource.class.getName())&& !name.equals(ResourceCode.class.getName())&& !name.equals(UserTracker.class.getName())) {
      CounterLocalServiceUtil.reset(name);
    }
  }
  UpgradeTable upgradeTable=UpgradeTableFactoryUtil.getUpgradeTable(_TABLE_COUNTER,_COLUMNS_COUNTER);
  upgradeTable.setCreateSQL(_CREATE_COUNTER);
  upgradeTable.updateTable();
}
