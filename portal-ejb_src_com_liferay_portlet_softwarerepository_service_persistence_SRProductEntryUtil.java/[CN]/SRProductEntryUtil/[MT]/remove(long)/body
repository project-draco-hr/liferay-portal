{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(productEntryId));
  }
  com.liferay.portlet.softwarerepository.model.SRProductEntry srProductEntry=getPersistence().remove(productEntryId);
  if (listener != null) {
    listener.onAfterRemove(srProductEntry);
  }
  return srProductEntry;
}
