{
  Object[] finderArgs=new Object[]{pk,rolePK};
  Boolean value=(Boolean)FinderCacheUtil.getResult(FINDER_PATH_CONTAINS_ROLE,finderArgs,this);
  if (value == null) {
    try {
      value=Boolean.valueOf(containsRole.contains(pk,rolePK));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (value == null) {
        value=Boolean.FALSE;
      }
      FinderCacheUtil.putResult(FINDER_PATH_CONTAINS_ROLE,finderArgs,value);
    }
  }
  return value.booleanValue();
}
