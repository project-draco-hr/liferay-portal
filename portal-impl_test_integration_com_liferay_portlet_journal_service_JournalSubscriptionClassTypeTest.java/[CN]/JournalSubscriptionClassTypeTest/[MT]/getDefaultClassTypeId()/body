{
  Company company=CompanyLocalServiceUtil.getCompany(group.getCompanyId());
  DDMStructure ddmStructure=DDMStructureLocalServiceUtil.getStructure(company.getGroupId(),PortalUtil.getClassNameId(JournalArticle.class),"BASIC-WEB-CONTENT");
  Assert.assertNotNull(ddmStructure);
  return ddmStructure.getStructureId();
}
