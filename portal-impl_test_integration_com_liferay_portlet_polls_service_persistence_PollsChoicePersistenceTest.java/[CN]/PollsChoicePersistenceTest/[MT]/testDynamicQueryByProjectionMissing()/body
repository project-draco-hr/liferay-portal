{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PollsChoice.class,PollsChoice.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("choiceId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("choiceId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
