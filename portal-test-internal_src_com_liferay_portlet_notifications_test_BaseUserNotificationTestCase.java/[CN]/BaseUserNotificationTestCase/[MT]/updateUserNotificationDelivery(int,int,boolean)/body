{
  boolean exists=false;
  for (  UserNotificationDelivery userNotificationDelivery : userNotificationDeliveries) {
    if ((userNotificationDelivery.getNotificationType() != notificationType) || (userNotificationDelivery.getDeliveryType() != deliveryType)) {
      continue;
    }
    UserNotificationDeliveryLocalServiceUtil.updateUserNotificationDelivery(userNotificationDelivery.getUserNotificationDeliveryId(),deliver);
    exists=true;
    break;
  }
  Assert.assertTrue("User notification does not exist",exists);
}
