{
  ResourceBlock resourceBlock=(ResourceBlock)EntityCacheUtil.getResult(ResourceBlockModelImpl.ENTITY_CACHE_ENABLED,ResourceBlockImpl.class,resourceBlockId,this);
  if (resourceBlock == _nullResourceBlock) {
    return null;
  }
  if (resourceBlock == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      resourceBlock=(ResourceBlock)session.get(ResourceBlockImpl.class,Long.valueOf(resourceBlockId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (resourceBlock != null) {
        cacheResult(resourceBlock);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(ResourceBlockModelImpl.ENTITY_CACHE_ENABLED,ResourceBlockImpl.class,resourceBlockId,_nullResourceBlock);
      }
      closeSession(session);
    }
  }
  return resourceBlock;
}
