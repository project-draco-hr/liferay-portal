{
  ClassLoader portletClassLoader=(ClassLoader)servletContext.getAttribute(PortletServlet.PORTLET_CLASS_LOADER);
  _jsonWebServiceServiceAction=new JSONWebServiceServiceAction(ContextPathUtil.getContextPath(servletContext),portletClassLoader);
  _jsonWebServiceServiceAction.setServletContext(servletContext);
  return _jsonWebServiceServiceAction;
}
