{
  _mockHttpServletRequest.setRequestURI(_STARTING_WITH_PROXY_FRIENDLY_URL);
  _portalPathContext=_PROXY;
  _portalPathProxy=_PROXY;
  String filteredFriendlyUrl=getFilteredFriendlyUrl(_mockHttpServletRequest,_mockHttpServletResponse,_mockFilterChain);
  Assert.assertEquals(_STARTING_WITH_PROXY_FRIENDLY_URL,filteredFriendlyUrl);
}
