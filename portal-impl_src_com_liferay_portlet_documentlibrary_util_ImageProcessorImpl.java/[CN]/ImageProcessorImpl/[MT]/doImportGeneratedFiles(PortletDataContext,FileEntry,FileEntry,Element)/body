{
  importThumbnails(portletDataContext,fileEntry,importedFileEntry,fileEntryElement,"image");
  FileVersion importedFileVersion=importedFileEntry.getFileVersion();
  if (!_previewGenerationRequired(importedFileVersion)) {
    return;
  }
  importPreview(portletDataContext,fileEntry,importedFileEntry,fileEntryElement,"image",getPreviewType(importedFileVersion));
}
