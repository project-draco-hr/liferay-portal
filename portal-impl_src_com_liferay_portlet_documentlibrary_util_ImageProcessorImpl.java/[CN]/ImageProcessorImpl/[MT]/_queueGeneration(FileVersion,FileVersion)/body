{
  if (!_fileVersionIds.contains(fileVersion.getFileVersionId()) && isSupported(fileVersion) && !hasThumbnails(fileVersion)) {
    _fileVersionIds.add(fileVersion.getFileVersionId());
    if (PropsValues.DL_FILE_ENTRY_PROCESSORS_TRIGGER_SYNCHRONOUSLY) {
      try {
        MessageBusUtil.sendSynchronousMessage(DestinationNames.DOCUMENT_LIBRARY_IMAGE_PROCESSOR,new Object[]{copyFromVersion,fileVersion});
      }
 catch (      MessageBusException mbe) {
        if (_log.isWarnEnabled()) {
          _log.warn(mbe,mbe);
        }
      }
    }
 else {
      MessageBusUtil.sendMessage(DestinationNames.DOCUMENT_LIBRARY_IMAGE_PROCESSOR,new Object[]{copyFromVersion,fileVersion});
    }
  }
}
