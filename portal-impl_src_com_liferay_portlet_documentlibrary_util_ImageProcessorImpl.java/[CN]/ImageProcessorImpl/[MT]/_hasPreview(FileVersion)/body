{
  if (PropsValues.DL_FILE_ENTRY_PREVIEW_ENABLED && _previewGenerationRequired(fileVersion)) {
    String type=getPreviewType(fileVersion);
    String previewFilePath=getPreviewFilePath(fileVersion,type);
    if (!DLStoreUtil.hasFile(fileVersion.getCompanyId(),REPOSITORY_ID,previewFilePath)) {
      return false;
    }
  }
  return true;
}
