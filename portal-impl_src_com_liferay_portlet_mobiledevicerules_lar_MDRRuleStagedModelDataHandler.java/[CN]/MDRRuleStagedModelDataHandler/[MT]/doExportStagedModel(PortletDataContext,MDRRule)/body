{
  MDRRuleGroup ruleGroup=MDRRuleGroupLocalServiceUtil.getRuleGroup(rule.getRuleGroupId());
  StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,ruleGroup);
  Element ruleElement=portletDataContext.getExportDataStagedModelElement(rule);
  portletDataContext.addClassedModel(ruleElement,StagedModelPathUtil.getPath(rule),rule,MDRPortletDataHandler.NAMESPACE);
}
