{
  MDRRuleGroup ruleGroup=MDRRuleGroupLocalServiceUtil.getRuleGroup(rule.getRuleGroupId());
  StagedModelDataHandlerUtil.exportReferencedStagedModel(portletDataContext,rule,ruleGroup,PortletDataContext.REFERENCE_TYPE_PARENT);
  Element ruleElement=portletDataContext.getExportDataElement(rule);
  portletDataContext.addClassedModel(ruleElement,ExportImportPathUtil.getModelPath(rule),rule,MDRPortletDataHandler.NAMESPACE);
}
