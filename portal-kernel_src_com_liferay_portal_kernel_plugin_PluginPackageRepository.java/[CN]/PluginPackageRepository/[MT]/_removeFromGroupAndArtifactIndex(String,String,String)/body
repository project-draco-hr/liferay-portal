{
  List plugins=findPluginsByGroupIdAndArtifactId(groupId,artifactId);
  if (plugins != null) {
    Iterator itr=plugins.iterator();
    while (itr.hasNext()) {
      PluginPackage pluginPackage=(PluginPackage)itr.next();
      if (pluginPackage.getModuleId().equals(moduleId)) {
        itr.remove();
        break;
      }
    }
  }
}
