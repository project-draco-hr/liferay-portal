{
  DateValue[] dates=rdates.getDatesUtc();
  Arrays.sort(dates);
  int k=0;
  for (int i=1; i < dates.length; ++i) {
    if (!dates[i].equals(dates[k])) {
      dates[++k]=dates[i];
    }
  }
  if (++k < dates.length) {
    DateValue[] uniqueDates=new DateValue[k];
    System.arraycopy(dates,0,uniqueDates,0,k);
    dates=uniqueDates;
  }
  return new RDateIteratorImpl(dates);
}
