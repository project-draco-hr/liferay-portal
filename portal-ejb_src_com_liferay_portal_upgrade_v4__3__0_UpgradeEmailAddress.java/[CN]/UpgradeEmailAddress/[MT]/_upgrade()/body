{
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(EmailAddressImpl.TABLE_NAME,EmailAddressImpl.TABLE_COLUMNS,new PKUpgradeColumnImpl(),new ClassNameUpgradeColumnImpl());
