{
  List emailAddresses=EmailAddressLocalServiceUtil.getEmailAddresses();
  EmailAddressLocalServiceUtil.deleteEmailAddresses();
  for (int i=0; i < emailAddresses.size(); i++) {
    EmailAddress emailAddress=(EmailAddress)emailAddresses.get(i);
    EmailAddressLocalServiceUtil.addEmailAddress(emailAddress.getUserId(),emailAddress.getClassName(),emailAddress.getClassPK(),emailAddress.getAddress(),emailAddress.getTypeId(),emailAddress.getPrimary());
  }
}
