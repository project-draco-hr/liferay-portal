{
  Group group1=(Group)obj1;
  Group group2=(Group)obj2;
  long groupId1=group1.getGroupId();
  long groupId2=group2.getGroupId();
  int value=0;
  if (groupId1 < groupId2) {
    value=-1;
  }
 else   if (groupId1 > groupId2) {
    value=1;
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
