{
  TrashEntry trashEntry=TrashEntryLocalServiceUtil.getEntry(WikiPage.class.getName(),_page.getResourcePrimKey());
  String trashTitle=TrashUtil.getTrashTitle(trashEntry.getEntryId());
  _page.setTitle(trashTitle);
  WikiPageLocalServiceUtil.restorePageAttachmentFromTrash(TestPropsValues.getUserId(),_page.getNodeId(),_page.getTitle(),_attachmentFileName);
  WikiPageLocalServiceUtil.restorePageFromTrash(TestPropsValues.getUserId(),_page);
}
