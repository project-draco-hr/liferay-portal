{
  JournalArticle article=null;
  if (approved == null) {
    try {
      article=journalArticleFinder.findByR_A(resourcePrimKey,true);
    }
 catch (    NoSuchArticleException nsae) {
      article=journalArticleFinder.findByR_A(resourcePrimKey,false);
    }
  }
 else {
    article=journalArticleFinder.findByR_A(resourcePrimKey,approved.booleanValue());
  }
  return article;
}
