{
  if (!article.isApproved()) {
    return;
  }
  double latestVersion=getLatestVersion(article.getGroupId(),article.getArticleId(),Boolean.TRUE);
  if (latestVersion != article.getVersion()) {
    return;
  }
  TagsAssetLocalServiceUtil.updateAsset(article.getUserId(),JournalArticle.class.getName(),article.getResourcePrimKey(),tagsEntries,null,null,article.getDisplayDate(),article.getExpirationDate(),ContentTypes.TEXT_HTML,article.getTitle(),article.getDescription(),article.getDescription(),null,0,0);
}
