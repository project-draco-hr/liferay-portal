{
  Date[] dateInterval=getDateInterval(article.getGroupId(),article.getArticleId(),article.getDisplayDate(),article.getExpirationDate());
  Date displayDate=dateInterval[0];
  Date expirationDate=dateInterval[1];
  tagsAssetLocalService.updateAsset(userId,article.getGroupId(),JournalArticle.class.getName(),article.getResourcePrimKey(),tagsEntries,categoriesEntries,null,null,displayDate,expirationDate,ContentTypes.TEXT_HTML,article.getTitle(),article.getDescription(),null,null,0,0,null,false);
}
