{
  if (Validator.isNotNull(urlTitle) && Validator.isNotNull(PropsValues.JOURNAL_ARTICLE_CONTEXT_URL_TITLE_REGEXP)) {
    Pattern pattern=Pattern.compile(PropsValues.JOURNAL_ARTICLE_CONTEXT_URL_TITLE_REGEXP);
    Matcher matcher=pattern.matcher(urlTitle);
    return matcher.matches();
  }
  return false;
}
