{
  StringBundler sb=new StringBundler(13);
  sb.append(articleURL);
  sb.append("&_");
  sb.append(PortletKeys.JOURNAL);
  sb.append("_groupId=");
  sb.append(groupId);
  sb.append("&_");
  sb.append(PortletKeys.JOURNAL);
  sb.append("_folderId=");
  sb.append(folderId);
  sb.append("&_");
  sb.append(PortletKeys.JOURNAL);
  sb.append("_articleId=");
  sb.append(articleId);
  return sb.toString();
}
