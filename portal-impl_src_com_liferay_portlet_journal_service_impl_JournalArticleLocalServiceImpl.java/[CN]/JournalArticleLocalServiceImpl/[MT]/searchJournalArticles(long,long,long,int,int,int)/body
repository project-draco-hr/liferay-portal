{
  for (int i=0; i < 10; i++) {
    Hits hits=search(groupId,userId,creatorUserId,status,start,end);
    List<JournalArticle> journalArticles=JournalUtil.getArticles(hits);
    if (journalArticles != null) {
      return new BaseModelSearchResult<JournalArticle>(journalArticles,hits.getLength());
    }
  }
  throw new SearchException("Unable to fix the search index after 10 attempts");
}
