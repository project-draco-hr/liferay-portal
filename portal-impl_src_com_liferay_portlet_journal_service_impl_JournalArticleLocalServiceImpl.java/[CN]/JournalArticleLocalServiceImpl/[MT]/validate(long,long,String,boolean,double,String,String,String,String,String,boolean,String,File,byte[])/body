{
  if (!autoArticleId) {
    validate(articleId);
    JournalArticle article=journalArticlePersistence.fetchByG_A_V(groupId,articleId,version);
    if (article != null) {
      throw new DuplicateArticleIdException();
    }
  }
  validate(companyId,groupId,title,content,type,structureId,templateId,smallImage,smallImageURL,smallFile,smallBytes);
}
