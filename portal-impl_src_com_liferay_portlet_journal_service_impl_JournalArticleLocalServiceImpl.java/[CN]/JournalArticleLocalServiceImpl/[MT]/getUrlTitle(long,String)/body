{
  String urlTitle=String.valueOf(id);
  title=title.trim().toLowerCase();
  if (Validator.isNull(title) || Validator.isNumber(title) || title.equals("rss")) {
    return urlTitle;
  }
  title=FriendlyURLNormalizer.normalize(title,_URL_TITLE_REPLACE_CHARS);
  char[] urlTitleCharArray=title.toCharArray();
  for (int i=0; i < urlTitleCharArray.length; i++) {
    char oldChar=urlTitleCharArray[i];
    char newChar=oldChar;
    if ((oldChar == CharPool.DASH) || (Validator.isChar(oldChar)) || (Validator.isDigit(oldChar))) {
    }
 else {
      return urlTitle;
    }
    if (oldChar != newChar) {
      urlTitleCharArray[i]=newChar;
    }
  }
  urlTitle=new String(urlTitleCharArray);
  return urlTitle;
}
