{
  if (!autoArticleId) {
    validate(articleId);
  }
  JournalArticle article=journalArticlePersistence.fetchByG_A_V(groupId,articleId,version);
  if (article != null) {
    throw new DuplicateArticleIdException();
  }
  validate(companyId,groupId,classNameId,titleMap,content,type,structureId,templateId,expirationDate,smallImage,smallImageURL,smallImageFile,smallImageBytes);
}
