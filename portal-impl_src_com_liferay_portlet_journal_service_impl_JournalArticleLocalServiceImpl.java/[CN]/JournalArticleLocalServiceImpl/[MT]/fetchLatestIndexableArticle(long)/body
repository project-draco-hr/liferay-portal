{
  OrderByComparator orderByComparator=new ArticleVersionComparator();
  Date now=new Date();
  int[] statuses=new int[]{WorkflowConstants.STATUS_APPROVED,WorkflowConstants.STATUS_IN_TRASH};
  List<JournalArticle> articles=journalArticlePersistence.findByR_NotC_LtD_I_ST(resourcePrimKey,PortalUtil.getClassNameId(DDMStructure.class),now,true,statuses,0,1,orderByComparator);
  if (articles.isEmpty()) {
    return null;
  }
  return articles.get(0);
}
