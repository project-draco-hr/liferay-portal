{
  if (status == WorkflowConstants.STATUS_ANY) {
    return journalArticlePersistence.findByG_A_NotST_First(groupId,articleId,WorkflowConstants.STATUS_IN_TRASH,orderByComparator);
  }
 else {
    return journalArticlePersistence.findByG_A_ST_First(groupId,articleId,status,orderByComparator);
  }
}
