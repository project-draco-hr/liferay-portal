{
  List<TagsEntry> entries=tagsEntryLocalService.getEntries(JournalArticle.class.getName(),article.getPrimaryKey(),false);
  String[] tagsCategories=new String[entries.size()];
  for (int i=0; i < entries.size(); i++) {
    tagsCategories[i]=entries.get(i).getName();
  }
  return tagsCategories;
}
