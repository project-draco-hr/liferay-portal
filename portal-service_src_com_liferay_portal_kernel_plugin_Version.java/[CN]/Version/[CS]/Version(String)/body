{
  StringTokenizer st=new StringTokenizer(version,_SEPARATOR);
  _major=st.nextToken();
  if (st.hasMoreTokens()) {
    _minor=st.nextToken();
  }
  if (st.hasMoreTokens()) {
    _bugFix=st.nextToken();
  }
  if (st.hasMoreTokens()) {
    _buildNumber=st.nextToken();
  }
 else {
    _buildNumber=null;
  }
}
