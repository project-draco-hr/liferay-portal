{
  if (!_clusterMasterExecutor.isMaster()) {
    return;
  }
  for (  ConfigurationModel configurationModel : configurationModelIterator.getResults()) {
    try {
      _configurationModelIndexer.delete(configurationModel);
    }
 catch (    SearchException e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to reindex models",e);
      }
    }
  }
  commit(_configurationModelIndexer);
}
