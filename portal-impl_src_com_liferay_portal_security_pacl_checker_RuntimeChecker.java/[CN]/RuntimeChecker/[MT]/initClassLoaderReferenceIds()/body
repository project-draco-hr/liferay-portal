{
  _classLoaderReferenceIds=getPropertySet("security-manager-class-loader-reference-ids");
  if (_log.isDebugEnabled()) {
    Set<String> referenceIds=new TreeSet<String>(_classLoaderReferenceIds);
    for (    String referenceId : referenceIds) {
      _log.debug("Allowing access to class loader for reference " + referenceId);
    }
  }
}
