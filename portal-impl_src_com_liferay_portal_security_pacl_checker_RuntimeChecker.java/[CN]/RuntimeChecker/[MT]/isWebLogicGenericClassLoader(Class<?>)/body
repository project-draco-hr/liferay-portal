{
  if (!ServerDetector.isWebLogic()) {
    return false;
  }
  if (clazz == null) {
    return false;
  }
  String className=clazz.getName();
  if (!className.equals(_CLASS_NAME_GENERIC_CLASS_LOADER)) {
    return false;
  }
  String classLocation=PACLClassUtil.getClassLocation(clazz);
  if (classLocation.contains("/modules/com.bea.core.utils.classloaders_") || classLocation.contains("/patch_jars/BUG")) {
    return true;
  }
  return false;
}
