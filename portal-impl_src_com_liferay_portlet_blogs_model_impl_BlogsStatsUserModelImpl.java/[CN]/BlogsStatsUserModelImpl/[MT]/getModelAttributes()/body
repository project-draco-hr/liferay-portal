{
  Map<String,Object> attributes=new HashMap<String,Object>();
  attributes.put("statsUserId",getStatsUserId());
  attributes.put("groupId",getGroupId());
  attributes.put("companyId",getCompanyId());
  attributes.put("userId",getUserId());
