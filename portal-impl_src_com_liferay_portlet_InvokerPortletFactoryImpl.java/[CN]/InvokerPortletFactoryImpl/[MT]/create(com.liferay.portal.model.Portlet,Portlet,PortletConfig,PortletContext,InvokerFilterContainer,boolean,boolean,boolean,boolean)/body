{
  try {
    return new InvokerPortletImpl(portletModel,portlet,portletConfig,portletContext,invokerFilterContainer,checkAuthToken,facesPortlet,strutsPortlet,strutsBridgePortlet);
  }
 catch (  PortletException pe) {
    throw pe;
  }
catch (  Exception e) {
    throw new PortletException(e);
  }
}
