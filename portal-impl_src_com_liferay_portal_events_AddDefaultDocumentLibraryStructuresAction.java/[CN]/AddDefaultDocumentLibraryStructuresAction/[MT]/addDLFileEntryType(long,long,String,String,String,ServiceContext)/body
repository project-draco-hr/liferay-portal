{
  String ddmStructureKey=ddmStructureName;
  DDMStructure ddmStructure=DDMStructureLocalServiceUtil.fetchStructure(groupId,ddmStructureKey);
  if (ddmStructure == null) {
    return;
  }
  long[] ddmStructureId=new long[]{ddmStructure.getStructureId()};
  List<DLFileEntryType> dlFileEntryTypes=DLFileEntryTypeLocalServiceUtil.getFileEntryTypes(groupId,dlFileEntryTypeName,dlFileEntryTypeDescription);
  if (dlFileEntryTypes.isEmpty()) {
    DLFileEntryTypeLocalServiceUtil.addFileEntryType(userId,groupId,dlFileEntryTypeName,dlFileEntryTypeDescription,ddmStructureId,serviceContext);
  }
}
