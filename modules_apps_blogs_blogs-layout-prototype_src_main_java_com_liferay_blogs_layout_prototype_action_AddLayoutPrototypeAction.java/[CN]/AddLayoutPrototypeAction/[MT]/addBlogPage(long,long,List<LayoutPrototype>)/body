{
  Layout layout=DefaultLayoutPrototypesUtil.addLayoutPrototype(companyId,defaultUserId,"layout-prototype-blog-title","layout-prototype-blog-description","2_columns_iii",layoutPrototypes,AddLayoutPrototypeAction.class.getClassLoader());
  if (layout == null) {
    return;
  }
  DefaultLayoutPrototypesUtil.addPortletId(layout,BlogsPortletKeys.BLOGS,"column-1");
  DefaultLayoutPrototypesUtil.addPortletId(layout,AssetTagsNavigationPortletKeys.ASSET_TAGS_CLOUD,"column-2");
  DefaultLayoutPrototypesUtil.addPortletId(layout,RecentBloggersPortletKeys.RECENT_BLOGGERS,"column-2");
}
