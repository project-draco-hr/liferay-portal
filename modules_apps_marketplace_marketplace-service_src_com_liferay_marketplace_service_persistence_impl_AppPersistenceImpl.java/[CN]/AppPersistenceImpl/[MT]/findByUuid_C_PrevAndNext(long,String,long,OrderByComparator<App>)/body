{
  App app=findByPrimaryKey(appId);
  Session session=null;
  try {
    session=openSession();
    App[] array=new AppImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,app,uuid,companyId,orderByComparator,true);
    array[1]=app;
    array[2]=getByUuid_C_PrevAndNext(session,app,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
