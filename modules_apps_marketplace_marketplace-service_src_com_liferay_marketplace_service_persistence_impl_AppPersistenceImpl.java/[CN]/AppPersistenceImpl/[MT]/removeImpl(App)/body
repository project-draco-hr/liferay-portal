{
  app=toUnwrappedModel(app);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(app)) {
      app=(App)session.get(AppImpl.class,app.getPrimaryKeyObj());
    }
    if (app != null) {
      session.delete(app);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (app != null) {
    clearCache(app);
  }
  return app;
}
