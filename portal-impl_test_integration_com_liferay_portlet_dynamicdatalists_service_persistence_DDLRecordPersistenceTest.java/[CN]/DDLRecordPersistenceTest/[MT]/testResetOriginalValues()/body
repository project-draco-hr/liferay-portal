{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  DDLRecord newDDLRecord=addDDLRecord();
  _persistence.clearCache();
  DDLRecord existingDDLRecord=_persistence.findByPrimaryKey(newDDLRecord.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingDDLRecord.getUuid(),ReflectionTestUtil.invoke(existingDDLRecord,"getOriginalUuid",new Class<?>[0])));
  Assert.assertEquals(existingDDLRecord.getGroupId(),ReflectionTestUtil.invoke(existingDDLRecord,"getOriginalGroupId",new Class<?>[0]));
}
