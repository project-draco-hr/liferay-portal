{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  DDLRecord newDDLRecord=addDDLRecord();
  _persistence.clearCache();
  DDLRecordModelImpl existingDDLRecordModelImpl=(DDLRecordModelImpl)_persistence.findByPrimaryKey(newDDLRecord.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingDDLRecordModelImpl.getUuid(),existingDDLRecordModelImpl.getOriginalUuid()));
  Assert.assertEquals(existingDDLRecordModelImpl.getGroupId(),existingDDLRecordModelImpl.getOriginalGroupId());
}
