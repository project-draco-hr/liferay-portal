{
  try {
    if (isEnabled()) {
      _log.info("Converting");
      doConvert();
      _log.info("Conversion complete");
    }
 else {
      _log.warn("Conversion is not enabled for " + getClass().getName());
    }
  }
 catch (  Exception e) {
    throw new ConvertException(e);
  }
 finally {
    MaintenanceUtil.cancel();
  }
}
