{
  Session session=null;
  try {
    session=openSession();
    KaleoTimer kaleoTimer=(KaleoTimer)session.get(KaleoTimerImpl.class,primaryKey);
    if (kaleoTimer == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchTimerException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(kaleoTimer);
  }
 catch (  NoSuchTimerException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
