{
  PACLPolicy paclPolicy=null;
  State state=State.parse(properties.getProperty("security-manager-enabled",State.DISABLED.getValue()));
  if (state == State.ENABLED) {
    paclPolicy=new ActivePACLPolicy(servletContextName,classLoader,properties);
  }
 else   if (state == State.GENERATING_AUTHORIZATION_PROPERTY) {
    paclPolicy=new GeneratingPACLPolicy(servletContextName,classLoader,properties);
  }
 else {
    paclPolicy=new InactivePACLPolicy(servletContextName,classLoader,properties);
  }
  return paclPolicy;
}
