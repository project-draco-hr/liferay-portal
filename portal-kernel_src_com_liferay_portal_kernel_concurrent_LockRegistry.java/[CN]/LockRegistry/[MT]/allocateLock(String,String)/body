{
  ConcurrentHashMap<String,ReentrantLock> lockGroup=_lockGroupMap.get(groupName);
  if (lockGroup == null) {
    lockGroup=new ConcurrentHashMap<>();
    ConcurrentHashMap<String,ReentrantLock> oldLockGroup=_lockGroupMap.putIfAbsent(groupName,lockGroup);
    if (oldLockGroup != null) {
      lockGroup=oldLockGroup;
    }
  }
  ReentrantLock lock=lockGroup.get(key);
  if (lock == null) {
    lock=new ReentrantLock();
    ReentrantLock oldLock=lockGroup.putIfAbsent(key,lock);
    if (oldLock != null) {
      lock=oldLock;
    }
  }
  return lock;
}
