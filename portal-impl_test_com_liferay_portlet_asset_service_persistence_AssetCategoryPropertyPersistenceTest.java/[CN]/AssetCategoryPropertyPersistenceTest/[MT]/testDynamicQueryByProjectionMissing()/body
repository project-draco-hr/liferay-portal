{
  AssetCategoryProperty newAssetCategoryProperty=addAssetCategoryProperty();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AssetCategoryProperty.class,AssetCategoryProperty.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("categoryPropertyId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("categoryPropertyId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
