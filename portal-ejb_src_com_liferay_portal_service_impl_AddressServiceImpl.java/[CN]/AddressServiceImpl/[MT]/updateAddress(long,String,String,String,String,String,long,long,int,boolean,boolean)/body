{
  Address address=AddressUtil.findByPrimaryKey(addressId);
  CommonPermission.checkPermission(getPermissionChecker(),address.getClassNameId(),address.getClassPK(),ActionKeys.UPDATE);
  return AddressLocalServiceUtil.updateAddress(addressId,street1,street2,street3,city,zip,regionId,countryId,typeId,mailing,primary);
}
