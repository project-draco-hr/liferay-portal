{
  Set<String> portletCSRFWhitelistActions=AuthTokenWhitelistUtil.getPortletCSRFWhitelistActions();
  Assert.assertTrue("The URL " + TestAuthTokenIgnoreActions.TEST_AUTH_TOKEN_IGNORE_ACTION_URL + " can't be found in "+ portletCSRFWhitelistActions,portletCSRFWhitelistActions.contains(TestAuthTokenIgnoreActions.TEST_AUTH_TOKEN_IGNORE_ACTION_URL));
}
