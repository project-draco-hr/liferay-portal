{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(listType);
    }
 else {
      if (listType.isNew()) {
        session.save(listType);
      }
    }
    session.flush();
    listType.setNew(false);
    return listType;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
