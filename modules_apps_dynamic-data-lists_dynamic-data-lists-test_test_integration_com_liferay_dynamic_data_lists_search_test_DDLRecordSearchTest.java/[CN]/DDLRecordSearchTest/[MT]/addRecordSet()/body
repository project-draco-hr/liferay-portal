{
  DDLRecordSetTestHelper recordSetTestHelper=new DDLRecordSetTestHelper(_group);
  DDMStructureTestHelper ddmStructureTestHelper=new DDMStructureTestHelper(PortalUtil.getClassNameId(DDLRecordSet.class),_group);
  DDMStructure ddmStructure=ddmStructureTestHelper.addStructure(createDDMForm(),StorageType.JSON.toString());
  return recordSetTestHelper.addRecordSet(ddmStructure);
}
