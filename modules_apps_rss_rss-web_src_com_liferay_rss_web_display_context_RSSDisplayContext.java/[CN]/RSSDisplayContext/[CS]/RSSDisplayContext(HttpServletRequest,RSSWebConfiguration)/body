{
  _request=request;
  _rssWebConfiguration=rssWebConfiguration;
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(com.liferay.portal.util.WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  _rssPortletInstanceConfiguration=portletDisplay.getPortletInstanceConfiguration(RSSPortletInstanceConfiguration.class);
}
