{
  if (actionId.equals(ActionKeys.ADD_LAYOUT_SET_BRANCH)) {
    return GroupPermissionUtil.contains(permissionChecker,groupId,ActionKeys.ADD_LAYOUT_SET_BRANCH);
  }
 else {
    LayoutSetBranch layoutSetBranch=LayoutSetBranchLocalServiceUtil.getLayoutSetBranch(layoutSetBranchId);
    return contains(permissionChecker,layoutSetBranch,actionId);
  }
}
