{
  PortletPermissionUtil.check(getPermissionChecker(),serviceContext.getPlid(),PortletKeys.CALENDAR,ActionKeys.ADD_EVENT);
  return calEventLocalService.addEvent(getUserId(),title,description,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,durationHour,durationMinute,allDay,timeZoneSensitive,type,repeating,recurrence,remindBy,firstReminder,secondReminder,serviceContext);
}
