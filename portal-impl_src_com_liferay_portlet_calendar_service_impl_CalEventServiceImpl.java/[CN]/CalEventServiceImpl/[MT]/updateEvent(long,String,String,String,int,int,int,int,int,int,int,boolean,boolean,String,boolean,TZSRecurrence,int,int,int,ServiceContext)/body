{
  CalEventPermission.check(getPermissionChecker(),eventId,ActionKeys.UPDATE);
  return calEventLocalService.updateEvent(getUserId(),eventId,title,description,location,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,durationHour,durationMinute,allDay,timeZoneSensitive,type,repeating,recurrence,remindBy,firstReminder,secondReminder,serviceContext);
}
