{
  List<CalEvent> events=calEventLocalService.getEvents(groupId,cal,types);
  events=ListUtil.copy(events);
  Iterator<CalEvent> itr=events.iterator();
  while (itr.hasNext()) {
    CalEvent event=itr.next();
    if (!CalEventPermission.contains(getPermissionChecker(),event,ActionKeys.VIEW)) {
      itr.remove();
    }
  }
  return events;
}
