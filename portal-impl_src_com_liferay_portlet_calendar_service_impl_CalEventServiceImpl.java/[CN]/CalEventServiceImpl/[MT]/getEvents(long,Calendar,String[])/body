{
  List<CalEvent> events=new ArrayList<CalEvent>();
  List<CalEvent> eventList=calEventLocalService.getEvents(groupId,cal,types);
  for (  CalEvent event : eventList) {
    long eventId=event.getEventId();
    if (CalEventPermission.contains(getPermissionChecker(),eventId,ActionKeys.VIEW)) {
      events.add(event);
    }
  }
  return events;
}
