{
  PortletPermission.check(getPermissionChecker(),plid,PortletKeys.CALENDAR,ActionKeys.ADD_EVENT);
  return CalEventLocalServiceUtil.addEvent(getUserId(),plid,title,description,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,durationHour,durationMinute,allDay,timeZoneSensitive,type,repeating,recurrence,remindBy,firstReminder,secondReminder,communityPermissions,guestPermissions);
}
