{
  CalEventPermission.check(getPermissionChecker(),eventId,ActionKeys.UPDATE);
  return CalEventLocalServiceUtil.updateEvent(getUserId(),eventId,title,description,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,durationHour,durationMinute,allDay,timeZoneSensitive,type,repeating,recurrence,remindBy,firstReminder,secondReminder);
}
