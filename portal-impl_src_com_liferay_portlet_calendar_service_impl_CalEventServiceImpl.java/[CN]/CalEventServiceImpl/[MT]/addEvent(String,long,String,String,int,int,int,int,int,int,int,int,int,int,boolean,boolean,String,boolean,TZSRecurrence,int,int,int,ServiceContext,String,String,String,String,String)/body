{
  CalendarPermission.check(getPermissionChecker(),serviceContext.getScopeGroupId(),ActionKeys.ADD_EVENT);
  return calEventLocalService.addEvent(uuid,getUserId(),title,description,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,durationHour,durationMinute,allDay,timeZoneSensitive,type,repeating,recurrence,remindBy,firstReminder,secondReminder,serviceContext,extCalServerUrl,extCalId,extCalUserId,extCalPassword,extCalSessionId);
}
