{
  CalendarPermission.check(getPermissionChecker(),groupId,ActionKeys.EXPORT_ALL_EVENTS);
  long userId=0;
  try {
    userId=getUserId();
  }
 catch (  Exception e) {
    userId=UserLocalServiceUtil.getDefaultUserId(CompanyThreadLocal.getCompanyId());
  }
  return calEventLocalService.exportGroupEvents(userId,groupId,fileName);
}
