{
  PortletPermissionUtil.check(getPermissionChecker(),plid,PortletKeys.CALENDAR,ActionKeys.ADD_EVENT);
  return calEventLocalService.addEvent(getUserId(),plid,title,description,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,durationHour,durationMinute,allDay,timeZoneSensitive,type,repeating,recurrence,remindBy,firstReminder,secondReminder,communityPermissions,guestPermissions);
}
