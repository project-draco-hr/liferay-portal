{
  CalendarPermission.check(getPermissionChecker(),serviceContext.getScopeGroupId(),ActionKeys.ADD_EVENT);
  return calEventLocalService.addEvent(getUserId(),title,description,location,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,durationHour,durationMinute,allDay,timeZoneSensitive,type,repeating,recurrence,remindBy,firstReminder,secondReminder,serviceContext);
}
