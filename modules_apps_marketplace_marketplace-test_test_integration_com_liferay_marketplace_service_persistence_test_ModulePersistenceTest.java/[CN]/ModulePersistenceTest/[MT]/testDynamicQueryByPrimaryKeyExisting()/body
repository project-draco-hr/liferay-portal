{
  Module newModule=addModule();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Module.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("moduleId",newModule.getModuleId()));
  List<Module> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Module existingModule=result.get(0);
  Assert.assertEquals(existingModule,newModule);
}
