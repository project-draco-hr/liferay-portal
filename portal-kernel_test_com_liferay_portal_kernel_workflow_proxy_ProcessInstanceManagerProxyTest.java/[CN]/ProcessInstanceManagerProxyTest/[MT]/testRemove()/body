{
  final long testProcessInstanceId=101;
  MessageListener removeListener=new MessageListener(){
    public void receive(    Message message){
      ProcessInstanceRequest request=(ProcessInstanceRequest)message.getPayload();
      assertEquals(ProcessInstanceRequestType.REMOVE,request.getType());
      assertEquals(testProcessInstanceId,request.getProcessInstanceId());
    }
  }
;
  messageBus.registerMessageListener(DestinationNames.WORKFLOW_INSTANCE,removeListener);
  _proxy.removeProcessInstance(testProcessInstanceId);
}
