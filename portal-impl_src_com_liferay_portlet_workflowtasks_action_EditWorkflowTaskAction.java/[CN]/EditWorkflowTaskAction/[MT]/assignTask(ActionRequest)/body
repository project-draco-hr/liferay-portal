{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long workflowTaskId=ParamUtil.getLong(actionRequest,"workflowTaskId");
  long assigneeUserId=ParamUtil.getLong(actionRequest,"assigneeUserId");
  String comment=ParamUtil.getString(actionRequest,"comment");
  WorkflowTaskManagerUtil.assignWorkflowTaskToUser(themeDisplay.getCompanyId(),themeDisplay.getUserId(),workflowTaskId,assigneeUserId,comment,null);
}
