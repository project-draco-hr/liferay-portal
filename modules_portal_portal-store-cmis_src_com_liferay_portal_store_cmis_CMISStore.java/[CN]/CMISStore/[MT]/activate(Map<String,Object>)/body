{
  _cmisConfiguration=Configurable.createConfigurable(CMISConfiguration.class,properties);
  _operationContext=createOperationContext();
  _sessionFactory=SessionFactoryImpl.newInstance();
  _session=createSession(_cmisConfiguration,_operationContext,_sessionFactory);
  _systemRootDir=getFolder(_session.getRootFolder(),_cmisConfiguration.systemRootDir());
  if (_systemRootDir == null) {
    _systemRootDir=createFolder(_session.getRootFolder(),_cmisConfiguration.systemRootDir());
  }
}
