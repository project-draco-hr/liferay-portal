{
  _cmisConfiguration=Configurable.createConfigurable(CMISConfiguration.class,properties);
  Folder systemRootDir=getFolder(SessionHolder.session.getRootFolder(),_cmisConfiguration.systemRootDir());
  if (systemRootDir == null) {
    systemRootDir=createFolder(SessionHolder.session.getRootFolder(),_cmisConfiguration.systemRootDir());
  }
  _systemRootDir=systemRootDir;
}
