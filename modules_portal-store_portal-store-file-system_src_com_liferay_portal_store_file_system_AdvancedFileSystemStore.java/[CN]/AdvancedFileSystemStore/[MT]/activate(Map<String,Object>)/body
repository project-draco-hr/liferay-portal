{
  _advancedFileSystemStoreConfiguration=Configurable.createConfigurable(AdvancedFileSystemStoreConfiguration.class,properties);
  if (Validator.isBlank(_advancedFileSystemStoreConfiguration.rootDir())) {
    throw new IllegalArgumentException("Advanced file system root directory is not set",new FileSystemStoreRootDirException());
  }
  FileSystemStoreConfigurationValidator fileSystemStoreConfigurationValidator=new FileSystemStoreConfigurationValidator();
  fileSystemStoreConfigurationValidator.validate("com.liferay.portal.store.file.system.configuration." + "FileSystemStoreConfiguration","com.liferay.portal.store.file.system.configuration." + "AdvancedFileSystemStoreConfiguration");
  initializeRootDir();
}
