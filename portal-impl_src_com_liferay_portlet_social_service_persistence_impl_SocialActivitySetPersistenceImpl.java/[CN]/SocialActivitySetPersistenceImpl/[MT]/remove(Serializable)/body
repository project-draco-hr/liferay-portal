{
  Session session=null;
  try {
    session=openSession();
    SocialActivitySet socialActivitySet=(SocialActivitySet)session.get(SocialActivitySetImpl.class,primaryKey);
    if (socialActivitySet == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchActivitySetException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(socialActivitySet);
  }
 catch (  NoSuchActivitySetException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
