{
  int[] propertiesIndexes=StringUtil.split(ParamUtil.getString(actionRequest,"propertiesIndexes"),0);
  String[] properties=new String[propertiesIndexes.length];
  int i=0;
  for (  int propertiesIndex : propertiesIndexes) {
    String key=ParamUtil.getString(actionRequest,"key" + propertiesIndex);
    if (Validator.isNull(key)) {
      continue;
    }
    String value=ParamUtil.getString(actionRequest,"value" + propertiesIndex);
    properties[i]=key + StringPool.COLON + value;
    i++;
  }
  return properties;
}
