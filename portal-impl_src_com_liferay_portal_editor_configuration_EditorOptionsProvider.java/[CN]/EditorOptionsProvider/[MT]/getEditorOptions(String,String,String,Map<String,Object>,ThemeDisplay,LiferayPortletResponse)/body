{
  EditorOptions editorOptions=new EditorOptions();
  Iterator<EditorOptionsContributor> iterator=ListUtil.reverseIterator(getContributors(portletName,editorConfigKey,editorName));
  while (iterator.hasNext()) {
    EditorOptionsContributor editorOptionsContributor=iterator.next();
    editorOptionsContributor.populateEditorOptions(editorOptions,inputEditorTaglibAttributes,themeDisplay,liferayPortletResponse);
  }
  return editorOptions;
}
