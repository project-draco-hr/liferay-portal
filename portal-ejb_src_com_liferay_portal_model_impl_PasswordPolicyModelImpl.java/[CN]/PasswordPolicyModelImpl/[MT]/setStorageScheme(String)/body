{
  if (((storageScheme == null) && (_storageScheme != null)) || ((storageScheme != null) && (_storageScheme == null)) || ((storageScheme != null) && (_storageScheme != null) && !storageScheme.equals(_storageScheme))) {
    if (!XSS_ALLOW_STORAGESCHEME) {
      storageScheme=XSSUtil.strip(storageScheme);
