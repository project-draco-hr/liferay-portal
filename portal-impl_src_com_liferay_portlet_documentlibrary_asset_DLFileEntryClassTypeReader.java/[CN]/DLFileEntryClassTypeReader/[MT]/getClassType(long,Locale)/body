{
  if (classTypeId == DLFileEntryTypeConstants.FILE_ENTRY_TYPE_ID_BASIC_DOCUMENT) {
    return getBasicDocumentClassType(locale);
  }
  DLFileEntryType dlFileEntryType=DLFileEntryTypeServiceUtil.getFileEntryType(classTypeId);
  return new DLFileEntryClassType(dlFileEntryType.getFileEntryTypeId(),dlFileEntryType.getName(locale),LocaleUtil.toLanguageId(locale));
}
