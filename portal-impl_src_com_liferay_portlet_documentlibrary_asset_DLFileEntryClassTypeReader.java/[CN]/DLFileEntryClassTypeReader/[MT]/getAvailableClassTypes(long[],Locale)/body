{
  List<ClassType> classTypes=new ArrayList<ClassType>();
  classTypes.add(getBasicDocumentClassType(locale));
  String languageId=LocaleUtil.toLanguageId(locale);
  List<DLFileEntryType> dlFileEntryTypes=DLFileEntryTypeServiceUtil.getFileEntryTypes(groupIds);
  for (  DLFileEntryType dlFileEntryType : dlFileEntryTypes) {
    classTypes.add(new DLFileEntryClassType(dlFileEntryType.getFileEntryTypeId(),dlFileEntryType.getName(locale),languageId));
  }
  return classTypes;
}
