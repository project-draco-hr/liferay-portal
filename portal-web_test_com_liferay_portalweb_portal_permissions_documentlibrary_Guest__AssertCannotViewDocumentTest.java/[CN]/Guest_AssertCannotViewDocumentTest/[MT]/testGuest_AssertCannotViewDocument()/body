{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Document Library Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Document Library Permissions Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("link=My Documents"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("link=Member Permissions Upload Edited.txt"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Forbidden"));
  assertTrue(selenium.isTextPresent("You do not have permission to access the requested resource"));
}
