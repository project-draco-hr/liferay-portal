{
  if (isNew) {
    Object[] args=new Object[]{assetTagStats.getTagId(),assetTagStats.getClassNameId()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_T_C,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_T_C,args,assetTagStats);
  }
 else {
    AssetTagStatsModelImpl assetTagStatsModelImpl=(AssetTagStatsModelImpl)assetTagStats;
    if ((assetTagStatsModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_T_C.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{assetTagStats.getTagId(),assetTagStats.getClassNameId()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_T_C,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_T_C,args,assetTagStats);
    }
  }
}
