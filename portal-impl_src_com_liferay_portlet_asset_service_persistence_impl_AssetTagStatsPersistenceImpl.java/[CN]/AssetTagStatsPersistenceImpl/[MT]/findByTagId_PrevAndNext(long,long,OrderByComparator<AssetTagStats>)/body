{
  AssetTagStats assetTagStats=findByPrimaryKey(tagStatsId);
  Session session=null;
  try {
    session=openSession();
    AssetTagStats[] array=new AssetTagStatsImpl[3];
    array[0]=getByTagId_PrevAndNext(session,assetTagStats,tagId,orderByComparator,true);
    array[1]=assetTagStats;
    array[2]=getByTagId_PrevAndNext(session,assetTagStats,tagId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
