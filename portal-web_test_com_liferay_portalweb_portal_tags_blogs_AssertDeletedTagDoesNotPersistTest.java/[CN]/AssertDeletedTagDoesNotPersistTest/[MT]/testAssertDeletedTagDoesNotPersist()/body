{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Blogs Tags Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Blogs Tags Test Page",RuntimeVariables.replace("Blogs Tags Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Tags Blog Entry1 Title"),selenium.getText("xPath=(//div[@class='entry-title']/h2/a)[3]"));
  selenium.clickAt("xPath=(//div[@class='entry-title']/h2/a)[3]",RuntimeVariables.replace("Tags Blog Entry1 Title"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isTextPresent("selenium2 liferay2"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Blogs Tags Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Blogs Tags Test Page",RuntimeVariables.replace("Blogs Tags Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Tags Blog Entry2 Title"),selenium.getText("xPath=(//div[@class='entry-title']/h2/a)[2]"));
  selenium.clickAt("xPath=(//div[@class='entry-title']/h2/a)[2]",RuntimeVariables.replace("Tags Blog Entry2 Title"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isTextPresent("selenium2 liferay2"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Blogs Tags Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Blogs Tags Test Page",RuntimeVariables.replace("Blogs Tags Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Tags Blog Entry3 Title"),selenium.getText("xPath=(//div[@class='entry-title']/h2/a)[1]"));
  selenium.clickAt("xPath=(//div[@class='entry-title']/h2/a)[1]",RuntimeVariables.replace("Tags Blog Entry3 Title"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isTextPresent("selenium2 liferay2"));
}
