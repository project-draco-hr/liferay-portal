{
  MeetupsEntry meetupsEntry=meetupsEntryPersistence.findByPrimaryKey(meetupsEntryId);
  meetupsEntryPersistence.remove(meetupsEntry);
  meetupsRegistrationPersistence.removeByMeetupsEntryId(meetupsEntryId);
  getImageLocalService().deleteImage(meetupsEntry.getThumbnailId());
  return meetupsEntry;
}
