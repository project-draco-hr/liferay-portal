{
  if (!(resourceResponse instanceof MimeResponse)) {
    return false;
  }
  if (!PortalUtil.isRSSFeedsEnabled()) {
    try {
      PortalUtil.sendRSSFeedsDisabledError(resourceRequest,resourceResponse);
    }
 catch (    Exception e) {
    }
    return false;
  }
  MimeResponse mimeResponse=(MimeResponse)resourceResponse;
  try {
    byte[] xml=_journalRSSUtil.getRSS(resourceRequest,resourceResponse);
    PortletResponseUtil.sendFile(resourceRequest,mimeResponse,null,xml,ContentTypes.TEXT_XML_UTF8);
  }
 catch (  Exception e) {
    throw new PortletException(e);
  }
  return true;
}
