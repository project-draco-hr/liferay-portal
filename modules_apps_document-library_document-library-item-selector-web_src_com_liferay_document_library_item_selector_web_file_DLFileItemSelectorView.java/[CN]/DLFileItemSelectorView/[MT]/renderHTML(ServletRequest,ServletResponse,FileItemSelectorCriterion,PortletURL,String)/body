{
  request.setAttribute(DL_ITEM_SELECTOR_CRITERION,fileItemSelectorCriterion);
  request.setAttribute(ITEM_SELECTED_EVENT_NAME,itemSelectedEventName);
  request.setAttribute(PORTLET_URL,portletURL);
  RequestDispatcher requestDispatcher=request.getRequestDispatcher("/o/document-library-item-selector-web/documents.jsp");
  requestDispatcher.include(request,response);
}
