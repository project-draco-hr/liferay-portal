{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select count(*) from DLFileEntry where groupId = " + groupId + " and folderId ="+ folderId+ " and title = '"+ title+ "'");
    rs=ps.executeQuery();
    while (rs.next()) {
      long count=rs.getLong(1);
      if (count > 0) {
        return true;
      }
 else {
        return false;
      }
    }
    return false;
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
