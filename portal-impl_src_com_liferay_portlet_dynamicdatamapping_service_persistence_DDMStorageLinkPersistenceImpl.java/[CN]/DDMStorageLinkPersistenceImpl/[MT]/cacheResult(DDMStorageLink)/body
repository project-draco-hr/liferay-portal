{
  EntityCacheUtil.putResult(DDMStorageLinkModelImpl.ENTITY_CACHE_ENABLED,DDMStorageLinkImpl.class,ddmStorageLink.getPrimaryKey(),ddmStorageLink);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_CLASSPK,new Object[]{ddmStorageLink.getClassPK()},ddmStorageLink);
  ddmStorageLink.resetOriginalValues();
}
