{
  fastWriteRaw("<?xml version='");
  fastWriteRaw(version);
  fastWriteRaw('\'');
  if (encoding != null && encoding.length() > 0) {
    fastWriteRaw(" encoding='");
    fastWriteRaw(encoding);
    fastWriteRaw('\'');
  }
  if (standalone != null) {
    fastWriteRaw(" standalone='");
    fastWriteRaw(standalone);
    fastWriteRaw('\'');
  }
  fastWriteRaw('?','>');
}
