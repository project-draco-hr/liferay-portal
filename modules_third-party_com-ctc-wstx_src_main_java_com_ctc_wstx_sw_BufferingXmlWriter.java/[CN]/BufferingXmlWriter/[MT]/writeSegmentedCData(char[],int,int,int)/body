{
  int end=start + len;
  while (index >= 0) {
    fastWriteRaw("<![CDATA[");
    writeRaw(c,start,(index + 2) - start);
    fastWriteRaw("]]>");
    start=index + 2;
    index=verifyCDataContent(c,start,end);
  }
  fastWriteRaw("<![CDATA[");
  writeRaw(c,start,end - start);
  fastWriteRaw("]]>");
}
