{
  if (node.getLevel() != 1) {
    List<Node> children=node.getChildren();
    if (!children.isEmpty()) {
      TextNode childNode=(TextNode)children.get(0);
      String text=childNode.getText().toLowerCase().replaceAll("[^a-z0-9 ]","").replace(' ','-');
      printer.print("<a name=\"" + text + "\" />");
    }
  }
  super.visit(node);
}
