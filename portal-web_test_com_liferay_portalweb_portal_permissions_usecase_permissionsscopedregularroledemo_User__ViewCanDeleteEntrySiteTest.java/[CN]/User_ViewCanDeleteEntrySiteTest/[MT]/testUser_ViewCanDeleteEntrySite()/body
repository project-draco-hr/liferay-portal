{
  selenium.open("/web/community-name/blogs-test-site-page/");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Blogs Test Site Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Blogs Test Site Page",RuntimeVariables.replace("Blogs Test Site Page"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertEquals(RuntimeVariables.replace("Delete"),selenium.getText("//td[contains(.,'Delete')]/span/a/span"));
}
