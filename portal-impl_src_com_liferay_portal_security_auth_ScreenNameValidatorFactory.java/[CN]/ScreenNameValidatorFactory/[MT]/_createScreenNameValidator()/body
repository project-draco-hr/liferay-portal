{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.USERS_SCREEN_NAME_VALIDATOR);
  }
  ScreenNameValidator screenNameValidator=null;
  ClassLoader classLoader=PACLClassLoaderUtil.getPortalClassLoader();
  try {
    screenNameValidator=(ScreenNameValidator)InstanceFactory.newInstance(classLoader,PropsValues.USERS_SCREEN_NAME_VALIDATOR);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return screenNameValidator;
}
