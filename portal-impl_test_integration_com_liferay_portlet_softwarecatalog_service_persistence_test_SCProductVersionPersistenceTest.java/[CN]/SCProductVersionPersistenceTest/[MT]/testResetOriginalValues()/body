{
  SCProductVersion newSCProductVersion=addSCProductVersion();
  _persistence.clearCache();
  SCProductVersion existingSCProductVersion=_persistence.findByPrimaryKey(newSCProductVersion.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingSCProductVersion.getDirectDownloadURL(),ReflectionTestUtil.invoke(existingSCProductVersion,"getOriginalDirectDownloadURL",new Class<?>[0])));
}
