{
  StringMaker sm=new StringMaker();
  sm.append(GROUP_NAME);
  sm.append(StringPool.POUND);
  sm.append(className);
  sm.append(StringPool.POUND);
  sm.append(methodName);
  for (int i=0; i < args.length; i++) {
    sm.append(StringPool.POUND);
    sm.append(args[0].toString());
  }
  return sm.toString();
}
