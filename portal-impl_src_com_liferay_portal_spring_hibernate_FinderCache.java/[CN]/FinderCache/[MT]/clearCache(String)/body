{
  String groupKey=_encodeKey(className);
  if (!_groups.containsKey(groupKey)) {
    return;
  }
  Map groupKeys=(Map)_groups.get(groupKey);
  Iterator keys=groupKeys.values().iterator();
  while (keys.hasNext()) {
    String key=(String)keys.next();
    _cache.remove(key);
  }
  groupKeys.clear();
}
