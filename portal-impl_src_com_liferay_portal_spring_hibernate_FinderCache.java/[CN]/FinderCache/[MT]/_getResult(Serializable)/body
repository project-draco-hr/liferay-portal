{
  if (result instanceof ArrayList) {
    ArrayList cachedList=(ArrayList)result;
    ArrayList list=new ArrayList(cachedList.size());
    for (int i=0; i < cachedList.size(); i++) {
      Serializable curResult=_getResult((Serializable)cachedList.get(i));
      list.add(curResult);
    }
    return list;
  }
 else   if (result instanceof BaseModel) {
    String modelImpl=result.getClass().getName();
    try {
      BaseModel model=(BaseModel)result;
      return (BaseModel)model.clone();
    }
 catch (    Exception e) {
      _log.error("Unable to clone " + modelImpl + ": "+ e.getMessage());
    }
  }
  return result;
}
