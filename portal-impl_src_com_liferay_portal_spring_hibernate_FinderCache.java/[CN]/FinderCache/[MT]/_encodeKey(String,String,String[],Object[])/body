{
  StringMaker sm=new StringMaker();
  sm.append(CACHE_NAME);
  sm.append(StringPool.POUND);
  sm.append(className);
  sm.append(StringPool.POUND);
  sm.append(methodName);
  sm.append(_PARAMS_SEPARATOR);
  for (  String param : params) {
    sm.append(StringPool.POUND);
    sm.append(param);
  }
  sm.append(_ARGS_SEPARATOR);
  for (  Object arg : args) {
    sm.append(StringPool.POUND);
    sm.append(String.valueOf(arg));
  }
  return sm.toString();
}
