{
  Object result=null;
  String key=_encodeKey(sql,methodName,params,args);
  try {
    result=_cache.getFromCache(key);
    return _getResult(result);
  }
 catch (  NeedsRefreshException nre) {
    result=null;
  }
 finally {
    if (result == null) {
      _cache.cancelUpdate(key);
    }
  }
  return result;
}
