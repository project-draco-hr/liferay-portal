{
  ServletRequest req=pageContext.getRequest();
  req.setAttribute("liferay-ui:input-time-zone:name",_name);
  req.setAttribute("liferay-ui:input-time-zone:value",_value);
  req.setAttribute("liferay-ui:input-time-zone:nullable",String.valueOf(_nullable));
  req.setAttribute("liferay-ui:input-time-zone:daylight",String.valueOf(_daylight));
  req.setAttribute("liferay-ui:input-time-zone:displayStyle",String.valueOf(_displayStyle));
  req.setAttribute("liferay-ui:input-time-zone:disabled",String.valueOf(_disabled));
  return EVAL_BODY_BUFFERED;
}
