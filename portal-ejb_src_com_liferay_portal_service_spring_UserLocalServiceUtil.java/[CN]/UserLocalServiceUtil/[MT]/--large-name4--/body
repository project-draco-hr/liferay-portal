{
  try {
    UserLocalService userLocalService=UserLocalServiceFactory.getService();
    return userLocalService.updateUser(userId,password,emailAddress,languageId,timeZoneId,greeting,resolution,comments,firstName,middleName,lastName,nickName,prefixId,suffixId,male,birthdayMonth,birthdayDay,birthdayYear,smsSn,aimSn,icqSn,msnSn,ymSn,jobTitle,organizationId,locationId);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
