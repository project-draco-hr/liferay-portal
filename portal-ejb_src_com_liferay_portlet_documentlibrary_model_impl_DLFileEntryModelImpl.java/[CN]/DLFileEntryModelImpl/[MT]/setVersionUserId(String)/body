{
  if (((versionUserId == null) && (_versionUserId != null)) || ((versionUserId != null) && (_versionUserId == null)) || ((versionUserId != null) && (_versionUserId != null) && !versionUserId.equals(_versionUserId))) {
    if (!XSS_ALLOW_VERSIONUSERID) {
      versionUserId=XSSUtil.strip(versionUserId);
    }
    _versionUserId=versionUserId;
  }
}
