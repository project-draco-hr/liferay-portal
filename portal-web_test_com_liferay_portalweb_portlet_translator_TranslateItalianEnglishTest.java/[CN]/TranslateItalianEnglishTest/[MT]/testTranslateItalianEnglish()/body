{
  selenium.select("_26_id",RuntimeVariables.replace("label=Italian to English"));
  selenium.type("_26_text",RuntimeVariables.replace("Il mio nome \u00e8 traduttore di Liferay, fluente dentro oltre 6 milione forme della comunicazione."));
  selenium.click(RuntimeVariables.replace("//input[@value='Translate']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("My name is translator of Liferay, flowing within beyond 6 million forms the communication."));
}
