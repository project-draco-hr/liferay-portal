{
  selenium.select("_26_id","label=Italian to English");
  selenium.type("_26_text","Il mio nome \u00e8 traduttore di Liferay, fluente dentro oltre 6 milione forme della comunicazione.");
  selenium.click("//input[@value='Translate']");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isTextPresent("My name is translator of Liferay, flowing within beyond 6 million forms the communication.")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
