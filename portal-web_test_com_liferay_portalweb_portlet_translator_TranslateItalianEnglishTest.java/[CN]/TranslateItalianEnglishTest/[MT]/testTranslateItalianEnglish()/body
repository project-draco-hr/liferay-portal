{
  selenium.select("_26_id","label=Italian to English");
  selenium.type("_26_text","Il mio nome \u00e8 traduttore di Liferay, fluente dentro oltre 6 milione forme della comunicazione.");
  selenium.click("//input[@value='Translate']");
  selenium.waitForPageToLoad("30000");
  verifyTrue(selenium.isTextPresent("My name is translator of Liferay, flowing within beyond 6 million shapes the communication."));
}
