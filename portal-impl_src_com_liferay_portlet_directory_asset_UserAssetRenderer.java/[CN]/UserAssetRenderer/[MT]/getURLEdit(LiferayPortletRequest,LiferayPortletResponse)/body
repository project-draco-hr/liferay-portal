{
  HttpServletRequest request=liferayPortletRequest.getHttpServletRequest();
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  PortletURL portletURL=PortletURLFactoryUtil.create(request,PortletKeys.ENTERPRISE_ADMIN_USERS,getControlPanelPlid(themeDisplay),PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/enterprise_admin_users/edit_user");
  portletURL.setParameter("p_u_i_d",String.valueOf(_user.getUserId()));
  return portletURL;
}
