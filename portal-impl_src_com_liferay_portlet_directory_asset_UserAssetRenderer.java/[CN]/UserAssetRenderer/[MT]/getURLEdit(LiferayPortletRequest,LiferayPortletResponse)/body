{
  String portletId=PortletProviderUtil.getPortletId(User.class.getName(),PortletProvider.Action.VIEW);
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),portletId,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcRenderCommandName","/users_admin/edit_user");
  portletURL.setParameter("p_u_i_d",String.valueOf(_user.getUserId()));
  return portletURL;
}
