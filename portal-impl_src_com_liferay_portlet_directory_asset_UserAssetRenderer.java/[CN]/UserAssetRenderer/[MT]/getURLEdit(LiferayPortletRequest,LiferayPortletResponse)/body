{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(PortalUtil.getControlPanelPlid(liferayPortletRequest),PortletKeys.USERS_ADMIN,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/users_admin/edit_user");
  portletURL.setParameter("p_u_i_d",String.valueOf(_user.getUserId()));
  return portletURL;
}
