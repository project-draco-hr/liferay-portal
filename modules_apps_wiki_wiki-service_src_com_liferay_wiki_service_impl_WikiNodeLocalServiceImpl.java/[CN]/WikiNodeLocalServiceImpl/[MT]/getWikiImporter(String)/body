{
  WikiImporter wikiImporter=_wikiImporters.get(importer);
  if (wikiImporter == null) {
    String importerClassName=PropsUtil.get(WikiPropsKeys.IMPORTERS_CLASS,new Filter(importer));
    if (importerClassName != null) {
      try {
        wikiImporter=(WikiImporter)InstanceFactory.newInstance(getClassLoader(),importerClassName);
        _wikiImporters.put(importer,wikiImporter);
      }
 catch (      Exception e) {
        throw new SystemException("Unable to instantiate wiki importer class " + importerClassName,e);
      }
    }
    if (importer == null) {
      throw new SystemException("Unable to instantiate wiki importer class " + importerClassName);
    }
  }
  return wikiImporter;
}
