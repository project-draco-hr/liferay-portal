{
  try {
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setCompanyId(companyId);
    List<KaleoDefinition> kaleoDefinitions=KaleoDefinitionLocalServiceUtil.getKaleoDefinitions(name,start,end,KaleoDefinitionOrderByComparator.getOrderByComparator(orderByComparator),serviceContext);
    return toWorkflowDefinitions(kaleoDefinitions);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
