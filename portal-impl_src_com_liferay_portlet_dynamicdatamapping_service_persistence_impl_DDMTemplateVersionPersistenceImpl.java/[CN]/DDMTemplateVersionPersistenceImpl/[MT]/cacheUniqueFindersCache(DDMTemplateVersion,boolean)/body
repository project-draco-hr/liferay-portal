{
  if (isNew) {
    Object[] args=new Object[]{ddmTemplateVersion.getTemplateId(),ddmTemplateVersion.getVersion()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_T_V,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_T_V,args,ddmTemplateVersion);
  }
 else {
    DDMTemplateVersionModelImpl ddmTemplateVersionModelImpl=(DDMTemplateVersionModelImpl)ddmTemplateVersion;
    if ((ddmTemplateVersionModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_T_V.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{ddmTemplateVersion.getTemplateId(),ddmTemplateVersion.getVersion()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_T_V,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_T_V,args,ddmTemplateVersion);
    }
  }
}
