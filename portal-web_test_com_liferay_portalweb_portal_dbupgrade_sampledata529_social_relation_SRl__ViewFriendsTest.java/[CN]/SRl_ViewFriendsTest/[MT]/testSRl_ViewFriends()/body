{
  selenium.open("/web/socialrelationsn1/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Friends Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Friends Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("You have 1 friends."),selenium.getText("//div[@class='portlet-msg-success'][1]"));
  assertTrue(selenium.isPartialText("//td[2]/div/a","socialrelationfn2 socialrelationmn2 socialrelationln2"));
}
