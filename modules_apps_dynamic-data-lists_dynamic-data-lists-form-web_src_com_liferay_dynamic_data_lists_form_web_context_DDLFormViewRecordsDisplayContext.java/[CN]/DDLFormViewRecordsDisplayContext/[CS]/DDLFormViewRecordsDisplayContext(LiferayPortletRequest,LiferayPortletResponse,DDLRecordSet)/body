{
  _liferayPortletRequest=liferayPortletRequest;
  _liferayPortletResponse=liferayPortletResponse;
  _recordSet=recordSet;
  ThemeDisplay themeDisplay=(ThemeDisplay)_liferayPortletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  portletDisplay.setShowBackIcon(true);
  portletDisplay.setURLBack(ParamUtil.getString(_liferayPortletRequest,"redirect"));
  createRecordSearchContainer(recordSet.getDDMStructure());
}
