{
  PollsChoice choice=fetchByUUID_G(uuid,groupId);
  if (choice != null) {
    return choice;
  }
  StringBundler sb=new StringBundler(5);
  sb.append("No PollsChoice exists with the key {uuid=");
  sb.append(uuid);
  sb.append(", groupId=");
  sb.append(groupId);
  sb.append("}");
  throw new NoSuchChoiceException(sb.toString());
}
