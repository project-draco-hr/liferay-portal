{
  if (Files.notExists(filePath)) {
    return null;
  }
  String name=String.valueOf(filePath.getFileName());
  SyncFile syncFile=addSyncFile(null,null,false,null,filePath.toString(),Files.probeContentType(filePath),name,parentFolderId,0,repositoryId,SyncFile.STATE_SYNCED,syncAccountId,SyncFile.TYPE_FOLDER);
  FileEventUtil.addFolder(parentFolderId,repositoryId,syncAccountId,name,syncFile);
  return syncFile;
}
