{
  KaleoInstanceToken kaleoInstanceToken=executionContext.getKaleoInstanceToken();
  int count=KaleoTimerInstanceTokenLocalServiceUtil.getKaleoTimerInstanceTokensCount(kaleoInstanceToken.getKaleoInstanceTokenId(),false,true,executionContext.getServiceContext());
  if (count > 0) {
    return true;
  }
  return false;
}
