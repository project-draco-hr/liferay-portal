{
  try {
    MethodKey methodKey=new MethodKey(LayoutServiceUtil.class,"schedulePublishToRemote",_schedulePublishToRemoteParameterTypes27);
    MethodHandler methodHandler=new MethodHandler(methodKey,sourceGroupId,privateLayout,layoutIdMap,parameterMap,remoteAddress,remotePort,remotePathContext,secureConnection,remoteGroupId,remotePrivateLayout,startDate,endDate,groupName,cronText,schedulerStartDate,schedulerEndDate,description);
    try {
      TunnelUtil.invoke(httpPrincipal,methodHandler);
    }
 catch (    Exception e) {
      if (e instanceof com.liferay.portal.kernel.exception.PortalException) {
        throw (com.liferay.portal.kernel.exception.PortalException)e;
      }
      if (e instanceof com.liferay.portal.kernel.exception.SystemException) {
        throw (com.liferay.portal.kernel.exception.SystemException)e;
      }
      throw new com.liferay.portal.kernel.exception.SystemException(e);
    }
  }
 catch (  com.liferay.portal.kernel.exception.SystemException se) {
    _log.error(se,se);
    throw se;
  }
}
