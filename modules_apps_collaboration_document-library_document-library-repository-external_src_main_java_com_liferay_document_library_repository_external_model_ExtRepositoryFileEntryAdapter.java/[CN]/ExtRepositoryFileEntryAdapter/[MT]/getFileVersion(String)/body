{
  List<ExtRepositoryFileVersionAdapter> extRepositoryFileVersionAdapters=_getExtRepositoryFileVersionAdapters();
  for (  ExtRepositoryFileVersionAdapter extRepositoryFileVersionAdapter : extRepositoryFileVersionAdapters) {
    String curVersion=extRepositoryFileVersionAdapter.getVersion();
    if (curVersion.equals(version)) {
      return extRepositoryFileVersionAdapter;
    }
  }
  throw new NoSuchFileVersionException("No file version with {fileEntryId=" + getFileEntryId() + ", version: "+ version+ "}");
}
