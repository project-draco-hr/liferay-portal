{
  _freemarkerEngineConfiguration=Configurable.createConfigurable(FreeMarkerEngineConfiguration.class,properties);
  for (  Bundle bundle : _bundles) {
    ClassLoader classLoader=_findClassLoader(_freemarkerEngineConfiguration.allowedClasses(),bundle.getBundleContext());
    if (classLoader != null) {
      _wwhitelistedClassloaders.add(classLoader);
    }
  }
}
