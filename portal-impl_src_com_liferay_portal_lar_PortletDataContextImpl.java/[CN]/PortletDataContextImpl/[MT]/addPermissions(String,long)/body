{
  StopWatch stopWatch=null;
  if (_log.isDebugEnabled()) {
    stopWatch=new StopWatch();
    stopWatch.start();
  }
  try {
    doAddPermissions(resourceName,resourcePK);
  }
  finally {
    if (stopWatch != null) {
      stopWatch.stop();
      _log.debug("doAddPermissions() took " + stopWatch.getTime() + " ms");
    }
  }
}
