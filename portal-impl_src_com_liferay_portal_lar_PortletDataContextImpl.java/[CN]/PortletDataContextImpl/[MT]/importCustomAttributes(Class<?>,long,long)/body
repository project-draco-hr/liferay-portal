{
  Map<String,Serializable> customAttributes=_customAttributesMap.get(getPrimaryKeyString(classObj,classPK));
  if (customAttributes == null) {
    return;
  }
  Iterator<Map.Entry<String,Serializable>> itr=customAttributes.entrySet().iterator();
  while (itr.hasNext()) {
    Map.Entry<String,Serializable> entry=itr.next();
    ExpandoValueLocalServiceUtil.addValue(_companyId,classObj.getName(),ExpandoTableConstants.DEFAULT_TABLE_NAME,entry.getKey(),newClassPK,entry.getValue());
  }
}
