{
  List<String> availableActionIds=new ArrayList<String>(actionIds.size());
  for (  String actionId : actionIds) {
    if (PermissionLocalServiceUtil.hasRolePermission(roleId,companyId,className,ResourceConstants.SCOPE_INDIVIDUAL,primKey,actionId)) {
      availableActionIds.add(actionId);
    }
  }
  return StringUtil.merge(availableActionIds);
}
