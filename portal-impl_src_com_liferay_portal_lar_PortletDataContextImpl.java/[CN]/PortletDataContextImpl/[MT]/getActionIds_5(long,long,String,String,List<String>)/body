{
  List<String> actionIds=new ArrayList<String>(allActionIds.size());
  for (  String actionId : allActionIds) {
    if (PermissionLocalServiceUtil.hasRolePermission(roleId,companyId,className,ResourceConstants.SCOPE_INDIVIDUAL,primKey,actionId)) {
      actionIds.add(actionId);
    }
  }
  return StringUtil.merge(actionIds);
}
