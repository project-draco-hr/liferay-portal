{
  if (parentElement == null) {
    return null;
  }
  StringBundler sb=new StringBundler(6);
  sb.append("staged-model");
  sb.append("[@");
  sb.append(attribute);
  sb.append("='");
  sb.append(value);
  sb.append("']");
  XPath xPath=SAXReaderUtil.createXPath(sb.toString());
  return (Element)xPath.selectSingleNode(parentElement);
}
