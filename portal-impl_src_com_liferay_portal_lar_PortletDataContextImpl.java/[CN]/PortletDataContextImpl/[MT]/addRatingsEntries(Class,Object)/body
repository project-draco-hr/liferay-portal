{
  boolean exportRatings=MapUtil.getBoolean(_parameterMap,PortletDataHandlerKeys.EXPORT_RATINGS);
  if (!exportRatings && (classObj != Layout.class)) {
    return;
  }
  List entries=RatingsEntryLocalServiceUtil.getEntries(classObj.getName(),((Long)primaryKey).longValue());
  if (entries.size() == 0) {
    return;
  }
  Iterator itr=entries.iterator();
  while (itr.hasNext()) {
    RatingsEntry entry=(RatingsEntry)itr.next();
    entry.setUserUuid(entry.getUserUuid());
  }
  _ratingsEntriesMap.put(getPrimaryKeyString(classObj,primaryKey),entries);
}
