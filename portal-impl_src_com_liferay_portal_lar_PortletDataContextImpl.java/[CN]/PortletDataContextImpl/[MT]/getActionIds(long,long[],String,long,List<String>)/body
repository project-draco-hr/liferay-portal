{
  if (ResourceBlockLocalServiceUtil.isSupported(className)) {
    return ResourceBlockPermissionLocalServiceUtil.getAvailableResourceBlockPermissionActionIds(roleIds,className,primKey,actionIds);
  }
 else {
    return ResourcePermissionLocalServiceUtil.getAvailableResourcePermissionActionIds(companyId,className,ResourceConstants.SCOPE_INDIVIDUAL,String.valueOf(primKey),roleIds,actionIds);
  }
}
