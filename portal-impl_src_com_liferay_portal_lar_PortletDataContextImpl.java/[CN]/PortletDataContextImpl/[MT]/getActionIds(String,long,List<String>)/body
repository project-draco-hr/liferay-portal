{
  if (ResourceBlockLocalServiceUtil.isSupported(className)) {
    return ResourceBlockPermissionLocalServiceUtil.getAvailableResourceBlockPermissionActionIds(className,primKey,actionIds);
  }
 else {
    return ResourcePermissionLocalServiceUtil.getAvailableResourcePermissionActionIds(_companyId,className,ResourceConstants.SCOPE_INDIVIDUAL,String.valueOf(primKey),actionIds);
  }
}
