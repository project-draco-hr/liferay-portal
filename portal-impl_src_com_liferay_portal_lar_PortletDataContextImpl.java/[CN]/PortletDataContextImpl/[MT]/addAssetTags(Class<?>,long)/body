{
  Asset asset=null;
  try {
    asset=AssetLocalServiceUtil.getAsset(classObj.getName(),classPK);
  }
 catch (  NoSuchAssetException nsae) {
    return;
  }
  List<AssetTag> tags=asset.getTags();
  if (tags.size() == 0) {
    return;
  }
  _assetTagsMap.put(getPrimaryKeyString(classObj,classPK),StringUtil.split(ListUtil.toString(tags,"name")));
}
