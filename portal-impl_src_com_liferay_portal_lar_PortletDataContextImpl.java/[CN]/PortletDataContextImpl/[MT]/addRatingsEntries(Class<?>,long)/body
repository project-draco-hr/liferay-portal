{
  List<RatingsEntry> ratingsEntries=RatingsEntryLocalServiceUtil.getEntries(classObj.getName(),classPK);
  if (ratingsEntries.size() == 0) {
    return;
  }
  Iterator<RatingsEntry> itr=ratingsEntries.iterator();
  while (itr.hasNext()) {
    RatingsEntry entry=itr.next();
    entry.setUserUuid(entry.getUserUuid());
  }
  _ratingsEntriesMap.put(getPrimaryKeyString(classObj,classPK),ratingsEntries);
}
