{
  List<RatingsEntry> entries=RatingsEntryLocalServiceUtil.getEntries(classObj.getName(),classPK);
  if (entries.size() == 0) {
    return;
  }
  Iterator<RatingsEntry> itr=entries.iterator();
  while (itr.hasNext()) {
    RatingsEntry entry=itr.next();
    entry.setUserUuid(entry.getUserUuid());
  }
  _ratingsEntriesMap.put(getPrimaryKeyString(classObj,classPK),entries);
}
