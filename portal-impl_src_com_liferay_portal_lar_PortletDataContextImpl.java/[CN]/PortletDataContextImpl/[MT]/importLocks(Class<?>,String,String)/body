{
  Lock lock=_locksMap.get(getPrimaryKeyString(classObj,key));
  if (lock == null) {
    return;
  }
  long userId=getUserId(lock.getUserUuid());
  long expirationTime=0;
  if (lock.getExpirationDate() != null) {
    expirationTime=lock.getExpirationDate().getTime();
  }
  LockLocalServiceUtil.lock(userId,classObj.getName(),newKey,lock.getOwner(),lock.getInheritable(),expirationTime);
}
