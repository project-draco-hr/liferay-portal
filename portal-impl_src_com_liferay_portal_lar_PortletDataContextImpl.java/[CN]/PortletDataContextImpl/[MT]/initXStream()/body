{
  _xStream=new XStream(null,new XppDriver(),new ClassLoaderReference(XStreamAliasRegistryUtil.getAliasesClassLoader(XStream.class.getClassLoader())));
  Map<Class<?>,String> aliases=XStreamAliasRegistryUtil.getAliases();
  for (  Map.Entry<Class<?>,String> alias : aliases.entrySet()) {
    _xStream.alias(alias.getValue(),alias.getKey());
  }
  Set<XStreamConverter> xStreamConverters=XStreamConverterRegistryUtil.getXStreamConverters();
  for (  XStreamConverter xStreamConverter : xStreamConverters) {
    _xStream.registerConverter(new ConverterAdapter(xStreamConverter),XStream.PRIORITY_VERY_HIGH);
  }
  _xStream.omitField(HashMap.class,"cache_bitmask");
}
