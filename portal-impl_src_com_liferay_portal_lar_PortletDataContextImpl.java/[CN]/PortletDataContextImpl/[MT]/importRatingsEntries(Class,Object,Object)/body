{
  boolean importRatings=MapUtil.getBoolean(_parameterMap,PortletDataHandlerKeys.IMPORT_RATINGS);
  if (!importRatings && (classObj != Layout.class)) {
    return;
  }
  List entries=(List)_ratingsEntriesMap.get(getPrimaryKeyString(classObj,primaryKey));
  if (entries == null) {
    return;
  }
  Iterator itr=entries.iterator();
  while (itr.hasNext()) {
    RatingsEntry entry=(RatingsEntry)itr.next();
    long userId=getUserId(entry.getUserUuid());
    RatingsEntryLocalServiceUtil.updateEntry(userId,classObj.getName(),((Long)newPrimaryKey).longValue(),entry.getScore());
  }
}
