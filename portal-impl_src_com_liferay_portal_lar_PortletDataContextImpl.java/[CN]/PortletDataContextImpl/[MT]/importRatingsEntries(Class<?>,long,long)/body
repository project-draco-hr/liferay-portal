{
  List<RatingsEntry> ratingsEntries=_ratingsEntriesMap.get(getPrimaryKeyString(classObj,classPK));
  if (ratingsEntries == null) {
    return;
  }
  for (  RatingsEntry ratingsEntry : ratingsEntries) {
    long userId=getUserId(ratingsEntry.getUserUuid());
    RatingsEntryLocalServiceUtil.updateEntry(userId,classObj.getName(),((Long)newClassPK).longValue(),ratingsEntry.getScore());
  }
}
