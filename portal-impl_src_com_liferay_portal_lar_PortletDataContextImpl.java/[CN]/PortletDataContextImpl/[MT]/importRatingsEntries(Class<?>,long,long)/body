{
  List<RatingsEntry> entries=_ratingsEntriesMap.get(getPrimaryKeyString(classObj,classPK));
  if (entries == null) {
    return;
  }
  for (  RatingsEntry entry : entries) {
    long userId=getUserId(entry.getUserUuid());
    RatingsEntryLocalServiceUtil.updateEntry(userId,classObj.getName(),((Long)newClassPK).longValue(),entry.getScore());
  }
}
