{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select resourceId from Resource_ where codeId = ?");
    ps.setLong(1,codeId);
    rs=ps.executeQuery();
    while (rs.next()) {
      long resourceId=rs.getLong("resourceId");
      return resourceId;
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return 0;
}
