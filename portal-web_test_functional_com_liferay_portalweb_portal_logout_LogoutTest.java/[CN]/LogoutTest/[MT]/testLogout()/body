{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home");
  selenium.waitForVisible("//span[@class='user-full-name']");
  selenium.clickAt("//span[@class='user-full-name']",RuntimeVariables.replace("User Full Name"));
  selenium.waitForVisible("//ul[@class='dropdown-menu']/li[@class='sign-out']/a");
  assertEquals(RuntimeVariables.replace("Sign Out"),selenium.getText("//ul[@class='dropdown-menu']/li[@class='sign-out']/a"));
  selenium.clickAt("//ul[@class='dropdown-menu']/li[@class='sign-out']/a",RuntimeVariables.replace("Sign Out"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForVisible("//input[@id='_58_login']");
}
