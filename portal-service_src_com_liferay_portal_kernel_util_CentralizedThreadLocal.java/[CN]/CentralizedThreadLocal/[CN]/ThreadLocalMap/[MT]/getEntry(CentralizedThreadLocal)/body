{
  int index=key._hashCode & (_table.length - 1);
  Entry entry=_table[index];
  if (entry == null) {
    return null;
  }
 else   if (entry._key == key) {
    return entry;
  }
 else {
    while ((entry=entry._next) != null) {
      if (entry._key == key) {
        return entry;
      }
    }
    return null;
  }
}
