{
  super.setAttributes(request);
  Object bean=getBean();
  if (bean == null) {
    bean=pageContext.getAttribute("aui:model-context:bean");
  }
  String helpMessage=getHelpMessage();
  if (Validator.isNull(helpMessage) && (getStatus() == WorkflowConstants.STATUS_APPROVED) && Validator.isNotNull(getVersion())) {
    helpMessage=_HELP_MESSAGE_DEFAULT;
  }
  Class<?> model=getModel();
  if (model == null) {
    model=(Class<?>)pageContext.getAttribute("aui:model-context:model");
  }
  setNamespacedAttribute(request,"bean",bean);
  setNamespacedAttribute(request,"helpMessage",helpMessage);
  setNamespacedAttribute(request,"model",model);
}
