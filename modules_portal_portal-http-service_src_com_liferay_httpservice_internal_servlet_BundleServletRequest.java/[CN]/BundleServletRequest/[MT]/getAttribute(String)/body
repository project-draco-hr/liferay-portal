{
  if ((name.equals(JavaConstants.JAVAX_SERVLET_FORWARD_SERVLET_PATH) || name.equals(WebKeys.INVOKER_FILTER_URI)) && _attributes.containsKey(WebKeys.SERVLET_PATH)) {
    return _attributes.get(WebKeys.SERVLET_PATH);
  }
  if (_maskedAttributes.contains(name)) {
    return _attributes.get(name);
  }
  return super.getAttribute(name);
}
