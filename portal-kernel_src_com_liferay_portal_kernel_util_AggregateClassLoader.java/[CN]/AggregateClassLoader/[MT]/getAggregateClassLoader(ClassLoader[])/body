{
  if ((classLoaders == null) || (classLoaders.length == 0)) {
    return null;
  }
  if (classLoaders.length == 1) {
    return classLoaders[0];
  }
  AggregateClassLoader aggregateLoader=new AggregateClassLoader(classLoaders[1],classLoaders[0]);
  for (int i=2; i < classLoaders.length; i++) {
    aggregateLoader=new AggregateClassLoader(classLoaders[i],aggregateLoader);
  }
  return aggregateLoader;
}
