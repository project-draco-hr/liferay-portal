{
  Class clazz=null;
  for (  ClassLoader classLoader : _classLoaders) {
    try {
      clazz=classLoader.loadClass(name);
    }
 catch (    ClassNotFoundException e) {
    }
  }
  if (clazz == null) {
    clazz=getParent().loadClass(name);
  }
  if (resolve) {
    resolveClass(clazz);
  }
  return clazz;
}
