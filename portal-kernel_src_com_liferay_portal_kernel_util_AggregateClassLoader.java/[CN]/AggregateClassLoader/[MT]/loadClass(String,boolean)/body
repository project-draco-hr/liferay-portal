{
  Class<?> loadedClass=null;
  try {
    loadedClass=super.loadClass(name,resolve);
  }
 catch (  ClassNotFoundException cnfe) {
    if (_backupClassLoader != null) {
      loadedClass=_backupClassLoader.loadClass(name);
    }
 else {
      throw cnfe;
    }
  }
  if (resolve) {
    resolveClass(loadedClass);
  }
  return loadedClass;
}
