{
  Class clazz=null;
  for (  ClassLoaderWrapper classLoader : _classLoaders) {
    try {
      clazz=classLoader.loadClass(name,resolve);
    }
 catch (    ClassNotFoundException e) {
    }
  }
  if (clazz == null) {
    clazz=super.loadClass(name,resolve);
  }
 else   if (resolve) {
    resolveClass(clazz);
  }
  return clazz;
}
