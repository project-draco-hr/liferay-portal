{
  friendlyURL=friendlyURL.toLowerCase();
  if (PortalInstances.isVirtualHostsIgnorePath(friendlyURL) || friendlyURL.startsWith(Portal.PATH_MODULE) || friendlyURL.startsWith(_PRIVATE_GROUP_SERVLET_MAPPING_SLASH)|| friendlyURL.startsWith(_PRIVATE_USER_SERVLET_MAPPING_SLASH)|| friendlyURL.startsWith(_PUBLIC_GROUP_SERVLET_MAPPING_SLASH)) {
    return false;
  }
  if (LayoutImpl.hasFriendlyURLKeyword(friendlyURL)) {
    return false;
  }
  int code=LayoutImpl.validateFriendlyURL(friendlyURL,false);
  if ((code > -1) && (code != LayoutFriendlyURLException.ENDS_WITH_SLASH)) {
    return false;
  }
  return true;
}
