{
  friendlyURL=friendlyURL.toLowerCase();
  if (PortalInstances.isVirtualHostsIgnorePath(friendlyURL) || friendlyURL.startsWith(_PATH_VTI) || friendlyURL.startsWith(_PRIVATE_GROUP_SERVLET_MAPPING_SLASH)|| friendlyURL.startsWith(_PRIVATE_USER_SERVLET_MAPPING_SLASH)|| friendlyURL.startsWith(_PUBLIC_GROUP_SERVLET_MAPPING_SLASH)) {
    return false;
  }
  for (  String keyword : _slashedKeywords) {
    if (friendlyURL.startsWith(keyword)) {
      return false;
    }
  }
  int code=LayoutImpl.validateFriendlyURL(friendlyURL);
  if ((code > -1) && (code != LayoutFriendlyURLException.ENDS_WITH_SLASH)) {
    return false;
  }
  return true;
}
