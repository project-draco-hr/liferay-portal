{
  if (PortalInstances.isIgnorePath(friendlyURL) || friendlyURL.startsWith(_PATH_HTML) || friendlyURL.startsWith(_PATH_IMAGE)|| friendlyURL.startsWith(_PATH_WAP)) {
    return false;
  }
  int code=LayoutImpl.validateFriendlyURL(friendlyURL);
  if ((code > -1) && (code != LayoutFriendlyURLException.ENDS_WITH_SLASH)) {
    return false;
  }
  return true;
}
