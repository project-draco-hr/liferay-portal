{
  super.init(filterConfig);
  _servletContext=filterConfig.getServletContext();
  _slashedKeywords=new String[PropsValues.LAYOUT_FRIENDLY_URL_KEYWORDS.length];
  for (int i=0; i < PropsValues.LAYOUT_FRIENDLY_URL_KEYWORDS.length; i++) {
    _slashedKeywords[i]=StringUtil.quote(PropsValues.LAYOUT_FRIENDLY_URL_KEYWORDS[i],StringPool.SLASH);
  }
}
