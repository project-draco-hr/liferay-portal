{
  if (!isEnabled()) {
    throw new IllegalStateException("Cannot call \"identify\" when ImageMagick is disabled");
  }
  ProcessExecutor exec=_getProcessExecutor();
  LiferayIdentifyCmd identifyCmd=new LiferayIdentifyCmd();
  ArrayListOutputConsumer arrayListOutputConsumer=new ArrayListOutputConsumer();
  identifyCmd.setOutputConsumer(arrayListOutputConsumer);
  ProcessTask processTask=identifyCmd.getProcessTask(_globalSearchPath,getResourceLimits(),arguments);
  exec.execute(processTask);
  processTask.get();
  List<String> output=arrayListOutputConsumer.getOutput();
  if (output != null) {
    return output.toArray(new String[0]);
  }
 else {
    return new String[0];
  }
}
