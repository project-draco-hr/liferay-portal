{
  if (!isEnabled()) {
    throw new IllegalStateException("Cannot call \"identify\" when ImageMagick is disabled");
  }
  ProcessExecutor processExecutor=_getProcessExecutor();
  LiferayIdentifyCmd liferayIdentifyCmd=new LiferayIdentifyCmd();
  ArrayListOutputConsumer arrayListOutputConsumer=new ArrayListOutputConsumer();
  liferayIdentifyCmd.setOutputConsumer(arrayListOutputConsumer);
  ProcessTask processTask=liferayIdentifyCmd.getProcessTask(_globalSearchPath,getResourceLimits(),arguments);
  processExecutor.execute(processTask);
  processTask.get();
  List<String> output=arrayListOutputConsumer.getOutput();
  if (output != null) {
    return output.toArray(new String[output.size()]);
  }
  return new String[0];
}
