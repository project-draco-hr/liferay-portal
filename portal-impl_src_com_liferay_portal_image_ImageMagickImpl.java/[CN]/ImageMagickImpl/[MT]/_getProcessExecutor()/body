{
  if (_processExecutor != null) {
    return _processExecutor;
  }
synchronized (ProcessExecutor.class) {
    if (_processExecutor == null) {
      _processExecutor=new ProcessExecutor();
      _processExecutor.setThreadFactory(new NamedThreadFactory(ImageMagickImpl.class.getName(),Thread.MIN_PRIORITY,PortalClassLoaderUtil.getClassLoader()));
    }
  }
  return _processExecutor;
}
