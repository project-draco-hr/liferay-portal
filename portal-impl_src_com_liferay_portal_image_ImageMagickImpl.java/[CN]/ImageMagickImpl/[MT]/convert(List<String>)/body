{
  if (!isEnabled()) {
    throw new IllegalStateException("Cannot call \"convert\" when ImageMagick is disabled");
  }
  ProcessExecutor exec=_getProcessExecutor();
  LiferayConvertCmd convertCmd=new LiferayConvertCmd();
  ProcessTask processTask=convertCmd.getProcessTask(_globalSearchPath,getResourceLimits(),arguments);
  exec.execute(processTask);
  return processTask;
}
