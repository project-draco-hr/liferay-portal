{
  Class<?> clazz=getClass();
  ClassLoader classLoader=clazz.getClassLoader();
  Properties systemProperties=System.getProperties();
  SystemEnv.setProperties(systemProperties);
  Log4JUtil.initLog4J(_serverId,_liferayHome,classLoader,new Log4jLogFactoryImpl(),_customLogSettings);
  try {
    if (_convert) {
      LiferayConvertCmd.run(_globalSearchPath,_resourceLimits,_commandArguments);
    }
 else {
      return LiferayIdentifyCmd.run(_globalSearchPath,_resourceLimits,_commandArguments);
    }
  }
 catch (  Exception e) {
    throw new ProcessException(e);
  }
  return null;
}
