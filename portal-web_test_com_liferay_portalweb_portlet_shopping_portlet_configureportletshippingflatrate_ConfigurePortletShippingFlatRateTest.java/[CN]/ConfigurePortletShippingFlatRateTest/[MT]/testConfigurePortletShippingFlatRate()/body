{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Shopping Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Shipping Calculation",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.select("_86_shippingFormula",RuntimeVariables.replace("label=Flat Amount"));
  selenium.type("_86_shipping0",RuntimeVariables.replace("2.49"));
  selenium.type("_86_shipping1",RuntimeVariables.replace("5.49"));
  selenium.type("_86_shipping2",RuntimeVariables.replace("12.49"));
  selenium.type("_86_shipping3",RuntimeVariables.replace("19.99"));
  selenium.type("_86_shipping4",RuntimeVariables.replace("25.61"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("You have successfully updated the setup."),selenium.getText("//div[@id='p_p_id_86_']/div/div"));
}
