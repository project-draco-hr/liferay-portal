{
  templateManager.addTaglibApplication(contextObjects,PortletDisplayTemplateConstants.FREEMARKER_SERVLET_APPLICATION,request.getServletContext());
  templateManager.addTaglibRequest(contextObjects,PortletDisplayTemplateConstants.FREEMARKER_SERVLET_REQUEST,request,response);
  templateManager.addTaglibFactory(contextObjects,PortletDisplayTemplateConstants.TAGLIB_LIFERAY_HASH,request.getServletContext());
}
