{
  FileEntry fileEntry=DLAppServiceUtil.updateFileEntry(_fileVersion.getFileEntryId(),_SOURCE_FILE_NAME,_fileVersion.getMimeType(),_fileVersion.getTitle(),_fileVersion.getDescription(),_fileVersion.getChangeLog(),false,_DATA_VERSION_1,_serviceContext);
  Assert.assertEquals(DLFileEntryConstants.VERSION_DEFAULT,fileEntry.getVersion());
  checkLogForFileDeletion(1);
}
