{
  List<String> fieldsDisplayValues=new ArrayList<>();
  for (  String fieldName : fieldNames) {
    fieldsDisplayValues.add(fieldName + "_INSTANCE_" + StringUtil.randomString());
  }
  Field fieldsDisplayField=new Field(ddmStructure.getStructureId(),"_fieldsDisplay",StringUtil.merge(fieldsDisplayValues));
  fieldsDisplayField.setDefaultLocale(LocaleUtil.US);
  return fieldsDisplayField;
}
