{
  if (_layoutSet != null) {
    return _layoutSet;
  }
  try {
    _layoutSet=LayoutSetLocalServiceUtil.getLayoutSet(getGroupId(),getPrivateLayout());
    LayoutSetStagingHandler layoutSetStagingHandler=LayoutStagingUtil.getLayoutSetStagingHandler(_layoutSet);
    if (layoutSetStagingHandler == null) {
      return _layoutSet;
    }
    _layoutSet=layoutSetStagingHandler.getLayoutSet();
    return _layoutSet;
  }
 catch (  SystemException se) {
  }
catch (  PortalException pe) {
  }
  return _layoutSet;
}
