{
  if (_totalSize < THRESHOLD_SIZE) {
    return new ServletInputStreamAdapter(new UnsyncByteArrayInputStream(_cachedBytes.unsafeGetByteArray(),0,_cachedBytes.size()));
  }
 else   if (_tempFile != null) {
    return new ServletInputStreamAdapter(new FileInputStream(_tempFile));
  }
 else {
    return this;
  }
}
