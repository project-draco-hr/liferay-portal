{
  Recurrence recurrence=new Recurrence(dtStart,duration,Recurrence.DAILY);
  DayAndPosition[] days={new DayAndPosition(MONDAY,0),new DayAndPosition(TUESDAY,0),new DayAndPosition(WEDNESDAY,0),new DayAndPosition(THURSDAY,0),new DayAndPosition(FRIDAY,0)};
  recurrence.setByDay(days);
  return recurrence;
}
