{
  BatchablePipe<String,Counter<?>> pipe=(BatchablePipe<String,Counter<?>>)message.getPayload();
  IncreasableCounterEntry entry=(IncreasableCounterEntry)pipe.take();
  if (entry == null) {
    return;
  }
  MethodInvocation methodInvocation=entry.getMethodInvocation();
  _deserializeMethodArguments(methodInvocation,entry);
  try {
    entry.getNextInterceptor().invoke(methodInvocation);
  }
 catch (  Throwable t) {
    _log.error("Cannot write buffered counter " + "value to the database",t);
  }
}
