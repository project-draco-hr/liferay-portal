{
  try {
    Indexer.deleteProductEntry(productEntry.getCompanyId(),productEntry.getProductEntryId());
  }
 catch (  IOException ioe) {
    _log.error("Deleting index " + productEntry.getProductEntryId(),ioe);
  }
  SRProductVersionLocalServiceUtil.deleteProductVersions(productEntry.getProductEntryId());
  ResourceLocalServiceUtil.deleteResource(productEntry.getCompanyId(),SRProductEntry.class.getName(),ResourceImpl.TYPE_CLASS,ResourceImpl.SCOPE_INDIVIDUAL,productEntry.getPrimaryKey());
  SRProductEntryUtil.remove(productEntry.getProductEntryId());
}
