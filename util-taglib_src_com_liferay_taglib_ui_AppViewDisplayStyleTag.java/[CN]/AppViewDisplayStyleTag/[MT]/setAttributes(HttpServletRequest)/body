{
  request.setAttribute("liferay-ui:app-view-display-style:displayStyle",_displayStyle);
  request.setAttribute("liferay-ui:app-view-display-style:displayStyles",_displayStyles);
  request.setAttribute("liferay-ui:app-view-display-style:eventName",_eventName);
  request.setAttribute("liferay-ui:app-view-display-style:requestParams",_requestParams);
}
