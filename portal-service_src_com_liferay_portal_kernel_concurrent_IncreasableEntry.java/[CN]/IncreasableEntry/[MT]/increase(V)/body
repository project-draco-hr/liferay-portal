{
  boolean[] marked={false};
  while (true) {
    V originalValue=_markedValue.get(marked);
    if (marked[0]) {
      return false;
    }
 else {
      V newValue=doIncrease(originalValue,deltaValue);
      if (_markedValue.compareAndSet(originalValue,newValue,false,false)) {
        return true;
      }
    }
  }
}
