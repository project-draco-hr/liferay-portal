{
  if (Validator.isNull(_ddmForm.availableLanguageIds())) {
    Locale defaultLocale=getDefaultLocale();
    return SetUtil.fromArray(new Locale[]{defaultLocale});
  }
  Set<Locale> availableLocales=new TreeSet<>();
  for (  String availableLanguageId : StringUtil.split(_ddmForm.availableLanguageIds())) {
    availableLocales.add(LocaleUtil.fromLanguageId(availableLanguageId));
  }
  return availableLocales;
}
