{
  Class<?> declaringClass=classLoader.loadClass(_declaringClass.getName());
  Class<?>[] parameterTypes=new Class<?>[_parameterTypes.length];
  for (int i=0; i < _parameterTypes.length; i++) {
    parameterTypes[i]=classLoader.loadClass(_parameterTypes[i].getName());
  }
  return new MethodKey(declaringClass,_methodName,parameterTypes);
}
