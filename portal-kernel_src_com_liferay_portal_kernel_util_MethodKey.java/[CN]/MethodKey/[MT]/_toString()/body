{
  if (_toString == null) {
    StringMaker sm=new StringMaker();
    sm.append(_className);
    sm.append(_methodName);
    if (_types != null && _types.length > 0) {
      sm.append("-");
      for (int i=0; i < _types.length; i++) {
        sm.append(_types[i].getClass().getName());
      }
    }
    _toString=sm.toString();
  }
  return _toString;
}
