{
  if (_toString == null) {
    StringBuilder sb=new StringBuilder();
    sb.append(_className);
    sb.append(_methodName);
    if (_types != null && _types.length > 0) {
      sb.append("-");
      for (int i=0; i < _types.length; i++) {
        sb.append(_types[i].getName());
      }
    }
    _toString=sb.toString();
  }
  return _toString;
}
