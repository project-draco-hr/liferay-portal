{
  Map<String,DDMFormField> ddmFormfieldsMap=new HashMap<String,DDMFormField>();
  for (  DDMFormField nestedDDMFormField : _nestedDDMFormFields) {
    ddmFormfieldsMap.put(nestedDDMFormField.getName(),nestedDDMFormField);
    ddmFormfieldsMap.putAll(nestedDDMFormField.getNestedDDMFormFieldsMap());
  }
  return ddmFormfieldsMap;
}
