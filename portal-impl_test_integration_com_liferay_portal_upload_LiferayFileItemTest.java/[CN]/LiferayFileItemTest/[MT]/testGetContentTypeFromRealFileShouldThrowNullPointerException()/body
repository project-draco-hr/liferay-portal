{
  File file=DependenciesTestUtil.getDependencyAsFile(this.getClass(),"LiferayFileItem.txt");
  String contentType=Files.probeContentType(file.toPath());
  String fieldName=RandomTestUtil.randomString();
  String fileName=file.getName();
  FileItem item=_liferayFileItemFactory.createItem(fieldName,contentType,false,fileName);
  Assert.assertNotNull(item);
  item.getContentType();
}
