{
  if (name.equals(JavaConstants.JAVAX_SERVLET_CONTEXT_TEMPDIR) || name.equals(PluginContextListener.PLUGIN_CLASS_LOADER)) {
    return;
  }
  Object originalValue=_contextAttributes.get(name);
  _contextAttributes.put(name,value);
  for (  ServletContextAttributeListener listener : _servletContextAttributeListeners) {
    if (originalValue != null) {
      listener.attributeReplaced(new ServletContextAttributeEvent(this,name,originalValue));
    }
 else {
      listener.attributeAdded(new ServletContextAttributeEvent(this,name,value));
    }
  }
}
