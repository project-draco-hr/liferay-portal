{
  if (_tempDir == null) {
    File parentTempDir=(File)super.getAttribute(JavaConstants.JAVAX_SERVLET_CONTEXT_TEMPDIR);
    String servletContextName=getServletContextName();
    File tempDir=new File(parentTempDir,servletContextName);
    if (!tempDir.exists() && !tempDir.mkdirs()) {
      throw new IllegalStateException("Can't create webapp tempDir for " + getServletContextName());
    }
    _tempDir=tempDir;
  }
  return _tempDir;
}
