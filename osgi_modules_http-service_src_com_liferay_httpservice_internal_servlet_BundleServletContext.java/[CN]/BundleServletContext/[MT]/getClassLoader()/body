{
  Object value=_contextAttributes.get(PluginContextListener.PLUGIN_CLASS_LOADER);
  if (value == null) {
    BundleWiring bundleWiring=_bundle.adapt(BundleWiring.class);
    value=bundleWiring.getClassLoader();
    _contextAttributes.put(PluginContextListener.PLUGIN_CLASS_LOADER,value);
  }
  return (ClassLoader)value;
}
