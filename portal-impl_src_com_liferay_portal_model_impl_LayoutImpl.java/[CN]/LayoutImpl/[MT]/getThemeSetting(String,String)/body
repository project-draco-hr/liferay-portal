{
  UnicodeProperties properties=getTypeSettingsProperties();
  String value=properties.getThemeProperty(key,device);
  if (value != null) {
    return value;
  }
  try {
    LayoutSet layoutSet=getLayoutSet();
    value=layoutSet.getThemeSetting(key,device);
  }
 catch (  Exception e) {
  }
  return value;
}
