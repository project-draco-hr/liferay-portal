{
  if (!isTypePortlet()) {
    return false;
  }
  if (GetterUtil.getBoolean(getTypeSettingsProperty(LayoutConstants.CUSTOMIZABLE_LAYOUT))) {
    return true;
  }
  LayoutTypePortlet layoutTypePortlet=(LayoutTypePortlet)getLayoutType();
  if (layoutTypePortlet.isCustomizable()) {
    return true;
  }
  return false;
}
