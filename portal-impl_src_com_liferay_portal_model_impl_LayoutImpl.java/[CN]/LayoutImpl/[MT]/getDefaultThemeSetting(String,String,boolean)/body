{
  if (!inheritLookAndFeel) {
    try {
      Theme theme=getTheme();
      return theme.getSetting(key);
    }
 catch (    Exception e) {
    }
  }
  try {
    LayoutSet layoutSet=getLayoutSet();
    return layoutSet.getThemeSetting(key,device);
  }
 catch (  Exception e) {
  }
  return StringPool.BLANK;
}
