{
  for (  String keyword : PropsValues.LAYOUT_FRIENDLY_URL_KEYWORDS) {
    if (friendlyURL.contains(StringUtil.quote(keyword,StringPool.SLASH)) || friendlyURL.endsWith(StringPool.SLASH + keyword)) {
      LayoutFriendlyURLException lfurle=new LayoutFriendlyURLException(LayoutFriendlyURLException.KEYWORD_CONFLICT);
      lfurle.setKeywordConflict(keyword);
      throw lfurle;
    }
  }
}
