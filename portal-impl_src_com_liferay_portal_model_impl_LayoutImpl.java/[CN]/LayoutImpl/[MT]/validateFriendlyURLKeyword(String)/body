{
  String[] keywords=PropsUtil.getArray(PropsUtil.LAYOUT_FRIENDLY_URL_KEYWORDS);
  for (int i=0; i < keywords.length; i++) {
    String keyword=keywords[i];
    if ((friendlyURL.indexOf(StringPool.SLASH + keyword + StringPool.SLASH) != -1) || (friendlyURL.endsWith(StringPool.SLASH + keyword))) {
      LayoutFriendlyURLException lfurle=new LayoutFriendlyURLException(LayoutFriendlyURLException.KEYWORD_CONFLICT);
      lfurle.setKeywordConflict(keyword);
      throw lfurle;
    }
  }
}
