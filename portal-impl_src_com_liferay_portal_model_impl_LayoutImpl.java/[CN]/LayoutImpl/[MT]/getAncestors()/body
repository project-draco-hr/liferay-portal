{
  List ancestors=new ArrayList();
  Layout itLayout=this;
  while (true) {
    if (!itLayout.isRootLayout()) {
      itLayout=LayoutLocalServiceUtil.getLayout(itLayout.getGroupId(),itLayout.isPrivateLayout(),itLayout.getParentLayoutId());
      ancestors.add(itLayout);
    }
 else {
      break;
    }
  }
  return ancestors;
}
