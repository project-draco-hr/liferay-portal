{
  if (selectable) {
    long plid=getPlid();
    try {
      List<Layout> ancestors=layout.getAncestors();
      for (      Layout ancestor : ancestors) {
        if (plid == ancestor.getPlid()) {
          return true;
        }
      }
    }
 catch (    Exception e) {
    }
  }
  return false;
}
