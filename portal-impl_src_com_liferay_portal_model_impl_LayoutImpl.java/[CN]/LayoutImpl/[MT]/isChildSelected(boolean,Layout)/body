{
  if (selectable) {
    long plid=getPlid();
    try {
      List<Layout> ancestors=layout.getAncestors();
      for (      Layout curLayout : ancestors) {
        if (plid == curLayout.getPlid()) {
          return true;
        }
      }
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
  return false;
}
