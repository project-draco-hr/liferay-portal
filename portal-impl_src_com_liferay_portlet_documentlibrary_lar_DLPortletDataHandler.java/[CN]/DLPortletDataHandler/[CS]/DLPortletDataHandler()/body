{
  setDataLocalized(true);
  setDataPortletPreferences("rootFolderId");
  setExportControls(new PortletDataHandlerBoolean(NAMESPACE,"documents",true,false,null,DLFileEntry.class.getName()),new PortletDataHandlerBoolean(NAMESPACE,"shortcuts",true,false,null,DLFileShortcut.class.getName()),new PortletDataHandlerBoolean(NAMESPACE,"previews-and-thumbnails"),new PortletDataHandlerBoolean(NAMESPACE,"ranks",true,false,null,DLFileRank.class.getName()));
  setExportMetadataControls(new PortletDataHandlerBoolean(NAMESPACE,"folders-and-documents",true,new PortletDataHandlerControl[]{new PortletDataHandlerBoolean(NAMESPACE,"categories"),new PortletDataHandlerBoolean(NAMESPACE,"comments"),new PortletDataHandlerBoolean(NAMESPACE,"ratings"),new PortletDataHandlerBoolean(NAMESPACE,"tags")}));
  setPublishToLiveByDefault(PropsValues.DL_PUBLISH_TO_LIVE_BY_DEFAULT);
}
