{
  try {
    User user=UserLocalServiceUtil.getUserById(userId);
    PluginSetting pluginSetting=PluginSettingLocalServiceUtil.getSettingOrDefault(user.getCompanyId(),pluginId,pluginType);
    if (!pluginSetting.hasPermission(userId)) {
      return false;
    }
 else {
      return true;
    }
  }
 catch (  Exception e) {
    _log.warn("Could not check permissions for " + pluginId,e);
    return false;
  }
}
