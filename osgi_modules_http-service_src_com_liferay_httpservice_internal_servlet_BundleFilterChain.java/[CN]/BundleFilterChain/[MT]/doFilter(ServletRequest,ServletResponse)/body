{
  Filter filter=_filters.poll();
  if (filter == null) {
    ServletConfig servletConfig=_servlet.getServletConfig();
    if (servletConfig instanceof BundleServletConfig) {
      BundleServletConfig bundleServletConfig=(BundleServletConfig)servletConfig;
      HttpContext httpContext=bundleServletConfig.getHttpContext();
      if (!httpContext.handleSecurity((HttpServletRequest)servletRequest,(HttpServletResponse)servletResponse)) {
        return;
      }
    }
    _servlet.service(servletRequest,servletResponse);
    return;
  }
  filter.doFilter(servletRequest,servletResponse,this);
}
