{
  DeletionEntry deletionEntry=findByPrimaryKey(entryId);
  Session session=null;
  try {
    session=openSession();
    DeletionEntry[] array=new DeletionEntryImpl[3];
    array[0]=getByGroupId_PrevAndNext(session,deletionEntry,groupId,orderByComparator,true);
    array[1]=deletionEntry;
    array[2]=getByGroupId_PrevAndNext(session,deletionEntry,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
