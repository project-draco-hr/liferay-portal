{
  Session session=null;
  try {
    session=openSession();
    DeletionEntry deletionEntry=(DeletionEntry)session.get(DeletionEntryImpl.class,new Long(entryId));
    if (deletionEntry == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + entryId);
      }
      throw new NoSuchEntryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + entryId);
    }
    return deletionEntryPersistence.remove(deletionEntry);
  }
 catch (  NoSuchEntryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
