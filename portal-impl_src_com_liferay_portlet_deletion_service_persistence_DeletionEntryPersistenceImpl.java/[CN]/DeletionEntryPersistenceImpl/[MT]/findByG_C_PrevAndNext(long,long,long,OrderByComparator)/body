{
  DeletionEntry deletionEntry=findByPrimaryKey(entryId);
  Session session=null;
  try {
    session=openSession();
    DeletionEntry[] array=new DeletionEntryImpl[3];
    array[0]=getByG_C_PrevAndNext(session,deletionEntry,groupId,classNameId,orderByComparator,true);
    array[1]=deletionEntry;
    array[2]=getByG_C_PrevAndNext(session,deletionEntry,groupId,classNameId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
