{
  List<CacheStatistics> cacheManagerStatistics=null;
  StringBundler sb=new StringBundler(5);
  sb.append("net.sf.ehcache:type=CacheStatistics,CacheManager=");
  sb.append(cacheManagerName);
  sb.append(",name=*");
  if (_isValidKeywords(keywords)) {
    sb.append(keywords);
    sb.append("*");
  }
  try {
    Set<ObjectName> objectNames=_mBeanServer.queryNames(null,new ObjectName(sb.toString()));
    List<ObjectName> cacheStatisticsNames=new ArrayList<>(objectNames);
    Collections.sort(cacheStatisticsNames);
    cacheManagerStatistics=new ArrayList<>(cacheStatisticsNames.size());
    for (    ObjectName objectName : cacheStatisticsNames) {
      cacheManagerStatistics.add(new CacheStatistics(_mBeanServer,objectName));
    }
  }
 catch (  MalformedObjectNameException mone) {
    throw new SystemException(mone);
  }
  return cacheManagerStatistics;
}
