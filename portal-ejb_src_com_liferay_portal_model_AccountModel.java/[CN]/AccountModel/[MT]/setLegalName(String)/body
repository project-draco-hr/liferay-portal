{
  if (((legalName == null) && (_legalName != null)) || ((legalName != null) && (_legalName == null)) || ((legalName != null) && (_legalName != null) && !legalName.equals(_legalName))) {
    if (!XSS_ALLOW_LEGALNAME) {
      legalName=XSSUtil.strip(legalName);
    }
    _legalName=legalName;
  }
}
