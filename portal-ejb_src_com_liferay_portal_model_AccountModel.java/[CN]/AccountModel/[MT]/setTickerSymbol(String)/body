{
  if (((tickerSymbol == null) && (_tickerSymbol != null)) || ((tickerSymbol != null) && (_tickerSymbol == null)) || ((tickerSymbol != null) && (_tickerSymbol != null) && !tickerSymbol.equals(_tickerSymbol))) {
    if (!XSS_ALLOW_TICKERSYMBOL) {
      tickerSymbol=XSSUtil.strip(tickerSymbol);
    }
    _tickerSymbol=tickerSymbol;
    setModified(true);
  }
}
