{
  if (((legalType == null) && (_legalType != null)) || ((legalType != null) && (_legalType == null)) || ((legalType != null) && (_legalType != null) && !legalType.equals(_legalType))) {
    if (!XSS_ALLOW_LEGALTYPE) {
      legalType=XSSUtil.strip(legalType);
    }
    _legalType=legalType;
    setModified(true);
  }
}
