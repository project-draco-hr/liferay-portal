{
  if (((accountId == null) && (_accountId != null)) || ((accountId != null) && (_accountId == null)) || ((accountId != null) && (_accountId != null) && !accountId.equals(_accountId))) {
    if (!XSS_ALLOW_ACCOUNTID) {
      accountId=XSSUtil.strip(accountId);
    }
    _accountId=accountId;
  }
}
