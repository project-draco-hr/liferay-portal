{
  if (AccessControlThreadLocal.isRemoteAccess()) {
    for (    AccessControlPolicy policy : _policies) {
      policy.onServiceRemoteAccess(methodInvocation.getMethod(),methodInvocation.getArguments(),accessControlled);
    }
  }
 else {
    for (    AccessControlPolicy policy : _policies) {
      policy.onServiceAccess(methodInvocation.getMethod(),methodInvocation.getArguments(),accessControlled);
    }
  }
}
