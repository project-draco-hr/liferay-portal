{
  final Portlet portlet=createPortletWithContextPathAndResources("/pluginContextPath","/css/main.css","/css/main1.css");
  ComboServletStaticURLGenerator comboServletStaticURLGenerator=new ComboServletStaticURLGenerator();
  comboServletStaticURLGenerator.setPortletResourceAccessors(PortletResourceAccessor.headerPortletCss);
  comboServletStaticURLGenerator.setVisitedURLs(new HashSet<String>());
  comboServletStaticURLGenerator.setURLPrefix(URL_PREFIX);
  Collection<String> urls=comboServletStaticURLGenerator.generate(portlets(portlet));
  assertURLs(urls,URL_PREFIX + "&/pluginContextPath:/css/main.css" + "&/pluginContextPath:/css/main1.css&t=0");
}
