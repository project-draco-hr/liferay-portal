{
  final Portlet portlet=createPortletWithContextPathAndResources("/pluginContextPath","/css/main.css");
  ComboServletStaticURLGenerator comboServletStaticURLGenerator=new ComboServletStaticURLGenerator();
  comboServletStaticURLGenerator.setPortletResourcesAccessors(PortletResourceAccessor.headerPortalCss);
  HashSet<String> visited=new HashSet<String>();
  comboServletStaticURLGenerator.setVisitedURLs(visited);
  comboServletStaticURLGenerator.setComboURLPrefix(COMBO_URL_PREFIX);
  comboServletStaticURLGenerator.generate(portlets(portlet));
  Assert.assertTrue(visited.contains("/css/main.css"));
}
