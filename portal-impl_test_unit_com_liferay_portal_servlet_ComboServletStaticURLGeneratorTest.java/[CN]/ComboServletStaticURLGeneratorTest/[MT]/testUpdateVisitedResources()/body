{
  final Portlet portlet=createPortlet("/pluginContextPath","/css/main.css");
  ComboServletStaticURLGenerator comboServletStaticURLGenerator=new ComboServletStaticURLGenerator();
  comboServletStaticURLGenerator.setPortletResourceAccessors(PortletResourceAccessor.headerPortalCss);
  HashSet<String> visited=new HashSet<String>();
  comboServletStaticURLGenerator.setVisitedURLs(visited);
  comboServletStaticURLGenerator.setURLPrefix(URL_PREFIX);
  comboServletStaticURLGenerator.generate(toList(portlet));
  Assert.assertTrue(visited.contains("/css/main.css"));
}
