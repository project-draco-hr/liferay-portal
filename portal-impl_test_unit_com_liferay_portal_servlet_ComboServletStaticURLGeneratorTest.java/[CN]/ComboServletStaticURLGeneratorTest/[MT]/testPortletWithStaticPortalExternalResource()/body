{
  final Portlet portlet=createPortlet("/pluginContextPath","http://www.someserver.com/someResource.css","/css/main.css");
  ComboServletStaticURLGenerator comboServletStaticURLGenerator=new ComboServletStaticURLGenerator();
  comboServletStaticURLGenerator.setPortletResourceAccessors(PortletResourceAccessor.headerPortalCss);
  comboServletStaticURLGenerator.setVisitedURLs(new HashSet<String>());
  comboServletStaticURLGenerator.setURLPrefix(URL_PREFIX);
  List<String> urls=comboServletStaticURLGenerator.generate(toList(portlet));
  assertURLs(urls,"http://www.someserver.com/someResource.css",URL_PREFIX + "&/css/main.css&t=0");
}
