{
  StringBundler sb=new StringBundler(9);
  sb.append("{clusterNodeId=");
  sb.append(_clusterNodeId);
  sb.append(", hostName=");
  sb.append(_hostName);
  sb.append(", inetAddress=");
  sb.append(_inetAddress);
  sb.append(", port=");
  sb.append(_port);
  sb.append("}");
  return sb.toString();
}
