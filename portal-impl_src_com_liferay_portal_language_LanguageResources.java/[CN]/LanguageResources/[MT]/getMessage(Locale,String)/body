{
  if (locale == null) {
    return null;
  }
  Map<String,String> languageMap=_languageMaps.get(locale);
  if (languageMap == null) {
    languageMap=_loadLocale(locale);
  }
  String value=languageMap.get(key);
  if (value == null) {
    return getMessage(getSuperLocale(locale),key);
  }
 else {
    return value;
  }
}
