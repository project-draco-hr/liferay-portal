{
  Locale superLocale=_superLocales.get(locale);
  if (superLocale != null) {
    if (superLocale == _nullLocale) {
      return null;
    }
    return superLocale;
  }
  superLocale=_getSuperLocale(locale);
  if (superLocale == null) {
    _superLocales.put(locale,_nullLocale);
  }
 else {
    _superLocales.put(locale,superLocale);
  }
  return superLocale;
}
