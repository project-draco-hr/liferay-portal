{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.MEMBERSHIP_POLICY_ROLES);
  }
  ClassLoader classLoader=ClassLoaderUtil.getPortalClassLoader();
  _originalMembershipPolicy=(RoleMembershipPolicy)InstanceFactory.newInstance(classLoader,PropsValues.MEMBERSHIP_POLICY_ROLES);
  _membershipPolicy=_originalMembershipPolicy;
}
