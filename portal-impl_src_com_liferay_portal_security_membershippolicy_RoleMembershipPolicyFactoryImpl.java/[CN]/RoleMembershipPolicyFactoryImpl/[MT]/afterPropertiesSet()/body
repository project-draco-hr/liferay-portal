{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.MEMBERSHIP_POLICY_ROLES);
  }
  ClassLoader classLoader=ClassLoaderUtil.getPortalClassLoader();
  _originalRoleMembershipPolicy=(RoleMembershipPolicy)InstanceFactory.newInstance(classLoader,PropsValues.MEMBERSHIP_POLICY_ROLES);
  _roleMembershipPolicy=_originalRoleMembershipPolicy;
}
