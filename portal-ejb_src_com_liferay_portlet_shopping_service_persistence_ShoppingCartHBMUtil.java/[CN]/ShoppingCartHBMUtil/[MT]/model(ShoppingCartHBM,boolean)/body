{
  com.liferay.portlet.shopping.model.ShoppingCart shoppingCart=null;
  if (checkPool) {
    shoppingCart=ShoppingCartPool.get(shoppingCartHBM.getPrimaryKey());
  }
  if (shoppingCart == null) {
    shoppingCart=new com.liferay.portlet.shopping.model.ShoppingCart(shoppingCartHBM.getCartId(),shoppingCartHBM.getCompanyId(),shoppingCartHBM.getUserId(),shoppingCartHBM.getCreateDate(),shoppingCartHBM.getModifiedDate(),shoppingCartHBM.getItemIds(),shoppingCartHBM.getCouponIds(),shoppingCartHBM.getAltShipping(),shoppingCartHBM.getInsure());
    ShoppingCartPool.put(shoppingCart.getPrimaryKey(),shoppingCart);
  }
  return shoppingCart;
}
