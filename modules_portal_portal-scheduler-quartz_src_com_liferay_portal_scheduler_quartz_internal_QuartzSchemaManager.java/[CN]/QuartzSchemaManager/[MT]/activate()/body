{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select count(*) from QUARTZ_JOB_DETAILS");
    rs=ps.executeQuery();
    if (rs.next()) {
      return;
    }
  }
 catch (  Exception e) {
    if (_log.isInfoEnabled()) {
      _log.info(e,e);
    }
  }
 finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  try {
    con=DataAccess.getConnection();
    populateSchema(con);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    DataAccess.cleanUp(con);
  }
}
