{
  InputStream is=new UnsyncByteArrayInputStream(bytes);
  try {
    updateFile(companyId,portletId,groupId,repositoryId,fileName,versionNumber,sourceFileName,fileEntryId,properties,modifiedDate,serviceContext,is);
  }
  finally {
    try {
      is.close();
    }
 catch (    IOException ioe) {
      _log.error(ioe);
    }
  }
}
