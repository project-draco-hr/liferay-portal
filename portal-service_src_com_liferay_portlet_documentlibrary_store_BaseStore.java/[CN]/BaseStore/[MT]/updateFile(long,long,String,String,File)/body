{
  InputStream is=null;
  try {
    is=new FileInputStream(file);
    updateFile(companyId,repositoryId,fileName,versionLabel,is);
  }
 catch (  FileNotFoundException fnfe) {
    throw new NoSuchFileException(fileName);
  }
 finally {
    try {
      if (is != null) {
        is.close();
      }
    }
 catch (    IOException ioe) {
      _log.error(ioe);
    }
  }
}
