{
  InputStream is=null;
  try {
    is=new FileInputStream(file);
    addFile(companyId,repositoryId,fileName,is);
  }
 catch (  FileNotFoundException fnfe) {
    throw new SystemException(fnfe);
  }
 finally {
    try {
      if (is != null) {
        is.close();
      }
    }
 catch (    IOException ioe) {
      _log.error(ioe);
    }
  }
}
