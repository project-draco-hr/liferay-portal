{
  objectOutput.writeLong(clusterGroupId);
  if (name == null) {
    objectOutput.writeUTF(StringPool.BLANK);
  }
 else {
    objectOutput.writeUTF(name);
  }
  if (clusterNodeIds == null) {
    objectOutput.writeUTF(StringPool.BLANK);
  }
 else {
    objectOutput.writeUTF(clusterNodeIds);
  }
  objectOutput.writeBoolean(wholeCluster);
}
