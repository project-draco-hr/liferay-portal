{
  if (ExportImportThreadLocal.isStagingInProcess() && !group.isStagedPortlet(portletDataContext.getPortletId())) {
    return;
  }
  WikiNode node=_wikiNodeLocalService.fetchNode(portletDataContext.getScopeGroupId(),nodeName);
  if (node == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to export referenced wiki node " + nodeName);
    }
    return;
  }
  String portletId=portletDataContext.getPortletId();
  StagedModelDataHandlerUtil.exportReferenceStagedModel(portletDataContext,portletId,node);
}
