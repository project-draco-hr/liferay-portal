{
  BookmarksFolder folder=null;
  try {
    folder=getParentFolder();
  }
 catch (  Exception e) {
    return null;
  }
  while (folder != null) {
    if (folder.isInTrash()) {
      return folder;
    }
    try {
      folder=folder.getParentFolder();
    }
 catch (    Exception e) {
      return null;
    }
  }
  return null;
}
