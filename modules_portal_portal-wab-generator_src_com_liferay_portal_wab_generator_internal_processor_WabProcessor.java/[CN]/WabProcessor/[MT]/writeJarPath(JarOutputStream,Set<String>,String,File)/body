{
  if (paths.contains(path)) {
    return;
  }
  paths.add(path);
  try {
    jarOutputStream.putNextEntry(new JarEntry(path));
    StreamUtil.transfer(new FileInputStream(file),StreamUtil.uncloseable(jarOutputStream));
    jarOutputStream.closeEntry();
  }
 catch (  IOException ioe) {
    _log.error(ioe,ioe);
  }
}
