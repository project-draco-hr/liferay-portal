{
  try {
    com.liferay.portal.model.LayoutReference[] returnValue=LayoutServiceUtil.getLayoutReferences(companyId,portletId,preferencesKey,preferencesValue);
    return com.liferay.portal.model.LayoutReferenceSoap.toSoapModels(returnValue);
  }
 catch (  Exception e) {
