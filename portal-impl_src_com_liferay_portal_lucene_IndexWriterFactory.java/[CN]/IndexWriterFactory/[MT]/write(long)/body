{
  if (LuceneUtil.INDEX_READ_ONLY) {
    return;
  }
  IndexWriterData writerData=(IndexWriterData)_writerLookup.get(new Long(companyId));
  if (writerData != null) {
    decrement(writerData);
  }
 else {
    if (_log.isWarnEnabled()) {
      _log.warn("IndexWriterData not found for " + companyId);
    }
  }
}
