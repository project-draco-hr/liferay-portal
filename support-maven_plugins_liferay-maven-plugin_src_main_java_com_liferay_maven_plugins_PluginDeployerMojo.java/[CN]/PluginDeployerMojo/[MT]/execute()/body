{
  try {
    Log log=getLog();
    log.info("Deploying " + warFileName + " to "+ autoDeployDir.getAbsolutePath());
    FileUtils.copyFile(warFile,new File(autoDeployDir,warFileName));
  }
 catch (  IOException e) {
    throw new MojoExecutionException("Deploying " + warFileName + "failed",e);
  }
}
