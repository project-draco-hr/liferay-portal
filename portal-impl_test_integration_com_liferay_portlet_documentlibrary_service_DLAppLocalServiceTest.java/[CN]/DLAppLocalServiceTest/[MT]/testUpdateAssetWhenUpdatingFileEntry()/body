{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId());
  FileEntry fileEntry=DLAppLocalServiceUtil.addFileEntry(TestPropsValues.getUserId(),_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,RandomTestUtil.randomString(),ContentTypes.TEXT_PLAIN,"Old Title",RandomTestUtil.randomString(),null,RandomTestUtil.randomBytes(),serviceContext);
  DLAppLocalServiceUtil.updateFileEntry(TestPropsValues.getUserId(),fileEntry.getFileEntryId(),RandomTestUtil.randomString(),ContentTypes.TEXT_PLAIN,"New Title",RandomTestUtil.randomString(),null,true,RandomTestUtil.randomBytes(),serviceContext);
  AssetEntry assetEntry=AssetEntryLocalServiceUtil.getEntry(DLFileEntryConstants.getClassName(),fileEntry.getFileEntryId());
  Assert.assertEquals("New Title",assetEntry.getTitle());
}
