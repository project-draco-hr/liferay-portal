{
  final int[] messagesReceived={0};
  MessageBusUtil.registerMessageListener(DestinationNames.DOCUMENT_LIBRARY_SYNC_EVENT_PROCESSOR,new MessageListener(){
    @Override public void receive(    Message message) throws MessageListenerException {
      Object event=message.get("event");
      if (targetEvent.equals(event)) {
        messagesReceived[0]++;
      }
    }
  }
);
  return messagesReceived;
}
