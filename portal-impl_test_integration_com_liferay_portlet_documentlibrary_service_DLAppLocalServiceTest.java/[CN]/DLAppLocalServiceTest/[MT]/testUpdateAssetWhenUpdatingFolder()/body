{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId());
  Folder folder=addFolder(false,"Test Folder");
  DLAppLocalServiceUtil.updateFolder(folder.getFolderId(),folder.getParentFolderId(),"foo","bar",serviceContext);
  AssetEntry assetEntry=AssetEntryLocalServiceUtil.getEntry(DLFolderConstants.getClassName(),folder.getFolderId());
  Assert.assertEquals("foo",assetEntry.getTitle());
}
