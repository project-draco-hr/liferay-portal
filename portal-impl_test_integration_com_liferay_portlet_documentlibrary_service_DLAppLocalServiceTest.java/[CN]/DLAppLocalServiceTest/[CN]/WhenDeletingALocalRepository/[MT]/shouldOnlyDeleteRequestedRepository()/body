{
  LocalRepository localRepository=RepositoryLocalServiceUtil.getLocalRepositoryImpl(_repository.getRepositoryId());
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId());
  FileEntry fileEntry=localRepository.addFileEntry(serviceContext.getUserId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,RandomTestUtil.randomString(),ContentTypes.APPLICATION_TEXT,RandomTestUtil.randomString(),RandomTestUtil.randomString(),RandomTestUtil.randomString(),new ByteArrayInputStream(new byte[0]),0,serviceContext);
  DLAppLocalServiceUtil.deleteAll(_group.getGroupId());
  Assert.assertNotNull(localRepository.getFileEntry(fileEntry.getFileEntryId()));
}
