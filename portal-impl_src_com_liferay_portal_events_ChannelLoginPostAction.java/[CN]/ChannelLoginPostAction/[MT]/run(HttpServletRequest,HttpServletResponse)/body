{
  try {
    User user=PortalUtil.getUser(request);
    if (!user.isDefaultUser()) {
      if (_log.isDebugEnabled()) {
        _log.debug("Creating channel " + user.getUserId());
      }
      ChannelHubManagerUtil.createChannel(user.getCompanyId(),user.getUserId());
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
