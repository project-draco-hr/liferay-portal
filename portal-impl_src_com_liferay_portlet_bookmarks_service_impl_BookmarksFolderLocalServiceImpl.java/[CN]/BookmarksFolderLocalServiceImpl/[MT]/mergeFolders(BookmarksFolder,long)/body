{
  Iterator<BookmarksFolder> foldersItr=bookmarksFolderPersistence.findByG_P(fromFolder.getGroupId(),fromFolder.getFolderId()).iterator();
  while (foldersItr.hasNext()) {
    BookmarksFolder folder=foldersItr.next();
    mergeFolders(folder,toFolderId);
  }
  Iterator<BookmarksEntry> entriesItr=bookmarksEntryPersistence.findByFolderId(fromFolder.getFolderId()).iterator();
  while (entriesItr.hasNext()) {
    BookmarksEntry entry=entriesItr.next();
    entry.setFolderId(toFolderId);
    bookmarksEntryPersistence.update(entry);
  }
  bookmarksFolderPersistence.remove(fromFolder.getFolderId());
}
