{
  Iterator itr=bookmarksFolderPersistence.findByG_P(fromFolder.getGroupId(),fromFolder.getFolderId()).iterator();
  while (itr.hasNext()) {
    BookmarksFolder folder=(BookmarksFolder)itr.next();
    mergeFolders(folder,toFolderId);
  }
  itr=bookmarksEntryPersistence.findByFolderId(fromFolder.getFolderId()).iterator();
  while (itr.hasNext()) {
    BookmarksEntry entry=(BookmarksEntry)itr.next();
    entry.setFolderId(toFolderId);
    bookmarksEntryPersistence.update(entry);
  }
  bookmarksFolderPersistence.remove(fromFolder.getFolderId());
}
