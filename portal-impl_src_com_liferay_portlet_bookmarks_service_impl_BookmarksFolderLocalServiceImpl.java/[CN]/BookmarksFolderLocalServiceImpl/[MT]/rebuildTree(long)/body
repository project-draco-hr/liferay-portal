{
  TreePathUtil.rebuildTree(companyId,BookmarksFolderConstants.DEFAULT_PARENT_FOLDER_ID,new TreeModelFinder<BookmarksFolder>(){
    @Override public List<BookmarksFolder> findTreeModels(    long previousId,    long companyId,    long parentId,    int size) throws SystemException {
      return bookmarksFolderPersistence.findByF_C_P_NotS(previousId,companyId,parentId,WorkflowConstants.STATUS_IN_TRASH,QueryUtil.ALL_POS,size,new FolderIdComparator());
    }
  }
);
}
