{
  String name=RandomTestUtil.randomString();
  if (deleteExisting) {
    try {
      DLFolder folder=DLFolderLocalServiceUtil.getFolder(groupId,DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,name);
      DLFolderLocalServiceUtil.deleteFolder(folder.getFolderId());
    }
 catch (    NoSuchFolderException nsfe) {
    }
  }
  return DLFolderLocalServiceUtil.addFolder(TestPropsValues.getUserId(),groupId,groupId,false,DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,name,StringPool.BLANK,false,serviceContext);
}
