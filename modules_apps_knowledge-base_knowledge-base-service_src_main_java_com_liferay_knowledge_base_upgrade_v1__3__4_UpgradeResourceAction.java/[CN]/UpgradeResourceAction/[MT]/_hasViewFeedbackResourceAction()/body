{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select count(*) from ResourceAction where actionId = ?");
    ps.setString(1,_ACTION_ID_VIEW_KB_FEEDBACK);
    rs=ps.executeQuery();
    if (rs.next()) {
      if (rs.getInt(1) > 0) {
        return true;
      }
      return false;
    }
    return false;
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
