{
synchronized (this) {
    boolean useQueryCache=GetterUtil.getBoolean(properties.get(PropsKeys.HIBERNATE_CACHE_USE_QUERY_CACHE));
    boolean useSecondLevelCache=GetterUtil.getBoolean(properties.get(PropsKeys.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE));
    if (useQueryCache || useSecondLevelCache) {
      _enabled=true;
    }
    _init(properties);
  }
}
