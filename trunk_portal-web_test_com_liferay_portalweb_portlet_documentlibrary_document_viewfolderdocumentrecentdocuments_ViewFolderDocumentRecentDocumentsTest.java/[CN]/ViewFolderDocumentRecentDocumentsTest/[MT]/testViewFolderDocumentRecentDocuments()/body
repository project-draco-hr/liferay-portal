{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Document Library Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Document Library Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Recent Documents",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Test1 Document1.txt This is test1 document1."),selenium.getText("//td[1]/a"));
  assertTrue(selenium.isElementPresent("//td[2]/a"));
  assertEquals(RuntimeVariables.replace("0"),selenium.getText("//td[3]/a"));
  assertEquals(RuntimeVariables.replace("No"),selenium.getText("//td[4]/a"));
}
