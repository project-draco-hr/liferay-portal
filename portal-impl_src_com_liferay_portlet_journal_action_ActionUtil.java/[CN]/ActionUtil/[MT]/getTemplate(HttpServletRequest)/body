{
  long groupId=ParamUtil.getLong(request,"groupId");
  String templateId=ParamUtil.getString(request,"templateId");
  DDMTemplate ddmTemplate=null;
  if (Validator.isNotNull(templateId)) {
    ddmTemplate=DDMTemplateServiceUtil.getTemplate(groupId,PortalUtil.getClassNameId(DDMStructure.class),templateId,true);
  }
  request.setAttribute(WebKeys.JOURNAL_TEMPLATE,ddmTemplate);
}
