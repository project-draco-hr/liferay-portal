{
  long groupId=ParamUtil.getLong(request,"groupId");
  String ddmTemplateKey=ParamUtil.getString(request,"templateId");
  DDMTemplate ddmTemplate=null;
  if (Validator.isNotNull(ddmTemplateKey)) {
    ddmTemplate=DDMTemplateServiceUtil.getTemplate(groupId,PortalUtil.getClassNameId(DDMStructure.class),ddmTemplateKey,true);
  }
  request.setAttribute(WebKeys.JOURNAL_TEMPLATE,ddmTemplate);
}
