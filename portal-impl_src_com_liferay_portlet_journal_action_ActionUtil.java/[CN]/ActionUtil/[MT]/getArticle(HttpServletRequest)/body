{
  long groupId=ParamUtil.getLong(request,"groupId");
  String articleId=ParamUtil.getString(request,"articleId");
  double version=ParamUtil.getDouble(request,"version",JournalArticleConstants.DEFAULT_VERSION);
  JournalArticle article=null;
  if (Validator.isNotNull(articleId)) {
    article=JournalArticleServiceUtil.getArticle(groupId,articleId,version);
  }
  request.setAttribute(WebKeys.JOURNAL_ARTICLE,article);
}
