{
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  List<JournalArticle> articles=new ArrayList<>();
  String[] articleIds=StringUtil.split(ParamUtil.getString(request,"articleIds"));
  for (  String articleId : articleIds) {
    try {
      JournalArticle article=JournalArticleServiceUtil.getArticle(themeDisplay.getScopeGroupId(),articleId);
      articles.add(article);
    }
 catch (    NoSuchArticleException nsfee) {
    }
  }
  return articles;
}
