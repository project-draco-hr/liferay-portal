{
  StringBundler sb=new StringBundler(8);
  sb.append("<?xml version=\"1.0\" encoding=\"UTF-8\"?>");
  sb.append("<root default-locale=\"");
  sb.append(LocaleUtil.getDefault());
  sb.append("\"><Name language-id=\"");
  sb.append(LocaleUtil.getDefault());
  sb.append("\">");
  sb.append(str);
  sb.append("</Name></root>");
  return sb.toString();
}
