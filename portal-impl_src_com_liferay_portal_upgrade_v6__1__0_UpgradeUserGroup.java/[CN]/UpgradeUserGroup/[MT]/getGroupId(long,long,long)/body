{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  long groupId=0;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select groupId from Group_ where (companyId = ?) and " + "(classNameId = ?) and (classPK = ?)");
    ps.setLong(1,companyId);
    ps.setLong(2,classNameId);
    ps.setLong(3,classPK);
    rs=ps.executeQuery();
    if (rs.next()) {
      groupId=rs.getLong("groupId");
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return groupId;
}
