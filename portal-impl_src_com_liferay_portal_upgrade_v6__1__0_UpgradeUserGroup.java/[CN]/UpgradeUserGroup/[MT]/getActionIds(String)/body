{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select sum(bitwiseValue) as actionIds from ResourceAction " + "where name = ?");
    ps.setString(1,name);
    rs=ps.executeQuery();
    if (rs.next()) {
      return rs.getLong("actionIds");
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return 0;
}
