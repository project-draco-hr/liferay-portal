{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("insert into LayoutSetPrototype (layoutSetPrototypeId, " + "companyId, name, description, active_) values (?, ?, ?, " + "?, ?)");
    ps.setLong(1,layoutSetPrototypeId);
    ps.setLong(2,companyId);
    ps.setString(3,getNameXML(name));
    ps.setString(4,name);
    ps.setBoolean(5,true);
    ps.execute();
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
