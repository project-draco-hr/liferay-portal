{
  Session session=null;
  try {
    session=openSession();
    DLSync dlSync=(DLSync)session.get(DLSyncImpl.class,Long.valueOf(syncId));
    if (dlSync == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + syncId);
      }
      throw new NoSuchSyncException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + syncId);
    }
    return dlSyncPersistence.remove(dlSync);
  }
 catch (  NoSuchSyncException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
