{
  DLSync dlSync=(DLSync)EntityCacheUtil.getResult(DLSyncModelImpl.ENTITY_CACHE_ENABLED,DLSyncImpl.class,syncId);
  if (dlSync == _nullDLSync) {
    return null;
  }
  if (dlSync == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      dlSync=(DLSync)session.get(DLSyncImpl.class,Long.valueOf(syncId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (dlSync != null) {
        cacheResult(dlSync);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(DLSyncModelImpl.ENTITY_CACHE_ENABLED,DLSyncImpl.class,syncId,_nullDLSync);
      }
      closeSession(session);
    }
  }
  return dlSync;
}
