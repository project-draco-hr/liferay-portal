{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Test Workflow Page");
  selenium.clickAt("link=Test Workflow Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("_145_addApplication",RuntimeVariables.replace(""));
  Thread.sleep(5000);
  selenium.waitForVisible("layout_configuration_content");
  selenium.sendKeys("layout_configuration_content",RuntimeVariables.replace("t"));
  selenium.waitForElementPresent("//span[@data-title='Test Workflow']");
  selenium.makeVisible("//span[@data-title='Test Workflow']");
  selenium.clickAt("//span[@data-title='Test Workflow']",RuntimeVariables.replace("Add"));
  selenium.waitForVisible("//section");
  assertTrue(selenium.isVisible("//section"));
}
