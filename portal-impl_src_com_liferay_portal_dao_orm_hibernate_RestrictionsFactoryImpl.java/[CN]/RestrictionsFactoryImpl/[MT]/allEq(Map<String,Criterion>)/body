{
  String[] arguments=new String[]{"-ALLEQ-"};
  for (  Criterion criterion : propertyNameValues.values()) {
    CriterionImpl criterionImpl=(CriterionImpl)criterion;
    arguments=ArrayUtil.append(arguments,criterionImpl.getArguments());
  }
  return new CriterionImpl(org.hibernate.criterion.Restrictions.allEq(propertyNameValues),arguments);
}
