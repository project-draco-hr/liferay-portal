{
  Element rootElement=document.getRootElement();
  Element summaryElement=rootElement.addElement("summary");
  for (  String modelClassName : manifestSummary.getModelNames()) {
    Element element=summaryElement.addElement("staged-model");
    element.addAttribute("class-name",modelClassName);
    long modelAdditionCount=manifestSummary.getModelAdditionCount(modelClassName);
    if (modelAdditionCount > 0) {
      element.addAttribute("addition-count",String.valueOf(modelAdditionCount));
    }
    long modelDeletionCount=manifestSummary.getModelDeletionCount(modelClassName);
    if (modelDeletionCount > 0) {
      element.addAttribute("deletion-count",String.valueOf(modelDeletionCount));
    }
  }
}
