{
  content=ExportImportUtil.replaceImportLayoutReferences(portletDataContext,content,importReferencedContent);
  content=ExportImportUtil.replaceImportLinksToLayouts(portletDataContext,content,importReferencedContent);
  content=ExportImportUtil.replaceImportDLReferences(portletDataContext,entityElement,content,importReferencedContent);
  return content;
}
