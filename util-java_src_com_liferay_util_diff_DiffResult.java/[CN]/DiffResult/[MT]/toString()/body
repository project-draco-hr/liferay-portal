{
  StringMaker sm=new StringMaker();
  sm.append("Type: ");
  sm.append(_changeType);
  sm.append("\n");
  sm.append("Line: ");
  sm.append(_lineNumber);
  sm.append("\n");
  Iterator itr=_changedLines.iterator();
  while (itr.hasNext()) {
    sm.append(itr.next());
    if (itr.hasNext()) {
      sm.append("\n");
    }
  }
  return sm.toString();
}
