{
  try {
    Path filePath=Paths.get(filePathName);
    FileUtils.deleteDirectory(filePath.toFile());
    syncAccount=SyncAccountService.fetchSyncAccount(syncAccount.getSyncAccountId());
    if (syncAccount != null) {
      SyncAccountService.deleteSyncAccount(syncAccount.getSyncAccountId());
    }
  }
 catch (  Exception e) {
    _logger.error(e.getMessage(),e);
  }
 finally {
    Path databaseFilePath=FileUtil.getFilePath(PropsValues.SYNC_CONFIGURATION_DIRECTORY,PropsValues.SYNC_DATABASE_NAME + ".h2.db");
    Files.deleteIfExists(databaseFilePath);
  }
}
