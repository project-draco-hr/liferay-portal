{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),PortletKeys.DOCUMENT_LIBRARY,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/document_library/edit_folder");
  portletURL.setParameter("folderId",String.valueOf(_folder.getFolderId()));
  return portletURL;
}
