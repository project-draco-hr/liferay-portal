{
  try {
    if (_folder.isMountPoint()) {
      return "icon-drive";
    }
    DLAppServiceUtil.getFolder(_folder.getFolderId());
  }
 catch (  NoSuchFolderException re) {
    return "icon-remove";
  }
catch (  PrincipalException pe) {
    return "icon-remove";
  }
  return super.getIconCssClass();
}
