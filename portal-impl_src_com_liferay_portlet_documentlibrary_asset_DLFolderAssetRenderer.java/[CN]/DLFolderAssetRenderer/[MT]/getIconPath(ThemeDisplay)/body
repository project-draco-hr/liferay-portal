{
  try {
    if (PropsValues.DL_FOLDER_ICON_CHECK_COUNT && DLAppServiceUtil.getFoldersAndFileEntriesAndFileShortcutsCount(_folder.getRepositoryId(),_folder.getFolderId(),WorkflowConstants.STATUS_APPROVED,true) > 0) {
      return themeDisplay.getPathThemeImages() + "/common/folder_full_document.png";
    }
  }
 catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      _log.debug(e,e);
    }
  }
  return themeDisplay.getPathThemeImages() + "/common/folder_empty.png";
}
