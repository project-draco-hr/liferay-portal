{
  String workflowDefinitionTitle1=workflowDefinition1.getTitle(_languageId);
  String workflowDefinitionTitle2=workflowDefinition2.getTitle(_languageId);
  int value=workflowDefinitionTitle1.compareTo(workflowDefinitionTitle2);
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
