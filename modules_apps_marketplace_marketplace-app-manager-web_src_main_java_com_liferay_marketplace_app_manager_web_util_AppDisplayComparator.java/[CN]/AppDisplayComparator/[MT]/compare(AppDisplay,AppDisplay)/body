{
  if (appDisplay1.hasModuleGroups() && !appDisplay2.hasModuleGroups()) {
    return -1;
  }
 else   if (!appDisplay1.hasModuleGroups() && appDisplay2.hasModuleGroups()) {
    return 1;
  }
  int value=appDisplay1.compareTo(appDisplay2);
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
