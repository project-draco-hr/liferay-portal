{
  long lastModified=-1;
  for (int i=0; (lastModified == -1) && (i < _templateLoaders.length); i++) {
    if (_templateLoaders[i] != null) {
      lastModified=_templateLoaders[i].getLastModified(templateSource);
    }
  }
  if (lastModified == -1) {
    return 0;
  }
 else {
    return lastModified;
  }
}
