{
  long lastModified=0;
  for (  FreeMarkerTemplateLoader freeMarkerTemplateLoader : _freeMarkerTemplateLoaders) {
    long curLastModified=freeMarkerTemplateLoader.getLastModified(templateSource);
    if (curLastModified > lastModified) {
      lastModified=curLastModified;
    }
  }
  return lastModified;
}
