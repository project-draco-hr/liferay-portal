{
  _templateLoaders=new FreeMarkerTemplateLoader[loaders.length];
  for (int i=0; i < loaders.length; i++) {
    try {
      _templateLoaders[i]=(FreeMarkerTemplateLoader)Class.forName(loaders[i]).newInstance();
    }
 catch (    Exception ex) {
      _log.error(ex);
      _templateLoaders[i]=null;
    }
  }
}
