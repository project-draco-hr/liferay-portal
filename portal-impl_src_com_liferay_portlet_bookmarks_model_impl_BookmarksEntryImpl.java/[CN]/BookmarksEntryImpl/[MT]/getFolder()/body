{
  BookmarksFolder folder=null;
  if (getFolderId() > 0) {
    try {
      folder=BookmarksFolderLocalServiceUtil.getFolder(getFolderId());
    }
 catch (    Exception e) {
      folder=new BookmarksFolderImpl();
      _log.error(e);
    }
  }
 else {
    folder=new BookmarksFolderImpl();
  }
  return folder;
}
