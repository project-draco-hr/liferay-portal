{
  try {
    ShoppingCouponLocalService shoppingCouponLocalService=ShoppingCouponLocalServiceFactory.getService();
    return shoppingCouponLocalService.searchCount(couponId,groupId,companyId,active,discountType,andOperator);
  }
 catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
