{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalArticle.class,JournalArticle.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id",RandomTestUtil.nextLong()));
  List<JournalArticle> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
