{
  DLSyncEvent dlSyncEvent=findByPrimaryKey(syncEventId);
  Session session=null;
  try {
    session=openSession();
    DLSyncEvent[] array=new DLSyncEventImpl[3];
    array[0]=getByModifiedDate_PrevAndNext(session,dlSyncEvent,modifiedDate,orderByComparator,true);
    array[1]=dlSyncEvent;
    array[2]=getByModifiedDate_PrevAndNext(session,dlSyncEvent,modifiedDate,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
