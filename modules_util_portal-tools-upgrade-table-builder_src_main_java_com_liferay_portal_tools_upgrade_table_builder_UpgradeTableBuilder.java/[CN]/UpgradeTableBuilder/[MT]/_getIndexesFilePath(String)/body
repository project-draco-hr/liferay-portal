{
  Path indexesFilePath=null;
  if (_osgiModule) {
    List<Path> paths=_findFiles(_baseDirName,"**/sql/indexes.sql",1);
    if (!paths.isEmpty()) {
      indexesFilePath=paths.get(0);
    }
  }
 else {
    indexesFilePath=Paths.get(_upgradeTableDirName,upgradeFileVersion,"indexes.sql");
    if (Files.notExists(indexesFilePath)) {
      indexesFilePath=Paths.get(_baseDirName,"../sql/indexes.sql");
    }
  }
  return indexesFilePath;
}
