{
  Map<String,String> arguments=ArgumentsUtil.parseArguments(args);
  String baseDirName=GetterUtil.getString(arguments.get("upgrade.base.dir"),UpgradeTableBuilderArgs.BASE_DIR_NAME);
  boolean osgiModule=GetterUtil.getBoolean(arguments.get("upgrade.osgi.module"),UpgradeTableBuilderArgs.OSGI_MODULE);
  String releaseInfoFileName=arguments.get("upgrade.release.info.file");
  String upgradeTableDirName=arguments.get("upgrade.table.dir");
  try {
    new UpgradeTableBuilder(baseDirName,osgiModule,releaseInfoFileName,upgradeTableDirName);
  }
 catch (  Exception e) {
    ArgumentsUtil.processMainException(arguments,e);
  }
}
