{
  Dictionary<String,Object> properties=new HashMapDictionary<>();
  properties.put(BACKGROUND_TASK_EXECUTOR_REGISTRY_KEY,backgroundTaskExecutor);
  ServiceRegistration<BackgroundTaskExecutor> serviceRegistration=_bundleContext.registerService(BackgroundTaskExecutor.class,backgroundTaskExecutor,properties);
  _backgroundTaskExecutorRegistrations.put(backgroundTaskExecutorClassName,serviceRegistration);
}
