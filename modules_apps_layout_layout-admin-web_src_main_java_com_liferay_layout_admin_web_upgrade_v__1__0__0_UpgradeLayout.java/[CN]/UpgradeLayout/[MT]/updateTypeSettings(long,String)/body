{
  Connection con=null;
  PreparedStatement ps=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("update Layout set typeSettings = ? where plid = ?");
    ps.setString(1,typeSettings);
    ps.setLong(2,plid);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
}
