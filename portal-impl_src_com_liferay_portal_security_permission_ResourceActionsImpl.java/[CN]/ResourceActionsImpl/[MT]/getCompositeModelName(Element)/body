{
  StringBundler sb=new StringBundler();
  List<Element> elements=new ArrayList<>(compositeModelNameElement.elements("model-name"));
  Collections.sort(elements,new Comparator<Element>(){
    @Override public int compare(    Element element1,    Element element2){
      String textTrim1=GetterUtil.getString(element1.getTextTrim());
      String textTrim2=GetterUtil.getString(element2.getTextTrim());
      return textTrim1.compareTo(textTrim2);
    }
  }
);
  Iterator<Element> itr=elements.iterator();
  while (itr.hasNext()) {
    Element modelNameElement=itr.next();
    sb.append(modelNameElement.getTextTrim());
    if (itr.hasNext()) {
      sb.append(_COMPOSITE_MODEL_NAME_SEPARATOR);
    }
  }
  return sb.toString();
}
