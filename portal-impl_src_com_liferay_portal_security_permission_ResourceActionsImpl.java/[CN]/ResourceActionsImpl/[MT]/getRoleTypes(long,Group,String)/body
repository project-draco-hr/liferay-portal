{
  int[] types=RoleConstants.TYPES_R_S;
  if (isPortalModelResource(modelResource)) {
    if (modelResource.equals(Organization.class.getName()) || modelResource.equals(User.class.getName())) {
      types=RoleConstants.TYPES_R_O;
    }
 else {
      types=RoleConstants.TYPES_R;
    }
  }
 else {
    if (group != null) {
      if (group.isLayout()) {
        try {
          group=GroupServiceUtil.getGroup(group.getParentGroupId());
        }
 catch (        Exception e) {
        }
      }
      if (group.isOrganization()) {
        types=RoleConstants.TYPES_R_O_S;
      }
 else       if (group.isUser()) {
        types=RoleConstants.TYPES_R;
      }
    }
  }
  return types;
}
