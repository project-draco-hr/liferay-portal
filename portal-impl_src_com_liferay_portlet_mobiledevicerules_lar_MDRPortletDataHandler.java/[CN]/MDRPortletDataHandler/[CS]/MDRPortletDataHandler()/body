{
  setDeletionEventClassNames(MDRAction.class.getName(),MDRRuleGroup.class.getName(),MDRRuleGroupInstance.class.getName());
  setExportControls(new PortletDataHandlerBoolean(NAMESPACE,"rules",true,false,null,MDRRule.class.getName()),new PortletDataHandlerBoolean(NAMESPACE,"actions",true,false,null,MDRAction.class.getName()));
  setImportControls(getExportControls());
  setPublishToLiveByDefault(true);
}
