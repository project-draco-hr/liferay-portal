{
  try {
    DLService dlService=DLServiceFactory.getService();
    return dlService.getFile(companyId,repositoryId,fileName,versionNumber);
  }
 catch (  PortalException pe) {
    throw pe;
  }
catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new SystemException(e);
  }
}
