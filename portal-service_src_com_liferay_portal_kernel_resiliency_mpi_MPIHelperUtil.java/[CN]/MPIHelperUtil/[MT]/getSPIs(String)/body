{
  List<SPI> spis=new ArrayList<SPI>();
  for (  Map.Entry<SPIKey,SPI> entry : _spis.entrySet()) {
    SPIKey spiKey=entry.getKey();
    if (!spiProviderName.equals(spiKey._spiProviderName)) {
      continue;
    }
    SPI spi=entry.getValue();
    spi=_checkSPILiveness(spi);
    if (spi != null) {
      spis.add(spi);
    }
  }
  return spis;
}
