{
  List<SPI> spis=new ArrayList<SPI>();
  for (  SPIProviderContainer spiProviderContainer : _spiProviderContainers.values()) {
    for (    SPI spi : spiProviderContainer.getSPIs()) {
      spi=_checkSPILiveness(spi);
      if (spi != null) {
        spis.add(spi);
      }
    }
  }
  return spis;
}
