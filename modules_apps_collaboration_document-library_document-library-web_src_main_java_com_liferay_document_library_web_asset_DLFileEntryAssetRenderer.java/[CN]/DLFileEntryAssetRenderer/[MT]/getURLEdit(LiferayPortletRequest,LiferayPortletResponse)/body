{
  Group group=GroupLocalServiceUtil.fetchGroup(_fileEntry.getGroupId());
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(liferayPortletRequest,group,DLPortletKeys.DOCUMENT_LIBRARY,0,0,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcRenderCommandName","/document_library/edit_file_entry");
  portletURL.setParameter("fileEntryId",String.valueOf(_fileEntry.getFileEntryId()));
  return portletURL;
}
