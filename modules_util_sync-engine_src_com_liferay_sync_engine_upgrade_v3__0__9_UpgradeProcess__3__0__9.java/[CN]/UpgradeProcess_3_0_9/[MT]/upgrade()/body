{
  SyncAccountPersistence syncAccountPersistence=SyncAccountService.getSyncAccountPersistence();
  syncAccountPersistence.executeRaw("ALTER TABLE `SyncAccount` ADD COLUMN batchFileMaxSize " + "VARCHAR(16777216) BEFORE filePathName;");
  syncAccountPersistence.executeRaw("ALTER TABLE `SyncFile` ADD COLUMN previousModifiedTime LONG " + "BEFORE repositoryId;");
}
