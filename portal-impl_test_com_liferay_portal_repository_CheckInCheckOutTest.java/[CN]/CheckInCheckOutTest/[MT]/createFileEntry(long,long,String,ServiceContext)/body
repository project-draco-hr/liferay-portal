{
  InputStream inputStream=new UnsyncByteArrayInputStream(_TEST_CONTENT.getBytes());
  FileEntry fileEntry=DLAppServiceUtil.addFileEntry(repositoryId,folderId,fileName,ContentTypes.TEXT_PLAIN,fileName,null,null,inputStream,_TEST_CONTENT.length(),serviceContext);
  assertNotNull(fileEntry);
  assertEquals("1.0",fileEntry.getVersion());
  AssetEntry assetEntry=getAssetEntry(fileEntry.getFileEntryId(),true);
  assertNotNull(assetEntry);
  return fileEntry;
}
