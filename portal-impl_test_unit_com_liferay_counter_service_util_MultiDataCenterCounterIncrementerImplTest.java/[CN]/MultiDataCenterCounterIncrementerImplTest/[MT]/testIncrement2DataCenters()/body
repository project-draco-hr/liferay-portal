{
  long value=Long.MAX_VALUE;
  MultiDataCenterCounterIncrementer multiDataCenterCounterIncrementer=new MultiDataCenterCounterIncrementerImpl();
  multiDataCenterCounterIncrementer.initialize(2,0);
  Assert.assertEquals(0l,multiDataCenterCounterIncrementer.getMultiClusterSafeValue(0));
  Assert.assertEquals(4611686018427387903l,multiDataCenterCounterIncrementer.getMultiClusterSafeValue(value));
  multiDataCenterCounterIncrementer.initialize(2,1);
  Assert.assertEquals(4611686018427387904l,multiDataCenterCounterIncrementer.getMultiClusterSafeValue(0));
  Assert.assertEquals(9223372036854775807l,multiDataCenterCounterIncrementer.getMultiClusterSafeValue(value));
}
