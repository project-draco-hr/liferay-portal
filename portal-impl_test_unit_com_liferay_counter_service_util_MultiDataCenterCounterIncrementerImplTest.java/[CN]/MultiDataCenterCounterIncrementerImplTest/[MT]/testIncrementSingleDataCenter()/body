{
  long value=Long.MAX_VALUE;
  MultiDataCenterCounterIncrementer multiDataCenterCounterIncrementer=new MultiDataCenterCounterIncrementerImpl();
  multiDataCenterCounterIncrementer.initialize(1,0);
  Assert.assertEquals(0,multiDataCenterCounterIncrementer.getMultiClusterSafeValue(0));
  Assert.assertEquals(value,multiDataCenterCounterIncrementer.getMultiClusterSafeValue(value));
}
