{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long portletPreferencesId=AssetPublisherSubscriptionUtil.getPortletPreferencesId(themeDisplay.getPlid(),themeDisplay.getPortletDisplay().getInstanceId());
  AssetPublisherSubscriptionUtil.unsubscribe(themeDisplay.getUserId(),themeDisplay.getScopeGroupId(),portletPreferencesId);
}
