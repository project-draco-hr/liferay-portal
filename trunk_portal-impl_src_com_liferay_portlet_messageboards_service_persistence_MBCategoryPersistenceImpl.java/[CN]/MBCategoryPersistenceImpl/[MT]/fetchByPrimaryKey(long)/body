{
  MBCategory mbCategory=(MBCategory)EntityCacheUtil.getResult(MBCategoryModelImpl.ENTITY_CACHE_ENABLED,MBCategoryImpl.class,categoryId,this);
  if (mbCategory == null) {
    Session session=null;
    try {
      session=openSession();
      mbCategory=(MBCategory)session.get(MBCategoryImpl.class,Long.valueOf(categoryId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (mbCategory != null) {
        cacheResult(mbCategory);
      }
      closeSession(session);
    }
  }
  return mbCategory;
}
