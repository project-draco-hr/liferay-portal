{
  try {
    ResourceLocalService resourceLocalService=ResourceLocalServiceFactory.getService();
    resourceLocalService.addResources(companyId,groupId,userId,name,primKey,portletActions,addCommunityPermissions,addGuestPermissions);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
