{
  ExpandoTable newExpandoTable=addExpandoTable();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ExpandoTable.class,ExpandoTable.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("tableId",newExpandoTable.getTableId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  ExpandoTable existingExpandoTable=(ExpandoTable)result.get(0);
  assertEquals(existingExpandoTable,newExpandoTable);
}
