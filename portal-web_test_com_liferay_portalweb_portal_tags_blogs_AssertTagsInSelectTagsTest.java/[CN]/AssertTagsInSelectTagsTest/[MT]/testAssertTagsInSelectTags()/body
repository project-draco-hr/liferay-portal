}
assertEquals(RuntimeVariables.replace("Categorization"),selenium.getText("xPath=(//div[@class='lfr-panel-title'])[2]/span"));
selenium.clickAt("xPath=(//div[@class='lfr-panel-title'])[2]/span",RuntimeVariables.replace("Categorization"));
