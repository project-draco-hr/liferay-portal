{
  RecentLayoutRevision newRecentLayoutRevision=addRecentLayoutRevision();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(RecentLayoutRevision.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("recentLayoutRevisionId"));
  Object newRecentLayoutRevisionId=newRecentLayoutRevision.getRecentLayoutRevisionId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("recentLayoutRevisionId",new Object[]{newRecentLayoutRevisionId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  Object existingRecentLayoutRevisionId=result.get(0);
  Assert.assertEquals(existingRecentLayoutRevisionId,newRecentLayoutRevisionId);
}
