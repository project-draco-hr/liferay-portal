{
  RecentLayoutRevision newRecentLayoutRevision=addRecentLayoutRevision();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(RecentLayoutRevision.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("recentLayoutRevisionId",newRecentLayoutRevision.getRecentLayoutRevisionId()));
  List<RecentLayoutRevision> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  RecentLayoutRevision existingRecentLayoutRevision=result.get(0);
  Assert.assertEquals(existingRecentLayoutRevision,newRecentLayoutRevision);
}
