{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Wiki Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Configuration",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.select("_86_nodeId",RuntimeVariables.replace("label=Main"));
  selenium.select("_86_title",RuntimeVariables.replace("label=Front Page Child Page Test"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("You have successfully updated the setup."),selenium.getText("//div[@id='p_p_id_86_']/div/div"));
  assertEquals("Main",selenium.getSelectedLabel("_86_nodeId"));
  assertEquals("Front Page Child Page Test",selenium.getSelectedLabel("_86_title"));
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Wiki Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//h1[@class='page-title']","Front Page Child Page Test Edited"));
  assertEquals(RuntimeVariables.replace("(Redirected from Front Page Child Page Test)"),selenium.getText("//div[@class='page-redirect']"));
}
