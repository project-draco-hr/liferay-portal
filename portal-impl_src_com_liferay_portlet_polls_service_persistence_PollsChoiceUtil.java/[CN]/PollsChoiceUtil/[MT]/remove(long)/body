{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(choiceId));
  }
  com.liferay.portlet.polls.model.PollsChoice pollsChoice=getPersistence().remove(choiceId);
  if (listener != null) {
    listener.onAfterRemove(pollsChoice);
  }
  return pollsChoice;
}
