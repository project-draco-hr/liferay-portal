{
  StorageAdapterRegistry storageAdapterRegistry=StorageAdapterRegistryUtil.getStorageAdapterRegistry();
  StorageAdapter storageAdapter=storageAdapterRegistry.getStorageAdapter("testStorageAdapterImpl");
  Assert.assertNotNull(storageAdapter);
  Class<?> clazz=storageAdapter.getClass();
  Assert.assertEquals(TestStorageAdapterImpl.class.getName(),clazz.getName());
}
