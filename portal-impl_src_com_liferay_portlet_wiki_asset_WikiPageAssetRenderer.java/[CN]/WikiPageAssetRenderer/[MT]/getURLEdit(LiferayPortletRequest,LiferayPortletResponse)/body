{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(PortalUtil.getControlPanelPlid(liferayPortletRequest),PortletKeys.WIKI,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/wiki/edit_page");
  portletURL.setParameter("nodeId",String.valueOf(_page.getNodeId()));
  portletURL.setParameter("title",_page.getTitle());
  return portletURL;
}
