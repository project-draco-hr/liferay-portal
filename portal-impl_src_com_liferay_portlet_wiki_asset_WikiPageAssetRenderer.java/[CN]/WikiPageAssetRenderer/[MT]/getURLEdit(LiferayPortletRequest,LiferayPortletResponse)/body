{
  HttpServletRequest request=liferayPortletRequest.getHttpServletRequest();
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  PortletURL editPortletURL=PortletURLFactoryUtil.create(request,PortletKeys.WIKI,getControlPanelPlid(themeDisplay),PortletRequest.RENDER_PHASE);
  editPortletURL.setParameter("struts_action","/wiki/edit_page");
  editPortletURL.setParameter("nodeId",String.valueOf(_page.getNodeId()));
  editPortletURL.setParameter("title",_page.getTitle());
  return editPortletURL;
}
