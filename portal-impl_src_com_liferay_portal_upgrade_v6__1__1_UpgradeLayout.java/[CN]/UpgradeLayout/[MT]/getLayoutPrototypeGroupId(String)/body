{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select groupId from Group_ where classPK = (select " + "layoutPrototypeId from LayoutPrototype where uuid_ = ?)");
    ps.setString(1,layoutPrototypeUuid);
    rs=ps.executeQuery();
    while (rs.next()) {
      long groupId=rs.getLong("groupId");
      return groupId;
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return 0;
}
