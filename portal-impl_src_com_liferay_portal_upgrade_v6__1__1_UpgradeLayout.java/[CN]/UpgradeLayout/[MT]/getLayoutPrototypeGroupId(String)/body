{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select groupId from Group_ where classPK = (select " + "layoutPrototypeId from LayoutPrototype where uuid_ = ?)");
    ps.setString(1,layoutPrototypeUuid);
    rs=ps.executeQuery();
    while (rs.next()) {
      long groupId=rs.getLong("groupId");
      return groupId;
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
  return 0;
}
