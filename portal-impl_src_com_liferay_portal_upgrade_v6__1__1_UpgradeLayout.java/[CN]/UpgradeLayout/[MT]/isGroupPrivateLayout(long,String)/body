{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select count(*) from Layout where uuid_ = ? and groupId = ? " + "and privateLayout = ?");
    ps.setString(1,sourcePrototypeLayoutUuid);
    ps.setLong(2,groupId);
    ps.setBoolean(3,true);
    rs=ps.executeQuery();
    while (rs.next()) {
      int count=rs.getInt(1);
      if (count > 0) {
        return true;
      }
 else {
        return false;
      }
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
  return false;
}
