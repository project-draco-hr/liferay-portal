{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select count(*) from Layout where uuid_ = ? and groupId = ? " + "and privateLayout = ?");
    ps.setString(1,sourcePrototypeLayoutUuid);
    ps.setLong(2,groupId);
    ps.setBoolean(3,true);
    rs=ps.executeQuery();
    while (rs.next()) {
      int count=rs.getInt(1);
      if (count > 0) {
        return true;
      }
 else {
        return false;
      }
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return false;
}
