{
  try (PreparedStatement ps=connection.prepareStatement("update AssetVocabulary set settings_ = ? where vocabularyId " + "= ?")){
    ps.setString(1,settings);
    ps.setLong(2,vocabularyId);
    ps.executeUpdate();
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to update vocabulary " + vocabularyId,e);
    }
  }
}
