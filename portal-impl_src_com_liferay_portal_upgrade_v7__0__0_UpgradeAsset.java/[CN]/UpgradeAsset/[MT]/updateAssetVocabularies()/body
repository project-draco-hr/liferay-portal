{
  try (PreparedStatement ps=connection.prepareStatement("select vocabularyId, settings_ from AssetVocabulary");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long vocabularyId=rs.getLong("vocabularyId");
      String settings=rs.getString("settings_");
      updateAssetVocabulary(vocabularyId,upgradeVocabularySettings(settings));
    }
  }
 }
