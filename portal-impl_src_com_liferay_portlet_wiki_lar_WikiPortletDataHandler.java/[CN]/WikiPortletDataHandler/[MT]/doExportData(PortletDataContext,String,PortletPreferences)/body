{
  Element rootElement=addExportDataRootElement(portletDataContext);
  if (!portletDataContext.getBooleanParameter(NAMESPACE,"wiki-pages")) {
    return getExportDataRootElementString(rootElement);
  }
  portletDataContext.addPortletPermissions(WikiPermission.RESOURCE_NAME);
  rootElement.addAttribute("group-id",String.valueOf(portletDataContext.getScopeGroupId()));
  ActionableDynamicQuery nodeActionableDynamicQuery=new WikiNodeExportActionableDynamicQuery(portletDataContext);
  nodeActionableDynamicQuery.performActions();
  ActionableDynamicQuery pageActionableDynamicQuery=new WikiPageExportActionableDynamicQuery(portletDataContext);
  pageActionableDynamicQuery.performActions();
  return getExportDataRootElementString(rootElement);
}
