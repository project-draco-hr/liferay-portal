{
  ManifestSummary manifestSummary=portletDataContext.getManifestSummary();
  ActionableDynamicQuery nodeActionableDynamicQuery=new WikiNodeExportActionableDynamicQuery(portletDataContext);
  manifestSummary.addModelCount(WikiNode.class,nodeActionableDynamicQuery.performCount());
  ActionableDynamicQuery pageExportActionableDynamicQuery=getPageActionableDynamicQuery(portletDataContext);
  manifestSummary.addModelCount(WikiPage.class,pageExportActionableDynamicQuery.performCount());
}
