{
  Element dynamicContentElement=element.element("dynamic-content");
  long articleImageId=GetterUtil.getLong(dynamicContentElement.attributeValue("id"));
  JournalArticleImage articleImage=_journalArticleImageLocalService.fetchJournalArticleImage(articleImageId);
  if (articleImage == null) {
    return;
  }
  articleImage.setElName(name + StringPool.UNDERLINE + index);
  _journalArticleImageLocalService.updateJournalArticleImage(articleImage);
}
