{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(resourcePrimKey));
  }
  com.liferay.portlet.journal.model.JournalArticleResource journalArticleResource=getPersistence().remove(resourcePrimKey);
  if (listener != null) {
    listener.onAfterRemove(journalArticleResource);
  }
  return journalArticleResource;
}
