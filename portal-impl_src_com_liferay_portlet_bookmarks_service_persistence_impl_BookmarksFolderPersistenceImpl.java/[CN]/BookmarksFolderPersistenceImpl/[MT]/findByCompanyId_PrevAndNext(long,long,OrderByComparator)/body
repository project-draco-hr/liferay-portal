{
  BookmarksFolder bookmarksFolder=findByPrimaryKey(folderId);
  Session session=null;
  try {
    session=openSession();
    BookmarksFolder[] array=new BookmarksFolderImpl[3];
    array[0]=getByCompanyId_PrevAndNext(session,bookmarksFolder,companyId,orderByComparator,true);
    array[1]=bookmarksFolder;
    array[2]=getByCompanyId_PrevAndNext(session,bookmarksFolder,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
