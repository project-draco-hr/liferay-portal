{
  SearchContext searchContext=new SearchContext();
  searchContext.setSearchEngineId(SearchEngineUtil.GENERIC_ENGINE_ID);
  searchContext.setKeywords("+title:test*.jpg +userName:bar*");
  BooleanQuery searchQuery=RepositorySearchQueryBuilderUtil.getFullQuery(searchContext);
  String cmisQuery=CMISQueryBuilder.buildQuery(searchContext,searchQuery);
  assertWhereEquals("(cmis:name LIKE 'test%.jpg' AND cmis:createdBy LIKE 'bar%')",cmisQuery);
}
