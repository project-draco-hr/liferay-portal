{
  SearchContext searchContext=getSearchContext();
  searchContext.setKeywords("\"test document\"~10");
  BooleanQuery searchQuery=RepositorySearchQueryBuilderUtil.getFullQuery(searchContext);
  String cmisQuery=CMISQueryBuilder.buildQuery(searchContext,searchQuery);
  assertQueryEquals("(cmis:name = 'test document') OR " + "(cmis:createdBy = 'test document')",cmisQuery);
}
