{
  StringBundler mergedSB=null;
  for (  Map.Entry<DataKey,StringBundler> entry : _dataMap.entrySet()) {
    DataKey dataKey=entry.getKey();
    if (dataKey._webKey.equals(webKey)) {
      if (mergedSB == null) {
        mergedSB=entry.getValue();
      }
 else {
        mergedSB.append(entry.getValue());
      }
    }
  }
  return mergedSB;
}
