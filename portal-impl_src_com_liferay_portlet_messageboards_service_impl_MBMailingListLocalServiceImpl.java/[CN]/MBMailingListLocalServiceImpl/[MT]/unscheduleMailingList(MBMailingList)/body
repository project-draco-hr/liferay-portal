{
  String groupName=getSchedulerGroupName(mailingList);
  List<SchedulerRequest> schedulerRequests=SchedulerEngineUtil.getScheduledJobs(groupName);
  for (  SchedulerRequest schedulerRequest : schedulerRequests) {
    Trigger trigger=schedulerRequest.getTrigger();
    SchedulerEngineUtil.unschedule(trigger.getJobName(),trigger.getGroupName());
  }
}
