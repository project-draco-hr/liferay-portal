{
  String resourceName=Layout.class.getName();
  exportGroupRoles(layoutCache,companyId,groupId,resourceName,"community",rolesElement);
  exportUserRoles(layoutCache,companyId,groupId,resourceName,rolesElement);
  exportInheritedRoles(layoutCache,companyId,groupId,resourceName,"organization",rolesElement);
  exportInheritedRoles(layoutCache,companyId,groupId,resourceName,"user-group",rolesElement);
}
