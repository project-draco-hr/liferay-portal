{
  if (_allPermittedNodes == null) {
    WikiPortletInstanceSettings wikiPortletInstanceSettings=_wikiRequestHelper.getWikiPortletInstanceSettings();
    _allPermittedNodes=WikiUtil.getNodes(getAllNodes(),wikiPortletInstanceSettings.getHiddenNodes(),_wikiRequestHelper.getPermissionChecker());
  }
  return _allPermittedNodes;
}
