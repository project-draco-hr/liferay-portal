{
  _allNodes=WikiNodeServiceUtil.getNodes(_wikiRequestHelper.getScopeGroupId());
  _allNodeNames=WikiUtil.getNodeNames(_allNodes);
  WikiPortletInstanceSettings wikiPortletInstanceSettings=_wikiRequestHelper.getWikiPortletInstanceSettings();
  _visibleNodeNames=wikiPortletInstanceSettings.getVisibleNodes();
  if (ArrayUtil.isNotEmpty(_visibleNodeNames)) {
    _allNodes=WikiUtil.orderNodes(_allNodes,_visibleNodeNames);
  }
 else {
    _visibleNodeNames=_allNodeNames.toArray(new String[_allNodeNames.size()]);
  }
}
