{
  if (!InlineSQLHelperUtil.isEnabled()) {
    return findByFormStructureId_PrevAndNext(formStructureLinkId,formStructureId,orderByComparator);
  }
  FormsStructureEntryLink formsStructureEntryLink=findByPrimaryKey(formStructureLinkId);
  Session session=null;
  try {
    session=openSession();
    FormsStructureEntryLink[] array=new FormsStructureEntryLinkImpl[3];
    array[0]=filterGetByFormStructureId_PrevAndNext(session,formsStructureEntryLink,formStructureId,orderByComparator,true);
    array[1]=formsStructureEntryLink;
    array[2]=filterGetByFormStructureId_PrevAndNext(session,formsStructureEntryLink,formStructureId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
