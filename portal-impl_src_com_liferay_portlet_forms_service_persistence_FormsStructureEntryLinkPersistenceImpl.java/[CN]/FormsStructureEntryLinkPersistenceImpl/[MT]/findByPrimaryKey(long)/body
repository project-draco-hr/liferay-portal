{
  FormsStructureEntryLink formsStructureEntryLink=fetchByPrimaryKey(formStructureLinkId);
  if (formsStructureEntryLink == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + formStructureLinkId);
    }
    throw new NoSuchStructureEntryLinkException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + formStructureLinkId);
  }
  return formsStructureEntryLink;
}
