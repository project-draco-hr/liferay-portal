{
  FormsStructureEntryLink formsStructureEntryLink=findByPrimaryKey(formStructureLinkId);
  Session session=null;
  try {
    session=openSession();
    FormsStructureEntryLink[] array=new FormsStructureEntryLinkImpl[3];
    array[0]=getByFormStructureId_PrevAndNext(session,formsStructureEntryLink,formStructureId,orderByComparator,true);
    array[1]=formsStructureEntryLink;
    array[2]=getByFormStructureId_PrevAndNext(session,formsStructureEntryLink,formStructureId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
