{
  Session session=null;
  try {
    session=openSession();
    ShoppingCoupon shoppingCoupon=(ShoppingCoupon)session.get(ShoppingCouponImpl.class,couponId);
    if (shoppingCoupon == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No ShoppingCoupon exists with the primary key " + couponId);
      }
      throw new NoSuchCouponException("No ShoppingCoupon exists with the primary key " + couponId);
    }
    return remove(shoppingCoupon);
  }
 catch (  NoSuchCouponException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
