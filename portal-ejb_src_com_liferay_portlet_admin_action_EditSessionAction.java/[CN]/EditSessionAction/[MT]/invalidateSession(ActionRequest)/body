{
  String userTrackerId=ParamUtil.getString(req,"userTrackerId");
  HttpSession userSession=PortalSessionContext.get(userTrackerId);
  if (userSession != null) {
    try {
      if (!req.getPortletSession().getId().equals(userTrackerId)) {
        userSession.invalidate();
      }
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
}
