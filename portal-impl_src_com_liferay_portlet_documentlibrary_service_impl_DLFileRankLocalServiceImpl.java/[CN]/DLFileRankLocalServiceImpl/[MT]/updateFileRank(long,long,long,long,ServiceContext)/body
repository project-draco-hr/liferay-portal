{
  if (!PropsValues.DL_FILE_RANK_ENABLED) {
    return null;
  }
  DLFileRank fileRank=dlFileRankPersistence.fetchByC_U_F(companyId,userId,fileEntryId);
  if (fileRank != null) {
    fileRank.setCreateDate(serviceContext.getCreateDate(null));
    dlFileRankPersistence.update(fileRank,false);
  }
 else {
    fileRank=addFileRank(groupId,companyId,userId,fileEntryId,serviceContext);
  }
  return fileRank;
}
