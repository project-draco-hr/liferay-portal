{
  DLFileEntryMetadata fileEntryMetadata=mock(DLFileEntryMetadata.class);
  when(_dlFileEntryMetadataLocalService.getFileEntryMetadata(Matchers.anyLong(),Matchers.anyLong())).thenReturn(fileEntryMetadata);
  Fields fields=mock(Fields.class);
  when(_storageEngine.getFields(fileEntryMetadata.getDDMStorageId())).thenReturn(fields);
}
