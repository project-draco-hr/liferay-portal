{
  DLFileEntryMetadata dlFileEntryMetadata=mock(DLFileEntryMetadata.class);
  when(_dlFileEntryMetadataLocalService.getFileEntryMetadata(Matchers.anyLong(),Matchers.anyLong())).thenReturn(dlFileEntryMetadata);
  Fields fields=mock(Fields.class);
  when(_storageEngine.getFields(dlFileEntryMetadata.getDDMStorageId())).thenReturn(fields);
}
