{
  ShoppingItemPrice itemPrice=null;
  List itemPrices=item.getItemPrices();
  for (int i=0; i < itemPrices.size(); i++) {
    ShoppingItemPrice temp=(ShoppingItemPrice)itemPrices.get(i);
    int minQty=temp.getMinQuantity();
    int maxQty=temp.getMaxQuantity();
    if ((temp.getStatus() != ShoppingItemPrice.STATUS_INACTIVE) && (count >= minQty) && (count < maxQty || maxQty == 0)) {
      itemPrice=temp;
    }
  }
  if (itemPrice == null) {
    return ShoppingItemPriceUtil.create(StringPool.BLANK);
  }
  return itemPrice;
}
