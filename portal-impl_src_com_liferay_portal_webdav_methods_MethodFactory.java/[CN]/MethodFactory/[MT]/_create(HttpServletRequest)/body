{
  String method=request.getMethod();
  if (_log.isDebugEnabled()) {
    _log.debug("Get method " + method);
  }
  Method methodImpl=(Method)_methods.get(method.toUpperCase());
  if (methodImpl == null) {
    throw new WebDAVException("Method " + method + " is not implemented");
  }
 else {
    if (_log.isDebugEnabled()) {
      _log.debug("Method " + method + " is mapped to "+ methodImpl.getClass().getName());
    }
  }
  return methodImpl;
}
