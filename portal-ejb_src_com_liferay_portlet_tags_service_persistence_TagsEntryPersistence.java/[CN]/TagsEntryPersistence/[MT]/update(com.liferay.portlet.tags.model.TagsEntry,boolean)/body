{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(tagsEntry);
    }
 else {
      if (tagsEntry.isNew()) {
        session.save(tagsEntry);
      }
    }
    session.flush();
    tagsEntry.setNew(false);
    return tagsEntry;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
