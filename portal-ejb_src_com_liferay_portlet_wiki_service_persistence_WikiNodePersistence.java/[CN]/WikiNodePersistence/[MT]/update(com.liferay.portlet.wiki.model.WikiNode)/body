{
  Session session=null;
  try {
    session=openSession();
    if (wikiNode.isNew()) {
      session.save(wikiNode);
    }
    session.flush();
    wikiNode.setNew(false);
    return wikiNode;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
