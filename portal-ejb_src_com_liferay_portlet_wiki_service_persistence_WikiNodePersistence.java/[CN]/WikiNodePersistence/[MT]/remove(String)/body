{
  Session session=null;
  try {
    session=openSession();
    WikiNode wikiNode=(WikiNode)session.get(WikiNodeImpl.class,nodeId);
    if (wikiNode == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No WikiNode exists with the primary key " + nodeId);
      }
      throw new NoSuchNodeException("No WikiNode exists with the primary key " + nodeId);
    }
    return remove(wikiNode);
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
