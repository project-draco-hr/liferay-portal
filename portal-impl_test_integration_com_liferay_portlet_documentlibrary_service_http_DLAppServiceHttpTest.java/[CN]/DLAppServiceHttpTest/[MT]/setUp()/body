{
  _group=GroupTestUtil.addGroup();
  String name="Test Folder";
  String description="This is a test folder.";
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  serviceContext.setScopeGroupId(_group.getGroupId());
  try {
    DLAppServiceHttp.deleteFolder(TestPropsValues.getHttpPrincipal(),_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,name);
  }
 catch (  Exception e) {
  }
  _folder=DLAppServiceHttp.addFolder(TestPropsValues.getHttpPrincipal(),_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,name,description,serviceContext);
}
