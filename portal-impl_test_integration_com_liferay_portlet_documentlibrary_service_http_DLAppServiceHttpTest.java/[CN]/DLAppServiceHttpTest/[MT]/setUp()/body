{
  _group=GroupTestUtil.addGroup();
  String name="Test Folder";
  String description="This is a test folder.";
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId());
  try {
    DLAppServiceHttp.deleteFolder(HttpPrincipalTestUtil.getHttpPrincipal(),_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,name);
  }
 catch (  Exception e) {
  }
  _folder=DLAppServiceHttp.addFolder(HttpPrincipalTestUtil.getHttpPrincipal(),_group.getGroupId(),DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,name,description,serviceContext);
}
