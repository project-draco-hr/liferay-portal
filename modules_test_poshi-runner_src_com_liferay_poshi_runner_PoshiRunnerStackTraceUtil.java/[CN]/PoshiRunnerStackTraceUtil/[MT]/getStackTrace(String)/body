{
  StringBuilder sb=new StringBuilder();
  sb.append("\nBUILD FAILED:");
  if (Validator.isNotNull(msg)) {
    sb.append(" ");
    sb.append(msg);
  }
  Stack<String> stackTrace=(Stack<String>)_stackTrace.clone();
  while (!stackTrace.isEmpty()) {
    sb.append("\n");
    sb.append(stackTrace.pop());
  }
  return sb.toString();
}
