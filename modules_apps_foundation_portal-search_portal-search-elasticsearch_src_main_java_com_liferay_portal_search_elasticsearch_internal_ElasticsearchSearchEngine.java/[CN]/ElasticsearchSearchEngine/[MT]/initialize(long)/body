{
  super.initialize(companyId);
  waitForYellowStatus();
  try {
    indexFactory.createIndices(elasticsearchConnectionManager.getAdminClient(),companyId);
    elasticsearchConnectionManager.registerCompanyId(companyId);
  }
 catch (  Exception e) {
    throw new IllegalStateException(e);
  }
  waitForYellowStatus();
}
