{
  ProcessDefinition definition=ProcessDefinition.parseXmlString(xml);
  jbpmContext.deployProcessDefinition(definition);
  StringBuffer sb=new StringBuffer();
  sb.append("<result>");
  sb.append("<definitionId>");
  sb.append(definition.getId());
  sb.append("</definitionId>");
  sb.append("</result>");
  return sb.toString();
}
