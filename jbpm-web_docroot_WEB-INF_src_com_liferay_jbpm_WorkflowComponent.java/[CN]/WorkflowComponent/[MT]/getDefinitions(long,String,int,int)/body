{
  List definitions=new ArrayList();
  if (definitionId > 0) {
    ProcessDefinition definition=graphSession.loadProcessDefinition(definitionId);
    WorkflowUtil.initDefinition(definition);
    definitions.add(definition);
  }
 else {
    definitions=graphSession.findProcessDefinitionsByName(name,begin,end);
  }
  return definitions;
}
