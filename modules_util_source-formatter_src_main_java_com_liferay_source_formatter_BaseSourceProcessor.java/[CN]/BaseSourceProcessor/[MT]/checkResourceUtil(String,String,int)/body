{
  if (!portalSource || fileName.endsWith("ResourceBundleUtil.java")) {
    return;
  }
  if (line.contains("ResourceBundle.getBundle(")) {
    processMessage(fileName,"Use ResourceBundleUtil.getBundle instead of " + "ResourceBundle.getBundle",lineCount);
  }
  if (line.contains("resourceBundle.getString(")) {
    processMessage(fileName,"Use ResourceBundleUtil.getString instead of " + "resourceBundle.getString",lineCount);
  }
}
