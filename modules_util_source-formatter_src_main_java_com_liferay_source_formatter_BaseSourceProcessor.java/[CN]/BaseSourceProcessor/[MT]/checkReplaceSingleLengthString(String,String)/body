{
  Matcher matcher=replaceSingleLengthStringPattern.matcher(newContent);
  if (!matcher.find()) {
    return;
  }
  String method=null;
  do {
    method=matcher.group(1);
    String fieldName=matcher.group(4);
    if (fieldName == null) {
      break;
    }
    Field field=StringPool.class.getDeclaredField(fieldName);
    String value=(String)field.get(null);
    if (value.length() == 1) {
      break;
    }
  }
 while (matcher.find());
  processErrorMessage(fileName,"Use StringUtil." + method + "(String, char, char) or "+ "StringUtil."+ method+ "(String, char, String) instead: "+ fileName);
}
