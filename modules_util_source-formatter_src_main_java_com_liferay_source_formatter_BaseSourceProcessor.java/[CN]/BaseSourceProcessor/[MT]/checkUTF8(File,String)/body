{
  byte[] bytes=FileUtil.getBytes(file);
  try {
    CharsetDecoder charsetDecoder=CharsetDecoderUtil.getCharsetDecoder(StringPool.UTF8,CodingErrorAction.REPORT);
    charsetDecoder.decode(ByteBuffer.wrap(bytes));
  }
 catch (  Exception e) {
    processErrorMessage(fileName,"UTF-8: " + fileName);
  }
}
