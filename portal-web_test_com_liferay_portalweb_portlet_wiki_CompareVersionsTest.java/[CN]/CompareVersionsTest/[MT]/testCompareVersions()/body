    Thread.sleep(1000);
  }
  selenium.click("//input[@value='Compare Versions']");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isTextPresent("Oh NOES! I've made a minor change. Please revert this!")) {
        break;
      }
    }
 catch (    Exception e) {
