{
  parameterMap.put(PortletDataHandlerKeys.PERFORM_DIRECT_BINARY_IMPORT,new String[]{Boolean.TRUE.toString()});
  File file=LayoutLocalServiceUtil.exportLayoutsAsFile(sourceGroupId,privateLayout,layoutIds,parameterMap,startDate,endDate);
  try {
    LayoutLocalServiceUtil.importLayouts(userId,targetGroupId,privateLayout,parameterMap,file);
  }
  finally {
    file.delete();
  }
}
