{
  List<DDMFormLayoutColumn> ddmFormLayoutColumns=new ArrayList<>();
  int ddmFormLayoutColumnSize=(DDMFormLayoutColumn.FULL / ddmFormFieldNames.length);
  for (  String ddmFormFieldName : ddmFormFieldNames) {
    ddmFormLayoutColumns.add(createDDMFormLayoutColumn(ddmFormFieldName,ddmFormLayoutColumnSize));
  }
  return ddmFormLayoutColumns;
}
