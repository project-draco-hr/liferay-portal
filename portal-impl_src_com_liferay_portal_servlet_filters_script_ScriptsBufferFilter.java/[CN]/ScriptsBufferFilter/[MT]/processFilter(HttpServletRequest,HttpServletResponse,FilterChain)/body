{
  if (_log.isDebugEnabled()) {
    String completeURL=HttpUtil.getCompleteURL(request);
    _log.debug("Flushing scripts buffer for " + completeURL);
  }
  request.setAttribute(SKIP_FILTER,Boolean.TRUE);
  BufferCacheServletResponse bufferCacheServletResponse=new BufferCacheServletResponse(response);
  processFilter(ScriptsBufferFilter.class,request,bufferCacheServletResponse,filterChain);
  String content=flushScriptsBuffer(request,bufferCacheServletResponse.getString());
  ServletResponseUtil.write(response,content);
}
