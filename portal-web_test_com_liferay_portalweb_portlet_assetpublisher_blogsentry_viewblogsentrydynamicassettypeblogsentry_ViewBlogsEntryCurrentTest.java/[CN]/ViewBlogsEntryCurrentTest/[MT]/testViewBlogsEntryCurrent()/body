{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("AP Blogs Entry Title"),selenium.getText("//div[1]/h3/a"));
  assertEquals(RuntimeVariables.replace("AP Blogs Entry Content."),selenium.getText("//div/div/div[1]/div[2]/div[1]"));
}
