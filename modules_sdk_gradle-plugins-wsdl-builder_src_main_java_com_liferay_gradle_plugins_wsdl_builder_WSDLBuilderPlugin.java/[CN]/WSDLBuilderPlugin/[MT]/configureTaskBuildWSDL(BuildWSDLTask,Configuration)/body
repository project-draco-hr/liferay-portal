{
  FileCollection fileCollection=buildWSDLTask.getSource();
  if (fileCollection.isEmpty()) {
    return;
  }
  for (  File inputFile : fileCollection) {
    addTaskBuildWSDLTasks(buildWSDLTask,inputFile,wsdlBuilderConfiguration);
  }
  if (buildWSDLTask.isBuildLibs()) {
    TaskOutputs taskOutputs=buildWSDLTask.getOutputs();
    GradleUtil.addDependency(buildWSDLTask.getProject(),JavaPlugin.COMPILE_CONFIGURATION_NAME,taskOutputs.getFiles());
  }
}
