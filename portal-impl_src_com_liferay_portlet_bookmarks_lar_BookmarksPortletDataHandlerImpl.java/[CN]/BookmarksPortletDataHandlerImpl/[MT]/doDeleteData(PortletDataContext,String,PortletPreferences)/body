{
  if (!context.addPrimaryKey(BookmarksPortletDataHandlerImpl.class,"deleteData")) {
    BookmarksFolderLocalServiceUtil.deleteFolders(context.getScopeGroupId());
    BookmarksEntryLocalServiceUtil.deleteEntries(context.getScopeGroupId(),BookmarksFolderConstants.DEFAULT_PARENT_FOLDER_ID);
  }
  return null;
}
