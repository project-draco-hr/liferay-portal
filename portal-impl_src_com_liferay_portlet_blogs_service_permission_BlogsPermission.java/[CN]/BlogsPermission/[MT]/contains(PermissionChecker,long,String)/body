{
  Boolean hasPermission=StagingPermissionUtil.hasPermission(permissionChecker,groupId,"com.liferay.portlet.blogs",groupId,PortletKeys.BLOGS,actionId);
  if (hasPermission != null) {
    return hasPermission.booleanValue();
  }
  return permissionChecker.hasPermission(groupId,"com.liferay.portlet.blogs",groupId,actionId);
}
