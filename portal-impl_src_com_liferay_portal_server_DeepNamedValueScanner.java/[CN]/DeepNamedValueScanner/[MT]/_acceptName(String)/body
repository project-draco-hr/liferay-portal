{
  if (name == null) {
    return true;
  }
  name=name.toLowerCase();
  if (_ignoredNames != null) {
    for (    String ignoredName : _ignoredNames) {
      ignoredName=ignoredName.toLowerCase();
      if (name.contains(ignoredName)) {
        return false;
      }
    }
  }
  if (_trackUsageCount) {
    _incrementUsageCount(_namesStats,name);
  }
  return true;
}
