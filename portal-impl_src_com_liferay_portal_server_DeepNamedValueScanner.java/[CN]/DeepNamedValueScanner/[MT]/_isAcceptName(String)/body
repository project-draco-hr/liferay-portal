{
  if (name == null) {
    return true;
  }
  name=StringUtil.toLowerCase(name);
  if (_excludedNames != null) {
    for (    String excludedNames : _excludedNames) {
      if (name.contains(excludedNames)) {
        return false;
      }
    }
  }
  if (_trackUsageCount) {
    _incrementUsageCount(_nameDatasets,name);
  }
  return true;
}
