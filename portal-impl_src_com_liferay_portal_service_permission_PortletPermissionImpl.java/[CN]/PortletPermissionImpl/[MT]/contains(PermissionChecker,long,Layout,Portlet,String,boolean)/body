{
  if (portlet.isUndeployedPortlet()) {
    return false;
  }
  boolean value=contains(permissionChecker,groupId,layout,portlet.getPortletId(),actionId,strict);
  if (value) {
    return true;
  }
 else {
    if (portlet.isSystem() && actionId.equals(ActionKeys.VIEW)) {
      return true;
    }
 else {
      return false;
    }
  }
}
