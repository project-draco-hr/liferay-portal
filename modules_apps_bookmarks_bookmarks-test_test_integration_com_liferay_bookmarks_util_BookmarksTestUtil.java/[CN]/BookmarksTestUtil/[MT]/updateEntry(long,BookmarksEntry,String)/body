{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(entry.getGroupId(),userId);
  serviceContext.setCommand(Constants.UPDATE);
  serviceContext.setLayoutFullURL("http://localhost");
  return BookmarksEntryLocalServiceUtil.updateEntry(userId,entry.getEntryId(),entry.getGroupId(),entry.getFolderId(),name,entry.getUrl(),entry.getDescription(),serviceContext);
}
