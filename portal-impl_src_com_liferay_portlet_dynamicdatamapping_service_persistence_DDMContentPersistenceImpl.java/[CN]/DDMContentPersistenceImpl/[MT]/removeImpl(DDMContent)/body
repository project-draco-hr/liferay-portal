{
  ddmContent=toUnwrappedModel(ddmContent);
  Session session=null;
  try {
    session=openSession();
    if (ddmContent.isCachedModel()) {
      ddmContent=(DDMContent)session.get(DDMContentImpl.class,ddmContent.getPrimaryKeyObj());
    }
    if (ddmContent != null) {
      session.delete(ddmContent);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (ddmContent != null) {
    clearCache(ddmContent);
  }
  return ddmContent;
}
