{
  ddmContent=toUnwrappedModel(ddmContent);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,ddmContent);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(ddmContent);
  return ddmContent;
}
