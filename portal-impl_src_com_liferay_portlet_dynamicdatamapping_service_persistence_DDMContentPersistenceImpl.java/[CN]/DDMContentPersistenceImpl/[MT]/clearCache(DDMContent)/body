{
  EntityCacheUtil.removeResult(DDMContentModelImpl.ENTITY_CACHE_ENABLED,DDMContentImpl.class,ddmContent.getPrimaryKey());
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddmContent.getUuid(),Long.valueOf(ddmContent.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_C,new Object[]{Long.valueOf(ddmContent.getGroupId()),ddmContent.getContentKey()});
}
