{
  AssetEntry entry;
  long entryId;
  if (_assetEntryId > 0 || assetLink.getEntryId1() == _assetEntryId) {
    entryId=assetLink.getEntryId2();
  }
 else {
    entryId=assetLink.getEntryId1();
  }
  entry=AssetEntryLocalServiceUtil.getEntry(entryId);
  if (escaped) {
    entry=entry.toEscapedModel();
  }
  return entry;
}
