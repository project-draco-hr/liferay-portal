{
  List<AssetRendererFactory> assetRendererFactories=new ArrayList<AssetRendererFactory>();
  for (  AssetRendererFactory assetRendererFactory : AssetRendererFactoryRegistryUtil.getAssetRendererFactories(_company.getCompanyId())) {
    if (assetRendererFactory.isLinkable() && assetRendererFactory.isSelectable()) {
      assetRendererFactories.add(assetRendererFactory);
    }
  }
  return assetRendererFactories;
}
