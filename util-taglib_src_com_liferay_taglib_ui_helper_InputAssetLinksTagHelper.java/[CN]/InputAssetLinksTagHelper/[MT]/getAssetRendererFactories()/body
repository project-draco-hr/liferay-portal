{
  List<AssetRendererFactory> factories=new ArrayList<AssetRendererFactory>();
  for (  AssetRendererFactory factory : AssetRendererFactoryRegistryUtil.getAssetRendererFactories(_companyId)) {
    if (factory.isLinkable() && factory.isSelectable()) {
      factories.add(factory);
    }
  }
  return ListUtil.sort(factories,new AssetRendererFactoryTypeNameComparator(_locale));
}
