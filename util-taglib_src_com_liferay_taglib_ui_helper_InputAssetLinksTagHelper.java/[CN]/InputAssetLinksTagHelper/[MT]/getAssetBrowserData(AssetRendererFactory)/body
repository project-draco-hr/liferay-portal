{
  Map<String,Object> data=new HashMap<String,Object>();
  data.put("href",_getAssetBrowserPortletURL(assetRendererFactory).toString());
  data.put("title",LanguageUtil.format(_pageContext,"select-x",assetRendererFactory.getTypeName(_locale),false));
  data.put("type",assetRendererFactory.getClassName());
  return data;
}
