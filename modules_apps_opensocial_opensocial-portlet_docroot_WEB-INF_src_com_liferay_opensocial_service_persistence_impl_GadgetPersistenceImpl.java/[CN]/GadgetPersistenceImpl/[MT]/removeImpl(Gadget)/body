{
  gadget=toUnwrappedModel(gadget);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(gadget)) {
      gadget=(Gadget)session.get(GadgetImpl.class,gadget.getPrimaryKeyObj());
    }
    if (gadget != null) {
      session.delete(gadget);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (gadget != null) {
    clearCache(gadget);
  }
  return gadget;
}
