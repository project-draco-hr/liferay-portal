{
  if (!_fileEntries.contains(fileVersion.getFileVersionId())) {
    boolean generateImages=false;
    String extension=fileVersion.getExtension();
    if (extension.equals("pdf")) {
      generateImages=true;
    }
 else     if (DocumentConversionUtil.isEnabled()) {
      String[] conversions=DocumentConversionUtil.getConversions(fileVersion.getExtension());
      for (      String conversion : conversions) {
        if (conversion.equals("pdf")) {
          generateImages=true;
          break;
        }
      }
    }
    if (generateImages) {
      _fileEntries.add(fileVersion.getFileVersionId());
      MessageBusUtil.sendMessage(DestinationNames.DOCUMENT_LIBRARY_PDF_PROCESSOR,fileVersion);
    }
  }
}
