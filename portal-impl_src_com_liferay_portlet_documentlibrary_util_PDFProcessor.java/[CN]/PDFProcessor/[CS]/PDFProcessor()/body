{
  FileUtil.mkdirs(PREVIEW_TMP_PATH);
  FileUtil.mkdirs(THUMBNAIL_TMP_PATH);
  if (PropsValues.IMAGEMAGICK_ENABLED) {
    String filterName=null;
    if (OSDetector.isApple()) {
      filterName="apple";
    }
 else     if (OSDetector.isWindows()) {
      filterName="windows";
    }
 else {
      filterName="unix";
    }
    String globalSearchPath=PropsUtil.get(PropsKeys.IMAGEMAGICK_GLOBAL_SEARCH_PATH,new Filter(filterName));
    ProcessStarter.setGlobalSearchPath(globalSearchPath);
    _convertCmd=new ConvertCmd();
  }
}
