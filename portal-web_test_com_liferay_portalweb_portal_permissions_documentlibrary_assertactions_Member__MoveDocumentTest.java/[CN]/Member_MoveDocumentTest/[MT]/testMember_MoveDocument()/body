  selenium.clickAt("link=Permissions2 Test2 Folder2",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Choose']",RuntimeVariables.replace(""));
  selenium.selectWindow("null");
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
