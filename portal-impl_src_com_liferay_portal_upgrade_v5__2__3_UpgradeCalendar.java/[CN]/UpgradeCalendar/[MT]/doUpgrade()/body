{
  if (isSupportsAlterColumnName()) {
    runSQL("alter_column_type CalEvent remindBy INTEGER");
  }
 else {
    UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(CalEventImpl.TABLE_NAME,CalEventImpl.TABLE_COLUMNS);
