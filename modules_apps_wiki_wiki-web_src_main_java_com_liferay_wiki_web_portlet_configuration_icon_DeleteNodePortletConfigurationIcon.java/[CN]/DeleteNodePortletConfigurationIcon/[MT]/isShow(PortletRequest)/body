{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  if (WikiNodePermissionChecker.contains(themeDisplay.getPermissionChecker(),_node,ActionKeys.DELETE) && (WikiNodeServiceUtil.getNodesCount(themeDisplay.getScopeGroupId()) > 1)) {
    return true;
  }
  return false;
}
