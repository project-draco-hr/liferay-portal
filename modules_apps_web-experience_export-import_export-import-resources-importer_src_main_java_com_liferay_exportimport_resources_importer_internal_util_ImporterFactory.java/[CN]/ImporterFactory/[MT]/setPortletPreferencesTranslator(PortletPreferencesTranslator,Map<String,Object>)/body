{
  String rootPortletId=GetterUtil.getString(properties.get("portlet.preferences.translator.portlet.id"));
  if (Validator.isNotNull(rootPortletId)) {
    _portletPreferencesTranslators.put(rootPortletId,portletPreferencesTranslator);
  }
  if (_log.isWarnEnabled()) {
    _log.warn("The property \"portlet.preferences.translator.portlet.id\" " + "is null");
  }
}
