{
  BlogsEntry entry=addBlogsEntry("image.jpg");
  FileEntry coverImageFileEntry=PortletFileRepositoryUtil.getPortletFileEntry(entry.getCoverImageFileEntryId());
  ImageSelector coverImageSelector=new ImageSelector(0,StringPool.BLANK,StringPool.BLANK);
  updateBlogsEntry(entry.getEntryId(),coverImageSelector);
  Assert.assertEquals(0,entry.getCoverImageFileEntryId());
  PortletFileRepositoryUtil.getPortletFileEntry(coverImageFileEntry.getFileEntryId());
}
