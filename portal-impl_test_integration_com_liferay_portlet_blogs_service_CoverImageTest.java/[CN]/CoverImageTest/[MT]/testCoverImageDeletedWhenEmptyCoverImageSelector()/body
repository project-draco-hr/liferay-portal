{
  BlogsEntry blogsEntry=addBlogsEntry("image1.jpg");
  FileEntry coverImageFileEntry=PortletFileRepositoryUtil.getPortletFileEntry(blogsEntry.getCoverImageFileEntryId());
  ImageSelector coverImageSelector=new ImageSelector(0,StringPool.BLANK,StringPool.BLANK);
  blogsEntry=updateBlogsEntry(blogsEntry.getEntryId(),coverImageSelector);
  Assert.assertEquals(0,blogsEntry.getCoverImageFileEntryId());
  PortletFileRepositoryUtil.getPortletFileEntry(coverImageFileEntry.getFileEntryId());
}
