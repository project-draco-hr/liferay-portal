{
  List<UserGroupGroupRole> userGroupGroupRoles=userGroupGroupRoleFinder.findByGroupRoleType(groupId,roleType);
  for (  UserGroupGroupRole userGroupGroupRole : userGroupGroupRoles) {
    userGroupGroupRolePersistence.removeByG_R(groupId,userGroupGroupRole.getRoleId());
  }
  PermissionCacheUtil.clearCache();
}
