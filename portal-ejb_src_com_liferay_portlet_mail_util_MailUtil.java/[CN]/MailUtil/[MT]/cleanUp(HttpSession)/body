{
  try {
    _closeFolder(ses);
    Store store=(Store)ses.getAttribute(WebKeys.MAIL_STORE);
    if (store != null) {
      store.close();
      ses.removeAttribute(WebKeys.MAIL_STORE);
    }
    ses.removeAttribute(WebKeys.MAIL_MESSAGE_ID);
  }
 catch (  MessagingException me) {
    throw new StoreException(me);
  }
}
