{
  String accountName=ParamUtil.getString(request,Constants.MAIL_ACCOUNT);
  try {
    if (Validator.isNotNull(accountName)) {
      MailAccounts.setAccount(request.getSession(),accountName);
    }
  }
 catch (  MailAccountsException e) {
    _log.warn("Error setting account " + accountName + ". "+ e.getMessage());
  }
}
