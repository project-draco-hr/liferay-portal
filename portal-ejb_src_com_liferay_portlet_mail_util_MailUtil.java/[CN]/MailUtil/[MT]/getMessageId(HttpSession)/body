{
  try {
    MailSessionLock.getInstance().lock(ses.getId());
    Long messageId=(Long)ses.getAttribute(WebKeys.MAIL_MESSAGE_ID);
    if (messageId != null) {
      return messageId.longValue();
    }
 else {
      return -1L;
    }
  }
  finally {
    MailSessionLock.getInstance().unlock(ses.getId());
  }
}
