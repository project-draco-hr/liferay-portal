{
  final String[] startTags={"<html","<head","<body","<meta","<o:SmartTagType"};
  final String[] endTags={"</html>","</head>","</body>"};
  for (int i=0; i < startTags.length; i++) {
    Pattern pattern=Pattern.compile(startTags[i],Pattern.CASE_INSENSITIVE);
    Matcher matcher=pattern.matcher(html);
    while (matcher.find()) {
      int start=matcher.start();
      int end=html.indexOf(">",start);
      if (end == -1) {
        html=StringUtil.replace(html,html.substring(start),StringPool.BLANK);
      }
 else {
        html=StringUtil.replace(html,html.substring(start,end + 1),StringPool.BLANK);
      }
      if (i < endTags.length) {
        html=Pattern.compile(endTags[i],Pattern.CASE_INSENSITIVE).matcher(html).replaceFirst(StringPool.BLANK);
      }
 else {
        matcher.reset(html);
      }
    }
  }
  return html.trim();
}
