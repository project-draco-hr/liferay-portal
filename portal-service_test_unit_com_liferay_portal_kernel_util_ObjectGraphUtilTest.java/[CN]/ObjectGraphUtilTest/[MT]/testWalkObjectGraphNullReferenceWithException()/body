{
  final Exception exception=new Exception();
  try {
    ObjectGraphUtil.walkObjectGraph(new NullReference(),new Visitor(){
      @Override public Object visit(      Field field,      Object target) throws Exception {
        throw exception;
      }
    }
);
    Assert.fail();
  }
 catch (  Exception e) {
    Assert.assertSame(exception,e);
  }
}
