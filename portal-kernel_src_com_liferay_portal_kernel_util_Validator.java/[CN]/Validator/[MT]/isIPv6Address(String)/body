{
  if (isNull(ipAddress)) {
    return false;
  }
  if ((ipAddress.charAt(0) == CharPool.OPEN_BRACKET) && (ipAddress.charAt(ipAddress.length() - 1) == CharPool.CLOSE_BRACKET)) {
    ipAddress=ipAddress.substring(1,ipAddress.length() - 1);
  }
  Matcher matcher=_ipv6AddressPattern.matcher(ipAddress);
  return matcher.matches();
}
