{
  if (isNull(ipAddress)) {
    return false;
  }
  if (StringUtil.startsWith(ipAddress,CharPool.OPEN_BRACKET) && StringUtil.endsWith(ipAddress,CharPool.CLOSE_BRACKET)) {
    ipAddress=ipAddress.substring(1,ipAddress.length() - 1);
  }
  Matcher matcher=_ipv6AddressPattern.matcher(ipAddress);
  return matcher.matches();
}
