{
  if (s == null) {
    return true;
  }
  int counter=0;
  for (int i=0; i < s.length(); i++) {
    char ch=s.charAt(i);
    if (ch == ' ') {
      continue;
    }
 else     if (counter > 3) {
      return false;
    }
switch (counter) {
case 0:
      if (ch != 'n') {
        return false;
      }
    break;
case 1:
  if (ch != 'u') {
    return false;
  }
break;
case 2:
case 3:
if (ch != 'l') {
return false;
}
break;
}
counter++;
}
return true;
}
