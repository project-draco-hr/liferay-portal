{
  Iterator itr=mbCategoryPersistence.findByG_P(groupId,categoryId).iterator();
  while (itr.hasNext()) {
    MBCategory category=(MBCategory)itr.next();
    categoryIds.add(new Long(category.getCategoryId()));
    getSubcategoryIds(categoryIds,category.getGroupId(),category.getCategoryId());
  }
}
