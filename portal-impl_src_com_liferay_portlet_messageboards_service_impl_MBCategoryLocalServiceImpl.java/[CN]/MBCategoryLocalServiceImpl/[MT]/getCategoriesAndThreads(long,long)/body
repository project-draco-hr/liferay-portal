{
  List<MBCategory> categories=getCategories(groupId,categoryId,QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  List<MBThread> threads=mbThreadLocalService.getThreads(groupId,categoryId,WorkflowConstants.STATUS_ANY,QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  List<Object> categoriesAndThreads=new ArrayList<Object>();
  categoriesAndThreads.addAll(categories);
  categoriesAndThreads.addAll(threads);
  return categoriesAndThreads;
}
