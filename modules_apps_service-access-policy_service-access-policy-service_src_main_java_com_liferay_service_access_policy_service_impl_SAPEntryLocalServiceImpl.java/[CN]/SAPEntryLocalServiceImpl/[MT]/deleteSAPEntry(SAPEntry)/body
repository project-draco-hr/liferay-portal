{
  if (sapEntry.isSystem() && !CompanyThreadLocal.isDeleteInProcess()) {
    throw new RequiredSAPEntryException();
  }
  sapEntry=super.deleteSAPEntry(sapEntry);
  resourceLocalService.deleteResource(sapEntry.getCompanyId(),SAPEntry.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,sapEntry.getSapEntryId());
  return sapEntry;
}
