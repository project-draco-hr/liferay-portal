{
  String[] paths=fileName.split("/");
  StringBuilder sb=new StringBuilder(paths.length * 2);
  for (  String path : paths) {
    if (path.isEmpty() || path.equals(".")) {
      continue;
    }
    if (path.equals("..") && (sb.length() >= 2)) {
      continue;
    }
    sb.append("/");
    sb.append(path);
  }
  return sb.toString();
}
