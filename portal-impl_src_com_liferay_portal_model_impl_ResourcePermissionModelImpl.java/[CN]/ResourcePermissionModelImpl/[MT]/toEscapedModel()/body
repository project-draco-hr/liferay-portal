{
  if (isEscapedModel()) {
    return (ResourcePermission)this;
  }
 else {
    if (_escapedModelProxy == null) {
      _escapedModelProxy=(ResourcePermission)ProxyUtil.newProxyInstance(_classLoader,_escapedModelProxyInterfaces,new AutoEscapeBeanHandler(this));
    }
    return _escapedModelProxy;
  }
}
