{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId());
  serviceContext.setAttribute("status",status);
  return DDMStructureLocalServiceUtil.addStructure(TestPropsValues.getUserId(),_group.getGroupId(),parentStructureId,classNameId,structureKey,getDefaultLocaleMap(name),getDefaultLocaleMap(description),ddmForm,ddmFormLayout,storageType,type,serviceContext);
}
