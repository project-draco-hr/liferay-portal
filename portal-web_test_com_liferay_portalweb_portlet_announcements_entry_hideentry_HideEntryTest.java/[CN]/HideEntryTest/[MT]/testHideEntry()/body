{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Announcements Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Announcements Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Show",RuntimeVariables.replace(""));
  Thread.sleep(5000);
  assertEquals(RuntimeVariables.replace("General Hello Everyone! This is a test general announcement for everyone! Yay."),selenium.getText("//p"));
  selenium.clickAt("link=Hide",RuntimeVariables.replace(""));
  Thread.sleep(5000);
  assertEquals(RuntimeVariables.replace("General Hello Everyone! This is a test general announcement for everyone! Yay."),selenium.getText("//p"));
  assertFalse(selenium.isVisible("//p"));
}
