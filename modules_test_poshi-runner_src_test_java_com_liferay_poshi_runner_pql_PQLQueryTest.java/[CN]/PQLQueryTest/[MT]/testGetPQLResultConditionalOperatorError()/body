  _validateGetPQLResultError("true == true OR","Invalid query: true == true OR",properties);
  _validateGetPQLResultError("true == true AND AND false == false","Invalid value: AND false == false",properties);
  _validateGetPQLResultError("(true == true) AND","Invalid query: (true == true) AND",properties);
  _validateGetPQLResultError("(true == true) OR","Invalid query: (true == true) OR",properties);
}
