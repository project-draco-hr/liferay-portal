{
  request.setAttribute(LAYOUT_ITEM_SELECTOR_CRITERION,layoutItemSelectorCriterion);
  request.setAttribute(ITEM_SELECTED_EVENT_NAME,itemSelectedEventName);
  request.setAttribute(PORTLET_URL,portletURL);
  RequestDispatcher requestDispatcher=request.getRequestDispatcher("/o/layout-item-selector-web/layouts.jsp");
  requestDispatcher.include(request,response);
}
