{
  LayoutItemSelectorViewDisplayContext layoutItemSelectorViewDisplayContext=new LayoutItemSelectorViewDisplayContext(layoutItemSelectorCriterion,itemSelectedEventName);
  request.setAttribute(LAYOUT_ITEM_SELECTOR_VIEW_DISPLAY_CONTEXT,layoutItemSelectorViewDisplayContext);
  RequestDispatcher requestDispatcher=request.getRequestDispatcher("/o/layout-item-selector-web/layouts.jsp");
  requestDispatcher.include(request,response);
}
