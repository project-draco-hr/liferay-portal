{
  Map<String,String[]> parameters=_itemSelectorImpl.getItemSelectorParameters("itemSelectedEventName",_mediaItemSelectorCriterion,_flickrItemSelectorCriterion);
  Assert.assertEquals("itemSelectedEventName",parameters.get(ItemSelectorImpl.PARAMETER_ITEM_SELECTED_EVENT_NAME)[0]);
  Assert.assertEquals(MediaItemSelectorCriterion.class.getName() + "," + FlickrItemSelectorCriterion.class.getName(),parameters.get(ItemSelectorImpl.PARAMETER_CRITERIA)[0]);
  Assert.assertNull(parameters.get("0_desiredReturnTypes"));
  Assert.assertNotNull(parameters.get("0_json")[0]);
  Assert.assertNotNull(parameters.get("1_json")[0]);
  Assert.assertEquals(4,parameters.size());
}
