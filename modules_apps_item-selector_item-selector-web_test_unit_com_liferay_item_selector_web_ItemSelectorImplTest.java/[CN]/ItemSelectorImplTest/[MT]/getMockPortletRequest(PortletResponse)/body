{
  Map<String,String[]> parameters=_itemSelectorImpl.getItemSelectorParameters("itemSelectedEventName",_mediaItemSelectorCriterion,_flickrItemSelectorCriterion);
  PortletRequest portletRequest=mock(PortletRequest.class);
  when(portletRequest.getParameterMap()).thenReturn(parameters);
  ThemeDisplay themeDisplay=mock(ThemeDisplay.class);
  when(portletRequest.getAttribute(WebKeys.THEME_DISPLAY)).thenReturn(themeDisplay);
  when(portletRequest.getAttribute(JavaConstants.JAVAX_PORTLET_RESPONSE)).thenReturn(portletResponse);
  return portletRequest;
}
