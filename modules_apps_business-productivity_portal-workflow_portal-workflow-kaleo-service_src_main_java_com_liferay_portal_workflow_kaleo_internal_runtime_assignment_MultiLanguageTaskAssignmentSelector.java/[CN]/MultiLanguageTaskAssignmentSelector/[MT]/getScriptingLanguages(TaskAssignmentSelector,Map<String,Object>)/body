{
  Object value=properties.get("scripting.language");
  String[] scriptingLanguages=GetterUtil.getStringValues(value,new String[]{String.valueOf(value)});
  if (ArrayUtil.isEmpty(scriptingLanguages)) {
    throw new IllegalArgumentException("Must have a scripting.language property for: " + ClassUtil.getClassName(taskAssignmentSelector));
  }
  return scriptingLanguages;
}
