{
  PortalCache<Serializable,Object> portalCache=_portalCaches.get(className);
  if ((portalCache == null) && createIfAbsent) {
    String groupKey=_GROUP_KEY_PREFIX.concat(className);
    portalCache=(PortalCache<Serializable,Object>)_multiVMPool.getCache(groupKey,PropsValues.VALUE_OBJECT_ENTITY_BLOCKING_CACHE);
    PortalCache<Serializable,Object> previousPortalCache=_portalCaches.putIfAbsent(className,portalCache);
    if (previousPortalCache != null) {
      portalCache=previousPortalCache;
    }
  }
  return portalCache;
}
