{
  String groupKey=_GROUP_KEY_PREFIX.concat(className);
  PortalCache portalCache=_portalCaches.get(groupKey);
  if (portalCache == null && createIfAbsent) {
    portalCache=_multiVMPool.getCache(groupKey,PropsValues.VALUE_OBJECT_ENTITY_BLOCKING_CACHE);
    PortalCache previousPortalCache=_portalCaches.putIfAbsent(groupKey,portalCache);
    if (previousPortalCache != null) {
      portalCache=previousPortalCache;
    }
  }
  return portalCache;
}
