{
  Element bookmarksEntryElement=_portletDataContext.getExportDataElement(_mockBookmarksEntry);
  _portletDataContext.addReferenceElement(_mockBookmarksEntry,bookmarksEntryElement,_mockBookmarksFolder,PortletDataContext.REFERENCE_TYPE_PARENT,true);
  Element missingReferencesElement=_portletDataContext.getMissingReferencesElement();
  List<Element> missingReferenceElements=missingReferencesElement.elements();
  Assert.assertEquals(1,missingReferenceElements.size());
  Element missingReferenceElement=missingReferenceElements.get(0);
  Assert.assertEquals(_mockBookmarksFolder.getModelClassName(),missingReferenceElement.attributeValue("class-name"));
  Assert.assertEquals(String.valueOf(_mockBookmarksFolder.getPrimaryKeyObj()),missingReferenceElement.attributeValue("class-pk"));
}
