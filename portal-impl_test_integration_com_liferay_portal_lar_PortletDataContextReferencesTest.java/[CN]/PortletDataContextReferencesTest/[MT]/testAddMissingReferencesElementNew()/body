{
  Element missingReferenceElement=_portletDataContext.addMissingReferenceElement(PortletKeys.BOOKMARKS,_bookmarksEntry);
  Assert.assertNotNull(missingReferenceElement);
  Element missingReferencesElement=_portletDataContext.getMissingReferencesElement();
  List<Element> missingReferenceElements=missingReferencesElement.elements("missing-reference");
  Assert.assertEquals(1,missingReferenceElements.size());
  Assert.assertEquals(missingReferenceElement,missingReferenceElements.get(0));
}
