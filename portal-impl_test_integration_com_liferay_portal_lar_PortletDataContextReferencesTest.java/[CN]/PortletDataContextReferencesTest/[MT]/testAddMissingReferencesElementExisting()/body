{
  Element missingReferenceElement1=_portletDataContext.addMissingReferenceElement(PortletKeys.BOOKMARKS,_bookmarksEntry);
  Assert.assertNotNull(missingReferenceElement1);
  Element missingReferenceElement2=_portletDataContext.addMissingReferenceElement(PortletKeys.BOOKMARKS,_bookmarksEntry);
  Assert.assertNotNull(missingReferenceElement2);
  Assert.assertEquals(missingReferenceElement1,missingReferenceElement2);
  Element missingReferencesElement=_portletDataContext.getMissingReferencesElement();
  List<Element> missingReferenceElements=missingReferencesElement.elements("missing-reference");
  Assert.assertEquals(1,missingReferenceElements.size());
}
