{
  List<ResourcePermission> resourcePermissions=resourcePermissionPersistence.findByC_N_S_P(companyId,name,scope,primKey);
  for (  ResourcePermission resourcePermission : resourcePermissions) {
    deleteResourcePermission(resourcePermission.getResourcePermissionId());
  }
  PermissionCacheUtil.clearResourcePermissionCache(name,primKey);
}
