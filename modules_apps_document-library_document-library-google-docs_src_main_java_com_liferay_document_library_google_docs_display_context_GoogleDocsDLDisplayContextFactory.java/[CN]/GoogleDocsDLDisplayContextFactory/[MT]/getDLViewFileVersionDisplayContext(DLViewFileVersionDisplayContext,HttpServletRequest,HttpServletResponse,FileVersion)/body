{
  Object model=fileVersion.getModel();
  if (model instanceof DLFileVersion) {
    GoogleDocsMetadataHelper googleDocsMetadataHelper=new GoogleDocsMetadataHelper(_ddmStructureLocalService,(DLFileVersion)model,_dlFileEntryMetadataLocalService,_storageEngine);
    if (googleDocsMetadataHelper.isGoogleDocs()) {
      return new GoogleDocsDLViewFileVersionDisplayContext(parentDLViewFileVersionDisplayContext,request,response,fileVersion,googleDocsMetadataHelper);
    }
  }
  return parentDLViewFileVersionDisplayContext;
}
