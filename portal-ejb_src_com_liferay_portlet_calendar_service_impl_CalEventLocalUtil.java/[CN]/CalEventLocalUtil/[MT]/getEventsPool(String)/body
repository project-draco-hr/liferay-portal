{
  key=_encodeKey(key);
  Map eventsPool=null;
  try {
    eventsPool=(Map)_cache.getFromCache(key);
  }
 catch (  NeedsRefreshException nfe) {
    eventsPool=CollectionFactory.getSyncHashMap();
    _cache.putInCache(key,eventsPool,GROUP_NAME_ARRAY);
  }
 finally {
    if (eventsPool == null) {
      _cache.cancelUpdate(key);
    }
  }
  return eventsPool;
}
