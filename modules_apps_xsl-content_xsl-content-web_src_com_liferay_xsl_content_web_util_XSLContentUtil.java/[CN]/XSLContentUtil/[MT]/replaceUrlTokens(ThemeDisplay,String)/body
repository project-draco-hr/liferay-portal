{
  Bundle bundle=FrameworkUtil.getBundle(XSLContentUtil.class);
  StringBundler sb=new StringBundler(3);
  sb.append(themeDisplay.getPortalURL());
  sb.append("/o/");
  sb.append(StringUtil.replace(bundle.getSymbolicName(),StringPool.PERIOD,StringPool.BLANK));
  String portletContextUrl=sb.toString();
  return StringUtil.replace(url,new String[]{"@portal_url@","@portlet_context_url@"},new String[]{themeDisplay.getPortalURL(),portletContextUrl});
}
