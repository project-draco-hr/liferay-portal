{
  MBMessage message=MBMessageLocalServiceUtil.getMessage(baseModelId);
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(message.getGroupId(),userId);
  MBTestUtil.populateNotificationsServiceContext(serviceContext,Constants.UPDATE);
  MBMessageLocalServiceUtil.updateMessage(userId,message.getMessageId(),RandomTestUtil.randomString(),serviceContext);
}
