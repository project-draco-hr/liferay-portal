{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId(),TestPropsValues.getUserId());
  serviceContext.setCommand(Constants.ADD);
  serviceContext.setLayoutFullURL("http://localhost");
  MBCategory category=MBCategoryLocalServiceUtil.addCategory(TestPropsValues.getUserId(),containerModelId,RandomTestUtil.randomString(),StringPool.BLANK,serviceContext);
  return category.getCategoryId();
}
