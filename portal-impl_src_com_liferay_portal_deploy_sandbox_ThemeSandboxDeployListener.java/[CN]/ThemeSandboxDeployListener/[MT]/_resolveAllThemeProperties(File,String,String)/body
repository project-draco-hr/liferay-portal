{
  String[] filesToChange=new String[]{"theme.txt","WEB-INF/liferay-look-and-feel.xml","WEB-INF/liferay-plugin-package.properties","WEB-INF/liferay-plugin-package.xml","WEB-INF/web.xml"};
  for (  String file : filesToChange) {
    _resolvePropertiesInFile(new File(themeDir,file),themeId,themeName);
  }
}
