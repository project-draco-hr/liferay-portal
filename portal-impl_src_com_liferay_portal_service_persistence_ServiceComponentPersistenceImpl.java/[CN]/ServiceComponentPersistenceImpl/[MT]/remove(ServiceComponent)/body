{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(serviceComponent);
    }
  }
  serviceComponent=removeImpl(serviceComponent);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(serviceComponent);
    }
  }
  return serviceComponent;
}
