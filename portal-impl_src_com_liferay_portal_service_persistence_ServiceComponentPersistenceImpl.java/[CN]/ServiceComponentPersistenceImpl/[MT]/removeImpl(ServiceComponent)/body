{
  Session session=null;
  try {
    session=openSession();
    session.delete(serviceComponent);
    session.flush();
    return serviceComponent;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(ServiceComponent.class.getName());
  }
}
