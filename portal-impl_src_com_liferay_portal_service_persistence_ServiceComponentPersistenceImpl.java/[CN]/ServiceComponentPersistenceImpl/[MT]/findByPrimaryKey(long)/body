{
  ServiceComponent serviceComponent=fetchByPrimaryKey(serviceComponentId);
  if (serviceComponent == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + serviceComponentId);
    }
    throw new NoSuchServiceComponentException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + serviceComponentId);
  }
  return serviceComponent;
}
