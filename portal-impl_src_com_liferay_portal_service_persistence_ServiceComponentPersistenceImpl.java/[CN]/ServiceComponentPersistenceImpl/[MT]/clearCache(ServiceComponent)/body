{
  EntityCacheUtil.removeResult(ServiceComponentModelImpl.ENTITY_CACHE_ENABLED,ServiceComponentImpl.class,serviceComponent.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_BNS_BNU,new Object[]{serviceComponent.getBuildNamespace(),Long.valueOf(serviceComponent.getBuildNumber())});
}
