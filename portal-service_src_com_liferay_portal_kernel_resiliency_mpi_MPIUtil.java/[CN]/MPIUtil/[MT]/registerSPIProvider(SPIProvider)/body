{
  String spiProviderName=spiProvider.getName();
  SPIProvider previousSPIProvider=null;
  _registerLock.lock();
  try {
    previousSPIProvider=_spiProviderMap.putIfAbsent(spiProviderName,spiProvider);
  }
  finally {
    _registerLock.unlock();
  }
  if (previousSPIProvider != null) {
    if (_log.isWarnEnabled()) {
      _log.warn("Can not register SPIProvider " + spiProvider + " with name "+ spiProviderName+ ". There is already a SPIProvider with the "+ "same name registered "+ previousSPIProvider);
    }
    return false;
  }
 else {
    if (_log.isInfoEnabled()) {
      _log.info("Registered new SPIProvider " + spiProvider + " with name "+ spiProviderName);
    }
    return true;
  }
}
