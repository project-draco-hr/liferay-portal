{
  try {
    Document doc=SAXReaderUtil.read(xml);
    Element root=doc.getRootElement();
    Element el=root.element(name);
    if (el != null) {
      return el.getText();
    }
 else {
      return StringPool.BLANK;
    }
  }
 catch (  DocumentException de) {
    _log.error("Error parsing " + name + " from:\n\n"+ xml);
    throw de;
  }
}
