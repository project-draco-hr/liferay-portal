{
  SettingsDescriptor<?> settingsDescriptor=new SettingsDescriptor<>(settingsClass);
  for (  String settingsId : settingsDescriptor.getIds()) {
    _settingsDescriptors.put(settingsId,settingsDescriptor);
    if (fallbackKeys != null) {
      _fallbackKeysMap.put(settingsId,fallbackKeys);
    }
    if (serviceConfigurationBean != null) {
      _serviceConfigurationBeans.put(settingsId,serviceConfigurationBean);
    }
    _resourceManagers.put(settingsId,new ClassLoaderResourceManager(settingsClass.getClassLoader()));
  }
}
