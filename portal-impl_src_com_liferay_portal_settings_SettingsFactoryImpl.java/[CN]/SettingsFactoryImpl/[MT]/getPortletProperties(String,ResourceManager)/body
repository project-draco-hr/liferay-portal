{
  Properties properties=_portletPropertiesMap.get(settingsId);
  if (properties != null) {
    return properties;
  }
  properties=new Properties();
  if (resourceManager == null) {
    return properties;
  }
  ResourceRetriever resourceRetriever=resourceManager.getResourceRetriever("portlet.properties");
  InputStream inputStream=resourceRetriever.getInputStream();
  try {
    properties.load(inputStream);
  }
 catch (  Exception e) {
  }
  _portletPropertiesMap.put(settingsId,properties);
  return properties;
}
