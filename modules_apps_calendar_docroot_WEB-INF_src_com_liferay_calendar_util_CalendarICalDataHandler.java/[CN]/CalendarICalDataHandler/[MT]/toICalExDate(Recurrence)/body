{
  List<java.util.Calendar> exceptionJCalendars=recurrence.getExceptionJCalendars();
  if (exceptionJCalendars.isEmpty()) {
    return null;
  }
  DateList dateList=new DateList();
  dateList.setUtc(true);
  for (  java.util.Calendar exceptionJCalendar : exceptionJCalendars) {
    DateTime dateTime=toICalDateTime(exceptionJCalendar.getTimeInMillis());
    dateList.add(dateTime);
  }
  ExDate exDate=new ExDate(dateList);
  return exDate;
}
