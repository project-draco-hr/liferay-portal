{
  try {
    ServletContext servletContext=getServletContext();
    HttpServletRequest request=getServletRequest();
    StringServletResponse response=getServletResponse();
    doTag(getPage(),_id,_showImage,_hideImage,_showMessage,_hideMessage,_defaultShowContent,_stateVar,servletContext,request,response);
    pageContext.getOut().print(response.getString());
    return EVAL_PAGE;
  }
 catch (  Exception e) {
    throw new JspException(e);
  }
}
