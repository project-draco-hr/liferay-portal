{
  ManifestSummary manifestSummary=portletDataContext.getManifestSummary();
  if ((portletPreferences == null) || (manifestSummary.getModelAdditionCount(PollsQuestion.class) > -1)) {
    return;
  }
  long questionId=GetterUtil.getLong(portletPreferences.getValue("questionId",StringPool.BLANK));
  if (questionId > 0) {
    manifestSummary.addModelAdditionCount(new StagedModelType(PollsQuestion.class),1);
  }
}
