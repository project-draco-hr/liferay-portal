{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SocialActivityCounter.class,SocialActivityCounter.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("activityCounterId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("activityCounterId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
