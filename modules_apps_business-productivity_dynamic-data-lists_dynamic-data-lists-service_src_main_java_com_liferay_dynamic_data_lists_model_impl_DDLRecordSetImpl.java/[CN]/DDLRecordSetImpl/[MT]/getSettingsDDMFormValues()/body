{
  if (_ddmFormValues == null) {
    try {
      DDMForm ddmForm=DDMFormFactory.create(DDLRecordSetSettings.class);
      _ddmFormValues=DDMFormValuesJSONDeserializerUtil.deserialize(ddmForm,getSettings());
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
  return _ddmFormValues;
}
