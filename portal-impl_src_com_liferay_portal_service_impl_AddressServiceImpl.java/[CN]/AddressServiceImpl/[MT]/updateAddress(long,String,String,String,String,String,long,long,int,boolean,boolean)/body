{
  Address address=addressPersistence.findByPrimaryKey(addressId);
  CommonPermissionUtil.checkPermission(getPermissionChecker(),address.getClassNameId(),address.getClassPK(),ActionKeys.UPDATE);
  return addressLocalService.updateAddress(addressId,street1,street2,street3,city,zip,regionId,countryId,typeId,mailing,primary);
}
