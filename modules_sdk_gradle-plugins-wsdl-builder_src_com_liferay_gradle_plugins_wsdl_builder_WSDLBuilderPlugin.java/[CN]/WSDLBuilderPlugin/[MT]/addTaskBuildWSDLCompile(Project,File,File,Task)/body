{
  String taskName=GradleUtil.getTaskName(BUILD_WSDL_TASK_NAME + "Compile",wsdlFile);
  JavaCompile javaCompile=GradleUtil.addTask(project,taskName,JavaCompile.class);
  javaCompile.setClasspath(GradleUtil.getConfiguration(project,CONFIGURATION_NAME));
  File tmpBinDir=new File(tmpDir,"bin");
  javaCompile.setDestinationDir(tmpBinDir);
  javaCompile.setSource(generateTask.getOutputs());
  return javaCompile;
}
