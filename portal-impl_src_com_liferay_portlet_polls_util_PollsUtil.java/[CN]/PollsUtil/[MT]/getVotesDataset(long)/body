{
  DefaultCategoryDataset dataset=new DefaultCategoryDataset();
  String seriesName=StringPool.BLANK;
  for (  PollsChoice choice : PollsChoiceLocalServiceUtil.getChoices(questionId)) {
    Integer number=new Integer(PollsVoteLocalServiceUtil.getChoiceVotesCount(choice.getChoiceId()));
    dataset.addValue(number,seriesName,choice.getName());
  }
  return dataset;
}
