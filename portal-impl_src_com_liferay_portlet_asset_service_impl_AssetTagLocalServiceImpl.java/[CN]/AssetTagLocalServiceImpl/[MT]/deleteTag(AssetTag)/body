{
  List<AssetEntry> entries=assetTagPersistence.getAssetEntries(tag.getTagId());
  assetTagPersistence.remove(tag);
  resourceLocalService.deleteResource(tag.getCompanyId(),AssetTag.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,tag.getTagId());
  assetTagStatsLocalService.deleteTagStatsByTagId(tag.getTagId());
  assetEntryLocalService.reindex(entries);
}
