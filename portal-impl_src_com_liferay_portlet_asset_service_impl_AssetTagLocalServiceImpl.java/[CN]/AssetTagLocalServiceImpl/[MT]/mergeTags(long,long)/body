{
  List<Asset> assets=assetTagPersistence.getAssets(fromTagId);
  assetTagPersistence.addAssets(toTagId,assets);
  List<AssetTagProperty> properties=assetTagPropertyPersistence.findByTagId(fromTagId);
  for (  AssetTagProperty fromProperty : properties) {
    AssetTagProperty toProperty=assetTagPropertyPersistence.fetchByE_K(toTagId,fromProperty.getKey());
    if (toProperty == null) {
      fromProperty.setTagId(toTagId);
      assetTagPropertyPersistence.update(fromProperty,false);
    }
  }
  deleteTag(fromTagId);
}
