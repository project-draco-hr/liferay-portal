{
  if (!isMySQL()) {
    return;
  }
  try {
    executeStatement("replace TestPACL_ReplaceFailure (userId) values (1)");
    Assert.fail();
  }
 catch (  SecurityException se) {
    String message=se.getMessage();
    Assert.assertTrue(message.startsWith("Attempted to execute unapproved SQL"));
  }
}
