{
  super(MicroblogsPortletKeys.MICROBLOGS,0,MicroblogsEntryConstants.NOTIFICATION_TYPE_TAG,"receive-a-notification-when-someone-tags-you-in-a-microblog");
  addUserNotificationDeliveryType(new UserNotificationDeliveryType("mobile",UserNotificationDeliveryConstants.TYPE_PUSH,true,true));
