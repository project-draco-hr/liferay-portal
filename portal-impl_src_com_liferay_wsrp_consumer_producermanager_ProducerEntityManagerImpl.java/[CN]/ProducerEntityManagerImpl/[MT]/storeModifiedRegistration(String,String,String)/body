{
  WSRPConfiguredProducer configuredProducer=getConfiguredProducer(producerEntityId);
  configuredProducer.setRegistrationData(registrationData);
  configuredProducer.setRegistrationContext(registrationContext);
  try {
    WSRPConfiguredProducerLocalServiceUtil.updateWSRPConfiguredProducer(configuredProducer);
  }
 catch (  Exception e) {
    throw new WSRPConsumerException(e.getMessage());
  }
}
