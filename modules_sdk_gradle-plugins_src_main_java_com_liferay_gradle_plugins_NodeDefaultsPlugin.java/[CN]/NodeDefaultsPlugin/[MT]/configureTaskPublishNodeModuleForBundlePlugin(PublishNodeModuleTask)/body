{
  publishNodeModuleTask.setModuleDescription(new Callable<String>(){
    @Override public String call() throws Exception {
      return getBundleInstruction(publishNodeModuleTask.getProject(),Constants.BUNDLE_NAME);
    }
  }
);
  publishNodeModuleTask.setModuleName(new Callable<String>(){
    @Override public String call() throws Exception {
      String bundleSymbolicName=getBundleInstruction(publishNodeModuleTask.getProject(),Constants.BUNDLE_SYMBOLICNAME);
      int pos=bundleSymbolicName.indexOf('.');
      String moduleName=bundleSymbolicName.substring(pos + 1);
      return moduleName.replace('.','-');
    }
  }
);
}
