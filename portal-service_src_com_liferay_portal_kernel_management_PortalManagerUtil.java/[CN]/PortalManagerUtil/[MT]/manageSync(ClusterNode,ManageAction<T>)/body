{
  ClusterGroup tempClusterGroup=ClusterGroupLocalServiceUtil.createClusterGroup(0);
  tempClusterGroup.setClusterNodeIds(clusterNode.getClusterNodeId());
  FutureClusterResponses futureClusterResponses=manage(tempClusterGroup,manageAction);
  return (T)futureClusterResponses.get().getClusterResponse(clusterNode).getResult();
}
