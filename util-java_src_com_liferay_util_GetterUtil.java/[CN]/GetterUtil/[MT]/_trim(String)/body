{
  if (value != null) {
    value=value.trim();
    StringBuffer sb=new StringBuffer();
    char[] charArray=value.toCharArray();
    for (int i=0; i < charArray.length; i++) {
      if ((Character.isDigit(charArray[i])) || (charArray[i] == '-' && i == 0) || (charArray[i] == '.')) {
        sb.append(charArray[i]);
      }
    }
    value=sb.toString();
  }
  return value;
}
