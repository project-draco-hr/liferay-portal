{
  if ((c >= CharPool.UPPER_CASE_A) && (c <= CharPool.UPPER_CASE_Z)) {
    return c - 65;
  }
  if ((c >= CharPool.LOWER_CASE_A) && (c <= CharPool.LOWER_CASE_Z)) {
    return (c - 97) + 26;
  }
  if (c >= CharPool.NUMBER_0 && c <= CharPool.NUMBER_9) {
    return (c - 48) + 52;
  }
  if (c == CharPool.PLUS) {
    return 62;
  }
  if (c == CharPool.SLASH) {
    return 63;
  }
  return c != CharPool.EQUAL ? -1 : 0;
}
