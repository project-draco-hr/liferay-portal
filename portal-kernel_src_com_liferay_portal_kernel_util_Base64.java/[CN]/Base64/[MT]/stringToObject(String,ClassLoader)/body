{
  if (s == null) {
    return null;
  }
  byte bytes[]=decode(s);
  ByteArrayInputStream bais=new ByteArrayInputStream(bytes);
  try {
    ObjectInputStream is=new ClassLoaderAwareObjectInputStream(bais,classLoader);
    return is.readObject();
  }
 catch (  Exception e) {
    _log.error(e.getMessage());
  }
  return null;
}
