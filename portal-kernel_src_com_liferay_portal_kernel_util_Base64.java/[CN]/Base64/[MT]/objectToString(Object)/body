{
  if (o == null) {
    return null;
  }
  UnsyncByteArrayOutputStream ubaos=new UnsyncByteArrayOutputStream(32000);
  try {
    ObjectOutputStream os=new ObjectOutputStream(ubaos);
    os.flush();
    os.writeObject(o);
    os.flush();
  }
 catch (  IOException e) {
    _log.error(e,e);
  }
  return encode(ubaos.toByteArray());
}
