{
  if (o == null) {
    return null;
  }
  ByteArrayOutputStream baos=new ByteArrayOutputStream(32000);
  try {
    ObjectOutputStream os=new ObjectOutputStream(new BufferedOutputStream(baos));
    os.flush();
    os.writeObject(o);
    os.flush();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return encode(baos.toByteArray());
}
