{
  if (o == null) {
    return null;
  }
  UnsyncByteArrayOutputStream baos=new UnsyncByteArrayOutputStream(32000);
  try {
    ObjectOutputStream os=new ObjectOutputStream(baos);
    os.flush();
    os.writeObject(o);
    os.flush();
  }
 catch (  IOException e) {
    _log.error(e.getMessage());
  }
  return encode(baos.toByteArray());
}
