{
  if (o == null) {
    return null;
  }
  ByteArrayMaker bam=new ByteArrayMaker(32000);
  try {
    ObjectOutputStream os=new ObjectOutputStream(new BufferedOutputStream(bam));
    os.flush();
    os.writeObject(o);
    os.flush();
  }
 catch (  IOException e) {
    _log.error(e.getMessage());
  }
  return encode(bam.toByteArray());
}
