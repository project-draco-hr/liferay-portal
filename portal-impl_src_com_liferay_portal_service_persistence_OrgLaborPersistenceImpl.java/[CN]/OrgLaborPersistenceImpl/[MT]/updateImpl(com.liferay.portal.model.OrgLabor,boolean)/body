{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,orgLabor,merge);
    orgLabor.setNew(false);
    return orgLabor;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(OrgLabor.class.getName());
  }
}
