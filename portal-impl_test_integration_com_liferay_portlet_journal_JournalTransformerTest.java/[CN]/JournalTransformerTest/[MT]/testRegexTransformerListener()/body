{
  Map<String,String> tokens=getTokens();
  String xml=getDefaultContent();
  String script="Hello $name.getData(), Welcome to beta.sample.com.";
  String content=JournalUtil.transform(null,tokens,Constants.VIEW,"en_US",xml,script,JournalTemplateConstants.LANG_TYPE_VM);
  Assert.assertEquals("Hello Joe Bloggs, Welcome to production.sample.com.",content);
}
