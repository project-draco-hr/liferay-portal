{
  Map<String,String> tokens=getTokens();
  String xml=getDefaultContent();
  String script="${name.getData()} - ${viewMode}";
  String content=JournalUtil.transform(null,tokens,Constants.PRINT,"en_US",xml,script,JournalTemplateConstants.LANG_TYPE_FTL);
  Assert.assertEquals("Joe Bloggs - print",content);
}
