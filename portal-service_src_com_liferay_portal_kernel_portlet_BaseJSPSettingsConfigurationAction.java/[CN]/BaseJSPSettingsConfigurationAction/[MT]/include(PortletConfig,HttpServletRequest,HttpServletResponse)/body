{
  ServletContext servletContext=getServletContext(request);
  RequestDispatcher requestDispatcher=servletContext.getRequestDispatcher(getJspPath(request));
  try {
    requestDispatcher.include(request,response);
  }
 catch (  ServletException se) {
    _log.error("Unable to include JSP " + getJspPath(request),se);
    throw new IOException("Unable to include " + getJspPath(request),se);
  }
}
