{
  ServletContext servletContext=getServletContext(PortalUtil.getHttpServletRequest(renderRequest));
  RequestDispatcher requestDispatcher=servletContext.getRequestDispatcher(getJspPath(renderRequest));
  try {
    requestDispatcher.include(PortalUtil.getHttpServletRequest(renderRequest),PortalUtil.getHttpServletResponse(renderResponse));
  }
 catch (  ServletException se) {
    if (_log.isErrorEnabled()) {
      _log.error("Unable to include JSP",se);
    }
    throw new IOException("Unable to include " + getJspPath(renderRequest),se);
  }
}
