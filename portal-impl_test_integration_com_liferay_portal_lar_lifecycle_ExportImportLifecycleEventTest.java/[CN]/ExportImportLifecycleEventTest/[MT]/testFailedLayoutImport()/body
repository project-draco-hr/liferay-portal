{
  LayoutImporter layoutImporter=LayoutImporter.getInstance();
  try {
    layoutImporter.importLayouts(TestPropsValues.getUserId(),0,false,StagingUtil.getStagingParameters(),null);
  }
 catch (  Throwable t) {
    if (_log.isInfoEnabled()) {
      _log.info(t,t);
    }
  }
  Assert.assertTrue(_firedExportImportLifecycleEventsMap.containsKey(ExportImportLifecycleConstants.EVENT_LAYOUT_IMPORT_FAILED));
}
