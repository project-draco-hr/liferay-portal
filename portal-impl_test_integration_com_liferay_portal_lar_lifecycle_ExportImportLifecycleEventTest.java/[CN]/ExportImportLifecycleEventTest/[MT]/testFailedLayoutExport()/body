{
  LayoutExporter layoutExporter=LayoutExporter.getInstance();
  try {
    layoutExporter.exportLayoutsAsFile(0,false,new long[0],StagingUtil.getStagingParameters(),new Date(),new Date());
  }
 catch (  Throwable t) {
  }
  Assert.assertTrue(_firedExportImportLifecycleEventsMap.containsKey(ExportImportLifecycleConstants.EVENT_LAYOUT_EXPORT_FAILED));
}
