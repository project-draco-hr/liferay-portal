{
  try {
    StagingUtil.publishLayouts(TestPropsValues.getUserId(),_group.getGroupId(),RandomTestUtil.nextInt(),false,new long[0],StagingUtil.getStagingParameters(),new Date(),new Date());
  }
 catch (  Throwable t) {
    if (_log.isInfoEnabled()) {
      _log.info(t,t);
    }
  }
  Assert.assertTrue(_firedExportImportLifecycleEventsMap.containsKey(ExportImportLifecycleConstants.EVENT_PUBLICATION_LAYOUT_LOCAL_FAILED));
}
