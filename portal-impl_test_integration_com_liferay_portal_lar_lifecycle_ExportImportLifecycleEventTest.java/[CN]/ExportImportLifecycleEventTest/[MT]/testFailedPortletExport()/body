{
  PortletExporter portletExporter=PortletExporter.getInstance();
  try {
    portletExporter.exportPortletInfoAsFile(RandomTestUtil.nextLong(),_group.getGroupId(),StringPool.BLANK,StagingUtil.getStagingParameters(),new Date(),new Date());
  }
 catch (  Throwable t) {
  }
  Assert.assertTrue(_firedExportImportLifecycleEventsMap.containsKey(ExportImportLifecycleConstants.EVENT_PORTLET_EXPORT_FAILED));
}
