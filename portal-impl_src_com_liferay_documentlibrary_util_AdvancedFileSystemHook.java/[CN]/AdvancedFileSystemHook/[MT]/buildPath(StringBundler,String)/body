{
  if (fileNameFragment.length() <= 2) {
    return;
  }
  if (getDepth(sb.toString()) > 3) {
    return;
  }
  for (int i=0; i < fileNameFragment.length(); i+=2) {
    if ((i + 2) < fileNameFragment.length()) {
      sb.append(fileNameFragment.substring(i,i + 2));
      sb.append(StringPool.SLASH);
      if (getDepth(sb.toString()) > 3) {
        return;
      }
    }
  }
  return;
}
