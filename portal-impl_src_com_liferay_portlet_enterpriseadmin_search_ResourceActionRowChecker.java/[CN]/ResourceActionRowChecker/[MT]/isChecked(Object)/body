{
  Object[] objArray=(Object[])obj;
  Role role=(Role)objArray[0];
  String actionId=(String)objArray[1];
  String resourceName=(String)objArray[2];
  Integer scope=(Integer)objArray[4];
  boolean selected=false;
  try {
    if (PropsValues.PERMISSIONS_USER_CHECK_ALGORITHM == 6) {
      selected=ResourcePermissionLocalServiceUtil.hasScopeResourcePermission(role.getCompanyId(),resourceName,scope,role.getRoleId(),actionId);
    }
 else {
      selected=PermissionLocalServiceUtil.hasRolePermission(role.getRoleId(),role.getCompanyId(),resourceName,scope,actionId);
    }
  }
 catch (  Exception e) {
  }
  return selected;
}
