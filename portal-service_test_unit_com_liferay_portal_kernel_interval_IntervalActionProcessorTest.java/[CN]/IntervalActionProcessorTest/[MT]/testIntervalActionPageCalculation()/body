{
  final IntervalActionProcessor intervalActionProcessor=new IntervalActionProcessor(125);
  intervalActionProcessor.setPerformIntervalActionMethod(new IntervalActionProcessor.PerformIntervalActionMethod(){
    @Override public void performIntervalAction(    int start,    int end){
      _count.incrementAndGet();
      intervalActionProcessor.incrementStart(end - start);
    }
  }
);
  intervalActionProcessor.performIntervalActions();
  Assert.assertEquals(2,_count.get());
}
