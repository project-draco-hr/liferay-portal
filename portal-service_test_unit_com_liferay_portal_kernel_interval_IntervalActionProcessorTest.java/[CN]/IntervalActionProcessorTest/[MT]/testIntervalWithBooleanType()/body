{
  final IntervalActionProcessor<Boolean> intervalActionProcessor=new IntervalActionProcessor<>(3,1);
  intervalActionProcessor.setPerformIntervalActionMethod(new IntervalActionProcessor.PerformIntervalActionMethod<Boolean>(){
    @Override public Boolean performIntervalAction(    int start,    int end){
      if ((start == 1) && (end == 2)) {
        return true;
      }
      intervalActionProcessor.incrementStart();
      return null;
    }
  }
);
  Assert.assertTrue(intervalActionProcessor.performIntervalActions());
}
