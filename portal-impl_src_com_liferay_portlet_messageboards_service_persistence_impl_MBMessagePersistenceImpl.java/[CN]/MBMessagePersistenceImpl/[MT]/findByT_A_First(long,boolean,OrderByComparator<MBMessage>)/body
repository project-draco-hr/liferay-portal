{
  MBMessage mbMessage=fetchByT_A_First(threadId,answer,orderByComparator);
  if (mbMessage != null) {
    return mbMessage;
  }
  StringBundler msg=new StringBundler(6);
  msg.append(_NO_SUCH_ENTITY_WITH_KEY);
  msg.append("threadId=");
  msg.append(threadId);
  msg.append(", answer=");
  msg.append(answer);
  msg.append(StringPool.CLOSE_CURLY_BRACE);
  throw new NoSuchMessageException(msg.toString());
}
