{
  String[] listenerClassNames=StringUtil.split(GetterUtil.getString(com.liferay.portal.util.PropsUtil.get("value.object.listener.com.liferay.portlet.messageboards.model.MBMessage")));
  if (listenerClassNames.length > 0) {
    try {
      List<ModelListener<MBMessage>> listenersList=new ArrayList<ModelListener<MBMessage>>();
      for (      String listenerClassName : listenerClassNames) {
        listenersList.add((ModelListener<MBMessage>)InstanceFactory.newInstance(getClassLoader(),listenerClassName));
      }
      listeners=listenersList.toArray(new ModelListener[listenersList.size()]);
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
}
