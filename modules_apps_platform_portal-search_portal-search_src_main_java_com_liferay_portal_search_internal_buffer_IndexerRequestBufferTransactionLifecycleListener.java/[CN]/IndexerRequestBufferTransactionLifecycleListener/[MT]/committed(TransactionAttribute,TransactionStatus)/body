{
  IndexerRequestBuffer indexerRequestBuffer=IndexerRequestBuffer.remove();
  if ((indexerRequestBuffer != null) && !indexerRequestBuffer.isEmpty()) {
    IndexerRequestBufferExecutor indexerRequestBufferExecutor=_indexerRequestBufferExecutorWatcher.getIndexerRequestBufferExecutor();
    indexerRequestBufferExecutor.execute(indexerRequestBuffer);
  }
}
