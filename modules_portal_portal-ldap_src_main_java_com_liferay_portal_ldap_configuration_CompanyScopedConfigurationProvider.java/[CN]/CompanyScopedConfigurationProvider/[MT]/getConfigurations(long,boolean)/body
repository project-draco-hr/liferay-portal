{
  List<Dictionary<String,Object>> configurationsProperties=getConfigurationsProperties(companyId,useDefault);
  if (ListUtil.isEmpty(configurationsProperties)) {
    return Collections.emptyList();
  }
  List<T> configurables=new ArrayList<>(configurationsProperties.size());
  for (  Dictionary<String,Object> configurationProperties : configurationsProperties) {
    T configurable=Configurable.createConfigurable(getMetatype(),configurationProperties);
    configurables.add(configurable);
  }
  return configurables;
}
