{
  Destination destination=new SerialDestination(DestinationNames.LAYOUTS_PUBLISHER);
  MessageBusUtil.addDestination(destination);
  destination.register(new LayoutsPublisherMessageListener());
  destination=new SerialDestination(DestinationNames.LAYOUTS_REMOTE_EXPORTER);
  MessageBusUtil.addDestination(destination);
  destination.register(new LayoutsRemoteExporterMessageListener());
}
