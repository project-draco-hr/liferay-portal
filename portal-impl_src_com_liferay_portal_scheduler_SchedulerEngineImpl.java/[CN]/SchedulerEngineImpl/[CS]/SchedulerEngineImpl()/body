{
  Destination layoutsLocalPublisherDestination=new SerialDestination(DestinationNames.LAYOUTS_LOCAL_PUBLISHER);
  MessageBusUtil.addDestination(layoutsLocalPublisherDestination);
  layoutsLocalPublisherDestination.register(new LayoutsLocalPublisherMessageListener());
  Destination layoutsRemotePublisherDestination=new SerialDestination(DestinationNames.LAYOUTS_REMOTE_PUBLISHER);
  MessageBusUtil.addDestination(layoutsRemotePublisherDestination);
  layoutsRemotePublisherDestination.register(new LayoutsRemotePublisherMessageListener());
}
