{
  SocialActivitySet socialActivitySet=findByPrimaryKey(activitySetId);
  Session session=null;
  try {
    session=openSession();
    SocialActivitySet[] array=new SocialActivitySetImpl[3];
    array[0]=getByCompanyId_PrevAndNext(session,socialActivitySet,companyId,orderByComparator,true);
    array[1]=socialActivitySet;
    array[2]=getByCompanyId_PrevAndNext(session,socialActivitySet,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
