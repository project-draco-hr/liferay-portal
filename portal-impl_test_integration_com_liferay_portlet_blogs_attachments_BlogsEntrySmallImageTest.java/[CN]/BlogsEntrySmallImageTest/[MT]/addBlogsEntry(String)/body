{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(group.getGroupId(),user.getUserId());
  FileEntry fileEntry=getTempFileEntry(user.getUserId(),imageTitle,serviceContext);
  ImageSelector imageSelector=new ImageSelector(FileUtil.getBytes(fileEntry.getContentStream()),fileEntry.getTitle(),fileEntry.getMimeType(),StringPool.BLANK);
  return addBlogsEntry(imageSelector);
}
