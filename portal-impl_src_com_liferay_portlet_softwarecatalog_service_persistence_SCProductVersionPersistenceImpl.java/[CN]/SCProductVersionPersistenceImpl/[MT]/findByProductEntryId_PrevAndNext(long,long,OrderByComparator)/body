{
  SCProductVersion scProductVersion=findByPrimaryKey(productVersionId);
  Session session=null;
  try {
    session=openSession();
    SCProductVersion[] array=new SCProductVersionImpl[3];
    array[0]=getByProductEntryId_PrevAndNext(session,scProductVersion,productEntryId,orderByComparator,true);
    array[1]=scProductVersion;
    array[2]=getByProductEntryId_PrevAndNext(session,scProductVersion,productEntryId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
