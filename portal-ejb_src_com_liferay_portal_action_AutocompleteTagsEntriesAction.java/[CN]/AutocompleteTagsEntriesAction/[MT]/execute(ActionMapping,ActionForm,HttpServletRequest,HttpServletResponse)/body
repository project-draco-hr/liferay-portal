{
  try {
    String companyId=PortalUtil.getCompanyId(req);
    String name=DAOParamUtil.getLike(req,"value");
    String[] properties=new String[0];
    int begin=0;
    int end=20;
    String xml=TagsEntryLocalServiceUtil.searchAutocomplete(companyId,name,properties,begin,end);
    ServletResponseUtil.sendFile(res,null,xml.getBytes(),Constants.TEXT_XML);
    return null;
  }
 catch (  Exception e) {
    req.setAttribute(PageContext.EXCEPTION,e);
    return mapping.findForward(Constants.COMMON_ERROR);
  }
}
