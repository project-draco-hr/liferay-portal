{
  WorkflowDefinitionLink workflowDefinitionLink=fetchByPrimaryKey(workflowDefinitionLinkId);
  if (workflowDefinitionLink == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + workflowDefinitionLinkId);
    }
    throw new NoSuchWorkflowDefinitionLinkException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + workflowDefinitionLinkId);
  }
  return workflowDefinitionLink;
}
