{
  String tableNameFieldName=getTableNameFieldName();
  return "select c." + tableNameFieldName + " from "+ "information_schema.columns c where c.table_schema = '"+ schema+ "' and c."+ tableNameFieldName+ " not in ("+ getPartitionedTableNamesSQL(schema)+ ") group by c."+ tableNameFieldName+ " order by c."+ tableNameFieldName;
}
