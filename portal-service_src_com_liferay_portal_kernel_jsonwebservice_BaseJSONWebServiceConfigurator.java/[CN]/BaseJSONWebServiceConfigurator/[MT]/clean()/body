{
  int count=JSONWebServiceActionsManagerUtil.unregisterJSONWebServiceActions(_servletContextPath);
  _registeredActionsCount-=count;
  if (_log.isDebugEnabled()) {
    if (count != 0) {
      _log.debug("Removed " + count + " existing JSON Web Service actions that belonged to "+ _servletContextPath);
    }
  }
}
