{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalArticle.class,PACLClassLoaderUtil.getPortalClassLoader());
  Property property=PropertyFactoryUtil.forName("id");
  dynamicQuery.add(property.ge(startId));
  dynamicQuery.add(property.lt(endId));
  addReindexCriteria(dynamicQuery,companyId,JournalArticleConstants.VERSION_DEFAULT,WorkflowConstants.STATUS_APPROVED);
  return JournalArticleLocalServiceUtil.dynamicQuery(dynamicQuery);
}
