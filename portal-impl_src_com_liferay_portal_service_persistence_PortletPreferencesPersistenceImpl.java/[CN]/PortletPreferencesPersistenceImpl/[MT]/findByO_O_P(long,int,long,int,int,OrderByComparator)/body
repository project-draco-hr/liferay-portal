  Object result=FinderCache.getResult(finderClassName,finderMethodName,finderParams,finderArgs,getSessionFactory());
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringMaker query=new StringMaker();
      query.append("FROM com.liferay.portal.model.PortletPreferences WHERE ");
      query.append("ownerId = ?");
      query.append(" AND ");
      query.append("ownerType = ?");
      query.append(" AND ");
      query.append("plid = ?");
      query.append(" ");
      if (obc != null) {
        query.append("ORDER BY ");
        query.append(obc.getOrderBy());
      }
      Query q=session.createQuery(query.toString());
      int queryPos=0;
      q.setLong(queryPos++,ownerId);
      q.setInteger(queryPos++,ownerType);
      q.setLong(queryPos++,plid);
