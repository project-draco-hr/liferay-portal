{
  PortletPreferences result=(PortletPreferences)EntityCacheUtil.getResult(PortletPreferencesModelImpl.ENTITY_CACHE_ENABLED,PortletPreferences.class,portletPreferencesId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      PortletPreferences portletPreferences=(PortletPreferences)session.get(PortletPreferencesImpl.class,new Long(portletPreferencesId));
      if (portletPreferences != null) {
        cacheResult(portletPreferences);
      }
      return portletPreferences;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (PortletPreferences)result;
  }
}
