{
  PortletPreferences portletPreferences=(PortletPreferences)EntityCacheUtil.getResult(PortletPreferencesModelImpl.ENTITY_CACHE_ENABLED,PortletPreferencesImpl.class,portletPreferencesId,this);
  if (portletPreferences == null) {
    Session session=null;
    try {
      session=openSession();
      portletPreferences=(PortletPreferences)session.get(PortletPreferencesImpl.class,new Long(portletPreferencesId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (portletPreferences != null) {
        cacheResult(portletPreferences);
      }
      closeSession(session);
    }
  }
  return portletPreferences;
}
