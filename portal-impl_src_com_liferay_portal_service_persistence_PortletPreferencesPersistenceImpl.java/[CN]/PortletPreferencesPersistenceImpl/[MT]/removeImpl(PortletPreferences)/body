{
  Session session=null;
  try {
    session=openSession();
    session.delete(portletPreferences);
    session.flush();
    return portletPreferences;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(PortletPreferences.class.getName());
  }
}
