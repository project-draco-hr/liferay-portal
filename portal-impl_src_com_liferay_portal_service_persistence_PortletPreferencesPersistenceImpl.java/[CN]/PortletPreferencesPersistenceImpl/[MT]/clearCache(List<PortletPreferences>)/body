{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  PortletPreferences portletPreferences : portletPreferenceses) {
    EntityCacheUtil.removeResult(PortletPreferencesModelImpl.ENTITY_CACHE_ENABLED,PortletPreferencesImpl.class,portletPreferences.getPrimaryKey());
    clearUniqueFindersCache(portletPreferences);
  }
}
