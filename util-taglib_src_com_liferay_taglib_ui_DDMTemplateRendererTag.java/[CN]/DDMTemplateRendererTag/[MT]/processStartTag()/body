{
  if (_displayStyleGroupId == 0) {
    ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
    _displayStyleGroupId=themeDisplay.getScopeGroupId();
  }
  _portletDisplayDDMTemplate=PortletDisplayTemplateUtil.getPortletDisplayTemplateDDMTemplate(_displayStyleGroupId,PortalUtil.getClassNameId(_className),_displayStyle,true);
  if (_portletDisplayDDMTemplate != null) {
    return SKIP_BODY;
  }
  return EVAL_BODY_INCLUDE;
}
