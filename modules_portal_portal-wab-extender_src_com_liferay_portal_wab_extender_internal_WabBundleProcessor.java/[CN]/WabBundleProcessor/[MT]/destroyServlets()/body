{
  Map<String,ServletDefinition> servletDefinitions=_webXMLDefinition.getServletDefinitions();
  List<ServletDefinition> servletDefinitionsList=new ArrayList<ServletDefinition>(servletDefinitions.values());
  Collections.reverse(servletDefinitionsList);
  for (  ServletDefinition servletDefinition : servletDefinitionsList) {
    Servlet servlet=servletDefinition.getServlet();
    try {
      _extendedHttpService.unregisterServlet(servlet,_contextName);
    }
 catch (    Exception e) {
      _logger.log(Logger.LOG_ERROR,e.getMessage(),e);
    }
  }
}
