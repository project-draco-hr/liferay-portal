{
  Map<String,String> contextParameters=_webXMLDefinition.getContextParameters();
  _wabServletContextHelper=new WabServletContextHelper(_bundle);
  Dictionary<String,Object> properties=new Hashtable<String,Object>();
  properties.put(HttpWhiteboardConstants.HTTP_WHITEBOARD_CONTEXT_NAME,_contextName);
  properties.put(HttpWhiteboardConstants.HTTP_WHITEBOARD_CONTEXT_PATH,_contextPath);
  properties.put(Constants.SERVICE_RANKING,1000);
  for (  String key : contextParameters.keySet()) {
    properties.put(key,contextParameters.get(key));
  }
  _serviceRegistration=_bundleContext.registerService(ServletContextHelper.class,_wabServletContextHelper,properties);
}
