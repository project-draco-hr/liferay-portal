{
  Set<Entry<String,LayoutTypeController>> entrySet=_defaultLayoutTypeControllers.entrySet();
  for (  Entry<String,LayoutTypeController> entry : entrySet) {
    Map<String,Object> properties=new HashMap<String,Object>();
    properties.put("layout.type",entry.getKey());
    registry.registerService(LayoutTypeController.class,entry.getValue(),properties);
  }
}
