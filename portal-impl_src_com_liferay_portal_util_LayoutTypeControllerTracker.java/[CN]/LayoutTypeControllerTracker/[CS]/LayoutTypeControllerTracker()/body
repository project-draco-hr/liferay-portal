{
  for (  String type : _LAYOUT_TYPES) {
    _defaultLayoutTypeControllers.put(type,new DefaultLayoutTypeController(type));
  }
  Registry registry=RegistryUtil.getRegistry();
  _registerDefaults(registry);
  Filter filter=registry.getFilter("(&(layout.type=*)(objectClass=" + LayoutTypeController.class.getName() + "))");
  _serviceTracker=registry.trackServices(filter,new LayoutTypeControllerServiceTrackerCustomizer());
  _serviceTracker.open();
}
