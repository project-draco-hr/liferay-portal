{
  boolean emailEntryAddedEnabled=ParamUtil.getBoolean(actionRequest,"emailEntryAddedEnabled");
  String emailEntryAddedSubject=ParamUtil.getString(actionRequest,"emailEntryAddedSubject");
  String emailEntryAddedBody=ParamUtil.getString(actionRequest,"emailEntryAddedBody");
  if (Validator.isNull(emailEntryAddedSubject)) {
    SessionErrors.add(actionRequest,"emailEntryAddedSubject");
  }
 else   if (Validator.isNull(emailEntryAddedBody)) {
    SessionErrors.add(actionRequest,"emailEntryAddedBody");
  }
 else {
    preferences.setValue("email-entry-added-enabled",String.valueOf(emailEntryAddedEnabled));
    preferences.setValue("email-entry-added-subject",emailEntryAddedSubject);
    preferences.setValue("email-entry-added-body",emailEntryAddedBody);
  }
}
