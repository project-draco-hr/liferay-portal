{
  MarkerThreadPoolHandler markerThreadPoolHandler=new MarkerThreadPoolHandler();
  ThreadPoolExecutor threadPoolExecutor=new ThreadPoolExecutor(1,2,TestUtil.KEEPALIVE_TIME,TimeUnit.MILLISECONDS,true,3,new CallerRunsPolicy(),Executors.defaultThreadFactory(),markerThreadPoolHandler);
  threadPoolExecutor.shutdown();
  MarkerBlockingJob markerBlockingJob=new MarkerBlockingJob();
  threadPoolExecutor.execute(markerBlockingJob);
  assertFalse(markerBlockingJob.isStarted());
  assertFalse(markerThreadPoolHandler.isBeforeExecuteRan());
  assertFalse(markerThreadPoolHandler.isAfterExecuteRan());
}
