{
  Cookie[] cookies=request.getCookies();
  if (cookies == null) {
    return null;
  }
  for (int i=0; i < cookies.length; i++) {
    Cookie cookie=cookies[i];
    String cookieName=GetterUtil.getString(cookie.getName());
    if (cookieName.equalsIgnoreCase(name)) {
      return cookie.getValue();
    }
  }
  return null;
}
