{
  if (cookie == null) {
    return "";
  }
  tag=tag + "=";
  if (cookie.startsWith(tag)) {
    int y=cookie.indexOf(';');
    return cookie.substring(tag.length(),y);
  }
  tag=";" + tag;
  int x=cookie.indexOf(tag);
  if (x != -1) {
    int y=cookie.indexOf(';',x + 1);
    return cookie.substring(x + tag.length(),y);
  }
  return "";
}
