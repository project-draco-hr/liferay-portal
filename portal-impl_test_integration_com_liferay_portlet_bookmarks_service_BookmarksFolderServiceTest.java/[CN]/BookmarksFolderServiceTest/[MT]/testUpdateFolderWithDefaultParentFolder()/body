{
  BookmarksFolder folder=BookmarksTestUtil.addFolder(_group.getGroupId(),ServiceTestUtil.randomString());
  String initialName=folder.getName();
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext();
  folder=BookmarksFolderLocalServiceUtil.updateFolder(folder.getUserId(),folder.getFolderId(),folder.getParentFolderId(),ServiceTestUtil.randomString(),ServiceTestUtil.randomString(50),false,serviceContext);
  Assert.assertNotEquals(initialName,folder.getName());
  Assert.assertEquals(BookmarksFolderConstants.DEFAULT_PARENT_FOLDER_ID,folder.getParentFolderId());
}
