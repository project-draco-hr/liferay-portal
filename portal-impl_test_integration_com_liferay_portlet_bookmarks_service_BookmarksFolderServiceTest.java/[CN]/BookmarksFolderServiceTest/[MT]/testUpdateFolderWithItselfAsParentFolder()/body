{
  BookmarksFolder folder=BookmarksTestUtil.addFolder(_group.getGroupId(),ServiceTestUtil.randomString());
  String initialName=folder.getName();
  long initialParentFolderId=folder.getParentFolderId();
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext();
  folder=BookmarksFolderLocalServiceUtil.updateFolder(folder.getUserId(),folder.getFolderId(),folder.getFolderId(),ServiceTestUtil.randomString(),ServiceTestUtil.randomString(50),false,serviceContext);
  Assert.assertNotEquals(initialName,folder.getName());
  Assert.assertEquals(initialParentFolderId,folder.getParentFolderId());
}
