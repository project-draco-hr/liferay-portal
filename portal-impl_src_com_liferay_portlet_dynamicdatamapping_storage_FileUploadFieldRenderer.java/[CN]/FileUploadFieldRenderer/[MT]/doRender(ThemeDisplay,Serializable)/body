{
  if (Validator.isNull(fieldValue) || fieldValue.equals(JSONFactoryUtil.getNullJSON())) {
    return StringPool.BLANK;
  }
  JSONObject fieldValueJSON=null;
  try {
    fieldValueJSON=JSONFactoryUtil.createJSONObject(String.valueOf(fieldValue));
  }
 catch (  JSONException e) {
    if (_log.isDebugEnabled()) {
      _log.debug("Unable to parse JSON",e);
    }
    return StringPool.BLANK;
  }
  return fieldValueJSON.getString("name");
}
