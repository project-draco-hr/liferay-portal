{
  ResourcePermissionChecker resourcePermissionChecker=_serviceTrackerMap.getService(className);
  if (resourcePermissionChecker == null) {
    return null;
  }
  Boolean resource=resourcePermissionChecker.checkResource(permissionChecker,classPK,actionId);
  if (resource != null) {
    return resource.booleanValue();
  }
  return null;
}
