{
  validate(fileName);
  try {
    if (((PropsValues.WEBDAV_LITMUS) || (PropsValues.DL_FILE_MAX_SIZE > 0)) && ((is == null) || (is.available() > PropsValues.DL_FILE_MAX_SIZE))) {
      throw new FileSizeException(fileName);
    }
  }
 catch (  IOException ioe) {
    throw new FileSizeException(ioe.getMessage());
  }
}
