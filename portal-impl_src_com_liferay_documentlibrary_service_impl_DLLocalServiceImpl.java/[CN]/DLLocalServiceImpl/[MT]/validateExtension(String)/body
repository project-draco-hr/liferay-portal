{
  boolean validFileExtension=false;
  String[] fileExtensions=PrefsPropsUtil.getStringArray(PropsKeys.DL_FILE_EXTENSIONS,StringPool.COMMA);
  if (!PropsValues.WEBDAV_LITMUS) {
    for (int i=0; i < fileExtensions.length; i++) {
      if (StringPool.STAR.equals(fileExtensions[i]) || StringUtil.endsWith(fileName,fileExtensions[i])) {
        validFileExtension=true;
        break;
      }
    }
    if (!validFileExtension) {
      throw new FileNameException(fileName);
    }
  }
}
