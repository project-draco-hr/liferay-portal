{
  StringMaker sm=new StringMaker();
  sm.append("(AnnouncementsEntry.classNameId = ?) AND (");
  for (int i=0; i < classPKs.length; i++) {
    sm.append("(AnnouncementsEntry.classPK = ?)");
    if ((i + 1) < classPKs.length) {
      sm.append(" OR ");
    }
 else {
      sm.append(")");
    }
  }
  return sm.toString();
}
