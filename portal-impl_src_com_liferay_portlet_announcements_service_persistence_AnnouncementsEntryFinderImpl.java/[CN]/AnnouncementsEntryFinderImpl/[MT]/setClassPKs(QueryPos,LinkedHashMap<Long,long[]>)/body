{
  if (scopes == null) {
    return;
  }
  Iterator<Map.Entry<Long,long[]>> itr=scopes.entrySet().iterator();
  while (itr.hasNext()) {
    Map.Entry<Long,long[]> entry=itr.next();
    Long classNameId=entry.getKey();
    long[] classPKs=entry.getValue();
    setClassPKs(qPos,classNameId.longValue(),classPKs);
  }
}
