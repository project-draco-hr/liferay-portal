{
  if (_rssEnabled != null) {
    return _rssEnabled;
  }
  LiferayPortletRequest liferayPortletRequest=getLiferayPortletRequest();
  PortletPreferences preferences=liferayPortletRequest.getPreferences();
  if (PortalUtil.isRSSFeedsEnabled()) {
    _rssEnabled=GetterUtil.getBoolean(preferences.getValue("enableRss",null),true);
  }
 else {
    _rssEnabled=false;
  }
  return _rssEnabled;
}
