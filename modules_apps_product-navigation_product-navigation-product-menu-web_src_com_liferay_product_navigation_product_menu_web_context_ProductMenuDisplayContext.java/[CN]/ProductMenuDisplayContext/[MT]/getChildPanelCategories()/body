{
  if (_childPanelCategories != null) {
    return _childPanelCategories;
  }
  ThemeDisplay themeDisplay=(ThemeDisplay)_portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  _childPanelCategories=_panelCategoryRegistry.getChildPanelCategories(PanelCategoryKeys.ROOT,themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroup());
  return _childPanelCategories;
}
