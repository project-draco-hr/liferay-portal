{
  ThemeDisplay themeDislay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long userId=themeDislay.getUserId();
  long[] restoreEntryIds=StringUtil.split(ParamUtil.getString(actionRequest,"trashedEntryIds"),0L);
  for (  long restoreEntryId : restoreEntryIds) {
    BlogsEntryLocalServiceUtil.restoreEntryFromTrash(userId,restoreEntryId);
  }
}
