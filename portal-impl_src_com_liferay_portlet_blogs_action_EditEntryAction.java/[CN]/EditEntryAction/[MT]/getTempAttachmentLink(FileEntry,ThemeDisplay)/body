{
  StringBundler sb=new StringBundler(8);
  sb.append("<img ");
  sb.append(EditorConstants.DATA_IMAGE_ID_ATTRIBUTE);
  sb.append("=\"");
  sb.append(tempAttachment.getFileEntryId());
  sb.append("\" ");
  sb.append("src=\"");
  sb.append(PortletFileRepositoryUtil.getPortletFileEntryURL(themeDisplay,tempAttachment,StringPool.BLANK));
  sb.append("\" />");
  return sb.toString();
}
