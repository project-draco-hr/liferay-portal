{
  List<FileEntry> fileEntryAttachments=new ArrayList<>();
  Pattern pattern=Pattern.compile(EditorConstants.DATA_IMAGE_ID_ATTRIBUTE + "=.(\\d+)");
  Matcher matcher=pattern.matcher(content);
  while (matcher.find()) {
    String fileEntryId=matcher.group(1);
    FileEntry fileEntryAttachment=DLAppServiceUtil.getFileEntry(Long.valueOf(fileEntryId));
    fileEntryAttachments.add(fileEntryAttachment);
  }
  return fileEntryAttachments;
}
