{
  List<FileEntry> attachments=new ArrayList<>();
  Pattern pattern=Pattern.compile(EditorConstants.DATA_IMAGE_ID_ATTRIBUTE + "=.(\\d+)");
  Matcher matcher=pattern.matcher(content);
  while (matcher.find()) {
    String fileEntryId=matcher.group(1);
    FileEntry attachment=DLAppServiceUtil.getFileEntry(Long.valueOf(fileEntryId));
    attachments.add(attachment);
  }
  return attachments;
}
