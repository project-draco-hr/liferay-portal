{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Control Panel");
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Portal Settings",RuntimeVariables.replace("Portal Settings"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForVisible("//a[@id='_130_addressesLink']");
  selenium.clickAt("//a[@id='_130_addressesLink']",RuntimeVariables.replace("Addresses"));
  selenium.waitForVisible("//div[2]/span/span/button[1]");
  selenium.clickAt("//div[2]/span/span/button[1]",RuntimeVariables.replace("Add Row"));
  selenium.waitForVisible("//input[@id='_130_addressStreet1_3']");
  selenium.type("//input[@id='_130_addressStreet1_3']",RuntimeVariables.replace("123. Liferay Ln."));
  selenium.type("//input[@id='_130_addressCity3']",RuntimeVariables.replace("Rays of Light"));
  selenium.type("//input[@id='_130_addressZip3']",RuntimeVariables.replace("12345"));
  selenium.select("//select[@id='_130_addressTypeId3']",RuntimeVariables.replace("label=Billing"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForVisible("//div[@class='portlet-msg-success']");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  selenium.waitForVisible("//input[@id='_130_addressStreet1_2']");
  assertEquals("123. Liferay Ln.",selenium.getValue("//input[@id='_130_addressStreet1_2']"));
  assertEquals("Rays of Light",selenium.getValue("//input[@id='_130_addressCity2']"));
  assertEquals("12345",selenium.getValue("//input[@id='_130_addressZip2']"));
  assertEquals("Billing",selenium.getSelectedLabel("//select[@id='_130_addressTypeId2']"));
}
