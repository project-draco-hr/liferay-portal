{
  if (isEscapedModel()) {
    return (WikiPageResource)this;
  }
 else {
    WikiPageResource model=new WikiPageResourceImpl();
    model.setEscapedModel(true);
    model.setResourcePrimKey(getResourcePrimKey());
    model.setNodeId(getNodeId());
    model.setTitle(Html.escape(getTitle()));
    model=(WikiPageResource)Proxy.newProxyInstance(WikiPageResource.class.getClassLoader(),new Class[]{WikiPageResource.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
