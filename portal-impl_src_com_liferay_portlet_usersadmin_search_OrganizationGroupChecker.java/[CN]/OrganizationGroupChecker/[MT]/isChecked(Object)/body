{
  Organization organization=(Organization)obj;
  try {
    return (OrganizationLocalServiceUtil.hasGroupOrganization(_group.getGroupId(),organization.getOrganizationId()) || _group.getOrganizationId() == organization.getOrganizationId());
  }
 catch (  Exception e) {
    _log.error(e,e);
    return false;
  }
}
