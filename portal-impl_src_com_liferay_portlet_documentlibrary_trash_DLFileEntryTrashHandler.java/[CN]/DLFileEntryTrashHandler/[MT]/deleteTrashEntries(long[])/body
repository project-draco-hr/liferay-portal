{
  for (int i=0; i < classPKs.length; i++) {
    try {
      TrashEntry trashEntry=TrashEntryLocalServiceUtil.getTrashEntry(classPKs[i]);
      TrashEntryLocalServiceUtil.deleteTrashEntry(trashEntry.getEntryId());
      DLFileEntry dlFileEntry=DLFileEntryLocalServiceUtil.getDLFileEntry(trashEntry.getClassPK());
      DLAppServiceUtil.deleteFileEntry(dlFileEntry.getFileEntryId());
    }
 catch (    PortalException e) {
      e.printStackTrace();
    }
catch (    SystemException e) {
      e.printStackTrace();
    }
  }
}
