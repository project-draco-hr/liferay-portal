{
  try {
    DLFileEntry dlFileEntry=getDLFileEntry(classPK);
    DLFileVersion dlFileVersion=dlFileEntry.getFileVersion();
    if (dlFileEntry.isInTrashContainer() || dlFileVersion.isInTrash()) {
      return true;
    }
    return false;
  }
 catch (  InvalidRepositoryException ire) {
    return false;
  }
}
