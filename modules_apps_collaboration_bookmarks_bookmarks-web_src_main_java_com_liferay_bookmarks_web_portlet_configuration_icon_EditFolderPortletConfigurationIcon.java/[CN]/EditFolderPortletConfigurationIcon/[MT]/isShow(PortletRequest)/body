{
  try {
    BookmarksFolder folder=ActionUtil.getFolder(portletRequest);
    if (folder.getFolderId() == BookmarksFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
      return false;
    }
    ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
    if (BookmarksFolderPermissionChecker.contains(themeDisplay.getPermissionChecker(),folder,ActionKeys.UPDATE)) {
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
