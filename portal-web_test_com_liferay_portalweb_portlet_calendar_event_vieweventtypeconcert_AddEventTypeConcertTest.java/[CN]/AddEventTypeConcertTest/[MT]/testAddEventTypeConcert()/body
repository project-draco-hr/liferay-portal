{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Calendar Test Page");
  selenium.clickAt("link=Calendar Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Event']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_8_title",RuntimeVariables.replace("Caedmon's Call Concert."));
  selenium.type("_8_description",RuntimeVariables.replace("I love this band guys. Everyone should see them. I've never seen them before."));
  selenium.clickAt("_8_timeZoneSensitiveCheckbox",RuntimeVariables.replace(""));
  selenium.select("_8_type",RuntimeVariables.replace("label=Concert"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request completed successfully."));
  assertEquals(RuntimeVariables.replace("Caedmon's Call Concert."),selenium.getText("//table[@class='taglib-search-iterator']/tbody/tr[3]/td[2]/a"));
}
