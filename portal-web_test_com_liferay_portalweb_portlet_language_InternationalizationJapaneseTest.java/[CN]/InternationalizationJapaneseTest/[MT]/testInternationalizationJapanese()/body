{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//img[@alt='\u65e5\u672c\u8a9e (\u65e5\u672c)']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("//img[@alt='\u65e5\u672c\u8a9e (\u65e5\u672c)']"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//div[@id='navigation']/ul/li[3]/a/span");
  selenium.typeKeys("new_page",RuntimeVariables.replace("\u8a00\u8a9e\u30c6\u30b9\u30c8\u30da\u30fc\u30b8"));
  selenium.type("new_page",RuntimeVariables.replace("\u8a00\u8a9e\u30c6\u30b9\u30c8\u30da\u30fc\u30b8"));
