{
  ThemeDisplay themeDisplay=(ThemeDisplay)req.getAttribute(WebKeys.THEME_DISPLAY);
  long resourceId=ParamUtil.getLong(req,"resourceId");
  String[] actionIds=StringUtil.split(ParamUtil.getString(req,"guestActionIds"));
  PermissionServiceUtil.setUserPermissions(themeDisplay.getDefaultUserId(),themeDisplay.getPortletGroupId(),actionIds,resourceId);
}
