{
  Layout layout=(Layout)req.getAttribute(WebKeys.LAYOUT);
  long resourceId=ParamUtil.getLong(req,"resourceId");
  long userGroupId=ParamUtil.getLong(req,"userGroupIdsPosValue");
  String[] actionIds=StringUtil.split(ParamUtil.getString(req,"userGroupIdActionIds"));
  PermissionServiceUtil.setGroupPermissions(UserGroup.class.getName(),String.valueOf(userGroupId),layout.getGroupId(),actionIds,resourceId);
}
