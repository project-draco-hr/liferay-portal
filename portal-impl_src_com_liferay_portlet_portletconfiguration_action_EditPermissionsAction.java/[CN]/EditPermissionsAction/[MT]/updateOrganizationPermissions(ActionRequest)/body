{
  Layout layout=(Layout)req.getAttribute(WebKeys.LAYOUT);
  long resourceId=ParamUtil.getLong(req,"resourceId");
  long organizationId=ParamUtil.getLong(req,"organizationIdsPosValue");
  String[] actionIds=StringUtil.split(ParamUtil.getString(req,"organizationIdActionIds"));
  PermissionServiceUtil.setGroupPermissions(Organization.class.getName(),String.valueOf(organizationId),layout.getGroupId(),actionIds,resourceId);
}
