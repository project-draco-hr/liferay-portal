{
  super(templateResource,errorTemplateResource,templateContextHelper,TemplateConstants.LANG_TYPE_VM,PropsValues.VELOCITY_ENGINE_RESOURCE_MODIFICATION_CHECK_INTERVAL);
  _privileged=privileged;
  if (velocityContext == null) {
    _velocityContext=new VelocityContext();
  }
 else {
    _velocityContext=new VelocityContext(velocityContext);
  }
  _velocityEngine=velocityEngine;
}
