{
  super(templateResource,errorTemplateResource,templateContextHelper,TemplateManager.VELOCITY,PropsValues.VELOCITY_ENGINE_RESOURCE_MODIFICATION_CHECK_INTERVAL);
  if (velocityContext == null) {
    _velocityContext=new VelocityContext();
  }
 else {
    _velocityContext=new VelocityContext(velocityContext);
  }
  _velocityEngine=velocityEngine;
}
