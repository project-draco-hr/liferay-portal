{
  try {
    Group group=arg.getCurrentGroup();
    IGFolder folder=_getFolder(group.getGroupId(),arg.getCurrentFolder());
    IGImageServiceUtil.addImage(folder.getFolderId(),fileName,fileName,file,extension,new String[0],true,true);
  }
 catch (  Exception e) {
    throw new FCKException(e);
  }
  return "0";
}
