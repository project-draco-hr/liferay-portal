{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Announcements Test Page",RuntimeVariables.replace("Announcements Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//div[@id='dockbar']",RuntimeVariables.replace("Dockbar"));
  selenium.waitForElementPresent("//script[contains(@src,'/liferay/store.js')]");
  assertEquals(RuntimeVariables.replace("Show"),selenium.getText("//a[contains(.,'Show')]"));
  selenium.clickAt("//a[contains(.,'Show')]",RuntimeVariables.replace("Show"));
  selenium.waitForText("//a[contains(.,'Hide')]","Hide");
  assertEquals(RuntimeVariables.replace("Hide"),selenium.getText("//a[contains(.,'Hide')]"));
  assertTrue(selenium.isVisible("//p"));
  selenium.clickAt("//a[contains(.,'Hide')]",RuntimeVariables.replace("Hide"));
  selenium.waitForText("//a[contains(.,'Show')]","Show");
  assertEquals(RuntimeVariables.replace("Show"),selenium.getText("//a[contains(.,'Show')]"));
  assertFalse(selenium.isVisible("//p"));
}
