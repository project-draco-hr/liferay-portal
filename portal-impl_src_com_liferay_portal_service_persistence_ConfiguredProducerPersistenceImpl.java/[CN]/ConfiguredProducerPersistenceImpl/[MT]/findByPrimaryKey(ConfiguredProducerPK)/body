{
  ConfiguredProducer configuredProducer=fetchByPrimaryKey(configuredProducerPK);
  if (configuredProducer == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ConfiguredProducer exists with the primary key " + configuredProducerPK);
    }
    throw new NoSuchConfiguredProducerException("No ConfiguredProducer exists with the primary key " + configuredProducerPK);
  }
  return configuredProducer;
}
