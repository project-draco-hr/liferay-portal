{
  Session session=null;
  try {
    session=openSession();
    session.delete(configuredProducer);
    session.flush();
    return configuredProducer;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ConfiguredProducer.class.getName());
  }
}
