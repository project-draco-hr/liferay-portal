{
  if (_log.isDebugEnabled()) {
    _log.debug("Invoking deploy for " + file.getPath());
  }
  if (!isLiferayPackage(file)) {
    return;
  }
  if (_log.isInfoEnabled()) {
    _log.info("Extracting Liferay package for " + file.getPath());
  }
  _autoDeployer.autoDeploy(file.getName(),context);
  if (_log.isInfoEnabled()) {
    _log.info("Liferay package for " + file.getPath() + " extracted "+ "successfully. Deployment will start in a few seconds.");
  }
}
