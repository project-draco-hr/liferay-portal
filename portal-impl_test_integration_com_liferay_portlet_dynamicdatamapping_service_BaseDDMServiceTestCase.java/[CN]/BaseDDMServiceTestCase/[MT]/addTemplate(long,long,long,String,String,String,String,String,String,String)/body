{
  return DDMTemplateLocalServiceUtil.addTemplate(TestPropsValues.getUserId(),group.getGroupId(),classNameId,classPK,sourceClassNameId,templateKey,LocaleTestUtil.getDefaultLocaleMap(name),LocaleTestUtil.getDefaultLocaleMap(description),type,mode,language,script,false,false,null,null,ServiceContextTestUtil.getServiceContext());
