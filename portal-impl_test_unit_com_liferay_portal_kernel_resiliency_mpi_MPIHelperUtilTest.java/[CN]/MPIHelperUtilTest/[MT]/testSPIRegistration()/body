  mockSPI1=new MockSPI();
  mockSPI1.mpi=MPIHelperUtil.getMPI();
  mockSPI1.spiProviderName="name1";
  Assert.assertFalse(MPIHelperUtil.registerSPI(mockSPI1));
  Assert.assertTrue(logRecords.isEmpty());
