{
  String portletId=_portletDisplay.getId();
  String portletResource=_portletDisplay.getPortletResource();
  if (portletId.equals(PortletKeys.DOCUMENT_LIBRARY) || portletId.equals(PortletKeys.DOCUMENT_LIBRARY_ADMIN) || portletResource.equals(PortletKeys.DOCUMENT_LIBRARY)|| portletResource.equals(PortletKeys.DOCUMENT_LIBRARY_ADMIN)) {
    return true;
  }
  return _dlPortletInstanceSettings.isShowActions();
}
