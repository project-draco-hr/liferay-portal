{
  if (!inlineSQLHelper || !InlineSQLHelperUtil.isEnabled(groupId)) {
    if (status != WorkflowConstants.STATUS_ANY) {
      return MBThreadUtil.findByG_C_S(groupId,categoryId,status,start,end);
    }
 else {
      return MBThreadUtil.findByG_C(groupId,categoryId,start,end);
    }
  }
  Session session=null;
  try {
    session=openSession();
    String sql=CustomSQLUtil.get(FIND_BY_G_C);
    if (status != WorkflowConstants.STATUS_ANY) {
      sql=CustomSQLUtil.appendCriteria(sql,"AND (MBThread.status = ?)");
    }
    sql=InlineSQLHelperUtil.replacePermissionCheck(sql,MBMessage.class.getName(),"MBThread.rootMessageId",groupId);
    SQLQuery q=session.createSQLQuery(sql);
    q.addEntity("MBThread",MBThreadImpl.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(groupId);
    qPos.add(categoryId);
    if (status != WorkflowConstants.STATUS_ANY) {
      qPos.add(status);
    }
    return (List<MBThread>)QueryUtil.list(q,getDialect(),start,end);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
}
