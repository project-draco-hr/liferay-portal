{
  String var=tagData.getAttributeString("var");
  String varImpl=tagData.getAttributeString("varImpl");
  if (Validator.isNotNull(var)) {
    return new VariableInfo[]{new VariableInfo(var,String.class.getName(),true,VariableInfo.AT_END)};
  }
 else   if (Validator.isNotNull(varImpl)) {
    return new VariableInfo[]{new VariableInfo(varImpl,PortletURL.class.getName(),true,VariableInfo.AT_END)};
  }
 else {
    return null;
  }
}
