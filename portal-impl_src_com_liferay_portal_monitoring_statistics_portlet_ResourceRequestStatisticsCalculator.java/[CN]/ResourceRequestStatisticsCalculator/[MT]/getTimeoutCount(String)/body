{
  CompanyStatistics companyStatistics=_ServerStatistics.getPortletContainerStatistics(companyWebId);
  long numTimeouts=0;
  Collection<RequestStatistics> portletsStatistics=companyStatistics.getResourceStatistics();
  for (  RequestStatistics portletStatistics : portletsStatistics) {
    numTimeouts+=portletStatistics.getNumTimeouts();
  }
  return numTimeouts;
}
