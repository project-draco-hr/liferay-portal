{
  Layout layout=LayoutTestUtil.addLayout(group.getGroupId(),ServiceTestUtil.randomString());
  long[] layoutIds=new long[]{layout.getLayoutId()};
  exportImportLayouts(layoutIds,getImportParameterMap());
  Assert.assertEquals(layoutIds.length,LayoutLocalServiceUtil.getLayoutsCount(importedGroup,false));
  importedLayout=LayoutLocalServiceUtil.fetchLayoutByUuidAndGroupId(layout.getUuid(),importedGroup.getGroupId(),false);
  Assert.assertNotNull(importedLayout);
}
