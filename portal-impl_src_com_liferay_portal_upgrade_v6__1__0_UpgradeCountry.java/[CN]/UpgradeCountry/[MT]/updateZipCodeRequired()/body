{
  Connection con=null;
  PreparedStatement ps=null;
  try {
    con=DataAccess.getConnection();
    for (    String country : countriesWithoutZips) {
      ps=con.prepareStatement("update Country set zipCodeRequired = false where name = " + country);
    }
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
}
