 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  assertTrue(selenium.isVisible("//div/span/a/img"));
  assertEquals(RuntimeVariables.replace("Joe Bloggs (joebloggs)"),selenium.getText("//div[@class='user-name']"));
  assertEquals(RuntimeVariables.replace("Microblogs Content"),selenium.getText("//div[@class='content']"));
  assertEquals(RuntimeVariables.replace("Reply"),selenium.getText("//span[2]/a/span"));
  selenium.clickAt("//span[2]/a/span",RuntimeVariables.replace("Reply"));
  for (int second=0; ; second++) {
    if (second >= 60) {
