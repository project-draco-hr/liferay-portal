{
  LiferayFileItem[] liferayFileItems=new LiferayFileItem[2];
  LiferayFileItemFactory fileItemFactory=new LiferayFileItemFactory(UploadServletRequestImpl.getTempDir());
  for (int i=0; i < liferayFileItems.length; i++) {
    liferayFileItems[i]=fileItemFactory.createItem(RandomTestUtil.randomString(),RandomTestUtil.randomString(),true,RandomTestUtil.randomString());
    try (OutputStream outputStream=liferayFileItems[i].getOutputStream()){
      outputStream.write(bytes);
    }
   }
  return liferayFileItems;
}
