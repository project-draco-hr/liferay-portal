{
  List<DDMFormField> ddmFormFields=new ArrayList<>();
  ddmFormFields.add(createNestedDDMFormFields("ParentField","ChildField"));
  DDMFormField booleanDDMFormField=createRadioDDMFormField("BooleanField");
  booleanDDMFormField.addDDMFormFieldRule(new DDMFormFieldRule("isReadOnly(SelectField)",DDMFormFieldRuleType.READ_ONLY));
  ddmFormFields.add(booleanDDMFormField);
  DDMFormField selectDDMFormField=createSelectDDMFormField("SelectField");
  selectDDMFormField.addDDMFormFieldRule(new DDMFormFieldRule("isVisible(TextField)",DDMFormFieldRuleType.VISIBILITY));
  ddmFormFields.add(selectDDMFormField);
  ddmFormFields.add(createTextDDMFormField("TextField"));
  ddmFormFields.add(createHTMLDDMFormField("HTMLField"));
  return ddmFormFields;
}
