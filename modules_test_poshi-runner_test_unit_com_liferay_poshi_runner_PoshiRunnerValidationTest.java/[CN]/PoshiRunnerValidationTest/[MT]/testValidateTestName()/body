{
  PoshiRunnerValidation.validateTestName("ValidateTestName#testName");
  Assert.assertEquals("validateTestName is failing","",_getExceptionMessage());
  PoshiRunnerValidation.clearExceptions();
  PoshiRunnerValidation.validateTestName("ValidateTestName#fail");
  Assert.assertEquals("validateTestName is failing","Invalid test case command fail",_getExceptionMessage());
}
