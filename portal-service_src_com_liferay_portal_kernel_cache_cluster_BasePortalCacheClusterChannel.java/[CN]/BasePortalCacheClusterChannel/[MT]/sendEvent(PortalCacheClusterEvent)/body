{
  if (_started == false) {
synchronized (this) {
      if (_started == false) {
        _dispatchThread.start();
        _started=true;
      }
    }
  }
  if (_destroy) {
    dispatchEvent(portalCacheClusterEvent);
    if (_log.isDebugEnabled()) {
      _log.debug("Directly sends PortalCacheClusterEvent " + portalCacheClusterEvent);
    }
  }
 else {
    try {
      _eventQueue.put(portalCacheClusterEvent);
    }
 catch (    InterruptedException ie) {
    }
  }
}
