{
  DDMFormTemplateSynchonizer ddmFormTemplateSynchonizer=new DDMFormTemplateSynchonizer(structure.getDDMForm());
  List<DDMTemplate> formTemplates=getFormTemplates(structure);
  ddmFormTemplateSynchonizer.setDDMFormTemplates(formTemplates);
  ddmFormTemplateSynchonizer.setDDMTemplatePersistence(ddmTemplatePersistence);
  ddmFormTemplateSynchonizer.synchronize();
}
