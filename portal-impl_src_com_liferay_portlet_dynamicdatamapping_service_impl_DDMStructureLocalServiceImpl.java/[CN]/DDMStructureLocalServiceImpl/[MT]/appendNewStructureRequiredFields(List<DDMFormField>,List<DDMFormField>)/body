{
  for (int i=0; i < structureDDMFormFields.size(); i++) {
    DDMFormField structureDDMFormField=structureDDMFormFields.get(i);
    List<String> templateDDMFormFieldNames=getDDMFormFieldNames(templateDDMFormFields);
    if (!templateDDMFormFieldNames.contains(structureDDMFormField.getName())) {
      if (structureDDMFormField.isRequired()) {
        templateDDMFormFields.add(i,structureDDMFormField);
      }
    }
 else {
      DDMFormField templateDDMFormField=templateDDMFormFields.get(i);
      appendNewStructureRequiredFields(structureDDMFormField.getNestedDDMFormFields(),templateDDMFormField.getNestedDDMFormFields());
    }
  }
}
