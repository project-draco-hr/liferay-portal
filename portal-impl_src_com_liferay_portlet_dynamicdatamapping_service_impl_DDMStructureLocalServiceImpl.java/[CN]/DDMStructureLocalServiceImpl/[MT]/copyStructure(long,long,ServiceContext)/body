{
  DDMStructure structure=ddmStructurePersistence.findByPrimaryKey(structureId);
  return addStructure(userId,structure.getGroupId(),structure.getParentStructureId(),structure.getClassNameId(),null,structure.getNameMap(),structure.getDescriptionMap(),structure.getDDMForm(),structure.getDDMFormLayout(),structure.getStorageType(),structure.getType(),serviceContext);
}
