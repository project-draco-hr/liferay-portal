{
  couponId=StringUtil.upperCase(couponId);
  List list=new ArrayList();
  Session session=null;
  try {
    session=HibernateUtil.openSession();
    String sql=CustomSQLUtil.get(FIND_BY_C_G_C_A_DT);
    sql=CustomSQLUtil.replaceAndOperator(sql,andOperator);
    SQLQuery q=session.createSQLQuery(sql);
    q.addEntity("ShoppingCoupon",ShoppingCouponHBM.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(groupId);
    qPos.add(companyId);
    qPos.add(couponId);
    qPos.add(couponId);
    qPos.add(active);
    qPos.add(discountType);
    qPos.add(discountType);
    Iterator itr=QueryUtil.iterate(q,HibernateUtil.getDialect(),begin,end);
    while (itr.hasNext()) {
      ShoppingCouponHBM couponHBM=(ShoppingCouponHBM)itr.next();
      ShoppingCoupon coupon=ShoppingCouponHBMUtil.model(couponHBM);
      list.add(coupon);
    }
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    HibernateUtil.closeSession(session);
  }
  return list;
}
