{
  final AtomicBoolean flag=new AtomicBoolean();
  DefaultNoticeableFuture<?> _defaultNoticeableFuture=new DefaultNoticeableFuture<Object>(new Callable<Object>(){
    @Override public Object call(){
      flag.set(true);
      return flag;
    }
  }
);
  _defaultNoticeableFuture.run();
  Assert.assertSame(flag,_defaultNoticeableFuture.get());
  Assert.assertTrue(flag.get());
  _defaultNoticeableFuture=new DefaultNoticeableFuture<Object>(new Runnable(){
    @Override public void run(){
      flag.set(false);
    }
  }
,flag);
  _defaultNoticeableFuture.run();
  Assert.assertSame(flag,_defaultNoticeableFuture.get());
  Assert.assertFalse(flag.get());
}
