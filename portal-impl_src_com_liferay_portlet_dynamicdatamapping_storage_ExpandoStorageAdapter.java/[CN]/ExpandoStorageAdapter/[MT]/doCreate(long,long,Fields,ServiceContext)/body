{
  ExpandoTable table=_getExpandoTable(companyId,structureId,fields);
  ExpandoRow row=ExpandoRowLocalServiceUtil.addRow(table.getTableId(),CounterLocalServiceUtil.increment());
  _updateFields(table,row.getClassPK(),fields);
  DDMStorageLinkLocalServiceUtil.addStorageLink(table.getClassNameId(),row.getRowId(),structureId,serviceContext);
  return row.getRowId();
}
