{
  WikiPageResource pageResource=_wikiPageResourceLocalService.fetchWikiPageResource(activity.getClassPK());
  if (pageResource == null) {
    return null;
  }
  String creatorUserName=getUserName(activity.getUserId(),serviceContext);
  title=wrapLink(link,title);
  return new Object[]{groupName,creatorUserName,title,getAttachmentTitle(activity,pageResource,serviceContext)};
}
