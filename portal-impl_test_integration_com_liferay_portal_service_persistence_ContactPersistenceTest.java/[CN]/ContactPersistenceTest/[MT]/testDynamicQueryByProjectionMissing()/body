{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Contact.class,Contact.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("contactId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("contactId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
