{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Contact.class,Contact.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("contactId",ServiceTestUtil.nextLong()));
  List<Contact> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
