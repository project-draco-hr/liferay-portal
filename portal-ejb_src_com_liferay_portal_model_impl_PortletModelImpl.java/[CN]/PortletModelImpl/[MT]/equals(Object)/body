{
  if (obj == null) {
    return false;
  }
  PortletImpl portlet=null;
  try {
    portlet=(PortletImpl)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  PortletPK pk=portlet.getPrimaryKey();
  if (getPrimaryKey().equals(pk)) {
    return true;
  }
 else {
    return false;
  }
}
