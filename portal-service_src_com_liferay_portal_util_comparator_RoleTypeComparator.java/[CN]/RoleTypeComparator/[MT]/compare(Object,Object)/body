{
  Role role1=(Role)obj1;
  Role role2=(Role)obj2;
  int value=0;
  if (role1.getType() > role2.getType()) {
    value=1;
  }
 else   if (role1.getType() < role2.getType()) {
    value=-1;
  }
  if (value == 0) {
    value=role1.getName().compareTo(role2.getName());
  }
  if (_asc) {
    return value;
  }
 else {
    return -value;
  }
}
