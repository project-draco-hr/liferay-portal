{
  user=toUnwrappedModel(user);
  userToGroupTableMapper.deleteLeftPrimaryKeyTableMappings(companyProvider.getCompanyId(),user.getPrimaryKey());
  userToOrganizationTableMapper.deleteLeftPrimaryKeyTableMappings(companyProvider.getCompanyId(),user.getPrimaryKey());
  userToRoleTableMapper.deleteLeftPrimaryKeyTableMappings(companyProvider.getCompanyId(),user.getPrimaryKey());
  userToTeamTableMapper.deleteLeftPrimaryKeyTableMappings(companyProvider.getCompanyId(),user.getPrimaryKey());
  userToUserGroupTableMapper.deleteLeftPrimaryKeyTableMappings(companyProvider.getCompanyId(),user.getPrimaryKey());
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(user)) {
      user=(User)session.get(UserImpl.class,user.getPrimaryKeyObj());
    }
    if (user != null) {
      session.delete(user);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (user != null) {
    clearCache(user);
  }
  return user;
}
