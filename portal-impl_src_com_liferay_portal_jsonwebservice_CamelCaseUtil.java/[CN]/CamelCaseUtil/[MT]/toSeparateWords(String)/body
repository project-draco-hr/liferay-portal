{
  StringBundler sb=new StringBundler();
  boolean upperCase=false;
  for (int i=0; i < s.length(); i++) {
    char c=s.charAt(i);
    if ((i > 0) && Character.isUpperCase(c)) {
      if (!upperCase) {
        sb.append(CharPool.DASH);
      }
      c=Character.toLowerCase(c);
      upperCase=true;
    }
 else {
      upperCase=false;
    }
    sb.append(c);
  }
  return sb.toString();
}
