{
  int length=string.length();
  StringBuilder s=new StringBuilder(length);
  boolean upperCase=false;
  for (int i=0; i < length; i++) {
    char ch=string.charAt(i);
    if (ch == CharPool.DASH) {
      upperCase=true;
    }
 else     if (upperCase) {
      s.append(Character.toUpperCase(ch));
      upperCase=false;
    }
 else {
      s.append(ch);
    }
  }
  return s.toString();
}
