{
  Session session=null;
  try {
    session=openSession();
    SocialRelation socialRelation=(SocialRelation)session.get(SocialRelationImpl.class,primaryKey);
    if (socialRelation == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchRelationException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(socialRelation);
  }
 catch (  NoSuchRelationException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
