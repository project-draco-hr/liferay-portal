{
  User dlc2=UserLocalServiceUtil.getUserByScreenName(TestPropsValues.getCompanyId(),"dlc2");
  int[] types={SocialRelationConstants.TYPE_BI_FRIEND,SocialRelationConstants.TYPE_BI_COWORKER};
  List<User> users=UserLocalServiceUtil.getUsersBySocialRelationsTypes("dlc",dlc2.getUserId(),types,null,QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  Assert.assertEquals(5,users.size());
}
