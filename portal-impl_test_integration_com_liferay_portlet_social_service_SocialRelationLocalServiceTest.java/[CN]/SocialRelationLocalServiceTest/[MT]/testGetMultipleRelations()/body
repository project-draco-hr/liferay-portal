{
  User dlc2User=UserLocalServiceUtil.getUserByScreenName(TestPropsValues.getCompanyId(),"dlc2");
  int[] types={SocialRelationConstants.TYPE_BI_FRIEND,SocialRelationConstants.TYPE_BI_COWORKER};
  List<User> users=UserLocalServiceUtil.getUsersBySocialRelationsTypes("dlc",dlc2User.getUserId(),types,QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  Assert.assertEquals(5,users.size());
}
