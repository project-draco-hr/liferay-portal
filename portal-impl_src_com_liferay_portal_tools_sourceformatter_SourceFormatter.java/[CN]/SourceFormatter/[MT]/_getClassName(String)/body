{
  int pos=line.indexOf(" implements ");
  if (pos == -1) {
    pos=line.indexOf(" extends ");
  }
  if (pos == -1) {
    pos=line.indexOf(StringPool.OPEN_CURLY_BRACE);
  }
  if (pos != -1) {
    line=line.substring(0,pos);
  }
  line=line.trim();
  pos=line.lastIndexOf(StringPool.SPACE);
  return line.substring(pos + 1);
}
