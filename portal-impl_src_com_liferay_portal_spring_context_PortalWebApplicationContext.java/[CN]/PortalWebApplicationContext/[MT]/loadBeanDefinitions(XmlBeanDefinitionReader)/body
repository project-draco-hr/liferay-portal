{
  String[] configLocations=getConfigLocations();
  if (configLocations != null) {
    for (int i=0; i < configLocations.length; i++) {
      try {
        reader.loadBeanDefinitions(configLocations[i]);
      }
 catch (      Exception e) {
        if (_log.isWarnEnabled()) {
          _log.warn(e);
        }
      }
    }
  }
}
