{
  List<WSRPConsumerPortlet> wsrpConsumerPortlets=wsrpConsumerPortletPersistence.findByUuid(wsrpConsumerPortletUuid);
  if (wsrpConsumerPortlets.isEmpty()) {
    throw new NoSuchConsumerPortletException("No WSRP consumer portlet exists with uuid " + wsrpConsumerPortletUuid);
  }
  return wsrpConsumerPortlets.get(0);
}
