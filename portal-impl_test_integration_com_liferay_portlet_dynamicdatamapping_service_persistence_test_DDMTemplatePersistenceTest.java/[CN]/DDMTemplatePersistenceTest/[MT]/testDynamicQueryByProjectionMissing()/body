{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMTemplate.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("templateId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("templateId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
