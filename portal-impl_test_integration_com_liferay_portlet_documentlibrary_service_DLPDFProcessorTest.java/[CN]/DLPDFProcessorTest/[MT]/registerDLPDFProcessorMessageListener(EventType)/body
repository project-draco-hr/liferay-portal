{
  final AtomicInteger counter=new AtomicInteger();
  MessageBusUtil.registerMessageListener(DestinationNames.DOCUMENT_LIBRARY_PDF_PROCESSOR,new MessageListener(){
    @Override public void receive(    Message message){
      Object[] payload=(Object[])message.getPayload();
      if (eventType.isMatch(payload[0])) {
        counter.incrementAndGet();
      }
    }
  }
);
  return counter;
}
