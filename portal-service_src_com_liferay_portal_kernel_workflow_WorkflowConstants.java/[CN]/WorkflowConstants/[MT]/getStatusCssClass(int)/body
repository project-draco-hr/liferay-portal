{
  if (status == STATUS_APPROVED) {
    return "label-success";
  }
 else   if (status == STATUS_DRAFT) {
    return "label-info";
  }
 else   if (status == STATUS_EXPIRED) {
    return "label-important";
  }
 else   if (status == STATUS_PENDING) {
    return "label-warning";
  }
  return StringPool.BLANK;
}
