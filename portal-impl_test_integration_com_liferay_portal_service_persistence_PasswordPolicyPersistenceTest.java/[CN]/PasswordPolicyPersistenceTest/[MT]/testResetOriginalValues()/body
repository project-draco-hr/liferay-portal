{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  PasswordPolicy newPasswordPolicy=addPasswordPolicy();
  _persistence.clearCache();
  PasswordPolicyModelImpl existingPasswordPolicyModelImpl=(PasswordPolicyModelImpl)_persistence.findByPrimaryKey(newPasswordPolicy.getPrimaryKey());
  Assert.assertEquals(existingPasswordPolicyModelImpl.getCompanyId(),existingPasswordPolicyModelImpl.getOriginalCompanyId());
  Assert.assertEquals(existingPasswordPolicyModelImpl.getDefaultPolicy(),existingPasswordPolicyModelImpl.getOriginalDefaultPolicy());
  Assert.assertEquals(existingPasswordPolicyModelImpl.getCompanyId(),existingPasswordPolicyModelImpl.getOriginalCompanyId());
  Assert.assertTrue(Validator.equals(existingPasswordPolicyModelImpl.getName(),existingPasswordPolicyModelImpl.getOriginalName()));
}
