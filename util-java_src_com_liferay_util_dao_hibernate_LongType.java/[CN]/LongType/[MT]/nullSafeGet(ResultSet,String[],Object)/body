{
  Object value=null;
  try {
    value=Hibernate.LONG.nullSafeGet(rs,names[0]);
  }
 catch (  SQLException sqle1) {
    try {
      value=new Long(GetterUtil.getLong((String)Hibernate.STRING.nullSafeGet(rs,names[0])));
    }
 catch (    SQLException sqle2) {
      throw sqle1;
    }
  }
  if (value == null) {
    return new Long(DEFAULT_VALUE);
  }
 else {
    return value;
  }
}
