{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(OrgGroupRole.class,OrgGroupRole.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id.organizationId",nextLong()));
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id.groupId",nextLong()));
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id.roleId",nextLong()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
