{
  OrgGroupRole newOrgGroupRole=addOrgGroupRole();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(OrgGroupRole.class,OrgGroupRole.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("id.organizationId"));
  Object newOrganizationId=newOrgGroupRole.getOrganizationId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("id.organizationId",new Object[]{newOrganizationId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Object existingOrganizationId=result.get(0);
  assertEquals(existingOrganizationId,newOrganizationId);
}
