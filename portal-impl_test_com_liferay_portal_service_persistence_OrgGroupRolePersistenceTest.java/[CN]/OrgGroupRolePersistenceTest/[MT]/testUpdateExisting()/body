{
  OrgGroupRolePK pk=new OrgGroupRolePK(nextLong(),nextLong(),nextLong());
  OrgGroupRole newOrgGroupRole=_persistence.create(pk);
  _persistence.update(newOrgGroupRole);
  OrgGroupRole existingOrgGroupRole=_persistence.findByPrimaryKey(newOrgGroupRole.getPrimaryKey());
  assertEquals(existingOrgGroupRole.getOrganizationId(),newOrgGroupRole.getOrganizationId());
  assertEquals(existingOrgGroupRole.getGroupId(),newOrgGroupRole.getGroupId());
  assertEquals(existingOrgGroupRole.getRoleId(),newOrgGroupRole.getRoleId());
}
