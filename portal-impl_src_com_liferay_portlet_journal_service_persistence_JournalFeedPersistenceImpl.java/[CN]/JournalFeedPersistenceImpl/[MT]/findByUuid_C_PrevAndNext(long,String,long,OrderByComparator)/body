{
  JournalFeed journalFeed=findByPrimaryKey(id);
  Session session=null;
  try {
    session=openSession();
    JournalFeed[] array=new JournalFeedImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,journalFeed,uuid,companyId,orderByComparator,true);
    array[1]=journalFeed;
    array[2]=getByUuid_C_PrevAndNext(session,journalFeed,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
