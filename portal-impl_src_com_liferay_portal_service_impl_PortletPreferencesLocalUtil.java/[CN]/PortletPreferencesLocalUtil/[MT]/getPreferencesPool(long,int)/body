{
  String key=_encodeKey(ownerId,ownerType);
  Map<String,PortletPreferencesImpl> preferencesPool=(Map<String,PortletPreferencesImpl>)MultiVMPoolUtil.get(_cache,key);
  if (preferencesPool == null) {
    preferencesPool=new ConcurrentHashMap<String,PortletPreferencesImpl>();
    MultiVMPoolUtil.put(_cache,key,preferencesPool);
  }
  return preferencesPool;
}
