{
  String key=_encodeKey(ownerId,ownerType);
  Map<String,PortletPreferencesImpl> prefsPool=(Map<String,PortletPreferencesImpl>)MultiVMPoolUtil.get(_cache,key);
  if (prefsPool == null) {
    prefsPool=new ConcurrentHashMap<String,PortletPreferencesImpl>();
    MultiVMPoolUtil.put(_cache,key,prefsPool);
  }
  return prefsPool;
}
