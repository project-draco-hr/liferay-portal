{
  long groupId=0;
  groupId=ParamUtil.getLong(request,"groupId");
  if (groupId <= 0) {
    long plid=ParamUtil.getLong(request,"p_l_id");
    if (plid > 0) {
      try {
        Layout layout=LayoutLocalServiceUtil.getLayout(plid);
        groupId=layout.getGroupId();
      }
 catch (      PortalException e) {
      }
    }
  }
  return groupId;
}
