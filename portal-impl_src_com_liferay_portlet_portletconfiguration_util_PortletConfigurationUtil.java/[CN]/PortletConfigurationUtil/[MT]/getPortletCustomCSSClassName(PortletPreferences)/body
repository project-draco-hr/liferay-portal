{
  String classNames=StringPool.BLANK;
  String css=portletSetup.getValue("portlet-setup-css",StringPool.BLANK);
  if (Validator.isNotNull(css)) {
    JSONObject portletCSSJSON=PortletSetupUtil.cssToJSON(portletSetup,css);
    JSONObject advancedData=portletCSSJSON.getJSONObject("advancedData");
    if (advancedData != null) {
      classNames=advancedData.getString("customCSSClassName");
    }
  }
  return classNames;
}
