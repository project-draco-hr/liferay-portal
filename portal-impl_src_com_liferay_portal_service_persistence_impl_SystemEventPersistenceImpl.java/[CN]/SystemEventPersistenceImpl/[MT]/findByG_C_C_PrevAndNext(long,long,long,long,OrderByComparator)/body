{
  SystemEvent systemEvent=findByPrimaryKey(systemEventId);
  Session session=null;
  try {
    session=openSession();
    SystemEvent[] array=new SystemEventImpl[3];
    array[0]=getByG_C_C_PrevAndNext(session,systemEvent,groupId,classNameId,classPK,orderByComparator,true);
    array[1]=systemEvent;
    array[2]=getByG_C_C_PrevAndNext(session,systemEvent,groupId,classNameId,classPK,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
