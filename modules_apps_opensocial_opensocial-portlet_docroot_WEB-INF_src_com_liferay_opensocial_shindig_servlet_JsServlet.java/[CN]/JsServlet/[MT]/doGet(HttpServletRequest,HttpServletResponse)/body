{
  String requestURI=request.getRequestURI();
  if (!requestURI.equals("/combo")) {
    super.doGet(request,response);
    return;
  }
  StringBundler sb=new StringBundler(3);
  sb.append(request.getAttribute(JavaConstants.JAVAX_SERVLET_INCLUDE_REQUEST_URI));
  sb.append(CharPool.QUESTION);
  sb.append(request.getAttribute(JavaConstants.JAVAX_SERVLET_INCLUDE_QUERY_STRING));
  String urlString=PortalUtil.getAbsoluteURL(request,sb.toString());
  URL url=new URL(urlString);
  URLConnection urlConnection=url.openConnection();
  ServletResponseUtil.write(response,urlConnection.getInputStream());
}
