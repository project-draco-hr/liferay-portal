{
  if (message.equals(_EMTPY_MESSAGE)) {
    message="{\"responseId\":\"" + _responseId + "\"}";
  }
 else {
    message=message.substring(0,message.length() - 1) + ",\"responseId\":\"" + _responseId+ "\"}";
  }
  _messageBus.sendMessage(destination,message);
  wait(_timeout);
  return _responseMessage;
}
