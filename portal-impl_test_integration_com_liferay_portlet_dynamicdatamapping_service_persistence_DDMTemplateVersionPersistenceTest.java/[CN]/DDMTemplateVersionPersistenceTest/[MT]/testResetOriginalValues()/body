{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  DDMTemplateVersion newDDMTemplateVersion=addDDMTemplateVersion();
  _persistence.clearCache();
  DDMTemplateVersionModelImpl existingDDMTemplateVersionModelImpl=(DDMTemplateVersionModelImpl)_persistence.findByPrimaryKey(newDDMTemplateVersion.getPrimaryKey());
  Assert.assertEquals(existingDDMTemplateVersionModelImpl.getTemplateId(),existingDDMTemplateVersionModelImpl.getOriginalTemplateId());
  Assert.assertTrue(Validator.equals(existingDDMTemplateVersionModelImpl.getVersion(),existingDDMTemplateVersionModelImpl.getOriginalVersion()));
}
