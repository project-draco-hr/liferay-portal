{
  try {
    JournalArticleLocalService journalArticleLocalService=JournalArticleLocalServiceFactory.getService();
    return journalArticleLocalService.search(companyId,articleId,version,groupId,title,content,type,structureId,templateId,displayDateGT,displayDateLT,approved,expired,reviewDate,andOperator,begin,end,obc);
  }
 catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
