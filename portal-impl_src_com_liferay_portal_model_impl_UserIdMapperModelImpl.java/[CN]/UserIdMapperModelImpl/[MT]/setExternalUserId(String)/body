{
  if (((externalUserId == null) && (_externalUserId != null)) || ((externalUserId != null) && (_externalUserId == null)) || ((externalUserId != null) && (_externalUserId != null) && !externalUserId.equals(_externalUserId))) {
    _externalUserId=externalUserId;
    if (_originalExternalUserId == null) {
      _originalExternalUserId=externalUserId;
    }
  }
