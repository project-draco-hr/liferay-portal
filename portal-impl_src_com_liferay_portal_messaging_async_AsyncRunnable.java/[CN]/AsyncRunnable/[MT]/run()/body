{
  try {
    if (_methodInvocation != null) {
      _methodInvocation.proceed();
    }
 else {
      AsyncInvokeThreadLocal.setEnabled(true);
      try {
        _methodHandler.invoke(null);
      }
  finally {
        AsyncInvokeThreadLocal.setEnabled(false);
      }
    }
  }
 catch (  Throwable t) {
    throw new RuntimeException(t);
  }
}
