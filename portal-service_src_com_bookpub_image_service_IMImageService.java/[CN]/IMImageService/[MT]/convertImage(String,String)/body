{
  String fileLocation=_DEFAULT_WORK_PATH + fileName + _DEFAULT_IMAGE_EXTENSION_DOT+ fileExtension;
  ImageFileEntry entry=getImageFileEntry(fileLocation);
  String targetFileLocation=entry.getWorkPath() + entry.getTargetFileName() + _DEFAULT_IMAGE_EXTENSION_DOT+ entry.getTargetExtension();
  System.out.println(entry.toString());
  int maxDimension=entry.getWidth();
  if (maxDimension == 0)   maxDimension=_DEFAULT_MAXIMUM_DIMENSION;
  resize(entry.getImPath(),fileLocation,targetFileLocation,maxDimension);
  entry=getImageFileEntry(targetFileLocation);
  System.out.println(entry.toString());
  crop(entry.getImPath(),targetFileLocation,targetFileLocation,_DEFAULT_WIDTH,_DEFAULT_HEIGHT);
}
