{
  try {
    return (List<ProcessInstanceInfo>)_synchronousMessageSender.send(ProcessInstanceRequest.createGetRequest(workflowDefinitionName,retrieveTokenInfo,finished));
  }
 catch (  MessageBusException ex) {
    throw new WorkflowException("Unable to get processInstances for " + "workflowDefinitionName:" + workflowDefinitionName + " retrieve token:"+ retrieveTokenInfo+ " finished:"+ finished,ex);
  }
}
