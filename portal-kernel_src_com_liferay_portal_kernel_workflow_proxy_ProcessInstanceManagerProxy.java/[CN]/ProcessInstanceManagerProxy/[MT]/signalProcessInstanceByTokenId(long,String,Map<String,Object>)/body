{
  try {
    return (ProcessInstanceInfo)_synchronousMessageSender.send(ProcessInstanceRequest.createSignalProcessInstanceByTokenIdRequest(tokenId,activityName,contextInfo));
  }
 catch (  MessageBusException ex) {
    throw new WorkflowException("Unable to signal by tokenId:" + tokenId + " to activityName:"+ activityName,ex);
  }
}
