{
  try {
    return (ProcessInstanceInfo)_synchronousMessageSender.send(ProcessInstanceRequest.createStartRequest(workflowDefinitionName,contextInfo,userId));
  }
 catch (  MessageBusException ex) {
    throw new WorkflowException("Unable to start " + "workflowDefinitionName:" + workflowDefinitionName + " by userId:"+ userId+ " with contextInfo:"+ contextInfo,ex);
  }
}
