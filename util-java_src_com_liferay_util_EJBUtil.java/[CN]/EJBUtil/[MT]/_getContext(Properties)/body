{
  if (props == null) {
    return new InitialContext();
  }
 else {
    String key=props.getProperty(Context.PROVIDER_URL);
    if (key == null) {
      return new InitialContext();
    }
    Context context=(Context)_contexts.get(key);
    if (context == null) {
      context=new InitialContext(props);
      _contexts.put(key,context);
    }
    return context;
  }
}
