{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Test Event 2")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//div[@id=\"portlet-wrapper-8\"]//td[2]/table/tbody/tr[5]/td[4]/ul/li/ul/li[4]/nobr/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("//div[@id=\"portlet-wrapper-8\"]//td[2]/table/tbody/tr[5]/td[4]/ul/li/ul/li[4]/nobr/a");
  assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this[\\s\\S]$"));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (!selenium.isElementPresent("link=Test Event 2")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
