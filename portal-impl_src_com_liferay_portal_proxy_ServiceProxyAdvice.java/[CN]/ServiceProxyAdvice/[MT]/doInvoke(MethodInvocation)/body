{
  ServiceRequest serviceRequest=createServiceRequest(methodInvocation);
  BaseServiceProxy baseServiceProxy=(BaseServiceProxy)methodInvocation.getThis();
  MessagingProxy messagingProxy=methodInvocation.getMethod().getAnnotation(MessagingProxy.class);
  if ((messagingProxy == null) || (messagingProxy.mode().equals(MessagingMode.ASYNC))) {
    doInvokeAsynchronous(serviceRequest,baseServiceProxy);
    return null;
  }
 else {
    return doInvokeSynchronous(serviceRequest,baseServiceProxy);
  }
}
