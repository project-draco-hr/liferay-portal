{
  int value=0;
  if (getParentCategoryId() < mbCategory.getParentCategoryId()) {
    value=-1;
  }
 else   if (getParentCategoryId() > mbCategory.getParentCategoryId()) {
    value=1;
  }
 else {
    value=0;
  }
  if (value != 0) {
    return value;
  }
  value=getName().toLowerCase().compareTo(mbCategory.getName().toLowerCase());
  if (value != 0) {
    return value;
  }
  return 0;
}
