{
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Media Gallery Test Page",RuntimeVariables.replace("Media Gallery Test Page"));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//section")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertTrue(selenium.isVisible("//section"));
}
