{
  try {
    SearchRequest searchRequest=new SearchRequest(SearchRequest.COMMAND_SEARCH,companyId,query,sort,start,end);
    Hits hits=(Hits)MessageBusUtil.sendSynchronizedMessage(DestinationNames.SEARCH_READER,new Message(searchRequest));
    return hits;
  }
 catch (  Exception e) {
    throw new SearchException(e);
  }
}
