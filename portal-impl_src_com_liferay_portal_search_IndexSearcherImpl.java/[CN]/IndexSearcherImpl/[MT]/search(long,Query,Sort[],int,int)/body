{
  try {
    SearchRequest searchRequest=SearchRequest.search(companyId,query,sorts,start,end);
    Hits hits=(Hits)MessageBusUtil.sendSynchronousMessage(DestinationNames.SEARCH_READER,searchRequest,DestinationNames.SEARCH_READER_RESPONSE);
    return hits;
  }
 catch (  Exception e) {
    throw new SearchException(e);
  }
}
