{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Blogs Permissions Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Blogs Permissions Page",RuntimeVariables.replace("Blogs Permissions Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isVisible("link=Permissions Blogs Test Entry"));
  selenium.clickAt("link=Permissions Blogs Test Entry",RuntimeVariables.replace("Permissions Blogs Test Entry"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("This is a permissions blogs test entry"),selenium.getText("//div[@class='entry-body']"));
}
