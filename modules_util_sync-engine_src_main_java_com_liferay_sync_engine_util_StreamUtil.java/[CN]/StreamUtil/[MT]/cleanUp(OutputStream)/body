{
  try {
    if (outputStream != null) {
      outputStream.flush();
    }
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn(e.getMessage(),e);
    }
  }
  try {
    if (outputStream != null) {
      outputStream.close();
    }
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn(e.getMessage(),e);
    }
  }
}
