{
  ClassLoader contextClassLoader=PACLClassLoaderUtil.getContextClassLoader();
  ClassLoader portalClassLoder=PACLClassLoaderUtil.getPortalClassLoader();
  if (contextClassLoader == portalClassLoder) {
    return template;
  }
  PACLPolicy policy=PACLPolicyManager.getPACLPolicy(contextClassLoader);
  return new PACLTemplateWrapper(template,policy);
}
