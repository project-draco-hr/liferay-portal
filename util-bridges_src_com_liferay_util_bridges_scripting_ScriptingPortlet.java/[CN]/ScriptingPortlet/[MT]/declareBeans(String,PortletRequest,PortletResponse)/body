{
  script=getGlobalScript() + script;
  PortletConfig portletConfig=getPortletConfig();
  PortletContext portletContext=getPortletContext();
  Map<String,Object> portletObjects=ScriptingHelperUtil.getPortletObjects(portletConfig,portletContext,portletRequest,portletResponse);
  ScriptingUtil.exec(null,portletObjects,language,script,StringPool.EMPTY_ARRAY);
}
