{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLSync.class,DLSync.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("syncId",ServiceTestUtil.nextLong()));
  List<DLSync> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
