{
  long pk=nextLong();
  ExpandoValue newExpandoValue=_persistence.create(pk);
  newExpandoValue.setColumnId(nextLong());
  newExpandoValue.setClassPK(nextLong());
  newExpandoValue.setValue(randomString());
  _persistence.update(newExpandoValue,false);
  ExpandoValue existingExpandoValue=_persistence.findByPrimaryKey(newExpandoValue.getPrimaryKey());
  assertEquals(existingExpandoValue.getValueId(),newExpandoValue.getValueId());
  assertEquals(existingExpandoValue.getColumnId(),newExpandoValue.getColumnId());
  assertEquals(existingExpandoValue.getClassPK(),newExpandoValue.getClassPK());
  assertEquals(existingExpandoValue.getValue(),newExpandoValue.getValue());
}
