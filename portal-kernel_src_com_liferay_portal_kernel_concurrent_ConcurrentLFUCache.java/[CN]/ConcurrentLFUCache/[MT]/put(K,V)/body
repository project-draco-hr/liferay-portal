{
  if (key == null) {
    throw new NullPointerException("Key is null");
  }
  ValueWrapper valueWrapper=new ValueWrapper(value);
  _writeLock.lock();
  try {
    if (!_cache.containsKey(key) && (_cache.size() >= _maxSize)) {
      _cleanUp();
    }
    _cache.put(key,valueWrapper);
  }
  finally {
    _writeLock.unlock();
  }
  _putCount.getAndIncrement();
}
