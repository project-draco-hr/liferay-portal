{
  String original="<%@ page import=\"java.text.DateFormat\" %>" + '\n' + "<%@ page import=\"java.io.Serializable\" %>";
  String expected="<%@ page import=\"java.io.Serializable\" %>" + '\n' + '\n'+ "<%@ page import=\"java.text.DateFormat\" %>"+ '\n';
  _assertFormat("different package groups are sorted and separated",original,expected);
}
