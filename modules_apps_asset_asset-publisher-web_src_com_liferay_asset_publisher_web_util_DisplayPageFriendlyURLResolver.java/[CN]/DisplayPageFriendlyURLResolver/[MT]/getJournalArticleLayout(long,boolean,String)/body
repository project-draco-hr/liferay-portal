{
  String urlTitle=friendlyURL.substring(JournalArticleConstants.CANONICAL_URL_SEPARATOR.length());
  JournalArticle journalArticle=JournalArticleLocalServiceUtil.getArticleByUrlTitle(groupId,urlTitle);
  return LayoutLocalServiceUtil.getLayoutByUuidAndGroupId(journalArticle.getLayoutUuid(),groupId,privateLayout);
}
