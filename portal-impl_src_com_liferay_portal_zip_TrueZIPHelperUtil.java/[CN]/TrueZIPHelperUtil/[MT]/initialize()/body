{
  try {
    Class.forName("de.schlichtherle.io.ArchiveControllers",true,TrueZIPHelperUtil.class.getClassLoader());
    Class<?> clazz=Class.forName("de.schlichtherle.io.ArchiveControllers$ShutdownHook");
    Field field=ReflectionUtil.getDeclaredField(clazz,"SINGLETON");
    Thread thread=(Thread)field.get(null);
    Runtime runtime=Runtime.getRuntime();
    if (runtime.removeShutdownHook(thread)) {
      _thread=thread;
    }
  }
 catch (  Exception e) {
    ReflectionUtil.throwException(e);
  }
}
