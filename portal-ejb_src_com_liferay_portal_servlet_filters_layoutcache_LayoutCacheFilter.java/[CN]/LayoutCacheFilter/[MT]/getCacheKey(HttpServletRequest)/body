{
  String url=req.getServletPath() + req.getPathInfo() + StringPool.QUESTION+ req.getQueryString();
  String languageId=LanguageUtil.getLanguageId(req);
  boolean isIe=BrowserSniffer.is_ie(req);
  boolean acceptsGzip=BrowserSniffer.acceptsGzip(req);
  String key=url + StringPool.POUND + languageId+ StringPool.POUND+ isIe+ StringPool.POUND+ acceptsGzip;
  return key.trim().toUpperCase();
}
