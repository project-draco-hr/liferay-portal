{
  String url=req.getServletPath() + req.getPathInfo() + StringPool.QUESTION+ req.getQueryString();
  String languageId=LanguageUtil.getLanguageId(req);
  String userAgent=req.getHeader(HttpHeaders.USER_AGENT);
  String key=url + StringPool.POUND + languageId+ StringPool.POUND+ userAgent;
  return key.trim().toUpperCase();
}
