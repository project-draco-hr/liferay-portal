{
  ServletContext ctx=_config.getServletContext();
  _companyId=PortalUtil.getCompanyIdByWebId(ctx);
  _pattern=GetterUtil.getInteger(_config.getInitParameter("pattern"));
  if ((_pattern != _PATTERN_FRIENDLY) && (_pattern != _PATTERN_LAYOUT) && (_pattern != _PATTERN_RESOURCE)) {
    _log.error("Layout cache pattern is invalid");
  }
}
