{
synchronized (FriendlyURLServlet.class) {
    ServletContext ctx=config.getServletContext();
    _companyId=ctx.getInitParameter("company_id");
    _pattern=GetterUtil.getInteger(config.getInitParameter("pattern"));
    if ((_pattern != _PATTERN_FRIENDLY) && (_pattern != _PATTERN_LAYOUT) && (_pattern != _PATTERN_RESOURCE)) {
      throw new ServletException("Layout cache pattern is invalid");
    }
  }
}
