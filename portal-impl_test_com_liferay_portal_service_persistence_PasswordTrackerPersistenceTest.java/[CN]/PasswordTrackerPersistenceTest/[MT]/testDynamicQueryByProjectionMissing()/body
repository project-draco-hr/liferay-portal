{
  PasswordTracker newPasswordTracker=addPasswordTracker();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PasswordTracker.class,PasswordTracker.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("passwordTrackerId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("passwordTrackerId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
