{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PasswordTracker.class,PasswordTracker.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("passwordTrackerId",nextLong()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
