{
  Calendar calendar=new GregorianCalendar(2011,Calendar.JANUARY,15);
  Assert.assertEquals(14,SocialCounterPeriodUtil.getStartPeriod(calendar.getTimeInMillis()));
  int startPeriod=SocialCounterPeriodUtil.getStartPeriod();
  Assert.assertEquals(startPeriod,SocialCounterPeriodUtil.getStartPeriod(0));
  Assert.assertEquals(startPeriod - 1,SocialCounterPeriodUtil.getStartPeriod(-1));
}
