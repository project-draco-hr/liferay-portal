{
  DLFolder dlFolder=_dlFolderLocalService.fetchFolder(groupId,parentFolderId,folderName);
  if (dlFolder == null) {
    dlFolder=_dlFolderLocalService.addFolder(userId,groupId,groupId,false,parentFolderId,folderName,null,false,serviceContext);
  }
  addPrimaryKey(DLFolder.class.getName(),dlFolder.getPrimaryKey());
  return dlFolder.getFolderId();
}
