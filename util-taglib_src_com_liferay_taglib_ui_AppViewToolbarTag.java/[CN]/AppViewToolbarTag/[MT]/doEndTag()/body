{
  try {
    HttpServletRequest request=(HttpServletRequest)pageContext.getRequest();
    request.setAttribute("liferay-ui:app_view_toolbar:includeDisplayStyle",_includeDisplayStyle);
    PortalIncludeUtil.include(pageContext,getEndPage());
    return EVAL_PAGE;
  }
 catch (  Exception e) {
