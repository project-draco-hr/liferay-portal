{
  value=value.trim();
  StringBuilder sb=new StringBuilder(value.length());
  for (int i=0; i < value.length(); i++) {
    char ch=value.charAt(i);
    if ((Character.isDigit(ch)) || ((ch == CharPool.DASH) && (i == 0)) || (ch == CharPool.PERIOD)|| (ch == CharPool.UPPER_CASE_E)|| (ch == CharPool.LOWER_CASE_E)) {
      sb.append(ch);
    }
  }
  value=sb.toString();
  return value;
}
