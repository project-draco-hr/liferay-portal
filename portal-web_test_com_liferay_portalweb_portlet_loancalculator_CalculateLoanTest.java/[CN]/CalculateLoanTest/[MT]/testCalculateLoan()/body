{
  selenium.type("_61_loanAmount","1,000");
  selenium.type("_61_interest","4.75");
  selenium.type("_61_years","20");
  selenium.click("//input[@value='Calculate']");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//tr[6]/td[2]/b")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
