{
  if (item.isInfiniteStock()) {
    return true;
  }
  if (!item.isFields()) {
    if (item.getStockQuantity() > 0) {
      return true;
    }
 else {
      return false;
    }
  }
 else {
    String[] fieldsQuantities=item.getFieldsQuantitiesArray();
    for (int i=0; i < fieldsQuantities.length; i++) {
      if (GetterUtil.getInteger(fieldsQuantities[i]) > 0) {
        return true;
      }
    }
    return false;
  }
}
