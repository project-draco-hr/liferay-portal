{
  UpgradeTable upgradeTable=new DefaultUpgradeTableImpl(WebsiteImpl.TABLE_NAME,WebsiteImpl.TABLE_COLUMNS,new PKUpgradeColumnImpl(),new ClassNameUpgradeColumnImpl());
  upgradeTable.updateTable();
  CounterLocalServiceUtil.reset(Website.class.getName());
  DBUtil.getInstance().executeSQL(_UPGRADE_SCHEMA);
}
