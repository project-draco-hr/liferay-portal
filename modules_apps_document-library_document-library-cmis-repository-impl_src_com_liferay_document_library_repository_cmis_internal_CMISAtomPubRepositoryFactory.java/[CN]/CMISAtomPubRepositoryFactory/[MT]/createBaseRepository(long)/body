{
  CMISAtomPubRepository cmisAtomPubRepository=new CMISAtomPubRepository();
  com.liferay.portal.model.Repository repository=_repositoryLocalService.getRepository(repositoryId);
  CMISRepository cmisRepository=new CMISRepository(cmisAtomPubRepository,_cmisSearchQueryBuilder);
  cmisAtomPubRepository.setCmisRepository(cmisRepository);
  setupRepository(repositoryId,repository,cmisRepository);
  setupRepository(repositoryId,repository,cmisAtomPubRepository);
  if (!ExportImportThreadLocal.isImportInProcess()) {
    cmisAtomPubRepository.initRepository();
  }
  return cmisAtomPubRepository;
}
