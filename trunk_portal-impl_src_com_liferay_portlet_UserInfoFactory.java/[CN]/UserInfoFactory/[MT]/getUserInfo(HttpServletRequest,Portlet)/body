{
  if (request.getRemoteUser() == null) {
    return null;
  }
  LinkedHashMap<String,String> userInfo=new LinkedHashMap<String,String>();
  try {
    User user=PortalUtil.getUser(request);
    userInfo=getUserInfo(user,userInfo,portlet);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return userInfo;
}
