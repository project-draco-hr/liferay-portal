{
  initPortalServices();
  registerActionClass(CamelFooService.class);
  MockHttpServletRequest mockHttpServletRequest=createHttpRequest("/camelfoo/post/value/123");
  try {
    lookupJSONWebServiceAction(mockHttpServletRequest);
    fail();
  }
 catch (  RuntimeException re) {
  }
  mockHttpServletRequest=createHttpRequest("/camelfoo/post/value/123",HttpMethods.POST);
  JSONWebServiceAction jsonWebServiceAction=lookupJSONWebServiceAction(mockHttpServletRequest);
  assertNotNull(jsonWebServiceAction);
  assertEquals("post 123",jsonWebServiceAction.invoke());
}
