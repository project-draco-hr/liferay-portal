{
  initPortalServices();
  registerActionClass(CamelFooService.class);
  MockHttpServletRequest mockHttpServletRequest=createHttpRequest("/camelfoo/post/value/123");
  try {
    lookupAction(mockHttpServletRequest);
    fail();
  }
 catch (  RuntimeException ignore) {
  }
  mockHttpServletRequest=createHttpRequest("/camelfoo/post/value/123","POST");
  JSONWebServiceAction jsonWebServiceAction=lookupAction(mockHttpServletRequest);
  assertNotNull(jsonWebServiceAction);
  assertEquals("post 123",jsonWebServiceAction.invoke());
}
