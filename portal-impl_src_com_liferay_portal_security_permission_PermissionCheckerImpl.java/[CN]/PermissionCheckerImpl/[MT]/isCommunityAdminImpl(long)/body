{
  if (isOmniadmin()) {
    return true;
  }
  if (groupId <= 0) {
    return false;
  }
  Group group=GroupLocalServiceUtil.getGroup(groupId);
  if (isCompanyAdmin(group.getCompanyId())) {
    return true;
  }
  PermissionCheckerBag bag=getBag(groupId);
  if (bag == null) {
    _log.error("Bag should never be null");
  }
  if (bag.isCommunityAdmin(this,group)) {
    return true;
  }
 else {
    return false;
  }
}
