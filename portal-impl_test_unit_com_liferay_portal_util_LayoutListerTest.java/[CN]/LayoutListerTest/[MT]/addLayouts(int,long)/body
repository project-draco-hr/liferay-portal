{
  if (depth >= _DEPTH) {
    return;
  }
  Layout layout=createLayout(++_plid,parentLayoutId);
  _layouts.add(layout);
  for (int i=0; i < _NODES; i++) {
    Layout childLayout=createLayout(++_plid,layout.getPlid());
    _layouts.add(childLayout);
    addLayouts(depth + 1,childLayout.getPlid());
  }
}
