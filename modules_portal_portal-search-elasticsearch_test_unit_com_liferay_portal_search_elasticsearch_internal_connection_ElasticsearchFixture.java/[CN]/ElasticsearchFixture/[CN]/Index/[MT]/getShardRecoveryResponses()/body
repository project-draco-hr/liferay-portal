{
  IndicesAdminClient indicesAdminClient=getIndicesAdminClient();
  RecoveryRequestBuilder recoveryRequestBuilder=indicesAdminClient.prepareRecoveries(_name);
  RecoveryResponse recoveryResponse=recoveryRequestBuilder.get();
  Map<String,List<ShardRecoveryResponse>> shardResponses=recoveryResponse.shardResponses();
  return shardResponses.get(_name);
}
