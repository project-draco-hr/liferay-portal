{
  if (_clusterSettingsContext == null) {
    return;
  }
  UnicastSettingsContributor unicastSettingsContributor=new UnicastSettingsContributor();
  unicastSettingsContributor.setClusterSettingsContext(_clusterSettingsContext);
  unicastSettingsContributor.activate(_elasticsearchConfigurationProperties);
  embeddedElasticsearchConnection.addSettingsContributor(unicastSettingsContributor);
}
