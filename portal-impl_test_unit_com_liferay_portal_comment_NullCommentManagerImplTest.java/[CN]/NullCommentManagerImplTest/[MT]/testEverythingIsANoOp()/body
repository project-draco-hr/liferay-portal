{
  Assert.assertEquals(0,_commentManager.addComment(_USER_ID,_GROUP_ID,_CLASS_NAME,_CLASS_PK,_USER_NAME,_SUBJECT,_BODY,null));
  _commentManager.addInitialDiscussion(_USER_ID,_GROUP_ID,_CLASS_NAME,_CLASS_PK,_USER_NAME);
  _commentManager.deleteComment(_COMMENT_ID);
  _commentManager.deleteDiscussion(_CLASS_NAME,_CLASS_PK);
}
