{
  if (names.length == 0) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(names.length * 2 - 1);
  for (int i=0; i < names.length; i++) {
    sb.append(StringUtil.quote(names[i],StringPool.APOSTROPHE));
    if ((i + 1) < names.length) {
      sb.append(StringPool.COMMA);
    }
  }
  return sb.toString();
}
