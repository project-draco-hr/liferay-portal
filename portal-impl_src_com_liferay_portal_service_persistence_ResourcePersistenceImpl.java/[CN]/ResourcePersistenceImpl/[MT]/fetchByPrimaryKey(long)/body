{
  Resource resource=(Resource)EntityCacheUtil.getResult(ResourceModelImpl.ENTITY_CACHE_ENABLED,ResourceImpl.class,resourceId);
  if (resource == _nullResource) {
    return null;
  }
  if (resource == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      resource=(Resource)session.get(ResourceImpl.class,Long.valueOf(resourceId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (resource != null) {
        cacheResult(resource);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(ResourceModelImpl.ENTITY_CACHE_ENABLED,ResourceImpl.class,resourceId,_nullResource);
      }
      closeSession(session);
    }
  }
  return resource;
}
