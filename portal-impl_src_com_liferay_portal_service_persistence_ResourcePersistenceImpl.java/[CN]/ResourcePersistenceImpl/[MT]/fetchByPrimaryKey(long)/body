{
  Resource result=(Resource)EntityCacheUtil.getResult(ResourceModelImpl.ENTITY_CACHE_ENABLED,ResourceImpl.class,resourceId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      Resource resource=(Resource)session.get(ResourceImpl.class,new Long(resourceId));
      if (resource != null) {
        cacheResult(resource);
      }
      return resource;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (Resource)result;
  }
}
