{
  Object[] finderArgs=new Object[]{new Long(codeId)};
  List<Resource> list=(List<Resource>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_CODEID,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT resource FROM Resource resource WHERE ");
      query.append("resource.codeId = ?");
      query.append(" ");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(codeId);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<Resource>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_CODEID,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
