{
  EntityCacheUtil.removeResult(ResourceModelImpl.ENTITY_CACHE_ENABLED,ResourceImpl.class,resource.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_P,new Object[]{Long.valueOf(resource.getCodeId()),resource.getPrimKey()});
}
