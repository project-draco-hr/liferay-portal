{
  try (PreparedStatement ps=connection.prepareStatement("update Group_ set name = ? where groupId = ?")){
    StringBundler sb=new StringBundler(3);
    sb.append(classPK);
    sb.append(_ORGANIZATION_NAME_DELIMETER);
    sb.append(name);
    ps.setString(1,sb.toString());
    ps.setLong(2,groupId);
    ps.executeUpdate();
  }
 }
