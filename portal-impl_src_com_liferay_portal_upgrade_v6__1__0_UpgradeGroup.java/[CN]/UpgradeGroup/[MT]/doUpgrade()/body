{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select groupId, classNameId from group_");
    rs=ps.executeQuery();
    while (rs.next()) {
      long groupId=rs.getLong("groupId");
      long classNameId=rs.getLong("classNameId");
      boolean site=isSite(groupId,classNameId);
      runSQL("update group_ set site = " + site + " where groupId = "+ groupId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
