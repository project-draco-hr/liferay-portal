{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select classNameId from ClassName_ where value = ?");
    ps.setString(1,className);
    rs=ps.executeQuery();
    if (rs.next()) {
      return rs.getLong("classNameId");
    }
    return 0;
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
