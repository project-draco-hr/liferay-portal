{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select value from classname_ where classNameId = ?");
    ps.setLong(1,classNameId);
    rs=ps.executeQuery();
    if (rs.next() && rs.getString("value").equals(Group.class.getName())) {
      return true;
    }
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select * from layout where groupId = ?");
    ps.setLong(1,groupId);
    rs=ps.executeQuery();
    if (rs.next()) {
      return true;
    }
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
  return false;
}
