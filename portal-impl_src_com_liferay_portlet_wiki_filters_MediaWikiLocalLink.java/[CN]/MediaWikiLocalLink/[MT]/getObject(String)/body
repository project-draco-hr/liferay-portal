{
  String value=key;
  Matcher match=_pattern.matcher(key);
  if (match.find()) {
    key=encodeKey(match.group(3).trim());
    String title=match.group(6).trim();
    if (_pages.contains(key)) {
      value="\nview{" + title + ","+ key+ "}\n";
    }
 else {
      value="\nedit{" + title + ","+ key+ "}\n";
    }
  }
  return value;
}
