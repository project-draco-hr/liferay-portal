{
  String ret=key;
  Matcher match=pattern.matcher(key);
  if (match.find()) {
    key=match.group(3);
    String title=match.group(6);
    if (pages.contains(_encodeKey(key.trim()))) {
      ret="\nview{" + title.trim() + ","+ _encodeKey(key.trim())+ "}\n";
    }
 else {
      ret="\nedit{" + title.trim() + ","+ _encodeKey(key.trim())+ "}\n";
    }
  }
  return ret;
}
