{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("link=Workflow Configuration"));
  selenium.waitForPageToLoad("30000");
  selenium.select("_152_workflowDefinitionName@com.liferay.portlet.documentlibrary.model.DLFileEntry",RuntimeVariables.replace("label=Single Approver (Version 1)"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request processed successfully."),selenium.getText("//section/div/div/div/div"));
  assertEquals("Single Approver (Version 1)",selenium.getSelectedLabel("_152_workflowDefinitionName@com.liferay.portlet.documentlibrary.model.DLFileEntry"));
}
