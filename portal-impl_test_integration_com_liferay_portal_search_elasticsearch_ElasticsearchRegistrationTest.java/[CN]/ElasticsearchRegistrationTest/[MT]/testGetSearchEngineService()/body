{
  Registry registry=RegistryUtil.getRegistry();
  Collection<SearchEngine> searchEngines=registry.getServices(SearchEngine.class,"(search.engine.id=SYSTEM_ENGINE)");
  Assert.assertEquals(1,searchEngines.size());
  for (  SearchEngine searchEngine : searchEngines) {
    Class<? extends SearchEngine> searchEngineClass=searchEngine.getClass();
    String searchEngineClassName=searchEngineClass.getName();
    Assert.assertTrue("The registered search engine is " + searchEngineClassName,searchEngineClassName.endsWith("ElasticsearchSearchEngine"));
  }
}
