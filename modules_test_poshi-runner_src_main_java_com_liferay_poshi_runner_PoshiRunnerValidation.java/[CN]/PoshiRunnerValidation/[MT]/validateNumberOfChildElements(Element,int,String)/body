{
  List<Element> childElements=element.elements();
  if (childElements.isEmpty()) {
    _exceptions.add(new Exception("Missing child elements\n" + filePath + ":"+ element.attributeValue("line-number")));
  }
 else   if (childElements.size() > number) {
    _exceptions.add(new Exception("Too many child elements\n" + filePath + ":"+ element.attributeValue("line-number")));
  }
 else   if (childElements.size() < number) {
    _exceptions.add(new Exception("Too few child elements\n" + filePath + ":"+ element.attributeValue("line-number")));
  }
}
