{
  String classCommandName=element.attributeValue(macroType);
  List<String> returns=PoshiRunnerContext.getMacroCommandReturns(classCommandName);
  String returnVariable=returnElement.attributeValue("from");
  if (!returns.contains(returnVariable)) {
    _exceptions.add(new Exception(returnVariable + " not specified as a return variable\n" + filePath+ ":"+ element.attributeValue("line-number")));
  }
}
