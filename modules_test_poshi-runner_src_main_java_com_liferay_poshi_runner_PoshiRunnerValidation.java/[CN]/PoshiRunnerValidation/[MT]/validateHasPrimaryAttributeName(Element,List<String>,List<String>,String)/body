{
  List<String> attributeNames=new ArrayList<>();
  for (  String primaryAttributeName : primaryAttributeNames) {
    if (Validator.isNotNull(element.attributeValue(primaryAttributeName))) {
      attributeNames.add(primaryAttributeName);
    }
  }
  if (attributeNames.size() < 1) {
    _exceptions.add(new Exception("Invalid or missing attribute\n" + filePath + ":"+ element.attributeValue("line-number")));
  }
 else   if (attributeNames.size() > 1) {
    if (Validator.isNull(multiplePrimaryAttributeNames)) {
      _exceptions.add(new Exception("Too many attributes\n" + filePath + ":"+ element.attributeValue("line-number")));
    }
 else {
      validateHasMultiplePrimaryAttributeNames(element,attributeNames,multiplePrimaryAttributeNames,filePath);
    }
  }
}
