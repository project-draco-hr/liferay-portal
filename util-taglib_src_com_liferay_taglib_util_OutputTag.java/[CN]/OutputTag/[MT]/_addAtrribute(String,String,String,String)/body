{
  int x=0;
  int y=0;
  while (x >= 0) {
    x=content.indexOf("<" + tagName,y);
    if (x < 0) {
      break;
    }
    y=content.indexOf(">",x);
    if (y < 0) {
      break;
    }
    String subcontent=content.substring(x,y);
    if (!subcontent.contains(attributeName)) {
      content=StringUtil.insert(content," " + attributeName + "="+ attributeValue,x + tagName.length() + 1);
    }
  }
  return content;
}
