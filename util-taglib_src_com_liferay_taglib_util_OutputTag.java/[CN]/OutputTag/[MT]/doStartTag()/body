{
  if (Validator.isNotNull(_outputKey)) {
    Set<String> outputKeys=getOutputKeys();
    if (!outputKeys.add(_outputKey)) {
      _output=false;
      return SKIP_BODY;
    }
  }
  if (isPositionInLine()) {
    _output=false;
    return EVAL_BODY_INCLUDE;
  }
  _output=true;
  return EVAL_BODY_BUFFERED;
}
