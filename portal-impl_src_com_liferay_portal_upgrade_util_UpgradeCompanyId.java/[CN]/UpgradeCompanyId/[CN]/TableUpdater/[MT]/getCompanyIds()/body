{
  List<Long> companyIds=new ArrayList<>();
  try (PreparedStatement ps=connection.prepareStatement("select companyId from Company");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      companyIds.add(rs.getLong(1));
    }
  }
 catch (  SQLException sqle) {
    throw new RuntimeException(sqle);
  }
  return companyIds;
}
