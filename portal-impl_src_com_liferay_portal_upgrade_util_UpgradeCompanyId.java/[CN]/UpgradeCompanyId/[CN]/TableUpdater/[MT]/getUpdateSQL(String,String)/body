{
  StringBundler sb=new StringBundler();
  sb.append("update ");
  sb.append(_tableName);
  sb.append(" set companyId = (select companyId from ");
  sb.append(foreignTableName);
  sb.append(" where ");
  sb.append(foreignTableName);
  sb.append(".");
  sb.append(foreignColumnName);
  sb.append(" = ");
  sb.append(_tableName);
  sb.append(".");
  sb.append(_columnName);
  sb.append(")");
  return sb.toString();
}
