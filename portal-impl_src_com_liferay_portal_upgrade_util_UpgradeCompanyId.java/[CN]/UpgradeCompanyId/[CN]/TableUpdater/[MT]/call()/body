{
  if (_log.isInfoEnabled()) {
    _log.info("Adding column companyId to table " + getTableName());
  }
  try {
    _con=DataAccess.getUpgradeOptimizedConnection();
    runSQL(_con,"alter table " + getTableName() + " add companyId LONG");
    update();
  }
  finally {
    DataAccess.cleanUp(_con);
  }
  return null;
}
