{
  String emailEntryAddedEnabled=preferences.getValue("emailEntryAddedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailEntryAddedEnabled)) {
    return GetterUtil.getBoolean(emailEntryAddedEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.BOOKMARKS_EMAIL_ENTRY_ADDED_ENABLED));
  }
}
