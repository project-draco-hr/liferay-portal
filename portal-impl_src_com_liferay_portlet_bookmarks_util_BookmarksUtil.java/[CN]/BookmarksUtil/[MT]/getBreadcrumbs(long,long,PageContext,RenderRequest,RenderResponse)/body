{
  if (entryId > 0) {
    BookmarksEntry entry=BookmarksEntryLocalServiceUtil.getEntry(entryId);
    return getBreadcrumbs(entry.getFolder(),entry,pageContext,renderRequest,renderResponse);
  }
 else {
    BookmarksFolder folder=null;
    try {
      folder=BookmarksFolderLocalServiceUtil.getFolder(folderId);
    }
 catch (    Exception e) {
    }
    return getBreadcrumbs(folder,null,pageContext,renderRequest,renderResponse);
  }
}
