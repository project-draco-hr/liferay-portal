{
  Group group=GroupLocalServiceUtil.getGroup(groupId);
  UnicodeProperties typeSettingsProperties=group.getTypeSettingsProperties();
  int trashEnabled=PrefsPropsUtil.getInteger(group.getCompanyId(),PropsKeys.TRASH_ENABLED);
  if (trashEnabled == TRASH_DISABLED) {
    return false;
  }
  return GetterUtil.getBoolean(typeSettingsProperties.getProperty("trashEnabled"),(trashEnabled == TRASH_ENABLED_BY_DEFAULT));
}
