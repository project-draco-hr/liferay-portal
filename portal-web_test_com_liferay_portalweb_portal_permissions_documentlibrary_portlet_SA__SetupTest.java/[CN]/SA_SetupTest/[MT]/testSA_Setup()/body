{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Document Library Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Document Library Permissions Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("//input[@value='Permissions']"));
  selenium.waitForPageToLoad("30000");
  selenium.check("15_ACTION_VIEW");
  selenium.click(RuntimeVariables.replace("//input[@value='Submit']"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("link=Document Library Permissions Test Page"));
  selenium.waitForPageToLoad("30000");
