{
  ModelListener listener=null;
  if (Validator.isNotNull(_LISTENER)) {
    try {
      listener=(ModelListener)Class.forName(_LISTENER).newInstance();
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(entryId));
  }
  com.liferay.portlet.blogs.model.BlogsEntry blogsEntry=getPersistence().remove(entryId);
  if (listener != null) {
    listener.onAfterRemove(blogsEntry);
  }
  return blogsEntry;
}
