{
  request.setAttribute("liferay-ui:search-toggle:autoFocus",String.valueOf(_autoFocus));
  request.setAttribute("liferay-ui:search-toggle:buttonLabel",_buttonLabel);
  request.setAttribute("liferay-ui:search-toggle:displayTerms",_displayTerms);
  request.setAttribute("liferay-ui:search-toggle:id",_id);
  request.setAttribute("liferay-ui:search-toggle:width",_width);
}
