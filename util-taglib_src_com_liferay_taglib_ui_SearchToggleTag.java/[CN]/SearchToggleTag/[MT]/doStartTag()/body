{
  try {
    ServletRequest req=pageContext.getRequest();
    String randomNamespace=PwdGenerator.getPassword(PwdGenerator.KEY3,4) + StringPool.UNDERLINE;
    req.setAttribute("liferay-ui:search-toggle:randomNamespace",randomNamespace);
    req.setAttribute("liferay-ui:search-toggle:id",_id);
    req.setAttribute("liferay-ui:search-toggle:displayTerms",_displayTerms);
    pageContext.include(getStartPage());
    return EVAL_BODY_INCLUDE;
  }
 catch (  Exception e) {
    throw new JspException(e);
  }
}
