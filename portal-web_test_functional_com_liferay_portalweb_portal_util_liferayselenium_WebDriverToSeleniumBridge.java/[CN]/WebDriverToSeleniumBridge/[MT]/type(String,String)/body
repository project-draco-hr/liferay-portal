{
  WebElement webElement=getWebElement(locator);
  if (TestPropsValues.MOBILE_DEVICE_ENABLED) {
    webElement.clear();
    webElement.click();
    try {
      Thread.sleep(1000);
    }
 catch (    Exception e) {
    }
    Screen _screen=new Screen();
    _screen.type(value);
  }
 else   if (webElement.isEnabled()) {
    webElement.clear();
    webElement.sendKeys(value);
  }
}
