{
  if (locator.contains("x:")) {
    String url=getHtmlNodeHref(locator);
    open(url);
  }
 else {
    WebElement webElement=getWebElement(webDriver,locator);
    try {
      webElement.click();
    }
 catch (    Exception e) {
      if (!webElement.isDisplayed()) {
        scrollWebElementIntoView(webDriver,webElement);
      }
      webElement.click();
    }
  }
}
