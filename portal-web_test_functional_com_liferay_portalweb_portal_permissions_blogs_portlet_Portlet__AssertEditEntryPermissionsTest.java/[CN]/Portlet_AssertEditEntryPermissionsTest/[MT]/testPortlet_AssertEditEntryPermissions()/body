{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Blogs Permissions Page",RuntimeVariables.replace("Blogs Permissions Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//div[@class='entry-title']/h2/a",RuntimeVariables.replace("Blogs Entry Title Temporary"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isVisible("link=Permissions"));
  selenium.clickAt("link=Permissions",RuntimeVariables.replace("Permissions"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isVisible("//div[@id='_86_rolesSearchContainer']"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
}
