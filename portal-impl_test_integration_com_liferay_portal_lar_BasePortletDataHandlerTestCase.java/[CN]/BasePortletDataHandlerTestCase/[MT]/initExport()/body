{
  Map<String,String[]> parameterMap=new LinkedHashMap<String,String[]>();
  parameterMap.put(PortletDataHandlerKeys.DELETIONS,new String[]{Boolean.TRUE.toString()});
  addParameters(parameterMap);
  TestReaderWriter testReaderWriter=new TestReaderWriter();
  portletDataContext=PortletDataContextFactoryUtil.createExportPortletDataContext(stagingGroup.getCompanyId(),stagingGroup.getGroupId(),parameterMap,getStartDate(),getEndDate(),testReaderWriter);
  rootElement=SAXReaderUtil.createElement("root");
  portletDataContext.setExportDataRootElement(rootElement);
  missingReferencesElement=SAXReaderUtil.createElement("missing-references");
  portletDataContext.setMissingReferencesElement(missingReferencesElement);
}
