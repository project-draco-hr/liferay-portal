{
  Session session=null;
  try {
    session=openSession();
    Lock lock=(Lock)session.get(LockImpl.class,new Long(lockId));
    if (lock == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + lockId);
      }
      throw new NoSuchLockException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + lockId);
    }
    return remove(lock);
  }
 catch (  NoSuchLockException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
