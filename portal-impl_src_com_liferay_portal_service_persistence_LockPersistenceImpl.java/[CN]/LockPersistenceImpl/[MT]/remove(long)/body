{
  Session session=null;
  try {
    session=openSession();
    Lock lock=(Lock)session.get(LockImpl.class,new Long(lockId));
    if (lock == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No Lock exists with the primary key " + lockId);
      }
      throw new NoSuchLockException("No Lock exists with the primary key " + lockId);
    }
    return remove(lock);
  }
 catch (  NoSuchLockException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
