{
  Object[] finderArgs=new Object[]{expirationDate};
  Long count=(Long)FinderCacheUtil.getResult(FINDER_PATH_COUNT_BY_EXPIRATIONDATE,finderArgs,this);
  if (count == null) {
    Session session=null;
    try {
      session=openSession();
      StringBundler query=new StringBundler();
      query.append(_SQL_COUNT_LOCK_WHERE);
      if (expirationDate == null) {
        query.append(_FINDER_COLUMN_EXPIRATIONDATE_EXPIRATIONDATE_1);
      }
 else {
        query.append(_FINDER_COLUMN_EXPIRATIONDATE_EXPIRATIONDATE_2);
      }
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      if (expirationDate != null) {
        qPos.add(CalendarUtil.getTimestamp(expirationDate));
      }
      count=(Long)q.uniqueResult();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (count == null) {
        count=Long.valueOf(0);
      }
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_EXPIRATIONDATE,finderArgs,count);
      closeSession(session);
    }
  }
  return count.intValue();
}
