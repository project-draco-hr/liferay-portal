{
  _name=name;
  if (_gadgetRemoteModel != null) {
    try {
      Class<?> clazz=_gadgetRemoteModel.getClass();
      Method method=clazz.getMethod("setName",String.class);
      method.invoke(_gadgetRemoteModel,name);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
