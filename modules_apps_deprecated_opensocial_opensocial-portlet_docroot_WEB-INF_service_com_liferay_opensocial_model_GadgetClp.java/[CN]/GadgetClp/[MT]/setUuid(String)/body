{
  _uuid=uuid;
  if (_gadgetRemoteModel != null) {
    try {
      Class<?> clazz=_gadgetRemoteModel.getClass();
      Method method=clazz.getMethod("setUuid",String.class);
      method.invoke(_gadgetRemoteModel,uuid);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
