{
  Registry registry=RegistryUtil.getRegistry();
  Class<?> clazz=getClass();
  Filter filter=registry.getFilter("(&(objectClass=" + DDMFormFieldRenderer.class.getName() + ")(!(objectClass="+ clazz.getName()+ ")))");
  _serviceTracker=registry.trackServices(filter,new DDMFormFieldRendererServiceTrackerCustomizer());
  _serviceTracker.open();
}
