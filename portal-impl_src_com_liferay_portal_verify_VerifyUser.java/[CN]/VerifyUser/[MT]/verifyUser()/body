{
  for (  String type : AnnouncementsEntryImpl.TYPES) {
    List<User> users=UserLocalServiceUtil.getNoAnnouncementsDeliveries(type);
    if (_log.isDebugEnabled()) {
      _log.debug("Processing " + users.size() + " users with no announcements deliveries of type "+ type);
    }
    for (    User user : users) {
      AnnouncementsDeliveryLocalServiceUtil.getUserDelivery(user.getUserId(),type);
    }
    if (_log.isDebugEnabled()) {
      _log.debug("Announcements deliveries of type " + type + " verified for users");
    }
  }
}
