{
  ThemeDisplay themeDisplay=(ThemeDisplay)renderRequest.getAttribute(WebKeys.THEME_DISPLAY);
  this._dateFormatDateTime=FastDateFormatFactoryUtil.getDateTime(themeDisplay.getLocale(),themeDisplay.getTimeZone());
  this._renderResponse=renderResponse;
  this._renderRequest=renderRequest;
  this._workflowTaskRequestHelper=new WorkflowTaskRequestHelper(renderRequest);
  this._roleMap=new HashMap<>();
  this._userMap=new HashMap<>();
}
