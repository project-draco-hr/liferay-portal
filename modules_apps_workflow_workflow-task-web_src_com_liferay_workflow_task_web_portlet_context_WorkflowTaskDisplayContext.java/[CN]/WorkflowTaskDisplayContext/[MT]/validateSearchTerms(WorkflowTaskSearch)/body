{
  WorkflowTaskDisplayTerms searchTerms=(WorkflowTaskDisplayTerms)searchContainer.getDisplayTerms();
  if (Validator.isNotNull(searchTerms.getKeywords()) || Validator.isNotNull(searchTerms.getName()) || Validator.isNotNull(searchTerms.getType())) {
    searchContainer.setEmptyResultsMessage(searchContainer.getEmptyResultsMessage() + WorkflowTaskConstants.WITH_THE_SPECIFIED_SEARCH_CRITERIA);
  }
}
