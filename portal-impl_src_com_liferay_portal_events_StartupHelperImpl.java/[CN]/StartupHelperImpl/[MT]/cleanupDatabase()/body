{
  try {
    DBUtil dbUtil=DBUtil.getInstance();
    dbUtil.runSQL(_DELETE_TEMP_IMAGES_1);
    dbUtil.runSQL(_DELETE_TEMP_IMAGES_2);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  if (_upgraded) {
    try {
      if (_log.isInfoEnabled()) {
        _log.info("Adding indexes");
      }
      DBUtil.getInstance().runSQLTemplate("indexes.sql",false);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
}
