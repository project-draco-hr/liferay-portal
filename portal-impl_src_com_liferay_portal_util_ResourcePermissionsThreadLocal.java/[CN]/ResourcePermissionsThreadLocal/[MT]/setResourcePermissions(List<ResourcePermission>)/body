{
  if (resourcePermissions != null) {
    Map<Long,ResourcePermission> resourcePermissionMap=new HashMap<>();
    for (    ResourcePermission resourcePermission : resourcePermissions) {
      resourcePermissionMap.put(resourcePermission.getRoleId(),resourcePermission);
    }
    _resourcePermissions.set(resourcePermissionMap);
  }
 else {
    _resourcePermissions.remove();
  }
}
