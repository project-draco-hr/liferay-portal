{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long groupId=ParamUtil.getLong(portletRequest,"groupId");
  String workflowDefinition=ParamUtil.getString(portletRequest,"workflowDefinition");
  _workflowDefinitionLinkLocalService.updateWorkflowDefinitionLink(themeDisplay.getUserId(),themeDisplay.getCompanyId(),groupId,DDLRecordSet.class.getName(),recordSet.getRecordSetId(),0,workflowDefinition);
}
