{
  MBCategory newMBCategory=addMBCategory();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBCategory.class,MBCategory.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("categoryId",nextLong()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
