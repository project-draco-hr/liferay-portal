{
  String property=analyzer.getProperty("Require-SchemaVersion");
  if (property == null) {
    return "";
  }
  StringBuffer sb=new StringBuffer(6);
  sb.append(Object.class.getName());
  sb.append(" (&(release.schema.version=");
  sb.append(property);
  sb.append(")(release.bundle.symbolic.name=");
  Map.Entry<String,Attrs> bundleSymbolicName=analyzer.getBundleSymbolicName();
  sb.append(bundleSymbolicName.getKey());
  sb.append("))");
  return sb.toString();
}
