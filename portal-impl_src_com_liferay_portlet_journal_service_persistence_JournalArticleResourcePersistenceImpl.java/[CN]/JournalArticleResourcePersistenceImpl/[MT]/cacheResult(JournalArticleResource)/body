{
  EntityCacheUtil.putResult(JournalArticleResourceModelImpl.ENTITY_CACHE_ENABLED,JournalArticleResourceImpl.class,journalArticleResource.getPrimaryKey(),journalArticleResource);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{journalArticleResource.getUuid(),journalArticleResource.getGroupId()},journalArticleResource);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_A,new Object[]{journalArticleResource.getGroupId(),journalArticleResource.getArticleId()},journalArticleResource);
  journalArticleResource.resetOriginalValues();
}
