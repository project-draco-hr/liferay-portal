{
  EntityCacheUtil.removeResult(JournalArticleResourceModelImpl.ENTITY_CACHE_ENABLED,JournalArticleResourceImpl.class,journalArticleResource.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{journalArticleResource.getUuid(),Long.valueOf(journalArticleResource.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_A,new Object[]{Long.valueOf(journalArticleResource.getGroupId()),journalArticleResource.getArticleId()});
}
