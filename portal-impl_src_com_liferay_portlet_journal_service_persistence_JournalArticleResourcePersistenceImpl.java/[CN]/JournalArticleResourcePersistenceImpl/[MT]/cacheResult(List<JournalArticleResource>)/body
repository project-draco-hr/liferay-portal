{
  for (  JournalArticleResource journalArticleResource : journalArticleResources) {
    if (EntityCacheUtil.getResult(JournalArticleResourceModelImpl.ENTITY_CACHE_ENABLED,JournalArticleResourceImpl.class,journalArticleResource.getPrimaryKey()) == null) {
      cacheResult(journalArticleResource);
    }
 else {
      journalArticleResource.resetOriginalValues();
    }
  }
}
