{
  ExpandoBridge expandoBridge=ExpandoBridgeFactoryUtil.getExpandoBridge(companyId,DLFileEntryConstants.getClassName(),0);
  if (preset.startsWith("Preset")) {
    return addPresetExpando(expandoBridge,preset,name);
  }
 else {
    int type=GetterUtil.getInteger(preset);
    expandoBridge.addAttribute(name,type);
    return type;
  }
}
