{
  mdrRule=toUnwrappedModel(mdrRule);
  Session session=null;
  try {
    session=openSession();
    if (mdrRule.isCachedModel()) {
      mdrRule=(MDRRule)session.get(MDRRuleImpl.class,mdrRule.getPrimaryKeyObj());
    }
    session.delete(mdrRule);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(mdrRule);
  return mdrRule;
}
