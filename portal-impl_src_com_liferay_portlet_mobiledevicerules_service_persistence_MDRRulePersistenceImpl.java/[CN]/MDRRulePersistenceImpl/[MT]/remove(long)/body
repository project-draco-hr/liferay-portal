{
  Session session=null;
  try {
    session=openSession();
    MDRRule mdrRule=(MDRRule)session.get(MDRRuleImpl.class,Long.valueOf(ruleId));
    if (mdrRule == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + ruleId);
      }
      throw new NoSuchRuleException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + ruleId);
    }
    return mdrRulePersistence.remove(mdrRule);
  }
 catch (  NoSuchRuleException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
