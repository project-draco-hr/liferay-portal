{
  try {
    StringBuilder sb=new StringBuilder();
    com.liferay.portal.kernel.xml.Document doc=SAXReaderUtil.read(content);
    Element root=doc.getRootElement();
    _getIndexableContent(sb,root);
    return sb.toString();
  }
 catch (  Exception e) {
    e.printStackTrace();
    return content;
  }
}
