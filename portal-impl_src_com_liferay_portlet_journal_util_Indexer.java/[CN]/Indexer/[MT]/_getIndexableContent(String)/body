{
  try {
    StringMaker sm=new StringMaker();
    SAXReader reader=new SAXReader();
    org.dom4j.Document doc=reader.read(new StringReader(content));
    Element root=doc.getRootElement();
    _getIndexableContent(sm,root);
    return sm.toString();
  }
 catch (  Exception e) {
    e.printStackTrace();
    return content;
  }
}
