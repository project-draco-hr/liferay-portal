{
  try {
    StringBuilder sb=new StringBuilder();
    SAXReader reader=new SAXReader();
    org.dom4j.Document doc=reader.read(new StringReader(content));
    Element root=doc.getRootElement();
    _getIndexableContent(sb,root);
    return sb.toString();
  }
 catch (  Exception e) {
    e.printStackTrace();
    return content;
  }
}
