{
  try {
    String xml=getInstancesXml(definitionId,instanceId,definitionName,definitionVersion,startDateGT,startDateLT,endDateGT,endDateLT,hideEndedTasks,retrieveUserInstances,andOperator,start,end);
    return WorkflowXMLUtil.parseList(xml,"instances");
  }
 catch (  Exception e) {
    throw new WorkflowComponentException(e);
  }
}
