{
  StringBundler sb=new StringBundler();
  String command=(String)arguments[0];
  _actionStepCount=1;
  sb.append("&nbsp;&nbsp;&nbsp;&nbsp;");
  sb.append("<b>");
  sb.append(_macroStepCount);
  sb.append(". ");
  sb.append(command);
  sb.append("</b>");
  _macroStepCount++;
  log("descriptionLog",sb.toString(),"descriptionLog");
}
