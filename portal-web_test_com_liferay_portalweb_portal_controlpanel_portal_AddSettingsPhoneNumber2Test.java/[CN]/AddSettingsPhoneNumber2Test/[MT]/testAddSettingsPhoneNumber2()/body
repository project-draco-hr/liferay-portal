{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Settings")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Settings"));
  selenium.waitForPageToLoad("30000");
  selenium.click("phoneNumbersLink");
  selenium.click("//div[@id='phoneNumbers']/fieldset/div[2]/div/span/a[1]");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("_130_phoneNumber2")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.typeKeys("_130_phoneNumber2",RuntimeVariables.replace("321-321-4321"));
  selenium.type("_130_phoneNumber2",RuntimeVariables.replace("321-321-4321"));
  selenium.type("_130_phoneExtension2",RuntimeVariables.replace("321"));
  selenium.select("_130_phoneTypeId2",RuntimeVariables.replace("label=Fax"));
  selenium.click(RuntimeVariables.replace("//input[@value='Save']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
}
