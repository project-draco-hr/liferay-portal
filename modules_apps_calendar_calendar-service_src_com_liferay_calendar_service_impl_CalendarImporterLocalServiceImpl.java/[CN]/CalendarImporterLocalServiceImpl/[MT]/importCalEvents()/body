{
  ActionableDynamicQuery actionableDynamicQuery=calEventLocalService.getActionableDynamicQuery();
  actionableDynamicQuery.setPerformActionMethod(new ActionableDynamicQuery.PerformActionMethod<CalEvent>(){
    @Override public void performAction(    CalEvent calEvent) throws PortalException {
      importCalEvent(calEvent);
    }
  }
);
  actionableDynamicQuery.performActions();
}
