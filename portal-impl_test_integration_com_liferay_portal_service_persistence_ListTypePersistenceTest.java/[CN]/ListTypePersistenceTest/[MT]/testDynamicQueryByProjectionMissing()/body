{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ListType.class,ListType.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("listTypeId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("listTypeId",new Object[]{RandomTestUtil.nextInt()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
