{
  if (classNameId != PortalUtil.getClassNameId(DDMStructure.class)) {
    return PortalUtil.getClassNameId("com.liferay.portlet.display.template.PortletDisplayTemplate");
  }
  if (classPK == 0) {
    return PortalUtil.getClassNameId("com.liferay.journal.model.JournalArticle");
  }
  return _structureClassNameIds.get(classPK);
}
