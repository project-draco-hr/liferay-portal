{
  try {
    runSQL("alter_column_type Resource_ primKey VARCHAR(255) null");
  }
 catch (  Exception e) {
    UpgradeTable upgradeTable=UpgradeTableFactoryUtil.getUpgradeTable(ResourceTable.TABLE_NAME,ResourceTable.TABLE_COLUMNS);
