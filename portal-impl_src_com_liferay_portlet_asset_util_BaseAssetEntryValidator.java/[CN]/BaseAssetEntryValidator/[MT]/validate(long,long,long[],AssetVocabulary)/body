{
  if (!vocabulary.isAssociatedToClassNameAndType(classNameId,classTypePK)) {
    return;
  }
  if (!isAssetCategorizable(classNameId)) {
    return;
  }
  if (vocabulary.isMissingRequiredCategory(classNameId,classTypePK,categoryIds)) {
    throw new AssetCategoryException(vocabulary,AssetCategoryException.AT_LEAST_ONE_CATEGORY);
  }
  if (!vocabulary.isMultiValued() && vocabulary.hasMoreThanOneCategorySelected(categoryIds)) {
    throw new AssetCategoryException(vocabulary,AssetCategoryException.TOO_MANY_CATEGORIES);
  }
}
