{
  if (!vocabulary.isAssociatedToAssetRendererFactory(classNameId)) {
    return;
  }
  if (!isAssetCategorizable(classNameId)) {
    return;
  }
  if (vocabulary.isMissingRequiredCategory(classNameId,categoryIds)) {
    throw new AssetCategoryException(vocabulary,AssetCategoryException.AT_LEAST_ONE_CATEGORY);
  }
  if (!vocabulary.isMultiValued()) {
    if (ListUtil.count(categories,existingCategoryFilter) > 1) {
      throw new AssetCategoryException(vocabulary,AssetCategoryException.TOO_MANY_CATEGORIES);
    }
  }
}
