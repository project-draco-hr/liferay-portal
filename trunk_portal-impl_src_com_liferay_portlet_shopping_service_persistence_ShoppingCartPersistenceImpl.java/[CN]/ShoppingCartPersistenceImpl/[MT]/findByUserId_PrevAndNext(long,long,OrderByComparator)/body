{
  ShoppingCart shoppingCart=findByPrimaryKey(cartId);
  Session session=null;
  try {
    session=openSession();
    ShoppingCart[] array=new ShoppingCartImpl[3];
    array[0]=getByUserId_PrevAndNext(session,shoppingCart,userId,orderByComparator,true);
    array[1]=shoppingCart;
    array[2]=getByUserId_PrevAndNext(session,shoppingCart,userId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
