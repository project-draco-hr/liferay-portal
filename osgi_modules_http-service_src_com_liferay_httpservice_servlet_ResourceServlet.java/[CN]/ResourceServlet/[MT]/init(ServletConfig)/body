{
  _bundleServletConfig=(BundleServletConfig)servletConfig;
  _alias=GetterUtil.getString(_bundleServletConfig.getInitParameter(HttpServicePropsKeys.HTTP_SERVICE_ALIAS));
  _httpContext=_bundleServletConfig.getHttpContext();
  _name=GetterUtil.getString(_bundleServletConfig.getInitParameter(HttpServicePropsKeys.HTTP_SERVICE_NAME));
}
