{
  DDMExpression<Boolean> ddmExpression1=_ddmExpressionFactory.createBooleanDDMExpression("between(22, 20, 25)");
  Assert.assertTrue(ddmExpression1.evaluate());
  DDMExpression<Boolean> ddmExpression2=_ddmExpressionFactory.createBooleanDDMExpression("between(19, 20, 25)");
  Assert.assertFalse(ddmExpression2.evaluate());
}
