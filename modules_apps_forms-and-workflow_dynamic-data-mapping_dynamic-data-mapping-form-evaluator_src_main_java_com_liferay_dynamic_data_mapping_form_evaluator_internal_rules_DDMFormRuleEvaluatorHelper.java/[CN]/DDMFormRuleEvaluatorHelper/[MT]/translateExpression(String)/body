{
  DDMExpression<Boolean> expression=_ddmExpressionFactory.createBooleanDDMExpression(expressionStr);
  Map<String,VariableDependencies> variableDependenciesMap=expression.getVariableDependenciesMap();
  for (  String variable : variableDependenciesMap.keySet()) {
    if (_ddmFormFieldEvaluationResults.containsKey(variable)) {
      expressionStr=expressionStr.replaceAll(String.format("([,\\s\\(]+|.*)(%s)([,\\s\\)]+|.*)",variable),String.format("$1get(fieldAt(\"%s\", 0), \"value\")$3",variable));
    }
  }
  return expressionStr;
}
