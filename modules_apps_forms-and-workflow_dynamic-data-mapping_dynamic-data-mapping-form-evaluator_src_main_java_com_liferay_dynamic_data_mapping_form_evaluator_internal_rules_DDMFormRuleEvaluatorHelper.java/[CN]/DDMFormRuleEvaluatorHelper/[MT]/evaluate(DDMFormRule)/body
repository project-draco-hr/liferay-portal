{
  DDMFormRuleEvaluator ddmFormRuleEvaluator=new DDMFormRuleEvaluator(_ddmDataProviderConsumerTracker,_ddmDataProviderInstanceService,_ddmExpressionFactory,_ddmFormFieldEvaluationResults,_ddmFormValuesJSONDeserializer,ddmFormRule.getCondition(),_jsonFactory);
  return ddmFormRuleEvaluator.evaluate();
}
