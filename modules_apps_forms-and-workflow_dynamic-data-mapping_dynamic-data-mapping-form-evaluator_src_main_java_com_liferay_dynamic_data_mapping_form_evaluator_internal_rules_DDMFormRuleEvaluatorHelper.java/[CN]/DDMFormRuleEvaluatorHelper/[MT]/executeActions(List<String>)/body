{
  for (  String action : actions) {
    DDMFormRuleEvaluator ddmFormRuleEvaluator=new DDMFormRuleEvaluator(_ddmDataProviderConsumerTracker,_ddmDataProviderInstanceService,_ddmExpressionFactory,_ddmFormFieldEvaluationResults,_ddmFormValuesJSONDeserializer,action,_jsonFactory);
    ddmFormRuleEvaluator.execute();
  }
}
