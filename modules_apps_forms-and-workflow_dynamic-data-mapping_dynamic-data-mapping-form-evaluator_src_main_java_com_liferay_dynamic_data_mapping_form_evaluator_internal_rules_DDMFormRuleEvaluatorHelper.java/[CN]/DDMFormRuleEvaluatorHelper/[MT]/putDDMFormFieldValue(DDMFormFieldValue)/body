{
  List<DDMFormFieldValue> ddmFormFieldValues=_ddmFormFieldValuesMap.get(ddmFormFieldValue.getName());
  if (ddmFormFieldValues == null) {
    ddmFormFieldValues=new ArrayList<>();
    _ddmFormFieldValuesMap.put(ddmFormFieldValue.getName(),ddmFormFieldValues);
  }
  ddmFormFieldValues.add(ddmFormFieldValue);
  for (  DDMFormFieldValue nestedDDMFormFieldValue : ddmFormFieldValue.getNestedDDMFormFieldValues()) {
    putDDMFormFieldValue(nestedDDMFormFieldValue);
  }
}
