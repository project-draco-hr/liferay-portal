{
  String action=String.format("set(fieldAt(\"%s\", 0), \"visible\", %s)",ddmFormFieldName,translateExpression(visibilityExpression));
  List<String> actions=new ArrayList<>();
  actions.add(action);
  DDMFormRule ddmFormRule=new DDMFormRule("TRUE",actions);
  _ddmForm.addDDMFormRule(ddmFormRule);
}
