{
  long entryIdFolderId=getDLFolderId(_groupId,ddmFolderId,String.valueOf(_entryId));
  if (entryIdFolderId > 0) {
    return entryIdFolderId;
  }
  entryIdFolderId=increment();
  addDLFolder(PortalUUIDUtil.generate(),entryIdFolderId,_groupId,_companyId,_userId,_userName,_now,_now,_groupId,ddmFolderId,String.valueOf(_entryId),StringPool.BLANK,_now);
  return entryIdFolderId;
}
