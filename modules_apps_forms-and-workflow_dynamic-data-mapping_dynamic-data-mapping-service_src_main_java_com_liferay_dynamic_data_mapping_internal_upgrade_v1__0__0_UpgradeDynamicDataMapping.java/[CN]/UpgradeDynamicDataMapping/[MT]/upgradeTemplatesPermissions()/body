{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps=connection.prepareStatement("select * from DDMTemplate");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long companyId=rs.getLong("companyId");
      long templateId=rs.getLong("templateId");
      upgradeTemplatePermissions(companyId,templateId);
    }
  }
