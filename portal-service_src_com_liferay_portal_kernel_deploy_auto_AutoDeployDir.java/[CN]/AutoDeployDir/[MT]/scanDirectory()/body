{
  File[] files=_deployDir.listFiles();
  for (  File file : files) {
    String fileName=file.getName().toLowerCase();
    if (file.isFile() && (fileName.endsWith(".jar") || fileName.endsWith(".lpkg") || fileName.endsWith(".war")|| fileName.endsWith(".xml")|| fileName.endsWith(".zip"))) {
      processFile(file);
    }
  }
}
