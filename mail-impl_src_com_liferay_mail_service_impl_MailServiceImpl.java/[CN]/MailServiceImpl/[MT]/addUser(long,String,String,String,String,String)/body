{
  if (_log.isDebugEnabled()) {
    _log.debug("addUser");
  }
  String hookImpl=PropsUtil.get(PropsUtil.MAIL_HOOK_IMPL);
  MethodWrapper methodWrapper=new MethodWrapper(hookImpl,"addUser",new Object[]{new LongWrapper(userId),password,firstName,middleName,lastName,emailAddress});
  MailProducer.produce(methodWrapper);
}
