{
  Session session=null;
  try {
    session=openSession();
    session.delete(expandoTableColumns);
    session.flush();
    return expandoTableColumns;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(ExpandoTableColumns.class.getName());
  }
}
