{
  ProcessDefinition definition=ProcessDefinition.parseXmlString(xml);
  jbpmContext.deployProcessDefinition(definition);
  StringMaker sm=new StringMaker();
  sm.append("<result>");
  sm.append("<definitionId>");
  sm.append(definition.getId());
  sm.append("</definitionId>");
  sm.append("</result>");
  return sm.toString();
}
