{
  List instances=getInstances(definitionId,instanceId,definitionName,definitionVersion,startDateGT,startDateLT,endDateGT,endDateLT,hideEndedTasks,retrieveUserInstances,andOperator,begin,end);
  Document doc=DocumentHelper.createDocument();
  Element root=doc.addElement("result");
  for (int i=0; i < instances.size(); i++) {
    ProcessInstance instance=(ProcessInstance)instances.get(i);
    createElement(instance,root,true);
  }
  return doc.asXML();
}
