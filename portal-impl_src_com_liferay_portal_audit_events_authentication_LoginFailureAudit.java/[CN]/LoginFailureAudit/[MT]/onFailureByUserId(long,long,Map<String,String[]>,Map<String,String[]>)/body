{
  try {
    String userName=PortalUtil.getUserName(userId,StringPool.BLANK);
    AuditMessageSenderUtil.send(LoginFailureAuditMessageBuilder.build(companyId,userId,userName,headerMap,LoginFailureAuditMessageBuilder.REASON_USERID));
  }
 catch (  Exception e) {
    throw new AuthException(e);
  }
}
