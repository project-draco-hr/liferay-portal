{
  Session session=null;
  try {
    session=openSession();
    session.delete(scProductScreenshot);
    session.flush();
    return scProductScreenshot;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(SCProductScreenshot.class.getName());
  }
}
