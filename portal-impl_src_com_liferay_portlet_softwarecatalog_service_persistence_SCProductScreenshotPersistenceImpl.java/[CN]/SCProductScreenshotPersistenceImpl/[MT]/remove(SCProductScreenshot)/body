{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(scProductScreenshot);
    }
  }
  scProductScreenshot=removeImpl(scProductScreenshot);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(scProductScreenshot);
    }
  }
  return scProductScreenshot;
}
