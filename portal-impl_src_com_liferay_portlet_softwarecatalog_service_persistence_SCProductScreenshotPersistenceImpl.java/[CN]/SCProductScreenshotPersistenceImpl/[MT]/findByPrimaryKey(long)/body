{
  SCProductScreenshot scProductScreenshot=fetchByPrimaryKey(productScreenshotId);
  if (scProductScreenshot == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No SCProductScreenshot exists with the primary key " + productScreenshotId);
    }
    throw new NoSuchProductScreenshotException("No SCProductScreenshot exists with the primary key " + productScreenshotId);
  }
  return scProductScreenshot;
}
