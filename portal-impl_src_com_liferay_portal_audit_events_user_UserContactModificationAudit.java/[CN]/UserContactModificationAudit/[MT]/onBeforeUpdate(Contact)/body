{
  try {
    Contact oldContact=ContactLocalServiceUtil.getContact(newContact.getContactId());
    Collection<ProfileAttribute> attributes=getChangedAttributes(newContact,oldContact);
    if (attributes.size() > 0) {
      AuditMessageSenderUtil.send(UserProfileModificationAuditMessageBuilder.build(newContact.getUserId(),attributes));
    }
  }
 catch (  Exception e) {
    throw new ModelListenerException(e);
  }
}
