{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//a[3]/span",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=12345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Add Child Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_36_title",RuntimeVariables.replace("Child Page Max Character"));
  selenium.type("_36_content",RuntimeVariables.replace("==Child Page of Max Character Article==\n\nI am a child page of a max character article."));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertEquals(RuntimeVariables.replace("Child Page Max Character"),selenium.getText("//div[6]/ul/li/a"));
  selenium.clickAt("link=Child Page Max Character",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//div[2]/div/div/h1","Child Page Max Character"));
  assertEquals(RuntimeVariables.replace("Child Page of Max Character Article"),selenium.getText("//h2"));
  assertEquals(RuntimeVariables.replace("I am a child page of a max character article."),selenium.getText("//p"));
}
