{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ResourceBlock.class,ResourceBlock.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("resourceBlockId",ServiceTestUtil.nextLong()));
  List<ResourceBlock> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
