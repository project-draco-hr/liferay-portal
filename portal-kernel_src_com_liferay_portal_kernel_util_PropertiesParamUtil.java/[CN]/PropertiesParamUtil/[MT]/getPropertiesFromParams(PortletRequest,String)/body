{
  UnicodeProperties props=new UnicodeProperties(true);
  for (  String paramName : portletRequest.getParameterMap().keySet()) {
    if (paramName.startsWith(prefix) && !paramName.endsWith(")Checkbox")) {
      String key=paramName.substring(prefix.length(),paramName.length() - 1);
      props.setProperty(key,portletRequest.getParameter(paramName));
    }
  }
  return props;
}
