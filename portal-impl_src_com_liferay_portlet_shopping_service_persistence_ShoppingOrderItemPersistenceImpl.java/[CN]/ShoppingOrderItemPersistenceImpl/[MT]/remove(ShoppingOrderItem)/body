{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(shoppingOrderItem);
    }
  }
  shoppingOrderItem=removeImpl(shoppingOrderItem);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(shoppingOrderItem);
    }
  }
  return shoppingOrderItem;
}
