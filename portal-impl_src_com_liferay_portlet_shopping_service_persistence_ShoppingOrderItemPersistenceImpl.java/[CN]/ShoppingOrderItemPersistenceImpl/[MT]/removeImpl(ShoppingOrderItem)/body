{
  Session session=null;
  try {
    session=openSession();
    session.delete(shoppingOrderItem);
    session.flush();
    return shoppingOrderItem;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ShoppingOrderItem.class.getName());
  }
}
