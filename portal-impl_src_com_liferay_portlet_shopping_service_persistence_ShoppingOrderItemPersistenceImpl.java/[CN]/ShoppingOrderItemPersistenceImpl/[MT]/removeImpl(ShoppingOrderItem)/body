{
  shoppingOrderItem=toUnwrappedModel(shoppingOrderItem);
  Session session=null;
  try {
    session=openSession();
    if (shoppingOrderItem.isCachedModel()) {
      shoppingOrderItem=(ShoppingOrderItem)session.get(ShoppingOrderItemImpl.class,shoppingOrderItem.getPrimaryKeyObj());
    }
    if (shoppingOrderItem != null) {
      session.delete(shoppingOrderItem);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (shoppingOrderItem != null) {
    clearCache(shoppingOrderItem);
  }
  return shoppingOrderItem;
}
