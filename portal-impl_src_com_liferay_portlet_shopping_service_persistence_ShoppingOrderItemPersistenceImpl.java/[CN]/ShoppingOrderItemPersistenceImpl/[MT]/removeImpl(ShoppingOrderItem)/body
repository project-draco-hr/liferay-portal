{
  Session session=null;
  try {
    session=openSession();
    if (shoppingOrderItem.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(ShoppingOrderItemImpl.class,shoppingOrderItem.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(shoppingOrderItem);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  ShoppingOrderItemModelImpl shoppingOrderItemModelImpl=(ShoppingOrderItemModelImpl)shoppingOrderItem;
  EntityCacheUtil.removeResult(ShoppingOrderItemModelImpl.ENTITY_CACHE_ENABLED,ShoppingOrderItemImpl.class,shoppingOrderItem.getPrimaryKey());
  return shoppingOrderItem;
}
