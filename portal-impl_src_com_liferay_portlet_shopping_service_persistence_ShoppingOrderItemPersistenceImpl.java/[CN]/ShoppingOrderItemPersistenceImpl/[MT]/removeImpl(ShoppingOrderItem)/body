{
  shoppingOrderItem=toUnwrappedModel(shoppingOrderItem);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,shoppingOrderItem);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  EntityCacheUtil.removeResult(ShoppingOrderItemModelImpl.ENTITY_CACHE_ENABLED,ShoppingOrderItemImpl.class,shoppingOrderItem.getPrimaryKey());
  return shoppingOrderItem;
}
