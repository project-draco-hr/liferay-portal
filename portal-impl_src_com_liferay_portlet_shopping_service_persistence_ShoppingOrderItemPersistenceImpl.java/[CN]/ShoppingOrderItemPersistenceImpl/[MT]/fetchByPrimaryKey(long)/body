{
  ShoppingOrderItem result=(ShoppingOrderItem)EntityCacheUtil.getResult(ShoppingOrderItemModelImpl.ENTITY_CACHE_ENABLED,ShoppingOrderItemImpl.class,orderItemId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      ShoppingOrderItem shoppingOrderItem=(ShoppingOrderItem)session.get(ShoppingOrderItemImpl.class,new Long(orderItemId));
      if (shoppingOrderItem != null) {
        cacheResult(shoppingOrderItem);
      }
      return shoppingOrderItem;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (ShoppingOrderItem)result;
  }
}
