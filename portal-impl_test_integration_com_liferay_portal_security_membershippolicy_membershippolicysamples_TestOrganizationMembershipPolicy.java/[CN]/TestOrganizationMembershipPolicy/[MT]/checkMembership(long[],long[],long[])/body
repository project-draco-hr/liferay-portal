{
  for (  long forbiddenOrganizationId : BaseOrganizationMembershipPolicyTestCase.getForbiddenOrganizationIds()) {
    if (forbiddenOrganizationId == 0) {
      continue;
    }
    if (ArrayUtil.contains(addOrganizationIds,forbiddenOrganizationId)) {
      throw new MembershipPolicyException(MembershipPolicyException.ORGANIZATION_MEMBERSHIP_NOT_ALLOWED);
    }
  }
  for (  long requiredOrganizationId : BaseOrganizationMembershipPolicyTestCase.getRequiredOrganizationIds()) {
    if (requiredOrganizationId == 0) {
      continue;
    }
    if (ArrayUtil.contains(removeOrganizationIds,requiredOrganizationId)) {
      throw new MembershipPolicyException(MembershipPolicyException.ORGANIZATION_MEMBERSHIP_REQUIRED);
    }
  }
}
