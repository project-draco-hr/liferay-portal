{
  JournalFolder folder1=JournalTestUtil.addFolder(_group.getGroupId(),JournalFolderConstants.DEFAULT_PARENT_FOLDER_ID,"Test 1");
  JournalFolder folder11=JournalTestUtil.addFolder(_group.getGroupId(),folder1.getFolderId(),"Test 1.1");
  JournalFolder folder111=JournalTestUtil.addFolder(_group.getGroupId(),folder11.getFolderId(),"Test 1.1.1");
  Assert.assertTrue(folder1.isRoot());
  Assert.assertFalse(folder11.isRoot());
  Assert.assertFalse(folder111.isRoot());
  Assert.assertEquals(folder1.getFolderId(),folder11.getParentFolderId());
  Assert.assertEquals(folder11.getFolderId(),folder111.getParentFolderId());
}
