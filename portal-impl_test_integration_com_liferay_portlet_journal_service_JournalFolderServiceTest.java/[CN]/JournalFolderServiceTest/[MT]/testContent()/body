{
  JournalFolder folder=JournalTestUtil.addFolder(group.getGroupId(),JournalFolderConstants.DEFAULT_PARENT_FOLDER_ID,"Test Folder");
  JournalArticle article=JournalTestUtil.addArticle(group.getGroupId(),folder.getFolderId(),"Test Article","This is a test article.");
  Assert.assertEquals(article.getFolderId(),folder.getFolderId());
}
