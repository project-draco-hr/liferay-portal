{
  Group group=GroupTestUtil.addGroup();
  JournalFolder folder=JournalTestUtil.addFolder(group.getGroupId(),0,"Test Folder");
  JournalArticle article=JournalTestUtil.addArticle(group.getGroupId(),folder.getFolderId(),"Test Article","This is a test article.");
  Assert.assertEquals(article.getFolderId(),folder.getFolderId());
  JournalFolderLocalServiceUtil.deleteFolder(folder);
  GroupLocalServiceUtil.deleteGroup(group);
}
