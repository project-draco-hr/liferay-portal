{
  try {
    for (int i=0; i < 10; i++) {
      _randomWait(100);
      _timeStampsBeforeWrite.put(System.currentTimeMillis());
      _writer.write(new char[_dataSize]);
    }
    if (_expectException) {
      _failed=true;
    }
  }
 catch (  Exception e) {
    if (!_expectException) {
      _failed=true;
    }
  }
}
