{
  try {
    Files.move(sourceFilePath,targetFilePath,StandardCopyOption.ATOMIC_MOVE,StandardCopyOption.REPLACE_EXISTING);
  }
 catch (  IOException ioe) {
    if (!retry) {
      throw ioe;
    }
    PathCallable pathCallable=new PathCallable(sourceFilePath){
      @Override public Object call() throws Exception {
        FileTime fileTime=Files.getLastModifiedTime(targetFilePath);
        if (fileTime.toMillis() <= getStartTime()) {
          Files.move(sourceFilePath,targetFilePath,StandardCopyOption.ATOMIC_MOVE,StandardCopyOption.REPLACE_EXISTING);
        }
 else {
          Files.deleteIfExists(sourceFilePath);
        }
        return null;
      }
    }
;
    FileLockRetryUtil.registerPathCallable(pathCallable);
  }
}
