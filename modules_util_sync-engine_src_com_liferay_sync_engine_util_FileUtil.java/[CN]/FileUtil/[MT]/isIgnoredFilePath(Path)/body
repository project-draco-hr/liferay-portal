{
  String fileName=String.valueOf(filePath.getFileName());
  if (_syncFileIgnoreNames.contains(StringEscapeUtils.escapeJava(fileName)) || MSOfficeFileUtil.isTempCreatedFile(filePath) || (PropsValues.SYNC_FILE_IGNORE_HIDDEN && isHidden(filePath))|| Files.isSymbolicLink(filePath)|| fileName.endsWith(".lnk")) {
    return true;
  }
  SyncFile syncFile=SyncFileService.fetchSyncFile(filePath.toString());
  if (syncFile == null) {
    return isIgnoredFilePath(filePath.getParent());
  }
  if (!syncFile.isSystem() && (syncFile.getState() == SyncFile.STATE_UNSYNCED)) {
    return true;
  }
  return false;
}
