{
  MBMessage mbMessage1=MBTestUtil.addMessage(_group.getGroupId());
  MBMessage mbMessage2=MBTestUtil.addMessage(_group.getGroupId());
  AssetEntry assetEntry=AssetEntryLocalServiceUtil.fetchEntry(MBMessage.class.getName(),mbMessage2.getMessageId());
  Assert.assertNotNull(assetEntry);
  AssetEntryLocalServiceUtil.deleteAssetEntry(assetEntry);
  List<MBMessage> messages=MBMessageLocalServiceUtil.getNoAssetMessages();
  Assert.assertEquals(1,messages.size());
  Assert.assertEquals(mbMessage2.getMessageId(),messages.get(0).getMessageId());
}
