{
  FileEntry tempFileEntry=TempFileEntryUtil.addTempFileEntry(_group.getGroupId(),_user.getUserId(),_TEMP_FOLDER_NAME,"image.jpg",_getInputStream(),ContentTypes.IMAGE_JPEG);
  String fileEntryURL=PortletFileRepositoryUtil.getPortletFileEntryURL(null,tempFileEntry,StringPool.BLANK);
  Assert.assertEquals("<img src=\"" + fileEntryURL + "\" />",_blogsEntryAttachmentFileEntryHelper.getBlogsEntryAttachmentFileEntryLink(tempFileEntry));
}
