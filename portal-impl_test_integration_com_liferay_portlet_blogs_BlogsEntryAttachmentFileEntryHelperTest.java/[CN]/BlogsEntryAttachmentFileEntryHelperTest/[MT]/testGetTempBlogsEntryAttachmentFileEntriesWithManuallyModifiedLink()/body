{
  FileEntry tempFileEntry=TempFileEntryUtil.addTempFileEntry(_group.getGroupId(),_user.getUserId(),_TEMP_FOLDER_NAME,"image.jpg",_getInputStream(),ContentTypes.IMAGE_JPEG);
  String tempFileEntryImgTag=_getManuallyModifiedTempFileEntryImgTag(tempFileEntry);
  List<FileEntry> tempBlogsEntryAttachments=_blogsEntryAttachmentFileEntryHelper.getTempBlogsEntryAttachmentFileEntries(getContent(tempFileEntryImgTag));
  Assert.assertEquals(1,tempBlogsEntryAttachments.size());
  for (  FileEntry tempBlogsEntryAttachment : tempBlogsEntryAttachments) {
    Assert.assertEquals(tempFileEntry.getFileEntryId(),tempBlogsEntryAttachment.getFileEntryId());
  }
}
