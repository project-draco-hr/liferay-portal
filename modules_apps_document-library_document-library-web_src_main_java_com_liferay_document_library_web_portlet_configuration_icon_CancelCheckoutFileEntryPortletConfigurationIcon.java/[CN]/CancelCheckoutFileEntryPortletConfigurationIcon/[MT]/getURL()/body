{
  PortletURL cancelCheckoutURL=PortalUtil.getControlPanelPortletURL(portletRequest,DLPortletKeys.DOCUMENT_LIBRARY_ADMIN,PortletRequest.ACTION_PHASE);
  cancelCheckoutURL.setParameter("javax.portlet.action","/document_library/edit_file_entry");
  cancelCheckoutURL.setParameter(Constants.CMD,Constants.CANCEL_CHECKOUT);
  cancelCheckoutURL.setParameter("redirect",themeDisplay.getURLCurrent());
  cancelCheckoutURL.setParameter("fileEntryId",String.valueOf(_fileEntry.getFileEntryId()));
  return cancelCheckoutURL.toString();
}
