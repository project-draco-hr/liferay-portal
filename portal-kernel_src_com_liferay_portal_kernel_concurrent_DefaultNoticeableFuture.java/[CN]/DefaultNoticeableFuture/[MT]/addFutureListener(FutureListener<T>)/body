{
  if (futureListener == null) {
    throw new NullPointerException("Future listener is null");
  }
  futureListener=new OnceFutureListener<>(futureListener);
  if (_futureListeners.add(futureListener)) {
    if (isDone()) {
      futureListener.complete(this);
    }
    return true;
  }
  return false;
}
