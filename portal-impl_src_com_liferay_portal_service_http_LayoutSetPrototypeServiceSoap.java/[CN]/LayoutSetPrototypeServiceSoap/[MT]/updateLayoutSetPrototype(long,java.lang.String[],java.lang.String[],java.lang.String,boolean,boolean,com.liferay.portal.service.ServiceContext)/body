{
  try {
    Map<Locale,String> nameMap=LocalizationUtil.getLocalizationMap(nameMapLanguageIds,nameMapValues);
    com.liferay.portal.model.LayoutSetPrototype returnValue=LayoutSetPrototypeServiceUtil.updateLayoutSetPrototype(layoutSetPrototypeId,nameMap,description,active,layoutsUpdateable,serviceContext);
    return com.liferay.portal.model.LayoutSetPrototypeSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
