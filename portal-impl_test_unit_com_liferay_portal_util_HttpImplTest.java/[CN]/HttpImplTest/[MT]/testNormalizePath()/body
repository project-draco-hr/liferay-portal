{
  Assert.assertEquals("/",_httpImpl.normalizePath("/.."));
  Assert.assertEquals("/api/axis",_httpImpl.normalizePath("/api/%61xis"));
  Assert.assertEquals("/api/%2561xis",_httpImpl.normalizePath("/api/%2561xis"));
  Assert.assertEquals("/api/ax%3Fs",_httpImpl.normalizePath("/api/ax%3fs"));
  Assert.assertEquals("/api/%2F/axis",_httpImpl.normalizePath("/api/%2f/;x=aaa_%2f_y/axis"));
  Assert.assertEquals("/api/axis",_httpImpl.normalizePath("/api/;x=aaa_%2f_y/axis"));
  Assert.assertEquals("/api/axis",_httpImpl.normalizePath("/api/;x=aaa_%5b_y/axis"));
  Assert.assertEquals("/api/axis",_httpImpl.normalizePath("/api/;x=aaa_LIFERAY_TEMP_SLASH_y/axis"));
  Assert.assertEquals("/api/axis",_httpImpl.normalizePath("/api///////%2e/../;x=y/axis"));
  Assert.assertEquals("/api/axis",_httpImpl.normalizePath("/////api///////%2e/a/../;x=y/axis"));
  Assert.assertEquals("/api/axis",_httpImpl.normalizePath("/////api///////%2e/../;x=y/axis"));
  Assert.assertEquals("/api/axis",_httpImpl.normalizePath("/api///////%2e/axis"));
  Assert.assertEquals("/api/axis",_httpImpl.normalizePath("./api///////%2e/axis"));
  Assert.assertEquals("/api/axis?foo=bar&bar=foo",_httpImpl.normalizePath("./api///////%2e/axis?foo=bar&bar=foo"));
}
