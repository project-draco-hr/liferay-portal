{
  try {
    ActionUtil.getEvent(req);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchEventException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.calendar.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward(getForward(req,"portlet.calendar.edit_event"));
}
