{
  selenium.open("/web/guest/home/");
  selenium.getEval("window.Liferay.fire('initDockbar');");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace("Asset Publisher Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.getEval("window.Liferay.fire('initDockbar');");
  assertFalse(selenium.isTextPresent("IG Folder Image Name"));
  assertFalse(selenium.isElementPresent("//div[@class='asset-resource-info']/div/img"));
}
