{
  T t=createNode(kaleoNode);
  Set<Action> actions=buildActions(KaleoNode.class.getName(),kaleoNode.getKaleoNodeId());
  t.setActions(actions);
  t.setMetadata(kaleoNode.getMetadata());
  Set<Notification> notifications=buildNotifications(KaleoNode.class.getName(),kaleoNode.getKaleoNodeId());
  t.setNotifications(notifications);
  Set<Timer> timers=buildTimers(KaleoNode.class.getName(),kaleoNode.getKaleoNodeId());
  t.setTimers(timers);
  return t;
}
