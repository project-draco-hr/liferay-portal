{
synchronized (FriendlyURLServlet.class) {
    super.init(config);
    ServletContext ctx=getServletContext();
    _companyId=ctx.getInitParameter("company_id");
    _private=GetterUtil.getBoolean(config.getInitParameter("private"));
    _user=GetterUtil.getBoolean(config.getInitParameter("user"),false);
  }
}
