{
  Map<String,ServletDefinition> servletDefinitions=_webXML.getServletDefinitions();
  for (  Map.Entry<String,ServletDefinition> entry : servletDefinitions.entrySet()) {
    ServletDefinition servletDefinition=entry.getValue();
    try {
      extendedHttpService.registerServlet(servletDefinition.getName(),servletDefinition.getURLPatterns(),servletDefinition.getServlet(),servletDefinition.getInitParameters(),httpContext);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
}
