{
  ActionableDynamicQuery actionableDynamicQuery=new CalEventActionableDynamicQuery(){
    @Override protected void performAction(    Object object) throws PortalException {
      CalEvent event=(CalEvent)object;
      Document document=getDocument(event);
      addDocument(document);
    }
  }
;
  actionableDynamicQuery.setCompanyId(companyId);
  actionableDynamicQuery.setSearchEngineId(getSearchEngineId());
  actionableDynamicQuery.performActions();
}
