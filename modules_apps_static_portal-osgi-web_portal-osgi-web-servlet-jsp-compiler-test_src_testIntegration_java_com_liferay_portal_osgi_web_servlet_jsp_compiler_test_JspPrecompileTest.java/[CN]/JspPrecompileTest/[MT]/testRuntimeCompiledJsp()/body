{
  try (CaptureAppender captureAppender=Log4JLoggerTestUtil.configureLog4JLogger(_JSP_COMPILER_CLASS_NAME,Level.DEBUG)){
    _doJspTest(_RUNTIME_COMPILE_JSP);
    List<LoggingEvent> loggingEvents=captureAppender.getLoggingEvents();
    StringBundler sb=new StringBundler(3);
    sb.append("Compiling JSP: ");
    sb.append(_JSP_PACKAGE);
    sb.append(StringUtil.replace(_RUNTIME_COMPILE_JSP,CharPool.PERIOD,CharPool.UNDERLINE));
    if (!_containsLog(loggingEvents,sb.toString())) {
      Assert.fail("No JSP was compiled at runtime");
    }
  }
 }
