{
  StringBundler sb=new StringBundler(6);
  sb.append("http://localhost:8080/web");
  sb.append(_group.getFriendlyURL());
  sb.append(StringPool.QUESTION);
  sb.append(JspPrecompilePortlet.JSP_PARAMETER);
  sb.append("=/");
  sb.append(jsp);
  URL url=new URL(sb.toString());
  try (InputStream inputStream=url.openStream()){
    String content=StringUtil.read(inputStream);
    Assert.assertTrue("Content {" + content + "} does not contain expected message "+ "{"+ expectedMessage+ "}",content.contains(expectedMessage));
  }
 }
