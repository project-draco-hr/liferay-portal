{
  if (this == obj) {
    return true;
  }
  if (!(obj instanceof NotificationRecipient)) {
    return false;
  }
  NotificationRecipient notificationRecipient=(NotificationRecipient)obj;
  if ((_companyId == notificationRecipient._companyId) && Validator.equals(_emailAddress,notificationRecipient._emailAddress) && (_userId == notificationRecipient._userId)) {
    return true;
  }
  return false;
}
