{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Blogs",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_161_keywords",RuntimeVariables.replace("Blogs Entry Title"));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Blogs Entry Title"),selenium.getText("//td[2]/a"));
}
