{
  String className=PortalUtil.getClassName(activityTracker.getClassNameId());
  for (int i=0; i < _activityTrackerInterpreters.size(); i++) {
    ActivityTrackerInterpreterImpl activityTrackerInterpreter=(ActivityTrackerInterpreterImpl)_activityTrackerInterpreters.get(i);
    if (activityTrackerInterpreter.hasClassName(className)) {
      ActivityFeedEntry activityFeedEntry=activityTrackerInterpreter.interpret(activityTracker,themeDisplay);
      if (activityFeedEntry != null) {
        return activityFeedEntry;
      }
    }
  }
  return null;
}
