{
  String className=properties.getProperty(EhcacheConstants.BOOTSTRAP_CACHE_LOADER_FACTORY_CLASS_NAME);
  if (Validator.isNull(className)) {
    return null;
  }
  try {
    BootstrapCacheLoaderFactory<?> bootstrapCacheLoaderFactory=(BootstrapCacheLoaderFactory<?>)InstanceFactory.newInstance(ClassLoaderUtil.getPortalClassLoader(),className);
    return new EhcacheBootstrapLoaderAdapter(bootstrapCacheLoaderFactory.createBootstrapCacheLoader(properties));
  }
 catch (  Exception e) {
    _log.error("Unable to instantiate bootstrap cache loader factory " + className,e);
    return null;
  }
}
