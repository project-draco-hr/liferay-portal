{
  Bundle bundle=bundleContext.getBundle(0);
  BundleContext systemBundleContext=bundle.getBundleContext();
  File overwrittenWarsPropertiesFile=systemBundleContext.getDataFile("overwritten-wars.properties");
  try (OutputStream outputStream=new FileOutputStream(overwrittenWarsPropertiesFile)){
    properties.store(outputStream,null);
  }
 }
