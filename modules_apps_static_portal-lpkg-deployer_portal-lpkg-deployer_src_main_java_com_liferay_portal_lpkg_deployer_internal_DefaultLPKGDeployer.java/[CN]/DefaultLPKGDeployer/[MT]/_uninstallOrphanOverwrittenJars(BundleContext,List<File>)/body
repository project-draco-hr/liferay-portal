{
  for (  Bundle bundle : bundleContext.getBundles()) {
    String location=bundle.getLocation();
    if (!location.startsWith(_LPKG_OVERWRITTEN_PREFIX)) {
      continue;
    }
    String filePath=location.substring(_LPKG_OVERWRITTEN_PREFIX.length());
    if (jarFiles.contains(new File(filePath))) {
      continue;
    }
    bundle.uninstall();
    if (_log.isInfoEnabled()) {
      _log.info("Uninstalled orphan overwritten JAR bundle " + location);
    }
  }
}
