{
  File overwrittenWarsPropertiesFile=bundleContext.getDataFile("overwritten-wars.properties");
  Properties overwrittenWarsProperties=new Properties();
  if (overwrittenWarsPropertiesFile.exists()) {
    try (InputStream inputStream=new FileInputStream(overwrittenWarsPropertiesFile)){
      overwrittenWarsProperties.load(inputStream);
    }
   }
  return overwrittenWarsProperties;
}
