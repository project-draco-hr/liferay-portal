{
  JournalArticleDDMFormValuesReader journalArticleDDMFormValuesReader=new JournalArticleDDMFormValuesReader(_article);
  journalArticleDDMFormValuesReader.setFieldsToDDMFormValuesConverter(_fieldsToDDMFormValuesConverter);
  journalArticleDDMFormValuesReader.setJournalConverter(_journalConverter);
  return journalArticleDDMFormValuesReader;
}
