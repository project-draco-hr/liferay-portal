{
  User user=UserTestUtil.addUser();
  _users.add(user);
  Calendar calendar=Calendar.getInstance();
  calendar.add(Calendar.YEAR,1000);
  ContactLocalServiceUtil.updateContact(user.getContactId(),user.getEmailAddress(),user.getFirstName(),user.getMiddleName(),user.getLastName(),0,0,user.getMale(),calendar.get(Calendar.MONTH),calendar.get(Calendar.DATE),calendar.get(Calendar.YEAR),"","","","","",user.getJobTitle());
}
