{
  User user=UserTestUtil.addUser();
  _users.add(user);
  Date future=new Date(System.currentTimeMillis() + 100000);
  ContactLocalServiceUtil.updateContact(user.getContactId(),user.getEmailAddress(),user.getFirstName(),user.getMiddleName(),user.getLastName(),0,0,user.getMale(),future.getMonth(),future.getDay(),future.getYear(),"","","","","",user.getJobTitle());
}
