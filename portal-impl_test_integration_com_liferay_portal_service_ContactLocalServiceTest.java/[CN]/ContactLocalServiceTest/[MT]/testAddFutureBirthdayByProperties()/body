{
  _user=UserTestUtil.addUser();
  Calendar calendar=Calendar.getInstance();
  calendar.add(Calendar.YEAR,1000);
  ContactLocalServiceUtil.addContact(_user.getUserId(),Contact.class.getName(),_user.getUserId(),_user.getEmailAddress(),_user.getFirstName(),_user.getMiddleName(),_user.getLastName(),0,0,_user.getMale(),calendar.get(Calendar.MONTH),calendar.get(Calendar.DATE),calendar.get(Calendar.YEAR),"","","","","",_user.getJobTitle());
}
