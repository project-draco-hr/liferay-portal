{
  try (CaptureHandler captureHandler=_doTestInvalidSetting(Level.WARNING)){
    List<LogRecord> logRecords=captureHandler.getLogRecords();
    Assert.assertEquals(2,logRecords.size());
    LogRecord logRecord1=logRecords.get(0);
    Assert.assertEquals(PropsKeys.BUFFERED_INCREMENT_THREADPOOL_KEEP_ALIVE_TIME + "[]=-3. Auto reset to 0.",logRecord1.getMessage());
    LogRecord logRecord2=logRecords.get(1);
    Assert.assertEquals(PropsKeys.BUFFERED_INCREMENT_THREADPOOL_MAX_SIZE + "[]=-4. Auto reset to 1.",logRecord2.getMessage());
  }
 }
