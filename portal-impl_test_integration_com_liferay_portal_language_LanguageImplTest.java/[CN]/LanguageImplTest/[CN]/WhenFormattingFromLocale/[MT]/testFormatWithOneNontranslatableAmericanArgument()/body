{
  Locale locale=LocaleUtil.US;
  String format=_languageImpl.format(locale,_LANG_KEY_WITH_ARGUMENT,_BIG_INTEGER,false);
  Assert.assertEquals("1,234,567,890 Hours",format);
  format=_languageImpl.format(locale,_LANG_KEY_WITH_ARGUMENT,_BIG_DOUBLE,false);
  Assert.assertEquals("1,234,567,890.12 Hours",format);
  format=_languageImpl.format(locale,_LANG_KEY_WITH_ARGUMENT,_BIG_FLOAT,false);
  Assert.assertEquals("1,234,567.875 Hours",format);
}
