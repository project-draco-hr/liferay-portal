{
  Locale locale=LocaleUtil.SPAIN;
  String message=_languageImpl.format(locale,_LANG_KEY_WITH_ARGUMENT,_BIG_INTEGER,false);
  Assert.assertEquals("1.234.567.890 horas",message);
  message=_languageImpl.format(locale,_LANG_KEY_WITH_ARGUMENT,_BIG_DOUBLE,false);
  Assert.assertEquals("1.234.567.890,12 horas",message);
  message=_languageImpl.format(locale,_LANG_KEY_WITH_ARGUMENT,_BIG_FLOAT,false);
  Assert.assertEquals("1.234.567,875 horas",message);
}
