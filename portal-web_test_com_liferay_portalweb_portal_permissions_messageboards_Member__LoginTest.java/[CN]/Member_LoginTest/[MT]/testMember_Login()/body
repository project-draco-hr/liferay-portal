{
  selenium.clickAt("link=Welcome",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_58_login",RuntimeVariables.replace("member@liferay.com"));
  selenium.type("_58_password",RuntimeVariables.replace("test"));
  selenium.clickAt("//input[@value='Sign In']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Message Boards Permissions Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=Categories"));
}
