{
  _dlConfiguration=Configurable.createConfigurable(DLConfiguration.class,properties);
  schedulerEntryImpl.setTrigger(TriggerFactoryUtil.createTrigger(getEventListenerClass(),getEventListenerClass(),_dlConfiguration.temporaryFileEntriesCheckInterval(),TimeUnit.HOUR));
  _schedulerEngineHelper.register(this,schedulerEntryImpl,DestinationNames.SCHEDULER_DISPATCH);
}
