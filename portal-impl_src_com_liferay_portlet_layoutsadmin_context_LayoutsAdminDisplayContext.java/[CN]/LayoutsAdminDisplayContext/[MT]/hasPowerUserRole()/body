{
  ThemeDisplay themeDisplay=(ThemeDisplay)_request.getAttribute(WebKeys.THEME_DISPLAY);
  try {
    User selUser=getSelUser();
    return RoleLocalServiceUtil.hasUserRole(selUser.getUserId(),themeDisplay.getCompanyId(),RoleConstants.POWER_USER,true);
  }
 catch (  Exception e) {
  }
  return false;
}
