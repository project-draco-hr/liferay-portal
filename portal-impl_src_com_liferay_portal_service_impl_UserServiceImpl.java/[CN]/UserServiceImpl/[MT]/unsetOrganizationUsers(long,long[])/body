{
  userIds=UsersAdminUtil.filterUnsetOrganizationUserIds(getPermissionChecker(),organizationId,userIds);
  if (userIds.length == 0) {
    return;
  }
  OrganizationPermissionUtil.check(getPermissionChecker(),organizationId,ActionKeys.ASSIGN_MEMBERS);
  OrganizationMembershipPolicyUtil.checkMembership(userIds,null,new long[]{organizationId});
  userLocalService.unsetOrganizationUsers(organizationId,userIds);
  OrganizationMembershipPolicyUtil.propagateMembership(userIds,null,new long[]{organizationId});
}
