{
  if (userIds.length == 0) {
    return;
  }
  UserGroupPermissionUtil.check(getPermissionChecker(),userGroupId,ActionKeys.ASSIGN_MEMBERS);
  UserGroupMembershipPolicyUtil.checkMembership(userIds,null,new long[]{userGroupId});
  userLocalService.unsetUserGroupUsers(userGroupId,userIds);
  UserGroupMembershipPolicyUtil.propagateMembership(userIds,null,new long[]{userGroupId});
}
