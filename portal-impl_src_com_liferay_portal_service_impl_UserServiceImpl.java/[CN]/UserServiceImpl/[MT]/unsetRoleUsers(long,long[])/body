{
  if (userIds.length == 0) {
    return;
  }
  RolePermissionUtil.check(getPermissionChecker(),roleId,ActionKeys.ASSIGN_MEMBERS);
  RoleMembershipPolicyUtil.checkRoles(userIds,null,new long[]{roleId});
  userLocalService.unsetRoleUsers(roleId,userIds);
  RoleMembershipPolicyUtil.propagateRoles(userIds,null,new long[]{roleId});
}
