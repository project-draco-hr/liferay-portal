{
  if (userIds.length == 0) {
    return;
  }
  OrganizationPermissionUtil.check(getPermissionChecker(),organizationId,ActionKeys.ASSIGN_MEMBERS);
  validateOrganizationUsers(userIds);
  OrganizationMembershipPolicyUtil.checkMembership(userIds,new long[]{organizationId},null);
  userLocalService.addOrganizationUsers(organizationId,userIds);
  OrganizationMembershipPolicyUtil.propagateMembership(userIds,new long[]{organizationId},null);
}
