{
  ClassLoader classLoader=ClassLoaderUtil.getPortalClassLoader();
  if (!(classLoader instanceof URLClassLoader)) {
    throw new XugglerInstallException.MuseBeURLClassLoader();
  }
  try {
    JarUtil.downloadAndInstallJar(new URL(PropsValues.XUGGLER_JAR_URL + name),PropsValues.LIFERAY_LIB_PORTAL_DIR,name,(URLClassLoader)classLoader);
    _nativeLibraryCopied=true;
  }
 catch (  Exception e) {
    throw new XugglerInstallException.MustInstallJar(name,e);
  }
}
