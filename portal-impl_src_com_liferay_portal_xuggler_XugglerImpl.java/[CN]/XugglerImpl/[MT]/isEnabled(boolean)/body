{
  boolean enabled=false;
  try {
    enabled=PrefsPropsUtil.getBoolean(PropsKeys.XUGGLER_ENABLED,PropsValues.XUGGLER_ENABLED);
  }
 catch (  Exception e) {
    _log.warn(e,e);
  }
  if (!checkNativeLibraries) {
    return enabled;
  }
  if (enabled) {
    return isNativeLibraryInstalled();
  }
 else {
    return false;
  }
}
