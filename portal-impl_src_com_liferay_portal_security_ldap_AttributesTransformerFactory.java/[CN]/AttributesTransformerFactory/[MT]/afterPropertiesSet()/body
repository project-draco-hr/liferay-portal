{
  if (_log.isDebugEnabled()) {
    _log.debug("Instantiate " + PropsValues.LDAP_ATTRS_TRANSFORMER_IMPL);
  }
  ClassLoader classLoader=ClassLoaderUtil.getPortalClassLoader();
  _originalAttributesTransformer=(AttributesTransformer)InstanceFactory.newInstance(classLoader,PropsValues.LDAP_ATTRS_TRANSFORMER_IMPL);
  _attributesTransformer=_originalAttributesTransformer;
}
