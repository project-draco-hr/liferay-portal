{
  ExportActionableDynamicQuery actionableDynamicQuery=_ratingsEntryLocalService.getExportActionableDynamicQuery(portletDataContext);
  actionableDynamicQuery.setStagedModelType(new StagedModelType(PortalUtil.getClassNameId(RatingsEntry.class.getName()),StagedModelType.REFERRER_CLASS_NAME_ID_ALL));
