{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MDRRule.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("ruleId",RandomTestUtil.nextLong()));
  List<MDRRule> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
