{
  if (!isStagingGroup()) {
    return null;
  }
  try {
    if (_liveGroup == null) {
      _liveGroup=GroupLocalServiceUtil.getGroup(getLiveGroupId());
    }
    return _liveGroup;
  }
 catch (  Exception e) {
    _log.error("Error getting live group for " + getLiveGroupId(),e);
    return null;
  }
}
