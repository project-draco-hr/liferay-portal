{
  try {
    _getFolders(commandArgument,document,rootNode);
    _getFiles(commandArgument,document,rootNode);
  }
 catch (  PrincipalException pe) {
    if (_log.isWarnEnabled()) {
      _log.warn(pe,pe);
    }
  }
catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      _log.debug(e,e);
    }
    throw new FCKException(e);
  }
}
