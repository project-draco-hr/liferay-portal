{
  try {
    Group group=commandArgument.getCurrentGroup();
    Folder folder=_getFolder(group.getGroupId(),commandArgument.getCurrentFolder());
    long folderId=folder.getFolderId();
    String title=fileName;
    String description=StringPool.BLANK;
    String changeLog=StringPool.BLANK;
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setAddGroupPermissions(true);
    serviceContext.setAddGuestPermissions(true);
    DLAppServiceUtil.addFileEntry(group.getGroupId(),folderId,title,contentType,title,description,changeLog,inputStream,size,serviceContext);
  }
 catch (  Exception e) {
    throw new FCKException(e);
  }
  return "0";
}
