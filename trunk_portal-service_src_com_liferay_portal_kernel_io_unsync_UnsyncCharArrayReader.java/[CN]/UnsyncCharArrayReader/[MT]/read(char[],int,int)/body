{
  if (buffer == null) {
    throw new IOException("Stream closed");
  }
  if (length <= 0) {
    return 0;
  }
  if (index >= capacity) {
    return -1;
  }
  int read=length;
  if ((index + read) > capacity) {
    read=capacity - index;
  }
  System.arraycopy(buffer,index,chars,offset,read);
  index+=read;
  return read;
}
