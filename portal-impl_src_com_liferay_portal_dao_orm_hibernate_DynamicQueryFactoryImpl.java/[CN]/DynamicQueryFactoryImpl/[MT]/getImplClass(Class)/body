{
  if (!clazz.getName().endsWith("Impl")) {
    String implClassName=clazz.getPackage().getName() + ".impl." + clazz.getSimpleName()+ "Impl";
    clazz=_classMap.get(implClassName);
    if (clazz == null) {
      try {
        Thread currentThread=Thread.currentThread();
        ClassLoader contextClassLoader=currentThread.getContextClassLoader();
        clazz=contextClassLoader.loadClass(implClassName);
        _classMap.put(implClassName,clazz);
      }
 catch (      Exception e) {
      }
    }
  }
  return clazz;
}
