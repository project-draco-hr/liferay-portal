{
  List<Object> foldersAndFileEntriesAndFileShortcuts=null;
  if (_dlFolderService != null) {
    foldersAndFileEntriesAndFileShortcuts=_dlFolderService.getFoldersAndFileEntriesAndFileShortcuts(groupId,folderId,mimeTypes,includeMountFolders,queryDefinition);
  }
 else {
    foldersAndFileEntriesAndFileShortcuts=_dlFolderLocalService.getFoldersAndFileEntriesAndFileShortcuts(groupId,folderId,mimeTypes,includeMountFolders,queryDefinition);
  }
  return foldersAndFileEntriesAndFileShortcuts;
}
