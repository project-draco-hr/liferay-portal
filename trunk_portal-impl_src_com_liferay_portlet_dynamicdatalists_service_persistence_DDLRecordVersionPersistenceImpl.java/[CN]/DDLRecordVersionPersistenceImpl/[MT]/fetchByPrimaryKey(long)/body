{
  DDLRecordVersion ddlRecordVersion=(DDLRecordVersion)EntityCacheUtil.getResult(DDLRecordVersionModelImpl.ENTITY_CACHE_ENABLED,DDLRecordVersionImpl.class,recordVersionId,this);
  if (ddlRecordVersion == null) {
    Session session=null;
    try {
      session=openSession();
      ddlRecordVersion=(DDLRecordVersion)session.get(DDLRecordVersionImpl.class,Long.valueOf(recordVersionId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (ddlRecordVersion != null) {
        cacheResult(ddlRecordVersion);
      }
      closeSession(session);
    }
  }
  return ddlRecordVersion;
}
