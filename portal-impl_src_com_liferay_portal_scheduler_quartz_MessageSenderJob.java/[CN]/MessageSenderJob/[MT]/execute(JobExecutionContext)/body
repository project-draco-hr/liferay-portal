{
  try {
    JobDetail detail=jobExecutionContext.getJobDetail();
    String messageBody=detail.getJobDataMap().getString(SchedulerEngine.MESSAGE_BODY);
    String destinationName=detail.getJobDataMap().getString(SchedulerEngine.DESTINATION_NAME);
    MessageBusUtil.sendMessage(destinationName,messageBody);
  }
 catch (  Exception e) {
    throw new JobExecutionException("Unable to send message",e);
  }
}
