{
  List<DDMFormRule> ddmFormRules=new ArrayList<>();
  if (Validator.isNull(jsonArray)) {
    return ddmFormRules;
  }
  for (int i=0; i < jsonArray.length(); i++) {
    DDMFormRule ddmFormRule=getDDMFormRule(jsonArray.getJSONObject(i));
    ddmFormRules.add(ddmFormRule);
  }
  return ddmFormRules;
}
