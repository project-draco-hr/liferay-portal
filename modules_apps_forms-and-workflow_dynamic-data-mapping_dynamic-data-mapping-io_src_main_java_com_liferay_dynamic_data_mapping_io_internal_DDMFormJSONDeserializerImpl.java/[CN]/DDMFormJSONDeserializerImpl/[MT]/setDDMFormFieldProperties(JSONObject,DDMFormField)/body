{
  DDMForm ddmFormFieldTypeSettingsDDMForm=getDDMFormFieldTypeSettingsDDMForm(ddmFormField.getType());
  for (  DDMFormField ddmFormFieldTypeSetting : ddmFormFieldTypeSettingsDDMForm.getDDMFormFields()) {
    setDDMFormFieldProperty(jsonObject,ddmFormField,ddmFormFieldTypeSetting);
  }
  setDDMFormFieldRules(jsonObject.getJSONArray("rules"),ddmFormField);
}
