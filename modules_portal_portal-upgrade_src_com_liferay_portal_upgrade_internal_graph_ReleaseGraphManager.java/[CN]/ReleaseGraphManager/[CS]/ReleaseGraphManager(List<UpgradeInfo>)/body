{
  _directedGraph=new DefaultDirectedGraph<>(new UpgradeProcessEdgeFactory(upgradeInfos));
  for (  UpgradeInfo upgradeInfo : upgradeInfos) {
    _directedGraph.addVertex(upgradeInfo.getFromVersionString());
    _directedGraph.addVertex(upgradeInfo.getToVersionString());
    _directedGraph.addEdge(upgradeInfo.getFromVersionString(),upgradeInfo.getToVersionString(),new UpgradeProcessEdge(upgradeInfo));
  }
}
