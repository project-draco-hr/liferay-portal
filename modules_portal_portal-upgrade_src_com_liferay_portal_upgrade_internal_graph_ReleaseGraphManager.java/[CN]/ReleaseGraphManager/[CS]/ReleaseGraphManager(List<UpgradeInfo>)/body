{
  _directedGraph=new DefaultDirectedGraph<>(new UpgradeProcessEdgeFactory(upgradeInfos));
  for (  UpgradeInfo upgradeInfo : upgradeInfos) {
    _directedGraph.addVertex(upgradeInfo.getFrom());
    _directedGraph.addVertex(upgradeInfo.getTo());
    _directedGraph.addEdge(upgradeInfo.getFrom(),upgradeInfo.getTo(),new UpgradeProcessEdge(upgradeInfo));
  }
}
