{
  Map<String,Object> data=new HashMap<String,Object>();
  PortletURL portletURL=_getAssetBrowserPortletURL(assetRendererFactory);
  portletURL.setParameter("subtypeSelectionId",String.valueOf(assetAvailableClassType.getKey()));
  data.put("href",portletURL.toString());
  String title=LanguageUtil.format(_pageContext,"select-x",assetAvailableClassType.getValue(),false);
  data.put("title",title);
  data.put("type",assetAvailableClassType.getValue());
  return data;
}
