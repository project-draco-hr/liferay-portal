{
  List<Map<String,Object>> selectorEntries=new ArrayList<Map<String,Object>>();
  for (  AssetRendererFactory assetRendererFactory : getAssetRendererFactories()) {
    if (assetRendererFactory.isSupportsClassTypes()) {
      selectorEntries.addAll(_getSelectorEntries(assetRendererFactory));
    }
 else {
      Map<String,Object> selectorEntry=new HashMap<String,Object>();
      selectorEntry.put("data",_geSelectorEntryData(assetRendererFactory));
      selectorEntry.put("id",_getSelectorEntryId(assetRendererFactory));
      selectorEntry.put("message",_getSelectorEntryMessage(assetRendererFactory));
      selectorEntry.put("src",_getSelectorEntrySrc(assetRendererFactory));
      selectorEntries.add(selectorEntry);
    }
  }
  return selectorEntries;
}
