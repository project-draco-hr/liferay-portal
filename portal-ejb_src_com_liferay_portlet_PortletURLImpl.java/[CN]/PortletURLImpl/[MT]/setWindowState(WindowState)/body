{
  if (_portletReq != null) {
    if (!_portletReq.isWindowStateAllowed(windowState)) {
      throw new WindowStateException("" + windowState,windowState);
    }
  }
  if (!LiferayWindowState.isWindowStateIgnored(getWindowState(),windowState)) {
    _windowState=windowState;
  }
  _toString=null;
}
