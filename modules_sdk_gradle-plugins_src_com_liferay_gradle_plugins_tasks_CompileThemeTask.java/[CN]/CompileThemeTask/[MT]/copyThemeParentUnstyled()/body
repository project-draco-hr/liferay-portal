{
  copyPortalThemeDir("_unstyled",new String[]{"templates/**"},"**");
  Set<String> themeTypes=getThemeTypes();
  String[] themeTypesArray=themeTypes.toArray(new String[themeTypes.size()]);
  String[] excludes=StringUtil.prepend(themeTypesArray,"templates/init.");
  String[] includes=StringUtil.prepend(themeTypesArray,"templates/**/*.");
  copyPortalThemeDir("_unstyled",excludes,includes);
}
