{
  ResourceCode resourceCode=fetchByPrimaryKey(codeId);
  if (resourceCode == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ResourceCode exists with the primary key " + codeId);
    }
    throw new NoSuchResourceCodeException("No ResourceCode exists with the primary key " + codeId);
  }
  return resourceCode;
}
