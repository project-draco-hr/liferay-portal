{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(resourceCode);
    }
  }
  resourceCode=removeImpl(resourceCode);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(resourceCode);
    }
  }
  return resourceCode;
}
