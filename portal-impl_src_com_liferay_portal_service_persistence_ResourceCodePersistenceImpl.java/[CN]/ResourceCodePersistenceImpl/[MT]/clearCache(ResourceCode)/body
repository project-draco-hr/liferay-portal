{
  EntityCacheUtil.removeResult(ResourceCodeModelImpl.ENTITY_CACHE_ENABLED,ResourceCodeImpl.class,resourceCode.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_N_S,new Object[]{Long.valueOf(resourceCode.getCompanyId()),resourceCode.getName(),Integer.valueOf(resourceCode.getScope())});
}
