{
  ResourceCode result=(ResourceCode)EntityCacheUtil.getResult(ResourceCodeModelImpl.ENTITY_CACHE_ENABLED,ResourceCodeImpl.class,codeId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      ResourceCode resourceCode=(ResourceCode)session.get(ResourceCodeImpl.class,new Long(codeId));
      if (resourceCode != null) {
        cacheResult(resourceCode);
      }
      return resourceCode;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (ResourceCode)result;
  }
}
