{
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_N_S,new Object[]{new Long(resourceCode.getCompanyId()),resourceCode.getName(),new Integer(resourceCode.getScope())},resourceCode);
  EntityCacheUtil.putResult(ResourceCodeModelImpl.ENTITY_CACHE_ENABLED,ResourceCode.class,resourceCode.getPrimaryKey(),resourceCode);
}
