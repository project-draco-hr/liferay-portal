{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,resourceCode,merge);
    resourceCode.setNew(false);
    return resourceCode;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ResourceCode.class.getName());
  }
}
