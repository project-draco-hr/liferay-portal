{
  resourceCode=toUnwrappedModel(resourceCode);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,resourceCode);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(resourceCode);
  return resourceCode;
}
