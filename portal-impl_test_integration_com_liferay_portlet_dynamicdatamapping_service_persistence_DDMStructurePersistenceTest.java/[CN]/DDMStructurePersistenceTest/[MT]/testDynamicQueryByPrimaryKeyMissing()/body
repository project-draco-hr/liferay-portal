{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMStructure.class,DDMStructure.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("structureId",ServiceTestUtil.nextLong()));
  List<DDMStructure> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
