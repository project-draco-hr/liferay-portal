{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMStructure.class,DDMStructure.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("structureId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("structureId",new Object[]{ServiceTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
