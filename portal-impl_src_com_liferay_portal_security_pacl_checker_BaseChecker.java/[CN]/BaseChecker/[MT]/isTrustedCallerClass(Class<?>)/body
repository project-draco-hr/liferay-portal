{
  String callerClassLocation=PACLClassUtil.getClassLocation(callerClass);
  if (callerClassLocation.startsWith(portalImplJarLocation) || callerClassLocation.startsWith(portalServiceJarLocation) || callerClassLocation.contains("/util-bridges.jar!/")|| callerClassLocation.contains("/util-java.jar!/")|| callerClassLocation.contains("/util-taglib.jar!/")) {
    return true;
  }
  return false;
}
