{
  String description=StringPool.BLANK;
  String changeLog=StringPool.BLANK;
  if ((bytes == null) && Validator.isNotNull(sourceFileName)) {
    bytes=_CONTENT.getBytes();
  }
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  serviceContext.setScopeGroupId(groupId);
  serviceContext.setWorkflowAction(workflowAction);
  return DLAppLocalServiceUtil.addFileEntry(userId,groupId,folderId,sourceFileName,mimeType,title,description,changeLog,bytes,serviceContext);
}
