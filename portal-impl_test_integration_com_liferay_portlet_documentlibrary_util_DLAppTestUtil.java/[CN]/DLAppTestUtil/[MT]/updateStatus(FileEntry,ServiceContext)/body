{
  Map<String,Serializable> workflowContext=new HashMap<String,Serializable>();
  workflowContext.put("event",DLSyncConstants.EVENT_ADD);
  workflowContext.put(WorkflowConstants.CONTEXT_URL,"http://localhost");
  DLAppHelperLocalServiceUtil.updateStatus(TestPropsValues.getUserId(),fileEntry,fileEntry.getLatestFileVersion(),WorkflowConstants.STATUS_PENDING,WorkflowConstants.STATUS_APPROVED,workflowContext,serviceContext);
}
