{
  int workflowAction=WorkflowConstants.ACTION_SAVE_DRAFT;
  if (approved) {
    workflowAction=WorkflowConstants.ACTION_PUBLISH;
  }
  return addFileEntry(serviceContext.getUserId(),serviceContext.getScopeGroupId(),folderId,sourceFileName,ContentTypes.TEXT_PLAIN,title,null,workflowAction,serviceContext);
}
