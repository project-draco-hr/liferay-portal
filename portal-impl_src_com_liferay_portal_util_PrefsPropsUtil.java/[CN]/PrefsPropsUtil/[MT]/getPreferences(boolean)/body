{
  PortalPreferencesWrapper portalPreferencesWrapper=PortalPreferencesWrapperCacheUtil.get(PortletKeys.PREFS_OWNER_ID_DEFAULT,PortletKeys.PREFS_OWNER_TYPE_COMPANY);
  if (portalPreferencesWrapper != null) {
    if (!readOnly) {
      portalPreferencesWrapper=portalPreferencesWrapper.clone();
    }
    return portalPreferencesWrapper;
  }
  return _portalPreferencesLocalService.getPreferences(PortletKeys.PREFS_OWNER_ID_DEFAULT,PortletKeys.PREFS_OWNER_TYPE_COMPANY);
}
