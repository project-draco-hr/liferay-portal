{
  ConfigurationFactoryUtil.setConfigurationFactory(new ConfigurationFactoryImpl());
  WikiPage wikiPage=new WikiPageImpl();
  wikiPage.setFormat(format);
  try {
    when(_wikiPageLocalService.getPage(primKey)).thenReturn(wikiPage);
