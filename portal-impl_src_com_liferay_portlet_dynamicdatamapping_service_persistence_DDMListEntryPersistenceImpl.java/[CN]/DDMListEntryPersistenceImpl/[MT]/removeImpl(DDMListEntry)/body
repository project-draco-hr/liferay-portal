{
  ddmListEntry=toUnwrappedModel(ddmListEntry);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,ddmListEntry);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.removeResult(DDMListEntryModelImpl.ENTITY_CACHE_ENABLED,DDMListEntryImpl.class,ddmListEntry.getPrimaryKey());
  return ddmListEntry;
}
