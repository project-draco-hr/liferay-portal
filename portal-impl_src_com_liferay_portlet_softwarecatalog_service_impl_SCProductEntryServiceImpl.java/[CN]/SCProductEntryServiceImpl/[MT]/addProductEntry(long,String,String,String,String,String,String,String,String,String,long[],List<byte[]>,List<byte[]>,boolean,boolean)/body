{
  PortletPermissionUtil.check(getPermissionChecker(),plid,PortletKeys.SOFTWARE_CATALOG,ActionKeys.ADD_PRODUCT_ENTRY);
  return scProductEntryLocalService.addProductEntry(getUserId(),plid,name,type,tags,shortDescription,longDescription,pageURL,author,repoGroupId,repoArtifactId,licenseIds,thumbnails,fullImages,addCommunityPermissions,addGuestPermissions);
}
