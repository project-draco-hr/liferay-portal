{
  PortletPermissionUtil.check(getPermissionChecker(),serviceContext.getPlid(),PortletKeys.SOFTWARE_CATALOG,ActionKeys.ADD_PRODUCT_ENTRY);
  return scProductEntryLocalService.addProductEntry(getUserId(),name,type,tags,shortDescription,longDescription,pageURL,author,repoGroupId,repoArtifactId,licenseIds,thumbnails,fullImages,serviceContext);
}
