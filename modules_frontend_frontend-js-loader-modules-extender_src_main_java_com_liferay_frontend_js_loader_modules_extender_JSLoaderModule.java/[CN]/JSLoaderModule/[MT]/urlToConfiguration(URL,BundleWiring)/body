{
  if (url == null) {
    return;
  }
  try (Reader reader=new InputStreamReader(url.openStream())){
    JSONTokener jsonTokener=new JSONTokener(reader);
    JSONObject jsonObject=new JSONObject(jsonTokener);
    _unversionedConfiguration=normalize(generateConfiguration(jsonObject,bundleWiring,false));
    _versionedConfiguration=normalize(generateConfiguration(jsonObject,bundleWiring,true));
    Dictionary<String,String> headers=_bundle.getHeaders();
    String exportJSSubmodules=GetterUtil.getString(headers.get("Liferay-Export-JS-Submodules"));
    if (Validator.isNotNull(exportJSSubmodules)) {
      _unversionedMapsConfiguration=normalize(generateMapsConfiguration(_unversionedConfiguration,StringUtil.split(exportJSSubmodules)));
    }
  }
 catch (  IOException ioe) {
    throw new RuntimeException(ioe);
  }
}
