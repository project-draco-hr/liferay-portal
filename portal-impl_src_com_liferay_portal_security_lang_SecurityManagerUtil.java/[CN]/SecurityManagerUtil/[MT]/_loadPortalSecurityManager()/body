{
  try {
    List<PortalSecurityManager> portalSecurityManagers=ServiceLoader.load(ModuleFrameworkAdapterHelper.getClassLoader(),PortalSecurityManager.class);
    if (portalSecurityManagers.isEmpty()) {
      return;
    }
    _portalSecurityManager=portalSecurityManagers.get(0);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
