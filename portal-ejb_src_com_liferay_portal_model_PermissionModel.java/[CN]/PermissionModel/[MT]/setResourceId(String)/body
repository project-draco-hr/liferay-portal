{
  if (((resourceId == null) && (_resourceId != null)) || ((resourceId != null) && (_resourceId == null)) || ((resourceId != null) && (_resourceId != null) && !resourceId.equals(_resourceId))) {
    if (!XSS_ALLOW_RESOURCEID) {
      resourceId=XSSUtil.strip(resourceId);
    }
    _resourceId=resourceId;
    setModified(true);
  }
}
