{
  if (((actionId == null) && (_actionId != null)) || ((actionId != null) && (_actionId == null)) || ((actionId != null) && (_actionId != null) && !actionId.equals(_actionId))) {
    if (!XSS_ALLOW_ACTIONID) {
      actionId=XSSUtil.strip(actionId);
    }
    _actionId=actionId;
    setModified(true);
  }
}
