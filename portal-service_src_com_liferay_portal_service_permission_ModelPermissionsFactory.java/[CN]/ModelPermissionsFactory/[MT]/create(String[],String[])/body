{
  ModelPermissions modelPermissions=new ModelPermissions();
  if ((groupPermissions != null) && (groupPermissions.length > 0)) {
    modelPermissions.addRolePermissions(RoleConstants.DEFAULT_GROUP_ROLE,groupPermissions);
  }
  if ((guestPermissions != null) && (guestPermissions.length > 0)) {
    modelPermissions.addRolePermissions(RoleConstants.GUEST,guestPermissions);
  }
  return modelPermissions;
}
