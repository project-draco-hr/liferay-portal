{
  SCProductEntry newSCProductEntry=addSCProductEntry();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SCProductEntry.class,SCProductEntry.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("productEntryId"));
  Object newProductEntryId=newSCProductEntry.getProductEntryId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("productEntryId",new Object[]{newProductEntryId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Object existingProductEntryId=result.get(0);
  assertEquals(existingProductEntryId,newProductEntryId);
}
