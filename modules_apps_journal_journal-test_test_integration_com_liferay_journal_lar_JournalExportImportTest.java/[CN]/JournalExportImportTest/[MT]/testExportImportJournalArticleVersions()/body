{
  JournalArticle article=(JournalArticle)addStagedModel(group.getGroupId());
  exportImportPortlet(PortletKeys.JOURNAL);
  JournalArticle liveArticle=JournalArticleLocalServiceUtil.fetchJournalArticleByUuidAndGroupId(article.getUuid(),importedGroup.getGroupId());
  Assert.assertNotNull(liveArticle);
  JournalTestUtil.updateArticle(article,RandomTestUtil.randomString());
  int articleVersionsCount=JournalArticleLocalServiceUtil.getArticlesCount(group.getGroupId(),article.getArticleId());
  Assert.assertEquals(2,articleVersionsCount);
  exportImportPortlet(PortletKeys.JOURNAL);
  articleVersionsCount=JournalArticleLocalServiceUtil.getArticlesCount(importedGroup.getGroupId(),liveArticle.getArticleId());
  Assert.assertEquals(2,articleVersionsCount);
}
