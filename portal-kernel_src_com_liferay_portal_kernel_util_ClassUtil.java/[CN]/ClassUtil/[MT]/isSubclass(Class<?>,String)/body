{
  if ((a == null) || (s == null)) {
    return false;
  }
  if (a.getName().equals(s)) {
    return true;
  }
  for (Class<?> x=a; x != null; x=x.getSuperclass()) {
    if (x.getName().equals(s)) {
      return true;
    }
    Class<?>[] interfaceClasses=x.getInterfaces();
    for (    Class<?> interfaceClass : interfaceClasses) {
      if (isSubclass(interfaceClass,s)) {
        return true;
      }
    }
  }
  return false;
}
