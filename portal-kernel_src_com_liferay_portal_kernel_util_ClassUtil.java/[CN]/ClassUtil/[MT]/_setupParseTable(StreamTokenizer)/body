{
  st.resetSyntax();
  st.slashSlashComments(true);
  st.slashStarComments(true);
  st.wordChars('a','z');
  st.wordChars('A','Z');
  st.wordChars('.','.');
  st.wordChars('0','9');
  st.wordChars('_','_');
  st.lowerCaseMode(false);
  st.eolIsSignificant(false);
  st.quoteChar('"');
  st.quoteChar('\'');
  st.parseNumbers();
}
