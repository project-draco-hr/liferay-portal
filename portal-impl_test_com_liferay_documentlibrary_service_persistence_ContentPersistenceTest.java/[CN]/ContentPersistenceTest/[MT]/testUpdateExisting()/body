{
  Content content=addContent();
  byte[] data=new byte[1024];
  Blob blob=new OutputBlob(new ByteArrayInputStream(data),data.length);
  Content newContent=new Content(content.getContentId(),nextLong(),randomString(),nextLong(),nextLong(),randomString(),randomString(),blob,blob.length());
  _persistence.update(newContent);
  Content existingContent=_persistence.findByPrimaryKey(newContent.getContentId());
  assertTrue(existingContent.equals(newContent));
}
