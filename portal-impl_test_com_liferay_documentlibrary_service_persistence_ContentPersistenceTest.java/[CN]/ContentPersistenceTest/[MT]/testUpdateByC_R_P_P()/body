{
  Content content=addContent();
  String newPath=randomString();
  _persistence.update(content.getCompanyId(),content.getRepositoryId(),content.getPath(),newPath);
  Content newContent=_persistence.fetchByC_R_P_V(content.getCompanyId(),content.getRepositoryId(),newPath,content.getVersion());
  assertFalse(content.getPath().equals(newContent.getPath()));
  assertFalse(content.equals(newContent));
}
