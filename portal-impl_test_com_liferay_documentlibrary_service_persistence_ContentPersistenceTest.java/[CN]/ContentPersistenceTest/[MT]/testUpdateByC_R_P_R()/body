{
  Content content=addContent();
  long newRepositoryId=nextLong();
  _persistence.update(content.getCompanyId(),content.getRepositoryId(),content.getPath(),newRepositoryId);
  Content newContent=_persistence.fetchByC_R_P_V(content.getCompanyId(),newRepositoryId,content.getPath(),content.getVersion());
  assertFalse(content.getRepositoryId() == newContent.getRepositoryId());
  assertFalse(content.equals(newContent));
}
