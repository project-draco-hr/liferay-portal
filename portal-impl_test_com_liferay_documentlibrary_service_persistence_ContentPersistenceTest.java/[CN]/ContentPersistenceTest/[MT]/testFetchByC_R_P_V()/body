{
  Content content=addContent();
  Content existingContent=_persistence.fetchByC_R_P_V(content.getCompanyId(),content.getRepositoryId(),content.getPath(),content.getVersion());
  assertTrue(existingContent.equals(content));
  _persistence.remove(existingContent.getContentId());
  Content missingContent=_persistence.fetchByC_R_P_V(content.getCompanyId(),content.getRepositoryId(),content.getPath(),randomString());
  assertNull(missingContent);
}
