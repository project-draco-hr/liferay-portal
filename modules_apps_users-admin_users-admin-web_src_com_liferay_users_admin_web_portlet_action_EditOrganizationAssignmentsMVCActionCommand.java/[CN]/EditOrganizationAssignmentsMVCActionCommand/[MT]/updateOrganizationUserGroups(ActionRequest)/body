{
  long organizationId=ParamUtil.getLong(actionRequest,"organizationId");
  Organization organization=_organizationLocalService.getOrganization(organizationId);
  long groupId=organization.getGroupId();
  long[] addUserGroupIds=StringUtil.split(ParamUtil.getString(actionRequest,"addUserGroupIds"),0L);
  long[] removeUserGroupIds=StringUtil.split(ParamUtil.getString(actionRequest,"removeUserGroupIds"),0L);
  _userGroupService.addGroupUserGroups(groupId,addUserGroupIds);
  _userGroupService.unsetGroupUserGroups(groupId,removeUserGroupIds);
}
