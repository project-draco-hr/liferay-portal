{
  Template template=TemplateManagerUtil.getTemplate(TemplateConstants.LANG_TYPE_SOY,templateResource,false);
  template.put(TemplateConstants.NAMESPACE,templateNamespace);
  populateRequiredContext(template,ddmFormField,ddmFormFieldRenderingContext);
  populateOptionalContext(template,ddmFormField,ddmFormFieldRenderingContext);
  return render(template);
}
