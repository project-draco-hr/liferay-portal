{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByG_F_PrevAndNext(entryId,groupId,folderId,orderByComparator);
  }
  BookmarksEntry bookmarksEntry=findByPrimaryKey(entryId);
  Session session=null;
  try {
    session=openSession();
    BookmarksEntry[] array=new BookmarksEntryImpl[3];
    array[0]=filterGetByG_F_PrevAndNext(session,bookmarksEntry,groupId,folderId,orderByComparator,true);
    array[1]=bookmarksEntry;
    array[2]=filterGetByG_F_PrevAndNext(session,bookmarksEntry,groupId,folderId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
