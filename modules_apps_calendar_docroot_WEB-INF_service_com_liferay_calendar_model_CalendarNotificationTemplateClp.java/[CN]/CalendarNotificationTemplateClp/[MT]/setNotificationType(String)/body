{
  _notificationType=notificationType;
  if (_calendarNotificationTemplateRemoteModel != null) {
    try {
      Class<?> clazz=_calendarNotificationTemplateRemoteModel.getClass();
      Method method=clazz.getMethod("setNotificationType",String.class);
      method.invoke(_calendarNotificationTemplateRemoteModel,notificationType);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
