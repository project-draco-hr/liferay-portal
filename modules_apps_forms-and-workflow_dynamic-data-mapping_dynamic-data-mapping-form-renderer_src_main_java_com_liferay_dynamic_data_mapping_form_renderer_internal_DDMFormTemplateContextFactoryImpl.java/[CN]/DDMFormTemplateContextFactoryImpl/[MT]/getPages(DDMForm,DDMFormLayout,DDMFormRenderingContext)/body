{
  Map<String,List<Object>> ddmFormFieldsTemplateContextMap=getDDMFormFieldsTemplateContextMap(ddmForm,ddmFormRenderingContext);
  DDMFormLayoutTransformer ddmFormLayoutTransformer=new DDMFormLayoutTransformer(ddmForm,ddmFormLayout,ddmFormFieldsTemplateContextMap,ddmFormRenderingContext.isShowRequiredFieldsWarning(),ddmFormRenderingContext.getLocale());
  return ddmFormLayoutTransformer.getPages();
}
