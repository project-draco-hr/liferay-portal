{
  String emailArticleApprovalRequestedSubject=preferences.getValue("email-article-approval-requested-subject",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleApprovalRequestedSubject)) {
    return emailArticleApprovalRequestedSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_APPROVAL_REQUESTED_SUBJECT));
  }
}
