{
  PortletSession ses=req.getPortletSession();
  Stack<JournalStructure> recentStructures=(Stack<JournalStructure>)ses.getAttribute(WebKeys.JOURNAL_RECENT_STRUCTURES);
  if (recentStructures == null) {
    recentStructures=new FiniteUniqueStack<JournalStructure>(MAX_STACK_SIZE);
    ses.setAttribute(WebKeys.JOURNAL_RECENT_STRUCTURES,recentStructures);
  }
  return recentStructures;
}
