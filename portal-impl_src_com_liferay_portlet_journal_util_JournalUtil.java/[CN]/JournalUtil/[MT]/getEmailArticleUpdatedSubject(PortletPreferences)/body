{
  String emailArticleUpdatedSubject=preferences.getValue("email-article-updated-subject",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleUpdatedSubject)) {
    return emailArticleUpdatedSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_UPDATED_SUBJECT));
  }
}
