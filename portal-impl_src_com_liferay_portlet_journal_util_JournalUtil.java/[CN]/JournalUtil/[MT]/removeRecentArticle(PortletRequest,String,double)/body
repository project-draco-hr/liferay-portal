{
  Stack<JournalArticle> stack=getRecentArticles(portletRequest);
  Iterator<JournalArticle> itr=stack.iterator();
  while (itr.hasNext()) {
    JournalArticle journalArticle=itr.next();
    if ((journalArticle.getArticleId().equals(articleId) && journalArticle.getVersion() == version) || (journalArticle.getArticleId().equals(articleId)) && version == -1.0) {
      itr.remove();
    }
  }
}
