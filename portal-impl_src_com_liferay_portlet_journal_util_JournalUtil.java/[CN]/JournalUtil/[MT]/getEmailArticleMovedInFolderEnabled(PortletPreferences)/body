{
  String emailArticleMovedInFolderEnabled=preferences.getValue("emailArticleMovedInFolderEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleMovedInFolderEnabled)) {
    return GetterUtil.getBoolean(emailArticleMovedInFolderEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_MOVED_IN_FOLDER_ENABLED));
  }
}
