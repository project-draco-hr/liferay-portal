{
  String emailArticleApprovalDeniedSubject=preferences.getValue("email-article-approval-denied-subject",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleApprovalDeniedSubject)) {
    return emailArticleApprovalDeniedSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_APPROVAL_DENIED_SUBJECT));
  }
}
