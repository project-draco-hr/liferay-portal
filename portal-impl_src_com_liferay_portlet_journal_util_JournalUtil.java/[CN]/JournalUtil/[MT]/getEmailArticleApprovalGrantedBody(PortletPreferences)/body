{
  String emailArticleApprovalGrantedBody=preferences.getValue("email-article-approval-granted-body",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleApprovalGrantedBody)) {
    return emailArticleApprovalGrantedBody;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_APPROVAL_GRANTED_BODY));
  }
}
