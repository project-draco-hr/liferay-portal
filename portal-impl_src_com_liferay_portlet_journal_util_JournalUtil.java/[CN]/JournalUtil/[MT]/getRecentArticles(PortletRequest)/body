{
  PortletSession ses=req.getPortletSession();
  Stack<JournalArticle> recentArticles=(Stack<JournalArticle>)ses.getAttribute(WebKeys.JOURNAL_RECENT_ARTICLES);
  if (recentArticles == null) {
    recentArticles=new FiniteUniqueStack<JournalArticle>(MAX_STACK_SIZE);
    ses.setAttribute(WebKeys.JOURNAL_RECENT_ARTICLES,recentArticles);
  }
  return recentArticles;
}
