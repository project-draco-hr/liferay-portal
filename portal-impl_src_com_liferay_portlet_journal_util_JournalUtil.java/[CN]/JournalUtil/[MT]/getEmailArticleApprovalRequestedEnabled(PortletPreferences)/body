{
  String emailArticleApprovalRequestedEnabled=preferences.getValue("email-article-approval-requested-enabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleApprovalRequestedEnabled)) {
    return GetterUtil.getBoolean(emailArticleApprovalRequestedEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_APPROVAL_REQUESTED_ENABLED));
  }
}
