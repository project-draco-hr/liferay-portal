{
  String emailArticleMovedOutOfFolderEnabled=preferences.getValue("emailArticleMovedOutOfFolderEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleMovedOutOfFolderEnabled)) {
    return GetterUtil.getBoolean(emailArticleMovedOutOfFolderEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_MOVED_OUT_OF_FOLDER_ENABLED));
  }
}
