{
  List<Long> classPKs=new ArrayList<Long>();
  for (  long groupId : groupIds) {
    @SuppressWarnings("deprecation") JournalStructure structure=com.liferay.portlet.journal.service.JournalStructureLocalServiceUtil.fetchStructure(groupId,structureId);
    if (structure != null) {
      classPKs.add(structure.getId());
    }
  }
  return ArrayUtil.toLongArray(classPKs);
}
