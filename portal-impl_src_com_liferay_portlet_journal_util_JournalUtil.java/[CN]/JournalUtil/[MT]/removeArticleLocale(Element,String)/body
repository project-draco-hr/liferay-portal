{
  for (  Element dynamicEl : el.elements("dynamic-element")) {
    for (    Element dynamicContentEl : dynamicEl.elements("dynamic-content")) {
      String curLanguageId=GetterUtil.getString(dynamicContentEl.attributeValue("language-id"));
      if (curLanguageId.equals(languageId)) {
        long id=GetterUtil.getLong(dynamicContentEl.attributeValue("id"));
        if (id > 0) {
          ImageLocalServiceUtil.deleteImage(id);
        }
        dynamicContentEl.detach();
      }
    }
    removeArticleLocale(dynamicEl,languageId);
  }
}
