{
  try {
    if (content.indexOf(" instance-id=\"") == -1) {
      Document doc=SAXReaderUtil.read(content);
      Element root=doc.getRootElement();
      _addDynamicElementInstanceId(root);
      content=formatXML(doc);
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return content;
}
