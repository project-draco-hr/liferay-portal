{
  String emailArticleUpdatedBody=preferences.getValue("email-article-updated-body",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleUpdatedBody)) {
    return emailArticleUpdatedBody;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_UPDATED_BODY));
  }
}
