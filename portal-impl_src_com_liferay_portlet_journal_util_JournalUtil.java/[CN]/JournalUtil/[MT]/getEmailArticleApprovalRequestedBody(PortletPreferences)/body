{
  String emailArticleApprovalRequestedBody=preferences.getValue("email-article-approval-requested-body",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleApprovalRequestedBody)) {
    return emailArticleApprovalRequestedBody;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_APPROVAL_REQUESTED_BODY));
  }
}
