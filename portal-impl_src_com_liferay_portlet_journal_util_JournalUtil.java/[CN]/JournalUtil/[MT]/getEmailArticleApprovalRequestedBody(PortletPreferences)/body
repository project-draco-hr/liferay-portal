{
  String emailArticleApprovalRequestedBody=prefs.getValue("email-article-approval-requested-body",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleApprovalRequestedBody)) {
    return emailArticleApprovalRequestedBody;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsUtil.JOURNAL_EMAIL_ARTICLE_APPROVAL_REQUESTED_BODY));
  }
}
