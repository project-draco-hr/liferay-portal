{
  PortletSession portletSession=portletRequest.getPortletSession();
  Stack<JournalTemplate> recentTemplates=(Stack<JournalTemplate>)portletSession.getAttribute(WebKeys.JOURNAL_RECENT_TEMPLATES);
  if (recentTemplates == null) {
    recentTemplates=new FiniteUniqueStack<JournalTemplate>(MAX_STACK_SIZE);
    portletSession.setAttribute(WebKeys.JOURNAL_RECENT_TEMPLATES,recentTemplates);
  }
  return recentTemplates;
}
