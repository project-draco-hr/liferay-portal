{
  String emailArticleAddedSubject=preferences.getValue("email-article-added-subject",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleAddedSubject)) {
    return emailArticleAddedSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_ADDED_SUBJECT));
  }
}
