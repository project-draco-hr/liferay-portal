{
  String emailArticleUpdatedEnabled=preferences.getValue("email-article-updated-enabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleUpdatedEnabled)) {
    return GetterUtil.getBoolean(emailArticleUpdatedEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_UPDATED_ENABLED));
  }
}
