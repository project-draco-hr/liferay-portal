{
  if (Validator.isNull(instanceId)) {
    return null;
  }
  XPath xPathSelector=SAXReaderUtil.createXPath("//dynamic-element[@instance-id=" + HtmlUtil.escapeXPathAttribute(instanceId) + "]");
  List<Node> nodes=xPathSelector.selectNodes(document);
  if (nodes.size() == 1) {
    return (Element)nodes.get(0);
  }
 else {
    return null;
  }
}
