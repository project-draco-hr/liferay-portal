{
  Stack<JournalTemplate> stack=getRecentTemplates(portletRequest);
  Iterator<JournalTemplate> itr=stack.iterator();
  while (itr.hasNext()) {
    JournalTemplate journalTemplate=itr.next();
    if (journalTemplate.getTemplateId().equals(templateId)) {
      itr.remove();
      break;
    }
  }
}
