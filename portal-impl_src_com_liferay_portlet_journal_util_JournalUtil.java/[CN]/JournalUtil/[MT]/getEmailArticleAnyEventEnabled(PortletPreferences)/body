{
  if (JournalUtil.getEmailArticleAddedEnabled(preferences) || JournalUtil.getEmailArticleApprovalDeniedEnabled(preferences) || JournalUtil.getEmailArticleApprovalGrantedEnabled(preferences)|| JournalUtil.getEmailArticleApprovalRequestedEnabled(preferences)|| JournalUtil.getEmailArticleReviewEnabled(preferences)|| JournalUtil.getEmailArticleUpdatedEnabled(preferences)) {
    return true;
  }
  return false;
}
