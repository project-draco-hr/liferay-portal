{
  String emailArticleMovedToFolderEnabled=preferences.getValue("emailArticleMovedToFolderEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleMovedToFolderEnabled)) {
    return GetterUtil.getBoolean(emailArticleMovedToFolderEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_MOVED_TO_FOLDER_ENABLED));
  }
}
