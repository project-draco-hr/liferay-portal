{
  String emailArticleApprovalGrantedSubject=preferences.getValue("email-article-approval-granted-subject",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleApprovalGrantedSubject)) {
    return emailArticleApprovalGrantedSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_APPROVAL_GRANTED_SUBJECT));
  }
}
