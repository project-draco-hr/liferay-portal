{
  String emailArticleMovedFromFolderEnabled=preferences.getValue("emailArticleMovedFromFolderEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleMovedFromFolderEnabled)) {
    return GetterUtil.getBoolean(emailArticleMovedFromFolderEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsKeys.JOURNAL_EMAIL_ARTICLE_MOVED_FROM_FOLDER_ENABLED));
  }
}
