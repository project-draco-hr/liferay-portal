{
  if (PropsValues.IMAGEMAGICK_ENABLED) {
    String os=System.getProperty("os.name").toLowerCase();
    String filterName;
    if (os.indexOf("mac") >= 0) {
      filterName="apple";
    }
 else     if (os.indexOf("win") >= 0) {
      filterName="windows";
    }
 else {
      filterName="unix";
    }
    String path=PropsUtil.get(PropsKeys.IMAGEMAGICK_GLOBAL_SEARCH_PATH,new Filter(filterName));
    ProcessStarter.setGlobalSearchPath(path);
    _convertCommand=new ConvertCmd();
  }
}
