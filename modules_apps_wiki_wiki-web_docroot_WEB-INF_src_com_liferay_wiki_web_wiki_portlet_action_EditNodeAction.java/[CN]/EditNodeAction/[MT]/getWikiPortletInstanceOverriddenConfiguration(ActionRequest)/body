{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  WikiPortletInstanceOverriddenConfiguration wikiPortletInstanceOverriddenConfiguration=ConfigurationFactoryUtil.getConfiguration(WikiPortletInstanceOverriddenConfiguration.class,new PortletInstanceSettingsLocator(themeDisplay.getLayout(),portletDisplay.getId()));
  return wikiPortletInstanceOverriddenConfiguration;
}
