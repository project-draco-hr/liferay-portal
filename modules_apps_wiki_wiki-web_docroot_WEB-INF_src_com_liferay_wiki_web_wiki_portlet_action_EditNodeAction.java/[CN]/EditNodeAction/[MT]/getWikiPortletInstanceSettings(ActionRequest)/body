{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  WikiPortletInstanceSettings wikiPortletInstanceSettings=ModuleConfigurationFactoryUtil.getConfiguration(WikiPortletInstanceSettings.class,new PortletInstanceSettingsLocator(themeDisplay.getLayout(),portletDisplay.getId()));
  return wikiPortletInstanceSettings;
}
