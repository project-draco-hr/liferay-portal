{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  WikiPortletInstanceOverriddenConfiguration wikiPortletInstanceConfiguration=ModuleConfigurationFactoryUtil.getModuleConfiguration(WikiPortletInstanceOverriddenConfiguration.class,new PortletInstanceSettingsLocator(themeDisplay.getLayout(),portletDisplay.getId()));
  return wikiPortletInstanceConfiguration;
}
