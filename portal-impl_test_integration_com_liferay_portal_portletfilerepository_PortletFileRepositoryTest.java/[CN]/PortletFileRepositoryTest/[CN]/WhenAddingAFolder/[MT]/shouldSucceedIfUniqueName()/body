{
  String portletId=RandomTestUtil.randomString();
  Folder folder=_addPortletFolder(portletId);
  _addPortletFileEntry(portletId,folder.getFolderId(),RandomTestUtil.randomString());
  _addPortletFileEntry(portletId,folder.getFolderId(),RandomTestUtil.randomString());
  int count=PortletFileRepositoryUtil.getPortletFileEntriesCount(TestPropsValues.getGroupId(),folder.getFolderId());
  Assert.assertEquals(count,2);
}
