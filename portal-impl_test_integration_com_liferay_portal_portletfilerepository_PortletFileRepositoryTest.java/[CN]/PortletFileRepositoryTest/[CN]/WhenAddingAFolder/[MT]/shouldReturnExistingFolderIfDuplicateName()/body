{
  String portletId=RandomTestUtil.randomString();
  Folder parentFolder=_addPortletFolder(portletId);
  String name=RandomTestUtil.randomString();
  Folder folder=_addPortletFolder(portletId,parentFolder.getFolderId(),name);
  Folder newFolder=_addPortletFolder(portletId,parentFolder.getFolderId(),name);
  Assert.assertEquals(newFolder,folder);
}
