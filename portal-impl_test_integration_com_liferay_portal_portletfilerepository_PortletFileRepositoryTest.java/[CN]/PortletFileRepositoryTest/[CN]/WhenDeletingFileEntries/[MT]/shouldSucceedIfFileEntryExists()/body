{
  String portletId=RandomTestUtil.randomString();
  Folder folder=_addPortletFolder(portletId);
  FileEntry fileEntry=_addPortletFileEntry(portletId,folder.getFolderId(),RandomTestUtil.randomString());
  PortletFileRepositoryUtil.deletePortletFileEntry(fileEntry.getFileEntryId());
  int count=PortletFileRepositoryUtil.getPortletFileEntriesCount(TestPropsValues.getGroupId(),folder.getFolderId());
  Assert.assertEquals(0,count);
}
