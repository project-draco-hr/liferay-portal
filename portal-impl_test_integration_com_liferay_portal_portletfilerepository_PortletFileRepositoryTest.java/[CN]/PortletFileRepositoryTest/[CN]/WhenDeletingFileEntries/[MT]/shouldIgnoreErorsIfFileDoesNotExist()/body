{
  FileEntry fileEntry=_addPortletFileEntry(_portletId,_folder.getFolderId(),RandomTestUtil.randomString());
  PortletFileRepositoryUtil.deletePortletFileEntry(fileEntry.getFileEntryId());
  PortletFileRepositoryUtil.deletePortletFileEntry(fileEntry.getFileEntryId());
  int count=PortletFileRepositoryUtil.getPortletFileEntriesCount(TestPropsValues.getGroupId(),_folder.getFolderId());
  Assert.assertEquals(0,count);
}
