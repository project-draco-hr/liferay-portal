{
  int fileEntriesToAdd=Math.abs(RandomTestUtil.randomInt()) % 10;
  for (int i=0; i < fileEntriesToAdd; i++) {
    _addPortletFileEntry(_group.getGroupId(),_portletId,_folder.getFolderId(),RandomTestUtil.randomString());
  }
  PortletFileRepositoryUtil.deletePortletFileEntries(_group.getGroupId(),_folder.getFolderId());
  int count=PortletFileRepositoryUtil.getPortletFileEntriesCount(_group.getGroupId(),_folder.getFolderId());
  Assert.assertEquals(0,count);
}
