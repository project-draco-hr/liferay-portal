{
  int fileEntriesToAdd=Math.abs(RandomTestUtil.randomInt()) % 10;
  for (int i=0; i < fileEntriesToAdd; i++) {
    _addPortletFileEntry(_portletId,_folder.getFolderId(),RandomTestUtil.randomString());
  }
  PortletFileRepositoryUtil.deletePortletFolder(_folder.getFolderId());
  int count=PortletFileRepositoryUtil.getPortletFileEntriesCount(TestPropsValues.getGroupId(),_folder.getFolderId());
  Assert.assertEquals(0,count);
}
