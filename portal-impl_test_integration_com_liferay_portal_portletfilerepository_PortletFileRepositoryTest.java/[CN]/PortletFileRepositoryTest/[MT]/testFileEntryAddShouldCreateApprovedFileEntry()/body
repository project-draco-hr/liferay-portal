{
  FileEntry fileEntry=_addPortletFileEntry(RandomTestUtil.randomString());
  Repository repository=RepositoryProviderUtil.getRepository(fileEntry.getRepositoryId());
  WorkflowCapability workflowCapability=repository.getCapability(WorkflowCapability.class);
  Assert.assertEquals(WorkflowConstants.STATUS_APPROVED,workflowCapability.getStatus(fileEntry));
}
