{
  try {
    ClassLoader classLoader=ClassLoader.getSystemClassLoader();
    InputStream is=classLoader.getResourceAsStream("test-portal-impl.properties");
    _props.load(is);
    is=classLoader.getResourceAsStream("test-portal-impl-ext.properties");
    if (is != null) {
      _props.load(is);
    }
    _printProperties(false);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
