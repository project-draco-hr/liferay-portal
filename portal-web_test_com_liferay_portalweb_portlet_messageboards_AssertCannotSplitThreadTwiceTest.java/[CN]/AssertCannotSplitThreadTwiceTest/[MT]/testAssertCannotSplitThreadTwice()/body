{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=M\u00e9ssag\u00e9 Boards T\u00e9st Pag\u00e9")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=M\u00e9ssag\u00e9 Boards T\u00e9st Pag\u00e9",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//tr[4]/td[1]/a/strong",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//li[5]/span/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//td[1]/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//li[6]/span/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//div[5]/table/tbody/tr[2]/td/ul/li[4]/span/a[2]",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//input[@value='OK']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//input[@value='OK']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.keyPress("//div[1]/span",RuntimeVariables.replace("\\8"));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//input[@value='OK']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//input[@value='OK']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have entered invalid data. Please try again."));
  assertTrue(selenium.isTextPresent("A thread cannot be split at its root message."));
}
