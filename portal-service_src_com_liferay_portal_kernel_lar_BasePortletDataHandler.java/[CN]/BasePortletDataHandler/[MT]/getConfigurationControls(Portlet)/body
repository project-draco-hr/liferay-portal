{
  if (Validator.isNull(portlet.getConfigurationActionClass())) {
    return null;
  }
  return new PortletDataHandlerBoolean[]{new PortletDataHandlerBoolean(null,PortletDataHandlerKeys.PORTLET_SETUP,"setup",true,false,null,null,null),new PortletDataHandlerBoolean(null,PortletDataHandlerKeys.PORTLET_ARCHIVED_SETUPS,"archived-setups",true,false,null,null,null),new PortletDataHandlerBoolean(null,PortletDataHandlerKeys.PORTLET_USER_PREFERENCES,"user-preferences",true,false,null,null,null)};
}
