{
  long controlCount=-1;
  for (  PortletDataHandlerControl exportControl : getExportControls()) {
    long currControlCount=manifestSummary.getModelCount(exportControl.getClassName());
    if (currControlCount != -1) {
      if (controlCount == -1) {
        controlCount=0;
      }
      controlCount+=currControlCount;
    }
  }
  return controlCount;
}
