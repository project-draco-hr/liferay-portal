{
  LayoutSetBranch layoutSetBranch=layoutSetBranchPersistence.findByPrimaryKey(layoutSetBranchId);
  validate(layoutSetBranch.getLayoutSetBranchId(),layoutSetBranch.getGroupId(),layoutSetBranch.getPrivateLayout(),name,layoutSetBranch.isMaster());
  layoutSetBranch.setName(name);
  layoutSetBranch.setDescription(description);
  layoutSetBranchPersistence.update(layoutSetBranch);
  return layoutSetBranch;
}
