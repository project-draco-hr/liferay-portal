{
  TemplateVariableGroup generalVariablesTemplateVariableGroup=new TemplateVariableGroup("general-variables");
  generalVariablesTemplateVariableGroup.addVariable("portal-instance",Company.class,"company");
  generalVariablesTemplateVariableGroup.addVariable("portal-instance-id",null,"companyId");
  generalVariablesTemplateVariableGroup.addVariable("device",Device.class,"device");
  generalVariablesTemplateVariableGroup.addVariable("site-id",null,"groupId");
  generalVariablesTemplateVariableGroup.addVariable("view-mode",String.class,"viewMode");
  return generalVariablesTemplateVariableGroup;
}
