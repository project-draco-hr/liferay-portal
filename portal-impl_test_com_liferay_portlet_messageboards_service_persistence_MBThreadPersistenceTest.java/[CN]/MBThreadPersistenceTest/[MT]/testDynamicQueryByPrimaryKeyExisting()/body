{
  MBThread newMBThread=addMBThread();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBThread.class,MBThread.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("threadId",newMBThread.getThreadId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  MBThread existingMBThread=(MBThread)result.get(0);
  assertEquals(existingMBThread,newMBThread);
}
