{
  long layoutPrototypeId=ParamUtil.getLong(actionRequest,"layoutPrototypeId");
  Map<Locale,String> localeTitlesMap=LocalizationUtil.getLocalizationMap(actionRequest,"title");
  String description=ParamUtil.getString(actionRequest,"description");
  boolean active=ParamUtil.getBoolean(actionRequest,"active");
  if (layoutPrototypeId <= 0) {
    LayoutPrototypeServiceUtil.addLayoutPrototype(localeTitlesMap,description,active);
  }
 else {
    LayoutPrototypeServiceUtil.updateLayoutPrototype(layoutPrototypeId,localeTitlesMap,description,active);
  }
}
