{
  try {
    MailReceiptLocalService mailReceiptLocalService=MailReceiptLocalServiceFactory.getService();
    return mailReceiptLocalService.updateReceipt(receiptId);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
