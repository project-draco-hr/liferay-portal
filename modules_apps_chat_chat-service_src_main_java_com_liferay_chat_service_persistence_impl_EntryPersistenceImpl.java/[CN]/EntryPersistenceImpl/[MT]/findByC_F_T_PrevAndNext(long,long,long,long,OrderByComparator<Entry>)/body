{
  Entry entry=findByPrimaryKey(entryId);
  Session session=null;
  try {
    session=openSession();
    Entry[] array=new EntryImpl[3];
    array[0]=getByC_F_T_PrevAndNext(session,entry,createDate,fromUserId,toUserId,orderByComparator,true);
    array[1]=entry;
    array[2]=getByC_F_T_PrevAndNext(session,entry,createDate,fromUserId,toUserId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
