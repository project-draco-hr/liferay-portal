{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Blogs Portlet Permissions Page");
  selenium.clickAt("link=Blogs Portlet Permissions Page",RuntimeVariables.replace("Blogs Portlet Permissions Page"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isTextPresent("You do not have the roles required to access this portlet."));
}
