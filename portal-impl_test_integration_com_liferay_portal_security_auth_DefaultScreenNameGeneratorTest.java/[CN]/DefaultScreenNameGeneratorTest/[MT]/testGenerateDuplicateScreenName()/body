{
  User user=TestPropsValues.getUser();
  String screenName=_screenNameGenerator.generate(TestPropsValues.getCompanyId(),TestPropsValues.getUserId(),user.getScreenName() + "@liferay.com");
  Assert.assertNotSame(user.getScreenName(),screenName);
  Assert.assertEquals(user.getScreenName() + ".1",screenName);
}
