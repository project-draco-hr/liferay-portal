{
  String screenName=_screenNameGenerator.generate(TestPropsValues.getCompanyId(),TestPropsValues.getUserId(),null);
  if (PropsValues.USERS_SCREEN_NAME_ALLOW_NUMERIC) {
    Assert.assertEquals(String.valueOf(TestPropsValues.getUserId()),screenName);
  }
 else {
    Assert.assertEquals("user." + TestPropsValues.getUserId(),screenName);
  }
}
