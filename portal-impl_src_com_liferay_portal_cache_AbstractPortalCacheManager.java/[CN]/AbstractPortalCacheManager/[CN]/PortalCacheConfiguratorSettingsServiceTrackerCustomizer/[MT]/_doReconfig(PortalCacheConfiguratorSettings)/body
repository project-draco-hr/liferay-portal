{
  String portalCacheConfigurationLocation=portalCacheConfiguratorSettings.getPortalCacheConfigrationLocation();
  if (Validator.isNull(portalCacheConfigurationLocation)) {
    return false;
  }
  ClassLoader classLoader=portalCacheConfiguratorSettings.getClassLoader();
  URL url=classLoader.getResource(portalCacheConfigurationLocation);
  if (url == null) {
    return false;
  }
  ClassLoader contextClassLoader=ClassLoaderUtil.getContextClassLoader();
  ClassLoaderUtil.setContextClassLoader(AggregateClassLoader.getAggregateClassLoader(ClassLoaderUtil.getPortalClassLoader(),portalCacheConfiguratorSettings.getClassLoader()));
  try {
    if (_log.isInfoEnabled()) {
      _log.info("Reconfiguring caches in cache manager " + _name + " using "+ url);
    }
    reconfigureCaches(url);
  }
  finally {
    ClassLoaderUtil.setContextClassLoader(contextClassLoader);
  }
  return true;
}
