{
  String json=doGet(url,parameters,headers);
  if ((json == null) || json.equals("") || json.equals("{}")|| json.equals("[]")) {
    return Collections.emptyList();
  }
  if (json.contains("exception\":\"")) {
    throw new JSONWebServiceInvocationException(getExceptionMessage(json),getStatus(json));
  }
  try {
    TypeFactory typeFactory=objectMapper.getTypeFactory();
    JavaType javaType=typeFactory.constructCollectionType(List.class,clazz);
    return objectMapper.readValue(json,javaType);
  }
 catch (  IOException ie) {
    throw new JSONWebServiceInvocationException(ie);
  }
}
