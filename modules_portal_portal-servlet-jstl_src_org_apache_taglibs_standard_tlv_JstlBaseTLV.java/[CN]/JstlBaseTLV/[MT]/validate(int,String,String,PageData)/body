{
  try {
    this.tlvType=type;
    this.uri=uri;
    messageVector=new Vector();
    this.prefix=prefix;
    try {
      if (config == null) {
        String expressionAttributes=null;
        Map<String,Object> initParameters=getInitParameters();
        if (initParameters != null) {
          expressionAttributes=(String)initParameters.get(EXP_ATT_PARAM);
        }
        configure(expressionAttributes);
      }
    }
 catch (    NoSuchElementException ex) {
      return vmFromString(Resources.getMessage("TLV_PARAMETER_ERROR",EXP_ATT_PARAM));
    }
    DefaultHandler h=getHandler();
    SAXParserFactory f=SAXParserFactory.newInstance();
    f.setValidating(false);
    f.setNamespaceAware(true);
    SAXParser p=f.newSAXParser();
    p.parse(page.getInputStream(),h);
    if (messageVector.size() == 0)     return null;
 else     return vmFromVector(messageVector);
  }
 catch (  SAXException ex) {
    return vmFromString(ex.toString());
  }
catch (  ParserConfigurationException ex) {
    return vmFromString(ex.toString());
  }
catch (  IOException ex) {
    return vmFromString(ex.toString());
  }
}
