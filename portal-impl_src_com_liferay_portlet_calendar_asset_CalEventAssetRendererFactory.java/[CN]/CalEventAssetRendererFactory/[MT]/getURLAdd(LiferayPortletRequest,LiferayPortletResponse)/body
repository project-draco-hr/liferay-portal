{
  HttpServletRequest request=liferayPortletRequest.getHttpServletRequest();
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(WebKeys.THEME_DISPLAY);
  if (!CalendarPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),ActionKeys.ADD_EVENT)) {
    return null;
  }
  PortletURL portletURL=PortletURLFactoryUtil.create(request,PortletKeys.CALENDAR,PortalUtil.getControlPanelPlid(themeDisplay.getCompanyId()),PortletRequest.RENDER_PHASE);
  portletURL.setParameter("struts_action","/calendar/edit_event");
  return portletURL;
}
