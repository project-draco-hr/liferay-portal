{
  ShoppingItem item=ShoppingItemUtil.findByPrimaryKey(itemId);
  List itemPrices=ShoppingItemPriceUtil.findByItemId(itemId);
  if (itemPrices.size() == 0) {
    itemPrices=new ArrayList();
    itemPrices.add(new ShoppingItemPrice(null,itemId,item.getMinQuantity(),item.getMaxQuantity(),item.getPrice(),item.getDiscount(),item.getTaxable(),item.getShipping(),item.isUseShippingFormula(),ShoppingItemPrice.STATUS_ACTIVE_DEFAULT));
  }
  return itemPrices;
}
