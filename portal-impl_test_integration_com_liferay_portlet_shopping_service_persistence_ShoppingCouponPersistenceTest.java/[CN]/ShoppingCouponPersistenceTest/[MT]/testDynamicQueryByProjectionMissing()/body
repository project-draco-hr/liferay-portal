{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ShoppingCoupon.class,ShoppingCoupon.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("couponId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("couponId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
