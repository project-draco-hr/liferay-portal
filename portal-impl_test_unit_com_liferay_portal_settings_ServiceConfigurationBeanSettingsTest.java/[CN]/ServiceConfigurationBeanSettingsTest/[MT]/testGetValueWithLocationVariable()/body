{
  when(_mockLocationVariableResolver.isLocationVariable(_serviceConfigurationBean.locationVariableValue())).thenReturn(true);
  final String expectedValue="Once upon a time...";
  when(_mockLocationVariableResolver.resolve(_serviceConfigurationBean.locationVariableValue())).thenReturn(expectedValue);
  Assert.assertEquals(expectedValue,_serviceConfigurationBeanSettings.getValue("locationVariableValue","defaultValue"));
}
