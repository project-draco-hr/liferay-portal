{
  User newUser=addUser();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(User.class,User.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("userId",newUser.getUserId()));
  List<User> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(1,result.size());
  User existingUser=result.get(0);
  Assert.assertEquals(existingUser,newUser);
}
