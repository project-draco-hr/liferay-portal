{
  Phone phone=findByPrimaryKey(phoneId);
  Session session=null;
  try {
    session=openSession();
    Phone[] array=new PhoneImpl[3];
    array[0]=getByUserId_PrevAndNext(session,phone,userId,orderByComparator,true);
    array[1]=phone;
    array[2]=getByUserId_PrevAndNext(session,phone,userId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
