{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      selenium.open("/web/guest/home/");
    for (int second=0; ; second++) {
      if (second >= 60) {
        fail("timeout");
      }
      try {
        if (selenium.isElementPresent("link=Control Panel")) {
          break;
        }
      }
 catch (      Exception e) {
      }
      Thread.sleep(1000);
    }
  selenium.saveScreenShotAndSource();
selenium.clickAt("link=Control Panel",RuntimeVariables.replace(""));
selenium.waitForPageToLoad("30000");
selenium.saveScreenShotAndSource();
selenium.clickAt("link=Web Content",RuntimeVariables.replace(""));
selenium.waitForPageToLoad("30000");
selenium.saveScreenShotAndSource();
selenium.clickAt("link=Templates",RuntimeVariables.replace(""));
selenium.waitForPageToLoad("30000");
selenium.saveScreenShotAndSource();
boolean advancedVisible=selenium.isVisible("link=Advanced \u00bb");
if (!advancedVisible) {
label=2;
continue;
}
selenium.clickAt("link=Advanced \u00bb",RuntimeVariables.replace(""));
case 2:
for (int second=0; ; second++) {
if (second >= 60) {
fail("timeout");
}
try {
if (selenium.isVisible("_15_searchTemplateId")) {
break;
}
}
 catch (Exception e) {
}
Thread.sleep(1000);
}
selenium.saveScreenShotAndSource();
selenium.type("_15_searchTemplateId",RuntimeVariables.replace("templateid"));
selenium.saveScreenShotAndSource();
selenium.clickAt("//div[2]/span[2]/span/input",RuntimeVariables.replace("Search"));
selenium.waitForPageToLoad("30000");
selenium.saveScreenShotAndSource();
selenium.type("_15_searchTemplateId",RuntimeVariables.replace(""));
selenium.saveScreenShotAndSource();
selenium.type("_15_searchTemplateId",RuntimeVariables.replace("templateid1"));
selenium.saveScreenShotAndSource();
selenium.clickAt("//div[2]/span[2]/span/input",RuntimeVariables.replace("Search"));
selenium.waitForPageToLoad("30000");
selenium.saveScreenShotAndSource();
selenium.type("_15_searchTemplateId",RuntimeVariables.replace(""));
selenium.saveScreenShotAndSource();
assertFalse(selenium.isElementPresent("link=TEMPLATEID"));
selenium.clickAt("link=\u00ab Basic",RuntimeVariables.replace(""));
case 100:
label=-1;
}
}
}
