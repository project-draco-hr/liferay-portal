{
  Dictionary<String,Object> properties=configuration.getProperties();
  if (properties == null) {
    properties=new HashMapDictionary<>();
  }
  LDAPServerConfiguration ldapServerConfiguration=Configurable.createConfigurable(getMetatype(),properties);
synchronized (_configurations) {
    Map<Long,Configuration> configurations=_configurations.get(ldapServerConfiguration.companyId());
    if (!MapUtil.isEmpty(configurations)) {
      configurations.remove(ldapServerConfiguration.ldapServerId());
    }
  }
}
