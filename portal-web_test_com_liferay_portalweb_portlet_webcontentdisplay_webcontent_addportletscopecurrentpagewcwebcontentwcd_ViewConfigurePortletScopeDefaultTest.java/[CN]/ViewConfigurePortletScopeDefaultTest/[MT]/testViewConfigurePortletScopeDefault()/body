{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Display Test Page1")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Web Content Display Test Page1",RuntimeVariables.replace("Web Content Display Test Page1"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.clickAt("//strong/a",RuntimeVariables.replace("Options"));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Configuration"),selenium.getText("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a"));
  selenium.click("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Scope")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Scope",RuntimeVariables.replace("Scope"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertTrue(selenium.isElementPresent("//select[@id='_86_scopeType']"));
  assertEquals("Default",selenium.getSelectedLabel("//select[@id='_86_scopeType']"));
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Web Content Display Test Page1")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Web Content Display Test Page1",RuntimeVariables.replace("Web Content Display Test Page1"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//section")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Web Content Display"),selenium.getText("//span[@class='portlet-title-text']"));
}
