{
  Map<String,String> parameters=null;
  try {
    parameters=(Map<String,String>)session.getAttribute(KEY);
    if (parameters == null) {
      parameters=new HashMap<String,String>();
      session.setAttribute(KEY,parameters);
    }
  }
 catch (  IllegalStateException ise) {
    parameters=new HashMap<String,String>();
  }
  return parameters;
}
