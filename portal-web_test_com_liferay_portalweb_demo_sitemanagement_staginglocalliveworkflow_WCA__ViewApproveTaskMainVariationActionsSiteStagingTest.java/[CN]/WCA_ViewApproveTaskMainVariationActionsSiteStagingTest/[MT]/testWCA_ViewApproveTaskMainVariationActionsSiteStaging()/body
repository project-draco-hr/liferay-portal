{
  selenium.open("/web/community-site-test/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//section")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertTrue(selenium.isVisible("//section"));
  assertFalse(selenium.isTextPresent("This is a Web Content article"));
}
