{
  String emailMessageAddedEnabled=ParamUtil.getString(req,"emailMessageAddedEnabled");
  String emailMessageAddedSubject=ParamUtil.getString(req,"emailMessageAddedSubject");
  String emailMessageAddedBody=ParamUtil.getString(req,"emailMessageAddedBody");
  if (Validator.isNull(emailMessageAddedSubject)) {
    SessionErrors.add(req,"emailMessageAddedSubject");
  }
 else   if (Validator.isNull(emailMessageAddedBody)) {
    SessionErrors.add(req,"emailMessageAddedBody");
  }
 else {
    prefs.setValue("email-message-added-enabled",emailMessageAddedEnabled);
    prefs.setValue("email-message-added-subject",emailMessageAddedSubject);
    prefs.setValue("email-message-added-body",emailMessageAddedBody);
  }
}
