{
  temporaryFolder.create();
  _headers=new Hashtable<>();
  _headers.put("Bundle-SymbolicName","com.liferay.portal.configuration.extender.test");
  _headers.put("ConfigurationPath","/configs");
  temporaryFolder.newFolder("configs");
  _file=temporaryFolder.newFile("/configs/com.liferay.test.aConfigFile");
  _writeToFile(_file,"key=value\nanotherKey=anotherValue");
}
