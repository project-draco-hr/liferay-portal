{
  String[][] originalRenamedSystemProperty=ReflectionTestUtil.getFieldValue(VerifyProperties.class,"_RENAMED_SYSTEM_KEYS");
  String renamedSystemPropertyKey=getFirstExistSystemPropertyName();
  ReflectionTestUtil.setFieldValue(VerifyProperties.class,"_RENAMED_SYSTEM_KEYS",new String[][]{new String[]{renamedSystemPropertyKey,renamedSystemPropertyKey}});
  try (CaptureAppender captureAppender=Log4JLoggerTestUtil.configureLog4JLogger(VerifyProperties.class.getName(),Level.ERROR)){
    doVerify();
    List<LoggingEvent> loggingEvents=captureAppender.getLoggingEvents();
    LoggingEvent loggingEvent=loggingEvents.get(0);
    Assert.assertEquals("System property \"" + renamedSystemPropertyKey + "\" was renamed to \""+ renamedSystemPropertyKey+ "\"",loggingEvent.getMessage());
  }
  finally {
    ReflectionTestUtil.setFieldValue(VerifyProperties.class,"_RENAMED_SYSTEM_KEYS",originalRenamedSystemProperty);
  }
}
