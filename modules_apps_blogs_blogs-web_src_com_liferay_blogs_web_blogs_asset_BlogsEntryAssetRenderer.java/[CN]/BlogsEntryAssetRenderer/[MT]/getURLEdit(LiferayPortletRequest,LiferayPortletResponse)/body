{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),BlogsPortletKeys.BLOGS,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcRenderCommandName","/blogs/edit_entry");
  portletURL.setParameter("entryId",String.valueOf(_entry.getEntryId()));
  return portletURL;
}
