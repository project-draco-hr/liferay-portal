{
  System.out.println("Starting test: " + algorithm);
  long startTime=System.currentTimeMillis();
  String actual=PasswordEncryptorUtil.encrypt(algorithm,password,encrypted);
  System.out.println("Hash [algorithm, time, result]: [" + algorithm + ", "+ (System.currentTimeMillis() - startTime)+ "ms , "+ actual+ "]");
  Assert.assertEquals(encrypted,actual);
  return actual;
}
