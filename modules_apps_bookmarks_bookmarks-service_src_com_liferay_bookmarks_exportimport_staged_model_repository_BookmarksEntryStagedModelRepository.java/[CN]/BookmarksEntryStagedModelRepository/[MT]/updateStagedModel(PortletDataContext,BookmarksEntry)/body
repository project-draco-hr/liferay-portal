{
  long userId=portletDataContext.getUserId(bookmarksEntry.getUserUuid());
  ServiceContext serviceContext=portletDataContext.createServiceContext(bookmarksEntry);
  return BookmarksEntryLocalServiceUtil.updateEntry(userId,bookmarksEntry.getEntryId(),bookmarksEntry.getGroupId(),bookmarksEntry.getFolderId(),bookmarksEntry.getName(),bookmarksEntry.getUrl(),bookmarksEntry.getDescription(),serviceContext);
}
