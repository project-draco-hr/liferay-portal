{
  String updateDLFileEntry="update DLSyncEvent " + "set companyId = (select dlfe.companyId " + "from DLFileEntry dlfe "+ "where DLSyncEvent.type_='file' and "+ "dlfe.fileEntryId = DLSyncEvent.typePK);";
  runSQL(updateDLFileEntry);
  String updateDLFolder="update DLSyncEvent " + "set companyId = (select dlf.companyId " + "from DLFolder dlf "+ "where DLSyncEvent.type_='folder' and "+ "dlf.folderId = DLSyncEvent.typePK);";
  runSQL(updateDLFolder);
}
