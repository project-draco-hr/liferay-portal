{
  String selectSQL="select companyId from Company where Company.companyId = " + "PortletPreferences.ownerId";
  runSQL(getUpdateSQL(selectSQL));
  selectSQL="select companyId from Group_ where Group_.organizationId = " + "PortletPreferences.ownerId";
  runSQL(getUpdateSQL(selectSQL));
