  runSQL(getUpdateSQL(selectSQL));
  selectSQL="select companyId from Organization_ where " + "Organization_.organizationId = PortletPreferences.ownerId";
  runSQL(getUpdateSQL(selectSQL));
  selectSQL="select companyId from User_ where User_.userId = " + "PortletPreferences.ownerId";
  runSQL(getUpdateSQL(selectSQL));
}
