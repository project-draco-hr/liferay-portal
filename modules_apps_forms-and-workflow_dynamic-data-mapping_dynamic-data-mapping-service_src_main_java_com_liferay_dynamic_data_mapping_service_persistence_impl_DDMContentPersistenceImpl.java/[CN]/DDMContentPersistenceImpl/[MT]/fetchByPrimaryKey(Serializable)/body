{
  Serializable serializable=entityCache.getResult(DDMContentModelImpl.ENTITY_CACHE_ENABLED,DDMContentImpl.class,primaryKey);
  if (serializable == nullModel) {
    return null;
  }
  DDMContent ddmContent=(DDMContent)serializable;
  if (ddmContent == null) {
    Session session=null;
    try {
      session=openSession();
      ddmContent=(DDMContent)session.get(DDMContentImpl.class,primaryKey);
      if (ddmContent != null) {
        cacheResult(ddmContent);
      }
 else {
        entityCache.putResult(DDMContentModelImpl.ENTITY_CACHE_ENABLED,DDMContentImpl.class,primaryKey,nullModel);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(DDMContentModelImpl.ENTITY_CACHE_ENABLED,DDMContentImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return ddmContent;
}
