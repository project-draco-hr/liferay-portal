{
  User user=(User)obj;
  try {
    if (_actionIds != null) {
      for (int i=0; i < _actionIds.length; i++) {
        if (!PermissionLocalServiceUtil.hasUserPermission(user.getUserId(),_actionIds[i],_resourceId)) {
          return false;
        }
      }
      return true;
    }
 else {
      return PermissionLocalServiceUtil.hasUserPermission(user.getUserId(),_actionId,_resourceId);
    }
  }
 catch (  Exception e) {
    _log.error(e);
    return false;
  }
}
