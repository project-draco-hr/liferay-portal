{
  long currentTime=System.currentTimeMillis();
synchronized (this) {
    if (currentTime > _nextCleanUpTime) {
      try {
        doCleanUp();
      }
 catch (      ChannelException ce) {
        throw ce;
      }
catch (      Exception e) {
        throw new ChannelException(e);
      }
    }
    _nextCleanUpTime=currentTime + _cleanUpInterval;
  }
}
