{
  long currentTime=System.currentTimeMillis();
  long nextCleanUpTime=_nextCleanUpTime.get();
  if ((currentTime > nextCleanUpTime) && _nextCleanUpTime.compareAndSet(nextCleanUpTime,currentTime + _cleanUpInterval)) {
    try {
      doCleanUp();
    }
 catch (    ChannelException ce) {
      throw ce;
    }
catch (    Exception e) {
      throw new ChannelException(e);
    }
  }
}
