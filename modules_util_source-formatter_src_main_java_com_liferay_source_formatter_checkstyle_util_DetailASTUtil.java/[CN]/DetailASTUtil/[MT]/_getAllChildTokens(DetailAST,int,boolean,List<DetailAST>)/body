{
  if (list == null) {
    list=new ArrayList<>();
  }
  DetailAST childAST=detailAST.getFirstChild();
  while (childAST != null) {
    if (childAST.getType() == tokenType) {
      list.add(childAST);
    }
    if (recursive) {
      list=_getAllChildTokens(childAST,tokenType,recursive,list);
    }
    childAST=childAST.getNextSibling();
  }
  return list;
}
