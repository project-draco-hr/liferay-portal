{
  List<DetailAST> list=new ArrayList<>();
  if ((detailAST.getType() != TokenTypes.CTOR_DEF) && (detailAST.getType() != TokenTypes.METHOD_DEF)) {
    return list;
  }
  DetailAST parametersAST=detailAST.findFirstToken(TokenTypes.PARAMETERS);
  return getAllChildTokens(parametersAST,TokenTypes.PARAMETER_DEF,false);
}
