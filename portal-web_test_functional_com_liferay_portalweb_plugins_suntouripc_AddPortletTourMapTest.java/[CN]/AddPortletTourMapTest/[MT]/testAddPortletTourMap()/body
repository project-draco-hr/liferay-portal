{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Sun Tour IPC Test Page");
  selenium.clickAt("link=Sun Tour IPC Test Page",RuntimeVariables.replace("Sun Tour IPC Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//a[@id='_145_addApplication']","More"));
  selenium.clickAt("//a[@id='_145_addApplication']",RuntimeVariables.replace("More"));
  selenium.waitForElementPresent("//li[@title='Tour Map']/p/a");
  selenium.clickAt("//li[@title='Tour Map']/p/a",RuntimeVariables.replace("Add"));
  selenium.waitForVisible("//section");
  assertTrue(selenium.isVisible("//section"));
  selenium.waitForVisible("//div[2]/div/section");
  assertTrue(selenium.isVisible("//div[2]/div/section"));
  selenium.waitForVisible("//div[3]/div/section");
  assertTrue(selenium.isVisible("//div[3]/div/section"));
}
