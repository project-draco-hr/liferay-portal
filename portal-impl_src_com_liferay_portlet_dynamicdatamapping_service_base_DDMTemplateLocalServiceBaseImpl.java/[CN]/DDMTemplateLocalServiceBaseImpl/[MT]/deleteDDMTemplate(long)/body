{
  DDMTemplate ddmTemplate=ddmTemplatePersistence.remove(templateId);
  Indexer indexer=IndexerRegistryUtil.getIndexer(getModelClassName());
  if (indexer != null) {
    try {
      indexer.delete(ddmTemplate);
    }
 catch (    SearchException se) {
      if (_log.isWarnEnabled()) {
        _log.warn(se,se);
      }
    }
  }
}
