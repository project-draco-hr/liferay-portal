{
  List<String> badURLs=new ArrayList<String>();
  String[] urls=preferences.getValues("urls",new String[0]);
  for (  String url : urls) {
    if (!Validator.isUrl(url)) {
      badURLs.add(url);
    }
  }
  if (!badURLs.isEmpty()) {
    throw new ValidatorException("Failed to retrieve URLs",badURLs);
  }
}
