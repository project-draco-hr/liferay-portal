{
  Map<String,Object> map=MapUtil.toLinkedHashMap(new String[]{"one:1:long"});
  Assert.assertTrue(map.size() == 1);
  Assert.assertTrue(map.containsKey("one"));
  Assert.assertTrue(map.containsValue(1l));
  Assert.assertTrue(map.get("one") instanceof Long);
  map=MapUtil.toLinkedHashMap(new String[]{"one:1:" + Long.class.getName()});
  Assert.assertTrue(map.size() == 1);
  Assert.assertTrue(map.containsKey("one"));
  Assert.assertTrue(map.containsValue(1l));
  Assert.assertTrue(map.get("one") instanceof Long);
}
