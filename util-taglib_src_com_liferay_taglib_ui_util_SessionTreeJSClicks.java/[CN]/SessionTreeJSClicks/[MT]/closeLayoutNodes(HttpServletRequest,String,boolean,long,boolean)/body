{
  try {
    List<String> layoutIds=new ArrayList<>();
    layoutIds.add(String.valueOf(layoutId));
    if (recursive) {
      getLayoutIds(request,privateLayout,layoutId,layoutIds);
    }
    closeNodes(request,treeId,layoutIds.toArray(new String[layoutIds.size()]));
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
