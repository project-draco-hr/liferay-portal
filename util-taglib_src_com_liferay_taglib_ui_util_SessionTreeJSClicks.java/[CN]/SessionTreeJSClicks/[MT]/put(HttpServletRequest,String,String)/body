{
  try {
    PortalPreferences preferences=PortletPreferencesFactoryUtil.getPortalPreferences(request);
    preferences.setValue(SessionTreeJSClicks.class.getName(),key,value);
  }
 catch (  Exception e) {
    if (e instanceof ConcurrentModificationException) {
      throw (ConcurrentModificationException)e;
    }
    _log.error(e,e);
  }
}
