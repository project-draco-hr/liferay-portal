{
  while (true) {
    try {
      String openNodesString=StringPool.BLANK;
      put(request,treeId,openNodesString,PortletPreferencesFactoryUtil.getPortalPreferences(request));
      return;
    }
 catch (    ConcurrentModificationException cme) {
      continue;
    }
catch (    Exception e) {
      _log.error(e,e);
      return;
    }
  }
}
