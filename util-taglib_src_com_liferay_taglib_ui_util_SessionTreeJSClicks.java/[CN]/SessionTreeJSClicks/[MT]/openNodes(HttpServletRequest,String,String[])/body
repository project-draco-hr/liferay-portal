{
  while (true) {
    try {
      String openNodesString=get(request,treeId);
      for (      String nodeId : nodeIds) {
        openNodesString=StringUtil.add(openNodesString,nodeId);
      }
      put(request,treeId,openNodesString);
      return;
    }
 catch (    ConcurrentModificationException cme) {
      continue;
    }
catch (    Exception e) {
      _log.error(e,e);
      return;
    }
  }
}
