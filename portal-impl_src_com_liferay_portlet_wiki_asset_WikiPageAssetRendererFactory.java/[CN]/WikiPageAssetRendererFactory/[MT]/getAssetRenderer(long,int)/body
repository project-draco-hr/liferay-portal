{
  WikiPage page;
  if (type == TYPE_LATEST_APPROVED) {
    page=WikiPageLocalServiceUtil.getPage(classPK);
  }
 else {
    WikiPageResource wikiPageResource=WikiPageResourceLocalServiceUtil.getPageResource(classPK);
    page=WikiPageLocalServiceUtil.getPage(wikiPageResource.getNodeId(),wikiPageResource.getTitle(),null);
  }
  return new WikiPageAssetRenderer(page);
}
