{
  WikiPage page=null;
  long resourcePrimKey=classPK;
  if (type == TYPE_LATEST_APPROVED) {
    page=WikiPageLocalServiceUtil.getPage(resourcePrimKey);
  }
 else {
    WikiPageResource wikiPageResource=WikiPageResourceLocalServiceUtil.getPageResource(resourcePrimKey);
    page=WikiPageLocalServiceUtil.getPage(wikiPageResource.getNodeId(),wikiPageResource.getTitle(),null);
  }
  return new WikiPageAssetRenderer(page);
}
