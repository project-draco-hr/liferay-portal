{
  if (configJSModulesTask.getOutputFile() != null) {
    return;
  }
  SourceSet sourceSet=GradleUtil.getSourceSet(configJSModulesTask.getProject(),SourceSet.MAIN_SOURCE_SET_NAME);
  SourceSetOutput sourceSetOutput=sourceSet.getOutput();
  File outputFile=new File(sourceSetOutput.getResourcesDir(),"META-INF/config.json");
  configJSModulesTask.setOutputFile(outputFile);
}
