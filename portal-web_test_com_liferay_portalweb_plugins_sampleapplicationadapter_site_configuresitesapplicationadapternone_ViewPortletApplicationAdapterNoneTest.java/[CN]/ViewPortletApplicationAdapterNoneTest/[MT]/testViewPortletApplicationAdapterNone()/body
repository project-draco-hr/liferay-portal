{
  selenium.open("/web/community-name/public-page");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Public Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Public Page",RuntimeVariables.replace("Public Page"));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//section")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertTrue(selenium.isVisible("//section"));
  assertEquals(RuntimeVariables.replace("Liferay"),selenium.getText("//div/div/div/div/ul/li/span/a"));
  assertEquals(RuntimeVariables.replace("Community Name"),selenium.getText("//div/ul/li[2]/span/a"));
  assertEquals(RuntimeVariables.replace("Public Page"),selenium.getText("//div/div/div/div/ul/li[3]/span/a"));
}
