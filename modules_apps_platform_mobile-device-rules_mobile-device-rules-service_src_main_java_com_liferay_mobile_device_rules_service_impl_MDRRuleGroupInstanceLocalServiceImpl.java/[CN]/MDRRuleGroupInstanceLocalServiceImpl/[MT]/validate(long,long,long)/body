{
  MDRRuleGroupInstance ruleGroupInstance=mdrRuleGroupInstancePersistence.fetchByC_C_R(classNameId,classPK,ruleGroupId);
  if (ruleGroupInstance != null) {
    StringBundler sb=new StringBundler(7);
    sb.append("{classNameId=");
    sb.append(classNameId);
    sb.append(", classPK=");
    sb.append(classPK);
    sb.append(", ruleGroupId=");
    sb.append(ruleGroupId);
    sb.append("}");
    throw new DuplicateRuleGroupInstanceException(sb.toString());
  }
  mdrRuleGroupLocalService.getMDRRuleGroup(ruleGroupId);
}
