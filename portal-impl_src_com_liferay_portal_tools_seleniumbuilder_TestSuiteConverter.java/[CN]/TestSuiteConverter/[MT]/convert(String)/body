{
  Map<String,Object> context=getContext();
  context.put("testSuiteName",testSuiteName);
  String content=processTemplate("test_suite.ftl",context);
  seleniumBuilderFileUtil.writeFile(seleniumBuilderContext.getTestSuiteJavaFileName(testSuiteName),content,true);
  String contentHTML=processTemplate("test_suite_html.ftl",context);
  seleniumBuilderFileUtil.writeFile(seleniumBuilderContext.getTestSuiteHTMLFileName(testSuiteName),contentHTML,false);
}
