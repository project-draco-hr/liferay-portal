{
  try {
    long companyId=PortalUtil.getCompanyId(request);
    OpenSSOConfiguration openSSOConfiguration=getOpenSSOConfiguration(companyId);
    if (openSSOConfiguration.enabled() && Validator.isNotNull(openSSOConfiguration.loginURL()) && Validator.isNotNull(openSSOConfiguration.logoutURL())&& Validator.isNotNull(openSSOConfiguration.serviceURL())) {
      return true;
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return false;
}
