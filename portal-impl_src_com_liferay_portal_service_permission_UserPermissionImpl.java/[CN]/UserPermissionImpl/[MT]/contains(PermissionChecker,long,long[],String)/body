{
  PermissionCheckerImpl permissionCheckerImpl=(PermissionCheckerImpl)permissionChecker;
  String organizationActionId=actionId + "_USER";
  if (actionId.equals(ActionKeys.ADD_USER)) {
    organizationActionId=actionId;
  }
  if (permissionCheckerImpl.getUserId() == userId) {
    return true;
  }
 else   if (permissionChecker.hasPermission(0,User.class.getName(),userId,actionId)) {
    return true;
  }
 else {
    if (organizationIds == null) {
      try {
        User user=UserLocalServiceUtil.getUserById(userId);
        organizationIds=user.getOrganizationIds();
      }
 catch (      Exception e) {
        _log.warn(e);
      }
    }
    if ((organizationIds != null) && (organizationIds.length > 0) && (hasOrganizationPermission(permissionChecker,organizationIds,organizationActionId))) {
      return true;
    }
  }
  return false;
}
