{
  Assert.assertNull(_htmlImpl.stripBetween(null,"test"));
  Assert.assertEquals("test--test",_htmlImpl.stripBetween("test-<honk>thiswillbestripped</honk>-test","honk"));
  Assert.assertEquals("test--test",_htmlImpl.stripBetween("test-<honk attribute=\"value\">thiswillbestripped</honk>-test","honk"));
  Assert.assertEquals("test-test-test",_htmlImpl.stripBetween("test-test-test","test"));
  Assert.assertEquals("works across  lines",_htmlImpl.stripBetween("works across <honk>\r\n a number of </honk> lines","honk"));
  Assert.assertEquals("multiple occurrences, multiple indeed",_htmlImpl.stripBetween("multiple <a>many</a>occurrences, multiple <a>HONK</a>indeed","a"));
  Assert.assertEquals("self-closing <test/> is unhandled",_htmlImpl.stripBetween("self-closing <test/> is unhandled","test"));
  Assert.assertEquals("self-closing <test /> is unhandled",_htmlImpl.stripBetween("self-closing <test /> is unhandled","test"));
}
