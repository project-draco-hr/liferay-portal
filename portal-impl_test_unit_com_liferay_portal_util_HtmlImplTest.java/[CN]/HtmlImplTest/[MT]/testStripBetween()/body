{
  assertNull(htmlImpl.stripBetween(null,"test"));
  assertEquals("test--test",htmlImpl.stripBetween("test-<honk>thiswillbestripped</honk>-test","honk"));
  assertEquals("test--test",htmlImpl.stripBetween("test-<honk attribute=\"value\">thiswillbestripped</honk>-test","honk"));
  assertEquals("test-test-test",htmlImpl.stripBetween("test-test-test","test"));
  assertEquals("works across  lines",htmlImpl.stripBetween("works across <honk>\r\n a number of </honk> lines","honk"));
  assertEquals("test-test-test",htmlImpl.stripBetween("test-test-test","test"));
  assertEquals("multiple occurrences, multiple indeed",htmlImpl.stripBetween("multiple <a>many</a>occurrences, multiple <a>HONK</a>indeed","a"));
  assertEquals("self-closing <test/> is unhandled",htmlImpl.stripBetween("self-closing <test/> is unhandled","test"));
  assertEquals("self-closing <test /> is unhandled",htmlImpl.stripBetween("self-closing <test /> is unhandled","test"));
}
