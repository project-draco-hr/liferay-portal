{
  assertEquals("&lt;",_htmlImpl.escape("<"));
  assertEquals("&gt;",_htmlImpl.escape(">"));
  assertEquals("&lt;script&gt;",_htmlImpl.escape("<script>"));
  assertEquals("&amp;",_htmlImpl.escape("&"));
  assertEquals("You &amp; Me",_htmlImpl.escape("You & Me"));
  assertEquals("&lt;span class=&#034;test&#034;&gt;Test&lt;/span&gt;",_htmlImpl.escape("<span class=\"test\">Test</span>"));
  assertEquals("I&#039;m quoting: &#034;this is a quote&#034;",_htmlImpl.escape("I'm quoting: \"this is a quote\""));
}
