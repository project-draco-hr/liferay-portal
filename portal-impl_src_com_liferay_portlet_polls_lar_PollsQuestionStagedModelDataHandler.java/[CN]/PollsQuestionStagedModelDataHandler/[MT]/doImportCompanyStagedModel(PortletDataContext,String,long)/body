{
  PollsQuestion existingQuestion=PollsQuestionLocalServiceUtil.fetchPollsQuestionByUuidAndGroupId(uuid,portletDataContext.getCompanyGroupId());
  Map<Long,Long> questionIds=(Map<Long,Long>)portletDataContext.getNewPrimaryKeysMap(PollsQuestion.class);
  questionIds.put(questionId,existingQuestion.getQuestionId());
}
