{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Image Gallery Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Image Gallery Permissions Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Portlet2 Temporary2 Folder2",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//div[2]/a/img",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//img[@alt='Permissions']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//img[@alt='Permissions']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.check("//tr[7]/td[4]/input");
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//section/div/div/div/div[1]"));
  assertTrue(selenium.isChecked("//tr[7]/td[4]/input"));
}
