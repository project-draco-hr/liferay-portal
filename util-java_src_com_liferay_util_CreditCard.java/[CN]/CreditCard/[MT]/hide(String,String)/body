{
  if (number == null) {
    return number;
  }
  int numberLen=number.length();
  if (numberLen > 4) {
    StringBundler sb=new StringBundler(numberLen - 3);
    for (int i=0; i < numberLen - 4; i++) {
      sb.append(x);
    }
    sb.append(number.substring(numberLen - 4,numberLen));
    number=sb.toString();
  }
  return number;
}
