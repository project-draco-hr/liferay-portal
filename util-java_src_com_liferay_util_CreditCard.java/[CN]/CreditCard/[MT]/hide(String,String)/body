{
  if (number == null) {
    return number;
  }
  int numberLen=number.length();
  if (numberLen > 4) {
    StringMaker sm=new StringMaker();
    for (int i=0; i < numberLen - 4; i++) {
      sm.append(x);
    }
    sm.append(number.substring(numberLen - 4,numberLen));
    number=sm.toString();
  }
  return number;
}
