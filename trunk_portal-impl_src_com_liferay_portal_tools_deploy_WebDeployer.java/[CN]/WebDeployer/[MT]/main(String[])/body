{
  InitUtil.initWithSpring();
  List<String> wars=new ArrayList<String>();
  List<String> jars=new ArrayList<String>();
  for (  String arg : args) {
    if (arg.endsWith(".war")) {
      wars.add(arg);
    }
 else     if (arg.endsWith(".jar")) {
      jars.add(arg);
    }
  }
  new WebDeployer(wars,jars);
}
