{
  SAPConfiguration sapConfiguration=ConfigurationFactoryUtil.getConfiguration(SAPConfiguration.class,new CompanyServiceSettingsLocator(getCompanyId(),SAPConstants.SERVICE_NAME));
  if (getName().equals(sapConfiguration.systemDefaultSAPEntryName())) {
    return true;
  }
  if (getName().equals(sapConfiguration.systemUserPasswordSAPEntryName())) {
    return true;
  }
  return false;
}
