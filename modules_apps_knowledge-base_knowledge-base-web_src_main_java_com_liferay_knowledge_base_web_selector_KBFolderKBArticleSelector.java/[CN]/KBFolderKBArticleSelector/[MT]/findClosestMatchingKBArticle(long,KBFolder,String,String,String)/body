{
  KBFolder kbFolder=getCandidateKBFolder(groupId,preferredKBFolderUrlTitle,ancestorKBFolder,kbFolderUrlTitle);
  KBArticle kbArticle=_kbArticleLocalService.fetchKBArticleByUrlTitle(groupId,kbFolder.getKbFolderId(),urlTitle);
  if (kbArticle != null) {
    return new KBArticleSelection(kbArticle,false);
  }
  kbArticle=_kbArticleLocalService.fetchFirstChildKBArticle(groupId,kbFolder.getKbFolderId());
  String[] keywords=StringUtil.split(urlTitle,'-');
  return new KBArticleSelection(kbArticle,keywords);
}
