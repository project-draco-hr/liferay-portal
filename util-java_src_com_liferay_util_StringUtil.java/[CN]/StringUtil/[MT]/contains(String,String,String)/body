{
  if ((s == null) || (text == null) || (delimiter == null)) {
    return false;
  }
  if (!s.endsWith(delimiter)) {
    s+=delimiter;
  }
  int pos=s.indexOf(delimiter + text + delimiter);
  if (pos == -1) {
    if (s.startsWith(text + delimiter)) {
      return true;
    }
    return false;
  }
  return true;
}
