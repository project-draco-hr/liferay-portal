{
  if ((s == null) || (text == null) || (delimiter == null)) {
    return false;
  }
  StringBuffer sb=null;
  if (!s.endsWith(delimiter)) {
    sb=new StringBuffer();
    sb.append(s);
    sb.append(delimiter);
    s=sb.toString();
  }
  sb=new StringBuffer();
  sb.append(delimiter);
  sb.append(text);
  sb.append(delimiter);
  String dtd=sb.toString();
  int pos=s.indexOf(dtd);
  if (pos == -1) {
    sb=new StringBuffer();
    sb.append(text);
    sb.append(delimiter);
    String td=sb.toString();
    if (s.startsWith(td)) {
      return true;
    }
    return false;
  }
  return true;
}
