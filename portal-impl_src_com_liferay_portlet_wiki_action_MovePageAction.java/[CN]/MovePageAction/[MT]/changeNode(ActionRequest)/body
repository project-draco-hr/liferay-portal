{
  long nodeId=ParamUtil.getLong(actionRequest,"nodeId");
  String title=ParamUtil.getString(actionRequest,"title");
  long newNodeId=ParamUtil.getLong(actionRequest,"newNodeId");
  ServiceContext serviceContext=ServiceContextFactory.getInstance(WikiPage.class.getName(),actionRequest);
  WikiPageServiceUtil.changeNode(nodeId,title,newNodeId,serviceContext);
}
