{
  LocalizedValue localizedValue=new LocalizedValue();
  if (Validator.isNull(serializedDDMFormFieldProperty)) {
    return localizedValue;
  }
  JSONObject jsonObject=JSONFactoryUtil.createJSONObject(serializedDDMFormFieldProperty);
  Iterator<String> itr=jsonObject.keys();
  while (itr.hasNext()) {
    String languageId=itr.next();
    localizedValue.addString(LocaleUtil.fromLanguageId(languageId),jsonObject.getString(languageId));
  }
  return localizedValue;
}
