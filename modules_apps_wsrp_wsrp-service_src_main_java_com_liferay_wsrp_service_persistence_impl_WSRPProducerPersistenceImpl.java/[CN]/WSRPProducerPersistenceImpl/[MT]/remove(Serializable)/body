{
  Session session=null;
  try {
    session=openSession();
    WSRPProducer wsrpProducer=(WSRPProducer)session.get(WSRPProducerImpl.class,primaryKey);
    if (wsrpProducer == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchProducerException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(wsrpProducer);
  }
 catch (  NoSuchProducerException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
