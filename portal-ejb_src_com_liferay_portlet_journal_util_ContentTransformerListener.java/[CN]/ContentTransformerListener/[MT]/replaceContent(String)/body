{
  try {
    SAXReader reader=new SAXReader();
    Document doc=reader.read(new StringReader(xml));
    Element root=doc.getRootElement();
    replaceContent(root);
    xml=JournalUtil.formatXML(doc);
  }
 catch (  Exception e) {
    _log.error(StackTraceUtil.getStackTrace(e));
  }
  return xml;
}
