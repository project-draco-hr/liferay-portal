{
  Indexer indexer=IndexerRegistryUtil.nullSafeGetIndexer(AssetVocabulary.class);
  for (int i=0; i < 10; i++) {
    Hits hits=indexer.search(searchContext,AssetVocabularyUtil.SELECTED_FIELD_NAMES);
    List<AssetVocabulary> vocabularies=AssetVocabularyUtil.getVocabularies(hits);
    if (vocabularies != null) {
      return new BaseModelSearchResult<AssetVocabulary>(vocabularies,hits.getLength());
    }
  }
  throw new SearchException("Unable to fix the search index after 10 attempts");
}
