{
  List<AssetVocabulary> vocabularies=assetVocabularyPersistence.findByGroupId(groupId);
  if (vocabularies.isEmpty()) {
    Group group=groupLocalService.getGroup(groupId);
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setScopeGroupId(groupId);
    addVocabulary(userLocalService.getDefaultUserId(group.getCompanyId()),PropsValues.ASSET_VOCABULARY_DEFAULT,serviceContext);
  }
  return vocabularies;
}
