{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Knowledge Base Article Test Page");
  selenium.clickAt("link=Knowledge Base Article Test Page",RuntimeVariables.replace("Knowledge Base Article Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Knowledge Base Article 1"),selenium.getText("//div[@class='kb-title']"));
  assertEquals(RuntimeVariables.replace("This is the content of Article 1"),selenium.getText("//div[@class='kb-entity-body']/p"));
}
