{
  LayoutSet layoutSet=findByPrimaryKey(layoutSetId);
  Session session=null;
  try {
    session=openSession();
    LayoutSet[] array=new LayoutSetImpl[3];
    array[0]=getByGroupId_PrevAndNext(session,layoutSet,groupId,orderByComparator,true);
    array[1]=layoutSet;
    array[2]=getByGroupId_PrevAndNext(session,layoutSet,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
