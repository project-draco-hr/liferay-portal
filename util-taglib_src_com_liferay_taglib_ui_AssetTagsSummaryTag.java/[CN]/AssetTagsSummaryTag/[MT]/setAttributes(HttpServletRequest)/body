{
  List<AssetTag> assetTags=new ArrayList<>();
  AssetTagsAvailableTag<R> assetTagsAvailableTag=(AssetTagsAvailableTag<R>)findAncestorWithClass(this,AssetTagsAvailableTag.class);
  if (assetTagsAvailableTag != null) {
    assetTags=assetTagsAvailableTag.getAssetTags();
  }
  request.setAttribute("liferay-ui:asset-categories-summary:assetTags",assetTags);
  request.setAttribute("liferay-ui:asset-tags-summary:assetTagNames",_assetTagNames);
  request.setAttribute("liferay-ui:asset-tags-summary:className",_className);
  request.setAttribute("liferay-ui:asset-tags-summary:classPK",String.valueOf(_classPK));
  request.setAttribute("liferay-ui:asset-tags-summary:message",_message);
  request.setAttribute("liferay-ui:asset-tags-summary:paramName",_paramName);
  request.setAttribute("liferay-ui:asset-tags-summary:portletURL",_portletURL);
}
