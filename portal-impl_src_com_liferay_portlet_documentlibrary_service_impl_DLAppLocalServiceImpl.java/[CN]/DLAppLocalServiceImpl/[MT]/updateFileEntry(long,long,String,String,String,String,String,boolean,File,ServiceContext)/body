{
  InputStream is=null;
  try {
    long size=0;
    if ((file != null) && file.exists()) {
      is=new FileInputStream(file);
      size=file.length();
    }
    return updateFileEntry(userId,fileEntryId,sourceFileName,mimeType,title,description,changeLog,majorVersion,is,size,serviceContext);
  }
 catch (  FileNotFoundException fnfe) {
    throw new NoSuchFileException();
  }
 finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
