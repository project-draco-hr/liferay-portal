{
  LocalRepository localRepository=getLocalRepository(0,fileEntryId,0);
  final FileEntry fileEntry=localRepository.updateFileEntry(userId,fileEntryId,sourceFileName,mimeType,title,description,changeLog,majorVersion,is,size,serviceContext);
  TransactionCommitCallbackUtil.registerCallback(new Callable<Void>(){
    public Void call() throws Exception {
      DLProcessorRegistryUtil.trigger(fileEntry);
      return null;
    }
  }
);
  return fileEntry;
}
