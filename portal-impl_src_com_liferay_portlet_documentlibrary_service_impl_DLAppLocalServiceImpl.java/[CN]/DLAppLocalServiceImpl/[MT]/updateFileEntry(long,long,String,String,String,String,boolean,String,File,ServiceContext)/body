{
  try {
    InputStream is=null;
    long size=0;
    if ((file != null) && file.exists()) {
      is=new UnsyncBufferedInputStream(new FileInputStream(file));
      size=file.length();
    }
    return dlRepositoryLocalService.updateFileEntry(userId,fileEntryId,sourceFileName,title,description,changeLog,majorVersion,extraSettings,is,size,serviceContext);
  }
 catch (  FileNotFoundException fnfe) {
    throw new NoSuchFileException();
  }
}
