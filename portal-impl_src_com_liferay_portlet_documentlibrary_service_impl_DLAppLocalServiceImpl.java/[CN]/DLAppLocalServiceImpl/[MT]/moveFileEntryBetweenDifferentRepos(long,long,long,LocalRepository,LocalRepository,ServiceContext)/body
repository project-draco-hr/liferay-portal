{
  FileEntry sourceFileEntry=fromRepository.getFileEntry(fileEntryId);
  FileEntry destinationFileEntry=deepCopyFileEntry(userId,toRepository,sourceFileEntry,newFolderId,serviceContext);
  deleteWithErrorHandling(fileEntryId,destinationFileEntry.getFileEntryId(),fromRepository,toRepository);
  registerDLProcessorCallback(destinationFileEntry);
  return destinationFileEntry;
}
