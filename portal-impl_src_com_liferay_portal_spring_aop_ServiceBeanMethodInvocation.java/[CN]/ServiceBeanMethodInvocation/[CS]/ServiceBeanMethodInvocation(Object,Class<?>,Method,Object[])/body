{
  _target=target;
  _targetClass=targetClass;
  _method=method;
  _arguments=arguments;
  if (!_method.isAccessible()) {
    _method.setAccessible(true);
  }
  if (_method.getDeclaringClass() == Object.class) {
    String methodName=_method.getName();
    if (methodName.equals("equals")) {
      _equalsMethod=true;
    }
  }
}
