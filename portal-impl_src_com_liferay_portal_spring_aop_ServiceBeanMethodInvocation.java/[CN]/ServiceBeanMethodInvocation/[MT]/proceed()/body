{
  if (_index < _methodInterceptors.size()) {
    MethodInterceptor methodInterceptor=_methodInterceptors.get(_index++);
    return methodInterceptor.invoke(this);
  }
  try {
    if (_isEqualsMethod) {
      Object argument=_arguments[0];
      if ((argument != null) && Proxy.isProxyClass(argument.getClass())) {
        InvocationHandler invocationHandler=Proxy.getInvocationHandler(argument);
        if (invocationHandler instanceof ServiceBeanAopProxy) {
          ServiceBeanAopProxy serviceBeanAopProxy=(ServiceBeanAopProxy)invocationHandler;
          argument=serviceBeanAopProxy.getTarget();
          _arguments[0]=argument;
        }
      }
    }
    return _method.invoke(_target,_arguments);
  }
 catch (  InvocationTargetException ite) {
    throw ite.getTargetException();
  }
}
