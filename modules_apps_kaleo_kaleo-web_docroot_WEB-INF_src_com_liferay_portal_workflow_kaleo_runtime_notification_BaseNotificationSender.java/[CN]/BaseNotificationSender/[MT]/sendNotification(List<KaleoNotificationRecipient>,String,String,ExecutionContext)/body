{
  try {
    Map<NotificationReceptionType,Set<NotificationRecipient>> notificationRecipientsMap=getNotificationRecipientsMap(kaleoNotificationRecipients,executionContext);
    if (notificationRecipientsMap.isEmpty()) {
      return;
    }
    doSendNotification(notificationRecipientsMap,defaultSubject,notificationMessage,executionContext);
  }
 catch (  Exception e) {
    throw new NotificationMessageSenderException("Unable to send notification message",e);
  }
}
