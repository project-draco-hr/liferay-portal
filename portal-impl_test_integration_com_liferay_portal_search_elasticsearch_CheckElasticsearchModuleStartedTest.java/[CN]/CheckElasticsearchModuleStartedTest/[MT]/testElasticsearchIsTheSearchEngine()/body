{
  Registry registry=RegistryUtil.getRegistry();
  SearchEngine searchEngine=registry.getService(SearchEngine.class);
  Assert.assertNotNull(searchEngine);
  Class<? extends SearchEngine> searchEngineClass=searchEngine.getClass();
  String searchEngineClassName=searchEngineClass.getName();
  Assert.assertTrue("The service registered as SearchEngine is " + searchEngineClassName,searchEngineClassName.endsWith("ElasticsearchSearchEngine"));
}
