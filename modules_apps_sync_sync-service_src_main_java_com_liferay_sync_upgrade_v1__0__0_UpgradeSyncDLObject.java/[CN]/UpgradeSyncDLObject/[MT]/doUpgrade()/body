{
  int syncDLObjectsCount=_syncDLObjectLocalService.getSyncDLObjectsCount();
  if (syncDLObjectsCount != 0) {
    return;
  }
  try (LoggingTimer loggingTimer=new LoggingTimer()){
    _syncDLObjectLocalService.deleteSyncDLObjects(DLFileEntryConstants.PRIVATE_WORKING_COPY_VERSION,SyncDLObjectConstants.TYPE_FILE);
    VerifyUtil.verify();
  }
 }
