{
  Attribute avaliableLocalesAttribute=rootElement.attribute("available-locales");
  String avaliableLocalesAttributeValue=avaliableLocalesAttribute.getValue();
  if (!avaliableLocalesAttributeValue.contains(newLocaleId)) {
    return false;
  }
  if (!newLocaleId.equals(rootElement.attributeValue("default-locale"))) {
    return false;
  }
  return checkElementLocale(rootElement,newLocaleId);
}
