{
  String sql="select substr(data,3,2) from TestSubStr where id = 1";
  String sqlBuild=_db.buildSQL(sql);
  String sqlTransform=SQLTransformer.transform(sqlBuild);
  try (Connection connection=DataAccess.getConnection();PreparedStatement preparedStatement=connection.prepareStatement(sqlTransform)){
    ResultSet resultSet=preparedStatement.executeQuery();
    Assert.assertNotNull(resultSet.next());
    String substring=resultSet.getString(1);
    Assert.assertEquals(substring,"AM");
  }
 }
