{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  Team newTeam=addTeam();
  _persistence.clearCache();
  TeamModelImpl existingTeamModelImpl=(TeamModelImpl)_persistence.findByPrimaryKey(newTeam.getPrimaryKey());
  Assert.assertEquals(existingTeamModelImpl.getGroupId(),existingTeamModelImpl.getOriginalGroupId());
  Assert.assertTrue(Validator.equals(existingTeamModelImpl.getName(),existingTeamModelImpl.getOriginalName()));
}
