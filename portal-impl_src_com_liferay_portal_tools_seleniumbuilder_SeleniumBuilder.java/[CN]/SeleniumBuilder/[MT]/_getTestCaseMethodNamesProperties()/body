{
  Map<String,Set<String>> testCaseMethodNameMap=_getTestCaseMethodNameMap();
  Set<String> testCaseMethodNames=new TreeSet<String>();
  StringBundler sb=new StringBundler();
  for (  Map.Entry<String,Set<String>> testCaseMethodNamesEntry : testCaseMethodNameMap.entrySet()) {
    String componentName=testCaseMethodNamesEntry.getKey();
    componentName=StringUtil.replace(componentName,"-","_");
    componentName=StringUtil.upperCase(componentName);
    sb.append(componentName);
    sb.append("_TEST_CASE_METHOD_NAMES");
    sb.append("=");
    Set<String> testCommandSet=testCaseMethodNamesEntry.getValue();
    String testCommands=StringUtil.merge(testCommandSet.toArray(new String[testCommandSet.size()]),StringPool.SPACE);
    sb.append(testCommands);
    sb.append("\n");
    testCaseMethodNames.add(testCommands);
  }
  String testCaseMethodNamesString=StringUtil.merge(testCaseMethodNames.toArray(new String[testCaseMethodNames.size()]),StringPool.SPACE);
  sb.append("\nTEST_CASE_METHOD_NAMES=");
  sb.append(testCaseMethodNamesString);
  return sb.toString();
}
