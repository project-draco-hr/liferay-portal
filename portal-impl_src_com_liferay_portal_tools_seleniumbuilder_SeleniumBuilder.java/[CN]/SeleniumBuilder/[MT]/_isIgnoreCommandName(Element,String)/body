{
  String ignoreCommandNamesString=rootElement.attributeValue("ignore-command-names");
  if (ignoreCommandNamesString == null) {
    return false;
  }
  String[] ignoreCommandNames=StringUtil.split(ignoreCommandNamesString);
  ignoreCommandNamesString=StringUtil.replace(ignoreCommandNamesString,new String[]{" ","\n","\t"},new String[]{"","",""});
  return ArrayUtil.contains(ignoreCommandNames,commandName);
}
