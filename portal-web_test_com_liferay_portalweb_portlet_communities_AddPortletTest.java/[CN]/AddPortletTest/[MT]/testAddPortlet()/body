{
  selenium.click("link=Add Application");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("document.getElementById('Community-Communities')")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("document.getElementById('Community-Communities').getElementsByTagName('p')[0].getElementsByTagName('a')[0]");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("document.getElementById('portlet-small-icon-bar_29').getElementsByTagName('nobr')[1].getElementsByTagName('a')[0].getElementsByTagName('img')[0]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
