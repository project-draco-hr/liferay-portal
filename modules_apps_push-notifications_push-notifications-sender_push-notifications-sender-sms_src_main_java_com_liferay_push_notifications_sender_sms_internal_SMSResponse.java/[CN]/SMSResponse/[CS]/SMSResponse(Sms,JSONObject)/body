{
  super(PushNotificationsConstants.PLATFORM_SMS);
  accountSid=sms.getAccountSid();
  id=sms.getSid();
  payload=payloadJSONObject.toString();
  price=sms.getPrice();
  status=sms.getStatus();
  if (Validator.isNotNull(status) && status.equals("queued")) {
    succeeded=true;
  }
  token=sms.getTo();
}
