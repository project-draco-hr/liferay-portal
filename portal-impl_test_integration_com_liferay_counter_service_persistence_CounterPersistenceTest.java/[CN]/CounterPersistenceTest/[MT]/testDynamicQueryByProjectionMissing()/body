{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Counter.class,Counter.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("name"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("name",new Object[]{ServiceTestUtil.randomString()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
