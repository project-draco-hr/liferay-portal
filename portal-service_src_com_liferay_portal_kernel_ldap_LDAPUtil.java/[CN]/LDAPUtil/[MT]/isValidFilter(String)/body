{
  if (Validator.isNull(filter)) {
    return true;
  }
  filter=filter.trim();
  if (filter.equals(StringPool.STAR)) {
    return true;
  }
  filter=StringUtil.replace(filter,StringPool.SPACE,StringPool.BLANK);
  if (!filter.startsWith(StringPool.OPEN_PARENTHESIS) || !filter.endsWith(StringPool.CLOSE_PARENTHESIS)) {
    return false;
  }
  int count=0;
  for (int i=0; i < filter.length(); i++) {
    char c=filter.charAt(i);
    if (c == CharPool.CLOSE_PARENTHESIS) {
      count--;
    }
 else     if (c == CharPool.OPEN_PARENTHESIS) {
      count++;
    }
    if (count < 0) {
      return false;
    }
  }
  if (count > 0) {
    return false;
  }
  Matcher matcher=_pattern1.matcher(filter);
  if (matcher.matches()) {
    return false;
  }
  matcher=_pattern2.matcher(filter);
  if (matcher.matches()) {
    return false;
  }
  matcher=_pattern3.matcher(filter);
  if (matcher.matches()) {
    return false;
  }
  matcher=_pattern4.matcher(filter);
  if (matcher.matches()) {
    return false;
  }
  return true;
}
