{
  selenium.select("_26_id",RuntimeVariables.replace("label=English to Portuguese"));
  selenium.type("_26_text",RuntimeVariables.replace("My name is Liferay Translator, fluent in over 6 million forms of communication."));
  selenium.click(RuntimeVariables.replace("//input[@value='Translate']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Meu nome \u00e9 tradutor de Liferay, fluente dentro sobre 6 milh\u00e3o formul\u00e1rios de uma comunica\u00e7\u00e3o. "));
}
