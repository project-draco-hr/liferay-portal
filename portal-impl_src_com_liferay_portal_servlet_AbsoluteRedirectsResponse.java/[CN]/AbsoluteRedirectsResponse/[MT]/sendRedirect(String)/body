{
  String portalURL=getPortalURL();
  if (Validator.isNotNull(portalURL) && redirect.startsWith(StringPool.SLASH)) {
    redirect=portalURL + redirect;
  }
  if (!PropsValues.SESSION_ENABLE_PERSISTENT_COOKIES) {
    redirect=PortalUtil.getURLWithSessionId(redirect,_req.getSession().getId());
  }
  super.sendRedirect(redirect);
}
