{
  IGImage newIGImage=addIGImage();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(IGImage.class,IGImage.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("imageId",newIGImage.getImageId()));
  List<IGImage> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  IGImage existingIGImage=result.get(0);
  assertEquals(existingIGImage,newIGImage);
}
