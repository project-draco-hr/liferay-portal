{
  long groupId=ParamUtil.getLong(actionRequest,"groupId");
  String oldTemplateId=ParamUtil.getString(actionRequest,"oldTemplateId");
  String newTemplateId=ParamUtil.getString(actionRequest,"newTemplateId");
  boolean autoTemplateId=ParamUtil.getBoolean(actionRequest,"autoTemplateId");
  JournalTemplateServiceUtil.copyTemplate(groupId,oldTemplateId,newTemplateId,autoTemplateId);
}
