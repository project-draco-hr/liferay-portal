{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      for (int second=0; ; second++) {
        if (second >= 60) {
          fail("timeout");
        }
        try {
          if (selenium.isElementPresent("link=Web Content Display Permissions Test Page")) {
            break;
          }
        }
 catch (        Exception e) {
        }
        Thread.sleep(1000);
      }
    selenium.click(RuntimeVariables.replace("link=Web Content Display Permissions Test Page"));
  selenium.waitForPageToLoad("30000");
selenium.click(RuntimeVariables.replace("link=Configuration"));
selenium.waitForPageToLoad("30000");
boolean Checked=selenium.isChecked("_86_enableCommentsCheckbox");
if (Checked) {
label=2;
continue;
}
selenium.click("_86_enableCommentsCheckbox");
case 2:
case 100:
label=-1;
}
}
}
