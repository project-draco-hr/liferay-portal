{
  if ((isThumbnailEnabled(THUMBNAIL_INDEX_DEFAULT) && !hasThumbnail(fileVersion,THUMBNAIL_INDEX_DEFAULT)) || (isThumbnailEnabled(THUMBNAIL_INDEX_CUSTOM_1) && !hasThumbnail(fileVersion,THUMBNAIL_INDEX_CUSTOM_1)) || (isThumbnailEnabled(THUMBNAIL_INDEX_CUSTOM_2) && !hasThumbnail(fileVersion,THUMBNAIL_INDEX_CUSTOM_2))) {
    return false;
  }
  return true;
}
