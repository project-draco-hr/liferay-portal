{
  if ((_policy != null) && !(permission instanceof CheckMemberAccessPermission) && !(permission instanceof PortalHookPermission)&& !(permission instanceof PortalMessageBusPermission)&& !(permission instanceof PortalRuntimePermission)&& !(permission instanceof PortalServicePermission)&& !(permission instanceof PACLUtil.Permission)) {
    return _policy.implies(protectionDomain,permission);
  }
  return true;
}
