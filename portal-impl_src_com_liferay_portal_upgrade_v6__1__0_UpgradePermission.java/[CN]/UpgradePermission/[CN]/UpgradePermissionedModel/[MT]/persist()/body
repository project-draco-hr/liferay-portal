{
  try {
    StringBundler sb=new StringBundler(8);
    sb.append("update ");
    sb.append(_tableName);
    sb.append(" set resourceBlockId = ");
    sb.append(_resourceBlockId);
    sb.append(" where ");
    sb.append(_pkColumnName);
    sb.append(" = ");
    sb.append(_primKey);
    runSQL(sb.toString());
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
