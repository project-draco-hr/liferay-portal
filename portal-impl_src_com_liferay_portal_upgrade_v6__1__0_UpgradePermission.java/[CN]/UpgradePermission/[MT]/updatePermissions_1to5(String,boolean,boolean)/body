{
  if (community) {
    setContainerResourcePermissions(name,RoleConstants.ORGANIZATION_USER,ActionKeys.VIEW);
    setContainerResourcePermissions(name,RoleConstants.SITE_MEMBER,ActionKeys.VIEW);
  }
  if (guest) {
    setContainerResourcePermissions(name,RoleConstants.GUEST,ActionKeys.VIEW);
  }
  setContainerResourcePermissions(name,RoleConstants.OWNER,ActionKeys.VIEW);
}
