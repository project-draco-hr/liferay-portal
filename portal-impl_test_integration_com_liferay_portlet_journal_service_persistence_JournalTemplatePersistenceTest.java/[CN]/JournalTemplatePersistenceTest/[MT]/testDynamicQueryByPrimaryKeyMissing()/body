{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalTemplate.class,JournalTemplate.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id",ServiceTestUtil.nextLong()));
  List<JournalTemplate> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
