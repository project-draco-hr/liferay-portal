{
  final IntegerWrapper count=new IntegerWrapper();
  ActionableDynamicQuery actionableDynamicQuery=new JournalTemplateActionableDynamicQuery(){
    @Override protected void performAction(    Object object){
      JournalTemplate journalTemplate=(JournalTemplate)object;
      Assert.assertNotNull(journalTemplate);
      count.increment();
    }
  }
;
  actionableDynamicQuery.performActions();
  Assert.assertEquals(count.getValue(),_persistence.countAll());
}
