{
  Set<String> portalImplPackageNames=_getPackageNames(Paths.get("portal-impl/src"));
  Set<String> portalKernelPackageNames=_getPackageNames(Paths.get("portal-kernel/src"));
  portalImplPackageNames.retainAll(portalKernelPackageNames);
  Assert.assertTrue("Detected split packages in portal-impl and portal-kernel: " + portalImplPackageNames,portalImplPackageNames.isEmpty());
}
