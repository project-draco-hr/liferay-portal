{
  Set<String> portalImplPackageNames=_getPackageNames(Paths.get("portal-impl/src"));
  Set<String> portalServicePackageNames=_getPackageNames(Paths.get("portal-service/src"));
  portalImplPackageNames.retainAll(portalServicePackageNames);
  Assert.assertTrue("Detected split packages in portal-service and portal-impl: " + portalImplPackageNames,portalImplPackageNames.isEmpty());
}
