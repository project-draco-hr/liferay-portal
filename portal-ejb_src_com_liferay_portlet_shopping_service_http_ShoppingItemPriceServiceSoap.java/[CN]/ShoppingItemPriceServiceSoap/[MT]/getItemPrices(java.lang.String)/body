{
  try {
    java.util.List returnValue=ShoppingItemPriceServiceUtil.getItemPrices(itemId);
    return (com.liferay.portlet.shopping.model.ShoppingItemPrice[])returnValue.toArray(new com.liferay.portlet.shopping.model.ShoppingItemPrice[0]);
