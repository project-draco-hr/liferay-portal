{
  Project project=printArtifactPublishCommandsTask.getProject();
  if (LiferayOSGiDefaultsPlugin.isTestProject(project)) {
    printArtifactPublishCommandsTask.setEnabled(false);
  }
  printArtifactPublishCommandsTask.setFirstPublishExcludedTaskName(LiferayOSGiDefaultsPlugin.UPDATE_FILE_VERSIONS_TASK_NAME);
  configureTaskPrintArtifactPublishCommandsPrepNextFiles(printArtifactPublishCommandsTask,LiferayOSGiDefaultsPlugin.UPDATE_BUNDLE_VERSION_TASK_NAME);
}
