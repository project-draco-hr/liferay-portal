{
  task.onlyIf(new Spec<Task>(){
    @Override public boolean isSatisfiedBy(    Task task){
      Project project=task.getProject();
      if (GradleUtil.hasStartParameterTask(project,task.getName()) || GradleUtil.isSnapshot(project)) {
        return true;
      }
      return false;
    }
  }
);
}
