{
  for (  BlogsEntry entry : entries) {
    if (statusInTrash) {
      Assert.assertEquals("The entry " + entry.getEntryId() + " should be in trash",WorkflowConstants.STATUS_IN_TRASH,entry.getStatus());
    }
 else {
      Assert.assertNotEquals("The entry " + entry.getEntryId() + " should not be in trash",WorkflowConstants.STATUS_IN_TRASH,entry.getStatus());
    }
  }
}
