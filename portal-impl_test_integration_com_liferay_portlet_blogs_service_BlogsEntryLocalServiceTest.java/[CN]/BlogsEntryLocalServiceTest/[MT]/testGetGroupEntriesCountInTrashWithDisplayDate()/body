{
  User user=TestPropsValues.getUser();
  int initialCount=BlogsEntryLocalServiceUtil.getGroupEntriesCount(_group.getGroupId(),new Date(),_queryStatusInTrash);
  addEntryTrashAndEntryNotTrash(user);
  int actualCount=BlogsEntryLocalServiceUtil.getGroupEntriesCount(_group.getGroupId(),new Date(),_queryStatusInTrash);
  Assert.assertEquals(initialCount + 1,actualCount);
}
