{
  User user=TestPropsValues.getUser();
  int initialCount=BlogsEntryLocalServiceUtil.getCompanyEntriesCount(user.getCompanyId(),new Date(),QUERY_NOT_IN_TRASH);
  addEntryTrashAndEntryNotTrash(user);
  int actualCount=BlogsEntryLocalServiceUtil.getCompanyEntriesCount(user.getCompanyId(),new Date(),QUERY_NOT_IN_TRASH);
  Assert.assertEquals(initialCount + 1,actualCount);
}
