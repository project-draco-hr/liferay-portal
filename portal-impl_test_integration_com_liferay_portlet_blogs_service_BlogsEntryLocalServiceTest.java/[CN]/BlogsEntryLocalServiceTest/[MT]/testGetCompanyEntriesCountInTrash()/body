{
  int initialCount=BlogsEntryLocalServiceUtil.getCompanyEntriesCount(_user.getCompanyId(),new Date(),_queryStatusInTrash);
  addEntryInTrashAndEntryNotInTrash();
  int actualCount=BlogsEntryLocalServiceUtil.getCompanyEntriesCount(_user.getCompanyId(),new Date(),_queryStatusInTrash);
  Assert.assertEquals(initialCount + 1,actualCount);
}
