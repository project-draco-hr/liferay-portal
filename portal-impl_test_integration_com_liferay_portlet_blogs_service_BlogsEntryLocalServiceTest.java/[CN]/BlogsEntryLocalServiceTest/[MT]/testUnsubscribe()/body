{
  int initialCount=SubscriptionLocalServiceUtil.getUserSubscriptionsCount(TestPropsValues.getUserId());
  BlogsEntryLocalServiceUtil.subscribe(TestPropsValues.getUserId(),_group.getGroupId());
  BlogsEntryLocalServiceUtil.unsubscribe(TestPropsValues.getUserId(),_group.getGroupId());
  int actualCount=SubscriptionLocalServiceUtil.getUserSubscriptionsCount(TestPropsValues.getUserId());
  Assert.assertEquals(initialCount,actualCount);
}
