{
  User user=TestPropsValues.getUser();
  int initialCount=BlogsEntryLocalServiceUtil.getGroupUserEntriesCount(_group.getGroupId(),user.getUserId(),new Date(),_queryStatusInTrash);
  addEntryTrashAndEntryNotTrash(user);
  int actualCount=BlogsEntryLocalServiceUtil.getGroupUserEntriesCount(_group.getGroupId(),user.getUserId(),new Date(),_queryStatusInTrash);
  Assert.assertEquals(initialCount + 1,actualCount);
}
