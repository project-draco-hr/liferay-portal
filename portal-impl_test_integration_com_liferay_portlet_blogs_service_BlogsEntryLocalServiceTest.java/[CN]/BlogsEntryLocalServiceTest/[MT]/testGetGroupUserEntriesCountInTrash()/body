{
  User user=TestPropsValues.getUser();
  int initialCount=BlogsEntryLocalServiceUtil.getGroupUserEntriesCount(group.getGroupId(),user.getUserId(),new Date(),QUERY_IN_TRASH);
  addEntryTrashAndEntryNotTrash(user);
  int actualCount=BlogsEntryLocalServiceUtil.getGroupUserEntriesCount(group.getGroupId(),user.getUserId(),new Date(),QUERY_IN_TRASH);
  Assert.assertEquals(initialCount + 1,actualCount);
}
