{
  int initialCount=BlogsEntryLocalServiceUtil.getGroupUserEntriesCount(_group.getGroupId(),_user.getUserId(),new Date(),_queryStatusInTrash);
  addEntryInTrashAndEntryNotInTrash();
  int actualCount=BlogsEntryLocalServiceUtil.getGroupUserEntriesCount(_group.getGroupId(),_user.getUserId(),new Date(),_queryStatusInTrash);
  Assert.assertEquals(initialCount + 1,actualCount);
}
