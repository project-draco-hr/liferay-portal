{
  User user=TestPropsValues.getUser();
  List<BlogsEntry> groupEntries=BlogsEntryLocalServiceUtil.getGroupUserEntries(group.getGroupId(),user.getUserId(),new Date(),queryStatusNotInTrash);
  int initialCount=groupEntries.size();
  addEntryTrashAndEntryNotTrash(user);
  List<BlogsEntry> groupEntriesNotInTrash=BlogsEntryLocalServiceUtil.getGroupUserEntries(group.getGroupId(),user.getUserId(),new Date(),queryStatusNotInTrash);
  Assert.assertEquals(initialCount + 1,groupEntriesNotInTrash.size());
  for (  BlogsEntry groupEntry : groupEntriesNotInTrash) {
    if (WorkflowConstants.STATUS_IN_TRASH == groupEntry.getStatus()) {
      Assert.fail("The blogEntry " + groupEntry.getEntryId() + " is in trash");
    }
  }
}
