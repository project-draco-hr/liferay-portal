{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId(),userId);
  BlogsEntry entry=BlogsEntryLocalServiceUtil.addEntry(userId,RandomTestUtil.randomString(),RandomTestUtil.randomString(),serviceContext);
  if (statusInTrash) {
    entry=BlogsEntryLocalServiceUtil.moveEntryToTrash(userId,entry);
  }
  return entry;
}
