{
  User user=TestPropsValues.getUser();
  int initialCount=BlogsEntryLocalServiceUtil.getGroupEntriesCount(group.getGroupId(),queryStatusInTrash);
  addEntryTrashAndEntryNotTrash(user);
  int actualCount=BlogsEntryLocalServiceUtil.getGroupEntriesCount(group.getGroupId(),queryStatusInTrash);
  Assert.assertEquals(initialCount + 1,actualCount);
}
