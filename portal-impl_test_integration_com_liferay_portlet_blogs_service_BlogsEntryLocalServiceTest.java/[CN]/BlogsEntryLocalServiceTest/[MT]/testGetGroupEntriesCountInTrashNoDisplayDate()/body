{
  int initialCount=BlogsEntryLocalServiceUtil.getGroupEntriesCount(_group.getGroupId(),_queryStatusInTrash);
  addEntryInTrashAndEntryNotInTrash();
  int actualCount=BlogsEntryLocalServiceUtil.getGroupEntriesCount(_group.getGroupId(),_queryStatusInTrash);
  Assert.assertEquals(initialCount + 1,actualCount);
}
