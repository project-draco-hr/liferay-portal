{
  int initialCount=BlogsEntryLocalServiceUtil.getGroupEntriesCount(_group.getGroupId(),new Date(),_queryStatusNotInTrash);
  addEntryInTrashAndEntryNotInTrash();
  int actualCount=BlogsEntryLocalServiceUtil.getGroupEntriesCount(_group.getGroupId(),new Date(),_queryStatusNotInTrash);
  Assert.assertEquals(initialCount + 1,actualCount);
}
