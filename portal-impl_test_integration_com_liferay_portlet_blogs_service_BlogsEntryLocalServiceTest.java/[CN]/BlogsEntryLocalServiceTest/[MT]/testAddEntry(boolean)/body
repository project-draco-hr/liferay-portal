{
  int initialCount=BlogsEntryLocalServiceUtil.getGroupEntriesCount(_group.getGroupId(),_queryStatusApproved);
  BlogsEntry entry=BlogsTestUtil.addEntry(TestPropsValues.getUserId(),_group,true,smallImage);
  int actualCount=BlogsEntryLocalServiceUtil.getGroupEntriesCount(_group.getGroupId(),_queryStatusApproved);
  Assert.assertEquals(initialCount + 1,actualCount);
  return entry;
}
