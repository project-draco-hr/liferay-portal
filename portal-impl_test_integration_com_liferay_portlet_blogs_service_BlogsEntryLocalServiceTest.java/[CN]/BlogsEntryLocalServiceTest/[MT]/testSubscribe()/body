{
  int initialCount=SubscriptionLocalServiceUtil.getUserSubscriptionsCount(TestPropsValues.getUserId());
  BlogsEntryLocalServiceUtil.subscribe(TestPropsValues.getUserId(),group.getGroupId());
  int actualCount=SubscriptionLocalServiceUtil.getUserSubscriptionsCount(TestPropsValues.getUserId());
  Assert.assertEquals(initialCount + 1,actualCount);
}
