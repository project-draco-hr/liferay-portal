{
  Organization organization=OrganizationTestUtil.addOrganization();
  User user=UserTestUtil.addOrganizationOwnerUser(organization);
  int initialCount=BlogsEntryLocalServiceUtil.getOrganizationEntriesCount(organization.getOrganizationId(),new Date(),queryStatusInTrash);
  addEntryTrashAndEntryNotTrash(user);
  int actualCount=BlogsEntryLocalServiceUtil.getOrganizationEntriesCount(organization.getOrganizationId(),new Date(),queryStatusInTrash);
  Assert.assertEquals(initialCount + 1,actualCount);
}
