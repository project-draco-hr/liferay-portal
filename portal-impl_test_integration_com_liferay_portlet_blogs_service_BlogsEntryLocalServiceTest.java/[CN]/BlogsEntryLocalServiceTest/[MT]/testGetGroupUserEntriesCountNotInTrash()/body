{
  User user=TestPropsValues.getUser();
  int initialCount=BlogsEntryLocalServiceUtil.getGroupUserEntriesCount(_group.getGroupId(),user.getUserId(),new Date(),_queryStatusNotInTrash);
  addEntryInTrashAndEntryNotInTrash(user);
  int actualCount=BlogsEntryLocalServiceUtil.getGroupUserEntriesCount(_group.getGroupId(),user.getUserId(),new Date(),_queryStatusNotInTrash);
  Assert.assertEquals(initialCount + 1,actualCount);
}
