{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  KBArticle kbArticle=(KBArticle)portletRequest.getAttribute("article_icons.jsp-kb_article");
  if (kbArticle.isRoot() && KBArticlePermission.contains(themeDisplay.getPermissionChecker(),kbArticle,KBActionKeys.PERMISSIONS)) {
    return true;
  }
  return false;
}
