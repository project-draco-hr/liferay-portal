{
  LayoutTypePortlet layoutTypePortlet=(LayoutTypePortlet)layout.getLayoutType();
  portletId=layoutTypePortlet.addPortletId(0,portletId,columnId,-1,false);
  updateLayout(layout);
  addResourcePermissions(layout,portletId);
  addResourcePermissions(layout,portletId);
  addResourcePermissions(layout,portletId);
  return portletId;
}
