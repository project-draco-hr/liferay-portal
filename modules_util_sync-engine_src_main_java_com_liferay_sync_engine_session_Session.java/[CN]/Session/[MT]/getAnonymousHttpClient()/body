{
  if (_anonymousHttpClient != null) {
    return _anonymousHttpClient;
  }
  HttpClientBuilder httpClientBuilder=createHttpClientBuilder(true,1000,PropsValues.SYNC_HTTP_CONNECTION_TIMEOUT,PropsValues.SYNC_HTTP_CONNECTION_TIMEOUT * 2);
  _anonymousHttpClient=httpClientBuilder.build();
  return _anonymousHttpClient;
}
