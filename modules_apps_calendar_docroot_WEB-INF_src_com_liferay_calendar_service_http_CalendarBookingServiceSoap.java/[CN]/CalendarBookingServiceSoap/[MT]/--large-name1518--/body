{
  try {
    Map<Locale,String> titleMap=LocalizationUtil.getLocalizationMap(titleMapLanguageIds,titleMapValues);
    Map<Locale,String> descriptionMap=LocalizationUtil.getLocalizationMap(descriptionMapLanguageIds,descriptionMapValues);
    com.liferay.calendar.model.CalendarBooking returnValue=CalendarBookingServiceUtil.updateOffsetAndDuration(calendarBookingId,calendarId,childCalendarIds,titleMap,descriptionMap,location,offset,duration,allDay,recurrence,firstReminder,firstReminderType,secondReminder,secondReminderType,status,serviceContext);
    return com.liferay.calendar.model.CalendarBookingSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
