{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select classNameId, classPK, threadId from MBDiscussion");
    rs=ps.executeQuery();
    while (rs.next()) {
      long classNameId=rs.getLong("classNameId");
      long classPK=rs.getLong("classPK");
      long threadId=rs.getLong("threadId");
      runSQL("update MBMessage set classNameId = " + classNameId + ", classPK = "+ classPK+ " where threadId = "+ threadId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
