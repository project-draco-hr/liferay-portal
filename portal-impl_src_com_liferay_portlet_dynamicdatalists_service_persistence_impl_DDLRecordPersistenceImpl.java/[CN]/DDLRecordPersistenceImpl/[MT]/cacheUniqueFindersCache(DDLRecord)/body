{
  if (ddlRecord.isNew()) {
    Object[] args=new Object[]{ddlRecord.getUuid(),ddlRecord.getGroupId()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_UUID_G,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,args,ddlRecord);
  }
 else {
    DDLRecordModelImpl ddlRecordModelImpl=(DDLRecordModelImpl)ddlRecord;
    if ((ddlRecordModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_UUID_G.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{ddlRecord.getUuid(),ddlRecord.getGroupId()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_UUID_G,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,args,ddlRecord);
    }
  }
}
