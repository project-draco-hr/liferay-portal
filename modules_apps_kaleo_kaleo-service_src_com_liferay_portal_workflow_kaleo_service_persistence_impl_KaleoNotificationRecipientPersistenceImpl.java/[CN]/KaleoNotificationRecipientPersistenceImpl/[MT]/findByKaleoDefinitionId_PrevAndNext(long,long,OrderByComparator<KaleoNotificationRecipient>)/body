{
  KaleoNotificationRecipient kaleoNotificationRecipient=findByPrimaryKey(kaleoNotificationRecipientId);
  Session session=null;
  try {
    session=openSession();
    KaleoNotificationRecipient[] array=new KaleoNotificationRecipientImpl[3];
    array[0]=getByKaleoDefinitionId_PrevAndNext(session,kaleoNotificationRecipient,kaleoDefinitionId,orderByComparator,true);
    array[1]=kaleoNotificationRecipient;
    array[2]=getByKaleoDefinitionId_PrevAndNext(session,kaleoNotificationRecipient,kaleoDefinitionId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
