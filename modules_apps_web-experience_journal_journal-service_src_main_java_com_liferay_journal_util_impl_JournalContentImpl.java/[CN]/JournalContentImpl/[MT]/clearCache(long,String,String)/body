{
  _clearCache(groupId,articleId,ddmTemplateKey);
  MethodHandler methodHandler=new MethodHandler(_clearCacheMethodKey,groupId,articleId,ddmTemplateKey);
  ClusterRequest clusterRequest=ClusterRequest.createMulticastRequest(methodHandler,true);
  clusterRequest.setFireAndForget(true);
  ClusterExecutorUtil.execute(clusterRequest);
}
