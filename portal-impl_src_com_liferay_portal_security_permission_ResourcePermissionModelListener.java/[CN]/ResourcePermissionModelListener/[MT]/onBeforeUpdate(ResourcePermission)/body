{
  ResourcePermissionModelImpl resourcePermissionModelImpl=(ResourcePermissionModelImpl)resourcePermission;
  long columnBitmask=resourcePermissionModelImpl.getColumnBitmask();
  if ((columnBitmask & _CLEAR_ON_BEFORE_BITMASK) != 0) {
    PermissionCacheUtil.clearResourcePermissionCache(resourcePermissionModelImpl.getOriginalName(),resourcePermissionModelImpl.getOriginalPrimKey());
  }
}
