{
  try {
    if (!_page.isDraft() && WikiPagePermissionChecker.contains(themeDisplay.getPermissionChecker(),_node.getNodeId(),HtmlUtil.unescape(_page.getTitle()),ActionKeys.DELETE)) {
      return true;
    }
 else     if (_page.isDraft()) {
      return true;
    }
  }
 catch (  PortalException pe) {
  }
  return false;
}
