{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBDiscussion.class,MBDiscussion.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("discussionId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("discussionId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
