{
  _url=url;
  _urlAnchor=StringPool.BLANK;
  int pos=_url.indexOf(CharPool.POUND);
  if (pos != -1) {
    _url=url.substring(0,pos);
    _urlAnchor=url.substring(pos,url.length());
  }
  if (_url.indexOf(CharPool.QUESTION) == -1) {
    _url+="?";
  }
 else   if (!_url.endsWith("&")) {
    _url+="&";
  }
}
