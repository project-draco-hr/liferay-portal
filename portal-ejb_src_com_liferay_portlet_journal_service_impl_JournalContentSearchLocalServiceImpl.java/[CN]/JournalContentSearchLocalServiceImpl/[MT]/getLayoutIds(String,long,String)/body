{
  List layoutIds=new ArrayList();
  Iterator itr=JournalContentSearchUtil.findByO_G_A(ownerId,groupId,articleId).iterator();
  while (itr.hasNext()) {
    JournalContentSearch contentSearch=(JournalContentSearch)itr.next();
    layoutIds.add(contentSearch.getLayoutId());
  }
  return layoutIds;
}
