{
  List layoutIds=new ArrayList();
  Iterator itr=JournalContentSearchUtil.findByG_P_A(groupId,privateLayout,articleId).iterator();
  while (itr.hasNext()) {
    JournalContentSearch contentSearch=(JournalContentSearch)itr.next();
    layoutIds.add(new Long(contentSearch.getLayoutId()));
  }
  return layoutIds;
}
