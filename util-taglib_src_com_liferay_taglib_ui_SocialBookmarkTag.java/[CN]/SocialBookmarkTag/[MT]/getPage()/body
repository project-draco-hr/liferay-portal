{
  String[] socialTypes=PropsUtil.getArray(PropsKeys.SOCIAL_BOOKMARK_TYPES);
  if (ArrayUtil.contains(socialTypes,_type)) {
    String displayStyle=getDisplayStyle();
    if (!displayStyle.equals("menu") && Validator.isNotNull(_jspPath)) {
      return _jspPath;
    }
 else {
      return _PAGE;
    }
  }
 else {
    return null;
  }
}
