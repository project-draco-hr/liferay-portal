{
  DB db=DBFactoryUtil.getDB();
  Connection connection=null;
  try {
    connection=DataAccess.getConnection();
    db.runSQL(connection,_CREATE_TABLE);
    db.runSQL(connection,createInserts(_AMOUNT));
    connection.commit();
  }
 catch (  Exception e) {
    DataAccess.cleanUp(connection);
  }
}
