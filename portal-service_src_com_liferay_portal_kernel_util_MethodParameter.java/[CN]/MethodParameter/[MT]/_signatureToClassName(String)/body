{
  String className=desc;
switch (desc.charAt(0)) {
case 'B':
case 'C':
case 'D':
case 'F':
case 'I':
case 'J':
case 'S':
case 'Z':
case 'V':
    if (desc.length() != 1) {
      throw new IllegalArgumentException("Invalid: " + desc);
    }
  break;
case 'L':
className=className.substring(1,className.length() - 1);
case '[':
className=className.replace('/','.');
break;
default :
throw new IllegalArgumentException("Invalid: " + desc);
}
return className;
}
