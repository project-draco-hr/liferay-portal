{
  if (signature == null) {
    return null;
  }
  int leftBracketIndex=signature.indexOf('<');
  if (leftBracketIndex == -1) {
    return null;
  }
  int rightBracketIndex=signature.lastIndexOf('>');
  if (rightBracketIndex == -1) {
    return null;
  }
  String generics=signature.substring(leftBracketIndex + 1,rightBracketIndex);
  int total=generics.length();
  StringBuilder sb=new StringBuilder(total);
  ArrayList<String> list=new ArrayList<String>();
  int i=0;
  int innerLevel=0;
  while (i < total) {
    char c=generics.charAt(i);
    if (c == '<') {
      innerLevel++;
    }
 else     if (c == '>') {
      innerLevel--;
    }
 else     if (innerLevel == 0) {
      sb.append(c);
      if (c == ';') {
        list.add(sb.toString());
        sb.setLength(0);
      }
    }
    i++;
  }
  return list.toArray(new String[list.size()]);
}
