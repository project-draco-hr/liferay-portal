{
  MBCategory result=(MBCategory)EntityCacheUtil.getResult(MBCategoryModelImpl.ENTITY_CACHE_ENABLED,MBCategory.class,categoryId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      MBCategory mbCategory=(MBCategory)session.get(MBCategoryImpl.class,new Long(categoryId));
      cacheResult(mbCategory);
      return mbCategory;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (MBCategory)result;
  }
}
