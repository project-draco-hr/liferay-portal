{
  Session session=null;
  try {
    session=openSession();
    mbCategory=(MBCategory)session.merge(mbCategory);
    session.delete(mbCategory);
    session.flush();
    return mbCategory;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(MBCategory.class.getName());
  }
}
