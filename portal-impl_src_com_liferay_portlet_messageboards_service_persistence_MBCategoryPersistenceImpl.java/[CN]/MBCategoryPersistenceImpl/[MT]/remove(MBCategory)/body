{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(mbCategory);
    }
  }
  mbCategory=removeImpl(mbCategory);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(mbCategory);
    }
  }
  return mbCategory;
}
