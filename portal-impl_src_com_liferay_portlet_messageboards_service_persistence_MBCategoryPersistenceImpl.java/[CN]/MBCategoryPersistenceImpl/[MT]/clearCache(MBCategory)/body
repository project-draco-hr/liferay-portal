{
  EntityCacheUtil.removeResult(MBCategoryModelImpl.ENTITY_CACHE_ENABLED,MBCategoryImpl.class,mbCategory.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{mbCategory.getUuid(),Long.valueOf(mbCategory.getGroupId())});
}
