{
  mbCategory=toUnwrappedModel(mbCategory);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,mbCategory);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(mbCategory);
  return mbCategory;
}
