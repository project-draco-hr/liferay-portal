{
  String presharedKey=PropsValues.TUNNELING_SERVLET_SHARED_SECRET;
  if (Validator.isNull(presharedKey)) {
    throw new InvalidKeyException("The tunneling servlet shared key is not set");
  }
  if ((presharedKey.length() != 16) && (presharedKey.length() != 32) && (presharedKey.length() != 64)) {
    throw new InvalidKeyException("The configured preshared key must be either 16, 32 or 64 " + "characters in length.");
  }
  return new SecretKeySpec(presharedKey.getBytes(),_TUNNEL_ENCRYPTION_ALGORITHM);
}
