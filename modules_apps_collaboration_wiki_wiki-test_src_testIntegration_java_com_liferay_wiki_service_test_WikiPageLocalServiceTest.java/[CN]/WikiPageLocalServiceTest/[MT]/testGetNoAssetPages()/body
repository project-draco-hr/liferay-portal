{
  List<WikiPage> initialPages=WikiPageLocalServiceUtil.getNoAssetPages();
  WikiTestUtil.addPage(_group.getGroupId(),_node.getNodeId(),true);
  WikiPage page=WikiTestUtil.addPage(_group.getGroupId(),_node.getNodeId(),true);
  AssetEntry assetEntry=AssetEntryLocalServiceUtil.fetchEntry(WikiPage.class.getName(),page.getResourcePrimKey());
  Assert.assertNotNull(assetEntry);
  AssetEntryLocalServiceUtil.deleteAssetEntry(assetEntry);
  List<WikiPage> pages=WikiPageLocalServiceUtil.getNoAssetPages();
  Assert.assertEquals(initialPages.size() + 1,pages.size());
  Assert.assertEquals(page,pages.get(pages.size() - 1));
}
