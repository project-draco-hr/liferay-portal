{
  try {
    long groupId=GetterUtil.getLong(doc.get(Field.GROUP_ID));
    String className=doc.get(Field.ENTRY_CLASS_NAME);
    String classPK=doc.get(Field.ENTRY_CLASS_PK);
    if ((PropsValues.PERMISSIONS_USER_CHECK_ALGORITHM == 5) && (Validator.isNotNull(className)) && (Validator.isNotNull(classPK))) {
      doAddPermissionFields(companyId,groupId,className,classPK,doc);
    }
  }
 catch (  NoSuchResourceException nsre) {
  }
catch (  Exception e) {
    _log.error(e,e);
  }
}
