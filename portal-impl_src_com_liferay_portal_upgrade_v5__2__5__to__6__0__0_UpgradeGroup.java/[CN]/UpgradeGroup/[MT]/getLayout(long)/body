{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement(_GET_LAYOUT);
    ps.setLong(1,plid);
    rs=ps.executeQuery();
    if (rs.next()) {
      long groupId=rs.getLong("groupId");
      return new Object[]{groupId};
    }
    return null;
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
