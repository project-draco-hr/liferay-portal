{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Control Panel");
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Portal Instances",RuntimeVariables.replace("Portal Instances"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add']",RuntimeVariables.replace("Add"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_135_webId']",RuntimeVariables.replace("www.able.com"));
  selenium.type("//input[@id='_135_virtualHostname']",RuntimeVariables.replace("www.able.com"));
  selenium.type("//input[@id='_135_mx']",RuntimeVariables.replace("able.com"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals(RuntimeVariables.replace("www.able.com"),selenium.getText("//tr[4]/td[2]/a"));
  assertEquals(RuntimeVariables.replace("www.able.com"),selenium.getText("//tr[4]/td[3]/a"));
  assertEquals(RuntimeVariables.replace("able.com"),selenium.getText("//tr[4]/td[4]/a"));
}
