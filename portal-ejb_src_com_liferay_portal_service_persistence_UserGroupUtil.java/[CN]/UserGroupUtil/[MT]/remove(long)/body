{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(userGroupId));
  }
  com.liferay.portal.model.UserGroup userGroup=getPersistence().remove(userGroupId);
  if (listener != null) {
    listener.onAfterRemove(userGroup);
  }
  return userGroup;
}
