{
  String function=element.attributeValue("function");
  int x=function.indexOf(StringPool.POUND);
  if (x == -1) {
    _seleniumBuilderFileUtil.throwValidationException(1006,fileName,element,"function");
  }
  String functionName=function.substring(0,x);
  if (!_isFunctionName(functionName)) {
    _seleniumBuilderFileUtil.throwValidationException(1011,fileName,element,"function",functionName);
  }
  String functionCommand=function.substring(x + 1);
  if (!_isFunctionCommand(functionName,functionCommand)) {
    _seleniumBuilderFileUtil.throwValidationException(1012,fileName,element,"function",functionCommand);
  }
}
