{
  String macro=element.attributeValue("macro");
  int x=macro.indexOf(StringPool.POUND);
  if (x == -1) {
    _seleniumBuilderFileUtil.throwValidationException(1006,fileName,element,"macro");
  }
  String macroName=macro.substring(0,x);
  if (!_isMacroName(macroName)) {
    _seleniumBuilderFileUtil.throwValidationException(1011,fileName,element,"macro",macroName);
  }
  String macroCommand=macro.substring(x + 1);
  if (!_isMacroCommand(macroName,macroCommand)) {
    _seleniumBuilderFileUtil.throwValidationException(1012,fileName,element,"macro",macroCommand);
  }
}
