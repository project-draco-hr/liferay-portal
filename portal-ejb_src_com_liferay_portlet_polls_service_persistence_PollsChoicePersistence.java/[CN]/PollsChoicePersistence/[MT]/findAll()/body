{
  Session session=null;
  try {
    session=openSession();
    StringBuffer query=new StringBuffer();
    query.append("FROM PollsChoice IN CLASS com.liferay.portlet.polls.service.persistence.PollsChoiceHBM ");
    query.append("ORDER BY ");
    query.append("choiceId ASC");
    Query q=session.createQuery(query.toString());
    Iterator itr=q.iterate();
    List list=new ArrayList();
    while (itr.hasNext()) {
      PollsChoiceHBM pollsChoiceHBM=(PollsChoiceHBM)itr.next();
      list.add(PollsChoiceHBMUtil.model(pollsChoiceHBM));
    }
    return list;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
