{
  Session session=null;
  try {
    session=openSession();
    if (pollsChoice.isNew()) {
      session.save(pollsChoice);
    }
    session.flush();
    pollsChoice.setNew(false);
    return pollsChoice;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
