{
  Project project=getDownloadProject(sampleDir.getName());
  int maxHostNames=LoadBalanceUtil.calculateMaxHostNames(project,sampleDir.getName());
  for (int i=1; i <= maxHostNames; i++) {
    downloadSampleURL(new File(sampleDir,sampleDir.getName() + "-" + i),createURL(project.getProperty("jenkins.local.url[" + sampleDir.getName() + "-"+ i+ "]")),"/computer/api/json?pretty&tree=computer[idle]");
  }
}
