{
  if (((toFolderId == null) && (_toFolderId != null)) || ((toFolderId != null) && (_toFolderId == null)) || ((toFolderId != null) && (_toFolderId != null) && !toFolderId.equals(_toFolderId))) {
    if (!XSS_ALLOW_TOFOLDERID) {
      toFolderId=XSSUtil.strip(toFolderId);
    }
    _toFolderId=toFolderId;
  }
}
