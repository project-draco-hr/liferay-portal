{
  if (uuid == null) {
    objectOutput.writeUTF(StringPool.BLANK);
  }
 else {
    objectOutput.writeUTF(uuid);
  }
  objectOutput.writeLong(resourcePrimKey);
  objectOutput.writeLong(companyId);
  objectOutput.writeLong(groupId);
  if (articleId == null) {
    objectOutput.writeUTF(StringPool.BLANK);
  }
 else {
    objectOutput.writeUTF(articleId);
  }
}
