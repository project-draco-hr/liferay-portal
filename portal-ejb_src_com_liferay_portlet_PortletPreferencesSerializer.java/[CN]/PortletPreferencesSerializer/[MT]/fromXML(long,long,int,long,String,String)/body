{
  try {
    PortletPreferencesImpl prefs=(PortletPreferencesImpl)fromDefaultXML(xml);
    prefs=new PortletPreferencesImpl(companyId,ownerId,ownerType,plid,portletId,prefs.getPreferences());
    return prefs;
  }
 catch (  PortalException pe) {
    throw pe;
  }
catch (  SystemException se) {
    throw se;
  }
}
