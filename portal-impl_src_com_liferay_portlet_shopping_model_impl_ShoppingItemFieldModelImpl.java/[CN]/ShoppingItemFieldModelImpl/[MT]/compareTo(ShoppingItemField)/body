{
  int value=0;
  if (getItemId() < shoppingItemField.getItemId()) {
    value=-1;
  }
 else   if (getItemId() > shoppingItemField.getItemId()) {
    value=1;
  }
 else {
    value=0;
  }
  if (value != 0) {
    return value;
  }
  value=getName().toLowerCase().compareTo(shoppingItemField.getName().toLowerCase());
  if (value != 0) {
    return value;
  }
  return 0;
}
