{
  if (((itemFieldId == null) && (_itemFieldId != null)) || ((itemFieldId != null) && (_itemFieldId == null)) || ((itemFieldId != null) && (_itemFieldId != null) && !itemFieldId.equals(_itemFieldId))) {
    if (!XSS_ALLOW_ITEMFIELDID) {
      itemFieldId=XSSUtil.strip(itemFieldId);
    }
    _itemFieldId=itemFieldId;
  }
}
