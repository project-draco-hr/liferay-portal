{
  if (isEscapedModel()) {
    return (ShoppingItemField)this;
  }
 else {
    ShoppingItemField model=new ShoppingItemFieldImpl();
    model.setNew(isNew());
    model.setEscapedModel(true);
    model.setItemFieldId(getItemFieldId());
    model.setItemId(getItemId());
    model.setName(HtmlUtil.escape(getName()));
    model.setValues(HtmlUtil.escape(getValues()));
    model.setDescription(HtmlUtil.escape(getDescription()));
    model=(ShoppingItemField)Proxy.newProxyInstance(ShoppingItemField.class.getClassLoader(),new Class[]{ShoppingItemField.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
