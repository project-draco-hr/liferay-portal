{
  if (PropsValues.SESSION_DISABLED) {
    return;
  }
  try {
    HttpSession session=_event.getSession();
    PortalSessionContext.put(session.getId(),session);
  }
 catch (  IllegalStateException ise) {
    _log.warn(ise,ise);
  }
  try {
    EventsProcessorUtil.process(PropsKeys.SERVLET_SESSION_CREATE_EVENTS,PropsValues.SERVLET_SESSION_CREATE_EVENTS,session);
  }
 catch (  ActionException ae) {
    _log.error(ae,ae);
  }
}
