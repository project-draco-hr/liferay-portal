{
  if (obj == null) {
    return false;
  }
  ResourceImpl resource=null;
  try {
    resource=(ResourceImpl)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  String pk=resource.getPrimaryKey();
  if (getPrimaryKey().equals(pk)) {
    return true;
  }
 else {
    return false;
  }
}
