{
  long dlFolderId=0;
  long groupId=0;
  com.liferay.portal.model.Repository repository=_repositoryLocalService.fetchRepository(repositoryId);
  if (repository == null) {
    groupId=repositoryId;
  }
 else {
    groupId=repository.getGroupId();
    dlFolderId=repository.getDlFolderId();
  }
  return new long[]{groupId,repositoryId,dlFolderId};
}
