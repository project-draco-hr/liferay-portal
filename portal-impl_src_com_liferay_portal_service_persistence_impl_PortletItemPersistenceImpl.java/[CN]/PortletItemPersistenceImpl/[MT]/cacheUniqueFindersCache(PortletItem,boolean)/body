{
  if (isNew) {
    Object[] args=new Object[]{portletItem.getGroupId(),portletItem.getName(),portletItem.getPortletId(),portletItem.getClassNameId()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_G_N_P_C,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_N_P_C,args,portletItem);
  }
 else {
    PortletItemModelImpl portletItemModelImpl=(PortletItemModelImpl)portletItem;
    if ((portletItemModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_G_N_P_C.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{portletItem.getGroupId(),portletItem.getName(),portletItem.getPortletId(),portletItem.getClassNameId()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_G_N_P_C,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_N_P_C,args,portletItem);
    }
  }
}
