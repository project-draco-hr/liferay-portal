{
  Serializable serializable=entityCache.getResult(PortletItemModelImpl.ENTITY_CACHE_ENABLED,PortletItemImpl.class,primaryKey);
  if (serializable == nullModel) {
    return null;
  }
  PortletItem portletItem=(PortletItem)serializable;
  if (portletItem == null) {
    Session session=null;
    try {
      session=openSession();
      portletItem=(PortletItem)session.get(PortletItemImpl.class,primaryKey);
      if (portletItem != null) {
        cacheResult(portletItem);
      }
 else {
        entityCache.putResult(PortletItemModelImpl.ENTITY_CACHE_ENABLED,PortletItemImpl.class,primaryKey,nullModel);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(PortletItemModelImpl.ENTITY_CACHE_ENABLED,PortletItemImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return portletItem;
}
