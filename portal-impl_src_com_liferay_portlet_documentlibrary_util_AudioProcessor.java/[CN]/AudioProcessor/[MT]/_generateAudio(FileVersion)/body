{
  String id=DLUtil.getTempFileId(fileVersion.getFileEntryId(),fileVersion.getVersion());
  File file=_getAudioTempFile(id,fileVersion.getExtension());
  try {
    if (!PrefsPropsUtil.getBoolean(PropsKeys.XUGGLER_ENABLED,PropsValues.XUGGLER_ENABLED) || _hasAudio(fileVersion)) {
      return;
    }
    if (_isGeneratePreview(fileVersion)) {
      InputStream inputStream=fileVersion.getContentStream(false);
      FileUtil.write(file,inputStream);
      try {
        _generateAudioXuggler(fileVersion,file);
      }
 catch (      Exception e) {
        _log.error(e);
      }
      if (_log.isInfoEnabled()) {
        _log.info("Xuggler generated a preview audio for " + id);
      }
    }
  }
 catch (  NoSuchFileEntryException nsfee) {
  }
catch (  Exception e) {
    _log.error(e,e);
  }
 finally {
    _fileEntries.remove(fileVersion.getFileVersionId());
    FileUtil.delete(file);
  }
}
