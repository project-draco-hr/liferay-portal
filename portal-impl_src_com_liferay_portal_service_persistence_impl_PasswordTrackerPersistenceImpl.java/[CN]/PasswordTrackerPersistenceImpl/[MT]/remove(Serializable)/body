{
  Session session=null;
  try {
    session=openSession();
    PasswordTracker passwordTracker=(PasswordTracker)session.get(PasswordTrackerImpl.class,primaryKey);
    if (passwordTracker == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchPasswordTrackerException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(passwordTracker);
  }
 catch (  NoSuchPasswordTrackerException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
