{
  PortletPreferencesImpl portletPreferencesImpl=(PortletPreferencesImpl)portletPreferences;
  Assert.assertTrue("The portlet preferences defined by (ownerType: " + portletPreferencesImpl.getOwnerType() + " ownerId: "+ portletPreferencesImpl.getOwnerId()+ " and plid: "+ portletPreferencesImpl.getPlid()+ ") has defined the preferences: "+ portletPreferencesImpl.getMap().keySet()+ " and was expected to be empty",portletPreferencesImpl.getMap().isEmpty());
}
