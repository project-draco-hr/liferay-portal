{
  MockPortletPreferencesLocalServiceImpl mockPortletPreferencesLocalServiceImpl=new MockPortletPreferencesLocalServiceImpl(true);
  PortletPreferencesIds portletPreferencesIds=new PortletPreferencesIds(_layout.getCompanyId(),PortletKeys.PREFS_OWNER_ID_DEFAULT,PortletKeys.PREFS_OWNER_TYPE_LAYOUT,_layout.getPlid(),_portlet.getPortletId());
  javax.portlet.PortletPreferences jxPortletPreferences=mockPortletPreferencesLocalServiceImpl.getStrictPreferences(portletPreferencesIds);
  assertStrictPortletPreferences(jxPortletPreferences);
