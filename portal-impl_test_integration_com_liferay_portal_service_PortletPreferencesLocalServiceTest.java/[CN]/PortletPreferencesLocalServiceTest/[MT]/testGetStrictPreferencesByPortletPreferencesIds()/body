{
  replaceService(true);
  try {
    PortletPreferencesIds portletPreferencesIds=new PortletPreferencesIds(_layout.getCompanyId(),PortletKeys.PREFS_OWNER_ID_DEFAULT,PortletKeys.PREFS_OWNER_TYPE_LAYOUT,_layout.getPlid(),_portlet.getPortletId());
    javax.portlet.PortletPreferences jxPortletPreferences=PortletPreferencesLocalServiceUtil.getStrictPreferences(portletPreferencesIds);
    assertStrictPortletPreferences(jxPortletPreferences);
  }
  finally {
    resetService();
  }
}
