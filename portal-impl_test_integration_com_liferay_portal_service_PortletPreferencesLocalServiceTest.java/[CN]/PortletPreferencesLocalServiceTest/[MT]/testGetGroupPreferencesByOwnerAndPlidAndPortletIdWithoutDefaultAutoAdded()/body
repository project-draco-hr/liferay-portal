{
  javax.portlet.PortletPreferences jxPortletPreferences=PortletPreferencesLocalServiceUtil.getPreferences(_group.getCompanyId(),_group.getGroupId(),PortletKeys.PREFS_OWNER_TYPE_GROUP,_layout.getPlid(),_portlet.getPortletId());
  assertEmptyPortletPreferencesMap(jxPortletPreferences);
  assertOwner(_layout.getGroup(),(PortletPreferencesImpl)jxPortletPreferences);
}
