{
  javax.portlet.PortletPreferences jxPortletPreferences=PortletPreferencesLocalServiceUtil.getPreferences(_layout.getCompanyId(),PortletKeys.PREFS_OWNER_ID_DEFAULT,PortletKeys.PREFS_OWNER_TYPE_LAYOUT,_layout.getPlid(),_portlet.getPortletId());
  assertEmptyPortletPreferencesMap(jxPortletPreferences);
  assertOwner(_layout,(PortletPreferencesImpl)jxPortletPreferences);
}
