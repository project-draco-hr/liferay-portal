{
  PortletPreferences portletPreferences=PortletPreferencesLocalServiceUtil.addPortletPreferences(TestPropsValues.getCompanyId(),PortletKeys.PREFS_OWNER_ID_DEFAULT,PortletKeys.PREFS_OWNER_TYPE_LAYOUT,_layout.getPlid(),_portlet.getPortletId(),null,null);
  PortletPreferencesImpl portletPreferencesImpl=PortletPreferencesTestUtil.toPortletPreferencesImpl(portletPreferences);
  assertOwner(_layout,portletPreferencesImpl);
  assertEmptyPortletPreferencesMap(portletPreferencesImpl);
}
