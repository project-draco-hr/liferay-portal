{
  MockPortletPreferencesLocalServiceImpl mockPortletPreferencesLocalServiceImpl=new MockPortletPreferencesLocalServiceImpl(false);
  javax.portlet.PortletPreferences jxPortletPreferences=mockPortletPreferencesLocalServiceImpl.getStrictPreferences(_layout.getCompanyId(),PortletKeys.PREFS_OWNER_ID_DEFAULT,PortletKeys.PREFS_OWNER_TYPE_LAYOUT,_layout.getPlid(),_portlet.getPortletId());
  assertEmptyPortletPreferencesMap(jxPortletPreferences);
}
