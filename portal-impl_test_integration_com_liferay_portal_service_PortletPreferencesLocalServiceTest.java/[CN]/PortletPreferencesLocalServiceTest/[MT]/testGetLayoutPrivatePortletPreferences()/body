{
  Layout layout=LayoutTestUtil.addLayout(GroupTestUtil.addGroup().getGroupId(),ServiceTestUtil.randomString(),true);
  PortletPreferencesTestUtil.addGroupPortletPreferences(layout,_portlet);
  List<PortletPreferences> portletPreferences=PortletPreferencesLocalServiceUtil.getPortletPreferences(layout.getCompanyId(),layout.getGroupId(),layout.getGroupId(),PortletKeys.PREFS_OWNER_TYPE_GROUP,_portlet.getPortletId(),true);
  Assert.assertEquals(1,portletPreferences.size());
}
