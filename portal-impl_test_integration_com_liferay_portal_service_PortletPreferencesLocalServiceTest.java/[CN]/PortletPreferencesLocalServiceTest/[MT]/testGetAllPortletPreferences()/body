{
  List<PortletPreferences> initialPortletPreferencesList=PortletPreferencesLocalServiceUtil.getPortletPreferences();
  PortletPreferencesTestUtil.addLayoutPortletPreferences(_layout,_portlet);
  List<PortletPreferences> currentPortletPreferencesList=PortletPreferencesLocalServiceUtil.getPortletPreferences();
  Assert.assertEquals(initialPortletPreferencesList.size() + 1,currentPortletPreferencesList.size());
}
