{
  String schema=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select current_schema();");
    rs=ps.executeQuery();
    if (rs.next()) {
      schema=(String)rs.getObject("current_schema");
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
  return schema;
}
