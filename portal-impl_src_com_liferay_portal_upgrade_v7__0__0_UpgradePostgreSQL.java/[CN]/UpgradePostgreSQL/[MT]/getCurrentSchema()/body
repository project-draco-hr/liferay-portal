{
  String schema=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  StringBundler sb=new StringBundler(1);
  sb.append("select current_schema();");
  try {
    ps=connection.prepareStatement(sb.toString());
    rs=ps.executeQuery();
    if (rs.next()) {
      schema=(String)rs.getObject("current_schema");
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
  return schema;
}
