{
  ShortURL shortURL=(ShortURL)EntityCacheUtil.getResult(ShortURLModelImpl.ENTITY_CACHE_ENABLED,ShortURLImpl.class,shortURLId,this);
  if (shortURL == null) {
    Session session=null;
    try {
      session=openSession();
      shortURL=(ShortURL)session.get(ShortURLImpl.class,Long.valueOf(shortURLId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (shortURL != null) {
        cacheResult(shortURL);
      }
      closeSession(session);
    }
  }
  return shortURL;
}
