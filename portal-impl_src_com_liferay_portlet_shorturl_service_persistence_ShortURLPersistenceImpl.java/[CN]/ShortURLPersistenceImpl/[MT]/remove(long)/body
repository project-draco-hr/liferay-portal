{
  Session session=null;
  try {
    session=openSession();
    ShortURL shortURL=(ShortURL)session.get(ShortURLImpl.class,Long.valueOf(shortURLId));
    if (shortURL == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + shortURLId);
      }
      throw new NoSuchShortURLException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + shortURLId);
    }
    return shortURLPersistence.remove(shortURL);
  }
 catch (  NoSuchShortURLException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
