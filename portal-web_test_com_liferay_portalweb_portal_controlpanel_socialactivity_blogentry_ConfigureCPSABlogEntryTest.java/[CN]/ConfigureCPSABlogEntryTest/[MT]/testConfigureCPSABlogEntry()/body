{
  selenium.open("/web/blog-community-name/blog-community-social-activity-blogs-public-page");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Go to")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Go to",RuntimeVariables.replace("Go to"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Social Activity")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Social Activity"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("_179_com.liferay.portlet.blogs.model.BlogsEntry.enabledCheckbox")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("_179_com.liferay.portlet.blogs.model.BlogsEntry.enabledCheckbox",RuntimeVariables.replace(""));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace("Save"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//section[@id='portlet_179']/div/div/div/div")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Your request completed successfully."),selenium.getText("//section[@id='portlet_179']/div/div/div/div"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Back to Blog Community Name")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Back to Blog Community Name",RuntimeVariables.replace("Back to Blog Community Name"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
}
