{
  if (layoutId.equals(LayoutImpl.DEFAULT_PARENT_LAYOUT_ID)) {
    long groupId=LayoutImpl.getGroupId(ownerId);
    if (GroupPermission.contains(permissionChecker,groupId,ActionKeys.MANAGE_LAYOUTS)) {
      return true;
    }
 else {
      return false;
    }
  }
 else {
    Layout layout=LayoutLocalServiceUtil.getLayout(layoutId,ownerId);
    return contains(permissionChecker,layout,actionId);
  }
}
