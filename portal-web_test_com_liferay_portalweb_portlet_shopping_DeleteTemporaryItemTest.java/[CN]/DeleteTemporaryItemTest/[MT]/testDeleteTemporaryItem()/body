{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Shopping Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Shopping Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//td[1]/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//tr[4]/td[5]/ul/li/strong/span")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//tr[4]/td[5]/ul/li/strong/span",RuntimeVariables.replace(""));
  selenium.click(RuntimeVariables.replace("//body/div[4]/ul/li[3]/a"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this[\\s\\S]$"));
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertFalse(selenium.isTextPresent("The Prodigal Project - J. H."));
  assertTrue(selenium.isTextPresent("Jona Lyons - Wheel"));
  selenium.clickAt("link=Return to Full Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
}
