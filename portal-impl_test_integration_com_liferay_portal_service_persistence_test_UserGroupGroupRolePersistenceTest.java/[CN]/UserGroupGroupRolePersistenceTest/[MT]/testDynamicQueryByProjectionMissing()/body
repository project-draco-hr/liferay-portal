{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(UserGroupGroupRole.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("id.userGroupId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("id.userGroupId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
