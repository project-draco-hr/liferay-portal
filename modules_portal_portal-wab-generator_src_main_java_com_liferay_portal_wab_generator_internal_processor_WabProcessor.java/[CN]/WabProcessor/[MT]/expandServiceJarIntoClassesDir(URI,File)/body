{
  URI webInfClasesURI=uri.resolve("WEB-INF/classes");
  try (ZipInputStream zipInputStream=new ZipInputStream(new FileInputStream(zipFile))){
    for (ZipEntry zipEntry; (zipEntry=zipInputStream.getNextEntry()) != null; zipInputStream.closeEntry()) {
      if (zipEntry.isDirectory()) {
        File dir=new File(webInfClasesURI.getPath(),zipEntry.getName());
        dir.mkdirs();
        continue;
      }
      File file=new File(webInfClasesURI.getPath(),zipEntry.getName());
      File parentFile=file.getParentFile();
      parentFile.mkdirs();
      StreamUtil.transfer(StreamUtil.uncloseable(zipInputStream),new FileOutputStream(file));
    }
  }
 }
