{
  String exportPackages=pluginPackageProperties.getProperty(Constants.EXPORT_PACKAGE);
  if (Validator.isNotNull(exportPackages)) {
    Collections.addAll(_exportPackageNames,StringUtil.split(exportPackages));
  }
  String importPackages=pluginPackageProperties.getProperty(Constants.IMPORT_PACKAGE);
  if (Validator.isNotNull(importPackages)) {
    Collections.addAll(_importPackageNames,StringUtil.split(importPackages));
  }
}
