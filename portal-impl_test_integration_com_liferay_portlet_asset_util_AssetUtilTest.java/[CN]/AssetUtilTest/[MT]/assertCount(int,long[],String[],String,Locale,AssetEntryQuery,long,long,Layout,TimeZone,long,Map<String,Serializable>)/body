{
  IdempotentRetryAssert.retryAssert(10,TimeUnit.SECONDS,1,TimeUnit.SECONDS,new Callable<Void>(){
    @Override public Void call() throws Exception {
      BaseModelSearchResult<AssetEntry> searchResult=AssetUtil.searchAssetEntries(assetCategoryIds,assetTagNames,keywords,locale,assetEntryQuery,companyId,scopeGroupId,layout,timezone,userId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,attributes);
      Assert.assertEquals(expectedCount,searchResult.getLength());
      return null;
    }
  }
);
}
