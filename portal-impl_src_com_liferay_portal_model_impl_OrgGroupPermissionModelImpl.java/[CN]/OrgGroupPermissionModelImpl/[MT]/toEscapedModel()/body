{
  if (isEscapedModel()) {
    return (OrgGroupPermission)this;
  }
 else {
    OrgGroupPermission model=new OrgGroupPermissionImpl();
    model.setNew(isNew());
    model.setEscapedModel(true);
    model.setOrganizationId(getOrganizationId());
    model.setGroupId(getGroupId());
    model.setPermissionId(getPermissionId());
    model=(OrgGroupPermission)Proxy.newProxyInstance(OrgGroupPermission.class.getClassLoader(),new Class[]{OrgGroupPermission.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
