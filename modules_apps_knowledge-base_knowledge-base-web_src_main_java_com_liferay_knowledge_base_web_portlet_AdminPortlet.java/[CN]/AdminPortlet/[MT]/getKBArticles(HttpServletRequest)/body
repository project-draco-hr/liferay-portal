{
  long[] kbArticleIds=ParamUtil.getLongValues(request,"rowIdsKBArticle");
  List<KBArticle> kbArticles=new ArrayList<>();
  for (  long kbArticleId : kbArticleIds) {
    KBArticle kbArticle=kbArticleService.getLatestKBArticle(kbArticleId,WorkflowConstants.STATUS_ANY);
    kbArticles.add(kbArticle);
  }
  return kbArticles;
}
