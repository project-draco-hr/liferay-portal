{
  long[] deleteKBArticleResourceClassPKs=ParamUtil.getLongValues(actionRequest,"rowIdsKBArticle");
  long[] deleteKBFolderIds=ParamUtil.getLongValues(actionRequest,"rowIdsKBFolder");
  for (  long deleteKBArticleResourceClassPK : deleteKBArticleResourceClassPKs) {
    kbArticleService.deleteKBArticle(deleteKBArticleResourceClassPK);
  }
  for (  long deleteKBFolderId : deleteKBFolderIds) {
    kbFolderService.deleteKBFolder(deleteKBFolderId);
  }
}
