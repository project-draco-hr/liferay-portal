{
  Collection<ProfileAttribute> attributes=new ArrayList<ProfileAttribute>();
  if (!newUserGroup.getName().equals(oldUserGroup.getName())) {
    attributes.add(new ProfileAttribute("name",oldUserGroup.getName(),newUserGroup.getName()));
  }
  if (!newUserGroup.getDescription().equals(oldUserGroup.getDescription())) {
    attributes.add(new ProfileAttribute("description",oldUserGroup.getDescription(),newUserGroup.getDescription()));
  }
  return attributes;
}
