{
  try {
    UserGroup oldUserGroup=UserGroupLocalServiceUtil.getUserGroup(userGroup.getUserGroupId());
    Collection<ProfileAttribute> attributes=getAttributes(userGroup,oldUserGroup);
    if (attributes.size() > 0) {
      AuditMessageSenderUtil.send(UserGroupModificationAuditMessageBuilder.build(userGroup,attributes));
    }
  }
 catch (  Exception e) {
    throw new ModelListenerException(e);
  }
}
