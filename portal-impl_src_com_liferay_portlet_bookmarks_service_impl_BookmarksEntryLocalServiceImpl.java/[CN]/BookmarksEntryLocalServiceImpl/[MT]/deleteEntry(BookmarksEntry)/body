{
  try {
    Indexer.deleteEntry(entry.getCompanyId(),entry.getEntryId());
  }
 catch (  IOException ioe) {
    _log.error("Deleting index " + entry.getEntryId(),ioe);
  }
  tagsAssetLocalService.deleteAsset(BookmarksEntry.class.getName(),entry.getEntryId());
  resourceLocalService.deleteResource(entry.getCompanyId(),BookmarksEntry.class.getName(),ResourceImpl.SCOPE_INDIVIDUAL,entry.getEntryId());
  bookmarksEntryPersistence.remove(entry.getEntryId());
}
