{
  bookmarksEntryPersistence.remove(entry);
  resourceLocalService.deleteResource(entry.getCompanyId(),BookmarksEntry.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,entry.getEntryId());
  assetEntryLocalService.deleteEntry(BookmarksEntry.class.getName(),entry.getEntryId());
  expandoValueLocalService.deleteValues(BookmarksEntry.class.getName(),entry.getEntryId());
  try {
    BookmarksIndexer.deleteEntry(entry.getCompanyId(),entry.getEntryId());
  }
 catch (  SearchException se) {
    _log.error("Deleting index " + entry.getEntryId(),se);
  }
}
