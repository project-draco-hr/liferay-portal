{
  if (entry.getFolderId() != folderId) {
    BookmarksFolder oldFolder=bookmarksFolderPersistence.findByPrimaryKey(entry.getFolderId());
    BookmarksFolder newFolder=bookmarksFolderPersistence.fetchByPrimaryKey(folderId);
    if ((newFolder == null) || (oldFolder.getGroupId() != newFolder.getGroupId())) {
      folderId=entry.getFolderId();
    }
  }
  return bookmarksFolderPersistence.findByPrimaryKey(folderId);
}
