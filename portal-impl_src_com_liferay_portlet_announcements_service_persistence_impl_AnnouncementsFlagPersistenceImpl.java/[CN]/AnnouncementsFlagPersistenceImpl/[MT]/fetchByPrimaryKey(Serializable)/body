{
  AnnouncementsFlag announcementsFlag=(AnnouncementsFlag)entityCache.getResult(AnnouncementsFlagModelImpl.ENTITY_CACHE_ENABLED,AnnouncementsFlagImpl.class,primaryKey);
  if (announcementsFlag == _nullAnnouncementsFlag) {
    return null;
  }
  if (announcementsFlag == null) {
    Session session=null;
    try {
      session=openSession();
      announcementsFlag=(AnnouncementsFlag)session.get(AnnouncementsFlagImpl.class,primaryKey);
      if (announcementsFlag != null) {
        cacheResult(announcementsFlag);
      }
 else {
        entityCache.putResult(AnnouncementsFlagModelImpl.ENTITY_CACHE_ENABLED,AnnouncementsFlagImpl.class,primaryKey,_nullAnnouncementsFlag);
      }
    }
 catch (    Exception e) {
      entityCache.removeResult(AnnouncementsFlagModelImpl.ENTITY_CACHE_ENABLED,AnnouncementsFlagImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return announcementsFlag;
}
