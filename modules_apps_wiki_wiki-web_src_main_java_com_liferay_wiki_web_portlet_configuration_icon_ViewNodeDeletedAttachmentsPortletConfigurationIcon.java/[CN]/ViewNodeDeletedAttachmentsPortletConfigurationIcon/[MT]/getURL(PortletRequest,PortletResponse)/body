{
  PortletURL portletURL=PortalUtil.getControlPanelPortletURL(portletRequest,WikiPortletKeys.WIKI_ADMIN,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcRenderCommandName","/wiki/view_node_deleted_attachments");
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  portletURL.setParameter("redirect",themeDisplay.getURLCurrent());
  portletURL.setParameter("nodeId",String.valueOf(_node.getNodeId()));
  portletURL.setParameter("viewTrashAttachments",Boolean.TRUE.toString());
  return portletURL.toString();
}
