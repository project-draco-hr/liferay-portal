{
  _columns=columns;
  _insertionOrder=new int[_columns.length];
  int clobCount=0;
  for (int i=0; i < _columns.length; ++i) {
    Integer type=(Integer)columns[i][1];
    if (type.intValue() == Types.CLOB) {
      clobCount++;
      int pos=_columns.length - clobCount;
      _insertionOrder[pos]=i;
    }
 else {
      int pos=i - clobCount;
      _insertionOrder[pos]=i;
    }
  }
}
