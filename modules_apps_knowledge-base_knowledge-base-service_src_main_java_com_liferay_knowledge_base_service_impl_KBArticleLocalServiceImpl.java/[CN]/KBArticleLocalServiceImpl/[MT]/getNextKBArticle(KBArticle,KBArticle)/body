{
  KBArticle firstChildKBArticle=kbArticlePersistence.fetchByG_P_S_First(kbArticle.getGroupId(),kbArticle.getResourcePrimKey(),WorkflowConstants.STATUS_APPROVED,new KBArticlePriorityComparator(true));
  if (firstChildKBArticle != null) {
    return firstChildKBArticle;
  }
  return getNextAncestorKBArticle(kbArticle.getKbArticleId(),nextKBArticle);
}
