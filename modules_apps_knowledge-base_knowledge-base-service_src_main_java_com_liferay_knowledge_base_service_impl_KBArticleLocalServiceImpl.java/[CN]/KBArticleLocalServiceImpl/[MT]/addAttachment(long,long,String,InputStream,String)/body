{
  KBArticle kbArticle=kbArticleLocalService.getLatestKBArticle(resourcePrimKey,WorkflowConstants.STATUS_ANY);
  portletFileRepository.addPortletFileEntry(kbArticle.getGroupId(),userId,KBArticle.class.getName(),kbArticle.getClassPK(),PortletKeys.KNOWLEDGE_BASE_ARTICLE,kbArticle.getAttachmentsFolderId(),inputStream,fileName,mimeType,false);
}
