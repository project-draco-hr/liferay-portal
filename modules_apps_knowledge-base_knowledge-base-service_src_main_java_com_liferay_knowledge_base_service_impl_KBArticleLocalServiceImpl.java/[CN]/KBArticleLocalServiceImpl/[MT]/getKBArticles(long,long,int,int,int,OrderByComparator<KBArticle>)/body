{
  if (status == WorkflowConstants.STATUS_ANY) {
    return kbArticlePersistence.findByG_P_L(groupId,parentResourcePrimKey,true,start,end,orderByComparator);
  }
 else   if (status == WorkflowConstants.STATUS_APPROVED) {
    return kbArticlePersistence.findByG_P_M(groupId,parentResourcePrimKey,true,start,end,orderByComparator);
  }
  return kbArticlePersistence.findByG_P_S(groupId,parentResourcePrimKey,status,start,end,orderByComparator);
}
