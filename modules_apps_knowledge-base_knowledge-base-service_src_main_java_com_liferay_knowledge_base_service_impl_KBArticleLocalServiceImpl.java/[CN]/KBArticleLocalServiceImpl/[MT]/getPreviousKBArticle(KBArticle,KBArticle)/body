{
  if (previousKBArticle == null) {
    return kbArticle.getParentKBArticle();
  }
  KBArticle lastSiblingChildKBArticle=kbArticlePersistence.fetchByG_P_S_Last(kbArticle.getGroupId(),previousKBArticle.getResourcePrimKey(),WorkflowConstants.STATUS_APPROVED,new KBArticlePriorityComparator(true));
  if (lastSiblingChildKBArticle == null) {
    return previousKBArticle;
  }
  return lastSiblingChildKBArticle;
}
