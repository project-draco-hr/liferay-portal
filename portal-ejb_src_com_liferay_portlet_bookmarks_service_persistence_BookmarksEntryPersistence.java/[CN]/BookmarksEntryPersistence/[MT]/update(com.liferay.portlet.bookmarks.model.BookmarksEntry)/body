{
  Session session=null;
  try {
    session=openSession();
    session.saveOrUpdate(bookmarksEntry);
    session.flush();
    bookmarksEntry.setNew(false);
    return bookmarksEntry;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
