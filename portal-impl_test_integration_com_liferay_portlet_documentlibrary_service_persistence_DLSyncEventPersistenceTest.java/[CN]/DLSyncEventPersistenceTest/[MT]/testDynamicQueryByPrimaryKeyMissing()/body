{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLSyncEvent.class,DLSyncEvent.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("syncEventId",ServiceTestUtil.nextLong()));
  List<DLSyncEvent> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
