{
  JournalSyndicatedFeed journalSyndicatedFeed=fetchByPrimaryKey(id);
  if (journalSyndicatedFeed == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No JournalSyndicatedFeed exists with the primary key " + id);
    }
    throw new NoSuchSyndicatedFeedException("No JournalSyndicatedFeed exists with the primary key " + id);
  }
  return journalSyndicatedFeed;
}
