{
  Dictionary<String,String> headers=bundleStorage.getHeaders();
  String configurationPath=headers.get("ConfigurationPath");
  if (configurationPath == null) {
    return null;
  }
  final Enumeration<URL> entries=bundleStorage.findEntries(configurationPath,"*",true);
  return ListUtil.fromEnumeration(new MappingEnumeration<>(entries,new Mapper<URL,NamedConfigurationContent>(){
    @Override public NamedConfigurationContent map(    URL url){
      return new PropertiesFileNamedConfigurationContent(url);
    }
  }
));
}
