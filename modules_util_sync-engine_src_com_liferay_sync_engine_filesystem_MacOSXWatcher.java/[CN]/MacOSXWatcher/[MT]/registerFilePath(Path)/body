{
  if (!filePath.equals(getFilePath())) {
    return;
  }
  WatchableFile watchableFile=new WatchableFile(filePath.toFile());
  watchableFile.register(_watchService,StandardWatchEventKind.ENTRY_CREATE,StandardWatchEventKind.ENTRY_DELETE,StandardWatchEventKind.ENTRY_MODIFY);
  if (_logger.isTraceEnabled()) {
    _logger.trace("Registered file path {}",filePath);
  }
}
