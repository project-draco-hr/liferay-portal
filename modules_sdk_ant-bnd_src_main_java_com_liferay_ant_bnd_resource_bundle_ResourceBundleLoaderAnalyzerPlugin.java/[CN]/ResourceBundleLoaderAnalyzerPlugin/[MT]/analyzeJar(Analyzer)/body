{
  Jar jar=analyzer.getJar();
  if (!jar.exists("content/Language.properties")) {
    return false;
  }
  Parameters provideCapabilityHeaders=new Parameters(analyzer.getProperty(Constants.PROVIDE_CAPABILITY));
  Parameters parameters=new Parameters();
  Attrs attrs=new Attrs();
  attrs.put("baseName","content.Language");
  parameters.add("liferay.resource.bundle",attrs);
  provideCapabilityHeaders.mergeWith(parameters,false);
  analyzer.setProperty(Constants.PROVIDE_CAPABILITY,provideCapabilityHeaders.toString());
  return true;
}
