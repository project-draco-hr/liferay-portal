{
  LayoutSetPrototype layoutSetPrototype=fetchByPrimaryKey(layoutSetPrototypeId);
  if (layoutSetPrototype == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No LayoutSetPrototype exists with the primary key " + layoutSetPrototypeId);
    }
    throw new NoSuchLayoutSetPrototypeException("No LayoutSetPrototype exists with the primary key " + layoutSetPrototypeId);
  }
  return layoutSetPrototype;
}
