{
  LayoutSetPrototype layoutSetPrototype=fetchByPrimaryKey(layoutSetPrototypeId);
  if (layoutSetPrototype == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + layoutSetPrototypeId);
    }
    throw new NoSuchLayoutSetPrototypeException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + layoutSetPrototypeId);
  }
  return layoutSetPrototype;
}
