{
  Object[] finderArgs=new Object[]{new Long(companyId),Boolean.valueOf(active)};
  List<LayoutSetPrototype> list=(List<LayoutSetPrototype>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_C_A,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBundler query=new StringBundler();
      query.append(_SQL_SELECT_LAYOUTSETPROTOTYPE_WHERE);
      query.append("layoutSetPrototype.companyId = ?");
      query.append(" AND ");
      query.append("layoutSetPrototype.active = ?");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(companyId);
      qPos.add(active);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<LayoutSetPrototype>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_C_A,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
