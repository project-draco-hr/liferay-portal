{
  EntityCacheUtil.removeResult(ResourceBlockPermissionModelImpl.ENTITY_CACHE_ENABLED,ResourceBlockPermissionImpl.class,resourceBlockPermission.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  clearUniqueFindersCache((ResourceBlockPermissionModelImpl)resourceBlockPermission);
}
