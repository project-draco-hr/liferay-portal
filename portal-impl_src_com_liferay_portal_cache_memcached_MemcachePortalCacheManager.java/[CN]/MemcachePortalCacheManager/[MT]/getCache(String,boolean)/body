{
  PortalCache cache=_cacheMap.get(name);
  if (cache == null) {
    try {
      MemcachedClientIF memcachedClient=_memcachedClientFactory.getMemcachedClient();
      cache=new MemcachePortalCache(memcachedClient,_timeout,_timeoutTimeUnit);
      _cacheMap.put(name,cache);
    }
 catch (    Exception e) {
      throw new IllegalStateException("Unable to initiatlize memcache connection",e);
    }
  }
  return cache;
}
