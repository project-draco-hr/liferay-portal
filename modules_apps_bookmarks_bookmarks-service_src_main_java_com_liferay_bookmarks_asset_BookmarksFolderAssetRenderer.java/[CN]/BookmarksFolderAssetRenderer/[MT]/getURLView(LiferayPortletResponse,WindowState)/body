{
  AssetRendererFactory<BookmarksFolder> assetRendererFactory=getAssetRendererFactory();
  PortletURL portletURL=assetRendererFactory.getURLView(liferayPortletResponse,windowState);
  portletURL.setParameter("mvcRenderCommandName","/bookmarks/view");
  portletURL.setParameter("folderId",String.valueOf(_folder.getFolderId()));
  portletURL.setWindowState(windowState);
  return portletURL;
}
