{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps=connection.prepareStatement("select fileEntryId, folderId from DLFileEntry");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long fileEntryId=rs.getLong("fileEntryId");
      long folderId=rs.getLong("folderId");
      StringBundler sb=new StringBundler(4);
      sb.append("update DLFileVersion set folderId = ");
      sb.append(folderId);
      sb.append(" where fileEntryId = ");
      sb.append(fileEntryId);
      runSQL(sb.toString());
    }
  }
 }
