{
  PreparedStatement ps=null;
  try {
    ps=connection.prepareStatement("insert into DLSync (syncId, companyId, createDate, " + "modifiedDate, fileId, repositoryId, parentFolderId, " + "event, type_) values (?, ?, ?, ?, ?, ?, ?, ?, ?)");
    ps.setLong(1,syncId);
    ps.setLong(2,companyId);
    ps.setTimestamp(3,createDate);
    ps.setTimestamp(4,createDate);
    ps.setLong(5,fileId);
    ps.setLong(6,repositoryId);
    ps.setLong(7,parentFolderId);
    ps.setString(8,event);
    ps.setString(9,type);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(ps);
  }
}
