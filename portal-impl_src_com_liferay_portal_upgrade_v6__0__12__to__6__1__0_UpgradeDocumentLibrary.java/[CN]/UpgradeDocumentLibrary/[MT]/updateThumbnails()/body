{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select fileEntryId from DLFileEntry");
    rs=ps.executeQuery();
    while (rs.next()) {
      long fileEntryId=rs.getLong("fileEntryId");
      updateThumbnails(fileEntryId);
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
