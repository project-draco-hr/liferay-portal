{
  MBStatsUser mbStatsUser=findByPrimaryKey(statsUserId);
  Session session=null;
  try {
    session=openSession();
    MBStatsUser[] array=new MBStatsUserImpl[3];
    array[0]=getByG_M_PrevAndNext(session,mbStatsUser,groupId,messageCount,orderByComparator,true);
    array[1]=mbStatsUser;
    array[2]=getByG_M_PrevAndNext(session,mbStatsUser,groupId,messageCount,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
