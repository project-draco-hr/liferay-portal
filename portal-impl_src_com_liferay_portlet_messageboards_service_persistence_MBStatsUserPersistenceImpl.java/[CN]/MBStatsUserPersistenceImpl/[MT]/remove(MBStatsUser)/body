{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(mbStatsUser);
    }
  }
  mbStatsUser=removeImpl(mbStatsUser);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(mbStatsUser);
    }
  }
  return mbStatsUser;
}
