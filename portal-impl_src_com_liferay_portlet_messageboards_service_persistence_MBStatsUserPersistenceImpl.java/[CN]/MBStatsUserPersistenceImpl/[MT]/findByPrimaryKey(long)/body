{
  MBStatsUser mbStatsUser=fetchByPrimaryKey(statsUserId);
  if (mbStatsUser == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No MBStatsUser exists with the primary key " + statsUserId);
    }
    throw new NoSuchStatsUserException("No MBStatsUser exists with the primary key " + statsUserId);
  }
  return mbStatsUser;
}
