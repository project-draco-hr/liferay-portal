{
  Session session=null;
  try {
    session=openSession();
    MBStatsUser mbStatsUser=(MBStatsUser)session.get(MBStatsUserImpl.class,new Long(statsUserId));
    if (mbStatsUser == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + statsUserId);
      }
      throw new NoSuchStatsUserException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + statsUserId);
    }
    return remove(mbStatsUser);
  }
 catch (  NoSuchStatsUserException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
