{
  EntityCacheUtil.putResult(MBStatsUserModelImpl.ENTITY_CACHE_ENABLED,MBStatsUserImpl.class,mbStatsUser.getPrimaryKey(),mbStatsUser);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_U,new Object[]{mbStatsUser.getGroupId(),mbStatsUser.getUserId()},mbStatsUser);
  mbStatsUser.resetOriginalValues();
}
