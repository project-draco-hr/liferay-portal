{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,mbStatsUser,merge);
    mbStatsUser.setNew(false);
    return mbStatsUser;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(MBStatsUser.class.getName());
  }
}
