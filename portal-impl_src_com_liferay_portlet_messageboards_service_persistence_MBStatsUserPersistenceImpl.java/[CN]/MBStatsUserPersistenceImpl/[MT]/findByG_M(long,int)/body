{
  int count=countByG_M(groupId,messageCount);
  if (count == 0) {
    return Collections.EMPTY_LIST;
  }
  Object[] finderArgs=new Object[]{new Long(groupId),new Integer(messageCount)};
  List<MBStatsUser> list=(List<MBStatsUser>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_G_M,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portlet.messageboards.model.MBStatsUser WHERE ");
      query.append("groupId = ?");
      query.append(" AND ");
      query.append("messageCount != ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("messageCount DESC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      qPos.add(messageCount);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<MBStatsUser>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_G_M,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
