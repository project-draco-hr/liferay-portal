{
  if (type1.equals(type2)) {
    return type1;
  }
  List<String> list1=new ArrayList<String>();
  list1.add(type1);
  List<String> list2=new ArrayList<String>();
  list2.add(type2);
  while (true) {
    type1=getSuperName(type1);
    if (type1 != null) {
      if (list2.contains(type1)) {
        return type1;
      }
      list1.add(type1);
    }
    type2=getSuperName(type2);
    if (type2 != null) {
      if (list1.contains(type2)) {
        return type2;
      }
      list2.add(type2);
    }
  }
}
