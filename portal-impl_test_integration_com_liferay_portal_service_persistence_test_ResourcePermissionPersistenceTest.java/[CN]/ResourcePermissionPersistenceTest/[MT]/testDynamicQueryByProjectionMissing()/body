{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ResourcePermission.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("resourcePermissionId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("resourcePermissionId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
