{
  if (_ratingsDataTransformer == null) {
    return;
  }
  for (  String portletId : RatingsDataTransformerHelperUtil.getPortletIds()) {
    String[] classNames=RatingsDataTransformerHelperUtil.getClassNames(portletId);
    for (    final String className : classNames) {
      String propertyName=_getPropertyName(className);
      transformRatingsData("companyId",companyId,className,oldPortletPreferences.getValue(propertyName,StringPool.BLANK),properties.getProperty(propertyName));
    }
  }
}
