{
  URLConnection urlConnection=null;
  try {
    urlConnection=_templateContent.openConnection();
    long lastModified=urlConnection.getLastModified();
    return lastModified;
  }
 catch (  Exception e) {
    _log.error("Unable to get last modified time for template " + _templateId,e);
    return 0;
  }
 finally {
    try {
      urlConnection.getInputStream().close();
    }
 catch (    Exception e) {
    }
 finally {
      urlConnection=null;
    }
  }
}
