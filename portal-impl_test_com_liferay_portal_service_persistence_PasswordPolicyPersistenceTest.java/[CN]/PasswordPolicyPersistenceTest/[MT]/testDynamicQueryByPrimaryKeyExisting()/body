{
  PasswordPolicy newPasswordPolicy=addPasswordPolicy();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PasswordPolicy.class,PasswordPolicy.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("passwordPolicyId",newPasswordPolicy.getPasswordPolicyId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  PasswordPolicy existingPasswordPolicy=(PasswordPolicy)result.get(0);
  assertEquals(existingPasswordPolicy,newPasswordPolicy);
}
