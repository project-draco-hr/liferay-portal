{
  Dictionary<String,Object> properties=new HashMapDictionary<>();
  properties.put(HttpWhiteboardConstants.HTTP_WHITEBOARD_CONTEXT_SELECT,contextName);
  properties.put(HttpWhiteboardConstants.HTTP_WHITEBOARD_RESOURCE_PREFIX,"/META-INF/resources");
  properties.put(HttpWhiteboardConstants.HTTP_WHITEBOARD_SERVLET_PATTERN,"/*");
  return bundleContext.registerService(Servlet.class,new HttpServlet(){
  }
,properties);
}
