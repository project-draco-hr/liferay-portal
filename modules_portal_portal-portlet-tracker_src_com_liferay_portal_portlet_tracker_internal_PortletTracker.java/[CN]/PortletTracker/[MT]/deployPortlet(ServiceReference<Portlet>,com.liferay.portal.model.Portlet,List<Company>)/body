{
  String categoryName=(String)serviceReference.getProperty("com.liferay.display.category");
  if (categoryName == null) {
    categoryName="category.undefined";
  }
  for (  Company company : companies) {
    com.liferay.portal.model.Portlet curPortletModel=(com.liferay.portal.model.Portlet)portletModel.clone();
    curPortletModel.setCompanyId(company.getCompanyId());
    _portletLocalService.deployRemotePortlet(curPortletModel,categoryName);
  }
}
