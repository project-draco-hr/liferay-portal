{
  if (configuration == null) {
    return;
  }
  Properties properties=configuration.getProperties();
  String[] resourceActionConfigs=StringUtil.split(properties.getProperty(PropsKeys.RESOURCE_ACTIONS_CONFIGS));
  for (  String resourceActionConfig : resourceActionConfigs) {
    try {
      ResourceActionsUtil.read(servletContextName,classLoader,resourceActionConfig);
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
}
