{
  if (serviceRegistrations.resource != null) {
    return;
  }
  Enumeration<URL> urls=bundle.findEntries("/META-INF/resources","*.*",true);
  if ((urls == null) || !urls.hasMoreElements()) {
    return;
  }
  BundleContext bundleContext=bundle.getBundleContext();
  createStaticResourceServlet(portletModel.getPortletApp(),bundleContext,serviceRegistrations);
  urls=bundle.findEntries("/META-INF/resources","*.jsp",true);
  if ((urls == null) || !urls.hasMoreElements()) {
    return;
  }
  createJspServlet(portletModel.getPortletApp(),bundleContext,serviceRegistrations);
}
