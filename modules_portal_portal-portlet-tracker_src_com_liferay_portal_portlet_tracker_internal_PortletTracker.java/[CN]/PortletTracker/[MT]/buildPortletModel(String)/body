{
  com.liferay.portal.model.Portlet portalPortletModel=_portletLocalService.getPortletById(CompanyConstants.SYSTEM,PortletKeys.PORTAL);
  com.liferay.portal.model.Portlet portletModel=new PortletImpl(CompanyConstants.SYSTEM,portletId);
  portletModel.setPluginPackage(portalPortletModel.getPluginPackage());
  portletModel.setPortletApp(portalPortletModel.getPortletApp());
  portletModel.setRoleMappers(portalPortletModel.getRoleMappers());
  return portletModel;
}
