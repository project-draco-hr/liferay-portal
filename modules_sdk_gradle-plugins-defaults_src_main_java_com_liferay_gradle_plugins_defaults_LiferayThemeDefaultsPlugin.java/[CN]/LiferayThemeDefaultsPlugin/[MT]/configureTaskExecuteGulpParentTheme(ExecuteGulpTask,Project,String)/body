{
  if (themeProject == null) {
    Project project=executeGulpTask.getProject();
    Logger logger=project.getLogger();
    if (logger.isWarnEnabled()) {
      logger.warn("Unable to configure {} parent theme",name);
    }
    return;
  }
  File dir=themeProject.file("src/main/resources/META-INF/resources/_" + name);
  executeGulpTask.args("--" + name + "-path="+ FileUtil.getAbsolutePath(dir));
  executeGulpTask.dependsOn(themeProject.getPath() + ":" + JavaPlugin.CLASSES_TASK_NAME);
}
