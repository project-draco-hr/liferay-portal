{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Sample LAR Test Page");
  selenium.clickAt("link=Sample LAR Test Page",RuntimeVariables.replace("Sample LAR Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("This is the Sample LAR Portlet. This was made to demonstrate the portlet LAR plugin feature."));
  selenium.clickAt("//strong/a",RuntimeVariables.replace("Options"));
  selenium.waitForVisible("link=Export / Import");
  selenium.click(RuntimeVariables.replace("link=Export / Import"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("//input[@id='_86_exportFileName']"));
  assertTrue(selenium.isElementPresent("//input[@value='Export']"));
  selenium.click(RuntimeVariables.replace("link=Import"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("//input[@id='_86_importFileName']"));
  assertTrue(selenium.isElementPresent("//input[@value='Import']"));
}
