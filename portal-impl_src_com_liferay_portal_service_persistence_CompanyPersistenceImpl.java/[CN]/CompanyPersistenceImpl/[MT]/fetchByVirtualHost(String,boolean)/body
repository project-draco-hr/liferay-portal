{
  Object[] finderArgs=new Object[]{virtualHost};
  Object result=null;
  if (retrieveFromCache) {
    result=FinderCacheUtil.getResult(FINDER_PATH_FETCH_BY_VIRTUALHOST,finderArgs,this);
  }
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringBundler query=new StringBundler();
      query.append(_SQL_SELECT_COMPANY_WHERE);
      if (virtualHost == null) {
        query.append("company.virtualHost IS NULL");
      }
 else {
        if (virtualHost.equals(StringPool.BLANK)) {
          query.append("(company.virtualHost IS NULL OR ");
        }
        query.append("company.virtualHost = ?");
        if (virtualHost.equals(StringPool.BLANK)) {
          query.append(")");
        }
      }
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      if (virtualHost != null) {
        qPos.add(virtualHost);
      }
      List<Company> list=q.list();
      result=list;
      Company company=null;
      if (list.isEmpty()) {
        FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_VIRTUALHOST,finderArgs,list);
      }
 else {
        company=list.get(0);
        cacheResult(company);
        if ((company.getVirtualHost() == null) || !company.getVirtualHost().equals(virtualHost)) {
          FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_VIRTUALHOST,finderArgs,company);
        }
      }
      return company;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (result == null) {
        FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_VIRTUALHOST,finderArgs,new ArrayList<Company>());
      }
      closeSession(session);
    }
  }
 else {
    if (result instanceof List<?>) {
      return null;
    }
 else {
      return (Company)result;
    }
  }
}
