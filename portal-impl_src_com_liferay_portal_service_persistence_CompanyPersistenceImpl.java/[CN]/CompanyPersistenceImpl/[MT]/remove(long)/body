{
  Session session=null;
  try {
    session=openSession();
    Company company=(Company)session.get(CompanyImpl.class,new Long(companyId));
    if (company == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + companyId);
      }
      throw new NoSuchCompanyException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + companyId);
    }
    return remove(company);
  }
 catch (  NoSuchCompanyException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
