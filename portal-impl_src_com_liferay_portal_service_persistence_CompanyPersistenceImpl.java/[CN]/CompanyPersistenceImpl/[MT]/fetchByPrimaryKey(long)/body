{
  Company company=(Company)EntityCacheUtil.getResult(CompanyModelImpl.ENTITY_CACHE_ENABLED,CompanyImpl.class,companyId);
  if (company == _nullCompany) {
    return null;
  }
  if (company == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      company=(Company)session.get(CompanyImpl.class,Long.valueOf(companyId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (company != null) {
        cacheResult(company);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(CompanyModelImpl.ENTITY_CACHE_ENABLED,CompanyImpl.class,companyId,_nullCompany);
      }
      closeSession(session);
    }
  }
  return company;
}
