{
  Company result=(Company)EntityCacheUtil.getResult(CompanyModelImpl.ENTITY_CACHE_ENABLED,CompanyImpl.class,companyId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      Company company=(Company)session.get(CompanyImpl.class,new Long(companyId));
      if (company != null) {
        cacheResult(company);
      }
      return company;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (Company)result;
  }
}
