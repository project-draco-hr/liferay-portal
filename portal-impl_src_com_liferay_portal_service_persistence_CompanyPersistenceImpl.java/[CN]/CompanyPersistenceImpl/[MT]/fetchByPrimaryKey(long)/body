{
  Company company=(Company)EntityCacheUtil.getResult(CompanyModelImpl.ENTITY_CACHE_ENABLED,CompanyImpl.class,companyId,this);
  if (company == null) {
    Session session=null;
    try {
      session=openSession();
      company=(Company)session.get(CompanyImpl.class,new Long(companyId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (company != null) {
        cacheResult(company);
      }
      closeSession(session);
    }
  }
  return company;
}
