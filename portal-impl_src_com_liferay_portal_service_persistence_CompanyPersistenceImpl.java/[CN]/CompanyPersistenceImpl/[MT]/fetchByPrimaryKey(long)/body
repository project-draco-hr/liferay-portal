{
  Company company=(Company)EntityCacheUtil.getResult(CompanyModelImpl.ENTITY_CACHE_ENABLED,CompanyImpl.class,companyId,this);
  if (company == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (company == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      company=(Company)session.get(CompanyImpl.class,Long.valueOf(companyId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (company == null)) {
        EntityCacheUtil.putResult(CompanyModelImpl.ENTITY_CACHE_ENABLED,CompanyImpl.class,companyId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(company);
      }
      closeSession(session);
    }
  }
  return company;
}
