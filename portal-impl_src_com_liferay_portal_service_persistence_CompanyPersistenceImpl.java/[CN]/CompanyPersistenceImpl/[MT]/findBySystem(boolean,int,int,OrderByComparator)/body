{
  Object[] finderArgs=new Object[]{Boolean.valueOf(system),String.valueOf(start),String.valueOf(end),String.valueOf(obc)};
  List<Company> list=(List<Company>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_OBC_SYSTEM,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portal.model.Company WHERE ");
      query.append("system = ?");
      query.append(" ");
      if (obc != null) {
        query.append("ORDER BY ");
        query.append(obc.getOrderBy());
      }
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(system);
      list=(List<Company>)QueryUtil.list(q,getDialect(),start,end);
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<Company>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_OBC_SYSTEM,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
