{
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(company);
    }
 else {
      if (company.isNew()) {
        session.save(company);
      }
    }
    session.flush();
    company.setNew(false);
    return company;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Company.class.getName());
  }
}
