{
  DesiredCapabilities desiredCapabilities=DesiredCapabilities.iphone();
  desiredCapabilities.setCapability("browserName","Safari");
  desiredCapabilities.setCapability("deviceName","iPhone 5s");
  desiredCapabilities.setCapability("newCommandTimeout",PropsValues.TIMEOUT_EXPLICIT_WAIT);
  desiredCapabilities.setCapability("platformName","iOS");
  desiredCapabilities.setCapability("platformVersion",PropsValues.BROWSER_VERSION);
  URL url=null;
  try {
    url=new URL("http://0.0.0.0:4723/wd/hub/");
  }
 catch (  Exception e) {
  }
  return new IOSDriver(url,desiredCapabilities);
}
