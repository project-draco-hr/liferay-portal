{
  Layout layout=(Layout)req.getAttribute(WebKeys.LAYOUT);
  String resourceId=ParamUtil.getString(req,"resourceId");
  String userGroupId=ParamUtil.getString(req,"userGroupIdsPosValue");
  String[] actionIds=StringUtil.split(ParamUtil.getString(req,"userGroupIdActionIds"));
  PermissionServiceUtil.setGroupPermissions(UserGroup.class.getName(),userGroupId,layout.getGroupId(),actionIds,resourceId);
}
