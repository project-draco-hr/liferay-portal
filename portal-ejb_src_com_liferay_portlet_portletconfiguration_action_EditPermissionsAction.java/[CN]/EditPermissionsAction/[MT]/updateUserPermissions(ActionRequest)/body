{
  ThemeDisplay themeDisplay=(ThemeDisplay)req.getAttribute(WebKeys.THEME_DISPLAY);
  long resourceId=ParamUtil.getLong(req,"resourceId");
  long userId=ParamUtil.getLong(req,"userIdsPosValue");
  String[] actionIds=StringUtil.split(ParamUtil.getString(req,"userIdActionIds"));
  PermissionServiceUtil.setUserPermissions(userId,themeDisplay.getPortletGroupId(),actionIds,resourceId);
}
