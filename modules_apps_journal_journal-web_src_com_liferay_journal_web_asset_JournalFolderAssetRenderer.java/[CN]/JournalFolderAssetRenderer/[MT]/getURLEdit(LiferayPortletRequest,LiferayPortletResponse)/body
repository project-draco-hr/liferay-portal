{
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),JournalPortletKeys.JOURNAL,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/edit_folder.jsp");
  portletURL.setParameter("folderId",String.valueOf(_folder.getFolderId()));
  return portletURL;
}
