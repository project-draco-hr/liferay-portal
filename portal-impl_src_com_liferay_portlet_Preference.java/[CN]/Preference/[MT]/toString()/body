{
  StringBundler sb=new StringBundler(6 + (_values.length * 2));
  sb.append("{name=");
  sb.append(getName());
  sb.append(",values=[");
  for (int i=0; i < _values.length; i++) {
    sb.append(_values[i]);
    if (i < _values.length - 1) {
      sb.append(StringPool.COMMA);
    }
  }
  sb.append("],readOnly=");
  sb.append(_readOnly);
  sb.append("}");
  return sb.toString();
}
