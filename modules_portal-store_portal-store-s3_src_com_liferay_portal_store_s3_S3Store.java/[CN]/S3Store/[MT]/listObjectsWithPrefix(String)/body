{
  ListObjectsRequest request=new ListObjectsRequest();
  request.withBucketName(_bucketName);
  request.withPrefix(prefix);
  try {
    ObjectListing objectListing=_amazonS3.listObjects(request);
    List<S3ObjectSummary> summaries=new ArrayList<>(objectListing.getMaxKeys());
    do {
      List<S3ObjectSummary> batchSummary=objectListing.getObjectSummaries();
      summaries.addAll(batchSummary);
    }
 while (objectListing.isTruncated());
    return summaries;
  }
 catch (  AmazonClientException ae) {
    throw convertAWSException(ae);
  }
}
