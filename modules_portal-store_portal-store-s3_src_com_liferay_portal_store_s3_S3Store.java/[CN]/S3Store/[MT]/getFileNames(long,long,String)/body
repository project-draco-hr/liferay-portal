{
  try {
    S3Object[] s3Objects=_s3Service.listObjects(_s3Bucket.getName(),getKey(companyId,repositoryId,dirName),null);
    return getFileNames(s3Objects);
  }
 catch (  S3ServiceException s3se) {
    if (isS3NoSuchKeyException(s3se)) {
      return new String[0];
    }
    throw new SystemException(s3se);
  }
}
