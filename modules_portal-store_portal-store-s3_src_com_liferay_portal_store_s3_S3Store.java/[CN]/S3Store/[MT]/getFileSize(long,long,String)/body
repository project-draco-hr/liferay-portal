{
  String version=getHeadVersionLabel(companyId,repositoryId,fileName);
  String key=_s3KeyTransformer.getFileVersionKey(companyId,repositoryId,fileName,version);
  GetObjectMetadataRequest request=new GetObjectMetadataRequest(_bucketName,key);
  ObjectMetadata objectMetadata=_amazonS3.getObjectMetadata(request);
  if (objectMetadata == null) {
    throw new NoSuchFileException(companyId,repositoryId,fileName);
  }
  return objectMetadata.getContentLength();
}
