{
  String headVersion=getHeadVersionLabel(companyId,repositoryId,fileName);
  String fileVersionKey=_s3KeyTransformer.getFileVersionKey(companyId,repositoryId,fileName,headVersion);
  GetObjectMetadataRequest getObjectMetadataRequest=new GetObjectMetadataRequest(_bucketName,fileVersionKey);
  ObjectMetadata objectMetadata=_amazonS3.getObjectMetadata(getObjectMetadataRequest);
  if (objectMetadata == null) {
    throw new NoSuchFileException(companyId,repositoryId,fileName);
  }
  return objectMetadata.getContentLength();
}
