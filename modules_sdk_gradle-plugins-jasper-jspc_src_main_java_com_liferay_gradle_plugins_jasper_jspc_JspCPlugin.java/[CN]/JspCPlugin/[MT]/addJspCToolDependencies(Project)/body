{
  JspCExtension jspCExtension=GradleUtil.getExtension(project,JspCExtension.class);
  GradleUtil.addDependency(project,TOOL_CONFIGURATION_NAME,"org.apache.ant","ant",jspCExtension.getAntVersion());
  GradleUtil.addDependency(project,TOOL_CONFIGURATION_NAME,"com.liferay","com.liferay.jasper.jspc",jspCExtension.getJspCVersion());
}
