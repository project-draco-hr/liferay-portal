{
  HttpSession ses=req.getSession();
  if (plid == null) {
    return new HashMap();
  }
  String rppId=plid + RenderParametersPool.class.getName() + portletId;
  Map params=(Map)_getRenderParametersPool(ses).get(rppId);
  if (params == null) {
    params=new HashMap();
    _getRenderParametersPool(ses).put(rppId,params);
  }
  return params;
}
