{
  Object[] finderArgs=new Object[]{new Long(nodeId),String.valueOf(start),String.valueOf(end),String.valueOf(obc)};
  List<WikiPage> list=(List<WikiPage>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_OBC_NODEID,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portlet.wiki.model.WikiPage WHERE ");
      query.append("nodeId = ?");
      query.append(" ");
      if (obc != null) {
        query.append("ORDER BY ");
        query.append(obc.getOrderBy());
      }
 else {
        query.append("ORDER BY ");
        query.append("nodeId ASC, ");
        query.append("title ASC, ");
        query.append("version ASC");
      }
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(nodeId);
      list=(List<WikiPage>)QueryUtil.list(q,getDialect(),start,end);
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<WikiPage>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_OBC_NODEID,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
