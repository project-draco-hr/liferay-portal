{
  Object[] finderArgs=new Object[]{new Long(nodeId),title};
  Object result=FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_N_T,finderArgs,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("FROM com.liferay.portlet.wiki.model.WikiPage WHERE ");
      query.append("nodeId = ?");
      query.append(" AND ");
      if (title == null) {
        query.append("title IS NULL");
      }
 else {
        query.append("title = ?");
      }
      query.append(" ");
      query.append("ORDER BY ");
      query.append("nodeId ASC, ");
      query.append("title ASC, ");
      query.append("version ASC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(nodeId);
      if (title != null) {
        qPos.add(title);
      }
      List<WikiPage> list=q.list();
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_N_T,finderArgs,list);
      cacheResult(list);
      return list;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (List<WikiPage>)result;
  }
}
