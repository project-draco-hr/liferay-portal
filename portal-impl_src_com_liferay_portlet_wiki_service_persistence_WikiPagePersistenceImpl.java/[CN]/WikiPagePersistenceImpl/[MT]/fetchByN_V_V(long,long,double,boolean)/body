{
  Object[] finderArgs=new Object[]{new Long(nodeId),new Long(resourcePrimKey),new Double(version)};
  Object result=null;
  if (retrieveFromCache) {
    result=FinderCacheUtil.getResult(FINDER_PATH_FETCH_BY_N_V_V,finderArgs,this);
  }
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringBundler query=new StringBundler(5);
      query.append(_SQL_SELECT_WIKIPAGE_WHERE);
      query.append(_FINDER_COLUMN_N_V_V_NODEID_2);
      query.append(_FINDER_COLUMN_N_V_V_RESOURCEPRIMKEY_2);
      query.append(_FINDER_COLUMN_N_V_V_VERSION_2);
      query.append(WikiPageModelImpl.ORDER_BY_JPQL);
      String sql=query.toString();
      Query q=session.createQuery(sql);
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(nodeId);
      qPos.add(resourcePrimKey);
      qPos.add(version);
      List<WikiPage> list=q.list();
      result=list;
      WikiPage wikiPage=null;
      if (list.isEmpty()) {
        FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_N_V_V,finderArgs,list);
      }
 else {
        wikiPage=list.get(0);
        cacheResult(wikiPage);
        if ((wikiPage.getNodeId() != nodeId) || (wikiPage.getResourcePrimKey() != resourcePrimKey) || (wikiPage.getVersion() != version)) {
          FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_N_V_V,finderArgs,wikiPage);
        }
      }
      return wikiPage;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (result == null) {
        FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_N_V_V,finderArgs,new ArrayList<WikiPage>());
      }
      closeSession(session);
    }
  }
 else {
    if (result instanceof List<?>) {
      return null;
    }
 else {
      return (WikiPage)result;
    }
  }
}
