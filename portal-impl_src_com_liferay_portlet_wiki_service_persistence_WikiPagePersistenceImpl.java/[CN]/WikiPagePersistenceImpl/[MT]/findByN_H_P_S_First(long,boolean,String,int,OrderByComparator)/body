{
  List<WikiPage> list=findByN_H_P_S(nodeId,head,parentTitle,status,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(10);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("nodeId=");
    msg.append(nodeId);
    msg.append(", head=");
    msg.append(head);
    msg.append(", parentTitle=");
    msg.append(parentTitle);
    msg.append(", status=");
    msg.append(status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchPageException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
