{
  List<WikiPage> list=findByNodeId(nodeId,0,1,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(4);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("nodeId=");
    msg.append(nodeId);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchPageException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
