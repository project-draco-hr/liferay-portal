{
  String userName=GetterUtil.getString(portletPreferences.getValue("userName",null));
  if (Validator.isNull(userName)) {
    userName=GetterUtil.getString(portletPreferences.getValue("user-name",null));
  }
  if (Validator.isNotNull(userName)) {
    portletPreferences.setValue("formUserName",userName);
  }
  portletPreferences.reset("userName");
  portletPreferences.reset("user-name");
}
