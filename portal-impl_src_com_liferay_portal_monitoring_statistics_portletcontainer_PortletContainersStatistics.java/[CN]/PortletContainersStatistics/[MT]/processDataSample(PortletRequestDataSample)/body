{
  long companyId=dataSample.getCompanyId();
  CompanyPortletContainerStatistics statistics=_companyIdToContainer.get(companyId);
  if (statistics == null) {
    try {
      Company company=_companyLocalService.getCompany(companyId);
      statistics=register(company.getWebId());
    }
 catch (    Exception e) {
      throw new IllegalStateException("Unable to get company defails for: " + companyId);
    }
  }
  statistics.processDataSample(dataSample);
}
