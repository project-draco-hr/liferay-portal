{
  Matcher matcher=pattern.matcher(content);
  if (!matcher.find()) {
    return;
  }
  String imports=matcher.group(2);
  matcher=_wilcardImportPattern.matcher(imports);
  while (matcher.find()) {
    processErrorMessage(fileName,"Don't use wildcard in Export-Package '" + matcher.group(1) + "': "+ fileName);
  }
}
