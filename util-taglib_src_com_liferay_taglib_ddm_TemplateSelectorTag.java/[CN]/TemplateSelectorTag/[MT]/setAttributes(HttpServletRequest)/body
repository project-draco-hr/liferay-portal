{
  request.setAttribute("liferay-ui:template-select:classNameId",String.valueOf(PortalUtil.getClassNameId(_className)));
  request.setAttribute("liferay-ui:template-select:displayStyle",getDisplayStyle());
  request.setAttribute("liferay-ui:template-select:displayStyleGroupId",String.valueOf(getDisplayStyleGroupId()));
  request.setAttribute("liferay-ui:template-select:displayStyles",_displayStyles);
  request.setAttribute("liferay-ui:template-select:icon",_icon);
  request.setAttribute("liferay-ui:template-select:label",_label);
  request.setAttribute("liferay-ui:template-select:portletDisplayDDMTemplate",getPortletDisplayDDMTemplate());
  request.setAttribute("liferay-ui:template-select:refreshURL",_refreshURL);
  request.setAttribute("liferay-ui:template-select:showEmptyOption",String.valueOf(_showEmptyOption));
}
