{
  LiferayPortletRequest liferayPortletRequest=(LiferayPortletRequest)pageContext.getAttribute("liferayPortletRequest");
  LiferayPortletResponse liferayPortletResponse=(LiferayPortletResponse)pageContext.getAttribute("liferayPortletResponse");
  if ((liferayPortletRequest == null) || (liferayPortletResponse == null)) {
    return null;
  }
  return PortletURLUtil.getCurrent(liferayPortletRequest,liferayPortletResponse);
}
