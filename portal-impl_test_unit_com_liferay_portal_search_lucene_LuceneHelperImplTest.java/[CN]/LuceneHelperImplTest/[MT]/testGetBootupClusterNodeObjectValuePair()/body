{
  _mockClusterExecutor.setNodeNumber(1);
  _mockClusterExecutor.setPort(1);
  _mockClusterExecutor.setPortalInetAddress(_localhostInetAddress);
  Method method=LuceneHelperImpl.class.getDeclaredMethod("_getBootupClusterNodeObjectValuePair",String.class);
  method.setAccessible(true);
  Object object=method.invoke(_luceneHelperImpl,_mockClusterExecutor.getLocalClusterNodeId());
  Assert.assertNotNull(object);
  ObjectValuePair<String,URL> result=(ObjectValuePair<String,URL>)object;
  URL url=result.getValue();
  Assert.assertEquals(_localhostInetAddress.getHostAddress(),url.getHost());
  Assert.assertEquals(1,url.getPort());
}
