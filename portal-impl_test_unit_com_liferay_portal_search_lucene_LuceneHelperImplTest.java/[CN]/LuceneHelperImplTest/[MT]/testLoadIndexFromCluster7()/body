{
  CaptureHandler captureHandler=JDKLoggerTestUtil.configureJDKLogger(LuceneHelperImpl.class.getName(),Level.FINE);
  try {
    List<LogRecord> logRecords=captureHandler.getLogRecords();
    _luceneHelperImpl.loadIndexesFromCluster(0);
    Assert.assertEquals(1,logRecords.size());
    _assertLogger(logRecords.get(0),"Load index from cluster is not enabled",null);
  }
  finally {
    captureHandler.close();
  }
}
