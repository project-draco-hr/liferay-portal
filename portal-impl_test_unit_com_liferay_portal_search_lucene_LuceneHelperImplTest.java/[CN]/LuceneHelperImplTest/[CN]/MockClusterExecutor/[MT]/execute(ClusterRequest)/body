{
  if (!_autoResponse) {
    return new FutureClusterResponses(Collections.<String>emptySet());
  }
  Set<String> clusterNodeIds=_clusterNodes.keySet();
  FutureClusterResponses futureClusterResponses=new FutureClusterResponses(clusterNodeIds);
  for (  ClusterNode clusterNode : _clusterNodes.values()) {
    try {
      clusterNode.setPortalInetSocketAddress(new InetSocketAddress(_portalInetAddress,_port));
      clusterNode.setPortalProtocol(Http.HTTP);
    }
 catch (    IllegalArgumentException iae) {
    }
    try {
      futureClusterResponses.addClusterNodeResponse(ClusterNodeResponse.createResultClusterNodeResponse(clusterNode,ClusterMessageType.EXECUTE,clusterRequest.getUuid(),clusterRequest.isMulticast(),_invoke(clusterRequest.getMethodHandler())));
    }
 catch (    Exception e) {
      futureClusterResponses.addClusterNodeResponse(ClusterNodeResponse.createExceptionClusterNodeResponse(clusterNode,ClusterMessageType.EXECUTE,clusterRequest.getUuid(),clusterRequest.isMulticast(),e));
    }
  }
  return futureClusterResponses;
}
