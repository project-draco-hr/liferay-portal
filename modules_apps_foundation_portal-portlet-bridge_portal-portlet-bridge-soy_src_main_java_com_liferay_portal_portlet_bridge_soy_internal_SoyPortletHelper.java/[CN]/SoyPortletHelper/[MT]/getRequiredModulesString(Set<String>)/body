{
  StringBundler sb=new StringBundler(requiredModules.size() * 4);
  Iterator<String> it=requiredModules.iterator();
  while (it.hasNext()) {
    sb.append(StringPool.QUOTE);
    sb.append(it.next());
    sb.append(StringPool.QUOTE);
    if (it.hasNext()) {
      sb.append(StringPool.COMMA);
    }
  }
  return sb.toString();
}
