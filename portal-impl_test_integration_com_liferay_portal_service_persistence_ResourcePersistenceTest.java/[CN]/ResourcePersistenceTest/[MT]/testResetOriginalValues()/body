{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  Resource newResource=addResource();
  _persistence.clearCache();
  ResourceModelImpl existingResourceModelImpl=(ResourceModelImpl)_persistence.findByPrimaryKey(newResource.getPrimaryKey());
  assertEquals(existingResourceModelImpl.getCodeId(),existingResourceModelImpl.getOriginalCodeId());
  assertTrue(Validator.equals(existingResourceModelImpl.getPrimKey(),existingResourceModelImpl.getOriginalPrimKey()));
}
