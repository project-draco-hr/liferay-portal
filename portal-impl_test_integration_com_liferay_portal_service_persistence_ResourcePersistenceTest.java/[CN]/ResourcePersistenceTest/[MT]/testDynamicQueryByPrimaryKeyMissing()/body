{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Resource.class,Resource.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("resourceId",ServiceTestUtil.nextLong()));
  List<Resource> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
