{
  registerActionClass(FooService.class);
  Map<String,FileItem[]> fileParams=new HashMap<String,FileItem[]>();
  fileParams.put("fileName",null);
  HttpServletRequest httpServletRequest=new UploadServletRequestImpl(createHttpRequest("/foo/add-file"),fileParams,null){
    @Override public String getFileName(    String name){
      return "test";
    }
  }
;
  JSONWebServiceAction jsonWebServiceAction=lookupJSONWebServiceAction(httpServletRequest);
  Assert.assertNotNull(jsonWebServiceAction);
}
