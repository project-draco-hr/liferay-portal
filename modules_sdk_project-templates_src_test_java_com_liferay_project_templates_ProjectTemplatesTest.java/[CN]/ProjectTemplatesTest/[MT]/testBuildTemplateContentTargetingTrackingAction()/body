{
  File gradleProjectDir=_buildTemplateWithGradle("content-targeting-tracking-action","foo-bar");
  _testExists(gradleProjectDir,"bnd.bnd");
  _testContains(gradleProjectDir,"src/main/java/foo/bar/content/targeting/tracking/action" + "/FooBarTrackingAction.java","public class FooBarTrackingAction extends BaseJSPTrackingAction");
