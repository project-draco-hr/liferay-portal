{
  File gradleProjectDir=_buildTemplateWithGradle("contenttargetingtrackingaction","foo-bar");
  File mavenProjectDir=_buildTemplateWithMaven("contenttargetingtrackingaction","foo-bar","-DclassName=FooBar","-Dpackage=foo.bar");
  _testExists(gradleProjectDir,"bnd.bnd");
  _testContains(gradleProjectDir,"src/main/java/foo/bar/content/targeting/tracking/action/" + "FooBarTrackingAction.java","public class FooBarTrackingAction extends BaseJSPTrackingAction");
  _buildProjects(gradleProjectDir,mavenProjectDir,"build/libs/foo.bar-1.0.0.jar","target/foo-bar-1.0.0.jar");
}
