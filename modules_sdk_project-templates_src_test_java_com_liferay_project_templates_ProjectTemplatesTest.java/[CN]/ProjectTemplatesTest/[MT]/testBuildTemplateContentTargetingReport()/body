{
  File projectDir=_buildTemplateWithGradle("contenttargetingreport","foo-bar");
  _testExists(projectDir,"bnd.bnd");
  _testContains(projectDir,"src/main/java/foo/bar/content/targeting/report/FooBarReport.java","public class FooBarReport extends BaseJSPReport");
  _executeGradle(projectDir,_TASK_PATH_BUILD);
  _testExists(projectDir,"build/libs/foo.bar-1.0.0.jar");
  File gradleBundleFile=new File(projectDir,"build/libs/foo.bar-1.0.0.jar");
  File mavenProjectDir=_buildTemplateWithMaven("contenttargetingreport","foo-bar","-Dpackage=foo.bar","-DclassName=FooBar","-DprojectType=standalone");
  _executeMaven(mavenProjectDir,new String[]{_TASK_PATH_PACKAGE});
  _testExists(mavenProjectDir,"target/foo-bar-1.0.0.jar");
  File mavenBundleFile=new File(mavenProjectDir,"target/foo-bar-1.0.0.jar");
  _executeBndDiff(gradleBundleFile,mavenBundleFile);
}
