{
  File projectDir=_buildTemplateWithGradle("controlmenuentry","foo-bar");
  _testExists(projectDir,"bnd.bnd");
  _testContains(projectDir,"src/main/java/foo/bar/control/menu/" + "FooBarProductNavigationControlMenuEntry.java","public class FooBarProductNavigationControlMenuEntry","extends BaseProductNavigationControlMenuEntry","implements ProductNavigationControlMenuEntry");
  _executeGradle(projectDir,_GRADLE_BUILD_ARGS);
  _testExists(projectDir,"build/libs/foo.bar-1.0.0.jar");
  File gradleBundleFile=new File(projectDir,"build/libs/foo.bar-1.0.0.jar");
  File mavenProjectDir=_buildTemplateWithMaven("controlmenuentry","foo-bar","-Dpackage=foo.bar","-DclassName=FooBar");
  _executeMaven(mavenProjectDir,_MAVEN_BUILD_ARGS);
  _testExists(mavenProjectDir,"target/foo-bar-1.0.0.jar");
  File mavenBundleFile=new File(mavenProjectDir,"target/foo-bar-1.0.0.jar");
  _executeBndDiff(gradleBundleFile,mavenBundleFile);
}
