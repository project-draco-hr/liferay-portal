{
  File projectDir=_buildTemplateWithGradle(null,"barfoo","--package-name","foo.bar");
  _testExists(projectDir,"src/main/resources/META-INF/resources/init.jsp");
  _testExists(projectDir,"src/main/resources/META-INF/resources/view.jsp");
  _testContains(projectDir,"bnd.bnd","Bundle-SymbolicName: foo.bar");
  _testContains(projectDir,"build.gradle","apply plugin: \"com.liferay.plugin\"");
  _executeGradle(projectDir,_TASK_PATH_BUILD);
  _testExists(projectDir,"build/libs/foo.bar-1.0.0.jar");
  File gradleBundleFile=new File(projectDir,"build/libs/foo.bar-1.0.0.jar");
  File mavenProjectDir=_buildTemplateWithMaven(null,"barfoo","-Dpackage=foo.bar","-DclassName=FooBar");
  _executeMaven(mavenProjectDir,new String[]{_TASK_PATH_PACKAGE});
  _testExists(mavenProjectDir,"target/barfoo-1.0.0.jar");
  File mavenBundleFile=new File(mavenProjectDir,"target/barfoo-1.0.0.jar");
  _executeBndDiff(gradleBundleFile,mavenBundleFile);
}
