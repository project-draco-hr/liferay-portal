{
  try (RandomAccessFile randomAccessFile=new RandomAccessFile(path.toFile(),"r")){
    long pos=randomAccessFile.length() - 1;
    if (pos < 0) {
      return false;
    }
    randomAccessFile.seek(pos);
    int c=randomAccessFile.read();
    if ((c == '\n') || (c == '\r')) {
      return true;
    }
  }
   return false;
}
