{
  File gradleProjectDir=_buildTemplateWithGradle("simulationpanelentry","simulator","--package-name","test.simulator");
  File mavenProjectDir=_buildTemplateWithMaven("simulationpanelentry","simulator","-DclassName=Simulator","-Dpackage=test.simulator");
  _testExists(gradleProjectDir,"bnd.bnd");
  _testContains(gradleProjectDir,"build.gradle","apply plugin: \"com.liferay.plugin\"");
