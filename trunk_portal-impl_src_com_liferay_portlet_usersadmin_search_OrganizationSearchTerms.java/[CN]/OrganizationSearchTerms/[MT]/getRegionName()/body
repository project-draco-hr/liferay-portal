{
  String regionName=null;
  if (regionId != 0) {
    try {
      Region region=RegionServiceUtil.getRegion(regionId);
      regionName=region.getName().toLowerCase();
    }
 catch (    NoSuchRegionException nsre) {
      if (_log.isWarnEnabled()) {
        _log.warn(nsre.getMessage());
      }
    }
  }
  return regionName;
}
