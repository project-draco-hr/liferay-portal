{
  String countryName=null;
  if (countryId != 0) {
    try {
      Country country=CountryServiceUtil.getCountry(countryId);
      countryName=country.getName().toLowerCase();
    }
 catch (    NoSuchCountryException nsce) {
      if (_log.isWarnEnabled()) {
        _log.warn(nsce.getMessage());
      }
    }
  }
  return countryName;
}
