{
  Expression<Double> expression=_expressionFactory.createDoubleExpression("sum(var1, var2, var3)");
  double var1=5.5;
  expression.setDoubleVariableValue("var1",var1);
  expression.setExpressionStringVariableValue("var2","var1 + 3.5",Double.class);
  expression.setExpressionStringVariableValue("var3","var2 + var1",Double.class);
  double var2=var1 + 3.5;
  double var3=var1 + var2;
  Assert.assertEquals((Double)MathUtil.sum(var1,var2,var3),expression.evaluate());
}
