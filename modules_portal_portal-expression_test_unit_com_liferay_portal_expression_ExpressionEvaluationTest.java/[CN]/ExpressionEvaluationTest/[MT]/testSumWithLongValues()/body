{
  Expression<Long> expression=_expressionFactory.createLongExpression("sum(var1, var2, var3)");
  long var1=5;
  expression.setLongVariableValue("var1",var1);
  expression.setExpressionStringVariableValue("var2","var1 + 3",Long.class);
  expression.setExpressionStringVariableValue("var3","var2 + var1",Long.class);
  long var2=var1 + 3;
  long var3=var1 + var2;
  Assert.assertEquals(MathUtil.sum(var1,var2,var3),(long)expression.evaluate());
}
