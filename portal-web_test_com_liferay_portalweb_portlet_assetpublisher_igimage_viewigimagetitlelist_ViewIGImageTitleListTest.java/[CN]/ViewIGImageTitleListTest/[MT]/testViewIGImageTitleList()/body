{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("AP IG Image Name"),selenium.getText("//div[2]/div/div/ul/li/a"));
  selenium.clickAt("//div[2]/div/div/ul/li/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("AP IG Image Name"),selenium.getText("//div/h3"));
  assertTrue(selenium.isElementPresent("//div[2]/img"));
  assertEquals(RuntimeVariables.replace("View Album \u00bb"),selenium.getText("//div[2]/div/a"));
  selenium.clickAt("//div[2]/div/a",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("//div[2]/a/img"));
  assertEquals(RuntimeVariables.replace("AP IG Image Name"),selenium.getText("//div[2]/a/span"));
}
