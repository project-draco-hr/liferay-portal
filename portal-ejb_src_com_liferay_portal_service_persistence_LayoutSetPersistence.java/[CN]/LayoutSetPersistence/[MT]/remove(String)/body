{
  Session session=null;
  try {
    session=openSession();
    LayoutSet layoutSet=(LayoutSet)session.get(LayoutSet.class,ownerId);
    if (layoutSet == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No LayoutSet exists with the primary key " + ownerId);
      }
      throw new NoSuchLayoutSetException("No LayoutSet exists with the primary key " + ownerId);
    }
    return remove(layoutSet);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
