{
  Session session=null;
  try {
    session=openSession();
    if (layoutSet.isNew()) {
      session.save(layoutSet);
    }
    session.flush();
    layoutSet.setNew(false);
    return layoutSet;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
