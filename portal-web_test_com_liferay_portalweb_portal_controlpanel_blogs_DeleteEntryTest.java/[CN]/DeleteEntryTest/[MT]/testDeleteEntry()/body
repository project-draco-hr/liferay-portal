{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      for (int second=0; ; second++) {
        if (second >= 60) {
          fail("timeout");
        }
        try {
          if (selenium.isElementPresent("link=Blogs")) {
            break;
          }
        }
 catch (        Exception e) {
        }
        Thread.sleep(1000);
      }
    selenium.click(RuntimeVariables.replace("link=Blogs"));
  selenium.waitForPageToLoad("30000");
boolean EntryPresent=selenium.isElementPresent("link=Delete");
if (!EntryPresent) {
label=2;
continue;
}
selenium.click(RuntimeVariables.replace("link=Delete"));
selenium.waitForPageToLoad("30000");
assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this[\\s\\S]$"));
assertFalse(selenium.isElementPresent("link=Test Entry"));
case 2:
case 100:
label=-1;
}
}
}
