{
  TasksProposal tasksProposal=findByPrimaryKey(proposalId);
  Session session=null;
  try {
    session=openSession();
    TasksProposal[] array=new TasksProposalImpl[3];
    array[0]=getByGroupId_PrevAndNext(session,tasksProposal,groupId,orderByComparator,true);
    array[1]=tasksProposal;
    array[2]=getByGroupId_PrevAndNext(session,tasksProposal,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
