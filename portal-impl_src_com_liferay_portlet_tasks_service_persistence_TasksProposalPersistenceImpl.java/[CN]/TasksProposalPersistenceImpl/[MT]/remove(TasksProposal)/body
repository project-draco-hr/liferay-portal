{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(tasksProposal);
    }
  }
  tasksProposal=removeImpl(tasksProposal);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(tasksProposal);
    }
  }
  return tasksProposal;
}
