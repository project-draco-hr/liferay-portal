{
  try {
    context.addPermissions("com.liferay.portlet.blogs",context.getGroupId());
    Document document=SAXReaderUtil.createDocument();
    Element rootElement=document.addElement("blogs-data");
    rootElement.addAttribute("group-id",String.valueOf(context.getGroupId()));
    List<BlogsEntry> entries=BlogsEntryUtil.findByGroupId(context.getGroupId());
    for (    BlogsEntry entry : entries) {
      exportEntry(context,rootElement,entry);
    }
    return document.formattedString();
  }
 catch (  Exception e) {
    throw new PortletDataException(e);
  }
}
