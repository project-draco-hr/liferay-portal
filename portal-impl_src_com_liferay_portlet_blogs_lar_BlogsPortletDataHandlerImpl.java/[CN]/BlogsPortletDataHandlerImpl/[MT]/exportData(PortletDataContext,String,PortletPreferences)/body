{
  try {
    Document doc=SAXReaderUtil.createDocument();
    Element root=doc.addElement("blogs-data");
    root.addAttribute("group-id",String.valueOf(context.getGroupId()));
    List<BlogsEntry> entries=BlogsEntryUtil.findByGroupId(context.getGroupId());
    for (    BlogsEntry entry : entries) {
      exportEntry(context,root,entry);
    }
    context.addPermissions("com.liferay.portlet.blogs",context.getGroupId());
    return doc.formattedString();
  }
 catch (  Exception e) {
    throw new PortletDataException(e);
  }
}
