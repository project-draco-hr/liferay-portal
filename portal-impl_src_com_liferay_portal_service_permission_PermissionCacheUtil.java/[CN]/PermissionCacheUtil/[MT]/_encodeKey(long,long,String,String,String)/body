{
  StringMaker sm=new StringMaker();
  sm.append(GROUP_NAME);
  sm.append(StringPool.POUND);
  sm.append(userId);
  sm.append(StringPool.POUND);
  sm.append(groupId);
  sm.append(StringPool.POUND);
  sm.append(name);
  sm.append(StringPool.POUND);
  sm.append(primKey);
  sm.append(StringPool.POUND);
  sm.append(actionId);
  return sm.toString();
}
