{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(EmailAddress.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("emailAddressId",RandomTestUtil.nextLong()));
  List<EmailAddress> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
