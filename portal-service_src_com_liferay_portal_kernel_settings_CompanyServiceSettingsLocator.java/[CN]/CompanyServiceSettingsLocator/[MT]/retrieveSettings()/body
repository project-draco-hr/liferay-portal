{
  Settings portalPropertiesSettings=_settingsLocatorHelper.getPortalPropertiesSettings();
  Settings configurationBeanSettings=_settingsLocatorHelper.getConfigurationBeanSettings(_serviceName,portalPropertiesSettings);
  Settings portalPreferencesSettings=_settingsLocatorHelper.getPortalPreferencesSettings(_companyId,configurationBeanSettings);
  return _settingsLocatorHelper.getCompanyPortletPreferencesSettings(_companyId,_serviceName,portalPreferencesSettings);
}
