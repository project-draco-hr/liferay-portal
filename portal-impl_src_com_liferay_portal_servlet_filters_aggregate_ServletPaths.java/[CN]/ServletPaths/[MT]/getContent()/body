{
  try {
    URL resourceURL=_servletContext.getResource(_resourcePath);
    if (resourceURL == null) {
      return null;
    }
    URLConnection urlConnection=resourceURL.openConnection();
    return StringUtil.read(urlConnection.getInputStream());
  }
 catch (  IOException ioe) {
    _log.error(ioe,ioe);
  }
  return null;
}
