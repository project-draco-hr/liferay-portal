{
  _request=request;
  ThemeDisplay themeDisplay=(ThemeDisplay)request.getAttribute(com.liferay.portal.util.WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  _siteNavigationBreadcrumbPortletInstanceConfiguration=portletDisplay.getPortletInstanceConfiguration(SiteNavigationBreadcrumbPortletInstanceConfiguration.class);
}
