{
  String generator=GetterUtil.getString(PropsUtil.get(PropsUtil.PASSWORDS_PASSWORDPOLICYTOOLKIT_GENERATOR));
  if (generator.equals("static")) {
    String password=GetterUtil.getString(PropsUtil.get(PropsUtil.PASSWORDS_PASSWORDPOLICYTOOLKIT_STATIC));
    return password;
  }
 else {
    return PwdGenerator.getPassword();
  }
}
