{
  _charsLowerCase=PropsValues.PASSWORDS_PASSWORDPOLICYTOOLKIT_CHARSET_LOWERCASE.toCharArray();
  _charsNumbers=PropsValues.PASSWORDS_PASSWORDPOLICYTOOLKIT_CHARSET_NUMBERS.toCharArray();
  _charsSymbols=PropsValues.PASSWORDS_PASSWORDPOLICYTOOLKIT_CHARSET_SYMBOLS.toCharArray();
  _charsUpperCase=PropsValues.PASSWORDS_PASSWORDPOLICYTOOLKIT_CHARSET_UPPERCASE.toCharArray();
  _charsAlphaNumeric=(PropsValues.PASSWORDS_PASSWORDPOLICYTOOLKIT_CHARSET_LOWERCASE + PropsValues.PASSWORDS_PASSWORDPOLICYTOOLKIT_CHARSET_UPPERCASE + PropsValues.PASSWORDS_PASSWORDPOLICYTOOLKIT_CHARSET_NUMBERS).toCharArray();
  Arrays.sort(_charsLowerCase);
  Arrays.sort(_charsNumbers);
  Arrays.sort(_charsSymbols);
  Arrays.sort(_charsUpperCase);
  Arrays.sort(_charsAlphaNumeric);
}
