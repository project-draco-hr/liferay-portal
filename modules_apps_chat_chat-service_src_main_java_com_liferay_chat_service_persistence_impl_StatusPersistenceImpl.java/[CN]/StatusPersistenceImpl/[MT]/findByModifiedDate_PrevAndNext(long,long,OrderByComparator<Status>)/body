{
  Status status=findByPrimaryKey(statusId);
  Session session=null;
  try {
    session=openSession();
    Status[] array=new StatusImpl[3];
    array[0]=getByModifiedDate_PrevAndNext(session,status,modifiedDate,orderByComparator,true);
    array[1]=status;
    array[2]=getByModifiedDate_PrevAndNext(session,status,modifiedDate,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
