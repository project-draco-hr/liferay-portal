{
  selenium.open("/web/guest/home");
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=My Account",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//div[4]/ul/li[1]/a",RuntimeVariables.replace(""));
  assertTrue(selenium.isChecked("//td[2]/span/span/span/input[2]"));
  assertTrue(selenium.isChecked("//tr[4]/td[2]/span/span/span/input[2]"));
  assertTrue(selenium.isChecked("//tr[5]/td[2]/span/span/span/input[2]"));
  assertTrue(selenium.isChecked("//td[3]/span/span/span/input[2]"));
  assertTrue(selenium.isChecked("//tr[4]/td[3]/span/span/span/input[2]"));
  assertTrue(selenium.isChecked("//tr[5]/td[3]/span/span/span/input[2]"));
}
