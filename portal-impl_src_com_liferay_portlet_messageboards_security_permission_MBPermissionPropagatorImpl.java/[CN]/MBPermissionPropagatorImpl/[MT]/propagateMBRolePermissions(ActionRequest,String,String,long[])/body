{
  long groupId=GetterUtil.getLong(primKey);
  List<MBCategory> categories=MBCategoryLocalServiceUtil.getCategories(groupId);
  List<MBMessage> messages=MBMessageLocalServiceUtil.getGroupMessages(groupId,WorkflowConstants.STATUS_ANY,QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  propagateRolePermissions(actionRequest,className,groupId,roleIds,categories,messages);
}
