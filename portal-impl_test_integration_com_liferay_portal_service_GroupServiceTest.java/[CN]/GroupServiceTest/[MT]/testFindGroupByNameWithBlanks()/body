{
  String name=ServiceTestUtil.randomString() + StringPool.SPACE + ServiceTestUtil.randomString();
  GroupTestUtil.addGroup(GroupConstants.DEFAULT_PARENT_GROUP_ID,name);
  LinkedHashMap<String,Object> groupParams=new LinkedHashMap<String,Object>();
  groupParams.put("manualMembership",Boolean.TRUE);
  groupParams.put("site",Boolean.TRUE);
  Assert.assertEquals(1,GroupLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),null,name,groupParams));
}
