{
  Group group=GroupTestUtil.addGroup();
  GroupLocalServiceUtil.updateGroup(group.getGroupId(),group.getGroupId(),group.getNameMap(),group.getDescriptionMap(),group.getType(),group.isManualMembership(),group.getMembershipRestriction(),group.getFriendlyURL(),group.isInheritContent(),group.isActive(),ServiceContextTestUtil.getServiceContext());
  Assert.fail("A group cannot be its own parent");
}
