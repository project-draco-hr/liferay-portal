{
  Group group1=GroupTestUtil.addGroup("Test 1");
  try {
    GroupLocalServiceUtil.updateGroup(group1.getGroupId(),group1.getGroupId(),group1.getName(),group1.getDescription(),group1.getType(),group1.getFriendlyURL(),group1.isActive(),ServiceTestUtil.getServiceContext());
    Assert.fail("A group cannot be its own parent.");
  }
 catch (  GroupParentException pe) {
    Assert.assertEquals(GroupParentException.SELF_DESCENDANT,pe.getType());
  }
}
