{
  Group group=GroupTestUtil.addGroup();
  try {
    GroupLocalServiceUtil.updateGroup(group.getGroupId(),group.getGroupId(),group.getName(),group.getDescription(),group.getType(),group.getFriendlyURL(),group.isActive(),ServiceTestUtil.getServiceContext());
    Assert.fail("A group cannot be its own parent");
  }
 catch (  GroupParentException gpe) {
    Assert.assertEquals(GroupParentException.SELF_DESCENDANT,gpe.getType());
  }
}
