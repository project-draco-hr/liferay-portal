{
  Group group=GroupTestUtil.addGroup(GroupConstants.DEFAULT_PARENT_GROUP_ID);
  LinkedHashMap<String,Object> groupParams=new LinkedHashMap<String,Object>();
  groupParams.put("manualMembership",Boolean.TRUE);
  groupParams.put("site",Boolean.TRUE);
  ThemeDisplay themeDisplay=new ThemeDisplay();
  Assert.assertEquals(1,GroupLocalServiceUtil.searchCount(TestPropsValues.getCompanyId(),null,group.getName(themeDisplay.getLocale()),groupParams));
}
