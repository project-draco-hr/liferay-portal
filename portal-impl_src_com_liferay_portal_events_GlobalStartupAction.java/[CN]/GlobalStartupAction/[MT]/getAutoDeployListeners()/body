{
  List<AutoDeployListener> autoDeployListeners=new ArrayList<AutoDeployListener>();
  String[] autoDeployListenerClassNames=PropsUtil.getArray(PropsKeys.AUTO_DEPLOY_LISTENERS);
  for (  String autoDeployListenerClassName : autoDeployListenerClassNames) {
    try {
      if (_log.isDebugEnabled()) {
        _log.debug("Instantiating " + autoDeployListenerClassName);
      }
      AutoDeployListener autoDeployListener=(AutoDeployListener)Class.forName(autoDeployListenerClassName).newInstance();
      autoDeployListeners.add(autoDeployListener);
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  return autoDeployListeners;
}
