{
  List<SandboxDeployListener> sandboxDeployListeners=new ArrayList<SandboxDeployListener>();
  String[] sandboxDeployListenerClassNames=PropsUtil.getArray(PropsKeys.SANDBOX_DEPLOY_LISTENERS);
  for (  String sandboxDeployListenerClassName : sandboxDeployListenerClassNames) {
    try {
      if (_log.isDebugEnabled()) {
        _log.debug("Instantiating " + sandboxDeployListenerClassName);
      }
      SandboxDeployListener sandboxDeployListener=(SandboxDeployListener)Class.forName(sandboxDeployListenerClassName).newInstance();
      sandboxDeployListeners.add(sandboxDeployListener);
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
  return sandboxDeployListeners;
}
