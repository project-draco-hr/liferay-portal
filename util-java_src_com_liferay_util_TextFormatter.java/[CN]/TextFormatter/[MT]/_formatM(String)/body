{
  char[] c=s.toCharArray();
  StringBuilder sb=new StringBuilder(c.length);
  for (int i=0; i < c.length; i++) {
    if (c[i] == '-') {
    }
 else     if ((i > 0) && (c[i - 1] == '-')) {
      sb.append(Character.toUpperCase(c[i]));
    }
 else {
      sb.append(c[i]);
    }
  }
  return sb.toString();
}
