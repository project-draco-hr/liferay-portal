{
  char[] c=s.toCharArray();
  StringBuilder sb=new StringBuilder(c.length * 2);
  for (int i=0; i < c.length; i++) {
    if (Character.isUpperCase(c[i])) {
      sb.append(StringPool.SPACE);
      sb.append(Character.toLowerCase(c[i]));
    }
 else {
      sb.append(c[i]);
    }
  }
  return sb.toString();
}
