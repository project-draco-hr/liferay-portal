{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByGroupId_PrevAndNext(structureEntryId,groupId,orderByComparator);
  }
  DDMStructureEntry ddmStructureEntry=findByPrimaryKey(structureEntryId);
  Session session=null;
  try {
    session=openSession();
    DDMStructureEntry[] array=new DDMStructureEntryImpl[3];
    array[0]=filterGetByGroupId_PrevAndNext(session,ddmStructureEntry,groupId,orderByComparator,true);
    array[1]=ddmStructureEntry;
    array[2]=filterGetByGroupId_PrevAndNext(session,ddmStructureEntry,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
