{
  List<DetailAST> annotationASTList=DetailASTUtil.getAllChildTokens(detailAST,TokenTypes.ANNOTATION,true);
  for (  DetailAST annotationAST : annotationASTList) {
    DetailAST nameAST=annotationAST.findFirstToken(TokenTypes.IDENT);
    String name=nameAST.getText();
    if (name.equals(annotationName)) {
      return true;
    }
  }
  return false;
}
