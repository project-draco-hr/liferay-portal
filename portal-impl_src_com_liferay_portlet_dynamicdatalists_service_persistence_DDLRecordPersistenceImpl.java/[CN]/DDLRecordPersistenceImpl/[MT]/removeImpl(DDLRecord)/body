{
  ddlRecord=toUnwrappedModel(ddlRecord);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(ddlRecord)) {
      ddlRecord=(DDLRecord)session.get(DDLRecordImpl.class,ddlRecord.getPrimaryKeyObj());
    }
    if (ddlRecord != null) {
      session.delete(ddlRecord);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (ddlRecord != null) {
    clearCache(ddlRecord);
  }
  return ddlRecord;
}
