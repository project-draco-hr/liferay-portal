{
  DDLRecord ddlRecord=(DDLRecord)EntityCacheUtil.getResult(DDLRecordModelImpl.ENTITY_CACHE_ENABLED,DDLRecordImpl.class,recordId,this);
  if (ddlRecord == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (ddlRecord == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      ddlRecord=(DDLRecord)session.get(DDLRecordImpl.class,Long.valueOf(recordId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (ddlRecord == null)) {
        EntityCacheUtil.putResult(DDLRecordModelImpl.ENTITY_CACHE_ENABLED,DDLRecordImpl.class,recordId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(ddlRecord);
      }
      closeSession(session);
    }
  }
  return ddlRecord;
}
