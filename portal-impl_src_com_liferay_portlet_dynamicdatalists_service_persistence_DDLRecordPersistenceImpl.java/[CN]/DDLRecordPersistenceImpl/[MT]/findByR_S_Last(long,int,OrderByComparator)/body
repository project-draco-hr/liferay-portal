{
  int count=countByR_S(recordSetId,status);
  List<DDLRecord> list=findByR_S(recordSetId,status,count - 1,count,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(6);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("recordSetId=");
    msg.append(recordSetId);
    msg.append(", status=");
    msg.append(status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchRecordException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
