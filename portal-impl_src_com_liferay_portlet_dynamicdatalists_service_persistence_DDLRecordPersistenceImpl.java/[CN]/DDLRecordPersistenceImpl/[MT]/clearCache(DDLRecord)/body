{
  EntityCacheUtil.removeResult(DDLRecordModelImpl.ENTITY_CACHE_ENABLED,DDLRecordImpl.class,ddlRecord.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddlRecord.getUuid(),Long.valueOf(ddlRecord.getGroupId())});
}
