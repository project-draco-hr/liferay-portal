{
  StringBundler sb=new StringBundler(9);
  sb.append("{uuid=");
  sb.append(_uuid);
  sb.append(", message type=");
  sb.append(_messageType);
  sb.append(", multicast=");
  sb.append(_multicast);
  if (_messageType.equals(ClusterMessageType.NOTIFY) || _messageType.equals(ClusterMessageType.UPDATE)) {
    sb.append(", clusterNode=");
    sb.append(_originatingClusterNode);
  }
 else {
    sb.append(", methodWrapper =");
    sb.append(_methodWrapper);
  }
  sb.append("}");
  return sb.toString();
}
