{
  StringBundler tableSQLSB=new StringBundler(7);
  tableSQLSB.append("update ");
  tableSQLSB.append(tableName);
  tableSQLSB.append(" set ");
  tableSQLSB.append(columnName);
  tableSQLSB.append(" = replace(");
  tableSQLSB.append(columnName);
  tableSQLSB.append(", '");
  String tableSQL=tableSQLSB.toString();
  try (LoggingTimer loggingTimer=new LoggingTimer(tableName)){
    StringBundler sb=new StringBundler(9);
    for (    String[] name : names) {
      sb.append(tableSQL);
      sb.append(name[0]);
      sb.append("', '");
      sb.append(name[1]);
      sb.append("') where ");
      sb.append(columnName);
      if (wildcardMode.equals(WildcardMode.LEADING) || wildcardMode.equals(WildcardMode.SURROUND)) {
        sb.append(" like '%");
      }
 else {
        sb.append(" like '");
      }
      sb.append(name[0]);
      if (wildcardMode.equals(WildcardMode.SURROUND) || wildcardMode.equals(WildcardMode.TRAILING)) {
        sb.append("%'");
      }
 else {
        sb.append("'");
      }
      runSQL(sb.toString());
      sb.setIndex(0);
    }
  }
 }
