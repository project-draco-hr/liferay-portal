{
  Session session=null;
  try {
    session=openSession();
    if (resourcePermission.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(ResourcePermissionImpl.class,resourcePermission.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(resourcePermission);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  ResourcePermissionModelImpl resourcePermissionModelImpl=(ResourcePermissionModelImpl)resourcePermission;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_R_R,new Object[]{new Long(resourcePermissionModelImpl.getOriginalResourceId()),new Long(resourcePermissionModelImpl.getOriginalRoleId())});
  EntityCacheUtil.removeResult(ResourcePermissionModelImpl.ENTITY_CACHE_ENABLED,ResourcePermissionImpl.class,resourcePermission.getPrimaryKey());
  return resourcePermission;
}
