{
  try {
    String versionLabel=getHeadVersionLabel(companyId,repositoryId,fileName);
    StorageObject storageObject=_s3Service.getObjectDetails(_s3Bucket.getName(),getKey(companyId,repositoryId,fileName,versionLabel));
    return storageObject.getContentLength();
  }
 catch (  ServiceException se) {
    throw new SystemException(se);
  }
}
