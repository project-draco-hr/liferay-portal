{
  if (((longDescription == null) && (_longDescription != null)) || ((longDescription != null) && (_longDescription == null)) || ((longDescription != null) && (_longDescription != null) && !longDescription.equals(_longDescription))) {
    if (!XSS_ALLOW_LONGDESCRIPTION) {
      longDescription=XSSUtil.strip(longDescription);
    }
    _longDescription=longDescription;
  }
}
