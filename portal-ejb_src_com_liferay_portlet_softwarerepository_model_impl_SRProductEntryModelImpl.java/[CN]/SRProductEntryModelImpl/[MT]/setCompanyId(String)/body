{
  if (((companyId == null) && (_companyId != null)) || ((companyId != null) && (_companyId == null)) || ((companyId != null) && (_companyId != null) && !companyId.equals(_companyId))) {
    if (!XSS_ALLOW_COMPANYID) {
      companyId=XSSUtil.strip(companyId);
    }
    _companyId=companyId;
  }
