{
  try {
    File mainCssFile=new File(cssDir + "/main.css");
    if (!mainCssFile.exists()) {
      System.out.println("Do not pack " + cssDir + "/main.css because it does not exist.");
      return;
    }
    FileImpl fileImpl=new FileImpl();
    String content=fileImpl.read(mainCssFile);
    content=replaceImports(cssDir,content);
    fileImpl.write(mergedFile,content);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
