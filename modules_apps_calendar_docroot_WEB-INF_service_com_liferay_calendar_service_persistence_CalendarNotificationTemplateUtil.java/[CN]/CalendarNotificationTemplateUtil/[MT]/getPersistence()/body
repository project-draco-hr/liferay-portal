{
  if (_persistence == null) {
    _persistence=(CalendarNotificationTemplatePersistence)PortletBeanLocatorUtil.locate(com.liferay.calendar.service.ClpSerializer.getServletContextName(),CalendarNotificationTemplatePersistence.class.getName());
    ReferenceRegistry.registerReference(CalendarNotificationTemplateUtil.class,"_persistence");
  }
  return _persistence;
}
