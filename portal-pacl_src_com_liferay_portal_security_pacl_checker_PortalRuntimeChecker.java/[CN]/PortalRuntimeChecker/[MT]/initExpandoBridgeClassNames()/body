{
  _expandoBridgeClassNames=getPropertySet("security-manager-expando-bridge");
  if (_log.isDebugEnabled()) {
    Set<String> classNames=new TreeSet<String>(_expandoBridgeClassNames);
    for (    String className : classNames) {
      _log.debug("Allowing Expando bridge on class " + className);
    }
  }
}
