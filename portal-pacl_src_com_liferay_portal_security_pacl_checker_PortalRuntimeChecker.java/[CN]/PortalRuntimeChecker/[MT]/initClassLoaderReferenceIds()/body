{
  _classLoaderReferenceIds=getPropertySet("security-manager-class-loader-reference-ids");
  if (_log.isDebugEnabled()) {
    Set<String> classLoaderReferenceIds=new TreeSet<>(_classLoaderReferenceIds);
    for (    String classLoaderReferenceId : classLoaderReferenceIds) {
      _log.debug("Allowing access to class loader for reference " + classLoaderReferenceId);
    }
  }
}
