{
  for (  Map.Entry<String,List<IndexerPostProcessor>> entry : _indexerPostProcessors.entrySet()) {
    String indexerClassName=entry.getKey();
    List<IndexerPostProcessor> indexerPostProcessors=entry.getValue();
    Indexer indexer=IndexerRegistryUtil.getIndexer(indexerClassName);
    for (    IndexerPostProcessor indexerPostProcessor : indexerPostProcessors) {
      indexer.unregisterIndexerPostProcessor(indexerPostProcessor);
    }
  }
}
