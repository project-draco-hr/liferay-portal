{
  Map<String,ServiceBag> serviceBags=_serviceBags.get(servletContextName);
  if (serviceBags == null) {
    serviceBags=new HashMap<String,ServiceBag>();
    _serviceBags.put(servletContextName,serviceBags);
  }
  serviceBags.put(serviceType,new ServiceBag(portletClassLoader,serviceTypeClass,serviceWrapper,advisedSupport));
}
