{
  String portalCtxPath=PropsUtil.get(PropsKeys.PORTAL_CTX);
  String portalCtxName=portalCtxPath.substring(1);
  ServletContext portalServletContext=ServletContextPool.get(portalCtxName);
  InvokerFilter invokerFilter=(InvokerFilter)portalServletContext.getAttribute(InvokerFilter.class.getName());
  return invokerFilter;
}
