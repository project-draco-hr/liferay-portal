{
  String[] publicPaths=StringUtil.split(portalProperties.getProperty(AUTH_PUBLIC_PATHS));
  for (  String publicPath : publicPaths) {
    AuthPublicPath authPublicPath=new DefaultAuthPublicPath(publicPath);
    registerService(servletContextName,AUTH_PUBLIC_PATHS + publicPath,AuthPublicPath.class,authPublicPath);
  }
}
