{
  String localeKey=null;
  int underlinePos=languagePropertiesLocation.indexOf(StringPool.UNDERLINE);
  int propertiesPos=languagePropertiesLocation.indexOf(".properties");
  if (underlinePos != -1 && propertiesPos != 1) {
    localeKey=languagePropertiesLocation.substring(underlinePos + 1,propertiesPos);
  }
  return localeKey;
}
