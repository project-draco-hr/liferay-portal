{
  Registry registry=RegistryUtil.getRegistry();
  Map<Object,ServiceRegistration<?>> serviceRegistrations=getServiceRegistrations(servletContextName);
  String[] autoLoginClassNames=StringUtil.split(portalProperties.getProperty(AUTO_LOGIN_HOOKS));
  for (  String autoLoginClassName : autoLoginClassNames) {
    AutoLogin autoLogin=(AutoLogin)newInstance(portletClassLoader,AutoLogin.class,autoLoginClassName);
    serviceRegistration=registry.registerService(AutoLogin.class,autoLogin);
    serviceRegistrations.put(autoLoginClassName,serviceRegistration);
    _autoLogins.add(autoLogin);
  }
}
