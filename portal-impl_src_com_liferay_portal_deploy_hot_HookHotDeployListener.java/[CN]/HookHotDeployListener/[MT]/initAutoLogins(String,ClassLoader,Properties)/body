{
  String[] autoLoginClassNames=StringUtil.split(portalProperties.getProperty(AUTO_LOGIN_HOOKS));
  for (  String autoLoginClassName : autoLoginClassNames) {
    AutoLogin autoLogin=(AutoLogin)newInstance(portletClassLoader,AutoLogin.class,autoLoginClassName);
    registerService(servletContextName,autoLoginClassName,AutoLogin.class,autoLogin);
  }
}
