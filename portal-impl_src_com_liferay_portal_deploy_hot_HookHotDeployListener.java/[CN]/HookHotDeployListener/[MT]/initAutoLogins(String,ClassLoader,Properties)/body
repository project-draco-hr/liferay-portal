{
  AutoLoginsContainer autoLoginsContainer=new AutoLoginsContainer();
  _autoLoginsContainerMap.put(servletContextName,autoLoginsContainer);
  String[] autoLoginClassNames=StringUtil.split(portalProperties.getProperty(AUTO_LOGIN_HOOKS));
  for (  String autoLoginClassName : autoLoginClassNames) {
    AutoLogin autoLogin=(AutoLogin)portletClassLoader.loadClass(autoLoginClassName).newInstance();
    autoLogin=(AutoLogin)Proxy.newProxyInstance(portletClassLoader,new Class[]{AutoLogin.class},new ContextClassLoaderBeanHandler(autoLogin,portletClassLoader));
    autoLoginsContainer.registerAutoLogin(autoLogin);
  }
}
