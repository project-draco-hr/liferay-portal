{
  List<Element> ddmFormFieldValueRenderersElements=parentElement.elements("dynamic-data-mapping-form-field-value-renderer");
  if (ddmFormFieldValueRenderersElements.isEmpty()) {
    return;
  }
  for (  Element ddmFormFieldValueRendererElement : ddmFormFieldValueRenderersElements) {
    String ddmFormFieldValueRendererClassName=ddmFormFieldValueRendererElement.getText();
    DDMFormFieldValueRenderer ddmFormFieldValueRenderer=(DDMFormFieldValueRenderer)newInstance(portletClassLoader,DDMFormFieldValueRenderer.class,ddmFormFieldValueRendererClassName);
    registerService(servletContextName,ddmFormFieldValueRendererClassName,DDMFormFieldValueRenderer.class,ddmFormFieldValueRenderer);
  }
}
