{
  PropsUtil.addProperties(portalProperties);
  if (_log.isDebugEnabled()) {
    _log.debug("Expected locales " + portalProperties.getProperty(PropsKeys.LOCALES));
    _log.debug("Actual locales " + PropsUtil.get(PropsKeys.LOCALES));
    _log.debug("Actual locales as array " + PropsUtil.getArray(PropsKeys.LOCALES).length);
  }
  resetPortalProperties(portalProperties);
}
