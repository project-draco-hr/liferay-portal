{
  PropsUtil.addProperties(portalProperties);
  if (_log.isDebugEnabled() && portalProperties.containsKey(PropsKeys.LOCALES)) {
    _log.debug("Portlet locales " + portalProperties.getProperty(PropsKeys.LOCALES));
    _log.debug("Merged locales " + PropsUtil.get(PropsKeys.LOCALES));
    _log.debug("Merged locales array length " + PropsUtil.getArray(PropsKeys.LOCALES).length);
  }
  resetPortalProperties(portalProperties);
}
