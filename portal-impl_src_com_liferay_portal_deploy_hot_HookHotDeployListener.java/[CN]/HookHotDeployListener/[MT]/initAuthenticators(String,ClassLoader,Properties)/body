{
  AuthenticatorsContainer authenticatorsContainer=new AuthenticatorsContainer();
  _authenticatorsContainerMap.put(servletContextName,authenticatorsContainer);
  initAuthenticators(portletClassLoader,portalProperties,AUTH_PIPELINE_PRE,authenticatorsContainer);
  initAuthenticators(portletClassLoader,portalProperties,AUTH_PIPELINE_POST,authenticatorsContainer);
}
