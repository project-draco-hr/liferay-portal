{
  if (Proxy.isProxyClass(oldTarget.getClass())) {
    InvocationHandler invocationHandler=Proxy.getInvocationHandler(oldTarget);
    if (invocationHandler instanceof ContextClassLoaderBeanHandler) {
      ContextClassLoaderBeanHandler contextClassLoaderBeanHandler=(ContextClassLoaderBeanHandler)invocationHandler;
      return contextClassLoaderBeanHandler.getBean();
    }
  }
  return oldTarget;
}
