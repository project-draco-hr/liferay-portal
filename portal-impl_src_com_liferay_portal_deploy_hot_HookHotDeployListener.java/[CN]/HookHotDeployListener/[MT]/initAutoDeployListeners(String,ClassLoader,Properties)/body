{
  String[] autoDeployListenerClassNames=StringUtil.split(portalProperties.getProperty(PropsKeys.AUTO_DEPLOY_LISTENERS));
  if (autoDeployListenerClassNames.length == 0) {
    return;
  }
  for (  String autoDeployListenerClassName : autoDeployListenerClassNames) {
    AutoDeployListener autoDeployListener=(AutoDeployListener)newInstance(portletClassLoader,AutoDeployListener.class,autoDeployListenerClassName);
    registerService(servletContextName,autoDeployListenerClassName,AutoDeployListener.class,autoDeployListener);
  }
}
