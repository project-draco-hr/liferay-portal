{
  String[] authFailureClassNames=StringUtil.split(portalProperties.getProperty(key));
  for (  String authFailureClassName : authFailureClassNames) {
    AuthFailure authFailure=(AuthFailure)portletClassLoader.loadClass(authFailureClassName).newInstance();
    authFailure=(AuthFailure)Proxy.newProxyInstance(portletClassLoader,new Class[]{AuthFailure.class},new ContextClassLoaderBeanHandler(authFailure,portletClassLoader));
    authFailuresContainer.registerAuthFailure(key,authFailure);
  }
}
