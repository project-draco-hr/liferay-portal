{
  for (  String key : propsValuesStringArray) {
    String fieldName=StringUtil.replace(key.toUpperCase(),CharPool.PERIOD,CharPool.UNDERLINE);
    if (!containsKey(portalProperties,key)) {
      continue;
    }
    try {
      resetPortalPropertiesStringArray(servletContextName,portalProperties,initPhase,propsValuesStringArray,stringArraysContainerMap,key,fieldName);
    }
 catch (    Exception e) {
      _log.error("Error setting field " + fieldName + ": "+ e.getMessage());
    }
  }
}
