{
  PropsUtil.removeProperties(portalProperties);
  if (_log.isDebugEnabled()) {
    _log.debug("Portlet locales " + portalProperties.getProperty(PropsKeys.LOCALES));
    _log.debug("Original locales " + PropsUtil.get(PropsKeys.LOCALES));
    _log.debug("Original locales as array " + PropsUtil.getArray(PropsKeys.LOCALES).length);
  }
  resetPortalProperties(portalProperties);
}
