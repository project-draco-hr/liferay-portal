{
  _active=active;
  if (_calendarResourceRemoteModel != null) {
    try {
      Class<?> clazz=_calendarResourceRemoteModel.getClass();
      Method method=clazz.getMethod("setActive",boolean.class);
      method.invoke(_calendarResourceRemoteModel,active);
    }
