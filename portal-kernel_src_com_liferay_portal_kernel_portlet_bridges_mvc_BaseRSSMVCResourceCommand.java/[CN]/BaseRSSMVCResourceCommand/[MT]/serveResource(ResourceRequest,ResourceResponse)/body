{
  if (!isRSSFeedsEnabled(resourceRequest)) {
    try {
      PortalUtil.sendRSSFeedsDisabledError(resourceRequest,resourceResponse);
    }
 catch (    Exception e) {
    }
    return false;
  }
  try {
    PortletResponseUtil.sendFile(resourceRequest,resourceResponse,null,getRSS(resourceRequest,resourceResponse),ContentTypes.TEXT_XML_UTF8);
  }
 catch (  Exception e) {
    throw new PortletException(e);
  }
  return true;
}
