{
  if (_validators == null) {
    return;
  }
  HttpServletRequest request=(HttpServletRequest)pageContext.getRequest();
  Map<String,List<ValidatorTag>> validatorTagsMap=(Map<String,List<ValidatorTag>>)request.getAttribute("aui:form:validatorTagsMap");
  List<ValidatorTag> validatorTags=ListUtil.fromMapValues(_validators);
  validatorTagsMap.put(getName(),validatorTags);
}
