{
  if (!Validator.equals(getBaseType(),"checkbox")) {
    return;
  }
  List<String> checkboxNames=(List<String>)request.getAttribute("aui:form:checkboxNames");
  if (checkboxNames != null) {
    String inputName=getInputName();
    String languageId=getLanguageId();
    if (Validator.isNotNull(languageId)) {
      inputName=LocalizationUtil.getLocalizedName(inputName,languageId);
    }
    checkboxNames.add(inputName);
  }
}
