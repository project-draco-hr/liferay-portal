{
  data=null;
  Reader reader=null;
  try {
    reader=_templateResource.getReader();
    data=rsvc.parse(reader,name);
    initDocument();
    return true;
  }
 catch (  Exception e) {
    throw new ParseErrorException("Unable to parse Velocity template");
  }
 finally {
    if (reader != null) {
      reader.close();
    }
  }
}
