{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ShoppingOrderItem.class,ShoppingOrderItem.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("orderItemId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("orderItemId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
