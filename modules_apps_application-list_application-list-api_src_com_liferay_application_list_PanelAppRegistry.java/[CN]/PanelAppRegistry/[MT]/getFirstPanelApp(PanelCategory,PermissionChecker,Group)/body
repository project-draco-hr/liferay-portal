{
  List<PanelApp> panelApps=getPanelApps(parentPanelCategory);
  for (  PanelApp panelApp : panelApps) {
    try {
      if (panelApp.hasAccessPermission(permissionChecker,group)) {
        return panelApp;
      }
    }
 catch (    PortalException e) {
      _log.error(e);
    }
  }
  return null;
}
