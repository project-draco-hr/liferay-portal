{
  try {
    ActionUtil.getFrameworkVersion(req);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchFrameworkVersionException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.software_repository.error");
    }
 else {
      throw e;
    }
  }
  return mapping.findForward(getForward(req,"portlet.software_repository.edit_framework_version"));
}
