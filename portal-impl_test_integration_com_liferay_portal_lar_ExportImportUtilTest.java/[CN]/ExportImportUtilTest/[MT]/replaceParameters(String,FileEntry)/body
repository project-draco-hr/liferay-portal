{
  return StringUtil.replace(content,new String[]{"[$GROUP_FRIENDLY_URL$]","[$GROUP_ID$]","[$IMAGE_ID$]","[$PATH_FRIENDLY_URL_PRIVATE_GROUP$]","[$PATH_FRIENDLY_URL_PRIVATE_USER$]","[$PATH_FRIENDLY_URL_PUBLIC$]","[$TITLE$]","[$UUID$]"},new String[]{_stagingGroup.getFriendlyURL(),String.valueOf(fileEntry.getGroupId()),String.valueOf(fileEntry.getFileEntryId()),PortalUtil.getPathFriendlyURLPrivateGroup(),PortalUtil.getPathFriendlyURLPrivateUser(),PortalUtil.getPathFriendlyURLPublic(),fileEntry.getTitle(),fileEntry.getUuid()});
}
