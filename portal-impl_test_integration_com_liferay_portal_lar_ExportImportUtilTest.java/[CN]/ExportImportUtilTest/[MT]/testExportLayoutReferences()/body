{
  String content=replaceParameters(getContent("layout_references.txt"),_fileEntry);
  Element rootElement=_portletDataContextExport.getExportDataRootElement();
  content=ExportImportUtil.exportContentReferences(_portletDataContextExport,rootElement.element("entry"),content);
  Assert.assertFalse(content.contains(PortalUtil.getPathFriendlyURLPrivateGroup()));
  Assert.assertFalse(content.contains(PortalUtil.getPathFriendlyURLPrivateUser()));
  Assert.assertFalse(content.contains(PortalUtil.getPathFriendlyURLPublic()));
  Assert.assertFalse(content.contains(_stagingGroup.getFriendlyURL()));
}
