{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLFileEntry.class,DLFileEntry.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("fileEntryId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("fileEntryId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
