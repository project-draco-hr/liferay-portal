{
  List<PortletConfigurationIconFactory> portletConfigurationIconFactories=_portletConfigurationIconFactoriesMap.get(StringPool.STAR);
  List<PortletConfigurationIconFactory> portletPortletConfigurationIconFactories=_portletConfigurationIconFactoriesMap.get(portletId);
  if (portletPortletConfigurationIconFactories != null) {
    portletConfigurationIconFactories.addAll(portletPortletConfigurationIconFactories);
  }
  return portletConfigurationIconFactories;
}
