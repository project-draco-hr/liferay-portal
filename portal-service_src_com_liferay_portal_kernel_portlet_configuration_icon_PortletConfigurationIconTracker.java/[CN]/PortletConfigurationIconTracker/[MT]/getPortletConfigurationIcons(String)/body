{
  List<PortletConfigurationIconFactory> portletConfigurationIconFactories=_portletConfigurationIconFactoryMap.get(StringPool.STAR);
  List<PortletConfigurationIconFactory> curPortletConfigurationIconFactories=_portletConfigurationIconFactoryMap.get(portletId);
  if (curPortletConfigurationIconFactories != null) {
    portletConfigurationIconFactories.addAll(curPortletConfigurationIconFactories);
  }
  return portletConfigurationIconFactories;
}
