{
  List<PortletConfigurationIconFactory> portletConfigurationIconFactories=_portletConfigurationIconFactoryMap.get(StringPool.STAR);
  List<PortletConfigurationIconFactory> portletPortletConfigurationIconFactories=_portletConfigurationIconFactoryiesMap.get(portletId);
