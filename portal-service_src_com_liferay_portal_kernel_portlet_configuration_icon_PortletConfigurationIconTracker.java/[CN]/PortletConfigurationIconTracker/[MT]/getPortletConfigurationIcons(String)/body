{
  List<PortletConfigurationIconFactory> portletConfigurationIconFactories=new ArrayList<>();
  List<PortletConfigurationIconFactory> portletPortletConfigurationIconFactories=_portletConfigurationIconFactoriesMap.get(StringPool.STAR);
  if (portletPortletConfigurationIconFactories != null) {
    portletConfigurationIconFactories.addAll(portletPortletConfigurationIconFactories);
  }
  portletPortletConfigurationIconFactories=_portletConfigurationIconFactoriesMap.get(portletId);
  if (portletPortletConfigurationIconFactories != null) {
    portletConfigurationIconFactories.addAll(portletPortletConfigurationIconFactories);
  }
  return portletConfigurationIconFactories;
}
