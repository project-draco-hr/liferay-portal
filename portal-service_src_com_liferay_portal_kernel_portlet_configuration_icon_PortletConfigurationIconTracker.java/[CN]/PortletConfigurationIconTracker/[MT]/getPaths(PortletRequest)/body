{
  Set<String> paths=new HashSet<>();
  for (  PortletConfigurationIconLocator portletConfigurationIconLocator : _serviceTrackerList) {
    String path=portletConfigurationIconLocator.getPath(portletRequest);
    if (Validator.isNotNull(path)) {
      paths.add(path);
      continue;
    }
    paths.addAll(portletConfigurationIconLocator.getDefaultViews(StringPool.STAR));
    String portletId=getPortletId(portletRequest);
    paths.addAll(portletConfigurationIconLocator.getDefaultViews(portletId));
  }
  return paths;
}
