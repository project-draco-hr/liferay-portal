{
  List<DLFileEntryMetadata> fileEntryMetadataList=DLFileEntryMetadataLocalServiceUtil.getFileEntryMetadataByFileVersionId(dlFileVersion.getFileVersionId());
  for (  DLFileEntryMetadata fileEntryMetadata : fileEntryMetadataList) {
    Fields fields=null;
    try {
      fields=StorageEngineUtil.getFields(fileEntryMetadata.getDDMStorageId());
    }
 catch (    Exception e) {
    }
    if (fields != null) {
      DDMStructure ddmStructure=DDMStructureLocalServiceUtil.getStructure(fileEntryMetadata.getDDMStructureId());
      DDMIndexerUtil.addAttributes(document,ddmStructure,fields);
    }
  }
}
