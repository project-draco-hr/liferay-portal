{
  Resource resource=resourceLocalService.fetchResource(companyId,name,scope,String.valueOf(primKey));
  if (resource == null) {
    resource=resourceLocalService.addResource(companyId,name,scope,String.valueOf(primKey));
  }
  if (resource == null) {
    return;
  }
  setRolesPermissions(companyId,roleIdsToActionIds,resource.getResourceId());
}
