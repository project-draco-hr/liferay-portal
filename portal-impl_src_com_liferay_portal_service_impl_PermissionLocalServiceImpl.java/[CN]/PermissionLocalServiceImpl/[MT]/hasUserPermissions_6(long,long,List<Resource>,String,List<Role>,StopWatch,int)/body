{
  for (int i=resources.size() - 1; i >= 0; i--) {
    Resource resource=resources.get(i);
    for (    Role role : roles) {
      if (resourcePermissionLocalService.hasResourcePermission(resource.getCompanyId(),resource.getName(),resource.getScope(),resource.getPrimKey(),role.getRoleId(),actionId)) {
        return true;
      }
    }
  }
  logHasUserPermissions(userId,resourceId,actionId,stopWatch,block++);
  return false;
}
