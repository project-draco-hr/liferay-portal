{
  List permissions=new ArrayList();
  List actions=null;
  if (portletActions) {
    actions=ResourceActionsUtil.getPortletResourceActions(companyId,name);
  }
 else {
    actions=ResourceActionsUtil.getModelResourceActions(name);
  }
  for (int i=0; i < actions.size(); i++) {
    String actionId=(String)actions.get(i);
    Permission permission=addPermission(companyId,actionId,resourceId);
    permissions.add(permission);
  }
  return permissions;
}
