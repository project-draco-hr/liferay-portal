{
  File file=new File(fileName);
  String content=FileUtil.read(file);
  String newContent=content;
  for (  String line : lines) {
    if (newContent.startsWith(line)) {
      if (newContent.equals(line)) {
        newContent=StringPool.BLANK;
      }
 else {
        newContent=StringUtil.replace(newContent,line + "\n",StringPool.BLANK);
      }
    }
 else {
      newContent=StringUtil.replace(newContent,"\n" + line,StringPool.BLANK);
    }
  }
  processFormattedFile(file,fileName,content,newContent);
}
