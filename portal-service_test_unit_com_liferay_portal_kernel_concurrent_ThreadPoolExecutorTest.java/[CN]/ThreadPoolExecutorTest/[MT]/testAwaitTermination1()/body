{
  ThreadPoolExecutor threadPoolExecutor=new ThreadPoolExecutor(1,2,TestUtil.KEEPALIVE_TIME,TimeUnit.MILLISECONDS,true,3);
  long startTime=System.nanoTime();
  Assert.assertFalse(threadPoolExecutor.awaitTermination(TestUtil.KEEPALIVE_TIME,TimeUnit.MILLISECONDS));
  long waitTime=System.nanoTime() - startTime;
  Assert.assertTrue(waitTime >= TimeUnit.MILLISECONDS.toNanos(TestUtil.KEEPALIVE_TIME));
}
