{
  if (!(portletResponse instanceof MimeResponse)) {
    return false;
  }
  MimeResponse mimeResponse=(MimeResponse)portletResponse;
  try {
    PortletResponseUtil.sendFile(portletRequest,mimeResponse,null,getRSS(portletRequest,portletResponse),ContentTypes.TEXT_XML_UTF8);
  }
 catch (  Exception e) {
    throw new PortletException(e);
  }
  return true;
}
