{
  List<String> dirNames=ListUtil.fromArray(new String[]{_DIR_NAME});
  String docrootDirName=_DOCROOT_DIR_NAME;
  String portalCommonDirName=_DOCROOT_DIR_NAME;
  new SassToCssBuilder(dirNames,docrootDirName,portalCommonDirName);
  String cacheContent=FileUtil.read(_DOCROOT_DIR_NAME + _DIR_NAME + _SASS_CACHE_DIR_NAME+ _FILE_NAME);
  String expectedCacheContent=FileUtil.read(_DOCROOT_DIR_NAME + _EXPECTED_DIR_NAME + _FILE_NAME);
  Assert.assertEquals(cacheContent,expectedCacheContent);
  String rtlCacheContent=FileUtil.read(_DOCROOT_DIR_NAME + _DIR_NAME + _SASS_CACHE_DIR_NAME+ _RTL_FILE_NAME);
  String rtlExpectedCacheContent=FileUtil.read(_DOCROOT_DIR_NAME + _EXPECTED_DIR_NAME + _RTL_FILE_NAME);
  Assert.assertEquals(rtlCacheContent,rtlExpectedCacheContent);
}
