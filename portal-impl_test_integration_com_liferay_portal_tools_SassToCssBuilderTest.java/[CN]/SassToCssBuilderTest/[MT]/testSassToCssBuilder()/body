{
  List<String> dirNames=ListUtil.fromArray(new String[]{_DIR_NAME});
  String docrootDirName=_DOCROOT_DIR_NAME;
  String portalCommonDirName=_DOCROOT_DIR_NAME;
  new SassToCssBuilder(dirNames,docrootDirName,portalCommonDirName);
  String expectedCacheContent=FileUtil.read(_DOCROOT_DIR_NAME + _EXPECTED_DIR_NAME + _FILE_NAME);
  String actualCacheContent=FileUtil.read(_DOCROOT_DIR_NAME + _DIR_NAME + _SASS_CACHE_DIR_NAME+ _FILE_NAME);
  Assert.assertEquals(expectedCacheContent,actualCacheContent);
  String expectedRtlCacheContent=FileUtil.read(_DOCROOT_DIR_NAME + _EXPECTED_DIR_NAME + _RTL_FILE_NAME);
  String actualRtlCacheContent=FileUtil.read(_DOCROOT_DIR_NAME + _DIR_NAME + _SASS_CACHE_DIR_NAME+ _RTL_FILE_NAME);
  Assert.assertEquals(expectedRtlCacheContent,actualRtlCacheContent);
}
