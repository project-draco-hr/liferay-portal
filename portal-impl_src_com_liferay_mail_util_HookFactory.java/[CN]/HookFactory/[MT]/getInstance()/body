{
  if (_hook == null) {
    if (_log.isDebugEnabled()) {
      _log.debug("Instantiate " + PropsValues.MAIL_HOOK_IMPL);
    }
    _hook=(Hook)InstancePool.get(PropsValues.MAIL_HOOK_IMPL);
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Return " + _hook.getClass().getName());
  }
  return _hook;
}
