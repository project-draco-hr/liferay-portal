{
  if (isEscapedModel()) {
    return (LayoutPrototype)this;
  }
 else {
    LayoutPrototype model=new LayoutPrototypeImpl();
    model.setNew(isNew());
    model.setEscapedModel(true);
    model.setLayoutPrototypeId(getLayoutPrototypeId());
    model.setCompanyId(getCompanyId());
    model.setName(getName());
    model.setDescription(HtmlUtil.escape(getDescription()));
    model.setSettings(HtmlUtil.escape(getSettings()));
    model.setActive(getActive());
    model=(LayoutPrototype)Proxy.newProxyInstance(LayoutPrototype.class.getClassLoader(),new Class[]{LayoutPrototype.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
