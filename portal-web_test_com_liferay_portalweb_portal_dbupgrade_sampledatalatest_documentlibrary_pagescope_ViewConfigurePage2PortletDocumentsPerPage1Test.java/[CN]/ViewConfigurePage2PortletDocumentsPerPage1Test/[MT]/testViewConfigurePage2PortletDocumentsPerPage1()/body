  assertFalse(selenium.isTextPresent("DL Folder2 Name"));
  selenium.clickAt("xPath=(//a[contains(@class,'aui-paginator-first-link')])[2]",RuntimeVariables.replace("First"));
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
