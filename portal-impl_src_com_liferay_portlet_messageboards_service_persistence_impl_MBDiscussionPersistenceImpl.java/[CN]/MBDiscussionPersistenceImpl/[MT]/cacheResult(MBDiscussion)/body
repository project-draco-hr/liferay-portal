{
  entityCache.putResult(MBDiscussionModelImpl.ENTITY_CACHE_ENABLED,MBDiscussionImpl.class,mbDiscussion.getPrimaryKey(),mbDiscussion);
  finderCache.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{mbDiscussion.getUuid(),mbDiscussion.getGroupId()},mbDiscussion);
  finderCache.putResult(FINDER_PATH_FETCH_BY_THREADID,new Object[]{mbDiscussion.getThreadId()},mbDiscussion);
  finderCache.putResult(FINDER_PATH_FETCH_BY_C_C,new Object[]{mbDiscussion.getClassNameId(),mbDiscussion.getClassPK()},mbDiscussion);
  mbDiscussion.resetOriginalValues();
}
