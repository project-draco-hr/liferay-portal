{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Web Content",RuntimeVariables.replace("Web Content"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Web Content Name"),selenium.getText("//td[3]/a"));
  selenium.clickAt("//td[3]/a",RuntimeVariables.replace("Web Content"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_15_title_en_US']",RuntimeVariables.replace("Web Content Name Edited"));
  selenium.clickAt("//input[@value='Publish']",RuntimeVariables.replace("Publish"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Web Content Name Edited"),selenium.getText("//td[3]/a"));
}
