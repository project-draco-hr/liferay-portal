{
  selenium.open("/web/guest/home/");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  selenium.clickAt("link=Web Content",RuntimeVariables.replace("Roles"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertEquals(RuntimeVariables.replace("Web Content Name"),selenium.getText("//td[3]/a"));
  selenium.clickAt("link=Web Content Name",RuntimeVariables.replace("Web Content"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  selenium.type("_15_title_en_US",RuntimeVariables.replace("Web Content Name Edited"));
  selenium.clickAt("_15_publishButton",RuntimeVariables.replace("Publish"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertEquals(RuntimeVariables.replace("Web Content Name Edited"),selenium.getText("//td[3]/a"));
}
