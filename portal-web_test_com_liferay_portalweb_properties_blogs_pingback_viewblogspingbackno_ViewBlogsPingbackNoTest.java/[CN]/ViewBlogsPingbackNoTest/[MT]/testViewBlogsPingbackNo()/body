{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Blogs Test Page");
  selenium.clickAt("link=Blogs Test Page",RuntimeVariables.replace("Blogs Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Blog Entry']",RuntimeVariables.replace("Add Blog Entry"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isTextPresent("Allow Pingbacks"));
  assertTrue(selenium.isElementNotPresent("//input[@id='_33_allowPingbacksCheckbox']"));
}
