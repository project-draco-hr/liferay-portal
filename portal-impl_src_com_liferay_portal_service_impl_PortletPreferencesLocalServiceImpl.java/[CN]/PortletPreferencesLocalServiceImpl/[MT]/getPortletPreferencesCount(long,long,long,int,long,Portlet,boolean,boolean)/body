{
  String portletId=StringPool.BLANK;
  if (plid == -1) {
    portletId=portlet.getRootPortletId();
  }
 else {
    portletId=portlet.getPortletId();
  }
  return portletPreferencesFinder.countByC_G_O_O_P_P_P(companyId,groupId,ownerId,ownerType,plid,portletId,privateLayout,excludeDefaultPreferences);
}
