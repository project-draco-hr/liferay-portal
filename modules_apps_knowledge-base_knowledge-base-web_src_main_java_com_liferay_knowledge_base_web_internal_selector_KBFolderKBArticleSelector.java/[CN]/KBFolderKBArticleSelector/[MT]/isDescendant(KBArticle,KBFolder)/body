{
  if (kbFolder.getKbFolderId() == KBFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    return true;
  }
  KBFolder parentKBFolder=_kbFolderLocalService.getKBFolder(kbArticle.getKbFolderId());
  List<Long> ancestorFolderIds=parentKBFolder.getAncestorFolderIds();
  if (ancestorFolderIds.contains(kbFolder.getKbFolderId())) {
    return true;
  }
  return false;
}
