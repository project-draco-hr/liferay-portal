{
  if (kbFolder.getKbFolderId() == KBFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    return true;
  }
  KBFolder parentKBFolder=_kbFolderLocalService.getKBFolder(kbArticle.getKbFolderId());
  List<Long> ancestorKBFolderIds=parentKBFolder.getAncestorKBFolderIds();
  if (ancestorKBFolderIds.contains(kbFolder.getKbFolderId())) {
    return true;
  }
  return false;
}
