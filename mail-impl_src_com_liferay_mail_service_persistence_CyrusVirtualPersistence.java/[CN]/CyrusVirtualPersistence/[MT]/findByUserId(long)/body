{
  Session session=null;
  try {
    session=getSessionFactory().openSession();
    Query q=session.createQuery(FIND_BY_USER_ID);
    q.setString(0,String.valueOf(userId));
    return q.list();
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    session.close();
  }
}
