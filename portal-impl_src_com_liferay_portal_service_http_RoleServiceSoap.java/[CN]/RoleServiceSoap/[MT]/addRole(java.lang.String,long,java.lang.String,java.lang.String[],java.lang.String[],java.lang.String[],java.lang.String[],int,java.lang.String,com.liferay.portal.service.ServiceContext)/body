{
  try {
    Map<Locale,String> titleMap=LocalizationUtil.getLocalizationMap(titleMapLanguageIds,titleMapValues);
    Map<Locale,String> descriptionMap=LocalizationUtil.getLocalizationMap(descriptionMapLanguageIds,descriptionMapValues);
    com.liferay.portal.model.Role returnValue=RoleServiceUtil.addRole(className,classPK,name,titleMap,descriptionMap,type,subtype,serviceContext);
    return com.liferay.portal.model.RoleSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
