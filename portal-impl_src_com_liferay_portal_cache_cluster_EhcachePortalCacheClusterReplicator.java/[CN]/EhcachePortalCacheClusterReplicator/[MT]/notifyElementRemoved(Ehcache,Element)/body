{
  if (!_replicateRemovals || !ClusterReplicationThreadLocal.isReplicate()) {
    return;
  }
  Serializable key=(Serializable)element.getObjectKey();
  PortalCacheClusterEvent portalCacheClusterEvent=new PortalCacheClusterEvent(ehcache.getName(),key,PortalCacheClusterEventType.REMOVE);
  PortalCacheClusterLinkUtil.sendEvent(portalCacheClusterEvent);
}
