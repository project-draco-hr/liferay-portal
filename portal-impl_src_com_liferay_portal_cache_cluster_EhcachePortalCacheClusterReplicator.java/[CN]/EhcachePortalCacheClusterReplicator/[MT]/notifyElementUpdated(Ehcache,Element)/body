{
  if (!_replicateUpdates) {
    return;
  }
  PortalCacheClusterEvent portalCacheClusterEvent=new PortalCacheClusterEvent(ehcache.getName(),element.getObjectKey(),PortalCacheClusterEventType.UPDATE);
  if (_replicateUpdatesViaCopy) {
    portalCacheClusterEvent.setElementValue(element.getObjectValue());
  }
  PortalCacheClusterLinkUtil.sendEvent(portalCacheClusterEvent);
}
