{
  if (properties != null) {
    _replicatePuts=GetterUtil.getBoolean(properties.getProperty(_REPLICATE_PUTS),false);
    _replicatePutsViaCopy=GetterUtil.getBoolean(properties.getProperty(_REPLICATE_PUTS_VIA_COPY),false);
    _replicateRemovals=GetterUtil.getBoolean(properties.getProperty(_REPLICATE_REMOVALS),true);
    _replicateUpdates=GetterUtil.getBoolean(properties.getProperty(_REPLICATE_UPDATES),true);
    _replicateUpdatesViaCopy=GetterUtil.getBoolean(properties.getProperty(_REPLICATE_UPDATES_VIA_COPY),false);
  }
}
