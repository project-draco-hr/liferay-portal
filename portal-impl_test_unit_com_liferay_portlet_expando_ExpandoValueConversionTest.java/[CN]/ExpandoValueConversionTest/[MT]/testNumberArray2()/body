{
  Number number1=345;
  Number number2=56;
  Number[] convertedNumber=_converter.convertType(ExpandoColumnConstants.NUMBER_ARRAY,number1 + ", " + number2);
  Assert.assertEquals(2,convertedNumber.length);
  Assert.assertEquals(new BigDecimal(number1.intValue()),convertedNumber[0]);
  Assert.assertEquals(new BigDecimal(number2.intValue()),convertedNumber[1]);
}
