{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLDocumentType.class,DLDocumentType.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("documentTypeId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("documentTypeId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
