{
  SocialActivity result=(SocialActivity)EntityCacheUtil.getResult(SocialActivityModelImpl.ENTITY_CACHE_ENABLED,SocialActivity.class,activityId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      SocialActivity socialActivity=(SocialActivity)session.get(SocialActivityImpl.class,new Long(activityId));
      cacheResult(socialActivity);
      return socialActivity;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (SocialActivity)result;
  }
}
