{
  SocialActivity socialActivity=(SocialActivity)EntityCacheUtil.getResult(SocialActivityModelImpl.ENTITY_CACHE_ENABLED,SocialActivityImpl.class,activityId,this);
  if (socialActivity == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (socialActivity == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      socialActivity=(SocialActivity)session.get(SocialActivityImpl.class,Long.valueOf(activityId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (socialActivity == null)) {
        EntityCacheUtil.putResult(SocialActivityModelImpl.ENTITY_CACHE_ENABLED,SocialActivityImpl.class,activityId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(socialActivity);
      }
      closeSession(session);
    }
  }
  return socialActivity;
}
