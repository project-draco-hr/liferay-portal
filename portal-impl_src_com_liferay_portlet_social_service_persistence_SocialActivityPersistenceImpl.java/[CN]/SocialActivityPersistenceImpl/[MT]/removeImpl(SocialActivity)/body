{
  Session session=null;
  try {
    session=openSession();
    session.delete(socialActivity);
    session.flush();
    return socialActivity;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(SocialActivity.class.getName());
  }
}
