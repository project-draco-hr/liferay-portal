{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(socialActivity);
    }
  }
  socialActivity=removeImpl(socialActivity);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(socialActivity);
    }
  }
  return socialActivity;
}
