{
  SocialActivity socialActivity=fetchByPrimaryKey(activityId);
  if (socialActivity == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No SocialActivity exists with the primary key " + activityId);
    }
    throw new NoSuchActivityException("No SocialActivity exists with the primary key " + activityId);
  }
  return socialActivity;
}
