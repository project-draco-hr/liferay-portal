{
  SocialActivity socialActivity=fetchByG_U_CD_C_C_T_R(groupId,userId,createDate,classNameId,classPK,type,receiverUserId);
  if (socialActivity == null) {
    StringBundler msg=new StringBundler();
    msg.append("No SocialActivity exists with the key {");
    msg.append("groupId=" + groupId);
    msg.append(", ");
    msg.append("userId=" + userId);
    msg.append(", ");
    msg.append("createDate=" + createDate);
    msg.append(", ");
    msg.append("classNameId=" + classNameId);
    msg.append(", ");
    msg.append("classPK=" + classPK);
    msg.append(", ");
    msg.append("type=" + type);
    msg.append(", ");
    msg.append("receiverUserId=" + receiverUserId);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    if (_log.isWarnEnabled()) {
      _log.warn(msg.toString());
    }
    throw new NoSuchActivityException(msg.toString());
  }
  return socialActivity;
}
