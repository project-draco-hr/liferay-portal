{
  try (PreparedStatement ps=connection.prepareStatement("update DDMStructure set definition = ? where structureId = " + "?")){
    ps.setString(1,definition);
    ps.setLong(2,structureId);
    ps.executeUpdate();
  }
 catch (  Exception e) {
    _log.error("Unable to upgrade dynamic data mapping structure with " + "structure ID " + structureId);
    throw e;
  }
}
