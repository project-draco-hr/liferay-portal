{
  try {
    for (    long ddmStructurePK : ddmStructurePKs) {
      addDDMStructure.add(pk,ddmStructurePK);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    FinderCacheUtil.clearCache(DLDocumentTypeModelImpl.MAPPING_TABLE_DLDOCUMENTTYPE_DDMSTRUCTURE_NAME);
  }
}
