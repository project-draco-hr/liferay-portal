{
  Session session=null;
  try {
    session=openSession();
    DLDocumentType dlDocumentType=(DLDocumentType)session.get(DLDocumentTypeImpl.class,Long.valueOf(documentTypeId));
    if (dlDocumentType == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + documentTypeId);
      }
      throw new NoSuchDocumentTypeException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + documentTypeId);
    }
    return dlDocumentTypePersistence.remove(dlDocumentType);
  }
 catch (  NoSuchDocumentTypeException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
