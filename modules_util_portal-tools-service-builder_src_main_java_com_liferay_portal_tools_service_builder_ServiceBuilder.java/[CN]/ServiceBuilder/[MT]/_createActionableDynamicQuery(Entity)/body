{
  File ejbFile=new File(_serviceOutputPath + "/service/persistence/" + entity.getName()+ "ActionableDynamicQuery.java");
  if (_osgiModule) {
    ejbFile.delete();
    return;
  }
  Map<String,Object> context=_getContext();
  context.put("entity",entity);
  String content=_processTemplate(_tplActionableDynamicQuery,context);
  ToolsUtil.writeFile(ejbFile,content,_author,_modifiedFileNames);
}
