{
  Matcher matcher=_missingFullyQualifiedNamePattern.matcher(declarationSignature);
  if (!matcher.find()) {
    return declarationSignature;
  }
  declarationSignature=StringUtil.insert(declarationSignature,packagePath + StringPool.PERIOD,matcher.start(2));
  return _fixDeclarationSignature(declarationSignature,packagePath);
}
