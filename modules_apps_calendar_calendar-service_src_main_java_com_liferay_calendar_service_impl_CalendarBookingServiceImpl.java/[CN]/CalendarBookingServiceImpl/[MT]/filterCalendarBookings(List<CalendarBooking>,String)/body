{
  calendarBookings=ListUtil.copy(calendarBookings);
  Iterator<CalendarBooking> itr=calendarBookings.iterator();
  while (itr.hasNext()) {
    CalendarBooking calendarBooking=itr.next();
    if (!CalendarPermission.contains(getPermissionChecker(),calendarBooking.getCalendarId(),CalendarActionKeys.VIEW_BOOKING_DETAILS)) {
      if (!CalendarPermission.contains(getPermissionChecker(),calendarBooking.getCalendarId(),actionId) || isPendingInWorkflow(calendarBooking)) {
        itr.remove();
      }
 else {
        filterCalendarBooking(calendarBooking);
      }
    }
  }
  return calendarBookings;
}
