{
  long[] childCalendarIds=calendarBookingLocalService.getChildCalendarIds(calendarBookingId,calendarId);
  return updateOffsetAndDuration(calendarBookingId,calendarId,childCalendarIds,titleMap,descriptionMap,location,offset,duration,allDay,recurrence,firstReminder,firstReminderType,secondReminder,secondReminderType,serviceContext);
}
