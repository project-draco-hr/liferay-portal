{
  try {
    CASConfiguration casCompanyServiceSettings=_configurationFactory.getConfiguration(CASConfiguration.class,new CompanyServiceSettingsLocator(companyId,CASConstants.SERVICE_NAME));
    return casCompanyServiceSettings;
  }
 catch (  ConfigurationException ce) {
    _log.error("Unable to get CAS configuration",ce);
  }
  return null;
}
