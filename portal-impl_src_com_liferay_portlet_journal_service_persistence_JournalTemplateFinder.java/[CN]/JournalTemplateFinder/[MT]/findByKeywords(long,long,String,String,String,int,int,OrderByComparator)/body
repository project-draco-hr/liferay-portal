{
  String[] templateIds=null;
  String[] names=null;
  String[] descriptions=null;
  boolean andOperator=false;
  if (Validator.isNotNull(keywords)) {
    templateIds=CustomSQLUtil.keywords(keywords,false);
    names=CustomSQLUtil.keywords(keywords);
    descriptions=CustomSQLUtil.keywords(keywords);
  }
 else {
    andOperator=true;
  }
  return findByC_G_T_S_N_D(companyId,groupId,templateIds,structureId,structureIdComparator,names,descriptions,andOperator,begin,end,obc);
}
