{
  String expectedException="Pattern \"This is Site 2\" does not match \"This is Site 1\" " + "at \"//html/body/h1\"";
  if (TestPropsValues.SELENIUM_LOGGER_ENABLED) {
    expectedException="Command failure \"assertText\" with parameters " + "\"//html/body/h1\" \"This is Site 2\" : Pattern " + "\"This is Site 2\" does not match "+ "\"This is Site 1\" at \"//html/body/h1\"";
  }
  try {
    selenium.assertText("//html/body/h1","This is Site 2");
  }
 catch (  Throwable e) {
    assertEquals(e.getMessage(),expectedException);
  }
}
