{
  String expectedException="Pattern \"This is Site 2\" does not match \"This is Site 1\" " + "at \"//html/body/h1\"";
  if (SELENIUM_LOGGER_ENABLED) {
    expectedException="Command failure \"assertText\" with parameters " + "\"//html/body/h1\" \"This is Site 2\" : " + expectedException;
  }
  try {
    selenium.assertText("//html/body/h1","This is Site 2");
  }
 catch (  Throwable t) {
    assertEquals(t.getMessage(),expectedException);
  }
}
