{
  Assert.assertTrue(ShoppingCategoryPermission.contains(permissionChecker,_category,ActionKeys.VIEW));
  Assert.assertTrue(ShoppingCategoryPermission.contains(permissionChecker,_subcategory,ActionKeys.VIEW));
  removePortletModelViewPermission();
  Assert.assertFalse(ShoppingCategoryPermission.contains(permissionChecker,_category,ActionKeys.VIEW));
  Assert.assertFalse(ShoppingCategoryPermission.contains(permissionChecker,_subcategory,ActionKeys.VIEW));
}
