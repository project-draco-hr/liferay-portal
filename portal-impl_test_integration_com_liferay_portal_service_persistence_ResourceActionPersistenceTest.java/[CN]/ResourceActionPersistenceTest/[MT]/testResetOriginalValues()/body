{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  ResourceAction newResourceAction=addResourceAction();
  _persistence.clearCache();
  ResourceAction existingResourceAction=_persistence.findByPrimaryKey(newResourceAction.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingResourceAction.getName(),ReflectionTestUtil.invoke(existingResourceAction,"getOriginalName",new Class<?>[0])));
  Assert.assertTrue(Validator.equals(existingResourceAction.getActionId(),ReflectionTestUtil.invoke(existingResourceAction,"getOriginalActionId",new Class<?>[0])));
}
