{
  request.setAttribute("liferay-staging:portlet-list:disableInputs",_disableInputs);
  request.setAttribute("liferay-staging:portlet-list:exportImportConfigurationId",_exportImportConfigurationId);
  request.setAttribute("liferay-staging:portlet-list:portlets",_portlets);
  request.setAttribute("liferay-staging:portlet-list:type",_type);
}
