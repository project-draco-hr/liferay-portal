{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalArticleImage.class,JournalArticleImage.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("articleImageId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("articleImageId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
