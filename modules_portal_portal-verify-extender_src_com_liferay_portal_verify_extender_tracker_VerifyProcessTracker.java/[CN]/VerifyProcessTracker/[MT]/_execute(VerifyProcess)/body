{
  if (_log.isDebugEnabled()) {
    Bundle bundle=_bundleContext.getBundle();
    _log.debug("Executing verify process " + verifyProcess.getClass() + " defined by "+ bundle.getSymbolicName());
  }
  try {
    verifyProcess.verify();
  }
 catch (  VerifyException ve) {
    if (_log.isErrorEnabled()) {
      _log.error("Unexpected error while executing the verifier " + verifyProcess.getClass(),ve);
    }
  }
}
