{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Wiki Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.clickAt("//strong/a",RuntimeVariables.replace("Options"));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("Configuration"),selenium.getText("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a"));
  selenium.click("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("_86_nodeId")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.select("_86_nodeId",RuntimeVariables.replace("label=Main"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (RuntimeVariables.replace("You have successfully updated the setup.").equals(selenium.getText("//div[@class='portlet-msg-success']"))) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("You have successfully updated the setup."),selenium.getText("//div[@class='portlet-msg-success']"));
  assertEquals(RuntimeVariables.replace("Main"),selenium.getText("_86_nodeId"));
  assertEquals(RuntimeVariables.replace("FrontPage"),selenium.getText("_86_title"));
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Display Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Wiki Display Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("This is a wiki frontpage article test."),selenium.getText("//div[@class='wiki-body']"));
}
