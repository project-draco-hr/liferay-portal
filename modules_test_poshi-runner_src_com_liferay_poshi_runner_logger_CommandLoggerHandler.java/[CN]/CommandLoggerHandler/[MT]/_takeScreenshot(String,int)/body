{
  String testClassCommandName=PoshiRunnerContext.getTestCaseCommandName();
  testClassCommandName=StringUtil.replace(testClassCommandName,"#","_");
  String currentDir=PoshiRunnerGetterUtil.getCanonicalPath(".");
  LiferaySeleniumHelper.captureScreen(currentDir + "/test-results/" + testClassCommandName+ "/screenshot/"+ screenshotName+ errorLinkId+ ".jpg");
}
