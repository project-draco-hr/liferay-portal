{
  String functionLinkID=xmlLoggerElement.getAttributeValue("data-functionlinkid");
  if (functionLinkID != null) {
    _functionLinkId=Integer.parseInt(functionLinkID.substring(15));
  }
  xmlLoggerElement.setAttribute("data-functionlinkid","functionLinkId-" + _functionLinkId);
  _lineGroupLoggerElement.setAttribute("data-functionlinkid","functionLinkId-" + _functionLinkId);
  _functionLinkId++;
}
