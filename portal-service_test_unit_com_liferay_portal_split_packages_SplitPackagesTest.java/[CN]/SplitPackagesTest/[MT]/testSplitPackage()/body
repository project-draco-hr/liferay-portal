{
  List<String> portalImplList=_getPackageNameList(Paths.get("portal-impl/src"));
  List<String> portalServiceList=_getPackageNameList(Paths.get("portal-service/src"));
  portalImplList.retainAll(portalServiceList);
  System.out.println("The following packages are present in both sides: ");
  for (  String duplicated : portalImplList) {
    System.out.println(duplicated);
  }
  Assert.assertTrue(portalImplList.isEmpty());
}
