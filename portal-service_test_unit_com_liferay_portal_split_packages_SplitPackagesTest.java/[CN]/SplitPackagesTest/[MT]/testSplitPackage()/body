{
  Set<String> portalImplPackages=_getPackages(Paths.get("portal-impl/src"));
  Set<String> portalServicePackages=_getPackages(Paths.get("portal-service/src"));
  portalImplPackages.retainAll(portalServicePackages);
  Assert.assertTrue("Detected split packages in portal-service and portal-impl: " + portalImplPackages,portalImplPackages.isEmpty());
}
