{
  PortletImporter portletImporter=PortletImporter.getInstance();
  try {
    portletImporter.importPortletInfo(TestPropsValues.getUserId(),0,0,StringPool.BLANK,_parameterMap,null);
  }
 catch (  Throwable t) {
    if (_log.isInfoEnabled()) {
      _log.info(t,t);
    }
  }
  Assert.assertTrue(_firedExportImportLifecycleEventsMap.containsKey(ExportImportLifecycleConstants.EVENT_PORTLET_IMPORT_FAILED));
}
