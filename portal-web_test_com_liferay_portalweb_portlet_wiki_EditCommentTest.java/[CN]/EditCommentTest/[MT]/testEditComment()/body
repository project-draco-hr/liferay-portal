{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//tr[5]/td[2]/table[1]/tbody/tr/td[4]/span/a[2]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//tr[5]/td[2]/table[1]/tbody/tr/td[4]/span/a[2]",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("_36_editReplyBody2")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.typeKeys("_36_editReplyBody2",RuntimeVariables.replace("This is a edited test comment."));
  selenium.type("_36_editReplyBody2",RuntimeVariables.replace("This is a edited test comment."));
  selenium.clickAt("_36_editReplyButton2",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertTrue(selenium.isTextPresent("This is a edited test comment."));
  assertFalse(selenium.isTextPresent("This is a second test comment."));
}
