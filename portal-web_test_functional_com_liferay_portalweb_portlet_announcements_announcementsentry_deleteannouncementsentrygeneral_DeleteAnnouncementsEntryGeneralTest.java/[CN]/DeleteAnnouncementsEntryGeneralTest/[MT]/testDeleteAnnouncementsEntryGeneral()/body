{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Announcements Test Page",RuntimeVariables.replace("Announcements Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Announcements Entry Title"),selenium.getText("//h3[@class='entry-title']/a"));
  assertEquals(RuntimeVariables.replace("General"),selenium.getText("//span[@class='entry-scope']"));
  assertTrue(selenium.isPartialText("//div[@class=' entry-content entry-type-general']","Announcements Entry Content"));
  assertEquals(RuntimeVariables.replace("Delete"),selenium.getText("//td[@class='delete-entry']/span/a/span"));
  selenium.click(RuntimeVariables.replace("//td[@class='delete-entry']/span/a/span"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForConfirmation("Are you sure you want to delete this? It will be deleted immediately.");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Announcements Test Page",RuntimeVariables.replace("Announcements Test Page"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isTextPresent("Announcements Entry Title"));
  assertFalse(selenium.isTextPresent("General Announcements Entry Content"));
}
