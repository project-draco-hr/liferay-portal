{
  if (!ddmFormDefaultLocale.equals(propertyValue.getDefaultLocale())) {
    throw new DDMFormValidationException("Invalid default locale set for property \"" + propertyName + "\" of field name "+ fieldName);
  }
  if (!ddmFormAvailableLocales.equals(propertyValue.getAvailableLocales())) {
    throw new DDMFormValidationException("Invalid available locales set for property \"" + propertyName + "\" of field name "+ fieldName);
  }
}
