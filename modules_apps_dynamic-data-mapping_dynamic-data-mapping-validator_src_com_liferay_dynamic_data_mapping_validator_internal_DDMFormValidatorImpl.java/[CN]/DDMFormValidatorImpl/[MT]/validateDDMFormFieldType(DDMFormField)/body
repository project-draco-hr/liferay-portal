{
  if ((ddmFormField.getType() == null) || (ddmFormField.getType().trim().length() == 0)) {
    throw new DDMFormValidationException("The field type was never set for DDM form field");
  }
  Matcher matcher=_ddmFormFieldTypePattern.matcher(ddmFormField.getType());
  if (!matcher.matches()) {
    throw new DDMFormValidationException("Invalid characters were defined for field type " + ddmFormField.getType());
  }
}
