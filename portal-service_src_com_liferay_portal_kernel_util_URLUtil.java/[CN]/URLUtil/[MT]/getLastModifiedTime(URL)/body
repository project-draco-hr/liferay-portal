{
  URLConnection urlConnection=null;
  try {
    urlConnection=url.openConnection();
    return urlConnection.getLastModified();
  }
  finally {
    if (urlConnection != null) {
      try {
        InputStream inputStream=urlConnection.getInputStream();
        inputStream.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
}
