{
  try {
    ObjectMessage objMsg=(ObjectMessage)msg;
    Object obj=objMsg.getObject();
    if (obj instanceof MailMessage) {
      _onMessage((MailMessage)obj);
    }
 else     if (obj instanceof MethodWrapper) {
      _onMessage((MethodWrapper)obj);
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
