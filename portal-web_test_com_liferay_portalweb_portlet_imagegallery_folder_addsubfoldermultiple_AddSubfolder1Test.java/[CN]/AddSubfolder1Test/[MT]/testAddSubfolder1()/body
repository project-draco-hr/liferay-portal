{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Image Gallery Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Image Gallery Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//a/strong",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//div[2]/ul/li[4]/a",RuntimeVariables.replace("Add Subfolder"));
  selenium.waitForPageToLoad("30000");
  selenium.type("_31_name",RuntimeVariables.replace("Test1 Subfolder1"));
  selenium.type("_31_description",RuntimeVariables.replace("This is Test1 Subfolder1."));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request completed successfully."));
  assertTrue(selenium.isTextPresent("Test1 Subfolder1\nThis is Test1 Subfolder1."));
}
