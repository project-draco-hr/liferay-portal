{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select groupId from Group_ where name = '" + GroupConstants.CONTROL_PANEL + "'");
    rs=ps.executeQuery();
    if (rs.next()) {
      return rs.getLong("groupId");
    }
    return 0;
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
