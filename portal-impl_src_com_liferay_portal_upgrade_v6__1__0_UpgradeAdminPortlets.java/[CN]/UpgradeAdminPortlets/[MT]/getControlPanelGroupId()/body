{
  long groupId=0;
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select groupId from Group_ where name= \"" + GroupConstants.CONTROL_PANEL + "\"");
    rs=ps.executeQuery();
    if (rs.next()) {
      groupId=rs.getLong("groupId");
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return groupId;
}
