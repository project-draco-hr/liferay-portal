{
  Long[] resourceIds=new Long[2];
  long resourceId=0;
  long codeId=0;
  codeId=increment();
  runSQL("insert into ResourceCode (codeId, companyId, name, scope) values" + " (" + codeId + ", "+ companyId+ ", "+ name+ ", "+ ResourceConstants.SCOPE_COMPANY+ ")");
  resourceId=increment();
  addResource(resourceId,String.valueOf(codeId),companyId);
  resourceIds[0]=resourceId;
  codeId=increment();
  runSQL("insert into ResourceCode (codeId, companyId, name, scope) values" + " (" + codeId + ", "+ companyId+ ", "+ name+ ", "+ ResourceConstants.SCOPE_INDIVIDUAL+ ")");
  resourceId=increment();
  long controlPanelGroupId=getControlPanelGroupId();
  addResource(resourceId,String.valueOf(codeId),controlPanelGroupId);
  resourceIds[1]=resourceId;
  return resourceIds;
}
