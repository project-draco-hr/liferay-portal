{
  Class<?> clazz=findLoadedClass(name);
  if (clazz == null) {
    if (_system != null) {
      try {
        clazz=_system.loadClass(name);
      }
 catch (      ClassNotFoundException classNotFoundException) {
      }
    }
    if (clazz == null) {
      try {
        clazz=findClass(name);
      }
 catch (      ClassNotFoundException classNotFoundException) {
        clazz=super.loadClass(name,resolve);
      }
    }
  }
  if (resolve) {
    resolveClass(clazz);
  }
  return clazz;
}
