{
  if (obj instanceof JSONObject && ((JSONObject)obj).has("javaClass")) {
    try {
      String javaClass=((JSONObject)obj).getString("javaClass");
      if (javaClass.contains("com.liferay") && javaClass.contains("Util")) {
        throw new RuntimeException("Not instantiating " + javaClass);
      }
    }
 catch (    JSONException ex) {
      _log.error(ex);
    }
  }
  return super.store(obj);
}
