{
  Map<Locale,String> nameMap=ResourceBundleUtil.getLocalizationMap("content.Language",getClass(),"layout-prototype-wiki-title");
  Map<Locale,String> descriptionMap=ResourceBundleUtil.getLocalizationMap("content.Language",getClass(),"layout-prototype-wiki-description");
  Layout layout=DefaultLayoutPrototypesUtil.addLayoutPrototype(companyId,defaultUserId,nameMap,descriptionMap,"2_columns_iii",layoutPrototypes);
  if (layout == null) {
    return;
  }
  DefaultLayoutPrototypesUtil.addPortletId(layout,WikiPortletKeys.WIKI,"column-1");
}
