{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDMContent.class,DDMContent.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("contentId",ServiceTestUtil.nextLong()));
  List<DDMContent> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
