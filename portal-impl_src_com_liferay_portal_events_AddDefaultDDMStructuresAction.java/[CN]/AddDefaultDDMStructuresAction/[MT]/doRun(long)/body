{
  ServiceContext serviceContext=new ServiceContext();
  Group group=GroupLocalServiceUtil.getGroup(companyId,GroupConstants.GUEST);
  serviceContext.setScopeGroupId(group.getGroupId());
  long defaultUserId=UserLocalServiceUtil.getDefaultUserId(companyId);
  serviceContext.setUserId(defaultUserId);
  String fileName="com/liferay/portal/events/dependencies/" + "dynamic-data-list-structures.xml";
  addDDMStructures(defaultUserId,group.getGroupId(),PortalUtil.getClassNameId(DDLRecordSet.class),serviceContext,fileName);
}
