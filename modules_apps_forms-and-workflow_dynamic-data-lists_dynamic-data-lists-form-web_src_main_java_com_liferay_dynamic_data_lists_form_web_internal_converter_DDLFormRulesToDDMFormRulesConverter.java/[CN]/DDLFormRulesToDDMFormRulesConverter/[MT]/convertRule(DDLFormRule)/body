{
  String condition=convertConditions(ddlFormRule.getLogicalOperator(),ddlFormRule.getDDLFormRuleConditions());
  List<String> actions=new ArrayList<>();
  for (  DDLFormRuleAction ddlFormRuleAction : ddlFormRule.getDDLFormRuleActions()) {
    actions.add(convertAction(ddlFormRuleAction));
  }
  return new DDMFormRule(condition,actions);
}
