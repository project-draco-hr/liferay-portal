{
  Session session=null;
  try {
    session=openSession();
    session.delete(passwordPolicyRel);
    session.flush();
    return passwordPolicyRel;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(PasswordPolicyRel.class.getName());
  }
}
