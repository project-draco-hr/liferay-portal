{
  PasswordPolicyRel result=(PasswordPolicyRel)EntityCacheUtil.getResult(PasswordPolicyRelModelImpl.ENTITY_CACHE_ENABLED,PasswordPolicyRel.class,passwordPolicyRelId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      PasswordPolicyRel passwordPolicyRel=(PasswordPolicyRel)session.get(PasswordPolicyRelImpl.class,new Long(passwordPolicyRelId));
      if (passwordPolicyRel != null) {
        cacheResult(passwordPolicyRel);
      }
      return passwordPolicyRel;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (PasswordPolicyRel)result;
  }
}
