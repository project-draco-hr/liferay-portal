{
  PasswordPolicyRel passwordPolicyRel=fetchByPrimaryKey(passwordPolicyRelId);
  if (passwordPolicyRel == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + passwordPolicyRelId);
    }
    throw new NoSuchPasswordPolicyRelException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + passwordPolicyRelId);
  }
  return passwordPolicyRel;
}
