{
  String cmd=ParamUtil.getString(actionRequest,Constants.CMD);
  try {
    if (cmd.equals("merge")) {
      merge(actionRequest,actionResponse);
    }
 else     if (cmd.equals("reassign")) {
      reassign(actionRequest,actionResponse);
    }
    sendRedirect(actionRequest,actionResponse);
  }
 catch (  Exception e) {
    if (e instanceof NoSuchRoleException || e instanceof PrincipalException || e instanceof RolePermissionsException) {
      SessionErrors.add(actionRequest,e.getClass().getName());
      setForward(actionRequest,"portlet.admin.error");
    }
 else {
      throw e;
    }
  }
}
