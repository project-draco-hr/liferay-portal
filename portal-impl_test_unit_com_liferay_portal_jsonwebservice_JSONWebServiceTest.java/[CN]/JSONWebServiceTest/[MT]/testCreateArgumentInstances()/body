{
  MockHttpServletRequest mockHttpServletRequest=_createHttpRequest("/foo/use1/+foo-data");
  JSONWebServiceAction jsonWebServiceAction=_lookupAction(mockHttpServletRequest);
  assertEquals("using #1: foo!",jsonWebServiceAction.invoke());
  mockHttpServletRequest=_createHttpRequest("/foo/use2/+foo-data");
  jsonWebServiceAction=_lookupAction(mockHttpServletRequest);
  try {
    jsonWebServiceAction.invoke();
    fail();
  }
 catch (  IllegalArgumentException ignore) {
  }
  mockHttpServletRequest=_createHttpRequest("/foo/use2/+foo-data:" + "com.liferay.portal.jsonwebservice.dependencies.FooDataImpl");
  jsonWebServiceAction=_lookupAction(mockHttpServletRequest);
  assertEquals("using #2: foo!",jsonWebServiceAction.invoke());
}
