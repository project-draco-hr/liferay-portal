{
  JSONWebServiceMappingResolver jsonWebServiceMappingResolver=new JSONWebServiceMappingResolver();
  Method[] methods=actionClass.getMethods();
  for (  Method actionMethod : methods) {
    if (actionMethod.getDeclaringClass() != actionClass) {
      continue;
    }
    String actionPath=jsonWebServiceMappingResolver.resolvePath(actionClass,actionMethod);
    String actionHttpMethod=jsonWebServiceMappingResolver.resolveHttpMethod(actionMethod);
    JSONWebServiceActionsManagerUtil.registerJSONWebServiceAction("",actionClass,actionMethod,actionPath,actionHttpMethod);
  }
}
