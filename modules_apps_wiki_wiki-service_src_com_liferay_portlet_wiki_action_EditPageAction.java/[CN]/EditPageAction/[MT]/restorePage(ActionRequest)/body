{
  long[] restoreEntryIds=StringUtil.split(ParamUtil.getString(actionRequest,"restoreTrashEntryIds"),0L);
  for (  long restoreEntryId : restoreEntryIds) {
    long overridePageResourcePrimKey=0;
    TrashEntry trashEntry=TrashEntryLocalServiceUtil.getTrashEntry(restoreEntryId);
    WikiPageResource pageResource=WikiPageResourceLocalServiceUtil.getPageResource(trashEntry.getClassPK());
    String title=TrashUtil.getOriginalTitle(pageResource.getTitle());
    if (title.equals(WikiPageConstants.FRONT_PAGE)) {
      WikiPage overridePage=WikiPageLocalServiceUtil.fetchPage(pageResource.getNodeId(),WikiPageConstants.FRONT_PAGE);
      if (overridePage != null) {
        overridePageResourcePrimKey=overridePage.getResourcePrimKey();
      }
    }
    TrashEntryServiceUtil.restoreEntry(restoreEntryId,overridePageResourcePrimKey,null);
  }
}
