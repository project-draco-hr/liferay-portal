{
  _componentContext=componentContext;
  BundleContext bundleContext=componentContext.getBundleContext();
  try {
    _serviceTracker=new ServiceTracker<Object,Object>(bundleContext,bundleContext.createFilter(_JSONWS_PATH_FILTER),this);
    _serviceTracker.open();
  }
 catch (  InvalidSyntaxException ise) {
    throw new RuntimeException("Unable to activate the JSON WS Tracker",ise);
  }
}
