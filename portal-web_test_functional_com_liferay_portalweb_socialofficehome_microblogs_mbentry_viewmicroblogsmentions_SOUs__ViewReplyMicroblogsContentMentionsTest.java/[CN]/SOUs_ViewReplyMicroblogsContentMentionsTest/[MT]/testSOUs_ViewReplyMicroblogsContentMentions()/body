{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/user/socialoffice01/so/dashboard");
  selenium.waitForVisible("link=Me");
  selenium.clickAt("link=Me",RuntimeVariables.replace("Me"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("@Joe Bloggs: Microblogs Post Comment [@joebloggs]"),selenium.getText("xPath=(//div[@class='activity-title'])[1]"));
}
