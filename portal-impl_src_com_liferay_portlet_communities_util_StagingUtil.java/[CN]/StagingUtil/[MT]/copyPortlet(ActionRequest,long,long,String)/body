{
  Map<String,String[]> parameterMap=getStagingParameters(req);
  byte[] bytes=LayoutLocalServiceUtil.exportPortletInfo(sourcePlid,portletId,parameterMap,null,null);
  ByteArrayInputStream bais=new ByteArrayInputStream(bytes);
  LayoutServiceUtil.importPortletInfo(targetPlid,portletId,parameterMap,bais);
}
