{
  FileCacheOutputStream fcos=LayoutLocalServiceUtil.exportLayoutsAsStream(sourceGroupId,privateLayout,layoutIds,parameterMap,startDate,endDate);
  InputStream is=fcos.getFileInputStream();
  try {
    LayoutServiceUtil.importLayouts(targetGroupId,privateLayout,parameterMap,is);
  }
  finally {
    try {
      is.close();
      fcos.cleanUp();
    }
 catch (    Exception e) {
    }
  }
}
