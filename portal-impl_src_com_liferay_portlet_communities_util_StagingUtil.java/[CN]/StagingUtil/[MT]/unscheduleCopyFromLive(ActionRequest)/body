{
  long stagingGroupId=ParamUtil.getLong(actionRequest,"stagingGroupId");
  String jobName=ParamUtil.getString(actionRequest,"jobName");
  String groupName=getSchedulerGroupName(DestinationNames.LAYOUTS_LOCAL_PUBLISHER,stagingGroupId);
  LayoutServiceUtil.unschedulePublishToLive(stagingGroupId,jobName,groupName);
}
