{
  Map<String,String[]> parameterMap=getStagingParameters(actionRequest);
  FileCacheOutputStream fcos=LayoutLocalServiceUtil.exportPortletInfoAsStream(sourcePlid,sourceGroupId,portletId,parameterMap,null,null);
  try {
    LayoutServiceUtil.importPortletInfo(targetPlid,targetGroupId,portletId,parameterMap,fcos.getFileInputStream());
  }
  finally {
    fcos.cleanUp();
  }
}
