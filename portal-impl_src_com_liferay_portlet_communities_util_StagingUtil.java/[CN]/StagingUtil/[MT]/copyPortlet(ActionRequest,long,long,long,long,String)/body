{
  Map<String,String[]> parameterMap=getStagingParameters(actionRequest);
  FileCacheOutputStream fileCacheOutputStream=LayoutLocalServiceUtil.exportPortletInfoAsStream(sourcePlid,sourceGroupId,portletId,parameterMap,null,null);
  try {
    LayoutServiceUtil.importPortletInfo(targetPlid,targetGroupId,portletId,parameterMap,fileCacheOutputStream.getFileInputStream());
  }
  finally {
    fileCacheOutputStream.close();
  }
}
