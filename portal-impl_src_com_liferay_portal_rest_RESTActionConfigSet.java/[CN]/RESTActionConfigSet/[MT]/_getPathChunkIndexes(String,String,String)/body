{
  int index=pathChunk.indexOf(leftBoundary,0);
  if (index == -1) {
    return null;
  }
  int[] indexes=new int[4];
  indexes[0]=index;
  index+=leftBoundary.length();
  indexes[1]=index;
  index=pathChunk.indexOf(rightBoundary,index);
  if (index == -1) {
    return null;
  }
  indexes[2]=index;
  indexes[3]=index + rightBoundary.length();
  return indexes;
}
