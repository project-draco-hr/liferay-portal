{
  if (portletPreferences == null) {
    return;
  }
  try {
    SubscriptionLocalServiceUtil.deleteSubscriptions(portletPreferences.getCompanyId(),portletPreferences.getModelClassName(),portletPreferences.getPortletPreferencesId());
  }
 catch (  Exception e) {
    _log.error("Unable to delete subscriptions",e);
  }
}
