{
  JSONObject jsonObj=new JSONObject();
  String banId=model.getBanId();
  if (banId == null) {
    jsonObj.put("banId",StringPool.BLANK);
  }
 else {
    jsonObj.put("banId",banId.toString());
  }
  jsonObj.put("groupId",model.getGroupId());
  String companyId=model.getCompanyId();
  if (companyId == null) {
    jsonObj.put("companyId",StringPool.BLANK);
  }
 else {
    jsonObj.put("companyId",companyId.toString());
  }
  String userId=model.getUserId();
  if (userId == null) {
    jsonObj.put("userId",StringPool.BLANK);
  }
 else {
    jsonObj.put("userId",userId.toString());
  }
  String userName=model.getUserName();
  if (userName == null) {
    jsonObj.put("userName",StringPool.BLANK);
  }
 else {
    jsonObj.put("userName",userName.toString());
  }
  Date createDate=model.getCreateDate();
  if (createDate == null) {
    jsonObj.put("createDate",StringPool.BLANK);
  }
 else {
    jsonObj.put("createDate",createDate.toString());
  }
  Date modifiedDate=model.getModifiedDate();
  if (modifiedDate == null) {
    jsonObj.put("modifiedDate",StringPool.BLANK);
  }
 else {
    jsonObj.put("modifiedDate",modifiedDate.toString());
  }
  String banUserId=model.getBanUserId();
  if (banUserId == null) {
    jsonObj.put("banUserId",StringPool.BLANK);
  }
 else {
    jsonObj.put("banUserId",banUserId.toString());
  }
  return jsonObj;
}
