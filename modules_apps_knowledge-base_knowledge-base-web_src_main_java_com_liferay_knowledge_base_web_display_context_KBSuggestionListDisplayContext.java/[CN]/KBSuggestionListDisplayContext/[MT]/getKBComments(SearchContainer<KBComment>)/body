{
  if (_kbArticle == null) {
    return KBCommentServiceUtil.getKBComments(_groupId,searchContainer.getStart(),searchContainer.getEnd(),new KBCommentStatusComparator());
  }
 else {
    return KBCommentServiceUtil.getKBComments(_groupId,KBArticleConstants.getClassName(),_kbArticle.getClassPK(),searchContainer.getStart(),searchContainer.getEnd(),new KBCommentStatusComparator());
  }
}
