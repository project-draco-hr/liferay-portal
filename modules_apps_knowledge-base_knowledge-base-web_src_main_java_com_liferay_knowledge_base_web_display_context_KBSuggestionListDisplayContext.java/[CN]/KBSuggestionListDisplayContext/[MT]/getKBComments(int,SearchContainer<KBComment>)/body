{
  if (_kbArticle == null) {
    return KBCommentServiceUtil.getKBComments(_groupId,status,searchContainer.getStart(),searchContainer.getEnd());
  }
 else {
    return KBCommentServiceUtil.getKBComments(_groupId,KBArticleConstants.getClassName(),_kbArticle.getClassPK(),status,searchContainer.getStart(),searchContainer.getEnd());
  }
}
