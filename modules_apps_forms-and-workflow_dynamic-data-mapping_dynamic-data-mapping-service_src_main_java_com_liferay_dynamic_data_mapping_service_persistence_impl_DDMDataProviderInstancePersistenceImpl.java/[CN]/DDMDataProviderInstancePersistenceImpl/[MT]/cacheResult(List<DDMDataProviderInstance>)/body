{
  for (  DDMDataProviderInstance ddmDataProviderInstance : ddmDataProviderInstances) {
    if (entityCache.getResult(DDMDataProviderInstanceModelImpl.ENTITY_CACHE_ENABLED,DDMDataProviderInstanceImpl.class,ddmDataProviderInstance.getPrimaryKey()) == null) {
      cacheResult(ddmDataProviderInstance);
    }
 else {
      ddmDataProviderInstance.resetOriginalValues();
    }
  }
}
