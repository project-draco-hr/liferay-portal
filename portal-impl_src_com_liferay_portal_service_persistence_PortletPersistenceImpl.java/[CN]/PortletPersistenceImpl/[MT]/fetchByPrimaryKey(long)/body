{
  Portlet portlet=(Portlet)EntityCacheUtil.getResult(PortletModelImpl.ENTITY_CACHE_ENABLED,PortletImpl.class,id,this);
  if (portlet == null) {
    Session session=null;
    try {
      session=openSession();
      portlet=(Portlet)session.get(PortletImpl.class,Long.valueOf(id));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (portlet != null) {
        cacheResult(portlet);
      }
      closeSession(session);
    }
  }
  return portlet;
}
