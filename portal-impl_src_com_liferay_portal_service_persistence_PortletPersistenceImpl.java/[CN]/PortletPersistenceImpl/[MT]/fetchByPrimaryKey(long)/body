{
  Portlet result=(Portlet)EntityCacheUtil.getResult(PortletModelImpl.ENTITY_CACHE_ENABLED,Portlet.class,id,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      Portlet portlet=(Portlet)session.get(PortletImpl.class,new Long(id));
      if (portlet != null) {
        cacheResult(portlet);
      }
      return portlet;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (Portlet)result;
  }
}
