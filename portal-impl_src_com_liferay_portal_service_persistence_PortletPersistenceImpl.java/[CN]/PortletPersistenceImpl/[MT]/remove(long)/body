{
  Session session=null;
  try {
    session=openSession();
    Portlet portlet=(Portlet)session.get(PortletImpl.class,new Long(id));
    if (portlet == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No Portlet exists with the primary key " + id);
      }
      throw new NoSuchPortletException("No Portlet exists with the primary key " + id);
    }
    return remove(portlet);
  }
 catch (  NoSuchPortletException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
