{
  Session session=null;
  try {
    session=openSession();
    Portlet portlet=(Portlet)session.get(PortletImpl.class,Long.valueOf(id));
    if (portlet == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + id);
      }
      throw new NoSuchPortletException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + id);
    }
    return portletPersistence.remove(portlet);
  }
 catch (  NoSuchPortletException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
