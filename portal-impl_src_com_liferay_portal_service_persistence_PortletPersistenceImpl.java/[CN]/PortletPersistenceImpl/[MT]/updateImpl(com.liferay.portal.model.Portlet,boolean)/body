{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,portlet,merge);
    portlet.setNew(false);
    return portlet;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Portlet.class.getName());
  }
}
