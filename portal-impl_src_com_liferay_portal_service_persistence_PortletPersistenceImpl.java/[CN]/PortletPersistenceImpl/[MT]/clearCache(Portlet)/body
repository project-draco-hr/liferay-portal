{
  EntityCacheUtil.removeResult(PortletModelImpl.ENTITY_CACHE_ENABLED,PortletImpl.class,portlet.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_P,new Object[]{Long.valueOf(portlet.getCompanyId()),portlet.getPortletId()});
}
