{
  String finderClassName=Portlet.class.getName();
  String finderMethodName="fetchByC_P";
  Object[] finderArgs=new Object[]{new Long(companyId),portletId};
  Object result=FinderCache.getResult(finderClassName,finderMethodName,finderArgs);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringMaker query=new StringMaker();
      query.append("FROM com.liferay.portal.model.Portlet WHERE ");
      query.append("companyId = ?");
      query.append(" AND ");
      if (portletId == null) {
        query.append("portletId IS NULL");
      }
 else {
        query.append("portletId = ?");
      }
      query.append(" ");
      Query q=session.createQuery(query.toString());
      int queryPos=0;
      q.setLong(queryPos++,companyId);
      if (portletId != null) {
        q.setString(queryPos++,portletId);
      }
      List list=q.list();
      if (list.size() == 0) {
        return null;
      }
      Portlet portlet=(Portlet)list.get(0);
      FinderCache.putResult(finderClassName,finderMethodName,finderArgs,portlet);
      return portlet;
    }
 catch (    Exception e) {
      throw HibernateUtil.processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (Portlet)result;
  }
}
