{
  String finderClassName=Portlet.class.getName();
  String finderMethodName="fetchByC_P";
  String[] finderParams=new String[]{Long.class.getName(),String.class.getName()};
  Object[] finderArgs=new Object[]{new Long(companyId),portletId};
  Object result=FinderCache.getResult(finderClassName,finderMethodName,finderParams,finderArgs);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      StringMaker query=new StringMaker();
      query.append("FROM com.liferay.portal.model.Portlet WHERE ");
      query.append("companyId = ?");
      query.append(" AND ");
      if (portletId == null) {
        query.append("portletId IS NULL");
      }
 else {
        query.append("portletId = ?");
      }
      query.append(" ");
      Query q=session.createQuery(query.toString());
      int queryPos=0;
      q.setLong(queryPos++,companyId);
      if (portletId != null) {
        q.setString(queryPos++,portletId);
      }
      List list=q.list();
      FinderCache.putResult(finderClassName,finderMethodName,finderParams,finderArgs,list);
      if (list.size() == 0) {
        return null;
      }
 else {
        return (Portlet)list.get(0);
      }
    }
 catch (    Exception e) {
      throw HibernateUtil.processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    List list=(List)result;
    if (list.size() == 0) {
      return null;
    }
 else {
      return (Portlet)list.get(0);
    }
  }
}
