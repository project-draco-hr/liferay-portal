{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(portlet);
    }
  }
  portlet=removeImpl(portlet);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(portlet);
    }
  }
  return portlet;
}
