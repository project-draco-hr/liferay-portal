{
  Session session=null;
  try {
    session=openSession();
    portlet=(Portlet)session.merge(portlet);
    session.delete(portlet);
    session.flush();
    return portlet;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Portlet.class.getName());
  }
}
