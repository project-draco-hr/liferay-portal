{
  long pk=nextLong();
  ServiceComponent newServiceComponent=_persistence.create(pk);
  newServiceComponent.setBuildNamespace(randomString());
  newServiceComponent.setBuildNumber(nextLong());
  newServiceComponent.setBuildDate(nextLong());
  newServiceComponent.setData(randomString());
  _persistence.update(newServiceComponent,false);
  ServiceComponent existingServiceComponent=_persistence.findByPrimaryKey(newServiceComponent.getPrimaryKey());
  assertEquals(existingServiceComponent.getServiceComponentId(),newServiceComponent.getServiceComponentId());
  assertEquals(existingServiceComponent.getBuildNamespace(),newServiceComponent.getBuildNamespace());
  assertEquals(existingServiceComponent.getBuildNumber(),newServiceComponent.getBuildNumber());
  assertEquals(existingServiceComponent.getBuildDate(),newServiceComponent.getBuildDate());
  assertEquals(existingServiceComponent.getData(),newServiceComponent.getData());
}
