{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLFileVersion.class,DLFileVersion.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("fileVersionId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("fileVersionId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
