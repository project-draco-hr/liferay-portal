{
  CategoriesProperty categoriesProperty=(CategoriesProperty)EntityCacheUtil.getResult(CategoriesPropertyModelImpl.ENTITY_CACHE_ENABLED,CategoriesPropertyImpl.class,propertyId,this);
  if (categoriesProperty == null) {
    Session session=null;
    try {
      session=openSession();
      categoriesProperty=(CategoriesProperty)session.get(CategoriesPropertyImpl.class,new Long(propertyId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (categoriesProperty != null) {
        cacheResult(categoriesProperty);
      }
      closeSession(session);
    }
  }
  return categoriesProperty;
}
