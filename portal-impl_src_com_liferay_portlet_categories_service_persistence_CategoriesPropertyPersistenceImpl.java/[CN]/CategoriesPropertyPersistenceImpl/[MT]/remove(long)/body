{
  Session session=null;
  try {
    session=openSession();
    CategoriesProperty categoriesProperty=(CategoriesProperty)session.get(CategoriesPropertyImpl.class,new Long(propertyId));
    if (categoriesProperty == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No CategoriesProperty exists with the primary key " + propertyId);
      }
      throw new NoSuchPropertyException("No CategoriesProperty exists with the primary key " + propertyId);
    }
    return remove(categoriesProperty);
  }
 catch (  NoSuchPropertyException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
