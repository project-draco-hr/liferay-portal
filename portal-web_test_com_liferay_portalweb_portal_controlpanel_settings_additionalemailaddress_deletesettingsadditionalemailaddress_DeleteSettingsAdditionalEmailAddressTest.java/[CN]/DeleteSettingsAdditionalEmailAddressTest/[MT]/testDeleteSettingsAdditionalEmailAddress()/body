{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Control Panel");
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Portal Settings",RuntimeVariables.replace("Portal Settings"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForVisible("//a[@id='_130_additionalEmailAddressesLink']");
  selenium.clickAt("//a[@id='_130_additionalEmailAddressesLink']",RuntimeVariables.replace("Additional Email Addresses"));
  assertEquals("Admin@Liferay.com",selenium.getValue("//input[@id='_130_emailAddressAddress0']"));
  selenium.waitForVisible("//button[2]");
  selenium.clickAt("//button[2]",RuntimeVariables.replace("Delete"));
  assertFalse(selenium.isTextPresent("Admin@Liferay.com"));
}
