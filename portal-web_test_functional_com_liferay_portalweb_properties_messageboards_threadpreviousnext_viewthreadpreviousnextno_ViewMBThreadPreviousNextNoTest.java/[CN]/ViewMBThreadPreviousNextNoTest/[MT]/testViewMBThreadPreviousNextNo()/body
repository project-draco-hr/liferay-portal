{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Message Boards Test Page",RuntimeVariables.replace("Message Boards Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("MB Thread2 Message Subject"),selenium.getText("//tr[contains(.,'MB Thread2 Message Subject')]/td[1]/a"));
  selenium.clickAt("//tr[contains(.,'MB Thread2 Message Subject')]/td[1]/a",RuntimeVariables.replace("MB Thread2 Message Subject"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isTextPresent("Previous"));
  assertFalse(selenium.isTextPresent("Next"));
}
