{
  ResourceBundle resourceBundle=_resourceBundleTracker.getResourceBundle(LocaleUtil.toLanguageId(locale));
  if (resourceBundle == null) {
    resourceBundle=_resourceBundleTracker.getResourceBundle(locale.getLanguage());
    if (resourceBundle == null) {
      resourceBundle=_resourceBundleTracker.getResourceBundle(LocaleUtil.toLanguageId(LocaleUtil.getDefault()));
    }
  }
  return resourceBundle;
}
