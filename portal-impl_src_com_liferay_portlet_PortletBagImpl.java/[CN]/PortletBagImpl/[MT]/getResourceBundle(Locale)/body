{
  ResourceBundle resourceBundle=_resourceBundleTracker.getResouceBundle(LocaleUtil.toLanguageId(locale));
  if (resourceBundle == null) {
    resourceBundle=_resourceBundleTracker.getResouceBundle(locale.getLanguage());
    if (resourceBundle == null) {
      resourceBundle=_resourceBundleTracker.getResouceBundle(LocaleUtil.toLanguageId(LocaleUtil.getDefault()));
    }
  }
  return resourceBundle;
}
