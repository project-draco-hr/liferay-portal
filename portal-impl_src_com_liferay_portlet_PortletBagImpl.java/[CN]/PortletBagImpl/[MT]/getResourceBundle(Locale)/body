{
  ResourceBundleLoader resourceBundleLoader=ResourceBundleLoaderUtil.getResourceBundleLoaderByServletContextNameAndBaseName(_servletContext.getServletContextName(),getResourceBundle());
  if (resourceBundleLoader == null) {
    return null;
  }
  return resourceBundleLoader.loadResourceBundle(LocaleUtil.toLanguageId(locale));
}
