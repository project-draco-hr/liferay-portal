{
  if (_request == null) {
    _request=(HttpServletRequest)pageContext.getRequest();
  }
  _request.setAttribute("liferay-ui:search-container-column-status:status",String.valueOf(_status));
  if (_servletContext != null) {
    RequestDispatcher requestDispatcher=DirectRequestDispatcherFactoryUtil.getRequestDispatcher(_servletContext,_PAGE);
    requestDispatcher.include(_request,new PipingServletResponse(pageContext));
  }
 else {
    pageContext.include(_PAGE);
  }
}
