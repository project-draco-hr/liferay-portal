{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select * from ExpandoValue where tableId = ? and rowId_ = ?");
    ps.setLong(1,wolTableId);
    ps.setLong(2,wolRowId);
    rs=ps.executeQuery();
    while (rs.next()) {
      long valueId=rs.getLong("valueId");
      long columnId=rs.getLong("columnId");
      long newTableId=0;
      long newRowId=0;
      if (columnId == scColumnId) {
        newRowId=scRowId;
        newTableId=scTableId;
      }
 else       if (columnId == snColumnId) {
        newRowId=snRowId;
        newTableId=snTableId;
      }
      runSQL("update ExpandoValue set tableId = " + newTableId + ", rowId_ = "+ newRowId+ " where "+ "valueId = "+ valueId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
