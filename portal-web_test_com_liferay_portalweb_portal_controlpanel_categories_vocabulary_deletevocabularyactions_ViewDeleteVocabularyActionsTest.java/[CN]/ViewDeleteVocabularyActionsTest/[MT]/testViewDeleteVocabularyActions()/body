{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Categories",RuntimeVariables.replace("Categories"));
  selenium.waitForPageToLoad("30000");
  assertFalse(selenium.isElementPresent("//span[@class='vocabulary-item']/a"));
  assertFalse(selenium.isTextPresent("Vocabulary Name"));
}
