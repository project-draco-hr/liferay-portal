{
  DDMFormFieldValue ddmFormFieldValue=new DDMFormFieldValue();
  ddmFormFieldValue.setName("Text");
  ddmFormFieldValue.setValue(new UnlocalizedValue(StringUtil.randomString()));
  _defaultDDMFormFieldValueRenderer.render(ddmFormFieldValue,LocaleUtil.US);
  Mockito.verify(_html).escape(Matchers.anyString());
}
