{
  try {
    List<Layout> layouts=LayoutLocalServiceUtil.getLayouts(getGroupId(),privateLayout,LayoutImpl.DEFAULT_PARENT_LAYOUT_ID,0,1);
    if (layouts.size() > 0) {
      Layout layout=layouts.get(0);
      return layout.getPlid();
    }
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn(e.getMessage());
    }
  }
  return LayoutImpl.DEFAULT_PLID;
}
