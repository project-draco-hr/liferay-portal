{
  String iconURL=StringPool.BLANK;
  if (isCompany()) {
    iconURL="../aui/globe";
  }
 else   if (isLayout()) {
    iconURL="../aui/page";
  }
 else   if (isOrganization()) {
    iconURL="../aui/globe";
  }
 else   if (isUser()) {
    iconURL="../aui/user";
  }
 else {
    iconURL="../aui/globe";
  }
  return iconURL;
}
