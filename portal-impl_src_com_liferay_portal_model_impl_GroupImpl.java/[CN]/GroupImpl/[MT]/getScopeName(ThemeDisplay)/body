{
  String name=null;
  if (getGroupId() == themeDisplay.getScopeGroupId()) {
    StringBundler sb=new StringBundler(5);
    sb.append(LanguageUtil.get(themeDisplay.getLocale(),"current-site"));
    sb.append(StringPool.SPACE);
    sb.append(StringPool.OPEN_PARENTHESIS);
    sb.append(HtmlUtil.escape(getDescriptiveName(themeDisplay.getLocale())));
    sb.append(StringPool.CLOSE_PARENTHESIS);
    name=sb.toString();
  }
 else   if (isLayout() && (getClassPK() == themeDisplay.getPlid())) {
    StringBundler sb=new StringBundler(5);
    sb.append(LanguageUtil.get(themeDisplay.getLocale(),"current-page"));
    sb.append(StringPool.SPACE);
    sb.append(StringPool.OPEN_PARENTHESIS);
    sb.append(HtmlUtil.escape(getDescriptiveName(themeDisplay.getLocale())));
    sb.append(StringPool.CLOSE_PARENTHESIS);
    name=sb.toString();
  }
 else   if (isLayoutPrototype()) {
    name=LanguageUtil.get(themeDisplay.getLocale(),"default");
  }
 else {
    name=HtmlUtil.escape(getDescriptiveName(themeDisplay.getLocale()));
  }
  return name;
}
