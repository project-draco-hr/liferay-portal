    if (location == null) {
      sm.append(CustomSQLUtil.get(COUNT_BY_C_PO_N_S_C_Z_R_C));
    }
 else {
      sm.append(CustomSQLUtil.get(COUNT_BY_C_PO_L_N_S_C_Z_R_C));
    }
    String sql=sm.toString();
    if (regionId == null) {
      sql=StringUtil.replace(sql,_REGION_ID_SQL,StringPool.BLANK);
    }
    if (countryId == null) {
      sql=StringUtil.replace(sql,_COUNTRY_ID_SQL,StringPool.BLANK);
    }
    sql=StringUtil.replace(sql,"[$JOIN$]",_getJoin("groupsPermissions"));
    sql=StringUtil.replace(sql,"[$WHERE$]",_getWhere("groupsPermissions"));
    sm=new StringMaker();
    sm.append(sql);
    sm.append(") UNION (");
    if (location == null) {
      sm.append(CustomSQLUtil.get(COUNT_BY_C_PO_N_S_C_Z_R_C));
    }
 else {
      sm.append(CustomSQLUtil.get(COUNT_BY_C_PO_L_N_S_C_Z_R_C));
    }
    sql=sm.toString();
    if (regionId == null) {
      sql=StringUtil.replace(sql,_REGION_ID_SQL,StringPool.BLANK);
    }
    if (countryId == null) {
      sql=StringUtil.replace(sql,_COUNTRY_ID_SQL,StringPool.BLANK);
    }
