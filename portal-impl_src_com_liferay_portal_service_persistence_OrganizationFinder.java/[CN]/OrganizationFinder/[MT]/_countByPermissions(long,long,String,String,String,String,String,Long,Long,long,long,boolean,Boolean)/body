        qPos.add(regionId);
        qPos.add(regionId);
      }
      if (countryId != null) {
        qPos.add(countryId);
        qPos.add(countryId);
      }
      qPos.add(city);
      qPos.add(city);
      qPos.add(zip);
      qPos.add(zip);
    }
    int count=0;
    Iterator itr=q.list().iterator();
    while (itr.hasNext()) {
      Long l=(Long)itr.next();
      if (l != null) {
        count+=l.intValue();
      }
    }
    return count;
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    HibernateUtil.closeSession(session);
  }
