{
  AssetLink link=null;
  try {
    link=assetLinkPersistence.findByE_E_T(entryId1,entryId2,type);
  }
 catch (  NoSuchLinkException nsle) {
  }
  if (Validator.isNull(link) && (type == AssetLinkConstants.TYPE_RELATED)) {
    try {
      link=assetLinkPersistence.findByE_E_T(entryId2,entryId1,type);
    }
 catch (    NoSuchLinkException nsle) {
    }
  }
  if (Validator.isNull(link)) {
    link=addLink(userId,entryId1,entryId2,type,weight);
  }
  return link;
}
