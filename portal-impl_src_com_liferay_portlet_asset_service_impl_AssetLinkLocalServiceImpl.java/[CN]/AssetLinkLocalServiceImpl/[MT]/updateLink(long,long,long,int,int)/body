{
  AssetLink link=assetLinkPersistence.fetchByE_E_T(entryId1,entryId2,type);
  if ((link == null) && (type == AssetLinkConstants.TYPE_RELATED)) {
    link=assetLinkPersistence.fetchByE_E_T(entryId2,entryId1,type);
  }
  if (link == null) {
    link=addLink(userId,entryId1,entryId2,type,weight);
  }
  return link;
}
