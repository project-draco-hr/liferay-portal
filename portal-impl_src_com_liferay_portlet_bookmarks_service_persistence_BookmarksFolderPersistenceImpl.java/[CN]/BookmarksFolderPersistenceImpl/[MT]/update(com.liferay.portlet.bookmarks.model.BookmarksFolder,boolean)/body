{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(bookmarksFolder);
    }
 else {
      if (bookmarksFolder.isNew()) {
        session.save(bookmarksFolder);
      }
 else {
        bookmarksFolder=(BookmarksFolder)session.merge(bookmarksFolder);
      }
    }
    session.flush();
    bookmarksFolder.setNew(false);
    return bookmarksFolder;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(BookmarksFolder.class.getName());
  }
}
