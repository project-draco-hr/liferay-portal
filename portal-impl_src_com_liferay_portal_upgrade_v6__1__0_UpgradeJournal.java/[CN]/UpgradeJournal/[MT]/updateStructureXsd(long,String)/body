{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("update JournalStructure set xsd = ? where id_ = ?");
    ps.setString(1,xsd);
    ps.setLong(2,id);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
