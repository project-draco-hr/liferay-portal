{
  Map parameterMap=context.getParameterMap();
  boolean exportData=MapUtil.getBoolean(parameterMap,_EXPORT_PAGE_COMMENTS_DATA);
  if (_log.isDebugEnabled()) {
    if (exportData) {
      _log.debug("Exporting data is enabled");
    }
 else {
      _log.debug("Exporting data is disabled");
    }
  }
  if (!exportData) {
    return null;
  }
  try {
    context.addComments(Layout.class,new Long(context.getPlid()));
    return String.valueOf(context.getPlid());
  }
 catch (  Exception e) {
    throw new PortletDataException(e);
  }
}
