{
  Map parameterMap=context.getParameterMap();
  boolean importData=MapUtil.getBoolean(parameterMap,_IMPORT_PAGE_COMMENTS_DATA);
  boolean staging=MapUtil.getBoolean(parameterMap,PortletDataHandlerKeys.STAGING);
  if (_log.isDebugEnabled()) {
    if (importData) {
      _log.debug("Importing data is enabled");
    }
 else {
      _log.debug("Importing data is disabled");
    }
  }
  if (!importData && !staging) {
    return null;
  }
  try {
    context.importComments(Layout.class,data,new Long(context.getPlid()),context.getGroupId());
    return null;
  }
 catch (  Exception e) {
    throw new PortletDataException(e);
  }
}
