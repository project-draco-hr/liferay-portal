{
  if (classPK <= 0) {
    return null;
  }
  long classNameId=PortalUtil.getClassNameId(className);
  Asset asset=assetPersistence.fetchByC_C(classNameId,classPK);
  if (asset != null) {
    asset.setViewCount(asset.getViewCount() + 1);
    assetPersistence.update(asset,false);
  }
  return asset;
}
