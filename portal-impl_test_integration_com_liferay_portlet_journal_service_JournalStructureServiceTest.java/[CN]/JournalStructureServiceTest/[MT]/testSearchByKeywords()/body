{
  addStructure(groupId,generateId(),"First Structure",getDefultXsd());
  addStructure(getCompanyGroupId(),generateId(),"Second Structure",getDefultXsd());
  long[] groupIds=new long[]{getCompanyGroupId(),groupId};
  List<JournalStructure> structures=JournalStructureLocalServiceUtil.search(companyId,groupIds,"Structure",QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  Assert.assertEquals(2,structures.size());
}
