{
  String structureId=generateId();
  String xsd=getDefultXsd();
  xsd.concat("\\n\\n");
  addStructure(groupId,structureId,xsd);
  JournalStructureLocalServiceUtil.checkNewLine(groupId,structureId);
  JournalStructure structure=JournalStructureLocalServiceUtil.getStructure(groupId,structureId);
  String xsl=structure.getXsd();
  Assert.assertFalse(xsl.contains("\\n"));
}
