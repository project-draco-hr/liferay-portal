{
  String oldStructureId=generateId();
  String xsd=getDefultXsd();
  addStructure(groupId,oldStructureId,xsd);
  String newStructureId=generateId();
  JournalStructure structure=JournalStructureLocalServiceUtil.copyStructure(TestPropsValues.getUserId(),groupId,oldStructureId,newStructureId,false);
  Assert.assertEquals(newStructureId,structure.getStructureId());
  Assert.assertEquals(JournalTestUtil.getXsdMap(xsd),JournalTestUtil.getXsdMap(structure.getXsd()));
}
