{
  String oldStructureId=generateId();
  String xsd=getDefultXsd();
  addStructure(groupId,oldStructureId,xsd);
  String newStructureId=generateId();
  JournalStructure template=JournalStructureLocalServiceUtil.copyStructure(TestPropsValues.getUserId(),groupId,oldStructureId,newStructureId,false);
  Assert.assertEquals(newStructureId,template.getStructureId());
  Assert.assertEquals(xsd,template.getXsd());
}
