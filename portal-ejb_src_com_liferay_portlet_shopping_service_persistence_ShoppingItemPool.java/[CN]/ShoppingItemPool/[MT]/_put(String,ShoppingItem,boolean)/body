{
  if (!_cacheable) {
    return obj;
  }
 else   if (itemId == null) {
    return obj;
  }
 else {
    String key=_encodeKey(itemId);
    if (Validator.isNotNull(key)) {
      if (flush) {
        _cache.flushEntry(key);
      }
      _cache.putInCache(key,obj,GROUP_NAME_ARRAY);
    }
    return obj;
  }
}
