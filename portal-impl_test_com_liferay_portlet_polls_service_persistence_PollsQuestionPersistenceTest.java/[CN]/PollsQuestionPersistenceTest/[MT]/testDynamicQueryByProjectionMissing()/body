{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PollsQuestion.class,PollsQuestion.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("questionId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("questionId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
