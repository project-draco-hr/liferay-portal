{
  PollsQuestion newPollsQuestion=addPollsQuestion();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PollsQuestion.class,PollsQuestion.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("questionId",newPollsQuestion.getQuestionId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  PollsQuestion existingPollsQuestion=(PollsQuestion)result.get(0);
  assertEquals(existingPollsQuestion,newPollsQuestion);
}
