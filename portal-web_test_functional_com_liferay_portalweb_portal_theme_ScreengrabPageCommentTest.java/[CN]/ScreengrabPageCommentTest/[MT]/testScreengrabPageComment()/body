{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.waitForElementPresent("link=Test Page 6");
  selenium.click(RuntimeVariables.replace("link=Test Page 6"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForElementPresent("link=Add Comment");
  FileUtil.mkdirs(RuntimeVariables.replace("L:\\portal\\build\\portal-web\\test-output\\brochure\\"));
  selenium.captureEntirePageScreenshot(RuntimeVariables.replace("L:\\portal\\build\\portal-web\\test-output\\brochure\\ScreengrabTest08.jpg"),"");
}
