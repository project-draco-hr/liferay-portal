{
  ObjectValuePair<Pattern,Analyzer> objectValuePair=_analyzers.get(fieldName);
  if (objectValuePair != null) {
    return objectValuePair.getValue();
  }
  for (  ObjectValuePair<Pattern,Analyzer> curObjectValuePair : _analyzers.values()) {
    Pattern pattern=curObjectValuePair.getKey();
    Matcher matcher=pattern.matcher(fieldName);
    if (matcher.matches()) {
      return curObjectValuePair.getValue();
    }
  }
  return _analyzer;
}
