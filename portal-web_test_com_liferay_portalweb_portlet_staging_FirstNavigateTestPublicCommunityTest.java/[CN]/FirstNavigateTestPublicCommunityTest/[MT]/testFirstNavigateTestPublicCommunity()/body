{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//input[@value='Search Communities']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//div[@id=\"banner\"]/div/div/ul/li[8]/ul/li[6]/ul/li[1]/a[2]")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("//div[@id=\"banner\"]/div/div/ul/li[8]/ul/li[6]/ul/li[1]/a[2]"));
  selenium.waitForPageToLoad("30000");
  selenium.click(RuntimeVariables.replace("link=New Page"));
  selenium.waitForPageToLoad("30000");
}
