{
  if (((parentGroupId == null) && (_parentGroupId != null)) || ((parentGroupId != null) && (_parentGroupId == null)) || ((parentGroupId != null) && (_parentGroupId != null) && !parentGroupId.equals(_parentGroupId))) {
    if (!XSS_ALLOW_PARENTGROUPID) {
      parentGroupId=XSSUtil.strip(parentGroupId);
    }
    _parentGroupId=parentGroupId;
  }
}
