{
  if (((creatorUserId == null) && (_creatorUserId != null)) || ((creatorUserId != null) && (_creatorUserId == null)) || ((creatorUserId != null) && (_creatorUserId != null) && !creatorUserId.equals(_creatorUserId))) {
    if (!XSS_ALLOW_CREATORUSERID) {
      creatorUserId=XSSUtil.strip(creatorUserId);
    }
    _creatorUserId=creatorUserId;
  }
}
