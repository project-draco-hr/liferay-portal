{
  if (!ClusterInvokeThreadLocal.isEnabled()) {
    return null;
  }
  Clusterable clusterable=findAnnotation(methodInvocation);
  if (clusterable == NullClusterable.NULL_CLUSTERABLE) {
    return null;
  }
  if (!clusterable.onMaster()) {
    return null;
  }
  Object result=null;
  if (ClusterMasterExecutorUtil.isMaster()) {
    result=methodInvocation.proceed();
  }
 else {
    result=ClusterableInvokerUtil.invokeOnMaster(clusterable.acceptor(),methodInvocation.getThis(),methodInvocation.getMethod(),methodInvocation.getArguments());
  }
  Method method=methodInvocation.getMethod();
  Class<?> returnType=method.getReturnType();
  if (returnType == void.class) {
    result=nullResult;
  }
  return result;
}
