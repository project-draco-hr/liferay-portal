{
  if (!ClusterInvokeThreadLocal.isEnabled()) {
    return;
  }
  Clusterable clusterable=findAnnotation(methodInvocation);
  if (clusterable == NullClusterable.NULL_CLUSTERABLE) {
    return;
  }
  ClusterableInvokerUtil.invokeOnCluster(clusterable.acceptor(),methodInvocation.getThis(),methodInvocation.getMethod(),methodInvocation.getArguments());
}
