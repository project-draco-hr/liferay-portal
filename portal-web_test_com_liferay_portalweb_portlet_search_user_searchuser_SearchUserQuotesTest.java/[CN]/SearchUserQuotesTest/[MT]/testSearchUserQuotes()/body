{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Search Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Search Test Page",RuntimeVariables.replace("Search Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//input[@name='_3_keywords']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.type("//input[@name='_3_keywords']",RuntimeVariables.replace("\"selenium\""));
  selenium.saveScreenShotAndSource();
  selenium.clickAt("//input[@type='image']",RuntimeVariables.replace("Search"));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("selen01 lenn nium01"),selenium.getText("//span[@class='asset-entry-title']/a"));
}
