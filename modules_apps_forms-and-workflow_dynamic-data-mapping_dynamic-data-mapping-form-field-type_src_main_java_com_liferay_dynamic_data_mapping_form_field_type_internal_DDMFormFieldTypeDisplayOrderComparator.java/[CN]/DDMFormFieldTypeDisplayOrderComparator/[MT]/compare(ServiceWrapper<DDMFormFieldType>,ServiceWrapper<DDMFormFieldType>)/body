{
  Integer propertyValue1=MapUtil.getInteger(serviceWrapper1.getProperties(),_DDM_FORM_FIELD_TYPE_DISPLAY_ORDER_PROPERTY);
  Integer propertyValue2=MapUtil.getInteger(serviceWrapper2.getProperties(),_DDM_FORM_FIELD_TYPE_DISPLAY_ORDER_PROPERTY);
  int value=propertyValue1.compareTo(propertyValue2);
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
