{
  List<IGFolder> folders=igFolderLocalService.getFolders(groupId,parentFolderId);
  List<IGFolder> sanitized=new ArrayList<IGFolder>(folders.size());
  for (  IGFolder folder : folders) {
    if (IGFolderPermission.contains(getPermissionChecker(),folder,ActionKeys.VIEW)) {
      sanitized.add(folder);
    }
  }
  return sanitized;
}
