{
  UploadPortletRequest uploadPortletRequest=PortalUtil.getUploadPortletRequest(actionRequest);
  long recordId=ParamUtil.getLong(uploadPortletRequest,"recordId");
  long recordSetId=ParamUtil.getLong(uploadPortletRequest,"recordSetId");
  return DDLUtil.updateRecord(uploadPortletRequest,recordId,recordSetId,false);
}
