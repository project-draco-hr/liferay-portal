{
  StagedModelRepository<T> stagedModelRepository=getStagedModelRepository();
  if (stagedModelRepository == null) {
    return null;
  }
  T stagedModel=null;
  if (stagedModel instanceof StagedGroupedModel) {
    stagedModel=fetchStagedModelByUuidAndGroupId(_uuid,_groupId);
  }
 else {
    List<T> stagedModels=fetchStagedModelsByUuidAndCompanyId(_uuid,_companyId);
    if (!stagedModels.isEmpty()) {
      stagedModel=stagedModels.get(0);
    }
  }
  if (stagedModel == null) {
    return null;
  }
  Date endDate=null;
  if (_dateRange != null) {
    endDate=_dateRange.getEndDate();
  }
  ExportImportDateUtil.updateLastPublishDate(stagedModel,_dateRange,endDate);
  stagedModelRepository.saveStagedModel(stagedModel);
  return null;
}
