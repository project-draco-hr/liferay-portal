{
  if (_ddmStructure.getParentStructureId() == 0) {
    return null;
  }
  try {
    DDMStructure parentDDMStructure=DDMStructureLocalServiceUtil.getStructure(_ddmStructure.getParentStructureId());
    return parentDDMStructure.getStructureKey();
  }
 catch (  Exception e) {
    _log.error(e);
  }
  return null;
}
