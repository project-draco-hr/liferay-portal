{
  String firstString=strings[0];
  int index=0;
  boolean match=true;
  while (match) {
    int nextIndex=firstString.indexOf(separator,index + 1);
    if (nextIndex == -1) {
      break;
    }
    for (int i=1; i < strings.length; i++) {
      if (!firstString.regionMatches(index,strings[i],index,(nextIndex - index + 1))) {
        match=false;
        break;
      }
    }
    if (match) {
      index=nextIndex;
    }
  }
  if (index == 0) {
    return null;
  }
  return firstString.substring(0,index);
}
