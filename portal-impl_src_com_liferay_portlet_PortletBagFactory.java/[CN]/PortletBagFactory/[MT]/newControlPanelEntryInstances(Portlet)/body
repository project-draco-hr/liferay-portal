{
  ServiceTrackerList<ControlPanelEntry> controlPanelEntryInstances=getServiceTrackerList(ControlPanelEntry.class,portlet);
  if (Validator.isNotNull(portlet.getControlPanelEntryClass())) {
    ControlPanelEntry controlPanelEntryInstance=(ControlPanelEntry)newInstance(ControlPanelEntry.class,portlet.getControlPanelEntryClass());
    controlPanelEntryInstances.add(controlPanelEntryInstance);
  }
  return controlPanelEntryInstances;
}
