{
  ServiceTrackerList<OpenSearch> openSearchInstances=ServiceTrackerCollections.openList(OpenSearch.class,filter,properties);
  if (Validator.isNotNull(portlet.getOpenSearchClass())) {
    OpenSearch openSearch=(OpenSearch)newInstance(OpenSearch.class,portlet.getOpenSearchClass());
    openSearchInstances.add(openSearch);
  }
  return openSearchInstances;
}
