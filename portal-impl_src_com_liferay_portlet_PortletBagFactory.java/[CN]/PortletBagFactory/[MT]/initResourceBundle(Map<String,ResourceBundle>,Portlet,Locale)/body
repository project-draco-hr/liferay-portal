{
  try {
    InputStream inputStream=getResourceBundleInputStream(portlet.getResourceBundle(),locale);
    if (inputStream != null) {
      ResourceBundle resourceBundle=new LiferayResourceBundle(inputStream,StringPool.UTF8);
      inputStream.close();
      resourceBundles.put(LocaleUtil.toLanguageId(locale),resourceBundle);
    }
  }
 catch (  Exception e) {
    _log.warn(e.getMessage());
  }
}
