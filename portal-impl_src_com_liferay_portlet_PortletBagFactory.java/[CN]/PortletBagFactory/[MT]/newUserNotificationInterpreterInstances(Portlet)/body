{
  List<UserNotificationInterpreter> userNotificationInterpreterInstances=new ArrayList<UserNotificationInterpreter>();
  for (  String userNotificationInterpreterClass : portlet.getUserNotificationInterpreterClasses()) {
    UserNotificationInterpreter userNotificationInterpreterInstance=newUserNotificationInterpreterInstance(portlet,userNotificationInterpreterClass);
    userNotificationInterpreterInstances.add(userNotificationInterpreterInstance);
  }
  return userNotificationInterpreterInstances;
}
