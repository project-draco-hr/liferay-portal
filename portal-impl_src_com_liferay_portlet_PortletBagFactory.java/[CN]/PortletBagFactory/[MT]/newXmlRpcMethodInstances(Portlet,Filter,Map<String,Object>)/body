{
  ServiceTrackerList<Method> xmlRpcMethodInstances=getServiceTrackerList(Method.class,filter,properties);
  if (Validator.isNotNull(portlet.getXmlRpcMethodClass())) {
    Method xmlRpcMethodInstance=(Method)newInstance(Method.class,portlet.getXmlRpcMethodClass());
    xmlRpcMethodInstances.add(xmlRpcMethodInstance);
  }
  return xmlRpcMethodInstances;
}
