{
  if (ddlRecordVersion.isNew()) {
    Object[] args=new Object[]{ddlRecordVersion.getRecordId(),ddlRecordVersion.getVersion()};
    FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_R_V,args,Long.valueOf(1));
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_R_V,args,ddlRecordVersion);
  }
 else {
    DDLRecordVersionModelImpl ddlRecordVersionModelImpl=(DDLRecordVersionModelImpl)ddlRecordVersion;
    if ((ddlRecordVersionModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_R_V.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{ddlRecordVersion.getRecordId(),ddlRecordVersion.getVersion()};
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_R_V,args,Long.valueOf(1));
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_R_V,args,ddlRecordVersion);
    }
  }
}
