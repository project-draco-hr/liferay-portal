{
  renderRequest.setAttribute(DLWebKeys.DOCUMENT_LIBRARY_PORTLET_TOOLBAR_CONTRIBUTOR,_dlPortletToolbarContributor);
  try {
    pingFolderRepository(renderRequest);
  }
 catch (  Exception e) {
    SessionErrors.add(renderRequest,e.getClass(),e);
    return getPath();
  }
  return super.render(renderRequest,renderResponse);
}
