{
  Map<String,String> isoLanguages=new TreeMap<String,String>(String.CASE_INSENSITIVE_ORDER);
  for (  String isoLanguageId : Locale.getISOLanguages()) {
    Locale isoLocale=_fromLanguageId(isoLanguageId);
    isoLanguages.put(isoLocale.getDisplayLanguage(locale),isoLanguageId);
  }
  return isoLanguages;
}
