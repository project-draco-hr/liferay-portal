{
  if (buildThumbnailsTask.getImagesDir() != null) {
    return;
  }
  File diffsDir=getDiffsDir(buildThumbnailsTask.getProject(),liferayThemeExtension);
  if (diffsDir != null) {
    File imagesDir=new File(liferayThemeExtension.getDiffsDir(),"images");
    buildThumbnailsTask.setImagesDir(imagesDir);
  }
}
