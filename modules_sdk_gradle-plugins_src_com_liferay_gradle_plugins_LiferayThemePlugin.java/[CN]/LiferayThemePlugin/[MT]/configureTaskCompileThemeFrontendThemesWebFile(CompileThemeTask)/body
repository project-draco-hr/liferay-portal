{
  if ((compileThemeTask.getFrontendThemesDir() != null) || (compileThemeTask.getFrontendThemesFile() != null)) {
    return;
  }
  Configuration configuration=GradleUtil.getConfiguration(compileThemeTask.getProject(),FRONTEND_THEMES_WEB_CONFIGURATION_NAME);
  compileThemeTask.setFrontendThemesFile(configuration.getSingleFile());
}
