{
  Configuration configuration=GradleUtil.addConfiguration(project,FRONTEND_CSS_WEB_CONFIGURATION_NAME);
  configuration.setDescription("Configures com.liferay.frontend.css.web for compiling themes.");
  configuration.setVisible(false);
  GradleUtil.executeIfEmpty(configuration,new Action<Configuration>(){
    @Override public void execute(    Configuration configuration){
      addDependenciesFrontendCssWeb(project);
    }
  }
);
  return configuration;
}
