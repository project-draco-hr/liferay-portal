{
  if ((compileThemeTask.getPortalWebDir() != null) || (compileThemeTask.getPortalWebFile() != null)) {
    return;
  }
  Configuration configuration=GradleUtil.getConfiguration(compileThemeTask.getProject(),PORTAL_WEB_CONFIGURATION_NAME);
  compileThemeTask.setPortalWebFile(configuration.getSingleFile());
}
