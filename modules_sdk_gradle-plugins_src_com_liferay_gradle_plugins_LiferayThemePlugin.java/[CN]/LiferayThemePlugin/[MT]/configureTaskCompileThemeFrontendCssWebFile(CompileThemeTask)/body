{
  if ((compileThemeTask.getFrontendCssWebDir() != null) || (compileThemeTask.getFrontendCssWebFile() != null)) {
    return;
  }
  Configuration configuration=GradleUtil.getConfiguration(compileThemeTask.getProject(),FRONTEND_CSS_WEB_CONFIGURATION_NAME);
  compileThemeTask.setFrontendCssWebFile(configuration.getSingleFile());
}
