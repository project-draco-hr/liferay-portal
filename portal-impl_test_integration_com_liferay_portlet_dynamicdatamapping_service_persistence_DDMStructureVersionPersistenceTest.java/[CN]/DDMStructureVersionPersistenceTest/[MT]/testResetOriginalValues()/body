{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  DDMStructureVersion newDDMStructureVersion=addDDMStructureVersion();
  _persistence.clearCache();
  DDMStructureVersionModelImpl existingDDMStructureVersionModelImpl=(DDMStructureVersionModelImpl)_persistence.findByPrimaryKey(newDDMStructureVersion.getPrimaryKey());
  Assert.assertEquals(existingDDMStructureVersionModelImpl.getStructureId(),existingDDMStructureVersionModelImpl.getOriginalStructureId());
  Assert.assertTrue(Validator.equals(existingDDMStructureVersionModelImpl.getVersion(),existingDDMStructureVersionModelImpl.getOriginalVersion()));
}
