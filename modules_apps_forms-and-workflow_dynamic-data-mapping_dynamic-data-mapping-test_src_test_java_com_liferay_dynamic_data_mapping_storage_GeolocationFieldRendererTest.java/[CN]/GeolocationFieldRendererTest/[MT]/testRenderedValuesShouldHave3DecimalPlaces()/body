{
  FieldRenderer fieldRenderer=new GeolocationFieldRenderer();
  if (JavaDetector.isJDK8()) {
    Assert.assertEquals("Latitude: 9.877, Longitude: 1.234",fieldRenderer.render(createField(),LocaleUtil.US));
  }
 else {
    Assert.assertEquals("Latitude: 9.876, Longitude: 1.234",fieldRenderer.render(createField(),LocaleUtil.US));
  }
}
