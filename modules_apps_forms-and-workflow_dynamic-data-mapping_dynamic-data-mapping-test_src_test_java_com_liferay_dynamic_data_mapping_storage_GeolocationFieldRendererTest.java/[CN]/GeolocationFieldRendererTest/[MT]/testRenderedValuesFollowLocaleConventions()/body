{
  FieldRenderer fieldRenderer=new GeolocationFieldRenderer();
  if (JavaDetector.isJDK8()) {
    Assert.assertEquals("Latitud: 9,877, Longitud: 1,234",fieldRenderer.render(createField(),LocaleUtil.SPAIN));
  }
 else {
    Assert.assertEquals("Latitud: 9,876, Longitud: 1,234",fieldRenderer.render(createField(),LocaleUtil.SPAIN));
  }
}
