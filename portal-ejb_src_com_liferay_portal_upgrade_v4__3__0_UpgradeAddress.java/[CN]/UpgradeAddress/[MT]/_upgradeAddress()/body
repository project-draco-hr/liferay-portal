{
  List addresses=AddressLocalServiceUtil.getAddresses();
  AddressLocalServiceUtil.deleteAddresses();
  for (int i=0; i < addresses.size(); i++) {
    Address address=(Address)addresses.get(i);
    AddressLocalServiceUtil.addAddress(address.getUserId(),address.getClassName(),address.getClassPK(),address.getStreet1(),address.getStreet2(),address.getStreet3(),address.getCity(),address.getZip(),address.getRegionId(),address.getCountryId(),address.getTypeId(),address.getMailing(),address.getPrimary());
  }
}
