{
  MBStatsUser mbStatsUser=fetchByPrimaryKey(mbStatsUserPK);
  if (mbStatsUser == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No MBStatsUser exists with the primary key " + mbStatsUserPK);
    }
    throw new NoSuchStatsUserException("No MBStatsUser exists with the primary key " + mbStatsUserPK);
  }
  return mbStatsUser;
}
