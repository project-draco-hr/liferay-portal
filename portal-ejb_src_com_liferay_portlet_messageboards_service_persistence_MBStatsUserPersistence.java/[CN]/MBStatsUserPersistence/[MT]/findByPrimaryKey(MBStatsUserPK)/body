{
  MBStatsUser mbStatsUser=fetchByPrimaryKey(mbStatsUserPK);
  if (mbStatsUser == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No MBStatsUser exists with the primary key " + mbStatsUserPK.toString());
    }
    throw new NoSuchStatsUserException("No MBStatsUser exists with the primary key " + mbStatsUserPK.toString());
  }
  return mbStatsUser;
}
