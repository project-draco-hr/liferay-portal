{
  Session session=null;
  try {
    session=openSession();
    MBStatsUserHBM mbStatsUserHBM=(MBStatsUserHBM)session.get(MBStatsUserHBM.class,mbStatsUserPK);
    if (mbStatsUserHBM == null) {
      _log.warn("No MBStatsUser exists with the primary key " + mbStatsUserPK.toString());
      throw new NoSuchStatsUserException("No MBStatsUser exists with the primary key " + mbStatsUserPK.toString());
    }
    return MBStatsUserHBMUtil.model(mbStatsUserHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
