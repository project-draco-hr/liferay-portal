{
  Session session=null;
  try {
    session=openSession();
    if (mbStatsUser.isNew()) {
      session.save(mbStatsUser);
    }
    session.flush();
    mbStatsUser.setNew(false);
    return mbStatsUser;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
