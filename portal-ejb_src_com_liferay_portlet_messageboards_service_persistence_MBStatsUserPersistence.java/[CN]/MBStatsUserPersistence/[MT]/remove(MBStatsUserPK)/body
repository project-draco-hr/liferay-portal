{
  Session session=null;
  try {
    session=openSession();
    MBStatsUser mbStatsUser=(MBStatsUser)session.get(MBStatsUserImpl.class,mbStatsUserPK);
    if (mbStatsUser == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No MBStatsUser exists with the primary key " + mbStatsUserPK);
      }
      throw new NoSuchStatsUserException("No MBStatsUser exists with the primary key " + mbStatsUserPK);
    }
    return remove(mbStatsUser);
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
