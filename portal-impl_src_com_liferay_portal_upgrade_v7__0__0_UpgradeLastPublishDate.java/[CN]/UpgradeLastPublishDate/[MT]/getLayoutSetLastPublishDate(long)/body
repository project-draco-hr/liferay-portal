{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select settings_ from LayoutSet where groupId = ?");
    ps.setLong(1,groupId);
    rs=ps.executeQuery();
    while (rs.next()) {
      String settings=rs.getString("settings");
      if (Validator.isNotNull(settings)) {
        int x=settings.indexOf("last-publish-date=");
        String dateString=settings.substring(x,settings.length());
        if (Validator.isNotNull(dateString)) {
          return new Date(GetterUtil.getLong(dateString));
        }
      }
    }
    return null;
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
