{
  Session session=null;
  try {
    session=openSession();
    SCProductEntry scProductEntry=(SCProductEntry)session.get(SCProductEntryImpl.class,new Long(productEntryId));
    if (scProductEntry == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + productEntryId);
      }
      throw new NoSuchProductEntryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + productEntryId);
    }
    return scProductEntryPersistence.remove(scProductEntry);
  }
 catch (  NoSuchProductEntryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
