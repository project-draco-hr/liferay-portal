{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByG_U_PrevAndNext(productEntryId,groupId,userId,orderByComparator);
  }
  SCProductEntry scProductEntry=findByPrimaryKey(productEntryId);
  Session session=null;
  try {
    session=openSession();
    SCProductEntry[] array=new SCProductEntryImpl[3];
    array[0]=filterGetByG_U_PrevAndNext(session,scProductEntry,groupId,userId,orderByComparator,true);
    array[1]=scProductEntry;
    array[2]=filterGetByG_U_PrevAndNext(session,scProductEntry,groupId,userId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
