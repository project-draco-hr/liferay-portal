{
  SCProductEntry scProductEntry=(SCProductEntry)EntityCacheUtil.getResult(SCProductEntryModelImpl.ENTITY_CACHE_ENABLED,SCProductEntryImpl.class,productEntryId,this);
  if (scProductEntry == null) {
    Session session=null;
    try {
      session=openSession();
      scProductEntry=(SCProductEntry)session.get(SCProductEntryImpl.class,new Long(productEntryId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (scProductEntry != null) {
        cacheResult(scProductEntry);
      }
      closeSession(session);
    }
  }
  return scProductEntry;
}
