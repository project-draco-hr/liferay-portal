{
  if (!className.equals(DLFileEntry.class.getName())) {
    return;
  }
  FileEntry fileEntry=PortletFileRepositoryUtil.getPortletFileEntry(classPK);
  MBMessage message=MBMessageAttachmentsUtil.getMessage(classPK);
  _mbMessageService.restoreMessageAttachmentFromTrash(message.getMessageId(),fileEntry.getTitle());
}
