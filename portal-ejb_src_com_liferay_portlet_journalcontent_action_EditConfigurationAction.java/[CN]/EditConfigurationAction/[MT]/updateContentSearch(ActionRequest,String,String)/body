{
  ThemeDisplay themeDisplay=(ThemeDisplay)req.getAttribute(WebKeys.THEME_DISPLAY);
  Layout layout=themeDisplay.getLayout();
  Portlet portlet=PortletServiceUtil.getPortletById(themeDisplay.getCompanyId(),portletResource);
  JournalContentSearchLocalServiceUtil.updateContentSearch(portletResource,layout.getLayoutId(),layout.getOwnerId(),layout.getCompanyId(),articleId);
}
