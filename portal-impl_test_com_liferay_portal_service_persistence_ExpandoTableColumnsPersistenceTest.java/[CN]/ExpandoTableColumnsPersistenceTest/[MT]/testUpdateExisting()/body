{
  ExpandoTableColumnsPK pk=new ExpandoTableColumnsPK(nextLong(),nextLong());
  ExpandoTableColumns newExpandoTableColumns=_persistence.create(pk);
  _persistence.update(newExpandoTableColumns);
  ExpandoTableColumns existingExpandoTableColumns=_persistence.findByPrimaryKey(newExpandoTableColumns.getPrimaryKey());
  assertEquals(existingExpandoTableColumns.getTableId(),newExpandoTableColumns.getTableId());
  assertEquals(existingExpandoTableColumns.getColumnId(),newExpandoTableColumns.getColumnId());
}
