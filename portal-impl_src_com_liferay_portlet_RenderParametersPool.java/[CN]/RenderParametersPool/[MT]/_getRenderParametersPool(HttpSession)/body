{
  Map<Long,Map<String,Map<String,String[]>>> renderParametersPool=(Map<Long,Map<String,Map<String,String[]>>>)ses.getAttribute(WebKeys.PORTLET_RENDER_PARAMETERS);
  if (renderParametersPool == null) {
    renderParametersPool=new ConcurrentHashMap<Long,Map<String,Map<String,String[]>>>();
    ses.setAttribute(WebKeys.PORTLET_RENDER_PARAMETERS,renderParametersPool);
  }
  return renderParametersPool;
}
