{
  Map<String,List<String>> fieldsParameterNamesMap=new HashMap<String,List<String>>();
  for (  String fieldName : fieldNames) {
    List<String> fieldParameterNames=getFieldParameterName(fieldName,fieldNamespace,fieldsTree);
    fieldsParameterNamesMap.put(fieldName,fieldParameterNames);
  }
  return fieldsParameterNamesMap;
}
