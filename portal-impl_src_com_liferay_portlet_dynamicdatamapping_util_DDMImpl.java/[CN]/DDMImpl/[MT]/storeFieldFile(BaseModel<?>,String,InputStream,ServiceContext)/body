{
  String dirName=getFileUploadPath(baseModel);
  try {
    DLStoreUtil.addDirectory(serviceContext.getCompanyId(),CompanyConstants.SYSTEM,dirName);
  }
 catch (  DuplicateDirectoryException dde) {
  }
  String fileName=dirName + StringPool.SLASH + fieldName;
  try {
    DLStoreUtil.addFile(serviceContext.getCompanyId(),CompanyConstants.SYSTEM,fileName,inputStream);
  }
 catch (  DuplicateFileException dfe) {
  }
  return fileName;
}
