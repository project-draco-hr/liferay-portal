{
  List<String> fieldNames=new ArrayList<String>();
  fieldNames.add(fieldNamespace + fieldName);
  boolean repeatable=ddmStructure.isFieldRepeatable(fieldName);
  if (repeatable && (repeatableFieldsMapJSONObject != null)) {
    JSONArray jsonArray=repeatableFieldsMapJSONObject.getJSONArray(fieldNamespace + fieldName);
    for (int i=0; i < jsonArray.length(); i++) {
      fieldNames.add(fieldNamespace + fieldName + jsonArray.getString(i));
    }
  }
  return fieldNames;
}
