{
  addDDMFormFieldLocalizedProperty(jsonObject,"label",ddmFormField.getLabel(),locale,defaultLocale,ddmFormField.getType());
  addDDMFormFieldLocalizedProperty(jsonObject,"predefinedValue",ddmFormField.getPredefinedValue(),locale,defaultLocale,ddmFormField.getType());
  addDDMFormFieldLocalizedProperty(jsonObject,"tip",ddmFormField.getTip(),locale,defaultLocale,ddmFormField.getType());
}
