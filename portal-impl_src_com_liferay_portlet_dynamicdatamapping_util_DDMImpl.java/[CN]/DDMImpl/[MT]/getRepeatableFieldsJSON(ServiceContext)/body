{
  String repeatabaleFieldsMap=GetterUtil.getString(serviceContext.getAttribute("__repeatabaleFieldsMap"));
  if (Validator.isNull(repeatabaleFieldsMap)) {
    return null;
  }
  try {
    return JSONFactoryUtil.createJSONObject(repeatabaleFieldsMap);
  }
 catch (  Exception e) {
    return null;
  }
}
