{
  DDMStructure ddmStructure=DDMStructureLocalServiceUtil.getStructure(ddmStructureId);
  DDMFormValues ddmFormValues=DDMFormValuesJSONDeserializerUtil.deserialize(ddmStructure.getFullHierarchyDDMForm(),serializedDDMFormValues);
  return DDMFormValuesToFieldsConverterUtil.convert(ddmStructure,ddmFormValues);
}
