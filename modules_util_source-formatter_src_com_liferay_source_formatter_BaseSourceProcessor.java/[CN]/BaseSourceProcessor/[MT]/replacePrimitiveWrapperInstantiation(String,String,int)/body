{
  String newLine=StringUtil.replace(line,new String[]{"new Boolean(","new Byte(","new Character(","new Double(","new Float(","new Integer(","new Long(","new Short("},new String[]{"Boolean.valueOf(","Byte.valueOf(","Character.valueOf(","Double.valueOf(","Float.valueOf(","Integer.valueOf(","Long.valueOf(","Short.valueOf("});
  if (!line.equals(newLine)) {
    processErrorMessage(fileName,"> new Primitive(: " + fileName + " "+ lineCount);
  }
  return newLine;
}
