{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=WSRP",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Consumers",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//strong/a",RuntimeVariables.replace(""));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Manage Portlets")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Manage Portlets",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Portlet']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_1_WAR_wsrpportlet_name",RuntimeVariables.replace("WSRP Blogs Portlet"));
  selenium.select("_1_WAR_wsrpportlet_portletHandle",RuntimeVariables.replace("label=Blogs"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertEquals(RuntimeVariables.replace("WSRP Blogs Portlet"),selenium.getText("//tr[@class='portlet-section-body results-row last']/td[1]"));
  assertEquals(RuntimeVariables.replace("Blogs"),selenium.getText("//tr[@class='portlet-section-body results-row last']/td[2]"));
}
