{
  Message message=new Message();
  message.setPayload(new Object());
  MessageRoutingBag messageRoutingBag=new MessageRoutingBag(message,true);
  try {
    messageRoutingBag.getMessageData();
    Assert.fail();
  }
 catch (  RuntimeException re) {
    message.remove(MessageRoutingBag.MESSAGE_ROUTING_BAG);
    Assert.assertEquals("Unable to write ordinary serializable object " + message,re.getMessage());
  }
}
