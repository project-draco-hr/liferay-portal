{
  Message message=new Message();
  MessageRoutingBag messageRoutingBag=new MessageRoutingBag(message,true);
  Message newMessage=new Message();
  messageRoutingBag.setMessage(newMessage);
  Field messageField=ReflectionUtil.getDeclaredField(MessageRoutingBag.class,"_message");
  Assert.assertSame(newMessage,messageField.get(messageRoutingBag));
  Assert.assertSame(messageRoutingBag,newMessage.get(MessageRoutingBag.MESSAGE_ROUTING_BAG));
}
