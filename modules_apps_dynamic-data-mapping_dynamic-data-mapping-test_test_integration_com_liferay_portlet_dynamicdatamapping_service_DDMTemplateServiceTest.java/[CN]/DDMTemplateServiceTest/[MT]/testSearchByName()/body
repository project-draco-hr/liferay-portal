{
  addDisplayTemplate(_classNameId,0,"Event");
  addDisplayTemplate(_classNameId,0,"Contact");
  addDisplayTemplate(_classNameId,0,"Meeting");
  List<DDMTemplate> templates=DDMTemplateLocalServiceUtil.search(TestPropsValues.getCompanyId(),new long[]{group.getGroupId()},null,null,0,"Event",null,null,null,null,true,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  Assert.assertEquals(1,templates.size());
