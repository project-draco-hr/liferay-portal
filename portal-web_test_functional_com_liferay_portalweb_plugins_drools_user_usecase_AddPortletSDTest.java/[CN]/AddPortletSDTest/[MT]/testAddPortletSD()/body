{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Sample Drools Test Page");
  selenium.click(RuntimeVariables.replace("link=Sample Drools Test Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//a[@id='_145_addApplication']","More"));
  selenium.clickAt("//a[@id='_145_addApplication']",RuntimeVariables.replace("More"));
  selenium.waitForElementPresent("//span[@data-title='Sample Drools']");
  selenium.makeVisible("//span[@data-title='Sample Drools']");
  selenium.clickAt("//span[@data-title='Sample Drools']",RuntimeVariables.replace("Add"));
  selenium.waitForVisible("//section");
  assertTrue(selenium.isVisible("//section"));
}
