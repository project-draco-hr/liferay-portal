{
  syncDevice=toUnwrappedModel(syncDevice);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(syncDevice)) {
      syncDevice=(SyncDevice)session.get(SyncDeviceImpl.class,syncDevice.getPrimaryKeyObj());
    }
    if (syncDevice != null) {
      session.delete(syncDevice);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (syncDevice != null) {
    clearCache(syncDevice);
  }
  return syncDevice;
}
