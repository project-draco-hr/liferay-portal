{
  SyncDevice syncDevice=findByPrimaryKey(syncDeviceId);
  Session session=null;
  try {
    session=openSession();
    SyncDevice[] array=new SyncDeviceImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,syncDevice,uuid,companyId,orderByComparator,true);
    array[1]=syncDevice;
    array[2]=getByUuid_C_PrevAndNext(session,syncDevice,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
