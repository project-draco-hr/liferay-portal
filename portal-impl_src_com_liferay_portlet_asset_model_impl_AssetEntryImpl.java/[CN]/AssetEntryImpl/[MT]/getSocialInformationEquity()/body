{
  if (_informationEquityBase == -1) {
    _informationEquityBase=SocialEquityAssetEntryLocalServiceUtil.getInformationEquity(getEntryId());
  }
  SocialEquity iqCounter=(SocialEquity)CounterCacheAdvice.getCounterValue("com.liferay.portlet.social.service." + "SocialEquityLogLocalService#" + "updateSocialEquityAssetEntry_IQ",getEntryId());
  if (iqCounter == null) {
    return _informationEquityBase;
  }
  return _informationEquityBase + iqCounter.getValue();
}
