{
  if (_socialInformationEquity == null) {
    try {
      SocialEquityAssetEntry equityAssetEntry=SocialEquityAssetEntryUtil.findByAssetEntryId(getEntryId());
      SocialEquityValue socialEquityValue=new SocialEquityValue(equityAssetEntry.getInformationK(),equityAssetEntry.getInformationB());
      _socialInformationEquity=new AtomicReference<Double>(socialEquityValue.getValue());
    }
 catch (    PortalException pe) {
      return 0;
    }
catch (    SystemException se) {
      return 0;
    }
  }
  return _socialInformationEquity.get();
}
