{
  if (_socialInformationEquity == null) {
    try {
      SocialEquityAssetEntry equityAssetEntry=SocialEquityAssetEntryUtil.findByAssetEntryId(getEntryId());
      _socialInformationEquity=new AtomicReference<Double>(new SocialEquity(equityAssetEntry.getInformationK(),equityAssetEntry.getInformationB()).getValue());
    }
 catch (    Exception se) {
      return 0;
    }
  }
  return _socialInformationEquity.get();
}
