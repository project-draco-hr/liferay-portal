{
  AssetRendererFactory assetRendererFactory=AssetRendererFactoryRegistryUtil.getAssetRendererFactoryByClassName(getClassName());
  try {
    return assetRendererFactory.getAssetRenderer(getClassPK());
  }
 catch (  PortalException pe) {
    _log.warn("Error obtaining asset renderer",pe);
  }
  return null;
}
