{
  if (((portalURL == null) && (_portalURL != null)) || ((portalURL != null) && (_portalURL == null)) || ((portalURL != null) && (_portalURL != null) && !portalURL.equals(_portalURL))) {
    if (!XSS_ALLOW_PORTALURL) {
      portalURL=XSSUtil.strip(portalURL);
    }
    _portalURL=portalURL;
  }
}
