{
  AppDisplay appDisplay=new AppDisplay(appTitle);
  if (appTitle.equals(AppDisplay.APP_NAME_UNCATEGORIZED)) {
    appTitle=null;
  }
  for (  Bundle bundle : bundles) {
    Dictionary<String,String> headers=bundle.getHeaders();
    String curAppTitle=headers.get(BundleConstants.LIFERAY_RELENG_APP_TITLE);
    if (Validator.isNotNull(appTitle) && !appTitle.equals(curAppTitle)) {
      continue;
    }
 else     if (curAppTitle != null) {
      continue;
    }
    appDisplay.addBundle(bundle);
  }
  return appDisplay;
}
