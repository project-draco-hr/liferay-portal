{
  AppDisplay appDisplay=null;
  if (appTitle.equals(AppDisplay.APP_TITLE_UNCATEGORIZED)) {
    appTitle=StringPool.BLANK;
  }
  for (  Bundle bundle : bundles) {
    Dictionary<String,String> headers=bundle.getHeaders();
    String curAppTitle=GetterUtil.getString(headers.get(BundleConstants.LIFERAY_RELENG_APP_TITLE));
    if (!appTitle.equals(curAppTitle)) {
      continue;
    }
    if (appDisplay == null) {
      String appDescription=GetterUtil.getString(headers.get(BundleConstants.LIFERAY_RELENG_APP_DESCRIPTION));
      Version appVersion=bundle.getVersion();
      appDisplay=new SimpleAppDisplay(appTitle,appDescription,appVersion);
    }
    appDisplay.addBundle(bundle);
  }
  List<Bundle> appDisplayBundles=appDisplay.getBundles();
  if (appDisplayBundles.isEmpty()) {
    return null;
  }
  return appDisplay;
}
