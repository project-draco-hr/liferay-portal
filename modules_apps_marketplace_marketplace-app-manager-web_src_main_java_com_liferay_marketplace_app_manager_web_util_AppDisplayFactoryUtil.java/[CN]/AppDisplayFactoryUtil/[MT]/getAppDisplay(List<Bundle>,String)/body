{
  AppDisplay appDisplay=null;
  if (appTitle.equals(AppDisplay.APP_TITLE_UNCATEGORIZED)) {
    appTitle=null;
  }
  for (  Bundle bundle : bundles) {
    Dictionary<String,String> headers=bundle.getHeaders();
    String curAppTitle=headers.get(BundleConstants.LIFERAY_RELENG_APP_TITLE);
    if (Validator.isNotNull(appTitle) && !appTitle.equals(curAppTitle)) {
      continue;
    }
 else     if (curAppTitle != null) {
      continue;
    }
    if (appDisplay == null) {
      String appDescription=headers.get(BundleConstants.LIFERAY_RELENG_APP_DESCRIPTION);
      Version appVersion=bundle.getVersion();
      appDisplay=new SimpleAppDisplay(appTitle,appDescription,appVersion);
    }
    appDisplay.addBundle(bundle);
  }
  return appDisplay;
}
