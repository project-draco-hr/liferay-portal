{
  if ((classes == null) || (classes.length == 0)) {
    return;
  }
  for (int i=0; i < classes.length; i++) {
    String className=classes[i];
    if (Validator.isNotNull(className)) {
      AuthFailure authFailure=(AuthFailure)InstancePool.get(classes[i]);
      try {
        if (authType.equals(CompanyImpl.AUTH_TYPE_EA)) {
          authFailure.onFailureByEmailAddress(companyId,login,headerMap,parameterMap);
        }
 else         if (authType.equals(CompanyImpl.AUTH_TYPE_SN)) {
          authFailure.onFailureByScreenName(companyId,login,headerMap,parameterMap);
        }
 else         if (authType.equals(CompanyImpl.AUTH_TYPE_ID)) {
          long userId=GetterUtil.getLong(login);
          authFailure.onFailureByUserId(companyId,userId,headerMap,parameterMap);
        }
      }
 catch (      AuthException ae) {
        throw ae;
      }
catch (      Exception e) {
        throw new AuthException(e);
      }
    }
  }
}
