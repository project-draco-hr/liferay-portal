{
  Object obj=null;
  if (key != null && (key.equals(WebKeys.JAVAX_PORTLET_TITLE) || key.equals(WebKeys.JAVAX_PORTLET_SHORT_TITLE) || key.equals(WebKeys.JAVAX_PORTLET_KEYWORDS))) {
    StringBuffer sb=new StringBuffer();
    sb.append(key);
    sb.append(StringPool.PERIOD);
    sb.append(_portletName);
    key=sb.toString();
  }
  try {
    obj=LanguageUtil.get(_companyId,_locale,key);
  }
 catch (  LanguageException le) {
  }
  return obj;
}
