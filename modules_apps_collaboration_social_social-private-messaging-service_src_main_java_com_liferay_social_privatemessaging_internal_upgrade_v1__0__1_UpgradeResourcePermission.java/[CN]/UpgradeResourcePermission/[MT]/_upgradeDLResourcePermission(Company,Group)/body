{
  int count=_resourcePermissionLocalService.getResourcePermissionsCount(company.getCompanyId(),_DL_RESOURCE_NAME,ResourceConstants.SCOPE_INDIVIDUAL,String.valueOf(group.getGroupId()));
  if (count > 0) {
    return;
  }
  _resourceLocalService.addResources(company.getCompanyId(),group.getGroupId(),0,_DL_RESOURCE_NAME,group.getGroupId(),false,true,true);
}
