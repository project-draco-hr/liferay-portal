{
  long deliverBy1=notificationEvent1.getDeliverBy();
  long deliverBy2=notificationEvent2.getDeliverBy();
  int value=0;
  if (deliverBy1 < deliverBy2) {
    value=-1;
  }
 else   if (deliverBy1 > deliverBy2) {
    value=1;
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
