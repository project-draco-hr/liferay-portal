{
  String portletId=(String)serviceReference.getProperty("javax.portlet.name");
  if (Validator.isNull(portletId)) {
    portletId=StringPool.STAR;
  }
  List<String> paths=StringPlus.asList(serviceReference.getProperty("path"));
  if (ListUtil.isEmpty(paths)) {
    paths=new ArrayList<>();
    paths.add(StringPool.DASH);
  }
  for (  String path : paths) {
    emitter.emit(getKey(portletId,path));
  }
}
