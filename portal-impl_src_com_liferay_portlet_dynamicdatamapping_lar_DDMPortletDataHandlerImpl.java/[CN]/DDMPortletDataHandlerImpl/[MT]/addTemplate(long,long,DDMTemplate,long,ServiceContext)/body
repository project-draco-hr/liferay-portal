{
  DDMTemplate newTemplate=null;
  try {
    newTemplate=DDMTemplateLocalServiceUtil.addTemplate(userId,groupId,template.getClassNameId(),classPK,template.getTemplateKey(),template.getNameMap(),template.getDescriptionMap(),template.getType(),template.getMode(),template.getLanguage(),template.getScript(),serviceContext);
  }
 catch (  TemplateDuplicateTemplateKeyException tdtke) {
    newTemplate=DDMTemplateLocalServiceUtil.addTemplate(userId,groupId,template.getClassNameId(),classPK,null,template.getNameMap(),template.getDescriptionMap(),template.getType(),template.getMode(),template.getLanguage(),template.getScript(),serviceContext);
    if (_log.isWarnEnabled()) {
      _log.warn("An article with the key " + template.getTemplateKey() + " already exists. The new generated key is "+ newTemplate.getTemplateKey());
    }
  }
  return newTemplate;
}
