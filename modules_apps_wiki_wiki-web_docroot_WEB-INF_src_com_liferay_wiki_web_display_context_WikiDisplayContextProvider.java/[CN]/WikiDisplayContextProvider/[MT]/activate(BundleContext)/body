{
  _bundleContext=bundleContext;
  for (  Map.Entry<ServiceReference<WikiDisplayContextFactory>,WikiDisplayContextFactory> entry : _wikiDisplayContextFactories.entrySet()) {
    if (entry.getValue() != null) {
      continue;
    }
    ServiceReference<WikiDisplayContextFactory> serviceReference=entry.getKey();
    WikiDisplayContextFactory wikiDisplayContextFactory=_bundleContext.getService(serviceReference);
    _wikiDisplayContextFactories.put(serviceReference,wikiDisplayContextFactory);
  }
}
