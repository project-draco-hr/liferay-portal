{
  LocalRepository localRepository=_repositoryFactory.createLocalRepository(repositoryId);
  DefaultCapabilityRegistry defaultCapabilityRegistry=new DefaultCapabilityRegistry(localRepository);
  _repositoryDefiner.registerCapabilities(defaultCapabilityRegistry);
  setUpCommonCapabilities(localRepository,defaultCapabilityRegistry);
  defaultCapabilityRegistry.registerCapabilityRepositoryEvents(this);
  CapabilityLocalRepository capabilityLocalRepository=new CapabilityLocalRepository(localRepository,defaultCapabilityRegistry,this);
  return capabilityLocalRepository;
}
