{
  if (!capabilityRegistry.isCapabilityProvided(ConfigurationCapability.class)) {
    capabilityRegistry.addExportedCapability(ConfigurationCapability.class,new ConfigurationCapabilityImpl(documentRepository));
  }
  if (!capabilityRegistry.isCapabilityProvided(RepositoryEventTriggerCapability.class)) {
    capabilityRegistry.addExportedCapability(RepositoryEventTriggerCapability.class,new LiferayRepositoryEventTriggerCapability(this));
  }
}
