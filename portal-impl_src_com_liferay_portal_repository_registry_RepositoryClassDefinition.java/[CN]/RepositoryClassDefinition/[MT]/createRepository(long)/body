{
  Repository repository=_repositoryFactory.createRepository(repositoryId);
  DefaultCapabilityRegistry defaultCapabilityRegistry=new DefaultCapabilityRegistry(repository);
  _repositoryDefiner.registerCapabilities(defaultCapabilityRegistry);
  DefaultRepositoryEventRegistry defaultRepositoryEventRegistry=new DefaultRepositoryEventRegistry(_rootRepositoryEventTrigger);
  setUpCommonCapabilities(repository,defaultCapabilityRegistry,defaultRepositoryEventRegistry);
  setUpCapabilityRepositoryCapabilities(repository,defaultCapabilityRegistry);
  defaultCapabilityRegistry.registerCapabilityRepositoryEvents(defaultRepositoryEventRegistry);
  CapabilityRepository capabilityRepository=new CapabilityRepository(repository,defaultCapabilityRegistry,defaultRepositoryEventRegistry);
  return capabilityRepository;
}
