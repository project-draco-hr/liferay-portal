{
  PortletURL editPublicLayoutURL=PortalUtil.getControlPanelPortletURL(_liferayPortletRequest,LayoutAdminPortletKeys.GROUP_PAGES,PortletRequest.RENDER_PHASE);
  editPublicLayoutURL.setParameter("privateLayout",String.valueOf(privateLayout));
  Group liveGroup=getLiveGroup();
  editPublicLayoutURL.setParameter("groupId",String.valueOf(liveGroup.getGroupId()));
  editPublicLayoutURL.setParameter("viewLayout",Boolean.TRUE.toString());
  return editPublicLayoutURL;
}
