{
  Map<String,Object> data=new HashMap<>();
  Layout selLayout=getSelLayout();
  data.put("selPlid",(selLayout != null) ? String.valueOf(selLayout.getPlid()) : StringPool.BLANK);
  data.put("privateLayout",String.valueOf(isPrivateLayout()));
  PortletURL genericAddLayoutURL=getCommonAddLayoutURL();
  data.put("url",StringUtil.replace(genericAddLayoutURL.toString(),new String[]{HttpUtil.encodePath("{selPlid}"),HttpUtil.encodePath("{privateLayout}")},new String[]{"{selPlid}","{privateLayout}"}));
  return data;
}
