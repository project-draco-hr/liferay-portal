{
  if (_selGroup != null) {
    return _selGroup;
  }
  _selGroup=_themeDisplay.getScopeGroup();
  if (_selGroup.isControlPanel()) {
    HttpServletRequest httpServletRequest=PortalUtil.getHttpServletRequest(_liferayPortletRequest);
    _selGroup=LatentGroupManagerUtil.getLatentGroup(httpServletRequest.getSession());
  }
  return _selGroup;
}
