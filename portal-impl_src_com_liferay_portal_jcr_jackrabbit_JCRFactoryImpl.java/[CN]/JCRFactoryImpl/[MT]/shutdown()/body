{
  if (_initialized) {
    Session session=null;
    try {
      session=createSession(null);
      JackrabbitRepository repository=(JackrabbitRepository)session.getRepository();
      repository.shutdown();
    }
 catch (    RepositoryException re) {
      _log.error("Could not shutdown Jackrabbit");
      throw re;
    }
  }
  _initialized=false;
}
