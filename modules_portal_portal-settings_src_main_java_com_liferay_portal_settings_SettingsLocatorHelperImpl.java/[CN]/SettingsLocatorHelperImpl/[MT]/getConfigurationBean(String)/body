{
  Class<?> configurationBeanClass=_configurationBeanClasses.get(configurationPid);
  if (configurationBeanClass == null) {
    return null;
  }
  ConfigurationBeanManagedService configurationBeanManagedService=_configurationBeanManagedServices.get(configurationBeanClass);
  return configurationBeanManagedService.getConfigurationBean();
}
