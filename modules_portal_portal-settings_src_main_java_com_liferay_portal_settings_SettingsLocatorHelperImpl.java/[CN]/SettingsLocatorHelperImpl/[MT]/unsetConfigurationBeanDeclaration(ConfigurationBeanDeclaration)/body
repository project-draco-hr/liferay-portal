{
  Class<?> configurationBeanClass=configurationBeanDeclaration.getConfigurationBeanClass();
  ConfigurationBeanManagedService configurationBeanManagedService=_configurationBeanManagedServices.get(configurationBeanClass);
  configurationBeanManagedService.unregister();
  _configurationBeanClasses.remove(configurationBeanManagedService.getConfigurationPid());
  _configurationBeanManagedServices.remove(configurationBeanClass);
}
