{
  try {
    com.liferay.portlet.documentlibrary.model.DLFolder returnValue=DLFolderServiceUtil.updateFolder(folderId,parentFolderId,name,description,serviceContext);
    return com.liferay.portlet.documentlibrary.model.DLFolderSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
