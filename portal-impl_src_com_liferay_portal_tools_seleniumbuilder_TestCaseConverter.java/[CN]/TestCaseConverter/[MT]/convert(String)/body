{
  Map<String,Object> context=getContext();
  context.put("testCaseName",testCaseName);
  String content=processTemplate("test_case.ftl",context);
  seleniumBuilderFileUtil.writeFile(seleniumBuilderContext.getTestCaseJavaFileName(testCaseName),content,true);
  String contentHTML=processTemplate("test_case_html.ftl",context);
  seleniumBuilderFileUtil.writeFile(seleniumBuilderContext.getTestCaseHTMLFileName(testCaseName),contentHTML,false);
}
