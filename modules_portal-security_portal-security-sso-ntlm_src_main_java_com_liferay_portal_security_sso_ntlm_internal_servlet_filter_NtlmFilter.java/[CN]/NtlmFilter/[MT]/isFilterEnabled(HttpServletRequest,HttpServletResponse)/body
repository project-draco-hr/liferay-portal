{
  try {
    long companyId=PortalInstances.getCompanyId(request);
    NtlmConfiguration ntlmConfiguration=_configurationFactory.getConfiguration(NtlmConfiguration.class,new CompanyServiceSettingsLocator(companyId,NtlmSettingsConstants.SERVICE_NAME));
    if (BrowserSnifferUtil.isIe(request) && ntlmConfiguration.enabled()) {
      return true;
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return false;
}
