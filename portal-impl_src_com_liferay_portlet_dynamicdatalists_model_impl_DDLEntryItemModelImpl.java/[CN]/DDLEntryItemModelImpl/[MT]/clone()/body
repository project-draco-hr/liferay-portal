{
  DDLEntryItemImpl ddlEntryItemImpl=new DDLEntryItemImpl();
  ddlEntryItemImpl.setUuid(getUuid());
  ddlEntryItemImpl.setEntryItemId(getEntryItemId());
  ddlEntryItemImpl.setClassNameId(getClassNameId());
  ddlEntryItemImpl.setClassPK(getClassPK());
  ddlEntryItemImpl.setEntryId(getEntryId());
  ddlEntryItemImpl.resetOriginalValues();
  return ddlEntryItemImpl;
}
