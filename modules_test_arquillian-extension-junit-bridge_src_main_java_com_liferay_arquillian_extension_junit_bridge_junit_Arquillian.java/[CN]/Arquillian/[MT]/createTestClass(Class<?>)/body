{
  return new TestClass(testClass){
    @Override public List<FrameworkMethod> getAnnotatedMethods(    Class<? extends Annotation> annotationClass){
      if ((annotationClass == AfterClass.class) || (annotationClass == BeforeClass.class)) {
        return Collections.emptyList();
      }
      List<FrameworkMethod> frameworkMethods=new ArrayList<>(super.getAnnotatedMethods(annotationClass));
      Collections.sort(frameworkMethods,FrameworkMethodComparator.INSTANCE);
      return frameworkMethods;
    }
  }
;
}
