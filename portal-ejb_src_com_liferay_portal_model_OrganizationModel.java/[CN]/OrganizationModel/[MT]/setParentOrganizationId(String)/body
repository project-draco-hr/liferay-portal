{
  if (((parentOrganizationId == null) && (_parentOrganizationId != null)) || ((parentOrganizationId != null) && (_parentOrganizationId == null)) || ((parentOrganizationId != null) && (_parentOrganizationId != null) && !parentOrganizationId.equals(_parentOrganizationId))) {
    if (!XSS_ALLOW_PARENTORGANIZATIONID) {
      parentOrganizationId=XSSUtil.strip(parentOrganizationId);
    }
    _parentOrganizationId=parentOrganizationId;
    setModified(true);
  }
}
