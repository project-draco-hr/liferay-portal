{
  ExportActionableDynamicQuery exportActionableDynamicQuery=RatingsEntryLocalServiceUtil.getExportActionableDynamicQuery(portletDataContext);
  exportActionableDynamicQuery.setStagedModelType(new StagedModelType(PortalUtil.getClassNameId(RatingsEntry.class.getName()),StagedModelType.REFERRER_CLASS_NAME_ID_ALL));
