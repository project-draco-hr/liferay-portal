{
  if (((approvedByUserName == null) && (_approvedByUserName != null)) || ((approvedByUserName != null) && (_approvedByUserName == null)) || ((approvedByUserName != null) && (_approvedByUserName != null) && !approvedByUserName.equals(_approvedByUserName))) {
    if (!XSS_ALLOW_APPROVEDBYUSERNAME) {
      approvedByUserName=XSSUtil.strip(approvedByUserName);
    }
    _approvedByUserName=approvedByUserName;
    setModified(true);
  }
}
