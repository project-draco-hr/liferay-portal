{
  if (_value == TransactionalPortalCache.NULL_HOLDER) {
    portalCache.remove(key);
  }
 else   if (_quiet) {
    if (_ttl >= 0) {
      portalCache.putQuiet(key,_value,_ttl);
    }
 else {
      portalCache.putQuiet(key,_value);
    }
  }
 else {
    if (_ttl >= 0) {
      portalCache.put(key,_value,_ttl);
    }
 else {
      portalCache.put(key,_value);
    }
  }
}
