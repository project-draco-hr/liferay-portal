{
  Map<PortalCache<? extends Serializable,?>,Map<? extends Serializable,?>> portalCacheMap=_peekPortalCacheMap();
  Map<Serializable,Object> uncommittedMap=(Map<Serializable,Object>)portalCacheMap.get(portalCache);
  if (uncommittedMap == null) {
    uncommittedMap=new HashMap<Serializable,Object>();
    portalCacheMap.put(portalCache,uncommittedMap);
  }
  uncommittedMap.put(key,value);
}
