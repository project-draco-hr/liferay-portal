{
  Map<PortalCache,Map<Serializable,Object>> portalCacheMap=_peekPortalCacheMap();
  Map<Serializable,Object> uncommittedMap=portalCacheMap.get(portalCache);
  if (uncommittedMap == null) {
    uncommittedMap=new HashMap<Serializable,Object>();
    portalCacheMap.put(portalCache,uncommittedMap);
  }
  uncommittedMap.put(key,value);
}
