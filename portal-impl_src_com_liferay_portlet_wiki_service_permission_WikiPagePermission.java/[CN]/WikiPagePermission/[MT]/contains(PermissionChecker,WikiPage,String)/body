{
  if (permissionChecker.hasOwnerPermission(page.getCompanyId(),WikiPage.class.getName(),page.getPageId(),page.getUserId(),actionId)) {
    return true;
  }
  WikiNode node=page.getNode();
  return permissionChecker.hasPermission(node.getGroupId(),WikiPage.class.getName(),page.getResourcePrimKey(),actionId);
}
