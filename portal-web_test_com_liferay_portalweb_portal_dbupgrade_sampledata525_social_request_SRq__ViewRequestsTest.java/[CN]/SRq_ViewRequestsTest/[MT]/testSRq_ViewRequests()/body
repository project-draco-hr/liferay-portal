{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/socialrequestsn1/home/");
  selenium.clickAt("link=Requests Test Page",RuntimeVariables.replace("Requests Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("socialrequestfn2 socialrequestmn2 socialrequestln2"),selenium.getText("//div/a[contains(.,'socialrequestfn2')]"));
  assertEquals(RuntimeVariables.replace("Confirm"),selenium.getText("//a[.='Confirm']"));
  assertEquals(RuntimeVariables.replace("Ignore"),selenium.getText("//a[.='Ignore']"));
}
