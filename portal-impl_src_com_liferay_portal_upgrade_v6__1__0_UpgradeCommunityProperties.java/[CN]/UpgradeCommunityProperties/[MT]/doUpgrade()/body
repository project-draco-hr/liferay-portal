{
  for (int i=0; i < _OLD_PORTLET_PREFERENCES.length; i++) {
    updatePreferences("PortletPreferences",_OLD_PORTLET_PREFERENCES[i],_NEW_PORTLET_PREFERENCES[i]);
  }
  for (int i=0; i < _OLD_PORTAL_PREFERENCES.length; i++) {
    updatePreferences("PortalPreferences",_OLD_PORTAL_PREFERENCES[i],_NEW_PORTAL_PREFERENCES[i]);
  }
}
