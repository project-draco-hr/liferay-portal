{
  StringBundler sb=new StringBundler();
  sb.append("update ");
  sb.append(tableName);
  sb.append(" set preferences = replace(CAST_TEXT(preferences), '");
  sb.append(oldValue);
  sb.append("', '");
  sb.append(newValue);
  sb.append("') where preferences like '%");
  sb.append(oldValue);
  sb.append("%'");
  runSQL(sb.toString());
}
