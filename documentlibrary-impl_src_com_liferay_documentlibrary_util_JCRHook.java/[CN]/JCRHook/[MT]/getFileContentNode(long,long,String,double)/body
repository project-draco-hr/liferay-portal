{
  Node contentNode=null;
  Session session=null;
  try {
    session=JCRFactoryUtil.createSession();
    contentNode=getFileContentNode(session,companyId,repositoryId,fileName,versionNumber);
  }
 catch (  RepositoryException re) {
    throw new SystemException(re);
  }
 finally {
    if (session != null) {
      session.logout();
    }
  }
  return contentNode;
}
