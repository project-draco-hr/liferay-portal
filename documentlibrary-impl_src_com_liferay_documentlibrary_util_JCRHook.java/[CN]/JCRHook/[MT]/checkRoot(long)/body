{
  Session session=null;
  try {
    session=JCRFactoryUtil.createSession();
    Node rootNode=getRootNode(session,companyId);
    session.save();
  }
 catch (  RepositoryException re) {
    throw new SystemException(re);
  }
 finally {
    if (session != null) {
      session.logout();
    }
  }
}
