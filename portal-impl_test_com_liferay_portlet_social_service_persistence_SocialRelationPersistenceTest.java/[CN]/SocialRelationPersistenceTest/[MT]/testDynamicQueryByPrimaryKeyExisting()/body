{
  SocialRelation newSocialRelation=addSocialRelation();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(SocialRelation.class,SocialRelation.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("relationId",newSocialRelation.getRelationId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  SocialRelation existingSocialRelation=(SocialRelation)result.get(0);
  assertEquals(existingSocialRelation,newSocialRelation);
}
