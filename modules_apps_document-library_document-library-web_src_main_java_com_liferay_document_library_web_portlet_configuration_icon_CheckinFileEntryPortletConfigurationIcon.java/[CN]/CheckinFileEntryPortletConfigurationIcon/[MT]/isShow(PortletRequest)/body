{
  try {
    ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
    FileEntryDisplayContextHelper fileEntryDisplayContextHelper=new FileEntryDisplayContextHelper(themeDisplay.getPermissionChecker(),_fileEntry);
    return fileEntryDisplayContextHelper.isCheckinActionAvailable();
  }
 catch (  Exception e) {
  }
  return false;
}
