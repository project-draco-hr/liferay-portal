{
  while (true) {
    List<FilterMapping> oldFilterMappings=_filterMappingsMap.get(filterName);
    if (oldFilterMappings == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No filter mapping is under filter name " + filterName);
      }
      return;
    }
    List<FilterMapping> newFilterMappings=new ArrayList<>();
    for (    FilterMapping oldFilterMapping : oldFilterMappings) {
      newFilterMappings.add(oldFilterMapping.replaceFilter(filter));
    }
    if (_filterMappingsMap.replace(filterName,oldFilterMappings,newFilterMappings)) {
      break;
    }
  }
}
