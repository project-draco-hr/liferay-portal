{
  Filter filter=_filters.get(filterName);
  if (filter == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No filter exists with filter name " + filterName);
    }
    return;
  }
  FilterConfig filterConfig=_filterConfigs.get(filterName);
  if (filterConfig == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No filter config exists with filter name " + filterName);
    }
    return;
  }
  _filterMappings.add(new FilterMapping(filter,filterConfig,urlPatterns,dispatchers));
}
