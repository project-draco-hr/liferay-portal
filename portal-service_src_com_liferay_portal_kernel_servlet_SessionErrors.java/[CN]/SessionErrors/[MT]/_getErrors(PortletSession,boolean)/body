{
  Map<String,Object> errors=null;
  try {
    errors=(Map<String,Object>)portletSession.getAttribute(SessionErrors.class.getName());
    if ((errors == null) && createIfAbsent) {
      errors=new LinkedHashMap<String,Object>();
      portletSession.setAttribute(SessionErrors.class.getName(),errors);
    }
  }
 catch (  IllegalStateException ise) {
  }
  return errors;
}
