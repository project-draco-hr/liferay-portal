{
  List<Class<? extends BaseModel<?>>> modelClassesName=new ArrayList<>();
  modelClassesName.addAll(HibernateModelLoaderUtil.getModelClassesName(ClassLoaderUtil.getPortalClassLoader(),".*"));
  for (  String servletContextName : ServletContextPool.keySet()) {
    ServletContext servletContext=ServletContextPool.get(servletContextName);
    ClassLoader classLoader=servletContext.getClassLoader();
    modelClassesName.addAll(HibernateModelLoaderUtil.getModelClassesName(classLoader,".*"));
  }
  return modelClassesName;
}
