{
  if ((Validator.isNull(name)) || (Validator.isNumber(name)) || (name.indexOf(StringPool.COMMA) != -1)|| (name.indexOf(StringPool.STAR) != -1)) {
    throw new TeamNameException();
  }
  Team team=teamPersistence.fetchByG_N(groupId,name);
  if (team != null) {
    if ((teamId <= 0) || (team.getTeamId() != teamId)) {
      throw new DuplicateTeamException();
    }
  }
}
