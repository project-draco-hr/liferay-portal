{
  String languageId=searchContext.getLanguageId();
  String localizedFieldName=DocumentImpl.getLocalizedName(languageId,Field.SPELL_CHECK_WORD);
  List<String> keywords=TokenizerUtil.tokenize(localizedFieldName,searchContext.getKeywords(),languageId);
  Map<String,List<String>> suggestions=spellCheckKeywords(keywords,localizedFieldName,searchContext,languageId,1);
  return CollatorUtil.collate(suggestions,keywords);
}
