{
  Locale locale=searchContext.getLocale();
  String localizedFieldName=DocumentImpl.getLocalizedName(locale,Field.SPELL_CHECK_WORD);
  List<String> keywords=TokenizerUtil.tokenize(localizedFieldName,searchContext.getKeywords(),locale);
  Map<String,List<String>> spellCheckResults=doSpellCheckKeywords(keywords,localizedFieldName,searchContext,locale,1);
  return CollatorUtil.collate(spellCheckResults,keywords);
}
