{
  if (Validator.isNotNull(_s3StoreConfiguration.accessKey()) && Validator.isNotNull(_s3StoreConfiguration.secretKey())) {
    AWSCredentials awsCredentials=new BasicAWSCredentials(_s3StoreConfiguration.accessKey(),_s3StoreConfiguration.secretKey());
    return new StaticCredentialsProvider(awsCredentials);
  }
  return new DefaultAWSCredentialsProviderChain();
}
