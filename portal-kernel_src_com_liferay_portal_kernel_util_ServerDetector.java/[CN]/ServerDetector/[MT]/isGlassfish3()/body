{
  if (_glassfish3 == null) {
    String value=StringPool.BLANK;
    if (isGlassfish()) {
      value=GetterUtil.getString(System.getProperty("product.name"));
    }
    if (value.equals("GlassFish/v3")) {
      _glassfish3=Boolean.TRUE;
    }
 else {
      _glassfish3=Boolean.FALSE;
    }
  }
  return _glassfish3.booleanValue();
}
