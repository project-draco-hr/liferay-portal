{
  for (  ModelListener<UserTrackerPath> listener : listeners) {
    listener.onBeforeRemove(userTrackerPath);
  }
  userTrackerPath=removeImpl(userTrackerPath);
  for (  ModelListener<UserTrackerPath> listener : listeners) {
    listener.onAfterRemove(userTrackerPath);
  }
  return userTrackerPath;
}
