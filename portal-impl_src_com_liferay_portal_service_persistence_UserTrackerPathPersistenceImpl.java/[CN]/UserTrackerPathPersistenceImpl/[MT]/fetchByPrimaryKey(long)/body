{
  UserTrackerPath userTrackerPath=(UserTrackerPath)EntityCacheUtil.getResult(UserTrackerPathModelImpl.ENTITY_CACHE_ENABLED,UserTrackerPathImpl.class,userTrackerPathId,this);
  if (userTrackerPath == null) {
    Session session=null;
    try {
      session=openSession();
      userTrackerPath=(UserTrackerPath)session.get(UserTrackerPathImpl.class,Long.valueOf(userTrackerPathId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (userTrackerPath != null) {
        cacheResult(userTrackerPath);
      }
      closeSession(session);
    }
  }
  return userTrackerPath;
}
