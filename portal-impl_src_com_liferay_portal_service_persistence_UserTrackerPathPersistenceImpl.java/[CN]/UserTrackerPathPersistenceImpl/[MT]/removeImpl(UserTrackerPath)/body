{
  Session session=null;
  try {
    session=openSession();
    session.delete(userTrackerPath);
    session.flush();
    return userTrackerPath;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(UserTrackerPath.class.getName());
  }
}
