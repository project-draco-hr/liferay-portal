{
  User dlc1User=UserLocalServiceUtil.getUserByScreenName(TestPropsValues.COMPANY_ID,"dlc1");
  User dlc2User=UserLocalServiceUtil.getUserByScreenName(TestPropsValues.COMPANY_ID,"dlc2");
  List<User> users=UserLocalServiceUtil.getSocialUsers(dlc1User.getUserId(),dlc2User.getUserId(),QueryUtil.ALL_POS,QueryUtil.ALL_POS,new UserScreenNameComparator(true));
  assertEquals(4,users.size());
}
