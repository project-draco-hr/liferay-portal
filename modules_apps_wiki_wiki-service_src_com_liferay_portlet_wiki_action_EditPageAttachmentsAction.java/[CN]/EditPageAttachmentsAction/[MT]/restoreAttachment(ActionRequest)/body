{
  long[] restoreEntryIds=StringUtil.split(ParamUtil.getString(actionRequest,"restoreTrashEntryIds"),0L);
  for (  long restoreEntryId : restoreEntryIds) {
    TrashEntry trashEntry=TrashEntryLocalServiceUtil.getTrashEntry(restoreEntryId);
    FileEntry fileEntry=PortletFileRepositoryUtil.getPortletFileEntry(trashEntry.getClassPK());
    WikiPage page=WikiPageAttachmentsUtil.getPage(fileEntry.getFileEntryId());
    WikiPageServiceUtil.restorePageAttachmentFromTrash(page.getNodeId(),page.getTitle(),fileEntry.getTitle());
  }
}
