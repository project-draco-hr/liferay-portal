{
  Jar jar=GradleUtil.addTask(project,JAR_JAVADOC_TASK_NAME,Jar.class);
  jar.setClassifier("javadoc");
  jar.setDescription("Assembles a zip archive containing the Javadoc files for this " + "project.");
  jar.setGroup(BasePlugin.BUILD_GROUP);
  Javadoc javadoc=(Javadoc)GradleUtil.getTask(project,JavaPlugin.JAVADOC_TASK_NAME);
  jar.dependsOn(javadoc);
  jar.from(javadoc);
  return jar;
}
