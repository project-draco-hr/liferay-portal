{
  Configuration configuration=GradleUtil.addConfiguration(project,BASELINE_CONFIGURATION_NAME);
  configuration.defaultDependencies(new Action<DependencySet>(){
    @Override public void execute(    DependencySet dependencySet){
      addDependenciesBaseline(project);
    }
  }
);
  configuration.setDescription("Configures the previous released version of this project for " + "baselining.");
  configuration.setVisible(false);
  return configuration;
}
