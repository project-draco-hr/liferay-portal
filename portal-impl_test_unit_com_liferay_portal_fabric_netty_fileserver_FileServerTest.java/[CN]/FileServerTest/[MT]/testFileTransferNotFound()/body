{
  _sourceFilePath=FileServerTestUtil.createNotExistFile(Paths.get("testFile"));
  Future<FileResponse> future=_asyncBroker.post(_sourceFilePath.toAbsolutePath());
  _clientChannel.writeAndFlush(new FileRequest(_sourceFilePath,0,false));
  FileResponse fileResponse=future.get(_TIME_OUT,TimeUnit.MINUTES);
  Assert.assertTrue(fileResponse.isFileNotFound());
}
