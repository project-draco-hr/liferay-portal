{
  Template template=getTemplate(_templatePath + "repeatable.ftl");
  String content=getTemplateContent(template,templateVariableDefinition,language);
  String[] lines=getContentLines(content);
  String tempDataContent0=dataContentArray[0];
  dataContentArray[0]=lines[0] + StringPool.NEW_LINE + StringPool.TAB+ lines[1];
  dataContentArray[1]=tempDataContent0 + dataContentArray[1] + dataContentArray[2];
  dataContentArray[2]=lines[2] + StringPool.NEW_LINE + lines[3];
  return dataContentArray;
}
