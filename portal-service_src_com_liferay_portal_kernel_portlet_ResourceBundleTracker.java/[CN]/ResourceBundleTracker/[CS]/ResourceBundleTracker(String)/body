{
  _portletId=portletId;
  Registry registry=RegistryUtil.getRegistry();
  Filter filter=registry.getFilter("(&(javax.portlet.name= " + portletId + ")(language.id=*)"+ "(objectClass="+ ResourceBundle.class.getName()+ "))");
  _serviceTracker=registry.trackServices(filter,new ResourceBundleServiceTrackerCustomizer());
  _serviceTracker.open();
}
