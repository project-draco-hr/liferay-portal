{
  if (languageId == null) {
    languageId=StringPool.BLANK;
  }
  ResourceBundle resourceBundle=_resourceBundles.get(languageId);
  if (resourceBundle != null) {
    return resourceBundle;
  }
  return ResourceBundleUtil.getBundle(_portlet.getResourceBundle(),LocaleUtil.fromLanguageId(languageId),_classLoader);
}
