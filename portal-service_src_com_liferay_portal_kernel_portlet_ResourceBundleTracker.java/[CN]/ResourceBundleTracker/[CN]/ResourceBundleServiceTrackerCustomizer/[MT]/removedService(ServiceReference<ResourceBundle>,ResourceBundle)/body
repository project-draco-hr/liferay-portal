{
  Registry registry=RegistryUtil.getRegistry();
  registry.ungetService(serviceReference);
  Object languageId=serviceReference.getProperty("language.id");
  MergingResourceBundle mergingResourceBundle=_resourceBundles.get(languageId);
  List<ResourceBundle> resourceBundles=mergingResourceBundle.getResourceBundles();
  resourceBundles.remove(resourceBundle);
}
