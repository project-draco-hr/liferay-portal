{
  if (title.equals("all_pages") || title.equals("orphan_pages") || title.equals("recent_changes")) {
    throw new PageTitleException(title + " is reserved");
  }
  if (Validator.isNotNull(wikiGroupServiceConfiguration.pageTitlesRegexp())) {
    Pattern pattern=Pattern.compile(wikiGroupServiceConfiguration.pageTitlesRegexp());
    Matcher matcher=pattern.matcher(title);
    if (!matcher.matches()) {
      throw new PageTitleException();
    }
  }
}
