{
  ThemeDisplay themeDisplay=(ThemeDisplay)_request.getAttribute(WebKeys.THEME_DISPLAY);
  String defaultAssetPublisherPortletId=AssetUtil.getDefaultAssetPublisherId(themeDisplay.getLayout());
  if (isDefaultAssetPublisher() || Validator.isNull(defaultAssetPublisherPortletId) || !PortletPermissionUtil.contains(themeDisplay.getPermissionChecker(),themeDisplay.getLayout(),defaultAssetPublisherPortletId,ActionKeys.VIEW)) {
    _request.setAttribute(WebKeys.LAYOUT_ASSET_ENTRY,assetEntry);
  }
}
