{
  if (_paginationType == null) {
    _paginationType=GetterUtil.getString(_portletPreferences.getValue("paginationType","none"));
    if (!_paginationType.equals(PAGINATION_TYPE_NONE) && !_paginationType.equals(PAGINATION_TYPE_SIMPLE) && !_paginationType.equals(PAGINATION_TYPE_REGULAR)) {
      _paginationType=PAGINATION_TYPE_NONE;
    }
  }
  return _paginationType;
}
