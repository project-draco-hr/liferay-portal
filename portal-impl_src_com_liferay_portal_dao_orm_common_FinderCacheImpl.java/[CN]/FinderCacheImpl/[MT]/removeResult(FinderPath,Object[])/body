{
  if (!PropsValues.VALUE_OBJECT_FINDER_CACHE_ENABLED || !finderPath.isFinderCacheEnabled() || !CacheRegistryUtil.isActive()) {
    return;
  }
  if (_localCacheAvailable) {
    Map<Serializable,Serializable> localCache=_localCache.get();
    Serializable localCacheKey=finderPath.encodeLocalCacheKey(_shardEnabled,args);
    localCache.remove(localCacheKey);
  }
  PortalCache<Serializable,Serializable> portalCache=_getPortalCache(finderPath.getCacheName(),true);
  Serializable cacheKey=finderPath.encodeCacheKey(_shardEnabled,args);
  portalCache.remove(cacheKey);
}
