{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  selenium.clickAt("//strong/a",RuntimeVariables.replace("Options"));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.click("//div[@class='lfr-component lfr-menu-list']/ul/li[2]/a");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("_86_selectionStyle")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.select("_86_selectionStyle",RuntimeVariables.replace("label=Manual"));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (RuntimeVariables.replace("Selection").equals(selenium.getText("//form[@id='_86_fm']/div[1]/div[1]/div[1]/div/span"))) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.saveScreenShotAndSource();
  assertEquals("Manual",selenium.getSelectedLabel("_86_selectionStyle"));
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (RuntimeVariables.replace("You have successfully updated the setup.").equals(selenium.getText("//div[@id='p_p_id_86_']/div/div[1]"))) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.saveScreenShotAndSource();
  assertEquals(RuntimeVariables.replace("You have successfully updated the setup."),selenium.getText("//div[@id='p_p_id_86_']/div/div[1]"));
  assertEquals(RuntimeVariables.replace("Selection"),selenium.getText("//form[@id='_86_fm']/div[1]/div[1]/div[1]/div/span"));
  assertEquals(RuntimeVariables.replace("Display Settings"),selenium.getText("//form[@id='_86_fm']/div[1]/div[2]/div[1]/div/span"));
}
