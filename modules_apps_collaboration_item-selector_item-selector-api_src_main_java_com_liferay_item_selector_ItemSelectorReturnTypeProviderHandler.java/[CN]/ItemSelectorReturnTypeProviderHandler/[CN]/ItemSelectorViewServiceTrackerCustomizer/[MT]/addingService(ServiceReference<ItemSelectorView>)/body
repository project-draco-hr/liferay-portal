{
  ItemSelectorView itemSelectorView=_bundleContext.getService(serviceReference);
  String itemSelectorViewKey=GetterUtil.getString(serviceReference.getProperty("item.selector.view.key"));
  if (Validator.isNotNull(itemSelectorViewKey)) {
    Class<? extends ItemSelectorView> itemSelectorViewClass=itemSelectorView.getClass();
    _itemSelectorViewKeysMap.put(itemSelectorViewClass.getName(),itemSelectorViewKey);
  }
  return itemSelectorView;
}
