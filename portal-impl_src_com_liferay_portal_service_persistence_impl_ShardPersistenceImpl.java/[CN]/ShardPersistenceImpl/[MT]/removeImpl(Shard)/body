{
  shard=toUnwrappedModel(shard);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(shard)) {
      shard=(Shard)session.get(ShardImpl.class,shard.getPrimaryKeyObj());
    }
    if (shard != null) {
      session.delete(shard);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (shard != null) {
    clearCache(shard);
  }
  return shard;
}
