{
  if (isThemePreview(request)) {
    request.setAttribute(StripFilter.SKIP_FILTER,Boolean.TRUE);
    StringServletResponse stringServerResponse=new StringServletResponse(response);
    processFilter(ThemePreviewFilter.class,request,stringServerResponse,filterChain);
    String content=getContent(request,stringServerResponse.getString());
    ServletResponseUtil.write(response,content);
  }
 else {
    processFilter(ThemePreviewFilter.class,request,response,filterChain);
  }
}
