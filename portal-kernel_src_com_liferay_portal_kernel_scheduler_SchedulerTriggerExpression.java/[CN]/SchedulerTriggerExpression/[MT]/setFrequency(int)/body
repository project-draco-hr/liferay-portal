{
  if (frequency != SchedulerTriggerExpression.DAILY_FREQUENCY && frequency != SchedulerTriggerExpression.MINUTES_FREQUENCY && frequency != SchedulerTriggerExpression.MONTHLY_FREQUENCY && frequency != SchedulerTriggerExpression.NO_FREQUENCY && frequency != SchedulerTriggerExpression.WEEKLY_FREQUENCY && frequency != SchedulerTriggerExpression.YEARLY_FREQUENCY) {
    throw new IllegalArgumentException(String.valueOf(frequency));
  }
  _frequency=frequency;
}
