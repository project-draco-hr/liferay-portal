{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  DDMStructureLink newDDMStructureLink=addDDMStructureLink();
  _persistence.clearCache();
  DDMStructureLinkModelImpl existingDDMStructureLinkModelImpl=(DDMStructureLinkModelImpl)_persistence.findByPrimaryKey(newDDMStructureLink.getPrimaryKey());
  assertEquals(existingDDMStructureLinkModelImpl.getClassNameId(),existingDDMStructureLinkModelImpl.getOriginalClassNameId());
  assertEquals(existingDDMStructureLinkModelImpl.getClassPK(),existingDDMStructureLinkModelImpl.getOriginalClassPK());
  assertEquals(existingDDMStructureLinkModelImpl.getStructureId(),existingDDMStructureLinkModelImpl.getOriginalStructureId());
}
