{
  assetEntryLocalService.deleteEntry(DLFileShortcut.class.getName(),fileShortcut.getFileShortcutId());
  resourceLocalService.deleteResource(fileShortcut.getCompanyId(),DLFileShortcut.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,fileShortcut.getFileShortcutId());
  dlFileShortcutPersistence.remove(fileShortcut);
}
