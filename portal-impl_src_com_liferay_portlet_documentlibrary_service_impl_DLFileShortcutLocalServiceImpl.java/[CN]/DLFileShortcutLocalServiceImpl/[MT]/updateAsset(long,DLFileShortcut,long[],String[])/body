{
  DLFileEntry fileEntry=DLFileEntryLocalServiceUtil.getFileEntry(shortcut.getToFolderId(),shortcut.getToName());
  String mimeType=MimeTypesUtil.getContentType(fileEntry.getName());
  assetEntryLocalService.updateEntry(userId,shortcut.getGroupId(),DLFileShortcut.class.getName(),shortcut.getFileShortcutId(),assetCategoryIds,assetTagNames,true,null,null,null,null,mimeType,fileEntry.getTitle(),fileEntry.getDescription(),null,null,0,0,null,false);
}
