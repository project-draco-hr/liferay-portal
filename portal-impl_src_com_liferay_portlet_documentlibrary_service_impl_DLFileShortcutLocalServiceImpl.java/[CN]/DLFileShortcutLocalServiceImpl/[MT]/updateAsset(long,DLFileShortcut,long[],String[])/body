{
  FileEntry fileEntry=dlAppLocalService.getFileEntry(fileShortcut.getToFileEntryId());
  assetEntryLocalService.updateEntry(userId,fileShortcut.getGroupId(),DLFileShortcut.class.getName(),fileShortcut.getFileShortcutId(),fileShortcut.getUuid(),0,assetCategoryIds,assetTagNames,false,null,null,null,null,fileEntry.getMimeType(),fileEntry.getTitle(),fileEntry.getDescription(),null,null,null,0,0,null,false);
}
