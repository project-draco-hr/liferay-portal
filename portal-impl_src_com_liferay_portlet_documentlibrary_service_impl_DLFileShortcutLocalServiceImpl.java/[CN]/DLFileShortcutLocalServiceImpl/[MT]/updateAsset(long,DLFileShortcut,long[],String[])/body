{
  FileEntry fileEntry=dlAppLocalService.getFileEntry(fileShortcut.getToFileEntryId());
  assetEntryLocalService.updateEntry(userId,fileShortcut.getGroupId(),fileShortcut.getCreateDate(),fileShortcut.getModifiedDate(),DLFileShortcutConstants.getClassName(),fileShortcut.getFileShortcutId(),fileShortcut.getUuid(),0,assetCategoryIds,assetTagNames,true,false,null,null,null,fileEntry.getMimeType(),fileEntry.getTitle(),fileEntry.getDescription(),null,null,null,0,0,null);
}
