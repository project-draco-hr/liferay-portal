{
  Registry registry=RegistryUtil.getRegistry();
  for (  String tokenIgnoreAction : tokenIgnoreActions) {
    Map<String,Object> properties=new HashMap<>();
    properties.put(PropsKeys.AUTH_TOKEN_IGNORE_ACTIONS,tokenIgnoreAction);
    properties.put("objectClass",Object.class.getName());
    ServiceRegistration<Object> serviceRegistration=registry.registerService(Object.class,new Object(),properties);
    _serviceRegistrations.put(tokenIgnoreAction,serviceRegistration);
  }
}
