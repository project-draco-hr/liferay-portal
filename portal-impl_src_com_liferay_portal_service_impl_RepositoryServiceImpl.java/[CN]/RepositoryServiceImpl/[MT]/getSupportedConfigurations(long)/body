{
  try {
    ClassName className=classNameLocalService.getClassName(classNameId);
    String repositoryImplClassName=className.getValue();
    RepositoryClassDefinition repositoryClassDefinition=_repositoryClassDefinitionCatalog.getRepositoryClassDefinition(repositoryImplClassName);
    return repositoryClassDefinition.getSupportedConfigurations();
  }
 catch (  PortalException pe) {
    throw new SystemException(pe);
  }
}
