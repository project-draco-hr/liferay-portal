{
  try {
    String repositoryImplClassName=PropsUtil.get(PropsKeys.DL_REPOSITORY_IMPL,new Filter(String.valueOf(type)));
    Class<BaseRepositoryImpl> repositoryImplClass=(Class<BaseRepositoryImpl>)Class.forName(repositoryImplClassName);
    return (String[])repositoryImplClass.getField("SUPPORTED_CONFIGURATIONS").get(null);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
