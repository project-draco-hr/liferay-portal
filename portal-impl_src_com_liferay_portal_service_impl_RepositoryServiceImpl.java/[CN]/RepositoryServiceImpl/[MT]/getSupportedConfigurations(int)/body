{
  try {
    Properties repositoryImpls=PropsUtil.getProperties(PropsKeys.DL_REPOSITORY_IMPL,true);
    String repositoryImpl=repositoryImpls.getProperty(Long.toString(type));
    Class<BaseRepositoryImpl> repositoryImplClass=(Class<BaseRepositoryImpl>)Class.forName(repositoryImpl);
    return (String[])repositoryImplClass.getField("SUPPORTED_CONFIGURATIONS").get(null);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
