{
  Group group=groupPersistence.fetchByPrimaryKey(repositoryId);
  if (group != null) {
    checkModelPermissions(folderId,fileEntryId,fileVersionId);
    return;
  }
  try {
    Repository repository=repositoryPersistence.fetchByPrimaryKey(repositoryId);
    if (repository != null) {
      DLFolderPermission.check(getPermissionChecker(),repository.getGroupId(),repository.getDlFolderId(),ActionKeys.VIEW);
      return;
    }
  }
 catch (  NoSuchRepositoryException nsre) {
    throw new RepositoryException(nsre.getMessage());
  }
}
