{
  long entryId=0;
  if (folderId != 0) {
    entryId=folderId;
  }
 else   if (fileEntryId != 0) {
    entryId=fileEntryId;
  }
 else   if (fileVersionId != 0) {
    entryId=fileVersionId;
  }
  if (entryId == 0) {
    throw new RepositoryException("Missing a valid ID for folder, fileEntry or fileVersion");
  }
  return entryId;
}
