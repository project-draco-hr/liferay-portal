{
  String portletId=PortletProviderUtil.getPortletId(DDMStructure.class.getName(),PortletProvider.Action.VIEW);
  PortletURL portletURL=liferayPortletResponse.createLiferayPortletURL(getControlPanelPlid(liferayPortletRequest),portletId,PortletRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/view_template.jsp");
  portletURL.setParameter("classNameId",String.valueOf(classNameId));
  portletURL.setParameter("classPK",String.valueOf(classPK));
  return portletURL.toString();
}
