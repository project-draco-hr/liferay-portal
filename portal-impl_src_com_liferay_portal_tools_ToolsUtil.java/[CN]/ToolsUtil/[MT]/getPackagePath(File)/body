{
  String fileName=StringUtil.replace(file.toString(),StringPool.BACK_SLASH,StringPool.SLASH);
  int x=fileName.lastIndexOf("/com/liferay/");
  int y=fileName.lastIndexOf(StringPool.SLASH);
  String packagePath=fileName.substring(x + 1,y);
  return StringUtil.replace(packagePath,StringPool.SLASH,StringPool.PERIOD);
}
