{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBDiscussion.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("discussionId",RandomTestUtil.nextLong()));
  List<MBDiscussion> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
