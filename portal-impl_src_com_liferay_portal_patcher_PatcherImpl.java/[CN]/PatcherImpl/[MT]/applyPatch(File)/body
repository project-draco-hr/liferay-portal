{
  File patchFolder=getPatchFolder();
  try {
    FileUtil.copyFile(patch,new File(patchFolder + StringPool.SLASH + patch.getName()));
    return true;
  }
 catch (  Exception e) {
    _log.error("Could not copy " + patch.getAbsolutePath() + " patch to the "+ "patch folder: "+ patchFolder);
    return false;
  }
}
