{
  File patchesFolder=getPatchesFolder();
  try {
    FileUtil.copyFile(patch,new File(patchesFolder + StringPool.SLASH + patch.getName()));
    return true;
  }
 catch (  Exception e) {
    _log.error("Couldn't copy " + patch.getAbsolutePath() + " patch to the "+ "patches folder: "+ patchesFolder);
    return false;
  }
}
