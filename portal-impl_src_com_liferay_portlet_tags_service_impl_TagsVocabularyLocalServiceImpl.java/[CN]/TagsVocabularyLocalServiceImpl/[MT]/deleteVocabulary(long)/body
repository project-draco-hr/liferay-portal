{
  TagsVocabulary vocabulary=tagsVocabularyPersistence.findByPrimaryKey(vocabularyId);
  tagsEntryLocalService.deleteVocabularyEntries(vocabularyId);
  resourceLocalService.deleteResource(vocabulary.getCompanyId(),TagsVocabulary.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,vocabulary.getVocabularyId());
  tagsVocabularyPersistence.remove(vocabularyId);
}
