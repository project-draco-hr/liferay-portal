{
  name=name.trim();
  TagsVocabulary vocabulary=tagsVocabularyPersistence.findByPrimaryKey(vocabularyId);
  if (!vocabulary.getName().equals(name) && hasVocabulary(vocabulary.getCompanyId(),name)) {
    throw new DuplicateVocabularyException(name);
  }
  vocabulary.setModifiedDate(new Date());
  vocabulary.setName(name);
  vocabulary.setFolksonomy(folksonomy);
  tagsVocabularyPersistence.update(vocabulary,false);
  return vocabulary;
}
