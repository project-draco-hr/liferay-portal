{
  _listeners=new VelocityResourceListener[listeners.length];
  for (int i=0; i < listeners.length; i++) {
    try {
      _listeners[i]=(VelocityResourceListener)Class.forName(listeners[i]).newInstance();
    }
 catch (    Exception ex) {
      _log.error(ex);
      _listeners[i]=null;
    }
  }
}
