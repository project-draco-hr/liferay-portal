{
  int value=StringUtil.toLowerCase(kbArticle1.getUserName()).compareTo(StringUtil.toLowerCase(kbArticle2.getUserName()));
  if (value == 0) {
    String title1=StringUtil.toLowerCase(kbArticle1.getTitle());
    String title2=StringUtil.toLowerCase(kbArticle2.getTitle());
    value=title1.compareTo(title2);
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
