{
  int value=StringUtil.toLowerCase(kbArticle1.getUserName()).compareTo(StringUtil.toLowerCase(kbArticle2.getUserName()));
  if (value == 0) {
    String title1=kbArticle1.getTitle();
    String title2=kbArticle2.getTitle();
    value=title1.compareToIgnoreCase(title2);
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
