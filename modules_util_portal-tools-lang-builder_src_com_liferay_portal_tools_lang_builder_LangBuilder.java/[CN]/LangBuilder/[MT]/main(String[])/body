{
  Map<String,String> arguments=ArgumentsUtil.parseArguments(args);
  System.setProperty("line.separator",StringPool.NEW_LINE);
  String langDirName=arguments.get("lang.dir");
  String langFileName=arguments.get("lang.file");
  boolean plugin=GetterUtil.getBoolean(arguments.get("lang.plugin"));
  String portalLanguagePropertiesFileName=arguments.get("lang.portal.language.properties.file");
  boolean translate=GetterUtil.getBoolean(arguments.get("lang.translate"),true);
  try {
    new LangBuilder(langDirName,langFileName,plugin,portalLanguagePropertiesFileName,translate);
  }
 catch (  Exception e) {
    ArgumentsUtil.processMainException(arguments,e);
  }
}
