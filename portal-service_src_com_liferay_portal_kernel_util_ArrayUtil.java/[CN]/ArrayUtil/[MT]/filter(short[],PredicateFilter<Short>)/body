{
  if ((array == null) || (array.length == 0)) {
    return array;
  }
  List<Short> filteredArray=new ArrayList<Short>();
  for (  short s : array) {
    if (predicateFilter.filter(s)) {
      filteredArray.add(s);
    }
  }
  return toArray(filteredArray.toArray(new Short[filteredArray.size()]));
}
