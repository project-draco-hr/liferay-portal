{
  if ((array == null) || (array.length == 0)) {
    return array;
  }
  List<Integer> filteredArray=new ArrayList<Integer>();
  for (  int i : array) {
    if (predicateFilter.filter(i)) {
      filteredArray.add(i);
    }
  }
  return toArray(filteredArray.toArray(new Integer[filteredArray.size()]));
}
