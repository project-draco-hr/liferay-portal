{
  if ((array == null) || (array.length == 0)) {
    return array;
  }
  List<Boolean> filteredArray=new ArrayList<Boolean>();
  for (  boolean b : array) {
    if (predicateFilter.filter(b)) {
      filteredArray.add(b);
    }
  }
  return toArray(filteredArray.toArray(new Boolean[filteredArray.size()]));
}
