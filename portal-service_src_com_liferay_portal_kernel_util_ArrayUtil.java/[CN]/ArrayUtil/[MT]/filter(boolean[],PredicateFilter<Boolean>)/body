{
  if ((array == null) || (array.length == 0)) {
    return array;
  }
  List<Boolean> filteredList=new ArrayList<Boolean>();
  for (  boolean b : array) {
    if (predicateFilter.filter(b)) {
      filteredList.add(b);
    }
  }
  return toArray(filteredList.toArray(new Boolean[filteredList.size()]));
}
