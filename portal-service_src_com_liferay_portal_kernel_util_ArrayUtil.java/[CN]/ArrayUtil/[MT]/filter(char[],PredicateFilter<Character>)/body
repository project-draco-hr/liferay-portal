{
  if ((array == null) || (array.length == 0)) {
    return array;
  }
  List<Character> filteredList=new ArrayList<Character>();
  for (  char c : array) {
    if (predicateFilter.filter(c)) {
      filteredList.add(c);
    }
  }
  return toArray(filteredList.toArray(new Character[filteredList.size()]));
}
