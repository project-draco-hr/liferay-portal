{
  if ((array == null) || (array.length == 0)) {
    return array;
  }
  List<Character> filteredArray=new ArrayList<Character>();
  for (  char c : array) {
    if (predicateFilter.filter(c)) {
      filteredArray.add(c);
    }
  }
  return toArray(filteredArray.toArray(new Character[filteredArray.size()]));
}
