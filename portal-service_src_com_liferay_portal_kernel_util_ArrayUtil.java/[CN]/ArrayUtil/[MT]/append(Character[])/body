{
  int length=0;
  for (  Character[] array : arrays) {
    length+=array.length;
  }
  Character[] newArray=new Character[length];
  int previousLength=0;
  for (  Character[] array : arrays) {
    System.arraycopy(array,0,newArray,previousLength,array.length);
    previousLength+=array.length;
  }
  return newArray;
}
