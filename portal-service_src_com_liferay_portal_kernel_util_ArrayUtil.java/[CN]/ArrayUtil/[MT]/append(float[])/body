{
  int length=0;
  for (  float[] array : arrays) {
    length+=array.length;
  }
  float[] newArray=new float[length];
  int previousLength=0;
  for (  float[] array : arrays) {
    System.arraycopy(array,0,newArray,previousLength,array.length);
    previousLength+=array.length;
  }
  return newArray;
}
