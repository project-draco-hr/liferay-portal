{
  if ((array == null) || (array.length == 0)) {
    return array;
  }
  List<Float> filteredArray=new ArrayList<Float>();
  for (  float f : array) {
    if (predicateFilter.filter(f)) {
      filteredArray.add(f);
    }
  }
  return toArray(filteredArray.toArray(new Float[filteredArray.size()]));
}
