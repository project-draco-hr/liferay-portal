{
  if (isEmpty(array)) {
    return array;
  }
  Set<String> set=null;
  if (comparator == null) {
    set=new TreeSet<String>();
  }
 else {
    set=new TreeSet<String>(comparator);
  }
  for (int i=0; i < array.length; i++) {
    String s=array[i];
    if (!set.contains(s)) {
      set.add(s);
    }
  }
  return set.toArray(new String[set.size()]);
}
