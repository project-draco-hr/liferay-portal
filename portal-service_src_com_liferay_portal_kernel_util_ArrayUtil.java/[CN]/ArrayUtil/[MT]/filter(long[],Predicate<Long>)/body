{
  if (input == null) {
    return null;
  }
  if (input.length == 0) {
    return input;
  }
  List<Long> survivors=new ArrayList<Long>();
  for (int i=0; i < input.length; i++) {
    if (predicate.keep(input[i])) {
      survivors.add(input[i]);
    }
  }
  return toArray(survivors.toArray(new Long[survivors.size()]));
}
