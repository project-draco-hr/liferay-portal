{
  if ((array == null) || (array.length == 0)) {
    return array;
  }
  List<Double> filteredArray=new ArrayList<Double>();
  for (  double d : array) {
    if (predicateFilter.filter(d)) {
      filteredArray.add(d);
    }
  }
  return toArray(filteredArray.toArray(new Double[filteredArray.size()]));
}
