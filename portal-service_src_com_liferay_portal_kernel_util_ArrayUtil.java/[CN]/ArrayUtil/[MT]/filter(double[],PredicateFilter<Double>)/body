{
  if ((array == null) || (array.length == 0)) {
    return array;
  }
  List<Double> filteredList=new ArrayList<Double>();
  for (  double d : array) {
    if (predicateFilter.filter(d)) {
      filteredList.add(d);
    }
  }
  return toArray(filteredList.toArray(new Double[filteredList.size()]));
}
