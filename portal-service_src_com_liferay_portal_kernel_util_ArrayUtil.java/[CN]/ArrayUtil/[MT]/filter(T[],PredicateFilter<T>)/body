{
  if (isEmpty(array)) {
    return array;
  }
  List<T> filteredList=new ArrayList<>();
  for (  T t : array) {
    if (filterPredicate.filter(t)) {
      filteredList.add(t);
    }
  }
  Object[] filteredArray=filteredList.toArray();
  return (T[])Arrays.copyOf(filteredArray,filteredArray.length,array.getClass());
}
