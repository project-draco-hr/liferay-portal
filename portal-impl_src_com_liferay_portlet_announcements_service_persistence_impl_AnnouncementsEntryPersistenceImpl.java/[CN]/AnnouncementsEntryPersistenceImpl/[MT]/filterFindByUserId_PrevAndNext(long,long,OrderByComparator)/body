{
  if (!InlineSQLHelperUtil.isEnabled()) {
    return findByUserId_PrevAndNext(entryId,userId,orderByComparator);
  }
  AnnouncementsEntry announcementsEntry=findByPrimaryKey(entryId);
  Session session=null;
  try {
    session=openSession();
    AnnouncementsEntry[] array=new AnnouncementsEntryImpl[3];
    array[0]=filterGetByUserId_PrevAndNext(session,announcementsEntry,userId,orderByComparator,true);
    array[1]=announcementsEntry;
    array[2]=filterGetByUserId_PrevAndNext(session,announcementsEntry,userId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
