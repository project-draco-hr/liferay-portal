{
  if (!InlineSQLHelperUtil.isEnabled()) {
    return findByUuid_C_PrevAndNext(entryId,uuid,companyId,orderByComparator);
  }
  AnnouncementsEntry announcementsEntry=findByPrimaryKey(entryId);
  Session session=null;
  try {
    session=openSession();
    AnnouncementsEntry[] array=new AnnouncementsEntryImpl[3];
    array[0]=filterGetByUuid_C_PrevAndNext(session,announcementsEntry,uuid,companyId,orderByComparator,true);
    array[1]=announcementsEntry;
    array[2]=filterGetByUuid_C_PrevAndNext(session,announcementsEntry,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
