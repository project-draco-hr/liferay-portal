{
  String pql=getPQL();
  if (!properties.containsKey(pql)) {
    return null;
  }
  String value=properties.getProperty(pql);
  if (!(value.startsWith("'") && value.endsWith("'")) && !(value.startsWith("\"") && value.endsWith("\"")) && value.contains(" ")) {
    value="'" + value + "'";
  }
  return getObjectValue(value);
}
