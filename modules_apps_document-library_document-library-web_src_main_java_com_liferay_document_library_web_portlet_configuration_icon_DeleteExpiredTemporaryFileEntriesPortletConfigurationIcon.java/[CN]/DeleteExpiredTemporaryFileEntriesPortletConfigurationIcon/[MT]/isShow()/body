{
  try {
    if (_folder.isMountPoint()) {
      LocalRepository localRepository=RepositoryProviderUtil.getLocalRepository(_folder.getRepositoryId());
      if (localRepository.isCapabilityProvided(TemporaryFileEntriesCapability.class)) {
        return true;
      }
    }
  }
 catch (  PortalException pe) {
  }
  return false;
}
