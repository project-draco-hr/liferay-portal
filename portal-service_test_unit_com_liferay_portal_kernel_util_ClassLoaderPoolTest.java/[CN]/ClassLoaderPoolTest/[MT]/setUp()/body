{
  Class<?> clazz=getClass();
  PortalClassLoaderUtil.setClassLoader(clazz.getClassLoader());
  Field classLoadersField=ReflectionUtil.getDeclaredField(ClassLoaderPool.class,"_classLoaders");
  _classLoaders=(Map<String,ClassLoader>)classLoadersField.get(null);
  _classLoaders.clear();
  Field contextNamesField=ReflectionUtil.getDeclaredField(ClassLoaderPool.class,"_contextNames");
  _contextNames=(Map<ClassLoader,String>)contextNamesField.get(null);
  _contextNames.clear();
}
