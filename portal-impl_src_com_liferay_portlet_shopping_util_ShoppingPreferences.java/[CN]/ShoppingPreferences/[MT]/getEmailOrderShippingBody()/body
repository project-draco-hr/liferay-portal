{
  String emailOrderShippingBody=_portletPreferences.getValue("emailOrderShippingBody",StringPool.BLANK);
  if (Validator.isNotNull(emailOrderShippingBody)) {
    return emailOrderShippingBody;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsKeys.SHOPPING_EMAIL_ORDER_SHIPPING_BODY));
  }
}
