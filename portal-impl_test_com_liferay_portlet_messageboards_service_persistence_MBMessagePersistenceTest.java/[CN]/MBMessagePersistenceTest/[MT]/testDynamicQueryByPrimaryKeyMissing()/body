{
  MBMessage newMBMessage=addMBMessage();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBMessage.class,MBMessage.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("messageId",nextLong()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
