{
  try (PreparedStatement ps=connection.prepareStatement("select max(fileVersionId) from DLFileVersion where " + "fileEntryId = " + fileEntryId);ResultSet rs=ps.executeQuery()){
    rs.next();
    return rs.getLong(1);
  }
 catch (  SQLException sqle) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to get file version for file entry " + fileEntryId,sqle);
    }
  }
  return 0;
}
