{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select folderId from DLFolder" + " where parentFolderId = " + parentFolderId + " and groupId = "+ groupId+ " and name = '"+ name+ "'");
    rs=ps.executeQuery();
    if (rs.next()) {
      long newFolderId=rs.getLong("folderId");
      updateIGFolderId(newFolderId,folderId);
      folderIds.put(folderId,newFolderId);
      return true;
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return false;
}
