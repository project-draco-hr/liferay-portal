{
  runSQL("delete from ResourcePermission where name = '" + _IG_IMAGE_CLASS_NAME + "' and primKey = '0'");
  runSQL("delete from ResourcePermission where name = '" + _IG_IMAGE_CLASS_NAME + "' and primKey = companyId");
  runSQL("update ResourcePermission set name = '" + DLFileEntry.class.getName() + "' where name = '"+ _IG_IMAGE_CLASS_NAME+ "'");
}
