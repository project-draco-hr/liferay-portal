{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select imageId from Image");
    rs=ps.executeQuery();
    while (rs.next()) {
      long imageId=rs.getLong("imageId");
      migrateImage(imageId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
