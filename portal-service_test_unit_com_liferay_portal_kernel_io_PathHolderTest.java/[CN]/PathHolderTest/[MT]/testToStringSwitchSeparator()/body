{
  PathHolder pathHolder=new PathHolder(Paths.get(""));
  char separatorChar=CharPool.SLASH;
  if (File.separatorChar == CharPool.SLASH) {
    separatorChar=CharPool.BACK_SLASH;
  }
  ReflectionTestUtil.setFieldValue(pathHolder,"_pathString","testFolder" + separatorChar + "testFile");
  ReflectionTestUtil.setFieldValue(pathHolder,"_separatorChar",separatorChar);
  Assert.assertEquals("testFolder" + separatorChar + "testFile",pathHolder.toString());
}
