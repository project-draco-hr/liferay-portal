{
  try {
    _started.await(_wabExtenderConfiguration.stopTimeout(),TimeUnit.MILLISECONDS);
  }
 catch (  InterruptedException ie) {
    _logger.log(Logger.LOG_ERROR,String.format("The wait for bundle {0}/{1} to start before " + "destroying was interrupted",_bundle.getSymbolicName(),_bundle.getBundleId()),ie);
  }
  _webBundleDeployer.doStop(_bundle);
}
