{
  StringBundler sb=new StringBundler(6);
  sb.append("{\"availableLanguageIds\": [\"en_US\"],");
  sb.append("\"defaultLanguageId\": \"en_US\", \"fieldValues\": [{");
  sb.append("\"instanceId\": \"");
  sb.append(StringUtil.randomId());
