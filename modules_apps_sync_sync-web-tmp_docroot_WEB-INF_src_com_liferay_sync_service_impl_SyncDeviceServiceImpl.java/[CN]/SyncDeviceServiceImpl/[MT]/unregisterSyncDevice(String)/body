{
  User user=getUser();
  SyncDevice syncDevice=syncDeviceLocalService.fetchSyncDeviceByUuidAndCompanyId(uuid,user.getCompanyId());
  if ((syncDevice == null) || (syncDevice.getUserId() != user.getUserId()) || (syncDevice.getStatus() == SyncDeviceConstants.STATUS_WIPED)) {
    return;
  }
  syncDeviceLocalService.deleteSyncDevice(syncDevice);
}
