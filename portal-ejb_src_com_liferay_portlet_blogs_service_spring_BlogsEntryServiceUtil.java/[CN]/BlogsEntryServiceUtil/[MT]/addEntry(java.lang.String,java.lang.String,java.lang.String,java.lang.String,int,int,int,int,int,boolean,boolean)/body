{
  try {
    BlogsEntryService blogsEntryService=BlogsEntryServiceFactory.getService();
    return blogsEntryService.addEntry(plid,categoryId,title,content,displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute,addCommunityPermissions,addGuestPermissions);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
