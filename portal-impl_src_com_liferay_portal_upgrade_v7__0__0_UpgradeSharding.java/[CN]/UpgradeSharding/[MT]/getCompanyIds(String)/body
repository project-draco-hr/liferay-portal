{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps=connection.prepareStatement("select classPK from Shard where name = '" + shardName + "'");ResultSet rs=ps.executeQuery()){
    List<Long> companyIds=new ArrayList<>();
    while (rs.next()) {
      companyIds.add(rs.getLong("classPK"));
    }
    return companyIds;
  }
 }
