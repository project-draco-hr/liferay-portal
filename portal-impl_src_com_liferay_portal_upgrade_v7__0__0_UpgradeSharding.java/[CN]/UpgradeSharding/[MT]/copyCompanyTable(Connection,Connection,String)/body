{
  copyControlTable(sourceConnection,targetConnection,CompanyTable.TABLE_NAME,CompanyTable.TABLE_COLUMNS,CompanyTable.TABLE_SQL_CREATE);
  List<Long> companyIdsList=getCompanyIds(shardName);
  String companyIds=ListUtil.toString(companyIdsList,StringPool.NULL,StringPool.COMMA);
  runSQL(targetConnection,"delete from Company where companyId not in (" + companyIds + ")");
}
