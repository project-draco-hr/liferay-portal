{
  if (!isTableEmpty(targetConnection,tableName)) {
    if (_log.isWarnEnabled()) {
      _log.warn("Control table " + tableName + " is present on shard and "+ "contains data, when it should not. Proceeding to "+ "delete it");
    }
  }
  dropTable(targetConnection,tableName);
  UpgradeTable upgradeTable=UpgradeTableFactoryUtil.getUpgradeTable(tableName,columns);
  upgradeTable.setCreateSQL(createSQL);
  upgradeTable.copyTable(sourceConnection,targetConnection);
}
