{
  PreparedStatement ps=null;
  ResultSet rs=null;
  List<String> shardNames=new ArrayList<>();
  try {
    ps=connection.prepareStatement("select name from Shard");
    rs=ps.executeQuery();
    while (rs.next()) {
      shardNames.add(rs.getString("name"));
    }
  }
  finally {
    DataAccess.cleanUp(null,ps,rs);
  }
  return shardNames;
}
