{
  try {
    FacebookConnectConfiguration facebookConnectCompanyServiceSettings=_settingsFactory.getSettings(FacebookConnectConfiguration.class,new CompanyServiceSettingsLocator(companyId,FacebookConnectConstants.SERVICE_NAME));
    return facebookConnectCompanyServiceSettings;
  }
 catch (  SettingsException se) {
    if (_log.isErrorEnabled()) {
      _log.error("Unable to get settings",se);
    }
  }
  return null;
}
