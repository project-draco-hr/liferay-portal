{
  boolean containsResourcePermission=ResourcePermissionCheckerUtil.containsResourcePermission(permissionChecker,DLPermission.RESOURCE_NAME,groupId,ActionKeys.ADD_DOCUMENT);
  if (!containsResourcePermission) {
    throw new PrincipalException.MustHavePermission(permissionChecker,DLPermission.RESOURCE_NAME,groupId,ActionKeys.ADD_DOCUMENT);
  }
}
