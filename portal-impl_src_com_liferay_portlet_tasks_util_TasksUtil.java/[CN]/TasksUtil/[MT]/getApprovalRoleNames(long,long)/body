{
  String[] approvalRoleNames=PropsUtil.getArray(PropsUtil.TASKS_APPROVAL_ROLE_NAMES);
  try {
    PortletPreferences prefs=getPreferences(companyId,groupId);
    return prefs.getValues("approval-role-names",approvalRoleNames);
  }
 catch (  Exception e) {
    if (_log.isDebugEnabled()) {
      _log.debug(e,e);
    }
  }
  return approvalRoleNames;
}
