{
  Class<?> beanClass=bean.getClass();
  for (  ObjectValuePair<BeanMatcher,InvocationHandlerFactory> objectValuePair : _beanMatcherInvocationHandlerFactories) {
    BeanMatcher beanMatcher=objectValuePair.getKey();
    if (beanMatcher.match(beanClass,beanName)) {
      InvocationHandlerFactory invocationHandlerFactory=objectValuePair.getValue();
      InvocationHandler invocationHandler=invocationHandlerFactory.createInvocationHandler(bean);
      bean=ProxyUtil.newProxyInstance(ClassLoaderUtil.getContextClassLoader(),beanClass.getInterfaces(),invocationHandler);
    }
  }
  return bean;
}
