{
  _bundleWiring=bundleWiring;
  _jspBundleWiring=jspBundleWiring;
  _bundleWirings=bundleWirings;
  _logger=logger;
  Bundle bundle=_bundleWiring.getBundle();
  BundleContext bundleContext=bundle.getBundleContext();
  try {
    _serviceTracker=ServiceTrackerFactory.open(bundleContext,"(&(jsp.compiler.resource.map=*)(objectClass=" + Map.class.getName() + "))");
  }
 catch (  InvalidSyntaxException ise) {
    throw new RuntimeException(ise);
  }
}
