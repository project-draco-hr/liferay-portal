{
  Bundle bundle=bundleWiring.getBundle();
  if ((bundleWiring == _bundleWiring) || (bundleWiring == _jspBundleWiring)) {
    return toJavaFileObjects(bundle,bundleWiring.listResources(path,"*.class",options));
  }
  if (!isExportsPackage(bundleWiring,path.replace('/','.'))) {
    return Collections.emptyList();
  }
  if (bundle.getBundleId() == 0) {
    return handleSystemBundle(bundleWiring,path);
  }
  return toJavaFileObjects(bundle,bundleWiring.listResources(path,"*.class",options));
}
