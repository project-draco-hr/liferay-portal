{
  StringBundler sb=new StringBundler(7);
  sb.append("{clusterNodeId=");
  sb.append(_clusterNodeId);
  sb.append(", inetAddress=");
  sb.append(_inetAddress);
  sb.append(", portalAddress=");
  sb.append(_portalAddress);
  sb.append(", port=");
  sb.append(_port);
  sb.append("}");
  return sb.toString();
}
