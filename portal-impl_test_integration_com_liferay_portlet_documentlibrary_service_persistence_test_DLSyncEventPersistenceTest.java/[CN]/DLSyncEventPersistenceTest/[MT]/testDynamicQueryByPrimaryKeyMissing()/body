{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLSyncEvent.class,_dynamicQueryClassLoader);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("syncEventId",RandomTestUtil.nextLong()));
  List<DLSyncEvent> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
