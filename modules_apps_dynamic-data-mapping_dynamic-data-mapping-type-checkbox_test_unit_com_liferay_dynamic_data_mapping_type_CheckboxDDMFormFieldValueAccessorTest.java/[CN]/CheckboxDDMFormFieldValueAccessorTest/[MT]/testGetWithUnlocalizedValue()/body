{
  DDMFormFieldValue ddmFormFieldValue=DDMFormValuesTestUtil.createDDMFormFieldValue("Checkbox",new UnlocalizedValue("true"));
  CheckboxDDMFormFieldValueAccessor checkboxDDMFormFieldValueAccessor=new CheckboxDDMFormFieldValueAccessor(LocaleUtil.US);
  Assert.assertEquals(Boolean.TRUE,checkboxDDMFormFieldValueAccessor.get(ddmFormFieldValue));
  ddmFormFieldValue.setValue(new UnlocalizedValue("false"));
  Assert.assertEquals(Boolean.FALSE,checkboxDDMFormFieldValueAccessor.get(ddmFormFieldValue));
}
