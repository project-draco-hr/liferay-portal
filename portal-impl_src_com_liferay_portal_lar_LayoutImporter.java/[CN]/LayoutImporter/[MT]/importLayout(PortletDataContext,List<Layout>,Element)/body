{
  String layoutPath=layoutElement.attributeValue("path");
  Layout layout=(Layout)portletDataContext.getZipEntryAsObject(layoutPath);
  StagedModelDataHandlerUtil.importStagedModel(portletDataContext,layout);
  newLayouts.addAll(portletDataContext.getNewLayouts());
  portletDataContext.getNewLayouts().clear();
}
