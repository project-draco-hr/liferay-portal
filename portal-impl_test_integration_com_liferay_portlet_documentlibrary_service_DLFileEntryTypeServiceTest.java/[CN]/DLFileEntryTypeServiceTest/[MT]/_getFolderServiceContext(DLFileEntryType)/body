{
  ServiceContext serviceContext=ServiceContextTestUtil.getServiceContext(_group.getGroupId());
  serviceContext.setAttribute("defaultFileEntryTypeId",dlFileEntryTypes[0].getPrimaryKey());
  serviceContext.setAttribute("dlFileEntryTypesSearchContainerPrimaryKeys",ArrayUtil.toString(dlFileEntryTypes,"primaryKey"));
  serviceContext.setAttribute("overrideFileEntryTypes",true);
  return serviceContext;
}
