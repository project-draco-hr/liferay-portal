{
  if (((userAgent == null) && (_userAgent != null)) || ((userAgent != null) && (_userAgent == null)) || ((userAgent != null) && (_userAgent != null) && !userAgent.equals(_userAgent))) {
    if (!XSS_ALLOW_USERAGENT) {
      userAgent=XSSUtil.strip(userAgent);
    }
    _userAgent=userAgent;
    setModified(true);
  }
}
