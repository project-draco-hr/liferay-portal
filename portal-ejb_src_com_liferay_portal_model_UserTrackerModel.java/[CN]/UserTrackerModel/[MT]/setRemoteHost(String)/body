{
  if (((remoteHost == null) && (_remoteHost != null)) || ((remoteHost != null) && (_remoteHost == null)) || ((remoteHost != null) && (_remoteHost != null) && !remoteHost.equals(_remoteHost))) {
    if (!XSS_ALLOW_REMOTEHOST) {
      remoteHost=XSSUtil.strip(remoteHost);
    }
    _remoteHost=remoteHost;
  }
}
