{
  super(jbpmContext.getSession());
  _userId=userId;
  _timeZoneId=timeZoneId;
  _jbpmContext=jbpmContext;
  _session=jbpmContext.getSession();
  if (_session != null) {
    SessionFactoryImplementor sessionFactory=(SessionFactoryImplementor)_session.getSessionFactory();
    _dialect=sessionFactory.getDialect();
  }
}
