{
  String[] languageIdsArray=StringUtil.split(languageIds,StringPool.COMMA);
  for (  String languageId : languageIdsArray) {
    if (!ArrayUtil.contains(PropsValues.LOCALES,languageId)) {
      LocaleException le=new LocaleException(LocaleException.TYPE_DISPLAY_SETTINGS);
      le.setSourceAvailableLocales(LocaleUtil.fromLanguageIds(PropsValues.LOCALES));
      le.setTargetAvailableLocales(LocaleUtil.fromLanguageIds(languageIdsArray));
      throw le;
    }
  }
}
