{
  PortletPreferences preferences=PrefsPropsUtil.getPreferences(companyId);
  try {
    for (    String key : properties.keySet()) {
      String value=properties.getProperty(key);
      preferences.setValue(key,value);
    }
    preferences.store();
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
}
