{
  List<Company> companies=companyPersistence.findAll();
  Company wsrpCompany=null;
  for (  Company company : companies) {
    if (company.getVirtualHost().equals(CompanyConstants.WSRP_VIRTUAL_HOST)) {
      wsrpCompany=company;
      break;
    }
  }
  if (wsrpCompany != null) {
    companies.remove(wsrpCompany);
  }
  return companies;
}
