{
  List<PortletPreferences> preferences=portletPreferencesLocalService.getPortletPreferencesByPlid(plid);
  for (  PortletPreferences preference : preferences) {
    portletPreferencesLocalService.addPortletPreferences(companyId,preference.getOwnerId(),preference.getOwnerType(),revisionId,preference.getPortletId(),null,preference.getPreferences());
  }
}
