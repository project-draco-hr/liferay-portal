{
  DDMExpression<Long> ddmExpression=_ddmExpressionFactory.createLongDDMExpression("var1 + var2 + var3");
  long var1=5l;
  ddmExpression.setLongVariableValue("var1",var1);
  ddmExpression.setDDMExpressionStringVariableValue("var2","var1 + 3");
  ddmExpression.setDDMExpressionStringVariableValue("var3","var2 + var1");
  long var2=var1 + 3;
  long var3=var1 + var2;
  Assert.assertEquals(var1 + var2 + var3,(long)ddmExpression.evaluate());
}
