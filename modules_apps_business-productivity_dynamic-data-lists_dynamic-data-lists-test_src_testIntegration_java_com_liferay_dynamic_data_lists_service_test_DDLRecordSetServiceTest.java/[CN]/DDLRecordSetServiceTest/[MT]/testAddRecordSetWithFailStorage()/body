{
  DDMForm ddmForm=DDMFormTestUtil.createDDMForm(_availableLocales,_defaultLocale);
  DDLRecordSet ddlRecordSet=addRecordSet(ddmForm,FailStorageAdapter.STORAGE_TYPE);
  DDMStructure ddmStructure=ddlRecordSet.getDDMStructure();
  Assert.assertEquals(ddmStructure.getStorageType(),FailStorageAdapter.STORAGE_TYPE);
}
