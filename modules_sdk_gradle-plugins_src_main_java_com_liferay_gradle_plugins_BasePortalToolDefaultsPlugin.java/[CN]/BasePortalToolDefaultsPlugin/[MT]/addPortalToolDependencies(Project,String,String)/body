{
  String portalToolVersion=_portalToolVersions.getProperty(portalToolName);
  portalToolVersion=GradleUtil.getProperty(project,portalToolName + ".version",portalToolVersion);
  if (Validator.isNotNull(portalToolVersion)) {
    GradleUtil.addDependency(project,configurationName,PORTAL_TOOL_GROUP,portalToolName,portalToolVersion);
  }
}
