{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ResourceBlock.class,ResourceBlock.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("resourceBlockId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("resourceBlockId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
