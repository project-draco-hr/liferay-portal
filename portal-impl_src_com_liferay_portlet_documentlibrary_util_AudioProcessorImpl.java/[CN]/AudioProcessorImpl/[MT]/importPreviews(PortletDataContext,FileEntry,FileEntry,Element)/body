{
  if (!portletDataContext.isPerformDirectBinaryImport()) {
    importPreviewFromLAR(portletDataContext,importedFileEntry,fileEntryElement,"bin-path-audio-preview-" + PREVIEW_TYPE,PREVIEW_TYPE);
  }
 else {
    FileVersion importedFileVersion=importedFileEntry.getFileVersion();
    String previewFilePath=getPreviewFilePath(importedFileVersion,PREVIEW_TYPE);
    FileVersion fileVersion=fileEntry.getFileVersion();
    InputStream is=doGetPreviewAsStream(fileVersion,PREVIEW_TYPE);
    addFileToStore(portletDataContext.getCompanyId(),PREVIEW_PATH,previewFilePath,is);
  }
}
