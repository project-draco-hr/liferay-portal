{
  if (_engine != null) {
    return;
  }
  _engine=new QuartzSchedulerEngineImpl();
  _engine.start();
  Destination destination=new ParallelDestination(DestinationNames.SCHEDULER);
  MessageBusUtil.addDestination(destination);
  MessageBusUtil.registerMessageListener(destination.getName(),new QuartzMessageListener());
}
