{
  if (_forwardCompatibleInvocationHandler == null) {
    _forwardCompatibleInvocationHandler=new ForwardCompatibleInvocationHandler(super.getRequest());
    ServletRequest proxyRequest=(ServletRequest)ProxyUtil.newProxyInstance(HttpServletRequest.class.getClassLoader(),new Class[]{HttpServletRequest.class},_forwardCompatibleInvocationHandler);
    super.setRequest(proxyRequest);
    return proxyRequest;
  }
 else {
    return super.getRequest();
  }
}
