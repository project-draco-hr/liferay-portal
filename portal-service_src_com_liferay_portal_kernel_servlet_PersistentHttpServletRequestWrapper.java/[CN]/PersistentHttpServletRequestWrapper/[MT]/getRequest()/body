{
  if (_servlet30SupportInvocationHandler != null) {
    return super.getRequest();
  }
  _servlet30SupportInvocationHandler=new Servlet30SupportInvocationHandler(super.getRequest());
  ServletRequest servletRequest=(ServletRequest)ProxyUtil.newProxyInstance(HttpServletRequest.class.getClassLoader(),new Class[]{HttpServletRequest.class},_servlet30SupportInvocationHandler);
  super.setRequest(servletRequest);
  return servletRequest;
}
