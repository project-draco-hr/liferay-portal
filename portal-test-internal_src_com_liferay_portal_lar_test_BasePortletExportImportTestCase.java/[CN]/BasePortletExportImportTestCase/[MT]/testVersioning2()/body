{
  if (!isVersioningEnabled()) {
    return;
  }
  StagedModel stagedModel=addStagedModel(group.getGroupId());
  Thread currentThread=Thread.currentThread();
  currentThread.sleep(4000);
  exportImportPortlet(getPortletId());
  validateVersions();
  addVersion(stagedModel);
  exportImportPortlet(getPortletId());
  validateVersions();
}
