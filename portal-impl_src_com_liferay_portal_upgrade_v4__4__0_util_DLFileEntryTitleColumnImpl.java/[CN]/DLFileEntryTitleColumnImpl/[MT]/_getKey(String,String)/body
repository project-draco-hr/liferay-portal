{
  StringMaker sm=new StringMaker();
  sm.append(_groupIdColumn.getOldValue());
  sm.append(StringPool.UNDERLINE);
  sm.append(_folderIdColumn.getOldValue());
  sm.append(StringPool.UNDERLINE);
  sm.append(title);
  if (Validator.isNotNull(extension)) {
    sm.append(StringPool.PERIOD);
    sm.append(extension);
  }
  return sm.toString();
}
