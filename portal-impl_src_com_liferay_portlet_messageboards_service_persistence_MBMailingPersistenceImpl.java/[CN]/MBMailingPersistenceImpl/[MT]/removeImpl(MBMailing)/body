{
  Session session=null;
  try {
    session=openSession();
    session.delete(mbMailing);
    session.flush();
    return mbMailing;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(MBMailing.class.getName());
  }
}
