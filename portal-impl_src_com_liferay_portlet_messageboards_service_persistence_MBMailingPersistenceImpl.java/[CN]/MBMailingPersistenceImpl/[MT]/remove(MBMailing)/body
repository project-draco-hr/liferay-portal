{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(mbMailing);
    }
  }
  mbMailing=removeImpl(mbMailing);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(mbMailing);
    }
  }
  return mbMailing;
}
