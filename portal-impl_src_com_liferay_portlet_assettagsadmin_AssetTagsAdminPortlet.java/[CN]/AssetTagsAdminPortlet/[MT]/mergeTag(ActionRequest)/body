{
  long[] mergeTagIds=StringUtil.split(ParamUtil.getString(actionRequest,"mergeTagIds"),0L);
  long targetTagId=ParamUtil.getLong(actionRequest,"targetTagId");
  boolean overrideTagsProperties=ParamUtil.getBoolean(actionRequest,"overrideTagsProperties");
  for (  long mergeTagId : mergeTagIds) {
    if (targetTagId == mergeTagId) {
      continue;
    }
    AssetTagServiceUtil.mergeTags(mergeTagId,targetTagId,overrideTagsProperties);
  }
}
