{
  try {
    return _settingsFactory.getSettings(SACPConfiguration.class,new CompanyServiceSettingsLocator(companyId,SACPConstants.SERVICE_NAME));
  }
 catch (  SettingsException se) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to get SACP configuration",se);
    }
    return null;
  }
}
