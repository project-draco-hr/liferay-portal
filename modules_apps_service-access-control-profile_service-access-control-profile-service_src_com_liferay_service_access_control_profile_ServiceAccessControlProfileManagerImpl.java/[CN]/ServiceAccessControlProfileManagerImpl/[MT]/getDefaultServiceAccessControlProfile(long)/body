{
  SACPConfiguration sacpConfiguration;
  try {
    sacpConfiguration=_settingsFactory.getSettings(SACPConfiguration.class,new CompanyServiceSettingsLocator(CompanyThreadLocal.getCompanyId(),SACPConstants.SERVICE_NAME));
  }
 catch (  SettingsException se) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to determine default service access control " + "profile",se);
    }
    return null;
  }
  try {
    return toServiceAccessControlProfile(_sacpEntryLocalService.getSACPEntry(companyId,sacpConfiguration.defaultSACPEntryName()));
  }
 catch (  PortalException pe) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to get default service access control profile",pe);
    }
    return null;
  }
}
