{
  long companyId=nextLong();
  long repositoryId=nextLong();
  String path1=randomString();
  String path2=randomString();
  DLContent dlContent1=_dlContentLocalService.addContent(companyId,repositoryId,path1 + "/" + randomString(),Store.VERSION_DEFAULT,_DATA_VERSION_1);
  DLContent dlContent2=_dlContentLocalService.addContent(companyId,repositoryId,path2 + "/" + randomString(),Store.VERSION_DEFAULT,_DATA_VERSION_1);
  List<DLContent> dlContents=_dlContentLocalService.getContentsByDirectory(companyId,repositoryId,path1);
  assertEquals(1,dlContents.size());
  assertTrue(dlContents.contains(dlContent1));
  dlContents=_dlContentLocalService.getContentsByDirectory(companyId,repositoryId,path2);
  assertEquals(1,dlContents.size());
  assertEquals(dlContent2,dlContents.get(0));
}
