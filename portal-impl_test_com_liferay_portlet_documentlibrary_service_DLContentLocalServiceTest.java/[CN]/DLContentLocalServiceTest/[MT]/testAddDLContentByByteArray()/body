{
  long companyId=nextLong();
  long repositoryId=nextLong();
  String path=randomString();
  DLContent dlContent=_dlContentLocalService.addContent(companyId,randomString(),nextLong(),repositoryId,path,Store.DEFAULT_VERSION,new byte[1024]);
  DLContent newDlContent=_dlContentLocalService.getContent(companyId,repositoryId,path);
  assertEquals(dlContent,newDlContent);
}
