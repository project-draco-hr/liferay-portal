{
  long companyId=nextLong();
  long repositoryId=nextLong();
  String path=randomString();
  DLContent dlContent1=_dlContentLocalService.addContent(companyId,repositoryId,path,Store.VERSION_DEFAULT,_DATA_VERSION_1);
  DLContent dlContent1Read=_dlContentLocalService.getContent(companyId,repositoryId,path,Store.VERSION_DEFAULT);
  assertEquals(dlContent1,dlContent1Read);
  DLContent dlContent2=_dlContentLocalService.addContent(companyId,repositoryId,path,"1.1",_DATA_VERSION_2);
  DLContent dlContent2Read=_dlContentLocalService.getContent(companyId,repositoryId,path,"1.1");
  assertEquals(dlContent2,dlContent2Read);
}
