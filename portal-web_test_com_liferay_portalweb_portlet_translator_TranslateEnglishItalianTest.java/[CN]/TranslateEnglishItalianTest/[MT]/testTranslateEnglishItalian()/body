{
  selenium.select("_26_id",RuntimeVariables.replace("label=English to Italian"));
  selenium.type("_26_text",RuntimeVariables.replace("My name is Liferay Translator, fluent in over 6 million forms of communication."));
  selenium.click(RuntimeVariables.replace("//input[@value='Translate']"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Il mio nome \u00e8 traduttore di Liferay, fluente dentro oltre 6 milione forme di comunicazione. "));
}
