{
  List<AssetVocabulary> vocabularies=new ArrayList<AssetVocabulary>();
  vocabularies.add(_companyVocabulary);
  vocabularies.add(_vocabulary);
  String unambiguousCompanyVocabularyTitle=AssetVocabularyUtil.getUnambiguousVocabularyTitle(vocabularies,_companyVocabulary,_group.getGroupId(),_LOCALE);
  Assert.assertTrue(unambiguousCompanyVocabularyTitle.contains(_companyGroup.getDescriptiveName(_LOCALE)));
  String unambiguousVocabularyTitle=AssetVocabularyUtil.getUnambiguousVocabularyTitle(vocabularies,_vocabulary,_group.getGroupId(),_LOCALE);
  Assert.assertEquals(_TITLE,unambiguousVocabularyTitle);
}
