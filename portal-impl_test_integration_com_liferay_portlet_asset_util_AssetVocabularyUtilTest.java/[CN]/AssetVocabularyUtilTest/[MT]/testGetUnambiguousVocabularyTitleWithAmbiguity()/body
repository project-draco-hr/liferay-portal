{
  List<AssetVocabulary> vocabularies=new ArrayList<AssetVocabulary>();
  vocabularies.add(_globalVocabulary);
  vocabularies.add(_vocabulary);
  String unambiguousGlobalVocabularyTitle=AssetVocabularyUtil.getUnambiguousVocabularyTitle(vocabularies,_globalVocabulary,_group.getGroupId(),_LOCALE);
  Assert.assertTrue(unambiguousGlobalVocabularyTitle.contains(_globalGroup.getDescriptiveName(_LOCALE)));
  String unambiguousVocabularyTitle=AssetVocabularyUtil.getUnambiguousVocabularyTitle(vocabularies,_vocabulary,_group.getGroupId(),_LOCALE);
  Assert.assertEquals(_TITLE,unambiguousVocabularyTitle);
}
