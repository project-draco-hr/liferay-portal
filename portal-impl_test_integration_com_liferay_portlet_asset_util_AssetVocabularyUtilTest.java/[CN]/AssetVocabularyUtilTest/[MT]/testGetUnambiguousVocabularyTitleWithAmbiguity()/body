{
  List<AssetVocabulary> vocabularies=new ArrayList<>();
  vocabularies.add(_companyVocabulary);
  vocabularies.add(_vocabulary);
  String unambiguousCompanyVocabularyTitle=_companyVocabulary.getUnambiguousTitle(vocabularies,_group.getGroupId(),_LOCALE);
  Assert.assertTrue(unambiguousCompanyVocabularyTitle.contains(_companyGroup.getDescriptiveName(_LOCALE)));
  String unambiguousVocabularyTitle=_vocabulary.getUnambiguousTitle(vocabularies,_group.getGroupId(),_LOCALE);
  Assert.assertEquals(_TITLE,unambiguousVocabularyTitle);
}
