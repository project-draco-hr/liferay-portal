{
  List<AssetVocabulary> vocabularies=new ArrayList<AssetVocabulary>();
  vocabularies.add(_globalVocabulary);
  String unambiguousGlobalVocabularyTitle=AssetVocabularyUtil.getUnambiguousVocabularyTitle(vocabularies,_globalVocabulary,_group.getGroupId(),_LOCALE);
  Assert.assertEquals(_TITLE,unambiguousGlobalVocabularyTitle);
}
