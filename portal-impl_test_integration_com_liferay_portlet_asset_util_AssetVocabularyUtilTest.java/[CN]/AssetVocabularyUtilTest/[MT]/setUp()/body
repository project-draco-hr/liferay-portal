{
  _group=GroupTestUtil.addGroup();
  long userId=TestPropsValues.getUserId();
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext(_group.getGroupId(),userId);
  Map<Locale,String> titleMap=new HashMap<Locale,String>();
  titleMap.put(_LOCALE,_TITLE);
  _vocabulary=AssetVocabularyLocalServiceUtil.addVocabulary(userId,_TITLE,titleMap,null,null,serviceContext);
  Company company=CompanyLocalServiceUtil.getCompany(_group.getCompanyId());
  _globalGroup=company.getGroup();
  serviceContext=ServiceTestUtil.getServiceContext(_globalGroup.getGroupId(),userId);
  _globalVocabulary=AssetVocabularyLocalServiceUtil.addVocabulary(userId,_TITLE,titleMap,null,null,serviceContext);
}
