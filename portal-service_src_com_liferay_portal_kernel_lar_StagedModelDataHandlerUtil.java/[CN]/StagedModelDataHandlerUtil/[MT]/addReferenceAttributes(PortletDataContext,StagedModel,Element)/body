{
  StagedModelDataHandler stagedModelDataHandler=_getStagedModelDataHandler(stagedModel);
  Map<String,String> referenceAttributes=stagedModelDataHandler.getReferenceAttributes(portletDataContext,stagedModel);
  for (  Map.Entry<String,String> referenceAttribute : referenceAttributes.entrySet()) {
    referenceElement.addAttribute(referenceAttribute.getKey(),referenceAttribute.getValue());
  }
}
