{
  Element referenceElement=null;
  if (stagedModel instanceof StagedGroupedModel) {
    StagedGroupedModel stagedGroupedModel=(StagedGroupedModel)stagedModel;
    if (portletDataContext.isCompanyStagedGroupedModel(stagedGroupedModel)) {
      referenceElement=portletDataContext.addReferenceElement(referrerStagedModel,referrerStagedModelElement,stagedModel,stagedModelClass,referenceType,true);
      return referenceElement;
    }
  }
  exportStagedModel(portletDataContext,stagedModel);
  referenceElement=portletDataContext.addReferenceElement(referrerStagedModel,referrerStagedModelElement,stagedModel,stagedModelClass,referenceType,false);
  return referenceElement;
}
