{
  Element foldersElement=elements[0];
  if (entry.getFolderId() != BookmarksFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
    StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,foldersElement,entry.getFolder());
  }
  Element entriesElement=elements[1];
  Element entryElement=entriesElement.addElement("entry");
  portletDataContext.addClassedModel(entryElement,StagedModelPathUtil.getPath(entry),entry,BookmarksPortletDataHandler.NAMESPACE);
}
