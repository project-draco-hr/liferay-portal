{
  mdrRuleGroup=toUnwrappedModel(mdrRuleGroup);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,mdrRuleGroup);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(mdrRuleGroup);
  return mdrRuleGroup;
}
