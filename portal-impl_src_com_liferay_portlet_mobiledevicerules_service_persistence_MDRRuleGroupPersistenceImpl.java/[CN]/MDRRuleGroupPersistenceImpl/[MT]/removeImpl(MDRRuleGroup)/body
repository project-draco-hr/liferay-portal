{
  mdrRuleGroup=toUnwrappedModel(mdrRuleGroup);
  Session session=null;
  try {
    session=openSession();
    if (mdrRuleGroup.isCachedModel()) {
      mdrRuleGroup=(MDRRuleGroup)session.get(MDRRuleGroupImpl.class,mdrRuleGroup.getPrimaryKeyObj());
    }
    if (mdrRuleGroup != null) {
      session.delete(mdrRuleGroup);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (mdrRuleGroup != null) {
    clearCache(mdrRuleGroup);
  }
  return mdrRuleGroup;
}
