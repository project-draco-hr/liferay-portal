{
  Session session=null;
  try {
    session=openSession();
    MDRRuleGroup mdrRuleGroup=(MDRRuleGroup)session.get(MDRRuleGroupImpl.class,Long.valueOf(ruleGroupId));
    if (mdrRuleGroup == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + ruleGroupId);
      }
      throw new NoSuchRuleGroupException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + ruleGroupId);
    }
    return mdrRuleGroupPersistence.remove(mdrRuleGroup);
  }
 catch (  NoSuchRuleGroupException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
