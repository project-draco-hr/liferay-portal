{
  AbstractApplicationContext applicationContext=new ArrayApplicationContext(PropsUtil.getArray(PropsKeys.SPRING_CONFIGS));
  applicationContext.registerShutdownHook();
  BeanLocator beanLocator=new BeanLocatorImpl(PortalClassLoaderUtil.getClassLoader(),applicationContext);
  PortalBeanLocatorUtil.setBeanLocator(beanLocator);
  initContext(applicationContext);
}
