{
  String list=request.getParameter("list");
  if (list.length() == 0) {
    _filters=null;
  }
 else {
    _filters=StringUtil.split(list,',');
  }
  _contextPath=request.getContextPath();
  _basePath=request.getServletPath();
  _baseURL=request.getRequestURL().toString();
  _types=new ArrayList<Class>();
}
