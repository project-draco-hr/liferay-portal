{
  Value value=new LocalizedValue(LocaleUtil.US);
  value.addString(LocaleUtil.BRAZIL,"true");
  value.addString(LocaleUtil.US,"false");
  DDMFormFieldValue ddmFormFieldValue=DDMFormValuesTestUtil.createDDMFormFieldValue("ButtonSwitch",value);
  ButtonSwitchDDMFormFieldValueAccessor buttonSwitchDDMFormFieldValueAccessor=new ButtonSwitchDDMFormFieldValueAccessor();
  Assert.assertEquals(Boolean.TRUE,buttonSwitchDDMFormFieldValueAccessor.getValue(ddmFormFieldValue,LocaleUtil.BRAZIL));
  Assert.assertEquals(Boolean.FALSE,buttonSwitchDDMFormFieldValueAccessor.getValue(ddmFormFieldValue,LocaleUtil.US));
}
