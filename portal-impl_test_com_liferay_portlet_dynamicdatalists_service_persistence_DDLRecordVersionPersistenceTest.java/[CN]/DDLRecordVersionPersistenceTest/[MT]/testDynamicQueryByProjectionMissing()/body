{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DDLRecordVersion.class,DDLRecordVersion.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("recordVersionId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("recordVersionId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
