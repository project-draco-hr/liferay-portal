{
  EntityCacheUtil.removeResult(DDMStructureModelImpl.ENTITY_CACHE_ENABLED,DDMStructureImpl.class,ddmStructure.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddmStructure.getUuid(),Long.valueOf(ddmStructure.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_S,new Object[]{Long.valueOf(ddmStructure.getGroupId()),ddmStructure.getStructureKey()});
}
