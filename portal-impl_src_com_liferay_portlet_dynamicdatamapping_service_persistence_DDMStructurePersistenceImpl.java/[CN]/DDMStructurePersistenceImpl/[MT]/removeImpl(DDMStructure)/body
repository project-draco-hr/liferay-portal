{
  ddmStructure=toUnwrappedModel(ddmStructure);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,ddmStructure);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(ddmStructure);
  return ddmStructure;
}
