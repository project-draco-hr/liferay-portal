{
  EntityCacheUtil.putResult(DDMStructureModelImpl.ENTITY_CACHE_ENABLED,DDMStructureImpl.class,ddmStructure.getPrimaryKey(),ddmStructure);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddmStructure.getUuid(),Long.valueOf(ddmStructure.getGroupId())},ddmStructure);
  ddmStructure.resetOriginalValues();
}
