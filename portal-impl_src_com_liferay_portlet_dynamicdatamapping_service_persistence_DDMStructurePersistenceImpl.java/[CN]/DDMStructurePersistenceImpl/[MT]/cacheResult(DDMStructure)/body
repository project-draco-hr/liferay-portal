{
  EntityCacheUtil.putResult(DDMStructureModelImpl.ENTITY_CACHE_ENABLED,DDMStructureImpl.class,ddmStructure.getPrimaryKey(),ddmStructure);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{ddmStructure.getUuid(),ddmStructure.getGroupId()},ddmStructure);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_S,new Object[]{ddmStructure.getGroupId(),ddmStructure.getStructureKey()},ddmStructure);
  ddmStructure.resetOriginalValues();
}
