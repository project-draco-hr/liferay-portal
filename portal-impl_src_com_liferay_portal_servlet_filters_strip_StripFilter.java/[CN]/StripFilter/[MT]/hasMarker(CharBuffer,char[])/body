{
  int position=oldCharBuffer.position();
  if ((position + marker.length) >= oldCharBuffer.limit()) {
    return false;
  }
  for (int i=0; i < marker.length; i++) {
    char c=marker[i];
    char oldC=oldCharBuffer.charAt(i);
    if ((c != oldC) && (Character.toUpperCase(c) != oldC)) {
      return false;
    }
  }
  return true;
}
