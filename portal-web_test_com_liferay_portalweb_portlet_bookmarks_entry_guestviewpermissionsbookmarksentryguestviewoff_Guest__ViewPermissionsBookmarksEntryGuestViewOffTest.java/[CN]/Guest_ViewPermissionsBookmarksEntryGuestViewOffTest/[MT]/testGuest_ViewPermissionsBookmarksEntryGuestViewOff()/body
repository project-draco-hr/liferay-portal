{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Bookmarks Test Page");
  selenium.clickAt("link=Bookmarks Test Page",RuntimeVariables.replace("Bookmarks Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("There are no bookmarks in this folder."),selenium.getText("//div[@class='portlet-msg-info']"));
  assertFalse(selenium.isTextPresent("Bookmarks Entry Name"));
}
