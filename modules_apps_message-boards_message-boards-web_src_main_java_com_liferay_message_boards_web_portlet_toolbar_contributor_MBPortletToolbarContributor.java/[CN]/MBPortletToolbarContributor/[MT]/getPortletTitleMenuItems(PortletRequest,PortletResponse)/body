{
  ThemeDisplay themeDisplay=(ThemeDisplay)portletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long categoryId=_getCategoryId(portletRequest);
  List<MenuItem> menuItems=new ArrayList<>();
  try {
    addPortletTitleAddCategoryMenuItem(menuItems,categoryId,themeDisplay,portletRequest);
  }
 catch (  PortalException pe) {
    _log.error("Unable to add folder menu item",pe);
  }
  try {
    addPortletTitleAddMessageMenuItem(menuItems,categoryId,themeDisplay,portletRequest);
  }
 catch (  PortalException pe) {
    _log.error("Unable to add shortcut menu item",pe);
  }
  return menuItems;
}
