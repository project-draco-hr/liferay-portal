{
  ShoppingItem item1=(ShoppingItem)obj1;
  ShoppingItem item2=(ShoppingItem)obj2;
  int value=item1.getCategoryId().compareTo(item2.getCategoryId());
  if (value == 0) {
    value=item1.getName().toLowerCase().compareTo(item2.getName().toLowerCase());
  }
  if (_asc) {
    return value;
  }
 else {
    return -value;
  }
}
