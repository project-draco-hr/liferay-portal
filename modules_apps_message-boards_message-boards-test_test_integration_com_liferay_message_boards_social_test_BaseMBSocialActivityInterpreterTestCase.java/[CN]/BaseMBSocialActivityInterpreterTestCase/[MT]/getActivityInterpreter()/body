{
  try {
    Registry registry=RegistryUtil.getRegistry();
    Collection<SocialActivityInterpreter> socialActivityInterpreters=registry.getServices(SocialActivityInterpreter.class,"(javax.portlet.name=" + MessageBoardsPortletKeys.MESSAGE_BOARDS + ")");
    for (    SocialActivityInterpreter socialActivityInterpreter : socialActivityInterpreters) {
      if (ArrayUtil.contains(socialActivityInterpreter.getClassNames(),getClassName())) {
        return socialActivityInterpreter;
      }
    }
    throw new IllegalStateException("No activity interpreter found for class " + getClassName());
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
