{
  if (file == null) {
    throw new FileSizeException();
  }
  try {
    InputStream is=new UnsyncBufferedInputStream(new FileInputStream(file));
    return dlRepositoryService.addFileEntry(groupId,folderId,title,description,changeLog,extraSettings,is,file.length(),serviceContext);
  }
 catch (  FileNotFoundException fnfe) {
    throw new FileSizeException();
  }
}
