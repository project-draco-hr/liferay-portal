{
  try {
    InputStream is=null;
    long size=0;
    if ((file != null) && file.exists()) {
      is=new FileInputStream(file);
      size=file.length();
    }
    return updateFileEntry(fileEntryId,sourceFileName,title,description,changeLog,majorVersion,is,size,serviceContext);
  }
 catch (  FileNotFoundException fnfe) {
    throw new NoSuchFileException();
  }
}
