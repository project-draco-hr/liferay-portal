{
  Repository fromRepository=getRepository(0,fileEntryId,0);
  Repository toRepository=getRepository(newFolderId,serviceContext);
  if (isSameRepository(fromRepository,toRepository)) {
    return fromRepository.moveFileEntry(fileEntryId,newFolderId,serviceContext);
  }
  return moveFileEntryBetweenDifferentRepos(fileEntryId,newFolderId,fromRepository,toRepository,serviceContext);
}
