{
  Repository repository=getRepository(0,fileEntryId,0);
  final FileEntry fileEntry=repository.updateFileEntry(fileEntryId,sourceFileName,mimeType,title,description,changeLog,majorVersion,is,size,serviceContext);
  repository.checkInFileEntry(fileEntryId,majorVersion,changeLog,serviceContext);
  TransactionCommitCallbackUtil.registerCallback(new Callable<Void>(){
    public Void call() throws Exception {
      DLProcessorRegistryUtil.trigger(fileEntry);
      return null;
    }
  }
);
  return fileEntry;
}
