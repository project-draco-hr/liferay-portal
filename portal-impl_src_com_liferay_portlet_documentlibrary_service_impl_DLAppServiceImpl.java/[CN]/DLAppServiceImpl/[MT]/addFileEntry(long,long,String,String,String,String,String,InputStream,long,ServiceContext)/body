{
  if (is == null) {
    is=new UnsyncByteArrayInputStream(new byte[0]);
    size=0;
  }
  Repository repository=getRepository(repositoryId);
  final FileEntry fileEntry=repository.addFileEntry(folderId,sourceFileName,mimeType,title,description,changeLog,is,size,serviceContext);
  registerDLProcessorCallback(fileEntry);
  return fileEntry;
}
