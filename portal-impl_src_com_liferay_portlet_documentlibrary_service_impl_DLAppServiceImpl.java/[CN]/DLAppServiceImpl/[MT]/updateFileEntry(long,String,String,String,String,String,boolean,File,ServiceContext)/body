{
  if (file == null || !file.exists() || (file.length() == 0)) {
    return updateFileEntry(fileEntryId,sourceFileName,mimeType,title,description,changeLog,majorVersion,null,0,serviceContext);
  }
  Repository repository=getRepository(0,fileEntryId,0);
  FileEntry fileEntry=repository.updateFileEntry(fileEntryId,sourceFileName,mimeType,title,description,changeLog,majorVersion,file,serviceContext);
  DLProcessorRegistryUtil.cleanUp(fileEntry.getLatestFileVersion());
  dlAppHelperLocalService.updateFileEntry(getUserId(),fileEntry,fileEntry.getFileVersion(),serviceContext);
  return fileEntry;
}
