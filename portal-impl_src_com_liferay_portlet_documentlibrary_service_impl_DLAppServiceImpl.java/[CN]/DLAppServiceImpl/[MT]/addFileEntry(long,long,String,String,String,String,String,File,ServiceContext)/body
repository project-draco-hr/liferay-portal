{
  if (file == null || !file.exists() || (file.length() == 0)) {
    return addFileEntry(repositoryId,folderId,sourceFileName,mimeType,title,description,changeLog,null,0,serviceContext);
  }
  Repository repository=getRepository(repositoryId);
  FileEntry fileEntry=repository.addFileEntry(folderId,sourceFileName,mimeType,title,description,changeLog,file,serviceContext);
  dlAppHelperLocalService.addFileEntry(getUserId(),fileEntry,fileEntry.getFileVersion(),serviceContext);
  return fileEntry;
}
