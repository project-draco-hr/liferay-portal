{
  Repository repository=repositoryProvider.getFileEntryRepository(fileEntryId);
  FileEntry fileEntry=repository.checkOutFileEntry(fileEntryId,owner,expirationTime,serviceContext);
  FileVersion fileVersion=fileEntry.getLatestFileVersion();
  dlAppHelperLocalService.updateFileEntry(getUserId(),fileEntry,null,fileVersion,fileEntryId);
  return fileEntry;
}
