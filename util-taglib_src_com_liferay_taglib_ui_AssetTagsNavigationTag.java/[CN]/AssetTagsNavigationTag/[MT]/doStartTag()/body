{
  HttpServletRequest request=(HttpServletRequest)pageContext.getRequest();
  request.setAttribute("liferay-ui:asset-tags-navigation:classNameId",String.valueOf(_classNameId));
  request.setAttribute("liferay-ui:asset-tags-navigation:displayStyle",_displayStyle);
  request.setAttribute("liferay-ui:asset-tags-navigation:showAssetCount",_showAssetCount);
  request.setAttribute("liferay-ui:asset-tags-navigation:showZeroAssetCount",_showZeroAssetCount);
  return EVAL_BODY_BUFFERED;
}
