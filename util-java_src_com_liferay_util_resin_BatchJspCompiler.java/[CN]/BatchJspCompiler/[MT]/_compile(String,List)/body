{
  if (files.size() == 0) {
    return;
  }
  System.out.println(sourcePath);
  for (int i=0; i < files.size(); i++) {
    File file=(File)files.get(i);
    String fileName=file.toString();
    String[] args=new String[]{"-app-dir",_appDir,"-class-dir",_classDir,fileName};
    MethodWrapper methodWrapper=new MethodWrapper("com.caucho.jsp.JspCompiler","main",new Object[]{args});
    try {
      MethodInvoker.invoke(methodWrapper);
    }
 catch (    Exception e) {
      FileUtil.write(fileName + ".jspc_error",StackTraceUtil.getStackTrace(e));
    }
  }
}
