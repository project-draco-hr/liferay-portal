{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    QueryDefinition<MBThread> queryDefinition=new QueryDefinition<MBThread>(status,start,end,null);
    return mbThreadFinder.findByG_U_LPD(groupId,userId,modifiedDate,queryDefinition);
  }
  long[] categoryIds=mbCategoryService.getCategoryIds(groupId,MBCategoryConstants.DEFAULT_PARENT_CATEGORY_ID);
  if (categoryIds.length == 0) {
    return Collections.emptyList();
  }
  List<Long> threadIds=mbMessageFinder.filterFindByG_U_MD_C_S(groupId,userId,modifiedDate,categoryIds,status,start,end);
  List<MBThread> threads=new ArrayList<>(threadIds.size());
  for (  long threadId : threadIds) {
    MBThread thread=mbThreadPersistence.findByPrimaryKey(threadId);
    threads.add(thread);
  }
  return threads;
}
