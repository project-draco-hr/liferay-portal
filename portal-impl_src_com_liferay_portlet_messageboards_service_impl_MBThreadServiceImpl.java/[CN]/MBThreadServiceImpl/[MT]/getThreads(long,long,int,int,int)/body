{
  if (status == WorkflowConstants.STATUS_ANY) {
    return mbThreadFinder.filterFindByG_C(groupId,categoryId,start,end);
  }
 else {
    QueryDefinition queryDefinition=new QueryDefinition(status,start,end,null);
    return mbThreadFinder.filterFindByG_C(groupId,new long[]{categoryId},queryDefinition);
  }
}
