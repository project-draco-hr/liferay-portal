{
  MBThread thread=mbThreadLocalService.getThread(threadId);
  MBCategoryPermission.check(getPermissionChecker(),thread.getCategoryId(),ActionKeys.MOVE_THREAD);
  MBCategoryPermission.check(getPermissionChecker(),categoryId,ActionKeys.MOVE_THREAD);
  return mbThreadLocalService.moveThread(categoryId,threadId);
}
