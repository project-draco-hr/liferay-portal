{
  shard=toUnwrappedModel(shard);
  Session session=null;
  try {
    session=openSession();
    if (shard.isCachedModel()) {
      shard=(Shard)session.get(ShardImpl.class,shard.getPrimaryKeyObj());
    }
    session.delete(shard);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(shard);
  return shard;
}
