{
  Shard shard=(Shard)EntityCacheUtil.getResult(ShardModelImpl.ENTITY_CACHE_ENABLED,ShardImpl.class,primaryKey);
  if (shard == _nullShard) {
    return null;
  }
  if (shard == null) {
    Session session=null;
    try {
      session=openSession();
      shard=(Shard)session.get(ShardImpl.class,primaryKey);
      if (shard != null) {
        cacheResult(shard);
      }
 else {
        EntityCacheUtil.putResult(ShardModelImpl.ENTITY_CACHE_ENABLED,ShardImpl.class,primaryKey,_nullShard);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(ShardModelImpl.ENTITY_CACHE_ENABLED,ShardImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return shard;
}
