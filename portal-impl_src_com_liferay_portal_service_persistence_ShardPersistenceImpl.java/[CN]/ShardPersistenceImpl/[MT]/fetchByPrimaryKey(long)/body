{
  Shard shard=(Shard)EntityCacheUtil.getResult(ShardModelImpl.ENTITY_CACHE_ENABLED,ShardImpl.class,shardId,this);
  if (shard == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (shard == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      shard=(Shard)session.get(ShardImpl.class,Long.valueOf(shardId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (shard == null)) {
        EntityCacheUtil.putResult(ShardModelImpl.ENTITY_CACHE_ENABLED,ShardImpl.class,shardId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(shard);
      }
      closeSession(session);
    }
  }
  return shard;
}
