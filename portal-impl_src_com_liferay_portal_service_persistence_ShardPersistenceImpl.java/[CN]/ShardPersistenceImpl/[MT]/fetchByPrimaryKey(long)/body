{
  Shard shard=(Shard)EntityCacheUtil.getResult(ShardModelImpl.ENTITY_CACHE_ENABLED,ShardImpl.class,shardId);
  if (shard == _nullShard) {
    return null;
  }
  if (shard == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      shard=(Shard)session.get(ShardImpl.class,Long.valueOf(shardId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (shard != null) {
        cacheResult(shard);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(ShardModelImpl.ENTITY_CACHE_ENABLED,ShardImpl.class,shardId,_nullShard);
      }
      closeSession(session);
    }
  }
  return shard;
}
