{
  Shard result=(Shard)EntityCacheUtil.getResult(ShardModelImpl.ENTITY_CACHE_ENABLED,ShardImpl.class,shardId,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      Shard shard=(Shard)session.get(ShardImpl.class,new Long(shardId));
      if (shard != null) {
        cacheResult(shard);
      }
      return shard;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (Shard)result;
  }
}
