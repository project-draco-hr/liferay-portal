{
  UserGroupGroupRole userGroupGroupRole=fetchByPrimaryKey(userGroupGroupRolePK);
  if (userGroupGroupRole == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No UserGroupGroupRole exists with the primary key " + userGroupGroupRolePK);
    }
    throw new NoSuchUserGroupGroupRoleException("No UserGroupGroupRole exists with the primary key " + userGroupGroupRolePK);
  }
  return userGroupGroupRole;
}
