  repositoryEventRegistry.registerRepositoryEventListener(TrashRepositoryEventType.EntryRestored.class,Folder.class,RESTORE_FOLDER_EVENT_LISTENER);
  repositoryEventRegistry.registerRepositoryEventListener(TrashRepositoryEventType.EntryTrashed.class,FileEntry.class,TRASH_FILE_ENTRY_EVENT_LISTENER);
  repositoryEventRegistry.registerRepositoryEventListener(TrashRepositoryEventType.EntryTrashed.class,Folder.class,TRASH_FOLDER_EVENT_LISTENER);
