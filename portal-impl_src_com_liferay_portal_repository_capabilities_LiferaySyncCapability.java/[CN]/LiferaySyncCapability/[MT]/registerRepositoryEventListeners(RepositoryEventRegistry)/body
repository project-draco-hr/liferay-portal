  repositoryEventRegistry.registerRepositoryEventListener(TrashRepositoryEventType.EntryRestored.class,Folder.class,_RESTORE_FOLDER_EVENT_LISTENER);
  repositoryEventRegistry.registerRepositoryEventListener(TrashRepositoryEventType.EntryTrashed.class,FileEntry.class,_TRASH_FILE_ENTRY_EVENT_LISTENER);
  repositoryEventRegistry.registerRepositoryEventListener(TrashRepositoryEventType.EntryTrashed.class,Folder.class,_TRASH_FOLDER_EVENT_LISTENER);
