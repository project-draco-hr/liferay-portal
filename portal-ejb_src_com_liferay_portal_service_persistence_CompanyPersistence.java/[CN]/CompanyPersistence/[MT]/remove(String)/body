{
  Session session=null;
  try {
    session=openSession();
    Company company=(Company)session.get(CompanyImpl.class,companyId);
    if (company == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No Company exists with the primary key " + companyId);
      }
      throw new NoSuchCompanyException("No Company exists with the primary key " + companyId);
    }
    return remove(company);
  }
 catch (  NoSuchCompanyException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
