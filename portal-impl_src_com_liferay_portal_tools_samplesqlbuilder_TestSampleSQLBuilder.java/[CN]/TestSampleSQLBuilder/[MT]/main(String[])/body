{
  Properties properties=new Properties();
  Reader reader=null;
  try {
    reader=new FileReader(args[0]);
    properties.load(reader);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    if (reader != null) {
      try {
        reader.close();
      }
 catch (      IOException ioe) {
        ioe.printStackTrace();
      }
    }
  }
}
