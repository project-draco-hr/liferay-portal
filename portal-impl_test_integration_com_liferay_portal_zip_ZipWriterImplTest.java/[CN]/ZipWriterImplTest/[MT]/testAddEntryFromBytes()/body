{
  File tempZipFile=temporaryFolder.newFile(_tempZipFilePath);
  FileUtil.copyFile(DependenciesTestUtil.getDependencyAsFile(getClass(),_ZIP_FILE_PATH),tempZipFile);
  ZipWriter zipWriter=new ZipWriterImpl(tempZipFile);
  File entry=DependenciesTestUtil.getDependencyAsFile(getClass(),_ENTRY_FILE_PATH);
  zipWriter.addEntry(_ENTRY_FILE_PATH,FileUtil.getBytes(entry));
  File file=zipWriter.getFile();
  File tempFile=new File(file.getPath());
  Assert.assertTrue(tempFile.exists());
  ZipReader zipReader=new ZipReaderImpl(tempZipFile);
  Assert.assertEquals(_expectedEntryContent,zipReader.getEntryAsString(_ENTRY_FILE_PATH));
  zipReader.close();
}
