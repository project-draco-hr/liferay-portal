{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(wsrpPortlet);
    }
  }
  wsrpPortlet=removeImpl(wsrpPortlet);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(wsrpPortlet);
    }
  }
  return wsrpPortlet;
}
