{
  Layout layout=(Layout)actionRequest.getAttribute(WebKeys.LAYOUT);
  long nodeId=ParamUtil.getLong(actionRequest,"nodeId");
  String name=ParamUtil.getString(actionRequest,"name");
  String description=ParamUtil.getString(actionRequest,"description");
  String[] communityPermissions=actionRequest.getParameterValues("communityPermissions");
  String[] guestPermissions=actionRequest.getParameterValues("guestPermissions");
  if (nodeId <= 0) {
    WikiNodeServiceUtil.addNode(layout.getPlid(),name,description,communityPermissions,guestPermissions);
  }
 else {
    WikiNodeServiceUtil.updateNode(nodeId,name,description);
  }
}
