{
  long nodeId=ParamUtil.getLong(actionRequest,"nodeId");
  String name=ParamUtil.getString(actionRequest,"name");
  String description=ParamUtil.getString(actionRequest,"description");
  ServiceContext serviceContext=ServiceContextFactory.getInstance(WikiNode.class.getName(),actionRequest);
  if (nodeId <= 0) {
    WikiNodeServiceUtil.addNode(name,description,serviceContext);
  }
 else {
    WikiNodeServiceUtil.updateNode(nodeId,name,description);
  }
}
