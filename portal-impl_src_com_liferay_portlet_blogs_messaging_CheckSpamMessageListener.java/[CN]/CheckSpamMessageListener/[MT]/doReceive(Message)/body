{
  Calendar cal=Calendar.getInstance();
  cal.add(Calendar.DATE,-14);
  Date expiration=cal.getTime();
  List<MBMessage> mbMessages=BlogsEntryLocalServiceUtil.getDiscussionMessages(-1,StatusConstants.DENIED,QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  for (  MBMessage mbMessage : mbMessages) {
    Date statusDate=mbMessage.getStatusDate();
    if (statusDate.before(expiration)) {
      MBMessageLocalServiceUtil.deleteDiscussionMessage(mbMessage.getMessageId());
    }
  }
}
