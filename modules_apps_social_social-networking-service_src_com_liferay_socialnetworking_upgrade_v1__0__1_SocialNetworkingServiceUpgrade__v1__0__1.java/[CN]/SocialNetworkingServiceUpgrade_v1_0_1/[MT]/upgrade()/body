{
  Release release=_releaseLocalService.fetchRelease("social-networking-portlet");
  if (release.getBuildNumber() >= 101) {
    return;
  }
  List<UpgradeProcess> upgradeProcesses=Collections.<UpgradeProcess>singletonList(new UpgradePortletId());
  _releaseLocalService.updateRelease("social-networking-portlet",upgradeProcesses,101,0,false);
}
