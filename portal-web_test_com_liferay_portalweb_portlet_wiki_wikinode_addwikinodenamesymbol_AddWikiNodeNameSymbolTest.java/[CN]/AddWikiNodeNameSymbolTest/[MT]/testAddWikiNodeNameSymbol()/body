{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//img[@alt='Manage Wikis']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Wiki']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_36_name",RuntimeVariables.replace("Test/"));
  selenium.type("_36_description",RuntimeVariables.replace("This is a wiki node test."));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have entered invalid data. Please try again."));
  assertTrue(selenium.isTextPresent("Please enter a valid name."));
  selenium.type("_36_name",RuntimeVariables.replace("Test!"));
  selenium.type("_36_description",RuntimeVariables.replace("This is a wiki node test."));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have entered invalid data. Please try again."));
  assertTrue(selenium.isTextPresent("Please enter a valid name."));
  selenium.type("_36_name",RuntimeVariables.replace("Test?"));
  selenium.type("_36_description",RuntimeVariables.replace("This is a wiki node test."));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("You have entered invalid data. Please try again."));
  assertTrue(selenium.isTextPresent("Please enter a valid name."));
}
