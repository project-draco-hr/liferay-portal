{
  entityCache.putResult(ExpandoColumnModelImpl.ENTITY_CACHE_ENABLED,ExpandoColumnImpl.class,expandoColumn.getPrimaryKey(),expandoColumn);
  finderCache.putResult(FINDER_PATH_FETCH_BY_T_N,new Object[]{expandoColumn.getTableId(),expandoColumn.getName()},expandoColumn);
  expandoColumn.resetOriginalValues();
}
