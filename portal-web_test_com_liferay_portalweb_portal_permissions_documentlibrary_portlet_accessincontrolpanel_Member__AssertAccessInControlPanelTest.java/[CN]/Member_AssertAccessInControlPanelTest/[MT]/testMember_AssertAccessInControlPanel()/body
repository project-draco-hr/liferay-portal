{
  selenium.open("/web/guest/home/");
  loadRequiredJavaScriptModules();
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Control Panel")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  selenium.clickAt("link=Documents and Media",RuntimeVariables.replace("Documents and Media"));
  selenium.waitForPageToLoad("30000");
  loadRequiredJavaScriptModules();
  assertEquals(RuntimeVariables.replace("Documents and Media"),selenium.getText("//span[@class='portlet-title-text']"));
}
