{
  StringMaker sm=new StringMaker();
  for (int i=0; i < entryIds.length; i++) {
    sm.append("TagsEntry.entryId ");
    sm.append(operator);
    sm.append(" ? ");
    if ((i + 1) != entryIds.length) {
      sm.append("OR ");
    }
  }
  if (sm.length() == 0) {
    sm.append("(1 = 1)");
  }
  return sm.toString();
}
