{
  if (_bundle == null) {
    throw new IllegalStateException("Bundle is null");
  }
  super.setClassLoader(_classLoader);
  ServletContext servletContext=(ServletContext)ProxyUtil.newProxyInstance(_classLoader,new Class<?>[]{ServletContext.class},new BundleServletContextInvocationHandler(_servletContext,_bundle,_classLoader));
  super.setServletContext(servletContext);
  return super.create(portlet);
}
