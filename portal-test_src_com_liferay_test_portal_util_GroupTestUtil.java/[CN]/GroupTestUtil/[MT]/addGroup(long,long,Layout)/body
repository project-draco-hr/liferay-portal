{
  Group scopeGroup=layout.getScopeGroup();
  if (scopeGroup != null) {
    return scopeGroup;
  }
  return GroupLocalServiceUtil.addGroup(userId,parentGroupId,Layout.class.getName(),layout.getPlid(),GroupConstants.DEFAULT_LIVE_GROUP_ID,String.valueOf(layout.getPlid()),null,0,true,GroupConstants.DEFAULT_MEMBERSHIP_RESTRICTION,null,false,true,null);
}
