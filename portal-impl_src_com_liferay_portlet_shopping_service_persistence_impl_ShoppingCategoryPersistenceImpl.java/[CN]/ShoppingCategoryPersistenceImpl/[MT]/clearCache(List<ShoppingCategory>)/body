{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  ShoppingCategory shoppingCategory : shoppingCategories) {
    EntityCacheUtil.removeResult(ShoppingCategoryModelImpl.ENTITY_CACHE_ENABLED,ShoppingCategoryImpl.class,shoppingCategory.getPrimaryKey());
    clearUniqueFindersCache(shoppingCategory);
  }
}
