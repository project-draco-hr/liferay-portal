{
  int type=socialActivity.getType();
  if ((type == SocialActivityConstants.TYPE_ADD_ATTACHMENT) || (type == SocialActivityConstants.TYPE_ADD_COMMENT) || (type == SocialActivityConstants.TYPE_MOVE_ATTACHMENT_TO_TRASH)|| (type == SocialActivityConstants.TYPE_MOVE_TO_TRASH)|| (type == SocialActivityConstants.TYPE_RESTORE_ATTACHMENT_FROM_TRASH)|| (type == SocialActivityConstants.TYPE_RESTORE_FROM_TRASH)|| (type == WikiActivityKeys.ADD_PAGE)|| (type == WikiActivityKeys.UPDATE_PAGE)) {
    return true;
  }
  return false;
}
