{
  DDMExpression<Double> ddmExpression=_ddmExpressionFactory.createDoubleDDMExpression("ABS(-791239147091293494358236324892348023834834920202734747)");
  Double expectedAbsValue=Double.parseDouble("791239147091293494358236324892348023834834920202734747");
  Assert.assertEquals(expectedAbsValue.doubleValue(),ddmExpression.evaluate(),0.1);
}
