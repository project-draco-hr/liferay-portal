{
  DDMExpression<Double> ddmExpression=_ddmExpressionFactory.createDoubleDDMExpression("SQRT(883830128361284217641725641754781647812547812547812548)");
  Double expectedSqrtValue=Double.valueOf("9.401224007336939E26");
  Assert.assertEquals(expectedSqrtValue.doubleValue(),ddmExpression.evaluate(),0.1);
}
