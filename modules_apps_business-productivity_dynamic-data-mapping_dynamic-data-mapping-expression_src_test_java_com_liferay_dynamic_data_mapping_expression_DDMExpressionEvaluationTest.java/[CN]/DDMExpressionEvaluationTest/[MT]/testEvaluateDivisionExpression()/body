{
  DDMExpression<Double> ddmExpression=_ddmExpressionFactory.createDoubleDDMExpression("54541180012973280712983712089370912739031219430349040" + "4040405 / 2.0");
  double divisionExpectedValue=Double.parseDouble("2.727059000648664E59");
  double divisionActualValue=ddmExpression.evaluate();
  Assert.assertEquals(divisionExpectedValue,divisionActualValue,0.1);
}
