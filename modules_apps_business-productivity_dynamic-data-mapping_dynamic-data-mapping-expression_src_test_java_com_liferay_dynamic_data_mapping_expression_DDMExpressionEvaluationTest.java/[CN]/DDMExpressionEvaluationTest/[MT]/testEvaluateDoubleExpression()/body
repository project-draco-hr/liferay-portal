{
  DDMExpression<Double> ddmExpression=_ddmExpressionFactory.createDoubleDDMExpression("var1 + var2 + var3");
  double var1=5.5;
  ddmExpression.setDoubleVariableValue("var1",var1);
  ddmExpression.setExpressionStringVariableValue("var2","var1 + 3");
  ddmExpression.setExpressionStringVariableValue("var3","var2 + var1");
  double var2=var1 + 3;
  double var3=var1 + var2;
  Assert.assertEquals((Double)(var1 + var2 + var3),ddmExpression.evaluate());
}
