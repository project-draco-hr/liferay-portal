{
  if (stagedModel instanceof StagedGroupedModel) {
    StagedGroupedModel stagedGroupedModel=(StagedGroupedModel)stagedModel;
    return getPath(_PATH_PREFIX_GROUP,stagedGroupedModel.getGroupId(),stagedModel.getModelClassName(),stagedModel.getPrimaryKeyObj(),dependentFileName);
  }
 else {
    return getPath(_PATH_PREFIX_COMPANY,stagedModel.getCompanyId(),stagedModel.getModelClassName(),stagedModel.getPrimaryKeyObj(),dependentFileName);
  }
}
