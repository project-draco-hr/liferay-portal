{
  List<DDMFormLayoutColumn> actualDDMFormLayoutColumns=actualDDMFormLayoutRow.getDDMFormLayoutColumns();
  Assert.assertEquals(1,actualDDMFormLayoutColumns.size());
  DDMFormLayoutColumn actualDDMFormLayoutColumn=actualDDMFormLayoutColumns.get(0);
  Assert.assertEquals(expectedDDMFormFieldName,actualDDMFormLayoutColumn.getDDMFormFieldName());
  Assert.assertEquals(DDMFormLayoutColumn.FULL,actualDDMFormLayoutColumn.getSize());
}
