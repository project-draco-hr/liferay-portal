{
  if (portalCacheConfiguration == null) {
    return;
  }
  Map<CallbackConfiguration,CacheListenerScope> cacheListenerConfigurations=portalCacheConfiguration.getCacheListenerConfigurations();
  for (  Map.Entry<CallbackConfiguration,CacheListenerScope> entry : cacheListenerConfigurations.entrySet()) {
    CallbackConfiguration callbackConfiguration=entry.getKey();
    CallbackFactory callbackFactory=callbackConfiguration.getCallbackFactory();
    PortalCacheListener<K,V> cacheListener=callbackFactory.createCacheListener(callbackConfiguration.getProperties());
    portalCache.registerCacheListener(cacheListener,entry.getValue());
  }
}
