{
  if (portalCacheConfiguration == null) {
    return;
  }
  Map<Properties,PortalCacheListenerScope> cacheListenerConfigurations=portalCacheConfiguration.getPortalCacheListenerConfigurations();
  for (  Map.Entry<Properties,PortalCacheListenerScope> entry : cacheListenerConfigurations.entrySet()) {
    PortalCacheListener<K,V> portalCacheListener=portalCacheListenerFactory.create(entry.getKey());
    portalCache.registerPortalCacheListener(portalCacheListener,entry.getValue());
  }
}
