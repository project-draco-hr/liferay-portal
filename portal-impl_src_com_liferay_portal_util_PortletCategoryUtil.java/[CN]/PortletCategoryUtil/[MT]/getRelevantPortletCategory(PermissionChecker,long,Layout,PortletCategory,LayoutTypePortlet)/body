{
  UnicodeProperties typeSettingsProperties=layout.getTypeSettingsProperties();
  Set<String> panelSelectedPortletIds=SetUtil.fromArray(StringUtil.split(typeSettingsProperties.getProperty("panelSelectedPortlets")));
  return getRelevantPortletCategory(permissionChecker,companyId,layout,portletCategory,panelSelectedPortletIds,layoutTypePortlet);
}
