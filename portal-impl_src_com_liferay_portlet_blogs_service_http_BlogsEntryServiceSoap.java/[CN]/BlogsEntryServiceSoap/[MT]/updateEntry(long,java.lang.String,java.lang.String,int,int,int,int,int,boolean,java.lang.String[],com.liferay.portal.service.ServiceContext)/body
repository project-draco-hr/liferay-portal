{
  try {
    com.liferay.portlet.blogs.model.BlogsEntry returnValue=BlogsEntryServiceUtil.updateEntry(entryId,title,content,displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute,allowTrackbacks,trackbacks,serviceContext);
    return com.liferay.portlet.blogs.model.BlogsEntrySoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
