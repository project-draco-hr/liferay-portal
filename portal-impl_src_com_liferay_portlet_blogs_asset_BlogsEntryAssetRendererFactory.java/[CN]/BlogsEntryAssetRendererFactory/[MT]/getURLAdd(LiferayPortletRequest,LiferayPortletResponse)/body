{
  ThemeDisplay themeDisplay=(ThemeDisplay)liferayPortletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  if (!BlogsPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),ActionKeys.ADD_ENTRY)) {
    return null;
  }
  PortletURL portletURL=liferayPortletResponse.createRenderURL(PortletKeys.BLOGS);
  portletURL.setParameter("struts_action","/blogs/edit_entry");
  return portletURL;
}
