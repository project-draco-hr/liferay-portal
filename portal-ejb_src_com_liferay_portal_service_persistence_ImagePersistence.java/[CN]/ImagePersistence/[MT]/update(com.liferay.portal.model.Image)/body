{
  Session session=null;
  try {
    session=openSession();
    if (image.isNew()) {
      session.save(image);
    }
    session.flush();
    image.setNew(false);
    return image;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
