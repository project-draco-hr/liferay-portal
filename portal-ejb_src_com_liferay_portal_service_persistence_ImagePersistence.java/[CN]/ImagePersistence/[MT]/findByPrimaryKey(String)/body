{
  Session session=null;
  try {
    session=openSession();
    Image image=(Image)session.get(Image.class,imageId);
    if (image == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No Image exists with the primary key " + imageId.toString());
        throw new NoSuchImageException("No Image exists with the primary key " + imageId.toString());
      }
    }
    return image;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
