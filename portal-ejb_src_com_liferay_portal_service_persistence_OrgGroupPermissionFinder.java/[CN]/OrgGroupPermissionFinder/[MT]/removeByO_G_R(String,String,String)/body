{
  Session session=null;
  try {
    session=HibernateUtil.openSession();
    String sql=CustomSQLUtil.get(FIND_BY_O_G_R);
    SQLQuery q=session.createSQLQuery(sql);
    q.setCacheable(false);
    q.addEntity("OrgGroupPermission",OrgGroupPermission.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(resourceId);
    qPos.add(organizatonId);
    qPos.add(groupId);
    Iterator itr=q.list().iterator();
    while (itr.hasNext()) {
      OrgGroupPermission orgGroupPermission=(OrgGroupPermission)itr.next();
      OrgGroupPermissionUtil.remove(orgGroupPermission.getPrimaryKey());
    }
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    HibernateUtil.closeSession(session);
  }
}
