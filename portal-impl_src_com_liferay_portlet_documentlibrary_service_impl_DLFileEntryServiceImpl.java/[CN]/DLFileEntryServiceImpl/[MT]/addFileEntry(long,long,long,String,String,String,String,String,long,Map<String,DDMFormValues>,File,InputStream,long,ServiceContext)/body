{
  DLFolderPermission.check(getPermissionChecker(),groupId,folderId,ActionKeys.ADD_DOCUMENT);
  return dlFileEntryLocalService.addFileEntry(getUserId(),groupId,repositoryId,folderId,sourceFileName,mimeType,title,description,changeLog,fileEntryTypeId,ddmFormValuesMap,file,is,size,serviceContext);
}
