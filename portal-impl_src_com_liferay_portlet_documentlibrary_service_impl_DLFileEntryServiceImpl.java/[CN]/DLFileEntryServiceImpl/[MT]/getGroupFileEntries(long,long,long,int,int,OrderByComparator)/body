{
  long[] folderIds=dlFolderService.getFolderIds(groupId,rootFolderId);
  folderIds=DLAppUtil.filterFolderIds(getPermissionChecker(),groupId,folderIds);
  if (folderIds.length == 0) {
    return Collections.emptyList();
  }
 else   if (userId <= 0) {
    return dlFileEntryPersistence.filterFindByG_F(groupId,folderIds,start,end,obc);
  }
 else {
    return dlFileEntryPersistence.filterFindByG_U_F(groupId,userId,folderIds,start,end,obc);
  }
}
