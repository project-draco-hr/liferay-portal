{
  PermissionChecker permissionChecker=getPermissionChecker();
  DLFileEntryPermission.check(permissionChecker,fileEntryId,ActionKeys.UPDATE);
  DLFolderPermission.check(permissionChecker,serviceContext.getScopeGroupId(),newFolderId,ActionKeys.ADD_DOCUMENT);
  return dlFileEntryLocalService.moveFileEntry(getUserId(),fileEntryId,newFolderId,serviceContext);
}
