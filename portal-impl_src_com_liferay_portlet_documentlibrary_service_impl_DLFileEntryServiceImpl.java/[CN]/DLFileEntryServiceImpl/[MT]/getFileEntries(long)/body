{
  List<DLFileEntry> fileEntries=dlFileEntryLocalService.getFileEntries(folderId);
  fileEntries=ListUtil.copy(fileEntries);
  Iterator<DLFileEntry> itr=fileEntries.iterator();
  while (itr.hasNext()) {
    DLFileEntry fileEntry=itr.next();
    if (!DLFileEntryPermission.contains(getPermissionChecker(),fileEntry,ActionKeys.VIEW)) {
      itr.remove();
    }
  }
  return fileEntries;
}
