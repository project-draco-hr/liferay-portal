{
  boolean isLocked=LockManagerUtil.isLocked(DLFileEntry.class.getName(),fileEntryId);
  if (isLocked && !hasFileEntryLock(fileEntryId)) {
    throw new PrincipalException.MustHavePermission(getUserId(),DLFileEntry.class.getName(),fileEntryId,ActionKeys.OVERRIDE_CHECKOUT);
  }
  dlFileEntryLocalService.checkInFileEntry(getUserId(),fileEntryId,major,changeLog,serviceContext);
}
