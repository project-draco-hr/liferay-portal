{
  if (!hasFileEntryLock(fileEntryId) && !_hasOverrideCheckoutPermission(fileEntryId)) {
    throw new PrincipalException.MustHavePermission(getUserId(),DLFileEntry.class.getName(),fileEntryId,ActionKeys.OVERRIDE_CHECKOUT);
  }
  dlFileEntryLocalService.checkInFileEntry(getUserId(),fileEntryId,lockUuid,serviceContext);
}
