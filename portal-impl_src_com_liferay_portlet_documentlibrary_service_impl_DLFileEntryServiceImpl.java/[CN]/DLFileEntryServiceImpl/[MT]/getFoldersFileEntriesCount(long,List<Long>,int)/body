{
  QueryDefinition<DLFileEntry> queryDefinition=new QueryDefinition<>(status);
  if (folderIds.size() <= PropsValues.SQL_DATA_MAX_PARAMETERS) {
    return dlFileEntryFinder.filterCountByG_F(groupId,folderIds,queryDefinition);
  }
 else {
    int start=0;
    int end=PropsValues.SQL_DATA_MAX_PARAMETERS;
    int filesCount=dlFileEntryFinder.filterCountByG_F(groupId,folderIds.subList(start,end),queryDefinition);
    folderIds.subList(start,end).clear();
    filesCount+=getFoldersFileEntriesCount(groupId,folderIds,status);
    return filesCount;
  }
}
