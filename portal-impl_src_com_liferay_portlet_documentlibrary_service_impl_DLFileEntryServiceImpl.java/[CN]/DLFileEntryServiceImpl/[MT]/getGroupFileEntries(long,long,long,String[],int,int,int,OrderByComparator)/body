{
  long[] folderIds=dlFolderService.getFolderIds(groupId,rootFolderId);
  if (folderIds.length == 0) {
    return Collections.emptyList();
  }
  List<Long> folderIdsList=ListUtil.toList(folderIds);
  QueryDefinition queryDefinition=new QueryDefinition(status,start,end,obc);
  return dlFileEntryFinder.findByG_U_F_M(groupId,userId,folderIdsList,mimeTypes,queryDefinition);
}
