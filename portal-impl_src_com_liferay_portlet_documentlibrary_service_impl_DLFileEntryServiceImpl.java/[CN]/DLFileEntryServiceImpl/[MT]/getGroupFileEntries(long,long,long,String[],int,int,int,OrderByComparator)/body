{
  List<Long> folderIds=dlFolderService.getFolderIds(groupId,rootFolderId);
  if (folderIds.size() == 0) {
    return Collections.emptyList();
  }
  QueryDefinition queryDefinition=new QueryDefinition(status,start,end,obc);
  return dlFileEntryFinder.findByG_U_F_M(groupId,userId,folderIds,mimeTypes,queryDefinition);
}
