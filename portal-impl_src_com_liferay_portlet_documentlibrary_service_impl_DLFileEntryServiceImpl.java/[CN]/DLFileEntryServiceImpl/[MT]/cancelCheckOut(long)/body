{
  boolean isLocked=lockLocalService.isLocked(DLFileEntry.class.getName(),fileEntryId);
  try {
    if (isLocked && !hasFileEntryLock(fileEntryId) && !_hasOverrideCheckoutPermission(fileEntryId)) {
      throw new PrincipalException();
    }
  }
 catch (  NoSuchFileEntryException nsfee) {
  }
  return dlFileEntryLocalService.cancelCheckOut(getUserId(),fileEntryId);
}
