{
  Map<Layout,Object> map=_layoutStagingHandlerProxyCache.get();
  Object proxy=map.get(layout);
  if (proxy != null) {
    return (Layout)proxy;
  }
  proxy=ProxyUtil.newProxyInstance(ClassLoaderUtil.getPortalClassLoader(),new Class[]{Layout.class},new LayoutStagingHandler(layout));
  map.put(layout,proxy);
  return (Layout)proxy;
}
