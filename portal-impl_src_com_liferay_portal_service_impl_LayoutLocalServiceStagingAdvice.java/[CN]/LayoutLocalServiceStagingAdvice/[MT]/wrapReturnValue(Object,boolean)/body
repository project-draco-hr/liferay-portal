{
  if (returnValue instanceof Layout) {
    returnValue=wrapLayout((Layout)returnValue);
  }
 else   if ((returnValue instanceof List<?>) && (!((List<?>)returnValue).isEmpty()) && (((List<?>)returnValue).get(0) instanceof Layout)) {
    returnValue=wrapLayouts((List<Layout>)returnValue,showIncomplete);
  }
  return returnValue;
}
