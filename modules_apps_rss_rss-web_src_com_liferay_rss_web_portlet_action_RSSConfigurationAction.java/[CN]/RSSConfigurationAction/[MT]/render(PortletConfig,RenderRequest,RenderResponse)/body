{
  ThemeDisplay themeDisplay=(ThemeDisplay)renderRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  try {
    RSSPortletInstanceSettings rssPortletInstanceSettings=_settingsFactory.getSettings(RSSPortletInstanceSettings.class,new ParameterMapSettingsLocator(renderRequest.getParameterMap(),new PortletInstanceSettingsLocator(themeDisplay.getLayout(),portletDisplay.getPortletResource())));
    renderRequest.setAttribute(RSSPortletInstanceSettings.class.getName(),rssPortletInstanceSettings);
  }
 catch (  PortalException pe) {
    throw new SystemException(pe);
  }
  return super.render(portletConfig,renderRequest,renderResponse);
}
