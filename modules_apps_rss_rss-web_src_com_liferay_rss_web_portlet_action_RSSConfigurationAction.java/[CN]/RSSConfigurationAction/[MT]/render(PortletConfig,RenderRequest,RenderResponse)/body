{
  ThemeDisplay themeDisplay=(ThemeDisplay)renderRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletDisplay portletDisplay=themeDisplay.getPortletDisplay();
  try {
    RSSPortletInstanceConfiguration rssPortletInstanceConfiguration=_settingsFactory.getSettings(RSSPortletInstanceConfiguration.class,new ParameterMapSettingsLocator(renderRequest.getParameterMap(),new PortletInstanceSettingsLocator(themeDisplay.getLayout(),portletDisplay.getPortletResource())));
    renderRequest.setAttribute(RSSPortletInstanceConfiguration.class.getName(),rssPortletInstanceConfiguration);
  }
 catch (  PortalException pe) {
    throw new SystemException(pe);
  }
  return super.render(portletConfig,renderRequest,renderResponse);
}
