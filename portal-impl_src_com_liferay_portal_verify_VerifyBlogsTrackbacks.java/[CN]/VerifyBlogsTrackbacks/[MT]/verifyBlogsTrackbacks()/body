{
  String className=BlogsEntry.class.getName();
  List<MBDiscussion> discussions=MBMessageLocalServiceUtil.getDiscussions(className);
  for (  MBDiscussion discussion : discussions) {
    long entryId=discussion.getClassPK();
    long threadId=discussion.getThreadId();
    try {
      BlogsEntry entry=BlogsEntryLocalServiceUtil.getBlogsEntry(entryId);
      List<MBMessage> messages=MBMessageLocalServiceUtil.getThreadMessages(threadId);
      for (      MBMessage message : messages) {
        TrackbackVerifierUtil.verifyPost(entry,message);
      }
    }
 catch (    Exception e) {
      _log.error(e,e);
    }
  }
}
