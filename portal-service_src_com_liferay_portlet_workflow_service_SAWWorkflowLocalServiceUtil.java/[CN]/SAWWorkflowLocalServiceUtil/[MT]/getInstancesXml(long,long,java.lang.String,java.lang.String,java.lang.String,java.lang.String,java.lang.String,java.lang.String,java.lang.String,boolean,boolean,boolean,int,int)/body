{
  SAWWorkflowLocalService sawWorkflowLocalService=SAWWorkflowLocalServiceFactory.getService();
  return sawWorkflowLocalService.getInstancesXml(definitionId,instanceId,definitionName,definitionVersion,startDateGT,startDateLT,endDateGT,endDateLT,userId,hideEndedTasks,retrieveUserInstances,andOperator,start,end);
}
