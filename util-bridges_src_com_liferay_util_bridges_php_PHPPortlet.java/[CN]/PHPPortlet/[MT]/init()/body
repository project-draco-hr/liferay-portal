{
  editUri=getInitParameter("edit-uri");
  helpUri=getInitParameter("help-uri");
  viewUri=getInitParameter("view-uri");
  String servletObjectsFactoryName=getInitParameter("servlet-objects-factory");
  try {
    Class<?> servletObjectsFactoryClass=Class.forName(servletObjectsFactoryName);
    servletObjectsFactory=(ServletObjectsFactory)servletObjectsFactoryClass.newInstance();
  }
 catch (  Exception e) {
    throw new PortletException("Unable to instantiate factory" + servletObjectsFactoryName,e);
  }
}
