{
  MockActionRequest actionRequest=new MockActionRequest();
  actionRequest.addParameter(ActionRequest.ACTION_NAME,TestActionCommand.TEST_ACTION_COMMAND_NAME);
  _portlet.processAction(actionRequest,new MockActionResponse());
  Assert.assertNotNull(actionRequest.getAttribute(TestActionCommand.TEST_ACTION_COMMAND_ATTRIBUTE));
  Assert.assertEquals(TestActionCommand.TEST_ACTION_COMMAND_ATTRIBUTE,actionRequest.getAttribute(TestActionCommand.TEST_ACTION_COMMAND_ATTRIBUTE));
}
