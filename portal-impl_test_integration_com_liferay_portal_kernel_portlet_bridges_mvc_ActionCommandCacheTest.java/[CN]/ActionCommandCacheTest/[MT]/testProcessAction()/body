{
  MockActionRequest actionRequest=new MockActionRequest();
  actionRequest.addParameter(ActionRequest.ACTION_NAME,TestActionCommand.TEST_ACTION_COMMAND_NAME);
  _portlet.processAction(actionRequest,new MockActionResponse());
  Assert.assertNotNull(actionRequest.getAttribute("TEST_ACTION_COMMAND"));
  Assert.assertEquals("TEST_ACTION_COMMAND",actionRequest.getAttribute("TEST_ACTION_COMMAND"));
}
