{
  if (_log.isDebugEnabled()) {
    _log.debug("Borrowing:\t" + _instance._pool.getNumIdle() + "\t"+ _instance._pool.getNumActive());
  }
  RenderResponseImpl renderResponseImpl=(RenderResponseImpl)_instance._pool.borrowObject();
  renderResponseImpl.init(req,res,portletName,companyId,plid);
  return renderResponseImpl;
}
