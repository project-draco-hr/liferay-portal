{
  DDMStructure ddmStructure=addDDMStructure(actionRequest);
  DDLRecordSet recordSet=addRecordSet(actionRequest,ddmStructure.getStructureId());
  String publish=ParamUtil.getString(actionRequest,"publish","false");
  UnicodeProperties typeSettingsProperties=new UnicodeProperties(true);
  typeSettingsProperties.setProperty("published",publish);
  ddlRecordSetService.updateRecordSet(recordSet.getRecordSetId(),typeSettingsProperties.toString());
}
