{
  List<ShoppingOrder> list=findByG_U_PPPS(groupId,userId,ppPaymentStatus,0,1,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler();
    msg.append("No ShoppingOrder exists with the key {");
    msg.append("groupId=" + groupId);
    msg.append(", ");
    msg.append("userId=" + userId);
    msg.append(", ");
    msg.append("ppPaymentStatus=" + ppPaymentStatus);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchOrderException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
