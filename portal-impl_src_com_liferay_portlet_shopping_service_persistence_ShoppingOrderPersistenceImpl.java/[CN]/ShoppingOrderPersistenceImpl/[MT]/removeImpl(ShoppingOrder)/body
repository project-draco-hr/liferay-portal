{
  Session session=null;
  try {
    session=openSession();
    session.delete(shoppingOrder);
    session.flush();
    return shoppingOrder;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ShoppingOrder.class.getName());
  }
}
