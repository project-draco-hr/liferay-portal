{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByG_U_PPPS_PrevAndNext(orderId,groupId,userId,ppPaymentStatus,orderByComparator);
  }
  ShoppingOrder shoppingOrder=findByPrimaryKey(orderId);
  Session session=null;
  try {
    session=openSession();
    ShoppingOrder[] array=new ShoppingOrderImpl[3];
    array[0]=filterGetByG_U_PPPS_PrevAndNext(session,shoppingOrder,groupId,userId,ppPaymentStatus,orderByComparator,true);
    array[1]=shoppingOrder;
    array[2]=filterGetByG_U_PPPS_PrevAndNext(session,shoppingOrder,groupId,userId,ppPaymentStatus,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
