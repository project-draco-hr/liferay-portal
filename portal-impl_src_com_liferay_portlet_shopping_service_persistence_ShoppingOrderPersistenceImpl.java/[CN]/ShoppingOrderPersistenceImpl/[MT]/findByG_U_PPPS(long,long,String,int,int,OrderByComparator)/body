{
  Object[] finderArgs=new Object[]{new Long(groupId),new Long(userId),ppPaymentStatus,String.valueOf(start),String.valueOf(end),String.valueOf(obc)};
  List<ShoppingOrder> list=(List<ShoppingOrder>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_OBC_G_U_PPPS,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT shoppingOrder FROM ShoppingOrder shoppingOrder WHERE ");
      query.append("shoppingOrder.groupId = ?");
      query.append(" AND ");
      query.append("shoppingOrder.userId = ?");
      query.append(" AND ");
      if (ppPaymentStatus == null) {
        query.append("shoppingOrder.ppPaymentStatus IS NULL");
      }
 else {
        if (ppPaymentStatus.equals(StringPool.BLANK)) {
          query.append("(shoppingOrder.ppPaymentStatus IS NULL OR ");
        }
        query.append("shoppingOrder.ppPaymentStatus = ?");
        if (ppPaymentStatus.equals(StringPool.BLANK)) {
          query.append(")");
        }
      }
      query.append(" ");
      if (obc != null) {
        query.append("ORDER BY ");
        String[] orderByFields=obc.getOrderByFields();
        for (int i=0; i < orderByFields.length; i++) {
          query.append("shoppingOrder.");
          query.append(orderByFields[i]);
          if (obc.isAscending()) {
            query.append(" ASC");
          }
 else {
            query.append(" DESC");
          }
          if ((i + 1) < orderByFields.length) {
            query.append(", ");
          }
        }
      }
 else {
        query.append("ORDER BY ");
        query.append("shoppingOrder.createDate DESC");
      }
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      qPos.add(userId);
      if (ppPaymentStatus != null) {
        qPos.add(ppPaymentStatus);
      }
      list=(List<ShoppingOrder>)QueryUtil.list(q,getDialect(),start,end);
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<ShoppingOrder>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_OBC_G_U_PPPS,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
