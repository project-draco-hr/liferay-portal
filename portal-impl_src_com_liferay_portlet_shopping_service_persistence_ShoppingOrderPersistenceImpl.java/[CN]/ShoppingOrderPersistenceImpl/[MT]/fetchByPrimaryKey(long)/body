{
  ShoppingOrder shoppingOrder=(ShoppingOrder)EntityCacheUtil.getResult(ShoppingOrderModelImpl.ENTITY_CACHE_ENABLED,ShoppingOrderImpl.class,orderId);
  if (shoppingOrder == _nullShoppingOrder) {
    return null;
  }
  if (shoppingOrder == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      shoppingOrder=(ShoppingOrder)session.get(ShoppingOrderImpl.class,Long.valueOf(orderId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (shoppingOrder != null) {
        cacheResult(shoppingOrder);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(ShoppingOrderModelImpl.ENTITY_CACHE_ENABLED,ShoppingOrderImpl.class,orderId,_nullShoppingOrder);
      }
      closeSession(session);
    }
  }
  return shoppingOrder;
}
