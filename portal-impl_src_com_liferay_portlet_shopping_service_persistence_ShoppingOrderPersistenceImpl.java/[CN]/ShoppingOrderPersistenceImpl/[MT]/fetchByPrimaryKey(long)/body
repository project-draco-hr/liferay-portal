{
  ShoppingOrder shoppingOrder=(ShoppingOrder)EntityCacheUtil.getResult(ShoppingOrderModelImpl.ENTITY_CACHE_ENABLED,ShoppingOrderImpl.class,orderId,this);
  if (shoppingOrder == _NULL_PLACE_HOLDER) {
    return null;
  }
  if (shoppingOrder == null) {
    Session session=null;
    boolean hasError=false;
    try {
      session=openSession();
      shoppingOrder=(ShoppingOrder)session.get(ShoppingOrderImpl.class,Long.valueOf(orderId));
    }
 catch (    Exception e) {
      hasError=true;
      throw processException(e);
    }
 finally {
      if ((!hasError) && (shoppingOrder == null)) {
        EntityCacheUtil.putResult(ShoppingOrderModelImpl.ENTITY_CACHE_ENABLED,ShoppingOrderImpl.class,orderId,_NULL_PLACE_HOLDER);
      }
 else {
        cacheResult(shoppingOrder);
      }
      closeSession(session);
    }
  }
  return shoppingOrder;
}
