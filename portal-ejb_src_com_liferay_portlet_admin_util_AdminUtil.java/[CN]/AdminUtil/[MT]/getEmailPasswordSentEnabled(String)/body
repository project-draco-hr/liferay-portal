{
  PortletPreferences prefs=getPreferences(companyId);
  String emailPasswordSentEnabled=prefs.getValue("email-password-sent-enabled",StringPool.BLANK);
  if (Validator.isNotNull(emailPasswordSentEnabled)) {
    return GetterUtil.getBoolean(emailPasswordSentEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsUtil.ADMIN_EMAIL_PASSWORD_SENT_ENABLED));
  }
}
