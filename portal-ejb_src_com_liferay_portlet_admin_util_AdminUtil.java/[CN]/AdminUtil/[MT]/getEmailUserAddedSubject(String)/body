{
  PortletPreferences prefs=getPreferences(companyId);
  String emailUserAddedSubject=prefs.getValue("email-user-added-subject",StringPool.BLANK);
  if (Validator.isNotNull(emailUserAddedSubject)) {
    return emailUserAddedSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsUtil.ADMIN_EMAIL_USER_ADDED_SUBJECT));
  }
}
