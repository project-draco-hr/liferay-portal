{
  PortletPreferences prefs=getPreferences(companyId);
  String emailPasswordSentSubject=prefs.getValue("email-password-sent-subject",StringPool.BLANK);
  if (Validator.isNotNull(emailPasswordSentSubject)) {
    return emailPasswordSentSubject;
  }
 else {
    return ContentUtil.get(PropsUtil.get(PropsUtil.ADMIN_EMAIL_PASSWORD_SENT_SUBJECT));
  }
}
