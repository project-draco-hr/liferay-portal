{
  PortletPreferences prefs=getPreferences(companyId);
  String emailUserAddedEnabled=prefs.getValue("email-user-added-enabled",StringPool.BLANK);
  if (Validator.isNotNull(emailUserAddedEnabled)) {
    return GetterUtil.getBoolean(emailUserAddedEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsUtil.ADMIN_EMAIL_USER_ADDED_ENABLED));
  }
}
