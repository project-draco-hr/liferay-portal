{
  try {
    ActionUtil.getOrder(req);
    return mapping.findForward("/portlet/shopping/edit_order_invoice.jsp");
  }
 catch (  Exception e) {
    if (e != null && e instanceof NoSuchOrderException || e instanceof PrincipalException) {
      SessionErrors.add(req,e.getClass().getName());
      return mapping.findForward("portlet.shopping.error");
    }
 else {
      req.setAttribute(PageContext.EXCEPTION,e);
      return mapping.findForward(Constants.COMMON_ERROR);
    }
  }
}
