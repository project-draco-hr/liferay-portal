{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),KaleoTransitionLocalService.class.getName());
    if (invokableLocalService instanceof KaleoTransitionLocalService) {
      _service=(KaleoTransitionLocalService)invokableLocalService;
    }
 else {
      _service=new KaleoTransitionLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(KaleoTransitionLocalServiceUtil.class,"_service");
  }
  return _service;
}
