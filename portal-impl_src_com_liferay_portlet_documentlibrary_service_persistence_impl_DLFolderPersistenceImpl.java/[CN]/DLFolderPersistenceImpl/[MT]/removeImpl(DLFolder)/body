{
  dlFolder=toUnwrappedModel(dlFolder);
  dlFolderToDLFileEntryTypeTableMapper.deleteLeftPrimaryKeyTableMappings(companyProvider.getCompanyId(),dlFolder.getPrimaryKey());
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(dlFolder)) {
      dlFolder=(DLFolder)session.get(DLFolderImpl.class,dlFolder.getPrimaryKeyObj());
    }
    if (dlFolder != null) {
      session.delete(dlFolder);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (dlFolder != null) {
    clearCache(dlFolder);
  }
  return dlFolder;
}
