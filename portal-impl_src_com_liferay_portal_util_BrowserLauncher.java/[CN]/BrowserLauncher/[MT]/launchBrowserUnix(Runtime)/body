{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < _BROWSERS.length; i++) {
    if (i != 0) {
      sb.append(" || ");
    }
    sb.append(_BROWSERS[i]);
    sb.append(" \"");
    sb.append(PropsValues.BROWSER_LAUNCHER_URL);
    sb.append("\" ");
  }
  runtime.exec(new String[]{"sh","-c",sb.toString()});
}
