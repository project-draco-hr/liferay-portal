{
  List<WorkflowInstanceLink> workflowInstanceLinks=getWorkflowInstanceLinks(companyId,groupId,className,classPK);
  for (  WorkflowInstanceLink workflowInstanceLink : workflowInstanceLinks) {
    deleteWorkflowInstanceLink(workflowInstanceLink);
    subscriptionLocalService.deleteSubscriptions(companyId,WorkflowInstance.class.getName(),workflowInstanceLink.getWorkflowInstanceId());
    WorkflowInstanceManagerUtil.deleteWorkflowInstance(companyId,workflowInstanceLink.getWorkflowInstanceId());
  }
}
