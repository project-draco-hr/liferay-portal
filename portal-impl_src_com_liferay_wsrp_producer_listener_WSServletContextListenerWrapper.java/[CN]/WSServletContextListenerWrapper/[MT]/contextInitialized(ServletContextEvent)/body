{
  if (!ServerDetector.isJBoss()) {
    try {
      if (_log.isInfoEnabled()) {
        _log.info("WSRP-JAX-WS context listener initializing");
      }
      Class clazz=Class.forName(_listenerClass);
      _listener=(ServletContextListener)clazz.newInstance();
      _listener.contextInitialized(event);
    }
 catch (    Exception e) {
      _log.error("WSRP-JAX-WS context listener initialization failed",e);
    }
  }
 else {
    if (_log.isWarnEnabled()) {
      _log.warn("WSRP is not supported on this application server");
    }
  }
}
