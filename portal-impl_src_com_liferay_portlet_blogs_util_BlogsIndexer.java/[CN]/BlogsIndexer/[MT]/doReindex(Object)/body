{
  BlogsEntry entry=(BlogsEntry)obj;
  if (!entry.isApproved() && (entry.getStatus() != WorkflowConstants.STATUS_IN_TRASH) && (entry.getStatus() != WorkflowConstants.STATUS_DRAFT)) {
    return;
  }
  Document document=getDocument(entry);
  SearchEngineUtil.updateDocument(getSearchEngineId(),entry.getCompanyId(),document);
}
