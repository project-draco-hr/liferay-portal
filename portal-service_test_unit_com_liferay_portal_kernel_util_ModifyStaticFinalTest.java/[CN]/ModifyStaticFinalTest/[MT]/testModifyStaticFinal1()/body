{
  assertFalse(StaticFinalClass.VALUE);
  System.setProperty(_testKey,"true");
  assertFalse(StaticFinalClass.VALUE);
  boolean result=runInNewClassLoader(new Callable<Boolean>(){
    public Boolean call() throws Exception {
      return StaticFinalClass.VALUE;
    }
  }
.getClass());
  assertTrue(result);
}
