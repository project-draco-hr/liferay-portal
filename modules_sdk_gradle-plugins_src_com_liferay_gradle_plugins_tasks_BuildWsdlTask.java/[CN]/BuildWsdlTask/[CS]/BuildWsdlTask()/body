{
  _project=getProject();
  Configuration configuration=GradleUtil.addConfiguration(_project,_CONFIGURATION_NAME);
  configuration.setDescription("Configures Apache Axis for generating WSDL client stubs.");
  configuration.setVisible(false);
  GradleUtil.executeIfEmpty(configuration,new Action<Configuration>(){
    @Override public void execute(    Configuration configuration){
      addDependencies();
    }
  }
);
}
