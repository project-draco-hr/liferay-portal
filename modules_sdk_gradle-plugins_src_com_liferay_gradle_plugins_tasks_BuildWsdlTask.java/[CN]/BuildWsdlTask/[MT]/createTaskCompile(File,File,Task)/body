{
  String taskName=GradleUtil.getTaskName(LiferayJavaPlugin.BUILD_WSDL_TASK_NAME + "Compile",wsdlFile);
  JavaCompile javaCompile=GradleUtil.addTask(_project,taskName,JavaCompile.class);
  javaCompile.setClasspath(getConfiguration());
  File tmpBinDir=new File(tmpDir,"bin");
  javaCompile.setDestinationDir(tmpBinDir);
  javaCompile.setSource(generateTask.getOutputs());
  return javaCompile;
}
