{
  SAXReader saxReader=new SAXReader();
  for (int i=0; i < _HTML_XML_REPLACEMENTS.length; i++) {
    xml=xml.replace(_HTML_XML_REPLACEMENTS[i][0],_HTML_XML_REPLACEMENTS[i][1]);
  }
  Document document=saxReader.read(new StringReader(xml));
  String formattedXML=JenkinsResultsParserUtil.format(document.getRootElement());
  for (int i=0; i < _HTML_XML_REPLACEMENTS.length; i++) {
    formattedXML=formattedXML.replace(_HTML_XML_REPLACEMENTS[i][1],_HTML_XML_REPLACEMENTS[i][0]);
  }
  return formattedXML;
}
