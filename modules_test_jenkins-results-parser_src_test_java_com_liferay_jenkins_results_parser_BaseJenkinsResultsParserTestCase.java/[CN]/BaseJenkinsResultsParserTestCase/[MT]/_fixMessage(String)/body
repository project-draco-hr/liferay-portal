{
  String fixedMessage=message;
  if (fixedMessage.contains(JenkinsResultsParserUtil.HTTP_DEPENDENCIES_URL)) {
    fixedMessage=fixedMessage.replace(JenkinsResultsParserUtil.HTTP_DEPENDENCIES_URL,"${dependencies.url}");
  }
  if (fixedMessage.contains(JenkinsResultsParserUtil.FILE_DEPENDENCIES_URL)) {
    fixedMessage=fixedMessage.replace(JenkinsResultsParserUtil.FILE_DEPENDENCIES_URL,"${dependencies.url}");
  }
  return fixedMessage;
}
