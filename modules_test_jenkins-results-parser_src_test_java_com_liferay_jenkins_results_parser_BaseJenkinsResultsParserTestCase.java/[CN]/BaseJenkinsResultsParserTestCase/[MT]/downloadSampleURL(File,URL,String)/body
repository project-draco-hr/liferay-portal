{
  String urlString=url + urlSuffix;
  if (urlString.endsWith("json")) {
    urlString+="?pretty";
  }
  urlSuffix=JenkinsResultsParserUtil.fixFileName(urlSuffix);
  JenkinsResultsParserUtil.write(new File(dir,urlSuffix),JenkinsResultsParserUtil.toString(JenkinsResultsParserUtil.getLocalURL(urlString)));
}
