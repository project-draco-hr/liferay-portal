{
  if (message.contains(JenkinsResultsParserUtil.DEPENDENCIES_URL_FILE)) {
    message=message.replace(JenkinsResultsParserUtil.DEPENDENCIES_URL_FILE,"${dependencies.url}");
  }
  if (message.contains(JenkinsResultsParserUtil.DEPENDENCIES_URL_HTTP)) {
    message=message.replace(JenkinsResultsParserUtil.DEPENDENCIES_URL_HTTP,"${dependencies.url}");
  }
  return message.replaceAll("[^\\S\\r\\n]+\n","\n");
}
