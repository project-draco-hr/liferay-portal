{
  URI uri=file.toURI();
  URL url=uri.toURL();
  String urlString=url.toString();
  String path=dependenciesDir.getPath();
  int x=path.indexOf("src/test/resources/dependencies/") + "src/test/resources/dependencies/".length();
  path=path.substring(x);
  return urlString.replace("file:" + dependenciesDir.getAbsolutePath(),"${dependencies.url}/" + path);
}
