{
  Mockito.when(_http.URLtoString("__url__")).thenReturn("__URLtoString_not_containing_entryURL__");
  long messageId=RandomTestUtil.randomLong();
  _linkbackConsumer.addNewTrackback(messageId,"__url__","__entryUrl__");
  _linkbackConsumer.verifyNewTrackbacks(_comments);
  Mockito.verify(_comments).deleteComment(messageId);
  Mockito.verify(_http).URLtoString("__url__");
}
