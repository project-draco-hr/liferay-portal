{
  String sql="INSERT INTO " + getInsertTableName() + " (";
  for (int i=0; i < _order.length; i++) {
    int pos=_order[i];
    sql+=_columns[pos][0];
    if ((i + 1) < _columns.length) {
      sql+=", ";
    }
 else {
      sql+=") VALUES (";
    }
  }
  for (int i=0; i < _columns.length; i++) {
    sql+="?";
    if ((i + 1) < _columns.length) {
      sql+=", ";
    }
 else {
      sql+=")";
    }
  }
  return sql;
}
