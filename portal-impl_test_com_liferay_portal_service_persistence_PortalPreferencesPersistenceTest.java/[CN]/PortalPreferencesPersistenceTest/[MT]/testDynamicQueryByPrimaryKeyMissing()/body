{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(PortalPreferences.class,PortalPreferences.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("portalPreferencesId",nextLong()));
  List<PortalPreferences> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
