{
  if (method.equals(Connection.class.getMethod("getMetaData"))) {
    return ProxyUtil.newProxyInstance(ClassLoader.getSystemClassLoader(),new Class<?>[]{DatabaseMetaData.class},new DatabaseMetaDataInvocationHandler(_supportBatchUpdates));
  }
  if (method.equals(Connection.class.getMethod("prepareStatement",String.class))) {
    return ProxyUtil.newProxyInstance(ClassLoader.getSystemClassLoader(),new Class<?>[]{PreparedStatement.class},getPSInvocationHandler());
  }
  throw new UnsupportedOperationException();
}
