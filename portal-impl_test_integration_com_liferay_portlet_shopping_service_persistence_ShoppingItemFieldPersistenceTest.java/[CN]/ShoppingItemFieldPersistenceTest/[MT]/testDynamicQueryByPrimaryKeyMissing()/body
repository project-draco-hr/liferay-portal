{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ShoppingItemField.class,ShoppingItemField.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("itemFieldId",ServiceTestUtil.nextLong()));
  List<ShoppingItemField> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
