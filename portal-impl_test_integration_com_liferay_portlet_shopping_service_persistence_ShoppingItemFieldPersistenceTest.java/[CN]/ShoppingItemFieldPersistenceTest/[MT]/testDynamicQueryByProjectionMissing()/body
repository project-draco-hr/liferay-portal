{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ShoppingItemField.class,ShoppingItemField.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("itemFieldId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("itemFieldId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
