{
  MethodHandler methodHandler=new MethodHandler(method,args);
  String threadContextServletContextName=ClassLoaderPool.getContextName(ClassLoaderUtil.getContextClassLoader());
  IdentifiableOSGiService identifiableOSGiService=(IdentifiableOSGiService)targetObject;
  return new MethodHandler(_invokeMethodKey,methodHandler,threadContextServletContextName,identifiableOSGiService.getOSGiServiceIdentifier());
}
