{
  selenium.open("/user/joebloggs/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("//div/div/div/div/ul/li/a")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("//div/div/div/div/ul/li/a",RuntimeVariables.replace("Home"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Sites"),selenium.getText("//div[3]/div/section/header/h1/span"));
  assertTrue(selenium.isVisible("//div/input[1]"));
  selenium.type("//div/input[1]",RuntimeVariables.replace("Open"));
  Thread.sleep(5000);
  assertEquals(RuntimeVariables.replace("Open Site Name"),selenium.getText("//li[contains(@class, 'social-office-enabled')]/span[2]/a"));
  assertEquals("Search",selenium.getValue("//input[@value='Search']"));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace("Search"));
  Thread.sleep(5000);
  assertEquals(RuntimeVariables.replace("Open Site Name"),selenium.getText("//li[contains(@class, 'social-office-enabled')]/span[2]/a"));
}
