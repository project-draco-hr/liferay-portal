{
  PortletCategory portletCategory1=(PortletCategory)obj1;
  PortletCategory portletCategory2=(PortletCategory)obj2;
  String name1=portletCategory1.getName();
  try {
    name1=LanguageUtil.get(_companyId,_locale,portletCategory1.getName());
  }
 catch (  LanguageException le) {
  }
  String name2=portletCategory2.getName();
  try {
    name2=LanguageUtil.get(_companyId,_locale,portletCategory2.getName());
  }
 catch (  LanguageException le) {
  }
  return name1.compareTo(name2);
}
