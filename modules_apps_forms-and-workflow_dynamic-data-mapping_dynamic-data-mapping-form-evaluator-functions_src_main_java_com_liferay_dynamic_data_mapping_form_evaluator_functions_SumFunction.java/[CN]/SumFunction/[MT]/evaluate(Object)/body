{
  if (parameters.length < 2) {
    throw new IllegalArgumentException("Two or more parameters are expected");
  }
  double sum=0;
  for (  Object parameter : parameters) {
    if (!Number.class.isInstance(parameter)) {
      throw new IllegalArgumentException("The parameters should be numbers");
    }
    Number parameterDouble=(Number)parameter;
    sum+=parameterDouble.doubleValue();
  }
  return sum;
}
