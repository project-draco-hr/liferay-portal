{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home");
  selenium.waitForElementPresent("link=Blogs Portlet Permissions Page");
  selenium.clickAt("link=Blogs Portlet Permissions Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=Add Application",RuntimeVariables.replace(""));
  selenium.waitForElementPresent("//div[@id='Collaboration-Blogs']/p/a");
  selenium.clickAt("//div[@id='Collaboration-Blogs']/p/a",RuntimeVariables.replace(""));
  selenium.waitForElementPresent("//td[1]/div/div[1]/div");
  assertTrue(selenium.isElementPresent("//td[1]/div/div[1]/div"));
}
