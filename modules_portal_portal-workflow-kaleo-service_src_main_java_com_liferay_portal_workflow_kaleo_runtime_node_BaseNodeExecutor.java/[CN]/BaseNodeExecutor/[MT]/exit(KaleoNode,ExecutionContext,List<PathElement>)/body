{
  ExecutionUtil.completeKaleoTimerInstances(executionContext);
  doExit(currentKaleoNode,executionContext,remainingPathElements);
  actionExecutorManagerHelper.executeKaleoActions(KaleoNode.class.getName(),currentKaleoNode.getKaleoNodeId(),ExecutionType.ON_EXIT,executionContext);
  NotificationUtil.sendKaleoNotifications(KaleoNode.class.getName(),currentKaleoNode.getKaleoNodeId(),ExecutionType.ON_EXIT,executionContext);
}
