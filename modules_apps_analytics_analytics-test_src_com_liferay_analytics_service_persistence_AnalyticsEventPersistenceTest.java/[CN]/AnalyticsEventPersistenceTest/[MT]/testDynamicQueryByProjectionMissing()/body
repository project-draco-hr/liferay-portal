{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(AnalyticsEvent.class,AnalyticsEvent.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("analyticsEventId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("analyticsEventId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
