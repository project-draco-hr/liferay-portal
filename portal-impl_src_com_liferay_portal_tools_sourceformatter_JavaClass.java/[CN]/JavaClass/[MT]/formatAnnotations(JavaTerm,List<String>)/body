{
  if (_fileName.contains("/test/") && !BaseSourceProcessor.isExcluded(testAnnotationsExclusions,_fileName) && !_fileName.endsWith("TestCase.java")) {
    checkTestAnnotations(javaTerm);
  }
  String javaTermContent=javaTerm.getContent();
  String newJavaTermContent=JavaSourceProcessor.sortAnnotations(javaTermContent,_indent);
  if (!javaTermContent.equals(newJavaTermContent)) {
    _content=_content.replace(javaTermContent,newJavaTermContent);
  }
}
