{
  int methodType=javaTerm.getType();
  if ((methodType != JavaTerm.TYPE_METHOD_PUBLIC) && (methodType != JavaTerm.TYPE_METHOD_PUBLIC_STATIC)) {
    return;
  }
  checkAnnotationForMethod(javaTerm,"After","^.*tearDown\\z",JavaTerm.TYPE_METHOD_PUBLIC,_fileName);
  checkAnnotationForMethod(javaTerm,"AfterClass","^.*tearDownClass\\z",JavaTerm.TYPE_METHOD_PUBLIC_STATIC,_fileName);
  checkAnnotationForMethod(javaTerm,"Before","^.*setUp\\z",JavaTerm.TYPE_METHOD_PUBLIC,_fileName);
  checkAnnotationForMethod(javaTerm,"BeforeClass","^.*setUpClass\\z",JavaTerm.TYPE_METHOD_PUBLIC_STATIC,_fileName);
  checkAnnotationForMethod(javaTerm,"Test","^.*test",JavaTerm.TYPE_METHOD_PUBLIC,_fileName);
}
