{
  int methodType=javaTerm.getType();
  if ((methodType != TYPE_METHOD_PUBLIC) && (methodType != TYPE_METHOD_PUBLIC_STATIC)) {
    return;
  }
  checkAnnotationForMethod(javaTerm,"After","^.*tearDown\\z",TYPE_METHOD_PUBLIC,_fileName);
  checkAnnotationForMethod(javaTerm,"AfterClass","^.*tearDownClass\\z",TYPE_METHOD_PUBLIC_STATIC,_fileName);
  checkAnnotationForMethod(javaTerm,"Before","^.*setUp\\z",TYPE_METHOD_PUBLIC,_fileName);
  checkAnnotationForMethod(javaTerm,"BeforeClass","^.*setUpClass\\z",TYPE_METHOD_PUBLIC_STATIC,_fileName);
  checkAnnotationForMethod(javaTerm,"Test","^.*test",TYPE_METHOD_PUBLIC,_fileName);
}
