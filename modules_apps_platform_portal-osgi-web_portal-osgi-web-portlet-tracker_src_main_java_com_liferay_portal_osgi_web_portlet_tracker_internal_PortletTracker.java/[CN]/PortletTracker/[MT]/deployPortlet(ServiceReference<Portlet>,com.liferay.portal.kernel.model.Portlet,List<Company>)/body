{
  List<String> categoryNames=StringPlus.asList(get(serviceReference,"display-category"));
  if (categoryNames.isEmpty()) {
    categoryNames.add("category.undefined");
  }
  String[] categoryNamesArray=ArrayUtil.toStringArray(categoryNames);
  for (  Company company : companies) {
    com.liferay.portal.kernel.model.Portlet companyPortletModel=(com.liferay.portal.kernel.model.Portlet)portletModel.clone();
    companyPortletModel.setCompanyId(company.getCompanyId());
    _portletLocalService.deployRemotePortlet(companyPortletModel,categoryNamesArray,false);
  }
}
