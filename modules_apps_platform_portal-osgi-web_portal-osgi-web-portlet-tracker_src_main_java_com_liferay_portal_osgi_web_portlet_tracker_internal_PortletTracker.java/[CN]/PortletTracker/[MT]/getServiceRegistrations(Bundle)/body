{
  ServiceRegistrations serviceRegistrations=_serviceRegistrations.get(bundle);
  if (serviceRegistrations == null) {
    serviceRegistrations=new ServiceRegistrations(bundle);
    ServiceRegistrations oldServiceRegistrations=_serviceRegistrations.putIfAbsent(bundle,serviceRegistrations);
    if (oldServiceRegistrations != null) {
      serviceRegistrations=oldServiceRegistrations;
    }
  }
  return serviceRegistrations;
}
