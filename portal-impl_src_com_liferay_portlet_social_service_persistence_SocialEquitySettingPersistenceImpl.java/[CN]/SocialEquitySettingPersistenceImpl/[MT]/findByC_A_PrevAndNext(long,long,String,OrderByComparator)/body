{
  SocialEquitySetting socialEquitySetting=findByPrimaryKey(equitySettingId);
  Session session=null;
  try {
    session=openSession();
    SocialEquitySetting[] array=new SocialEquitySettingImpl[3];
    array[0]=getByC_A_PrevAndNext(session,socialEquitySetting,classNameId,actionId,orderByComparator,true);
    array[1]=socialEquitySetting;
    array[2]=getByC_A_PrevAndNext(session,socialEquitySetting,classNameId,actionId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
