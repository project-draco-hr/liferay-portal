{
  Session session=null;
  try {
    session=openSession();
    SocialEquitySetting socialEquitySetting=(SocialEquitySetting)session.get(SocialEquitySettingImpl.class,new Long(equitySettingId));
    if (socialEquitySetting == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + equitySettingId);
      }
      throw new NoSuchEquitySettingException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + equitySettingId);
    }
    return remove(socialEquitySetting);
  }
 catch (  NoSuchEquitySettingException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
