{
  List<SocialEquitySetting> list=findByG_C_A(groupId,classNameId,actionId,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(8);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("groupId=");
    msg.append(groupId);
    msg.append(", classNameId=");
    msg.append(classNameId);
    msg.append(", actionId=");
    msg.append(actionId);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchEquitySettingException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
