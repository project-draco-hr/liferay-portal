{
  ResourceBundle resourceBundle=(ResourceBundle)_resourceBundles.get(LocaleUtil.toLanguageId(locale));
  if (resourceBundle == null) {
    resourceBundle=(ResourceBundle)_resourceBundles.get(locale.getLanguage());
    if (resourceBundle == null) {
      resourceBundle=(ResourceBundle)_resourceBundles.get(LocaleUtil.toLanguageId(LocaleUtil.getDefault()));
    }
  }
  return resourceBundle;
}
