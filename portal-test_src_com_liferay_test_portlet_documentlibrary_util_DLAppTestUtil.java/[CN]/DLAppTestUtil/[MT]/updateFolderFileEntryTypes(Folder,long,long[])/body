{
  DLFolder dlFolder=(DLFolder)folder.getModel();
  dlFolder.setDefaultFileEntryTypeId(defaultFileEntryTypeId);
  dlFolder.setOverrideFileEntryTypes(true);
  DLFolderLocalServiceUtil.updateDLFolder(dlFolder);
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext(folder.getGroupId());
  DLFileEntryTypeLocalServiceUtil.updateFolderFileEntryTypes(dlFolder,ListUtil.toList(fileEntryTypeIds),defaultFileEntryTypeId,serviceContext);
}
