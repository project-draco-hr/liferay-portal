{
  for (  Class<?> serviceClass : serviceClasses) {
    String label=TextFormatter.format(serviceClass.getSimpleName(),TextFormatter.I);
    label=TextFormatter.format(label,TextFormatter.K);
    TemplateVariableDefinition templateVariableDefinition=new TemplateVariableDefinition(label,serviceClass,"service-locator",serviceClass.getCanonicalName(),label + "-help",false,null);
    _templateVariableDefinitions.add(templateVariableDefinition);
  }
}
