{
  if (_ioException != null) {
    throw _ioException;
  }
  try {
    long receipt=(Long)ReflectionTestUtil.invoke(MailboxUtil.class,"depositMail",new Class<?>[]{ByteBuffer.class},datagram.getDataByteBuffer());
    _receiptData=new byte[8];
    BigEndianCodec.putLong(_receiptData,0,receipt);
    return Datagram.createResponseDatagram(datagram,ByteBuffer.wrap(_receiptData));
  }
 catch (  Exception e) {
    throw new IOException(e);
  }
}
