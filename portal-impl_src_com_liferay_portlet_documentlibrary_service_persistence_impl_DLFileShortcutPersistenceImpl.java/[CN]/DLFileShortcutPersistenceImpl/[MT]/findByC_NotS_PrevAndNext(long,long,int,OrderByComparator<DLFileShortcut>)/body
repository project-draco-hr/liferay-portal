{
  DLFileShortcut dlFileShortcut=findByPrimaryKey(fileShortcutId);
  Session session=null;
  try {
    session=openSession();
    DLFileShortcut[] array=new DLFileShortcutImpl[3];
    array[0]=getByC_NotS_PrevAndNext(session,dlFileShortcut,companyId,status,orderByComparator,true);
    array[1]=dlFileShortcut;
    array[2]=getByC_NotS_PrevAndNext(session,dlFileShortcut,companyId,status,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
