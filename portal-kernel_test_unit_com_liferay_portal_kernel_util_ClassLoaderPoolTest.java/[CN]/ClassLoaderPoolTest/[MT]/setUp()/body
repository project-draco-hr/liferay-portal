{
  Class<?> clazz=getClass();
  PortalClassLoaderUtil.setClassLoader(clazz.getClassLoader());
  _classLoaders=ReflectionTestUtil.getFieldValue(ClassLoaderPool.class,"_classLoaders");
  _classLoaders.clear();
  _contextNames=ReflectionTestUtil.getFieldValue(ClassLoaderPool.class,"_contextNames");
  _contextNames.clear();
}
