{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ExpandoValue.class,_dynamicQueryClassLoader);
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("valueId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("valueId",new Object[]{RandomTestUtil.nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
