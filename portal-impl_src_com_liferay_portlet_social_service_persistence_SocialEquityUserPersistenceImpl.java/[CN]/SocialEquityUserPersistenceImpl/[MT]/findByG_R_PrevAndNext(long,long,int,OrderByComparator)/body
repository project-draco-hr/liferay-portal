{
  SocialEquityUser socialEquityUser=findByPrimaryKey(equityUserId);
  Session session=null;
  try {
    session=openSession();
    SocialEquityUser[] array=new SocialEquityUserImpl[3];
    array[0]=getByG_R_PrevAndNext(session,socialEquityUser,groupId,rank,orderByComparator,true);
    array[1]=socialEquityUser;
    array[2]=getByG_R_PrevAndNext(session,socialEquityUser,groupId,rank,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
