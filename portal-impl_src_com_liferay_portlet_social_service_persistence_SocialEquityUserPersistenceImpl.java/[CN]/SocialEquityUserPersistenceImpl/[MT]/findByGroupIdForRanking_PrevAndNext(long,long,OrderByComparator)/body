{
  SocialEquityUser socialEquityUser=findByPrimaryKey(equityUserId);
  Session session=null;
  try {
    session=openSession();
    SocialEquityUser[] array=new SocialEquityUserImpl[3];
    array[0]=getByGroupIdForRanking_PrevAndNext(session,socialEquityUser,groupId,orderByComparator,true);
    array[1]=socialEquityUser;
    array[2]=getByGroupIdForRanking_PrevAndNext(session,socialEquityUser,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
