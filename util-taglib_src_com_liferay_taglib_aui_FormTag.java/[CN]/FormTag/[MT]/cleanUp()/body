{
  _action=null;
  _cssClass=null;
  _escapeXml=true;
  _inlineLabels=false;
  _name="fm";
  _onSubmit=null;
  _useNamespace=true;
  if (_validatorTagsMap != null) {
    for (    List<ValidatorTag> list : _validatorTagsMap.values()) {
      for (      ValidatorTag validatorTag : list) {
        validatorTag.cleanUp();
      }
    }
    _validatorTagsMap=new HashMap<String,List<ValidatorTag>>();
  }
}
