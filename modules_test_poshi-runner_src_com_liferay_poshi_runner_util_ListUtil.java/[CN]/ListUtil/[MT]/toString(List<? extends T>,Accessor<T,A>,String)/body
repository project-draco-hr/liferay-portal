{
  if (isEmpty(list)) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(2 * list.size() - 1);
  for (int i=0; i < list.size(); i++) {
    T bean=list.get(i);
    A attribute=accessor.get(bean);
    if (attribute != null) {
      sb.append(attribute);
    }
    if ((i + 1) != list.size()) {
      sb.append(delimiter);
    }
  }
  return sb.toString();
}
