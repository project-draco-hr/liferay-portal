{
  if (_recordSetSearchTotal != null) {
    return _recordSetSearchTotal;
  }
  _recordSetSearchTotal=_ddlRecordSetService.searchCount(_ddlFormWebRequestHelper.getCompanyId(),_ddlFormWebRequestHelper.getScopeGroupId(),getKeywords(),DDLRecordSetConstants.SCOPE_FORMS);
  return _recordSetSearchTotal;
}
