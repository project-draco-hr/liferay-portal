{
  for (  Long newFolderId : _folderIds) {
    DLFolder newFolder=DLFolderLocalServiceUtil.fetchDLFolder(newFolderId);
    DLFolder rootFolder=getProcessableRootFolder(newFolder);
    if (Validator.isNotNull(rootFolder)) {
      DLFileEntryTypeLocalServiceUtil.cascadeFileEntryTypes(rootFolder.getUserId(),rootFolder);
    }
  }
  return null;
}
