{
  Bundle bundle=bundleContext.getBundle(0);
  Class<?> clazz=bundle.getClass();
  ClassLoader classLoader=clazz.getClassLoader();
  Dictionary<String,Object> properties=new HashMapDictionary<>();
  properties.put(URLConstants.URL_HANDLER_PROTOCOL,new String[]{"webbundle"});
  bundleContext.registerService(URLStreamHandlerService.class.getName(),new WabURLStreamHandlerService(bundleContext,classLoader),properties);
}
