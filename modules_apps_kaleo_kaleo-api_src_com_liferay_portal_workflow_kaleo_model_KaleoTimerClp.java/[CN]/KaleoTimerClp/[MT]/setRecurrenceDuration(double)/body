{
  _recurrenceDuration=recurrenceDuration;
  if (_kaleoTimerRemoteModel != null) {
    try {
      Class<?> clazz=_kaleoTimerRemoteModel.getClass();
      Method method=clazz.getMethod("setRecurrenceDuration",double.class);
