{
  EntityCacheUtil.putResult(PollsQuestionModelImpl.ENTITY_CACHE_ENABLED,PollsQuestionImpl.class,pollsQuestion.getPrimaryKey(),pollsQuestion);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{pollsQuestion.getUuid(),pollsQuestion.getGroupId()},pollsQuestion);
  pollsQuestion.resetOriginalValues();
}
