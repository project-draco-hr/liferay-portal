{
  if (permissionChecker.hasOwnerPermission(categoryVocabulary.getCompanyId(),AssetCategoryVocabulary.class.getName(),categoryVocabulary.getCategoryVocabularyId(),categoryVocabulary.getUserId(),actionId)) {
    return true;
  }
  return permissionChecker.hasPermission(categoryVocabulary.getGroupId(),AssetCategoryVocabulary.class.getName(),categoryVocabulary.getCategoryVocabularyId(),actionId);
}
