{
  Team newTeam=addTeam();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Team.class,Team.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("teamId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("teamId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
