{
  try {
    com.liferay.portlet.blogs.model.BlogsEntry returnValue=BlogsEntryServiceUtil.updateEntry(entryId,categoryId,title,content,displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute);
