{
  StringBuffer sb=new StringBuffer(8);
  sb.append(dataRootDir);
  sb.append(StringPool.FORWARD_SLASH);
  sb.append(themeDisplay.getScopeGroupId());
  sb.append(StringPool.FORWARD_SLASH);
  sb.append(themeDisplay.getPlid());
  sb.append(StringPool.FORWARD_SLASH);
  sb.append(portletId);
  sb.append(".csv");
  return sb.toString();
}
