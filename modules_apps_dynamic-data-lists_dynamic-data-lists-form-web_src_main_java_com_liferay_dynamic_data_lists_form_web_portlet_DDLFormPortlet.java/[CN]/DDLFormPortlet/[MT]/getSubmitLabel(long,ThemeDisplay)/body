{
  boolean assignedToWorkflow=isAssignedToWorkflow(recordSetId,themeDisplay);
  String submitLabel=null;
  if (assignedToWorkflow) {
    submitLabel=LanguageUtil.get(themeDisplay.getRequest(),"submit-for-publication");
  }
 else {
    submitLabel=LanguageUtil.get(themeDisplay.getRequest(),"submit");
  }
  return submitLabel;
}
