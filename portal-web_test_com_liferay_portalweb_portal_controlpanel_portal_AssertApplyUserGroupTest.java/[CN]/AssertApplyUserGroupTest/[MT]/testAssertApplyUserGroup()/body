{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=User Groups")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=User Groups"));
  selenium.waitForPageToLoad("30000");
  selenium.click("//td[4]/ul/li/strong/span");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=View Users")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=View Users"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementPresent("link=selenium01"));
  assertTrue(selenium.isElementPresent("link=selenium02"));
}
