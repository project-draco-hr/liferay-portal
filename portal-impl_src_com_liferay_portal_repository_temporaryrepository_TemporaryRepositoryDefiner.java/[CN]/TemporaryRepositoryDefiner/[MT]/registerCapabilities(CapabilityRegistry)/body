{
  DocumentRepository documentRepository=capabilityRegistry.getDocumentRepository();
  if (documentRepository instanceof LocalRepository) {
    capabilityRegistry.addExportedCapability(TemporaryFilesCapability.class,new TemporaryFilesCapabilityImpl((LocalRepository)documentRepository));
    capabilityRegistry.addExportedCapability(BulkOperationCapability.class,new LiferayBulkOperationCapability(documentRepository.getRepositoryId()));
  }
}
