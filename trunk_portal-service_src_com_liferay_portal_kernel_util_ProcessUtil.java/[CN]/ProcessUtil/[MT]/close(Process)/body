{
  try {
    process.waitFor();
  }
 catch (  InterruptedException ie) {
    ie.printStackTrace();
  }
  try {
    process.getInputStream().close();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
  try {
    process.getOutputStream().close();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
  try {
    process.getErrorStream().close();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
}
