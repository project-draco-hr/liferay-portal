{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/user/joebloggs/so/dashboard/");
  selenium.waitForText("xPath=(//span[@class='portlet-title-text'])[4]","Events");
  assertEquals(RuntimeVariables.replace("Events"),selenium.getText("xPath=(//span[@class='portlet-title-text'])[4]"));
  assertEquals(RuntimeVariables.replace("There are no more events today."),selenium.getText("//div[2]/div/div[2]/div/section/div/div/div"));
  assertTrue(selenium.isElementNotPresent("//h2[contains(.,'Events')]"));
  assertTrue(selenium.isElementNotPresent("//span[@class='event-name']/a"));
  assertFalse(selenium.isTextPresent("Calendar Event Title"));
}
