{
  mdrRule=toUnwrappedModel(mdrRule);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(mdrRule)) {
      mdrRule=(MDRRule)session.get(MDRRuleImpl.class,mdrRule.getPrimaryKeyObj());
    }
    if (mdrRule != null) {
      session.delete(mdrRule);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (mdrRule != null) {
    clearCache(mdrRule);
  }
  return mdrRule;
}
