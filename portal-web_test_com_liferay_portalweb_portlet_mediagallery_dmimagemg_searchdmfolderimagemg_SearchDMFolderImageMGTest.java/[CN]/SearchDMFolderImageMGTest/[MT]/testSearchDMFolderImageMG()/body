{
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Media Gallery Test Page",RuntimeVariables.replace("Media Gallery Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_31_keywords1']",RuntimeVariables.replace("DM Folder Image Title"));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace("Search"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isVisible("//a[@class='image-thumbnail preview aui-image-viewer-link']/img"));
  assertEquals(RuntimeVariables.replace("DM Folder Image Title"),selenium.getText("//span[@class='image-title']"));
  selenium.open("/web/guest/home/");
  selenium.clickAt("link=Media Gallery Test Page",RuntimeVariables.replace("Media Gallery Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.type("//input[@id='_31_keywords1']",RuntimeVariables.replace("Image2"));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace("Search"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isElementNotPresent("//a[@class='image-thumbnail preview aui-image-viewer-link']/img"));
  assertFalse(selenium.isTextPresent("DM Folder Image Title"));
}
