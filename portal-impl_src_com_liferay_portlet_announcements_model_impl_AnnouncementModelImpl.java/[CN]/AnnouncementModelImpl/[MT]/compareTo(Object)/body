{
  if (obj == null) {
    return -1;
  }
  AnnouncementImpl announcement=(AnnouncementImpl)obj;
  int value=0;
  if (getPriority() < announcement.getPriority()) {
    value=-1;
  }
 else   if (getPriority() > announcement.getPriority()) {
    value=1;
  }
 else {
    value=0;
  }
  if (value != 0) {
    return value;
  }
  value=DateUtil.compareTo(getModifiedDate(),announcement.getModifiedDate());
  if (value != 0) {
    return value;
  }
  return 0;
}
