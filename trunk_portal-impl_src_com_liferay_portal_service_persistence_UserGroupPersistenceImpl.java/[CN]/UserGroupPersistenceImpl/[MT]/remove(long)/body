{
  Session session=null;
  try {
    session=openSession();
    UserGroup userGroup=(UserGroup)session.get(UserGroupImpl.class,Long.valueOf(userGroupId));
    if (userGroup == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + userGroupId);
      }
      throw new NoSuchUserGroupException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + userGroupId);
    }
    return userGroupPersistence.remove(userGroup);
  }
 catch (  NoSuchUserGroupException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
