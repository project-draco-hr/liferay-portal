{
  try {
    for (    com.liferay.portal.model.Group group : groups) {
      removeGroup.remove(pk,group.getPrimaryKey());
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    FinderCacheUtil.clearCache(UserGroupModelImpl.MAPPING_TABLE_GROUPS_USERGROUPS_NAME);
  }
}
