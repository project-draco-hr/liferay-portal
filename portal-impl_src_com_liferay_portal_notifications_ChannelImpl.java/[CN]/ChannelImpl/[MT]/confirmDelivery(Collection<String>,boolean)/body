{
  lock.lock();
  try {
    if (PropsValues.USER_NOTIFICATION_EVENT_CONFIRMATION_ENABLED) {
      if (archive) {
        UserNotificationEventLocalServiceUtil.updateUserNotificationEvents(notificationEventUuids,getCompanyId(),archive);
      }
 else {
        UserNotificationEventLocalServiceUtil.deleteUserNotificationEvents(notificationEventUuids,getCompanyId());
      }
    }
    for (    String notificationEventUuid : notificationEventUuids) {
      Map<String,NotificationEvent> unconfirmedNotificationEvents=_getUnconfirmedNotificationEvents();
      unconfirmedNotificationEvents.remove(notificationEventUuid);
    }
  }
 catch (  Exception e) {
    throw new ChannelException("Unable to confirm delivery for user " + getUserId(),e);
  }
 finally {
    lock.unlock();
  }
}
