{
  Set<String> notificationEventUuidsSet=new HashSet<String>(notificationEventUuids);
  _reentrantLock.lock();
  try {
    Iterator<NotificationEvent> itr=_getNotificationEvents().iterator();
    while (itr.hasNext()) {
      NotificationEvent notificationEvent=itr.next();
      if (notificationEventUuidsSet.contains(notificationEvent.getUuid())) {
        itr.remove();
      }
    }
  }
  finally {
    _reentrantLock.unlock();
  }
}
