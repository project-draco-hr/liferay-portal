{
  Lock writeLock=_readWriteLock.writeLock();
  try {
    writeLock.lock();
    Iterator<NotificationEvent> itr=_notificationEvents.iterator();
    while (itr.hasNext()) {
      NotificationEvent notificationEvent=itr.next();
      if (notificationEvent.getTimestamp() < timestamp) {
        itr.remove();
      }
    }
  }
  finally {
    writeLock.unlock();
  }
}
