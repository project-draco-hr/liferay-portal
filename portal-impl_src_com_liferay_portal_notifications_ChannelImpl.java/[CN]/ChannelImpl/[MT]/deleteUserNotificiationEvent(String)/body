{
  _reentrantLock.lock();
  try {
    Map<String,NotificationEvent> unconfirmedNotificationEvents=_getUnconfirmedNotificationEvents();
    unconfirmedNotificationEvents.remove(notificationEventUuid);
    UserNotificationEventLocalServiceUtil.deleteUserNotificationEvent(notificationEventUuid,getCompanyId());
  }
 catch (  Exception e) {
    throw new ChannelException("Unable to delete event " + notificationEventUuid,e);
  }
 finally {
    _reentrantLock.unlock();
  }
}
