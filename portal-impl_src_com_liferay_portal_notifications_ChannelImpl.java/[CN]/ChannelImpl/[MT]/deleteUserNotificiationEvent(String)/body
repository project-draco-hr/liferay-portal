{
  _reentrantLock.lock();
  try {
    UserNotificationEventLocalServiceUtil.deleteUserNotificationEvent(notificationEventUuid);
  }
 catch (  Exception e) {
    throw new ChannelException("Uanble to delete Notification for " + notificationEventUuid,e);
  }
 finally {
    _reentrantLock.unlock();
  }
}
