{
  Lock writeLock=_readWriteLock.writeLock();
  try {
    writeLock.lock();
    long currentTime=System.currentTimeMillis();
    List<NotificationEvent> persistedNotificationEvents=new ArrayList<NotificationEvent>(notificationEvents.size());
    for (    NotificationEvent notificationEvent : notificationEvents) {
      storeNotificationEvent(notificationEvent,currentTime);
      if (notificationEvent.isDeliveryRequired()) {
        persistedNotificationEvents.add(notificationEvent);
      }
    }
    UserNotificationEventLocalServiceUtil.addUserNotificationEvents(getUserId(),persistedNotificationEvents);
  }
 catch (  Exception e) {
    throw new ChannelException("Unable to send event",e);
  }
 finally {
    writeLock.unlock();
  }
  notifyChannelListeners();
}
