{
  Boolean hasPermission=StagingPermissionUtil.hasPermission(permissionChecker,groupId,_CLASS_NAME,groupId,PortletKeys.IMAGE_GALLERY,actionId);
  if (hasPermission != null) {
    return hasPermission.booleanValue();
  }
  return permissionChecker.hasPermission(groupId,_CLASS_NAME,groupId,actionId);
}
