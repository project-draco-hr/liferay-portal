{
  User user=fetchByC_U(companyId,userId);
  if (user == null) {
    String msg="No User exists with the key ";
    msg+=StringPool.OPEN_CURLY_BRACE;
    msg+="companyId=";
    msg+=companyId;
    msg+=", ";
    msg+="userId=";
    msg+=userId;
    msg+=StringPool.CLOSE_CURLY_BRACE;
    if (_log.isWarnEnabled()) {
      _log.warn(msg);
    }
    throw new NoSuchUserException(msg);
  }
  return user;
}
