{
  BTEntry btEntry=findByPrimaryKey(btEntryId);
  Session session=null;
  try {
    session=openSession();
    BTEntry[] array=new BTEntryImpl[3];
    array[0]=getByG_S_T_PrevAndNext(session,btEntry,groupId,status,taskExecutorClassName,orderByComparator,true);
    array[1]=btEntry;
    array[2]=getByG_S_T_PrevAndNext(session,btEntry,groupId,status,taskExecutorClassName,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
