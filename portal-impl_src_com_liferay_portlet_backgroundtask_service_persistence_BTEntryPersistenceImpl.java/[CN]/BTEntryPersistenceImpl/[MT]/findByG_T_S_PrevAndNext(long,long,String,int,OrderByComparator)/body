{
  BTEntry btEntry=findByPrimaryKey(btEntryId);
  Session session=null;
  try {
    session=openSession();
    BTEntry[] array=new BTEntryImpl[3];
    array[0]=getByG_T_S_PrevAndNext(session,btEntry,groupId,taskExecutorClassName,status,orderByComparator,true);
    array[1]=btEntry;
    array[2]=getByG_T_S_PrevAndNext(session,btEntry,groupId,taskExecutorClassName,status,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
