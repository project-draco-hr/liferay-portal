{
  if (!passwordPolicy.isDefaultPolicy()) {
    return;
  }
  try {
    List<SACPEntry> sacpEntries=_sacpEntryLocalService.getCompanySACPEntries(passwordPolicy.getCompanyId(),QueryUtil.ALL_POS,QueryUtil.ALL_POS);
    for (    SACPEntry sacpEntry : sacpEntries) {
      _sacpEntryLocalService.deleteSACPEntry(sacpEntry);
    }
  }
 catch (  PortalException pe) {
    throw new ModelListenerException(pe);
  }
}
