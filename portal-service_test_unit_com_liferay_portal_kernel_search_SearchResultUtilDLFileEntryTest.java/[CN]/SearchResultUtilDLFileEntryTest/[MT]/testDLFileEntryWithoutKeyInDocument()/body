{
  SearchResult searchResult=searchSingleDocument(createDLFileEntryDocument());
  Assert.assertEquals(DLFILEENTRY_CLASS_NAME,searchResult.getClassName());
  Assert.assertEquals(ENTRY_CLASS_PK,searchResult.getClassPK());
  Assert.assertThat(searchResult.getFileEntryTuples(),IsEmptyCollection.empty());
  Assert.assertNull(searchResult.getSummary());
  Assert.assertThat("DLAppLocalService must not be invoked at all",searchResult.getFileEntryTuples(),IsEmptyCollection.empty());
  verifyZeroInteractions(dlAppLocalService);
  assertThatEverythingUnrelatedIsEmpty(searchResult);
}
