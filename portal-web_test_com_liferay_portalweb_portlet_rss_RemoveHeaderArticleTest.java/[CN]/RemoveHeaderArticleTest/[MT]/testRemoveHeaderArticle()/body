      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//td[1]/div/div/div/div[1]/span")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertFalse(selenium.isTextPresent("This is a header article."));
}
