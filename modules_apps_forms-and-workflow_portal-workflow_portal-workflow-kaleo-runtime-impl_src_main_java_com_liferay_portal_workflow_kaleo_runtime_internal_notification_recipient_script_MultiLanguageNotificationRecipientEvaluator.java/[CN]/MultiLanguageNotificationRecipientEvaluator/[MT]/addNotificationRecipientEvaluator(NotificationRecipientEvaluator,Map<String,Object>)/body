{
  String[] scriptingLanguages=_getScriptingLanguages(notificationRecipientEvaluator,properties);
  for (  String scriptingLanguage : scriptingLanguages) {
    ScriptLanguage scriptLanguage=ScriptLanguage.parse(scriptingLanguage);
    _notificationRecipientEvaluators.put(scriptLanguage,notificationRecipientEvaluator);
  }
}
