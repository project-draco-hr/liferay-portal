{
  AuditMessage auditMessage=new AuditMessage(SchedulerEngine.AUDIT_ACTION,CompanyConstants.SYSTEM,0,"",SchedulerEngine.class.getName(),"0",jobStatus,new Date(),JSONFactoryUtil.createJSONObject(JSONFactoryUtil.serialize(message)));
  auditMessage.setServerName(InetAddressUtil.getLocalInetAddressHostName());
  auditMessage.setServerPort(PortalUtil.getPortalPort(false));
  AuditRouterUtil.route(auditMessage);
}
