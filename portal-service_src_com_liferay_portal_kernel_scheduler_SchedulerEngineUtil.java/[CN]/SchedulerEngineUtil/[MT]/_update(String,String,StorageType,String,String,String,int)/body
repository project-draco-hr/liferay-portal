{
  SchedulerResponse schedulerResponse=_getScheduledJob(jobName,groupName,storageType);
  if (schedulerResponse == null) {
    return;
  }
  Trigger trigger=schedulerResponse.getTrigger();
  if (trigger == null) {
    return;
  }
  Message message=schedulerResponse.getMessage();
  if (message == null) {
    return;
  }
  _addScriptingJob(trigger,storageType,description,language,script,exceptionsMaxSize);
}
