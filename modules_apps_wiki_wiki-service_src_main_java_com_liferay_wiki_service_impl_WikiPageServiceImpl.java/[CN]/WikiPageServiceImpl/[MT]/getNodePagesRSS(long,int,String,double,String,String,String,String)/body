{
  WikiNodePermissionChecker.check(getPermissionChecker(),nodeId,ActionKeys.VIEW);
  WikiNode node=wikiNodePersistence.findByPrimaryKey(nodeId);
  List<WikiPage> pages=getNodePages(nodeId,max);
  return exportToRSS(node.getName(),node.getDescription(),type,version,displayStyle,feedURL,entryURL,attachmentURLPrefix,pages,false,null);
}
