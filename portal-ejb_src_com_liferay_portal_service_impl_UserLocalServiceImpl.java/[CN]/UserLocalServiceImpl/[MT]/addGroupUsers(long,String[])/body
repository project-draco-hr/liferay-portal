{
  GroupUtil.addUsers(groupId,userIds);
  Group group=GroupLocalServiceUtil.getGroup(groupId);
  Role communityAdministratorRole=RoleLocalServiceUtil.getRole(group.getCompanyId(),RoleImpl.COMMUNITY_MEMBER);
  for (int i=0; i < userIds.length; i++) {
    String userId=userIds[i];
    UserGroupRoleLocalServiceUtil.addUserGroupRoles(userId,groupId,new String[]{communityAdministratorRole.getRoleId()});
  }
}
