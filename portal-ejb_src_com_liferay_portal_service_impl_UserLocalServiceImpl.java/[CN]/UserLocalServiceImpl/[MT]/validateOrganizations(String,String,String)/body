{
  if (Validator.isNull(organizationId) && Validator.isNull(locationId) && !GetterUtil.getBoolean(PropsUtil.get(PropsUtil.ORGANIZATIONS_REQUIRED))) {
    return;
  }
  Organization organization=OrganizationUtil.findByPrimaryKey(organizationId);
  Organization location=OrganizationUtil.findByPrimaryKey(locationId);
  if (!location.getParentOrganizationId().equals(organization.getOrganizationId())) {
    throw new OrganizationParentException();
  }
}
