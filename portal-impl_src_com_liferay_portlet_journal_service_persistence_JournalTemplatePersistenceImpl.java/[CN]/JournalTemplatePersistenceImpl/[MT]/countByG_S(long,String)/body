{
  Object[] finderArgs=new Object[]{new Long(groupId),structureId};
  Long count=(Long)FinderCacheUtil.getResult(FINDER_PATH_COUNT_BY_G_S,finderArgs,this);
  if (count == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT COUNT(*) ");
      query.append("FROM com.liferay.portlet.journal.model.JournalTemplate WHERE ");
      query.append("groupId = ?");
      query.append(" AND ");
      if (structureId == null) {
        query.append("structureId IS NULL");
      }
 else {
        query.append("structureId = ?");
      }
      query.append(" ");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      if (structureId != null) {
        qPos.add(structureId);
      }
      count=(Long)q.uniqueResult();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (count == null) {
        count=Long.valueOf(0);
      }
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_G_S,finderArgs,count);
      closeSession(session);
    }
  }
  return count.intValue();
}
