{
  Session session=null;
  try {
    session=openSession();
    session.delete(journalTemplate);
    session.flush();
    return journalTemplate;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(JournalTemplate.class.getName());
  }
}
