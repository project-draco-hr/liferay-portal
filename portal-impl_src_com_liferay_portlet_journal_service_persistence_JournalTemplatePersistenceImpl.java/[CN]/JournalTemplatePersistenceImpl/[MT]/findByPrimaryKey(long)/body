{
  JournalTemplate journalTemplate=fetchByPrimaryKey(id);
  if (journalTemplate == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No JournalTemplate exists with the primary key " + id);
    }
    throw new NoSuchTemplateException("No JournalTemplate exists with the primary key " + id);
  }
  return journalTemplate;
}
