{
  Session session=null;
  try {
    session=openSession();
    JournalTemplate journalTemplate=(JournalTemplate)session.get(JournalTemplateImpl.class,new Long(id));
    if (journalTemplate == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + id);
      }
      throw new NoSuchTemplateException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + id);
    }
    return journalTemplatePersistence.remove(journalTemplate);
  }
 catch (  NoSuchTemplateException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
