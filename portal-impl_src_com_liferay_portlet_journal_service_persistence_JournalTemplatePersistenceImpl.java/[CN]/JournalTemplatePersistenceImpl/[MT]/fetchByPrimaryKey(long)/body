{
  JournalTemplate result=(JournalTemplate)EntityCacheUtil.getResult(JournalTemplateModelImpl.ENTITY_CACHE_ENABLED,JournalTemplate.class,id,this);
  if (result == null) {
    Session session=null;
    try {
      session=openSession();
      JournalTemplate journalTemplate=(JournalTemplate)session.get(JournalTemplateImpl.class,new Long(id));
      if (journalTemplate != null) {
        cacheResult(journalTemplate);
      }
      return journalTemplate;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
 else {
    return (JournalTemplate)result;
  }
}
