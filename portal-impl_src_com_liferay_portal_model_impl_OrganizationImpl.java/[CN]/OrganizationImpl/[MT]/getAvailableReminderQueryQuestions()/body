{
  Set<String> questions=new TreeSet<String>();
  PortletPreferences preferences=PrefsPropsUtil.getOrganizationPreferences(getOrganizationId());
  String[] questionsArray=StringUtil.split(preferences.getValue("reminderQueries",null),StringPool.NEW_LINE);
  questions.addAll(SetUtil.fromArray(questionsArray));
  return questions;
}
