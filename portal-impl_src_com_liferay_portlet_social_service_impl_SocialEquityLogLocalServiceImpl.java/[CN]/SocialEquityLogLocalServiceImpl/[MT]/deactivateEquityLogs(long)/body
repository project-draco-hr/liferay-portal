{
  List<SocialEquityLog> equityLogs=socialEquityLogPersistence.findByAEI_T_A(assetEntryId,SocialEquitySettingConstants.TYPE_INFORMATION,true);
  SocialEquityAssetEntry socialEquityAssetEntry=socialEquityAssetEntryPersistence.findByAssetEntryId(assetEntryId);
  socialEquityAssetEntryPersistence.removeByAssetEntryId(assetEntryId);
  User user=null;
  try {
    user=userPersistence.findByPrimaryKey(socialEquityAssetEntry.getUserId());
    if (!user.isDefaultUser()) {
      updateSocialEquityUser_CQ(socialEquityAssetEntry.getGroupId(),user.getUserId());
      updateSocialEquityUser_PEQ(socialEquityAssetEntry.getGroupId(),user.getUserId());
      updateUser_CQPQ(user.getUserId());
      updateUser_PEQ(user.getUserId());
      userPersistence.clearCache(user);
    }
  }
 catch (  NoSuchUserException nsue) {
  }
  for (  SocialEquityLog equityLog : equityLogs) {
    equityLog.setActive(false);
    socialEquityLogPersistence.update(equityLog,false);
  }
}
