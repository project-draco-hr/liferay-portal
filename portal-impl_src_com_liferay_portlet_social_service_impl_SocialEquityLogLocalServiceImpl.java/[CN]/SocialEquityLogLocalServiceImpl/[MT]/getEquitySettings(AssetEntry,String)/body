{
  List<SocialEquitySetting> equitySettings=new ArrayList<SocialEquitySetting>(socialEquitySettingPersistence.findByC_A(assetEntry.getClassNameId(),actionId));
  SocialEquityActionMapping defaultEquitySettings=ResourceActionsUtil.getSocialEquityActionMapping(assetEntry.getClassName(),actionId);
  if (defaultEquitySettings != null) {
    if (equitySettings.size() == 0) {
      if (defaultEquitySettings.getInformationValue() > 0) {
        equitySettings.add(new SocialEquitySettingImpl(actionId,SocialEquitySettingConstants.TYPE_INFORMATION,defaultEquitySettings.getInformationValue(),defaultEquitySettings.getInformationLifespan()));
      }
      if (defaultEquitySettings.getParticipationValue() > 0) {
        equitySettings.add(new SocialEquitySettingImpl(actionId,SocialEquitySettingConstants.TYPE_PARTICIPATION,defaultEquitySettings.getParticipationValue(),defaultEquitySettings.getParticipationLifespan()));
      }
    }
 else     if (equitySettings.size() == 1) {
      if (defaultEquitySettings.getInformationValue() > 0 && equitySettings.get(0).getType() == SocialEquitySettingConstants.TYPE_PARTICIPATION) {
        equitySettings.add(new SocialEquitySettingImpl(actionId,SocialEquitySettingConstants.TYPE_INFORMATION,defaultEquitySettings.getInformationValue(),defaultEquitySettings.getInformationLifespan()));
      }
 else       if (defaultEquitySettings.getParticipationValue() > 0 && equitySettings.get(0).getType() == SocialEquitySettingConstants.TYPE_INFORMATION) {
        equitySettings.add(new SocialEquitySettingImpl(actionId,SocialEquitySettingConstants.TYPE_PARTICIPATION,defaultEquitySettings.getParticipationValue(),defaultEquitySettings.getParticipationLifespan()));
      }
    }
  }
  return equitySettings;
}
