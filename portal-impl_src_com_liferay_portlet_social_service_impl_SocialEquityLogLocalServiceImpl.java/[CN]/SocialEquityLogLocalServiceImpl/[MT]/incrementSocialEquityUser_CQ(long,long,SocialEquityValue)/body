{
  User user=userLocalService.getUser(userId);
  int count=socialEquityUserPersistence.countByG_U(groupId,userId);
  if (count == 0) {
    addSocialEquityUser(groupId,user);
  }
  user.updateSocialContributionEquity(socialEquityValue.getValue());
  String sql=CustomSQLUtil.get(_UPDATE_SOCIAL_EQUITY_USER_CQ);
  sql=StringUtil.replace(sql,new String[]{"[$CONTRIBUTION_B$]","[$CONTRIBUTION_K$]","[$GROUP_ID$]","[$USER_ID$]"},new String[]{String.valueOf(socialEquityValue.getB()),String.valueOf(socialEquityValue.getK()),String.valueOf(groupId),String.valueOf(userId)});
  runSQL(sql);
}
