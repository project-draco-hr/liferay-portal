{
  AssetEntry assetEntry=assetEntryPersistence.fetchByPrimaryKey(assetEntryId);
  assetEntry.setSocialInformationEquity(calculateEquity(getEquityDate(),k,b));
  String sql=CustomSQLUtil.get(_UPDATE_SOCIAL_EQUITY_ASSET_ENTRY_IQ);
  sql=StringUtil.replace(sql,new String[]{"[$ASSET_ENTRY_ID$]","[$INFORMATION_B$]","[$INFORMATION_K$]"},new String[]{String.valueOf(assetEntryId),String.valueOf(b),String.valueOf(k)});
  runSQL(sql);
}
