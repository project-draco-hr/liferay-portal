{
  if (!PropsValues.SOCIAL_EQUITY_EQUITY_LOG_ENABLED) {
    return;
  }
  AssetEntry assetEntry=null;
  try {
    assetEntry=assetEntryLocalService.getEntry(className,classPK);
  }
 catch (  NoSuchEntryException nsee) {
    return;
  }
  deactivateEquityLogs(userId,assetEntry.getEntryId(),actionId,extraData);
}
