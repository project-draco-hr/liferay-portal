{
  String sql=CustomSQLUtil.get(_UPDATE_SOCIAL_EQUITY_USER_PQ);
  sql=StringUtil.replace(sql,new String[]{"[$ACTIVITY_DATE$]","[$GROUP_ID$]","[$PARTICIPATION_B$]","[$PARTICIPATION_K$]","[$USER_ID$]"},new String[]{String.valueOf(activityDate),String.valueOf(groupId),String.valueOf(b),String.valueOf(k),String.valueOf(userId)});
  runSQL(sql);
}
