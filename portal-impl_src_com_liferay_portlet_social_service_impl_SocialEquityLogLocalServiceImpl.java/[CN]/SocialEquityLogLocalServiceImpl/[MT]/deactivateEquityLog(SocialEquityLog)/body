{
  if (equityLog.getType() == SocialEquitySettingConstants.TYPE_INFORMATION) {
    User user=null;
    try {
      user=userPersistence.findByPrimaryKey(equityLog.getUserId());
    }
 catch (    NoSuchUserException nsue) {
    }
    if ((user == null) || !user.isDefaultUser()) {
      socialEquityAssetEntryPersistence.removeByAssetEntryId(equityLog.getAssetEntryId());
      updateSocialEquityUser_CQ(equityLog.getGroupId(),equityLog.getUserId());
      updateUser(equityLog.getUserId());
      if (user != null) {
        userPersistence.clearCache(user);
      }
    }
  }
  equityLog.setActive(false);
  socialEquityLogPersistence.update(equityLog,false);
}
