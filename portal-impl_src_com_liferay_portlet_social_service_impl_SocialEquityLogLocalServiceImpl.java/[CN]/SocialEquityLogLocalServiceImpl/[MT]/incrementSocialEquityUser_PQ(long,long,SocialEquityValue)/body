{
  User user=userLocalService.getUser(userId);
  int count=socialEquityUserPersistence.countByG_U(groupId,userId);
  if (count == 0) {
    try {
      socialEquityLogLocalService.addSocialEquityUser(groupId,user);
    }
 catch (    SystemException se) {
    }
  }
  user.updateSocialParticipationEquity(groupId,socialEquityValue.getValue());
  String sql=CustomSQLUtil.get(_UPDATE_SOCIAL_EQUITY_USER_PQ);
  sql=StringUtil.replace(sql,new String[]{"[$GROUP_ID$]","[$PARTICIPATION_B$]","[$PARTICIPATION_K$]","[$USER_ID$]"},new String[]{String.valueOf(groupId),String.valueOf(socialEquityValue.getB()),String.valueOf(socialEquityValue.getK()),String.valueOf(userId)});
  runSQL(sql);
}
