{
  long classNameId=PortalUtil.getClassNameId(Role.class.getName());
  List<Map<String,Object>> companyMaps=getMaps("select * from Company");
  for (  Map<String,Object> companyMap : companyMaps) {
    long companyId=(Long)companyMap.get("companyId");
    addRole(companyId,classNameId,_ROLE_COMMUNITY_CONTENT_REVIEWER,RoleConstants.TYPE_COMMUNITY);
    addRole(companyId,classNameId,_ROLE_ORGANIZATION_CONTENT_REVIEWER,RoleConstants.TYPE_ORGANIZATION);
    addRole(companyId,classNameId,_ROLE_PORTAL_CONTENT_REVIEWER,RoleConstants.TYPE_REGULAR);
  }
}
