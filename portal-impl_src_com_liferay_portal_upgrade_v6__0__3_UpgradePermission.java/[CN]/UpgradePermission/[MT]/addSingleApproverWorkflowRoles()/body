{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select * from Company");
    rs=ps.executeQuery();
    while (rs.next()) {
      long companyId=rs.getLong("companyId");
      long classNameId=PortalUtil.getClassNameId(Role.class.getName());
      long roleId=increment();
      addRole(roleId,companyId,classNameId,roleId,_ROLE_COMMUNITY_CONTENT_REVIEWER,RoleConstants.TYPE_COMMUNITY);
      classNameId=PortalUtil.getClassNameId(Organization.class.getName());
      roleId=increment();
      addRole(roleId,companyId,classNameId,roleId,_ROLE_ORGANIZATION_CONTENT_REVIEWER,RoleConstants.TYPE_ORGANIZATION);
      classNameId=PortalUtil.getClassNameId(Company.class.getName());
      roleId=increment();
      addRole(roleId,companyId,classNameId,roleId,_ROLE_PORTAL_CONTENT_REVIEWER,RoleConstants.TYPE_REGULAR);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
