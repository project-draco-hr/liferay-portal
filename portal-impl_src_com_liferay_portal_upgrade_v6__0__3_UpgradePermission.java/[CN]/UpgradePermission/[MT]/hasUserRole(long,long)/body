{
  try (PreparedStatement ps=connection.prepareStatement("select count(*) from Users_Roles where userId = ? and " + "roleId = ?")){
    ps.setLong(1,userId);
    ps.setLong(2,roleId);
    try (ResultSet rs=ps.executeQuery()){
      if (rs.next()) {
        int count=rs.getInt(1);
        if (count > 0) {
          return true;
        }
      }
      return false;
    }
   }
 }
