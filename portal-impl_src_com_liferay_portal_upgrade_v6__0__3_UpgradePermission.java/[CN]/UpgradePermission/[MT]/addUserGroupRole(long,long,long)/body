{
  if (hasUserGroupRole(userId,groupId,roleId)) {
    return;
  }
  try (PreparedStatement ps=connection.prepareStatement("insert into UserGroupRole (userId, groupId, roleId) values " + "(?, ?, ?)")){
    ps.setLong(1,userId);
    ps.setLong(2,groupId);
    ps.setLong(3,roleId);
    ps.executeUpdate();
  }
 }
