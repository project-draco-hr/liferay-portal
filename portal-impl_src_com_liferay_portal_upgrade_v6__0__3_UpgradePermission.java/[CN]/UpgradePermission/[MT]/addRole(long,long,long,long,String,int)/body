{
  PreparedStatement ps=null;
  try {
    ps=connection.prepareStatement("insert into Role_ (roleId, companyId, classNameId, classPK, " + "name, type_) values (?, ?, ?, ?, ?, ?)");
    ps.setLong(1,roleId);
    ps.setLong(2,companyId);
    ps.setLong(3,classNameId);
    ps.setLong(4,classPK);
    ps.setString(5,name);
    ps.setInt(6,type);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(ps);
  }
}
