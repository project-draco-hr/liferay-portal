{
  if (hasUserRole(userId,roleId)) {
    return;
  }
  PreparedStatement ps=null;
  try {
    ps=connection.prepareStatement("insert into Users_Roles (userId, roleId) values (?, ?)");
    ps.setLong(1,userId);
    ps.setLong(2,roleId);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(ps);
  }
}
