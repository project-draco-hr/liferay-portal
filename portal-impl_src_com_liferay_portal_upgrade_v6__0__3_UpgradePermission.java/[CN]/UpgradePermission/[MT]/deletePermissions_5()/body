{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("delete from Roles_Permissions where permissionId in (" + "select permissionId from Permission_ where " + "actionId = 'APPROVE_ARTICLE')");
    ps.executeUpdate();
    ps=con.prepareStatement("delete from Permission_ where actionId = 'APPROVE_ARTICLE'");
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
