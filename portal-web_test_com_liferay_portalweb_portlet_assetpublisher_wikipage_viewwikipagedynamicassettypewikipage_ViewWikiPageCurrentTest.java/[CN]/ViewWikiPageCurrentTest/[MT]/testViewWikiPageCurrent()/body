{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Asset Publisher Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Asset Publisher Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("FrontPage"),selenium.getText("//h3[@class='asset-title']/a"));
  assertEquals(RuntimeVariables.replace("AP Wiki Page Body"),selenium.getText("//div[@class='asset-summary']"));
}
