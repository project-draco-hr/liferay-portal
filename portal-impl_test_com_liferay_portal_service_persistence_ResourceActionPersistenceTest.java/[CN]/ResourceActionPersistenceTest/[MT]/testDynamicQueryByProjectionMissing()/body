{
  ResourceAction newResourceAction=addResourceAction();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(ResourceAction.class,ResourceAction.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("resourceActionId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("resourceActionId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
