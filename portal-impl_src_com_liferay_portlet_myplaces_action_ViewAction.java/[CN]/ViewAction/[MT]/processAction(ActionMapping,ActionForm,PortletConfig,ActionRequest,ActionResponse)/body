{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long groupId=ParamUtil.getLong(actionRequest,"groupId");
  String privateLayoutParam=actionRequest.getParameter("privateLayout");
  String redirect=getRedirect(themeDisplay,groupId,privateLayoutParam);
  if (redirect == null) {
    redirect=ParamUtil.getString(actionRequest,"redirect");
    SessionErrors.add(actionRequest,NoSuchGroupException.class.getName(),new NoSuchGroupException("groupId=" + groupId));
  }
  actionResponse.sendRedirect(redirect);
}
