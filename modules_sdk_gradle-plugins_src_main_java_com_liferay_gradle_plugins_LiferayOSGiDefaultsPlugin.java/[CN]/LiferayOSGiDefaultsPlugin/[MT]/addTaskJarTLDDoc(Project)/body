{
  Jar jar=GradleUtil.addTask(project,JAR_TLDDOC_TASK_NAME,Jar.class);
  jar.setClassifier("taglibdoc");
  jar.setDescription("Assembles a jar archive containing the Tag Library " + "Documentation files for this project.");
  jar.setGroup(BasePlugin.BUILD_GROUP);
  TLDDocTask tlddocTask=(TLDDocTask)GradleUtil.getTask(project,TLDDocBuilderPlugin.TLDDOC_TASK_NAME);
  jar.from(tlddocTask);
  return jar;
}
