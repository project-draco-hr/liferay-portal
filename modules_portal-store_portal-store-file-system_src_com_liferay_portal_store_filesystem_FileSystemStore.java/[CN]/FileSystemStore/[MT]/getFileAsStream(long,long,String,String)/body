{
  if (Validator.isNull(versionLabel)) {
    versionLabel=getHeadVersionLabel(companyId,repositoryId,fileName);
  }
  File fileNameVersionFile=getFileNameVersionFile(companyId,repositoryId,fileName,versionLabel);
  try {
    return new FileInputStream(fileNameVersionFile);
  }
 catch (  FileNotFoundException fnfe) {
    throw new NoSuchFileException(companyId,repositoryId,fileName,fnfe);
  }
}
