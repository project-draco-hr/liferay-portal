{
  super.setAttributes(request);
  setNamespacedAttribute(request,"ddmForm",getDDMForm());
  setNamespacedAttribute(request,"ddmFormValuesInputName",getDDMFormValuesInputName());
  setNamespacedAttribute(request,"fields",getFields());
  setNamespacedAttribute(request,"mode",getMode());
  setNamespacedAttribute(request,"randomNamespace",getRandomNamespace());
}
