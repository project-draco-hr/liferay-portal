{
  SyncDLFileVersionDiff syncDLFileVersionDiff=findByPrimaryKey(syncDLFileVersionDiffId);
  Session session=null;
  try {
    session=openSession();
    SyncDLFileVersionDiff[] array=new SyncDLFileVersionDiffImpl[3];
    array[0]=getByExpirationDate_PrevAndNext(session,syncDLFileVersionDiff,expirationDate,orderByComparator,true);
    array[1]=syncDLFileVersionDiff;
    array[2]=getByExpirationDate_PrevAndNext(session,syncDLFileVersionDiff,expirationDate,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
