{
  List<BreadcrumbEntry> breadcrumbEntries=BreadcrumbUtil.getParentGroupBreadcrumbEntries(themeDisplay);
  for (  BreadcrumbEntry breadcrumbEntry : breadcrumbEntries) {
    sb.append("<li><a href=\"");
    sb.append(breadcrumbEntry.getURL());
    sb.append("\">");
    sb.append(HtmlUtil.escape(breadcrumbEntry.getTitle()));
    sb.append("</a></li>");
  }
}
