{
  List<BreadcrumbEntry> parentGroupBreadcrumbEntries=BreadcrumbUtil.getParentGroupBreadcrumbEntries(themeDisplay);
  for (  BreadcrumbEntry parentGroupBreadcrumbEntry : parentGroupBreadcrumbEntries) {
    sb.append("<li><a href=\"");
    sb.append(parentGroupBreadcrumbEntry.getURL());
    sb.append("\">");
    sb.append(HtmlUtil.escape(parentGroupBreadcrumbEntry.getTitle()));
    sb.append("</a><span class=\"divider\">/</span></li>");
  }
}
