{
  initShowParentGroups(request);
  request.setAttribute("liferay-ui:breadcrumb:breadcrumbEntries",getBreadcrumbEntries(request));
  request.setAttribute("liferay-ui:breadcrumb:displayStyle",getDisplayStyle(request));
  request.setAttribute("liferay-ui:breadcrumb:displayStyleGroupId",getDisplayStyleGroupId(request));
}
