{
  if (Validator.isNotNull(breadcrumbString)) {
    int x=breadcrumbString.indexOf("<li") + 3;
    int y=breadcrumbString.lastIndexOf("<li") + 3;
    if (x == y) {
      breadcrumbString=StringUtil.insert(breadcrumbString," class=\"active only\"",x);
    }
 else {
      breadcrumbString=StringUtil.insert(breadcrumbString," class=\"active last\"",y);
      breadcrumbString=StringUtil.insert(breadcrumbString," class=\"first\"",x);
    }
  }
  return breadcrumbString;
}
