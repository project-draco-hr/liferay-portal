{
  return new ValueAccessor(locale){
    @Override public String get(    DDMFormFieldValue ddmFormFieldValue){
      Value value=ddmFormFieldValue.getValue();
      return StringUtil.replace(_HTML,new String[]{"[$DDM_FORM_FIELD_VALUE$]","[$PREVIEW$]"},new String[]{HtmlUtil.escapeJS(value.getString(locale)),LanguageUtil.get(locale,"preview")});
    }
  }
;
}
