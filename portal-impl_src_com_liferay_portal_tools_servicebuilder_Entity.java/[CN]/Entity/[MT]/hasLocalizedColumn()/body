{
  for (  EntityColumn column : _columnList) {
    Map<String,String> hints=ModelHintsUtil.getHints(_packagePath + ".model." + _name,column.getName());
    if (hints != null) {
      boolean localized=GetterUtil.getBoolean(hints.get("localized"));
      if (localized) {
        return true;
      }
    }
  }
  return false;
}
