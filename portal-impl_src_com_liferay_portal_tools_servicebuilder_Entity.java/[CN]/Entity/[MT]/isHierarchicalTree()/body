{
  if (!hasPrimitivePK()) {
    return false;
  }
  EntityColumn col=_pkList.get(0);
  if ((_columnList.indexOf(new EntityColumn("parent" + col.getMethodName())) != -1) && (_columnList.indexOf(new EntityColumn("left" + col.getMethodName())) != -1) && (_columnList.indexOf(new EntityColumn("right" + col.getMethodName())) != -1)) {
    return true;
  }
 else {
    return false;
  }
}
