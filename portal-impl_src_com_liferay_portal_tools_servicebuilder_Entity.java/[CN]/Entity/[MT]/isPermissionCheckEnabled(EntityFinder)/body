{
  if (_name.equals("Group") || _name.equals("User") || finder.getName().equals("UUID_G")|| !hasPrimitivePK()|| !hasColumn("userId")|| !ResourceActionsUtil.hasModelResourceActions(_packagePath + ".model." + _name)) {
    return false;
  }
  if (hasColumn("groupId") && !finder.hasColumn("groupId")) {
    return false;
  }
  return true;
}
