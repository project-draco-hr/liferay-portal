{
  if (key == null) {
    throw new IllegalArgumentException();
  }
  Preference preference=getPreferences().get(key);
  String[] values=null;
  if (preference != null) {
    values=preference.getValues();
  }
  if ((values != null) && (values.length > 0)) {
    return getActualValue(values[0]);
  }
 else {
    return getActualValue(def);
  }
}
