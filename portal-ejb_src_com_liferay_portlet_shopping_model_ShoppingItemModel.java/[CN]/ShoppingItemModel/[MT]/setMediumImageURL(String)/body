{
  if (((mediumImageURL == null) && (_mediumImageURL != null)) || ((mediumImageURL != null) && (_mediumImageURL == null)) || ((mediumImageURL != null) && (_mediumImageURL != null) && !mediumImageURL.equals(_mediumImageURL))) {
    if (!XSS_ALLOW_MEDIUMIMAGEURL) {
      mediumImageURL=XSSUtil.strip(mediumImageURL);
    }
