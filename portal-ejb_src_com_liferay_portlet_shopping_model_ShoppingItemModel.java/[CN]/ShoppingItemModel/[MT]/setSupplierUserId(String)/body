{
  if (((supplierUserId == null) && (_supplierUserId != null)) || ((supplierUserId != null) && (_supplierUserId == null)) || ((supplierUserId != null) && (_supplierUserId != null) && !supplierUserId.equals(_supplierUserId))) {
    if (!XSS_ALLOW_SUPPLIERUSERID) {
      supplierUserId=XSSUtil.strip(supplierUserId);
    }
    _supplierUserId=supplierUserId;
    setModified(true);
  }
}
