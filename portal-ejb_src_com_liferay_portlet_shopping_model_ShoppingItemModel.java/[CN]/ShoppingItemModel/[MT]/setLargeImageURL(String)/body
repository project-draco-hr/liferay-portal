{
  if (((largeImageURL == null) && (_largeImageURL != null)) || ((largeImageURL != null) && (_largeImageURL == null)) || ((largeImageURL != null) && (_largeImageURL != null) && !largeImageURL.equals(_largeImageURL))) {
    if (!XSS_ALLOW_LARGEIMAGEURL) {
      largeImageURL=XSSUtil.strip(largeImageURL);
    }
    _largeImageURL=largeImageURL;
    setModified(true);
  }
}
