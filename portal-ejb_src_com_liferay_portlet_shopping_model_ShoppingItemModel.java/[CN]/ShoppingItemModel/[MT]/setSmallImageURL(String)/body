{
  if (((smallImageURL == null) && (_smallImageURL != null)) || ((smallImageURL != null) && (_smallImageURL == null)) || ((smallImageURL != null) && (_smallImageURL != null) && !smallImageURL.equals(_smallImageURL))) {
    if (!XSS_ALLOW_SMALLIMAGEURL) {
      smallImageURL=XSSUtil.strip(smallImageURL);
    }
    _smallImageURL=smallImageURL;
  }
}
