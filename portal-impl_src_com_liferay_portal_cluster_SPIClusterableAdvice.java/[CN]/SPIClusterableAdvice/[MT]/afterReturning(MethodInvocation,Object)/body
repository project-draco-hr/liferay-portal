{
  Clusterable clusterable=findAnnotation(methodInvocation);
  if (clusterable == NullClusterable.NULL_CLUSTERABLE) {
    return;
  }
  SPI spi=SPIUtil.getSPI();
  IntrabandRPCUtil.execute(spi.getRegistrationReference(),new MethodHandlerProcessCallable<Serializable>(ClusterableInvokerUtil.createMethodHandler(clusterable.acceptor(),methodInvocation)));
}
