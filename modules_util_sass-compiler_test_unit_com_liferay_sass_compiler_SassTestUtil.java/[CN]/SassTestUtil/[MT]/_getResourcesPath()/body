{
  StringBuilder sb=new StringBuilder("resources/");
  if (Platform.isLinux()) {
    sb.append("linux");
  }
 else   if (Platform.isMac()) {
    sb.append("darwin");
  }
 else   if (Platform.isWindows()) {
    sb.append("win32");
  }
  if (!Platform.isMac()) {
    sb.append("-x86");
    if (Platform.is64Bit()) {
      sb.append("-64");
    }
  }
  File file=new File(sb.toString());
  return file.getCanonicalPath();
}
