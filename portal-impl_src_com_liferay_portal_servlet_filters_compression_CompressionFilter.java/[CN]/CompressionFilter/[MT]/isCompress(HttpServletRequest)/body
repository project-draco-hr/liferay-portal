{
  if (!ParamUtil.get(request,_COMPRESS,true)) {
    return false;
  }
 else {
    String lifecycle=ParamUtil.getString(request,"p_p_lifecycle");
    if ((lifecycle.equals("1") && LiferayWindowState.isExclusive(request)) || lifecycle.equals("2")) {
      return false;
    }
 else {
      return true;
    }
  }
}
