{
  long groupId=ParamUtil.getLong(request,"groupId");
  String structureKey=ParamUtil.getString(request,"structureKey");
  DDMStructureEntry structureEntry=null;
  if (Validator.isNotNull(structureKey)) {
    structureEntry=DDMStructureEntryServiceUtil.getStructureEntry(groupId,structureKey);
  }
  request.setAttribute(WebKeys.DYNAMIC_DATA_MAPPING_STRUCTURE_ENTRY,structureEntry);
}
