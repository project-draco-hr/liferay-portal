{
  if ((type == null) || type.contains(StringPool.BACK_SLASH) || type.contains(StringPool.COLON)|| type.contains(StringPool.GREATER_THAN)|| type.contains(StringPool.LESS_THAN)|| type.contains(StringPool.PERCENT)|| type.contains(StringPool.PERIOD)|| type.contains(StringPool.PIPE)|| type.contains(StringPool.QUESTION)|| type.contains(StringPool.QUOTE)|| type.contains(StringPool.SLASH)|| type.contains(StringPool.SPACE)|| type.contains(StringPool.STAR)) {
    throw new ImageTypeException();
  }
}
