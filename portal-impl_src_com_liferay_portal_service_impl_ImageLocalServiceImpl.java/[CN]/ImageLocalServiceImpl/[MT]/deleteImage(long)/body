{
  if (imageId <= 0) {
    return null;
  }
  Image image=getImage(imageId);
  if (image != null) {
    imagePersistence.remove(image);
    Hook hook=HookFactory.getInstance();
    try {
      hook.deleteImage(image);
    }
 catch (    NoSuchImageException nsie) {
      if (_log.isWarnEnabled()) {
        _log.warn(nsie,nsie);
      }
    }
  }
  return image;
}
