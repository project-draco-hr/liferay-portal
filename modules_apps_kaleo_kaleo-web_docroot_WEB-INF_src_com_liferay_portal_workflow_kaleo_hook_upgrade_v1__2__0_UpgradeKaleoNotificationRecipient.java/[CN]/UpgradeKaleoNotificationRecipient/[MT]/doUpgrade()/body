{
  Connection con=null;
  PreparedStatement ps=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("update KaleoNotificationRecipient set recipientClassName = " + "'ADDRESS' where recipientClassName is null or " + "recipientClassName = ''");
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
}
