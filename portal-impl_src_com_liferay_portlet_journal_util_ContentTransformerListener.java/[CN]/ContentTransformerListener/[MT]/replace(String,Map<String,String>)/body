{
  try {
    Document document=SAXReaderUtil.read(xml);
    Element rootElement=document.getRootElement();
    replace(rootElement,tokens);
    xml=DDMXMLUtil.formatXML(document);
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn(e.getMessage());
    }
  }
  return xml;
}
