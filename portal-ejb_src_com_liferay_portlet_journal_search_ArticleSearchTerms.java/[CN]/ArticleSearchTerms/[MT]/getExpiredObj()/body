{
  if (status.equals("approved")) {
    return Boolean.FALSE;
  }
 else   if (status.equals("not-approved")) {
    return Boolean.FALSE;
  }
 else   if (status.equals("expired")) {
    return Boolean.TRUE;
  }
 else   if (status.equals("review")) {
    return Boolean.FALSE;
  }
 else {
    return null;
  }
}
