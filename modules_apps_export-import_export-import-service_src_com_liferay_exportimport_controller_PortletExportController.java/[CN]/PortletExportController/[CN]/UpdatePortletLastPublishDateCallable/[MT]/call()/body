{
  Group group=GroupLocalServiceUtil.getGroup(_groupId);
  Layout layout=LayoutLocalServiceUtil.fetchLayout(_plid);
  if (group.hasStagingGroup()) {
    group=group.getStagingGroup();
    if (layout != null) {
      layout=LayoutLocalServiceUtil.fetchLayoutByUuidAndGroupId(layout.getUuid(),group.getGroupId(),layout.isPrivateLayout());
    }
  }
  if (layout == null) {
    layout=new LayoutImpl();
    layout.setCompanyId(group.getCompanyId());
    layout.setGroupId(group.getGroupId());
  }
  javax.portlet.PortletPreferences jxPortletPreferences=PortletPreferencesFactoryUtil.getStrictPortletSetup(layout,_portletId);
  ExportImportDateUtil.updateLastPublishDate(_portletId,jxPortletPreferences,_dateRange,_endDate);
  return null;
}
