{
  DLFileShortcutPermission.check(getPermissionChecker(),fileShortcutId,ActionKeys.UPDATE);
  try {
    DLFileEntryPermission.check(getPermissionChecker(),toFolderId,toName,ActionKeys.VIEW);
  }
 catch (  PrincipalException pe) {
    throw new FileShortcutPermissionException();
  }
  return dlFileShortcutLocalService.updateFileShortcut(getUserId(),fileShortcutId,folderId,toFolderId,toName);
}
