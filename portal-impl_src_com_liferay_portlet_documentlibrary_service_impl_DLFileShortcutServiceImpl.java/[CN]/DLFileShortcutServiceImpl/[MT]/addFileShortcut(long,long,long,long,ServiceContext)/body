{
  DLFolderPermission.check(getPermissionChecker(),groupId,folderId,ActionKeys.ADD_SHORTCUT);
  try {
    DLFileEntryPermission.check(getPermissionChecker(),toFileEntryId,ActionKeys.VIEW);
  }
 catch (  PrincipalException pe) {
    throw new FileShortcutPermissionException(pe);
  }
  return dlFileShortcutLocalService.addFileShortcut(getUserId(),groupId,repositoryId,folderId,toFileEntryId,serviceContext);
}
