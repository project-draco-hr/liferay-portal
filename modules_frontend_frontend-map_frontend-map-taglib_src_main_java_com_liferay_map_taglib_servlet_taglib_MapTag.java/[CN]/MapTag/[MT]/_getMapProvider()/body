{
  MapProviderTracker mapProviderTracker=ServletContextUtil.getMapProviderTracker();
  String mapProviderKey=_getMapProviderKey();
  MapProvider mapProvider=null;
  if (Validator.isNotNull(mapProviderKey)) {
    mapProvider=mapProviderTracker.getMapProvider(mapProviderKey);
  }
  if (mapProvider == null) {
    List<MapProvider> mapProviders=new ArrayList(mapProviderTracker.getMapProviders());
    if (!mapProviders.isEmpty()) {
      mapProvider=mapProviders.get(0);
    }
  }
  return mapProvider;
}
