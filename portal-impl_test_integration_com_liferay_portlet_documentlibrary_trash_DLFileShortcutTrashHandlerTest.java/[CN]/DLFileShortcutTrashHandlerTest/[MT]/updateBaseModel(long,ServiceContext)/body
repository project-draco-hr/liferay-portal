{
  DLFileShortcut fileShortcut=DLFileShortcutLocalServiceUtil.getFileShortcut(primaryKey);
  if (serviceContext.getWorkflowAction() == WorkflowConstants.ACTION_SAVE_DRAFT) {
    DLFileShortcutLocalServiceUtil.updateStatus(TestPropsValues.getUserId(),primaryKey,WorkflowConstants.STATUS_DRAFT,serviceContext);
  }
  return fileShortcut;
}
