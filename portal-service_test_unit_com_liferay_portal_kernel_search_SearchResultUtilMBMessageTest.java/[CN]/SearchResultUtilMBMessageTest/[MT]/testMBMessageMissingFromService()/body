{
  when(mbMessageLocalService.getMessage(ENTRY_CLASS_PK)).thenReturn(null);
  searchSingleDocument(createMBMessageDocumentWithAlternateKey());
  Assert.assertEquals(DOCUMENT_CLASS_NAME,result.getClassName());
  Assert.assertEquals(DOCUMENT_CLASS_PK,result.getClassPK());
  Assert.assertThat("MBMessageLocalService is attempted, no message returned",result.getMBMessages(),IsEmptyCollection.empty());
  Mockito.verify(mbMessageLocalService).getMessage(ENTRY_CLASS_PK);
  Assert.assertNull("Indexer and AssetRenderer are both attempted, no summary returned",result.getSummary());
  verifyStatic();
  IndexerRegistryUtil.getIndexer(DOCUMENT_CLASS_NAME);
  verifyStatic();
  AssetRendererFactoryRegistryUtil.getAssetRendererFactoryByClassName(DOCUMENT_CLASS_NAME);
  assertThatEverythingUnrelatedIsEmpty();
}
