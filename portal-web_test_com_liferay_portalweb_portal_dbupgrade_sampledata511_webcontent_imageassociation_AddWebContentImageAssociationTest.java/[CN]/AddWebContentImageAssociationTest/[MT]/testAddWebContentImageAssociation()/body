        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  assertEquals(RuntimeVariables.replace("Image Structure Test"),selenium.getText("_15_structureName"));
  selenium.type("_15_structure_el0_content",RuntimeVariables.replace("Text Test"));
