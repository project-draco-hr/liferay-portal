{
  FileEntry fileEntry=addFileEntry(RandomTestUtil.randomString() + ".txt",RandomTestUtil.randomString());
  deleteGuestPermission(fileEntry);
  List<SocialActivity> activities=SocialActivityLocalServiceUtil.getGroupActivities(_group.getGroupId(),QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  Assert.assertEquals(1,activities.size());
  ServiceTestUtil.setUser(_user);
  activities=SocialActivityServiceUtil.getGroupActivities(_group.getGroupId(),QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  Assert.assertEquals(0,activities.size());
  ServiceTestUtil.setUser(TestPropsValues.getUser());
}
