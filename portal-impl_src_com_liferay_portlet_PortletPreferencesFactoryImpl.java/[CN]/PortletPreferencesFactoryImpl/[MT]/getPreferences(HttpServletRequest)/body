{
  PortletRequest portletReq=(PortletRequest)req.getAttribute(JavaConstants.JAVAX_PORTLET_REQUEST);
  PortletPreferences prefs=null;
  if (portletReq != null) {
    PortletPreferencesWrapper prefsWrapper=(PortletPreferencesWrapper)portletReq.getPreferences();
    prefs=prefsWrapper.getPreferencesImpl();
  }
  return prefs;
}
