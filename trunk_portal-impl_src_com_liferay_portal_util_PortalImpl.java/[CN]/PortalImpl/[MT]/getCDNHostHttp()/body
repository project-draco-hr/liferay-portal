{
  String cdnHostHttp=_cdnHostHttp.get();
  if (cdnHostHttp != null) {
    return cdnHostHttp;
  }
  long companyId=CompanyThreadLocal.getCompanyId();
  try {
    cdnHostHttp=PrefsPropsUtil.getString(companyId,PropsKeys.CDN_HOST_HTTP,PropsValues.CDN_HOST_HTTP);
  }
 catch (  Exception e) {
  }
  if ((cdnHostHttp == null) || cdnHostHttp.startsWith("${")) {
    cdnHostHttp=StringPool.BLANK;
  }
  _cdnHostHttp.set(cdnHostHttp);
  return cdnHostHttp;
}
