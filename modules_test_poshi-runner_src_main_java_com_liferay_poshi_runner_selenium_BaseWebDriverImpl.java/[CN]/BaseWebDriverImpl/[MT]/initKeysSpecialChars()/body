{
  _keysSpecialChars.put("!","1");
  _keysSpecialChars.put("#","3");
  _keysSpecialChars.put("$","4");
  _keysSpecialChars.put("%","5");
  _keysSpecialChars.put("&","7");
  _keysSpecialChars.put("(","9");
  _keysSpecialChars.put(")","0");
  _keysSpecialChars.put("<",",");
  _keysSpecialChars.put(">",".");
}
