{
  for (int second=0; ; second++) {
    if (second >= PropsValues.TIMEOUT_EXPLICIT_WAIT) {
      assertNotSelectedLabel(selectLocator,pattern);
    }
    try {
      if (isNotSelectedLabel(selectLocator,pattern)) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
