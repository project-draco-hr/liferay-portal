{
  value=RuntimeVariables.replace(value);
  for (int second=0; ; second++) {
    if (second >= PropsValues.TIMEOUT_EXPLICIT_WAIT) {
      assertTextNotPresent(value);
    }
    try {
      if (isTextNotPresent(value)) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
