{
  Collection<DLDisplayContextFactory> dlDisplayContextFactories=_dlDisplayContextFactories.values();
  DLEditFileEntryDisplayContext dlEditFileEntryDisplayContext=new DefaultDLEditFileEntryDisplayContext(request,response,fileEntry);
  for (  DLDisplayContextFactory dlDisplayContextFactory : dlDisplayContextFactories) {
    dlEditFileEntryDisplayContext=dlDisplayContextFactory.getDLEditFileEntryDisplayContext(dlEditFileEntryDisplayContext,request,response,fileEntry);
  }
  return dlEditFileEntryDisplayContext;
}
