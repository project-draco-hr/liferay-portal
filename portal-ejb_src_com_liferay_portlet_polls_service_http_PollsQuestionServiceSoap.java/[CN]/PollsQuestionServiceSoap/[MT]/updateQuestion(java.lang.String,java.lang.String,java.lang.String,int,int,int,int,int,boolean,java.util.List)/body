{
  try {
    com.liferay.portlet.polls.model.PollsQuestion returnValue=PollsQuestionServiceUtil.updateQuestion(questionId,title,description,expirationDateMonth,expirationDateDay,expirationDateYear,expirationDateHour,expirationDateMinute,neverExpire,choices);
