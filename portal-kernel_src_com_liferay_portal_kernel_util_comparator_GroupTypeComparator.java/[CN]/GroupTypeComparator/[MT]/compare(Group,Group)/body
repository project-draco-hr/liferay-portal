{
  int value=0;
  if (group1.getType() > group2.getType()) {
    value=1;
  }
 else   if (group1.getType() < group2.getType()) {
    value=-1;
  }
  if (value == 0) {
    String name1=group1.getName();
    String name2=group2.getName();
    value=name1.compareTo(name2);
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
