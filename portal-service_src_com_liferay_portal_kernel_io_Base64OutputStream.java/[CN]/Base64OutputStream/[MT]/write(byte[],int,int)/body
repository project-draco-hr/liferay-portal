{
  if (length <= 0) {
    return;
  }
  while ((_unitBufferIndex != 0) && (length > 0)) {
    write(bytes[offset++]);
    length--;
  }
  if (length <= 0) {
    return;
  }
  int bytesLength=length - length % 3;
  length-=bytesLength;
  while (bytesLength > 0) {
    encodeUnit(bytes[offset],bytes[offset + 1],bytes[offset + 2]);
    bytesLength-=3;
    offset+=3;
  }
  while (length > 0) {
    write(bytes[offset++]);
    length--;
  }
}
