{
  Cookie cookie=new Cookie(CookieKeys.ID,StringPool.BLANK);
  cookie.setMaxAge(0);
  cookie.setPath(StringPool.SLASH);
  CookieKeys.addCookie(request,response,cookie);
  cookie=new Cookie(CookieKeys.PASSWORD,StringPool.BLANK);
  cookie.setMaxAge(0);
  cookie.setPath(StringPool.SLASH);
  CookieKeys.addCookie(request,response,cookie);
}
