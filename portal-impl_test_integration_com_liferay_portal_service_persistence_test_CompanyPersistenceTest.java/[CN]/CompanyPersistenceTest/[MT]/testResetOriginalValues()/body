{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  Company newCompany=addCompany();
  _persistence.clearCache();
  Company existingCompany=_persistence.findByPrimaryKey(newCompany.getPrimaryKey());
  Assert.assertTrue(Validator.equals(existingCompany.getWebId(),ReflectionTestUtil.invoke(existingCompany,"getOriginalWebId",new Class<?>[0])));
  Assert.assertTrue(Validator.equals(existingCompany.getMx(),ReflectionTestUtil.invoke(existingCompany,"getOriginalMx",new Class<?>[0])));
  Assert.assertEquals(existingCompany.getLogoId(),ReflectionTestUtil.invoke(existingCompany,"getOriginalLogoId",new Class<?>[0]));
}
