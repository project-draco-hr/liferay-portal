{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Company.class,Company.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("companyId",RandomTestUtil.nextLong()));
  List<Company> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
