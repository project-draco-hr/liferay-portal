{
  if (_type == "primary") {
    setAddMenuItems((List<AddMenuItem>)request.getAttribute("liferay-frontend:add-menu:addMenuPrimaryItems"));
  }
 else   if (_type == "favorite") {
    setAddMenuItems((List<AddMenuItem>)request.getAttribute("liferay-frontend:add-menu:addMenuFavItems"));
  }
 else   if (_type == "recent") {
    setAddMenuItems((List<AddMenuItem>)request.getAttribute("liferay-frontend:add-menu:addMenuRecentItems"));
  }
 else {
    setAddMenuItems((List<AddMenuItem>)request.getAttribute("liferay-frontend:add-menu:addMenuItems"));
  }
}
