{
  if (_customTokens == null) {
synchronized (JournalUtil.class) {
      _customTokens=new HashMap<>();
      for (      String customToken : JournalServiceConfigurationValues.JOURNAL_ARTICLE_CUSTOM_TOKENS) {
        String value=PropsUtil.get(JournalServiceConfigurationKeys.JOURNAL_ARTICLE_CUSTOM_TOKEN_VALUE,new Filter(customToken));
        _customTokens.put(customToken,value);
      }
    }
  }
  if (!_customTokens.isEmpty()) {
    tokens.putAll(_customTokens);
  }
}
