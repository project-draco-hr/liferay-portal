{
  for (  String tokenValue : journalServiceConfiguration.customTokenValues()) {
    String[] tokenValueParts=tokenValue.split("\\=");
    if (tokenValueParts.length != 2) {
      continue;
    }
    if (tokenValueParts[0].equals(tokenName)) {
      return tokenValueParts[1];
    }
  }
  return null;
}
