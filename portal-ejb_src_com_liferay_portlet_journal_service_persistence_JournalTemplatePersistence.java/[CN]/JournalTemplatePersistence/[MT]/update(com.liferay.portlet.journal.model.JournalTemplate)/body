{
  Session session=null;
  try {
    session=openSession();
    if (journalTemplate.isNew()) {
      session.save(journalTemplate);
    }
    session.flush();
    journalTemplate.setNew(false);
    return journalTemplate;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
