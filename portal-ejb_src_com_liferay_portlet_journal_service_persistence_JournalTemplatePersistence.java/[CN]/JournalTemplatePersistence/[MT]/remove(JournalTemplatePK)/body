{
  Session session=null;
  try {
    session=openSession();
    JournalTemplate journalTemplate=(JournalTemplate)session.get(JournalTemplateImpl.class,journalTemplatePK);
    if (journalTemplate == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No JournalTemplate exists with the primary key " + journalTemplatePK);
      }
      throw new NoSuchTemplateException("No JournalTemplate exists with the primary key " + journalTemplatePK);
    }
    return remove(journalTemplate);
  }
 catch (  NoSuchTemplateException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
