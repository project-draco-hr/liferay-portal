{
  Session session=null;
  try {
    session=openSession();
    JournalTemplateHBM journalTemplateHBM=(JournalTemplateHBM)session.get(JournalTemplateHBM.class,journalTemplatePK);
    if (journalTemplateHBM == null) {
      _log.warn("No JournalTemplate exists with the primary key " + journalTemplatePK.toString());
      throw new NoSuchTemplateException("No JournalTemplate exists with the primary key " + journalTemplatePK.toString());
    }
    return JournalTemplateHBMUtil.model(journalTemplateHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
