{
  DynamicQuery dynamicQuery=AssetTagLocalServiceUtil.dynamicQuery();
  Property companyIdProperty=PropertyFactoryUtil.forName("companyId");
  dynamicQuery.add(companyIdProperty.eq(companyId));
  Property nameProperty=PropertyFactoryUtil.forName("name");
  dynamicQuery.add(nameProperty.eq(uuid));
  List<AssetTag> tags=AssetTagLocalServiceUtil.dynamicQuery(dynamicQuery);
  if (ListUtil.isEmpty(tags)) {
    return null;
  }
  return ModelAdapterUtil.adapt(tags.get(0),AssetTag.class,StagedAssetTag.class);
}
