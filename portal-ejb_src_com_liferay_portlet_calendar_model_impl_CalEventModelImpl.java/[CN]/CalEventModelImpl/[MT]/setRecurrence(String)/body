{
  if (((recurrence == null) && (_recurrence != null)) || ((recurrence != null) && (_recurrence == null)) || ((recurrence != null) && (_recurrence != null) && !recurrence.equals(_recurrence))) {
    if (!XSS_ALLOW_RECURRENCE) {
      recurrence=XSSUtil.strip(recurrence);
    }
    _recurrence=recurrence;
  }
}
