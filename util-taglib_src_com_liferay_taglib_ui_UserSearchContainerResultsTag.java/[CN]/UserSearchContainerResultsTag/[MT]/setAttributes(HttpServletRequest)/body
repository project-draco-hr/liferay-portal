{
  SearchContainerTag<R> searchContainerTag=(SearchContainerTag<R>)findAncestorWithClass(this,SearchContainerTag.class);
  _searchContainer=searchContainerTag.getSearchContainer();
  _searchTerms=_searchContainer.getSearchTerms();
  request.setAttribute("liferay-ui:user-search-container-results:indexerDisabled",_indexerDisabled);
  request.setAttribute("liferay-ui:user-search-container-results:searchContainer",_searchContainer);
  request.setAttribute("liferay-ui:user-search-container-results:searchTerms",_searchTerms);
  request.setAttribute("liferay-ui:user-search-container-results:userParams",_userParams);
}
