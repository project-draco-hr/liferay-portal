{
  List<AssetRendererFactory<?>> assetRendererFactories=AssetRendererFactoryRegistryUtil.getAssetRendererFactories(companyId);
  List<String> assetTypes=new ArrayList<>();
  for (int i=0; i < assetRendererFactories.size(); i++) {
    AssetRendererFactory<?> assetRendererFactory=assetRendererFactories.get(i);
    if (!assetRendererFactory.isSearchable()) {
      continue;
    }
    assetTypes.add(assetRendererFactory.getClassName());
  }
  return ArrayUtil.toStringArray(assetTypes);
}
