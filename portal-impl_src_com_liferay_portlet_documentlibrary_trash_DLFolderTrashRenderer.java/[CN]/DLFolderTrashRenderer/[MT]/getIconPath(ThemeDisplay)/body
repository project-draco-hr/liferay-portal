{
  int foldersCount=0;
  int fileEntriesAndFileShortcutsCount=0;
  try {
    foldersCount=DLAppServiceUtil.getFoldersCount(_folder.getRepositoryId(),_folder.getFolderId());
    fileEntriesAndFileShortcutsCount=DLAppServiceUtil.getFileEntriesAndFileShortcutsCount(_folder.getRepositoryId(),_folder.getFolderId(),WorkflowConstants.STATUS_APPROVED);
  }
 catch (  Exception e) {
  }
  if ((foldersCount + fileEntriesAndFileShortcutsCount) > 0) {
    return themeDisplay.getPathThemeImages() + "/common/folder_full_document.png";
  }
  return themeDisplay.getPathThemeImages() + "/common/folder_empty.png";
}
