{
  Set<String> set=_flushResourceBlockEnabled.get();
  StringBundler sb=new StringBundler(5);
  sb.append(companyId);
  sb.append(StringPool.UNDERLINE);
  sb.append(groupId);
  sb.append(StringPool.UNDERLINE);
  sb.append(name);
  if (enabled) {
    set.remove(sb.toString());
  }
 else {
    set.add(sb.toString());
  }
}
