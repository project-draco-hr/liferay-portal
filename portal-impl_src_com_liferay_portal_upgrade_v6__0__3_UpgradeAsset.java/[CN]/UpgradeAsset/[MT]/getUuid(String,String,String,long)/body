{
  String uuid=StringPool.BLANK;
  try (PreparedStatement ps=connection.prepareStatement("select uuid_ from " + tableName + " where "+ columnName1+ " = ? or "+ columnName2+ " = ?")){
    ps.setLong(1,classPK);
    ps.setLong(2,classPK);
    try (ResultSet rs=ps.executeQuery()){
      while (rs.next()) {
        uuid=rs.getString("uuid_");
      }
    }
   }
   return uuid;
}
