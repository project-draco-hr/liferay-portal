{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select classNameId, classPK from AssetEntry");
    rs=ps.executeQuery();
    while (rs.next()) {
      long classNameId=rs.getLong("classNameId");
      long classPK=rs.getLong("classPK");
      String className=PortalUtil.getClassName(classNameId);
      String[] tableAndColumn=getTableAndColumnName(className);
      if (Validator.isNull(tableAndColumn[0]) || Validator.isNull(tableAndColumn[1])) {
        continue;
      }
      String uuid=getUuid(tableAndColumn[0],tableAndColumn[1],tableAndColumn[2],classPK);
      updateAssetEntry(classNameId,classPK,uuid);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
