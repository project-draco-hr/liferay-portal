{
  List<FactoryConfiguration> factoryConfigurations=configuration.getCacheManagerPeerListenerFactoryConfigurations();
  factoryConfigurations.clear();
  factoryConfigurations=configuration.getCacheManagerPeerProviderFactoryConfiguration();
  factoryConfigurations.clear();
  for (  CacheConfiguration cacheConfiguration : cacheConfigurations) {
    String properties=_clearCacheEventListenerConfigurations(cacheConfiguration,usingDefault);
    if ((properties != null) && PropsValues.EHCACHE_CLUSTER_LINK_REPLICATION_ENABLED) {
      _enableClusterLinkReplication(cacheConfiguration,properties);
    }
  }
}
