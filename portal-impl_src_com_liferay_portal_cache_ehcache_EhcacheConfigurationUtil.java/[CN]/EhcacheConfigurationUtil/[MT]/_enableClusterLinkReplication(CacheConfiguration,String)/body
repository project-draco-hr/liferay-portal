{
  if ((cacheEventListenerProperties == null) || !PropsValues.EHCACHE_CLUSTER_LINK_REPLICATION_ENABLED) {
    return;
  }
  if (cacheEventListenerProperties.equals(StringPool.BLANK)) {
    cacheEventListenerProperties=null;
  }
  CacheEventListenerFactoryConfiguration cacheEventListenerFactoryConfiguration=new CacheEventListenerFactoryConfiguration();
  cacheEventListenerFactoryConfiguration.setClass(EhcachePortalCacheClusterReplicatorFactory.class.getName());
  cacheEventListenerFactoryConfiguration.setProperties(cacheEventListenerProperties);
  cacheConfiguration.addCacheEventListenerFactory(cacheEventListenerFactoryConfiguration);
}
