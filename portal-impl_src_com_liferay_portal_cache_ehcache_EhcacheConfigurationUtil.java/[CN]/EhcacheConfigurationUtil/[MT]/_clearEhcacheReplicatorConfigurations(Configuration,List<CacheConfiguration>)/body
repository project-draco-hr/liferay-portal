{
  configuration.getCacheManagerPeerListenerFactoryConfigurations().clear();
  configuration.getCacheManagerPeerProviderFactoryConfiguration().clear();
  Map<CacheConfiguration,String> cacheEventListenerProperties=new HashMap<CacheConfiguration,String>();
  for (  CacheConfiguration cacheConfiguration : cacheConfigurations) {
    String properties=_clearCacheEventListenerConfigurations(cacheConfiguration);
    cacheEventListenerProperties.put(cacheConfiguration,properties);
  }
  return cacheEventListenerProperties;
}
