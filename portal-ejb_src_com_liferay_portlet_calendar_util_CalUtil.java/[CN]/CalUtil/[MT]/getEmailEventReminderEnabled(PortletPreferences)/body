{
  String emailEventReminderEnabled=prefs.getValue("email-event-reminder-enabled",StringPool.BLANK);
  if (Validator.isNotNull(emailEventReminderEnabled)) {
    return GetterUtil.getBoolean(emailEventReminderEnabled);
  }
 else {
    return GetterUtil.getBoolean(PropsUtil.get(PropsUtil.CALENDAR_EMAIL_EVENT_REMINDER_ENABLED));
  }
}
