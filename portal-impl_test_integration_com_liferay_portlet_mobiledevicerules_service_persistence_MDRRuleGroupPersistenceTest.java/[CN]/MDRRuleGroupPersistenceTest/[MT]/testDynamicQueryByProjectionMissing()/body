{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MDRRuleGroup.class,MDRRuleGroup.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("ruleGroupId"));
  dynamicQuery.add(RestrictionsFactoryUtil.in("ruleGroupId",new Object[]{nextLong()}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(0,result.size());
}
