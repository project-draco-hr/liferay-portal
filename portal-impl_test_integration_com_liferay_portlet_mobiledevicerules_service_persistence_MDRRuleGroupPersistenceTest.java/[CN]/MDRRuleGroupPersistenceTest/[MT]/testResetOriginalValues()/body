{
  if (!PropsValues.HIBERNATE_CACHE_USE_SECOND_LEVEL_CACHE) {
    return;
  }
  MDRRuleGroup newMDRRuleGroup=addMDRRuleGroup();
  _persistence.clearCache();
  MDRRuleGroupModelImpl existingMDRRuleGroupModelImpl=(MDRRuleGroupModelImpl)_persistence.findByPrimaryKey(newMDRRuleGroup.getPrimaryKey());
  assertTrue(Validator.equals(existingMDRRuleGroupModelImpl.getUuid(),existingMDRRuleGroupModelImpl.getOriginalUuid()));
  assertEquals(existingMDRRuleGroupModelImpl.getGroupId(),existingMDRRuleGroupModelImpl.getOriginalGroupId());
}
