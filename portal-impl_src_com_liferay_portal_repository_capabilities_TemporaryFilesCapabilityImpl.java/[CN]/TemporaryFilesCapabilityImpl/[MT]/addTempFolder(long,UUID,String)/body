{
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  Folder tempFolder=addFolder(userId,DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,_FOLDER_NAME_TEMP,serviceContext);
  Folder callerFolder=addFolder(userId,tempFolder.getFolderId(),callerUuid.toString(),serviceContext);
  return addFolders(userId,callerFolder.getFolderId(),folderPath,serviceContext);
}
