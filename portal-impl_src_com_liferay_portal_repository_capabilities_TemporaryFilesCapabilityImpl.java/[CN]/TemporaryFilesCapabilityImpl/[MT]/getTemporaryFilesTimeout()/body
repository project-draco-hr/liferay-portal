{
  ConfigurationCapability configurationCapability=_localRepository.getCapability(ConfigurationCapability.class);
  String minimumLifespanMilliseconds=configurationCapability.getProperty(getClass(),_PROPERTY_TEMPORARY_FILES_TIMEOUT);
  if (minimumLifespanMilliseconds == null) {
    return _TEMPORARY_FILES_TIMEOUT_DEFAULT;
  }
  return GetterUtil.getLong(minimumLifespanMilliseconds);
}
