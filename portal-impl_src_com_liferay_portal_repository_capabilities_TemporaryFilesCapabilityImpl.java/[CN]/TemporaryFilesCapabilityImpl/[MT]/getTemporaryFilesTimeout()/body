{
  ConfigurationCapability configurationCapability=_localRepository.getCapability(ConfigurationCapability.class);
  String temporaryFilesTimeout=configurationCapability.getProperty(getClass(),_PROPERTY_TEMPORARY_FILES_TIMEOUT);
  if (temporaryFilesTimeout == null) {
    return _TEMPORARY_FILES_TIMEOUT_DEFAULT;
  }
  return GetterUtil.getLong(temporaryFilesTimeout);
}
