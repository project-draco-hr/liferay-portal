{
  ConfigurationCapability configurationCapability=_localRepository.getCapability(ConfigurationCapability.class);
  String minimumLifespanMilliseconds=configurationCapability.getProperty(getClass(),PROPERTY_TEMPORARY_FILES_TIMEOUT);
  if (minimumLifespanMilliseconds == null) {
    return DEFAULT_TEMPORARY_FILES_TIMEOUT;
  }
  return Long.valueOf(minimumLifespanMilliseconds);
}
