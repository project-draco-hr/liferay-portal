{
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  Folder tempFolder=_getFolder(DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,FOLDER_NAME_TEMP);
  Folder invokerFolder=_getFolder(tempFolder.getFolderId(),invokerUuid.toString());
  return _getFolders(invokerFolder.getFolderId(),folderPath);
}
