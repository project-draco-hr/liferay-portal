{
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  Folder tmpFolder=_getFolder(DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,FOLDER_NAME_TMP);
  Folder callerFolder=_getFolder(tmpFolder.getFolderId(),callerUuid.toString());
  return _getFolders(callerFolder.getFolderId(),folderPath);
}
