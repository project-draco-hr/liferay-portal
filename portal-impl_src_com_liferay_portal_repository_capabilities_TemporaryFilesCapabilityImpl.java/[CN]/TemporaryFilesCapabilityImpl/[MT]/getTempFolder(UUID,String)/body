{
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  Folder tempFolder=getFolder(DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,_FOLDER_NAME_TEMP);
  Folder folder=getFolder(tempFolder.getFolderId(),callerUuid.toString());
  return getDeepestFolder(folder.getFolderId(),folderPath);
}
