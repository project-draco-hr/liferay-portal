{
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  Folder tempFolder=_getFolder(DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,_FOLDER_NAME_TEMP);
  Folder callerFolder=_getFolder(tempFolder.getFolderId(),callerUuid.toString());
  return getDeepestFolder(callerFolder.getFolderId(),folderPath);
}
