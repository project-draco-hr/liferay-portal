{
  Session session=null;
  try {
    session=openSession();
    try {
      CyrusVirtual virtualModel=(CyrusVirtual)session.load(CyrusVirtual.class,virtual.getEmailAddress());
      virtualModel.setUserId(virtual.getUserId());
      session.flush();
    }
 catch (    ObjectNotFoundException onfe) {
      CyrusVirtual virtualModel=new CyrusVirtual(virtual.getEmailAddress(),virtual.getUserId());
      session.save(virtualModel);
      session.flush();
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
