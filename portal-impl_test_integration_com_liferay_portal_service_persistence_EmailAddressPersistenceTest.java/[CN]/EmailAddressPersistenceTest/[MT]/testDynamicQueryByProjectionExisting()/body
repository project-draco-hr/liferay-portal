{
  EmailAddress newEmailAddress=addEmailAddress();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(EmailAddress.class,EmailAddress.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("emailAddressId"));
  Object newEmailAddressId=newEmailAddress.getEmailAddressId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("emailAddressId",new Object[]{newEmailAddressId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Object existingEmailAddressId=result.get(0);
  assertEquals(existingEmailAddressId,newEmailAddressId);
}
