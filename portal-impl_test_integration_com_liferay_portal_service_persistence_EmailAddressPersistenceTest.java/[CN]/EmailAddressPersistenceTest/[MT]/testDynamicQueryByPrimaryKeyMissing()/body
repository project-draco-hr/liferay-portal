{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(EmailAddress.class,EmailAddress.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("emailAddressId",ServiceTestUtil.nextLong()));
  List<EmailAddress> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
