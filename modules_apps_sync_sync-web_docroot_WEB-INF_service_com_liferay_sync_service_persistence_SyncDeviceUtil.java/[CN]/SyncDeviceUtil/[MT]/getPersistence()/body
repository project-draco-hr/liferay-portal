{
  if (_persistence == null) {
    _persistence=(SyncDevicePersistence)PortletBeanLocatorUtil.locate(com.liferay.sync.service.ClpSerializer.getServletContextName(),SyncDevicePersistence.class.getName());
    ReferenceRegistry.registerReference(SyncDeviceUtil.class,"_persistence");
  }
  return _persistence;
}
