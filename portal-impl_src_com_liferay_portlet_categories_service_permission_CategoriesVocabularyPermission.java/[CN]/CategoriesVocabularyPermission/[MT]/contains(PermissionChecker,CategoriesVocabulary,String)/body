{
  if (permissionChecker.hasOwnerPermission(vocabulary.getCompanyId(),CategoriesVocabulary.class.getName(),vocabulary.getVocabularyId(),vocabulary.getUserId(),actionId)) {
    return true;
  }
  return permissionChecker.hasPermission(vocabulary.getGroupId(),CategoriesVocabulary.class.getName(),vocabulary.getVocabularyId(),actionId);
}
