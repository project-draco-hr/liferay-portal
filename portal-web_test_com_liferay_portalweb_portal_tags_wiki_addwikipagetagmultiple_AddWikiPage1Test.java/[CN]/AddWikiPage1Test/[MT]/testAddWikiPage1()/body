{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Wiki Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Wiki Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("link=All Pages",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//input[@value='Add Page']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.type("_36_title",RuntimeVariables.replace("Wiki1 Page1 Test1"));
  selenium.type("_36_content",RuntimeVariables.replace("This is a wiki1 page1 test1."));
  selenium.clickAt("//input[@value='Publish']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request processed successfully."));
  assertTrue(selenium.isElementPresent("link=Wiki1 Page1 Test1"));
  selenium.clickAt("link=Wiki1 Page1 Test1",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//h1[@class='header-title']/span","Wiki1 Page1 Test1"));
  assertEquals(RuntimeVariables.replace("This is a wiki1 page1 test1."),selenium.getText("//div[@class='wiki-body']"));
}
