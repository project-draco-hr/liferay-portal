{
  if (((employeeStatusId == null) && (_employeeStatusId != null)) || ((employeeStatusId != null) && (_employeeStatusId == null)) || ((employeeStatusId != null) && (_employeeStatusId != null) && !employeeStatusId.equals(_employeeStatusId))) {
    if (!XSS_ALLOW_EMPLOYEESTATUSID) {
      employeeStatusId=XSSUtil.strip(employeeStatusId);
    }
    _employeeStatusId=employeeStatusId;
    setModified(true);
  }
}
