{
  if (((suffixId == null) && (_suffixId != null)) || ((suffixId != null) && (_suffixId == null)) || ((suffixId != null) && (_suffixId != null) && !suffixId.equals(_suffixId))) {
    if (!XSS_ALLOW_SUFFIXID) {
      suffixId=XSSUtil.strip(suffixId);
    }
    _suffixId=suffixId;
    setModified(true);
  }
}
