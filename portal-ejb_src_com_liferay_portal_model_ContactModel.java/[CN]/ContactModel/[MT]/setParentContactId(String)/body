{
  if (((parentContactId == null) && (_parentContactId != null)) || ((parentContactId != null) && (_parentContactId == null)) || ((parentContactId != null) && (_parentContactId != null) && !parentContactId.equals(_parentContactId))) {
    if (!XSS_ALLOW_PARENTCONTACTID) {
      parentContactId=XSSUtil.strip(parentContactId);
    }
    _parentContactId=parentContactId;
    setModified(true);
  }
