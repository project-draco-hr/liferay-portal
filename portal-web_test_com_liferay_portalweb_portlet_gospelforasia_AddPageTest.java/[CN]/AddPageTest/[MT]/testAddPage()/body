{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("document.getElementById('my-community-private-pages')")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("document.getElementById('my-community-private-pages')");
  selenium.waitForPageToLoad("30000");
  selenium.click("document.getElementById('add-page').getElementsByTagName('a')[0].getElementsByTagName('span')[0]");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("new_page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.typeKeys("new_page",RuntimeVariables.replace("Gospel for Asia Test Page"));
  selenium.type("new_page",RuntimeVariables.replace("Gospel for Asia Test Page"));
  selenium.click("link=Save");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Gospel for Asia Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("link=Gospel for Asia Test Page");
  selenium.waitForPageToLoad("30000");
}
