{
  String resourceID=GetterUtil.getString(resourceRequest.getResourceID());
  if (resourceID.equals("getMobileDeviceRules")) {
    PortletSession portletSession=resourceRequest.getPortletSession();
    PortletContext portletContext=portletSession.getPortletContext();
    PortletRequestDispatcher portletRequestDispatcher=portletContext.getRequestDispatcher("/layout/mobile_device_rules_rule_group_instances.jsp");
    portletRequestDispatcher.include(resourceRequest,resourceResponse);
  }
 else {
    super.serveResource(resourceRequest,resourceResponse);
  }
}
