{
  if (((countryId == null) && (_countryId != null)) || ((countryId != null) && (_countryId == null)) || ((countryId != null) && (_countryId != null) && !countryId.equals(_countryId))) {
    if (!XSS_ALLOW_COUNTRYID) {
      countryId=XSSUtil.strip(countryId);
    }
    _countryId=countryId;
    setModified(true);
  }
}
