{
  TemplateManager templateManager=TemplateManagerUtil.getTemplateManager(langType);
  TemplateHandler templateHandler=TemplateHandlerRegistryUtil.getTemplateHandler(JournalArticle.class.getName());
  Map<String,Object> contextObjects=new HashMap<>();
  templateManager.addContextObjects(contextObjects,templateHandler.getCustomContextObjects());
  return _journalTransformer.transform(themeDisplay,contextObjects,tokens,viewMode,languageId,document,portletRequestModel,script,langType,propagateException);
}
