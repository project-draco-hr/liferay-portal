{
  selenium.click("my-community-private-pages");
  sleep(5000);
  selenium.click("//span[. ='suntheme']");
  sleep(5000);
  selenium.chooseOkOnNextConfirmation();
  selenium.click("//div[id('navigation')]/ul/li/a/span[contains(.,'sunthemepage2')]/../../span");
  if (selenium.isConfirmationPresent())   selenium.getConfirmation();
  sleep(5000);
  selenium.click("my-community-private-pages");
  sleep(5000);
  assertFalse(selenium.isElementPresent("//span[. ='sunthemepage2']"));
}
