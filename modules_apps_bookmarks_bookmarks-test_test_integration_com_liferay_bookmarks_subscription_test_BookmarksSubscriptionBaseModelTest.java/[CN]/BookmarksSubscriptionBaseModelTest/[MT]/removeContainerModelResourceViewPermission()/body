{
  List<String> actionIds=new ArrayList<>(2);
  actionIds.add(ActionKeys.ACCESS);
  actionIds.add(ActionKeys.VIEW);
  ResourceBlockPermissionTestUtil.removeResourceBlockPermissions(_folder.getCompanyId(),_folder.getGroupId(),BookmarksPortletKeys.BOOKMARKS,BookmarksFolder.class.getName(),_folder.getFolderId(),new String[]{RoleConstants.GUEST,RoleConstants.SITE_MEMBER},actionIds);
