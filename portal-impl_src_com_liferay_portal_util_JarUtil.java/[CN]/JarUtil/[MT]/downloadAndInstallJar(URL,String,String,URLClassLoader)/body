{
  Path path=downloadAndInstallJar(url,libPath,name);
  URI uri=path.toUri();
  if (_log.isInfoEnabled()) {
    _log.info("Installing " + path + " to "+ urlClassLoader);
  }
  _addURLMethod.invoke(urlClassLoader,uri.toURL());
  if (_log.isInfoEnabled()) {
    _log.info("Installed " + path + " to "+ urlClassLoader);
  }
}
