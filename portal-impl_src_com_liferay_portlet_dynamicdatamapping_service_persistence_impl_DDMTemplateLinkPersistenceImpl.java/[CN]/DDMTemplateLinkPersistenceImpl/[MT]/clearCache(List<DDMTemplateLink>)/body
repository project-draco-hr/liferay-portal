{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  DDMTemplateLink ddmTemplateLink : ddmTemplateLinks) {
    EntityCacheUtil.removeResult(DDMTemplateLinkModelImpl.ENTITY_CACHE_ENABLED,DDMTemplateLinkImpl.class,ddmTemplateLink.getPrimaryKey());
    clearUniqueFindersCache(ddmTemplateLink);
  }
}
