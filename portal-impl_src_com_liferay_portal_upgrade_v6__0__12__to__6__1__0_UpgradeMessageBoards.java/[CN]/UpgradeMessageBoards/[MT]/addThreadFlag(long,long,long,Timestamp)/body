{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("insert into MBThreadFlag (threadFlagId, userId, " + "modifiedDate, threadId) values (?, ?, ?, ?)");
    ps.setLong(1,threadFlagId);
    ps.setLong(2,userId);
    ps.setTimestamp(3,modifiedDate);
    ps.setLong(4,threadId);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
