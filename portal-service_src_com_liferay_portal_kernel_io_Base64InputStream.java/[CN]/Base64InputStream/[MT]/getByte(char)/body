{
  if ((character >= 'A') && (character <= 'Z')) {
    return character - 65;
  }
  if ((character >= 'a') && (character <= 'z')) {
    return (character - 97) + 26;
  }
  if (character >= '0' && character <= '9') {
    return (character - 48) + 52;
  }
  if (character == '+') {
    return 62;
  }
  if (character == '/') {
    return 63;
  }
  return character != '=' ? -1 : 0;
}
