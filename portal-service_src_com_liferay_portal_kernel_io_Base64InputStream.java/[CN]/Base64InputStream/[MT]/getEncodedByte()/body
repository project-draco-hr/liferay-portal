{
  while (true) {
    int returnValue=_inputStream.read();
    if (returnValue == -1) {
      return -1;
    }
    char character=(char)(returnValue & 0xff);
    if (character == '=') {
      return -2;
    }
    int byteValue=getByte(character);
    if (byteValue == -1) {
      continue;
    }
    return byteValue;
  }
}
