{
  Reader reader=new StringReader(_TEST_STRING_ENGLISH);
  int skipLength=3;
  byte[] bytes=new byte[_expectedBytesForEnglish.length - skipLength];
  try (ReaderInputStream readerInputStream=new ReaderInputStream(reader,StringPool.UTF8)){
    readerInputStream.skip(skipLength);
    int result=readerInputStream.read(bytes);
    Assert.assertEquals(_expectedBytesForEnglish.length - skipLength,result);
  }
   Assert.assertEquals(_expectedBytesForEnglish.length - skipLength,bytes.length);
  for (int i=skipLength; i < _expectedBytesForEnglish.length; i++) {
    Assert.assertEquals(_expectedBytesForEnglish[i],bytes[i - skipLength]);
  }
}
