{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select fileShortcutId, groupId, folderId, toFolderId " + "from DLFileShortcut");
    rs=ps.executeQuery();
    while (rs.next()) {
      long fileShortcutId=rs.getLong("fileShortcutId");
      long groupId=rs.getLong("groupId");
      long toFolderId=rs.getLong("toFolderId");
      long toGroupId=groupId;
      if (toFolderId != DLFolderConstants.DEFAULT_PARENT_FOLDER_ID) {
        DLFolder folder=DLFolderLocalServiceUtil.getFolder(toFolderId);
        toGroupId=folder.getGroupId();
      }
      runSQL("update DLFileShortcut set toGroupId = '" + toGroupId + "' where fileShortcutId = "+ fileShortcutId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
