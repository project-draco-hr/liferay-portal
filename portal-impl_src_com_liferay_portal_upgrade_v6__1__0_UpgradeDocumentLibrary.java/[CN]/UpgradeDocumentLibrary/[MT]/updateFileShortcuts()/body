{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select fileShortcutId, groupId, toFolderId, toName " + "from DLFileShortcut");
    rs=ps.executeQuery();
    while (rs.next()) {
      long fileShortcutId=rs.getLong("fileShortcutId");
      long groupId=rs.getLong("groupId");
      long toFolderId=rs.getLong("toFolderId");
      String toName=rs.getString("toName");
      DLFileEntry fileEntry=DLRepositoryServiceUtil.getFileEntry(groupId,toFolderId,toName);
      long toFileEntryId=fileEntry.getFileEntryId();
      runSQL("update DLFileShortcut set toGroupId = '" + toFileEntryId + "' where fileShortcutId = "+ fileShortcutId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
