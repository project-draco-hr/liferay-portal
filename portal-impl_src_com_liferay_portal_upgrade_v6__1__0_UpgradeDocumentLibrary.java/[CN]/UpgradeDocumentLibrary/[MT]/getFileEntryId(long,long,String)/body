{
  try (PreparedStatement ps=connection.prepareStatement("select fileEntryId from DLFileEntry where groupId = ? and " + "folderId = ? and name = ?")){
    ps.setLong(1,groupId);
    ps.setLong(2,folderId);
    ps.setString(3,name);
    try (ResultSet rs=ps.executeQuery()){
      if (rs.next()) {
        return rs.getLong("fileEntryId");
      }
      return 0;
    }
   }
 }
