{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select fileEntryId, extension from DLFileEntry");
    rs=ps.executeQuery();
    while (rs.next()) {
      long fileEntryId=rs.getLong("fileEntryId");
      String extension=rs.getString("extension");
      String mimeType=MimeTypesUtil.getContentTypeFromExtension(extension);
      runSQL("update DLFileEntry set mimeType = '" + mimeType + "' where fileEntryId = "+ fileEntryId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
