{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select fileEntryId, extension from DLFileEntry");
    rs=ps.executeQuery();
    while (rs.next()) {
      long fileEntryId=rs.getLong("fileEntryId");
      String extension=rs.getString("extension");
      String mimeType=MimeTypesUtil.getExtensionContentType(extension);
      runSQL("update DLFileEntry set mimeType = '" + mimeType + "' where fileEntryId = "+ fileEntryId);
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
}
