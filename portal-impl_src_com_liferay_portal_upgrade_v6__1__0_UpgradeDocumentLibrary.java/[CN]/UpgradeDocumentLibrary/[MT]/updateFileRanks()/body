{
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    ps=connection.prepareStatement("select groupId, fileRankId, folderId, name from DLFileRank");
    rs=ps.executeQuery();
    while (rs.next()) {
      long groupId=rs.getLong("groupId");
      long fileRankId=rs.getLong("fileRankId");
      long folderId=rs.getLong("folderId");
      String name=rs.getString("name");
      long fileEntryId=getFileEntryId(groupId,folderId,name);
      runSQL("update DLFileRank set fileEntryId = " + fileEntryId + " where fileRankId = "+ fileRankId);
    }
  }
  finally {
    DataAccess.cleanUp(ps,rs);
  }
  runSQL("alter table DLFileRank drop column folderId");
  runSQL("alter table DLFileRank drop column name");
}
