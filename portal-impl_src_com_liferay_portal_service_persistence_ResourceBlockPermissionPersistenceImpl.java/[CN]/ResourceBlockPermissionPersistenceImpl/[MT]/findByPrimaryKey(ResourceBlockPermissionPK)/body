{
  ResourceBlockPermission resourceBlockPermission=fetchByPrimaryKey(resourceBlockPermissionPK);
  if (resourceBlockPermission == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + resourceBlockPermissionPK);
    }
    throw new NoSuchResourceBlockPermissionException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + resourceBlockPermissionPK);
  }
  return resourceBlockPermission;
}
