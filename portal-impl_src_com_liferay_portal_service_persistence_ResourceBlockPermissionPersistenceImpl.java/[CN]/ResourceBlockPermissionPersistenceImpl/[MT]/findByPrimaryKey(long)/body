{
  ResourceBlockPermission resourceBlockPermission=fetchByPrimaryKey(resourceBlockPermissionId);
  if (resourceBlockPermission == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + resourceBlockPermissionId);
    }
    throw new NoSuchResourceBlockPermissionException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + resourceBlockPermissionId);
  }
  return resourceBlockPermission;
}
