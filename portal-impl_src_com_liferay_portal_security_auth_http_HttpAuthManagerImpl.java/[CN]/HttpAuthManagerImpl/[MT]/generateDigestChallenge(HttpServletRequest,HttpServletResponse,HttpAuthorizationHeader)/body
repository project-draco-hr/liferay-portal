{
  long companyId=PortalInstances.getCompanyId(request);
  String remoteAddress=request.getRemoteAddr();
  String nonce=NonceUtil.generate(companyId,remoteAddress);
  authorizationHeader.setAuthParameter(HttpAuthorizationHeader.AUTHPARAM_NONCE,nonce);
  response.setHeader(HttpHeaders.WWW_AUTHENTICATE,authorizationHeader.toString());
  response.setStatus(HttpServletResponse.SC_UNAUTHORIZED);
}
