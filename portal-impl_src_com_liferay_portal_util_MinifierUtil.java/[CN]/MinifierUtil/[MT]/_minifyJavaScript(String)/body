{
  StringWriter stringWriter=new StringWriter();
  try {
    JavaScriptCompressor javaScriptCompressor=new JavaScriptCompressor(new BufferedReader(new StringReader(content)),new JavaScriptErrorReporter());
    javaScriptCompressor.compress(stringWriter,_JS_LINE_BREAK,_JS_MUNGE,_JS_VERBOSE,_JS_PRESERVE_ALL_SEMICOLONS,_JS_DISABLE_OPTIMIZATIONS);
  }
 catch (  Exception e) {
    _log.error("JavaScript Minifier failed for\n" + content);
    stringWriter.append(content);
  }
  return stringWriter.toString();
}
