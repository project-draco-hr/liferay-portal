{
  try {
    Class<JavaScriptMinifier> javaScriptMinifierClass=(Class<JavaScriptMinifier>)Class.forName(PropsValues.JAVASCRIPT_MINIFIER);
    return javaScriptMinifierClass.newInstance();
  }
 catch (  Exception e) {
    if (_log.isErrorEnabled()) {
      _log.error("Could not instantiate " + PropsValues.JAVASCRIPT_MINIFIER + ". Returning YUIJavaScriptMinifier as default");
    }
    return new YUIJavaScriptMinifier();
  }
}
