{
  if (((wapColorSchemeId == null) && (_wapColorSchemeId != null)) || ((wapColorSchemeId != null) && (_wapColorSchemeId == null)) || ((wapColorSchemeId != null) && (_wapColorSchemeId != null) && !wapColorSchemeId.equals(_wapColorSchemeId))) {
    if (!XSS_ALLOW_WAPCOLORSCHEMEID) {
      wapColorSchemeId=XSSUtil.strip(wapColorSchemeId);
    }
    _wapColorSchemeId=wapColorSchemeId;
  }
}
