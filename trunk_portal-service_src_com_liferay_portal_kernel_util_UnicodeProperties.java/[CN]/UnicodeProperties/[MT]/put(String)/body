{
  line=line.trim();
  if (!_isComment(line)) {
    int pos=line.indexOf(CharPool.EQUAL);
    if (pos != -1) {
      String key=line.substring(0,pos).trim();
      String value=line.substring(pos + 1).trim();
      if (_safe) {
        value=_decode(value);
      }
      setProperty(key,value);
    }
 else {
      _log.error("Invalid property on line " + line);
    }
  }
}
