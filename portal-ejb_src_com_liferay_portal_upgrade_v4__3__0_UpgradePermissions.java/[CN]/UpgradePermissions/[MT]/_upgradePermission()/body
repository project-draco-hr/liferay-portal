{
  UpgradeTable upgradeTable=new LongPKUpgradeTableImpl(_TABLE_PERMISSION,_COLUMNS_PERMISSION,true);
  upgradeTable.updateTable();
  Map permissionIdMap=upgradeTable.getPKMap();
  UpgradeTable groupMap=new MapUpgradeTableImpl(_TABLE_GROUPS_PERMISSIONS,_COLUMNS_GROUP_PERMISSIONS,permissionIdMap);
  groupMap.updateTable();
  UpgradeTable roleMap=new MapUpgradeTableImpl(_TABLE_ROLES_PERMISSIONS,_COLUMNS_ROLES_PERMISSIONS,permissionIdMap);
  roleMap.updateTable();
  UpgradeTable userMap=new MapUpgradeTableImpl(_TABLE_USERS_PERMISSIONS,_COLUMNS_USERS_PERMISSIONS,permissionIdMap);
  userMap.updateTable();
  UpgradeTable orgGroupMap=new MapUpgradeTableImpl(_TABLE_ORG_GROUP_PERMISSION,_COLUMNS_ORG_GROUP_PERMISSION,permissionIdMap);
  orgGroupMap.updateTable();
}
