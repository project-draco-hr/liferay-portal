{
  List resources=ResourceLocalServiceUtil.getResources();
  ResourceLocalServiceUtil.deleteResources();
  for (int i=0; i < resources.size(); i++) {
    Resource oldResource=(Resource)resources.get(i);
    Resource newResource=ResourceLocalServiceUtil.addResource(oldResource.getCompanyId(),oldResource.getName(),oldResource.getTypeId(),oldResource.getScope(),oldResource.getPrimKey());
    PermissionLocalServiceUtil.updateResourceId(oldResource.getResourceId(),newResource.getResourceId());
  }
}
