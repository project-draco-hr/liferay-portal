{
  UpgradeTable upgradeTable=new LongPKUpgradeTableImpl(_TABLE_RESOURCE,_COLUMNS_RESOURCE);
  upgradeTable.updateTable();
  Map resourceIdMap=upgradeTable.getPKMap();
  Iterator itr=resourceIdMap.keySet().iterator();
  while (itr.hasNext()) {
    Long oldId=(Long)itr.next();
    Long newId=(Long)resourceIdMap.get(oldId);
    PermissionLocalServiceUtil.updateResourceId(oldId.longValue(),newId.longValue());
  }
}
