{
  resetOriginCSRFWhitelist();
  resetPortletCSRFWhitelist();
  resetPortletInvocationWhitelist();
  resetPortletInvocationWhitelistActions();
  Registry registry=RegistryUtil.getRegistry();
  _serviceTracker=registry.trackServices(registry.getFilter("(&(" + PropsKeys.AUTH_TOKEN_IGNORE_ACTIONS + "=*)"+ "(objectClass=java.lang.Object))"),new AuthTokenIgnoreActionsServiceTrackerCustomizer());
  _serviceTracker.open();
  registerPortalProperty();
}
