{
  int initialFileEntryCount=DLFileEntryLocalServiceUtil.getFileEntriesCount();
  addWikiPageAttachment();
  int firstFileEntryCount=DLFileEntryLocalServiceUtil.getFileEntriesCount();
  Assert.assertEquals(initialFileEntryCount + NEW_FILE_ENTRY,firstFileEntryCount);
  WikiNodeLocalServiceUtil.deleteNode(_page.getNodeId());
  int finalFileEntryCount=DLFileEntryLocalServiceUtil.getFileEntriesCount();
  Assert.assertEquals(firstFileEntryCount - NEW_FILE_ENTRY,finalFileEntryCount);
}
