{
  ManifestSummary manifestSummary=portletDataContext.getManifestSummary();
  if ((portletPreferences == null) || (manifestSummary.getModelAdditionCount(JournalArticle.class) > -1)) {
    return;
  }
  String articleId=portletPreferences.getValue("articleId",StringPool.BLANK);
  if (Validator.isNotNull(articleId)) {
    manifestSummary.addModelAdditionCount(new StagedModelType(JournalArticle.class),1);
  }
}
