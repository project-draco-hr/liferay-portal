{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Back to My Community")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Back to My Community"));
  selenium.waitForPageToLoad("30000");
  assertEquals("Test Community",selenium.getText("//div[@id='_145_myPlacesContainer']/ul/li[6]/a/span[1]"));
  assertEquals("Public",selenium.getText("//div[@id='_145_myPlacesContainer']/ul/li[6]/a/span[2]"));
  assertEquals("Test Community",selenium.getText("//div[@id='_145_myPlacesContainer']/ul/li[7]/a/span[1]"));
  assertEquals("Private",selenium.getText("//div[@id='_145_myPlacesContainer']/ul/li[7]/a/span[2]"));
}
