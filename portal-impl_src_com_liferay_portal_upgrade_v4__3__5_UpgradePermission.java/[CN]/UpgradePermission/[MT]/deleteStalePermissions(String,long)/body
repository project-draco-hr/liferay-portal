{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=HibernateUtil.getConnection();
    ps=con.prepareStatement(sql);
    rs=ps.executeQuery();
    while (rs.next()) {
      long permissionId=rs.getLong("permissionId");
      runSQL("delete from Groups_Permissions where groupId = " + guestGroupId + " and permissionId = "+ permissionId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
