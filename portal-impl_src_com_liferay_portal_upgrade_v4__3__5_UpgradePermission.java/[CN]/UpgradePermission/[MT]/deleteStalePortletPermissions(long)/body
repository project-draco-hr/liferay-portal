{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=HibernateUtil.getConnection();
    Iterator itr=getPlidsWithStalePermissions(guestGroupId).iterator();
    while (itr.hasNext()) {
      Long plid=(Long)itr.next();
      ps=con.prepareStatement("select primKey from Resource_ where primKey like ?");
      ps.setString(1,plid + "_LAYOUT_%");
      rs=ps.executeQuery();
      while (rs.next()) {
        String primKey=rs.getString("primKey");
        String sql=getStalePermissionsSQL("19","Layout","plid",guestGroupId);
        sql=StringUtil.replace(sql,"Layout.plid = Resource_.primKey ","Layout.plid = " + plid + " ");
        deleteStalePermissions(sql,guestGroupId);
      }
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
