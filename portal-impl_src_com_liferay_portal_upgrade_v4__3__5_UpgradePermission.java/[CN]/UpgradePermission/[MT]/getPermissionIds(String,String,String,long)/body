{
  List permissionIds=new ArrayList();
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=HibernateUtil.getConnection();
    ps=con.prepareStatement("select " + tablePKCol + " from "+ tableName+ " "+ "where groupId != "+ guestGroupId);
    rs=ps.executeQuery();
    while (rs.next()) {
      String primKey=String.valueOf(rs.getLong(tablePKCol));
      permissionIds.addAll(getPermissionIds(className,primKey,guestGroupId));
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return permissionIds;
}
