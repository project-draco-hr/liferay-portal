{
  String sql="select Groups_Permissions.permissionId from Groups_Permissions " + "inner join Permission_ on Permission_.permissionId = " + "Groups_Permissions.permissionId "+ "inner join Resource_ on Resource_.resourceId = "+ "Permission_.resourceId "+ "inner join ResourceCode on ResourceCode.codeId = "+ "Resource_.codeId and Resource_.primKey = '" + primKey + "' "+ "where Groups_Permissions.groupId = "+ guestGroupId+ ";";
