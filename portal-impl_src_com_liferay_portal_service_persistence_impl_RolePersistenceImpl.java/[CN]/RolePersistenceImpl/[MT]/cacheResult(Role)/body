{
  entityCache.putResult(RoleModelImpl.ENTITY_CACHE_ENABLED,RoleImpl.class,role.getPrimaryKey(),role);
  finderCache.putResult(FINDER_PATH_FETCH_BY_C_N,new Object[]{role.getCompanyId(),role.getName()},role);
  finderCache.putResult(FINDER_PATH_FETCH_BY_C_C_C,new Object[]{role.getCompanyId(),role.getClassNameId(),role.getClassPK()},role);
  role.resetOriginalValues();
}
