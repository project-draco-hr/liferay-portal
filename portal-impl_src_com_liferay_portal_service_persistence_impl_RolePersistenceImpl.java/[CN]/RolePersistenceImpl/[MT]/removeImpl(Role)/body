{
  role=toUnwrappedModel(role);
  roleToGroupTableMapper.deleteLeftPrimaryKeyTableMappings(0,role.getPrimaryKey());
  roleToUserTableMapper.deleteLeftPrimaryKeyTableMappings(0,role.getPrimaryKey());
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(role)) {
      role=(Role)session.get(RoleImpl.class,role.getPrimaryKeyObj());
    }
    if (role != null) {
      session.delete(role);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (role != null) {
    clearCache(role);
  }
  return role;
}
