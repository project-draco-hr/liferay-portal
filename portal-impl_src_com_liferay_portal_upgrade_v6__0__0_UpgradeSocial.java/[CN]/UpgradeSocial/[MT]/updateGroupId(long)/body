{
  Object[] group=getGroup(groupId);
  if (group == null) {
    return;
  }
  long classNameId=(Long)group[0];
  if (classNameId != PortalUtil.getClassNameId("com.liferay.portal.model.Layout")) {
    return;
  }
  long classPK=(Long)group[1];
  Object[] layout=getLayout(classPK);
  if (layout == null) {
    return;
  }
  long layoutGroupId=(Long)layout[0];
  runSQL("update SocialActivity set groupId = " + layoutGroupId + " where groupId = "+ groupId);
}
