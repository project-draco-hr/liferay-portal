{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps=connection.prepareStatement("select distinct(groupId) from SocialActivity where groupId " + "> 0");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      long groupId=rs.getLong("groupId");
      try {
        updateGroupId(groupId);
      }
 catch (      Exception e) {
        if (_log.isWarnEnabled()) {
          _log.warn(e);
        }
      }
    }
  }
 }
