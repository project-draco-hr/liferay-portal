{
  try {
    MethodWrapper methodWrapper=new MethodWrapper(IndexerImpl.class.getName(),"updateMessage",new Object[]{new LongWrapper(companyId),new LongWrapper(groupId),userName,new LongWrapper(categoryId),new LongWrapper(threadId),new LongWrapper(messageId),title,content});
    IndexProducer.produce(methodWrapper);
  }
 catch (  Exception e) {
    if (e instanceof IOException) {
      throw (IOException)e;
    }
 else {
      _log.error(e);
    }
  }
}
