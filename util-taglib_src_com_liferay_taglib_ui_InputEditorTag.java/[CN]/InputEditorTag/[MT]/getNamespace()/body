{
  LiferayPortletRequest portletRequest=(LiferayPortletRequest)request.getAttribute(JavaConstants.JAVAX_PORTLET_REQUEST);
  LiferayPortletResponse portletResponse=(LiferayPortletResponse)request.getAttribute(JavaConstants.JAVAX_PORTLET_RESPONSE);
  if ((portletRequest == null) || (portletResponse == null)) {
    return AUIUtil.getNamespace(request);
  }
  return AUIUtil.getNamespace(portletRequest,portletResponse);
}
