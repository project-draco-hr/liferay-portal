{
  Session session=null;
  try {
    session=openSession();
    session.delete(announcementFlag);
    session.flush();
    return announcementFlag;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(AnnouncementFlag.class.getName());
  }
}
