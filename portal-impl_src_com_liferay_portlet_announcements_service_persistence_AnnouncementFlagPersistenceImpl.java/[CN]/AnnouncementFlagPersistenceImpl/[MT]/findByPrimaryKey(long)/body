{
  AnnouncementFlag announcementFlag=fetchByPrimaryKey(flagId);
  if (announcementFlag == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No AnnouncementFlag exists with the primary key " + flagId);
    }
    throw new NoSuchAnnouncementFlagException("No AnnouncementFlag exists with the primary key " + flagId);
  }
  return announcementFlag;
}
