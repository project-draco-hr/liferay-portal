{
  AnnouncementFlag announcementFlag=fetchByPrimaryKey(announcementFlagId);
  if (announcementFlag == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No AnnouncementFlag exists with the primary key " + announcementFlagId);
    }
    throw new NoSuchAnnouncementFlagException("No AnnouncementFlag exists with the primary key " + announcementFlagId);
  }
  return announcementFlag;
}
