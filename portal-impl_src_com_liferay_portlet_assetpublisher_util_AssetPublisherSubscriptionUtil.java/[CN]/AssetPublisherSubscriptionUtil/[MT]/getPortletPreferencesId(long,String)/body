{
  List<PortletPreferences> portletPreferencesList=PortletPreferencesLocalServiceUtil.getPortletPreferences(plid,PortletKeys.ASSET_PUBLISHER);
  for (  PortletPreferences portletPreferences : portletPreferencesList) {
    if (portletPreferences.getPortletId().equals(instanceId)) {
      return portletPreferences.getPortletPreferencesId();
    }
  }
  return 0;
}
