{
  try {
    JournalStructureLocalService journalStructureLocalService=JournalStructureLocalServiceFactory.getService();
    return journalStructureLocalService.addStructure(userId,structureId,autoStructureId,plid,name,description,xsd,addCommunityPermissions,addGuestPermissions);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
