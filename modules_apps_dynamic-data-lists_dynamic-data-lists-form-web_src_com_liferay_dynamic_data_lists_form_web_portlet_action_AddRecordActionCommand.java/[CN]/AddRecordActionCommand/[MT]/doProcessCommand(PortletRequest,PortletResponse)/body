{
  long groupId=ParamUtil.getLong(portletRequest,"groupId");
  long recordSetId=ParamUtil.getLong(portletRequest,"recordSetId");
  DDMForm ddmForm=getDDMForm(recordSetId);
  DDMFormValues ddmFormValues=_ddmFormValuesFactory.create(portletRequest,ddmForm);
  ServiceContext serviceContext=ServiceContextFactory.getInstance(DDLRecord.class.getName(),portletRequest);
  _ddlRecordService.addRecord(groupId,recordSetId,DDLRecordConstants.DISPLAY_INDEX_DEFAULT,ddmFormValues,serviceContext);
}
