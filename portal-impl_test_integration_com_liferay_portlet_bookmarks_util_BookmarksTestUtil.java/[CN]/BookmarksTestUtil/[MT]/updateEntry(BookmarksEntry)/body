{
  ServiceContext serviceContext=ServiceTestUtil.getServiceContext(entry.getGroupId());
  serviceContext.setCommand(Constants.UPDATE);
  serviceContext.setLayoutFullURL("http://localhost");
  return BookmarksEntryLocalServiceUtil.updateEntry(TestPropsValues.getUserId(),entry.getEntryId(),entry.getGroupId(),entry.getFolderId(),ServiceTestUtil.randomString(),entry.getUrl(),entry.getDescription(),serviceContext);
}
