{
  try (LoggingTimer loggingTimer=new LoggingTimer()){
    if (!hasColumn("CalendarBooking","recurringCalendarBookingId")) {
      runSQL("alter table CalendarBooking add " + "recurringCalendarBookingId LONG null");
    }
    runSQL("update CalendarBooking set recurringCalendarBookingId = " + "calendarBookingId");
  }
 }
