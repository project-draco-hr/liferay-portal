{
  long pageId=GetterUtil.getLong(primaryKey);
  WikiPage page=null;
  try {
    page=WikiPageLocalServiceUtil.getPageByPageId(pageId);
  }
 catch (  PortalException pe) {
    return StringPool.BLANK;
  }
  String name=WikiPage.class.getSimpleName();
  boolean showInput=false;
  if (WikiPagePermissionChecker.contains(_permissionChecker,page,ActionKeys.DELETE)) {
    showInput=true;
  }
  if (!showInput) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(5);
  sb.append("['");
  sb.append(_liferayPortletResponse.getNamespace());
  sb.append(RowChecker.ROW_IDS);
  sb.append(WikiPage.class.getSimpleName());
  sb.append("']");
  String checkBoxRowIds=sb.toString();
  return getRowCheckBox(request,checked,disabled,_liferayPortletResponse.getNamespace() + RowChecker.ROW_IDS + name+ "",page.getTitle(),checkBoxRowIds,"'#" + getAllRowIds() + "'",StringPool.BLANK);
}
