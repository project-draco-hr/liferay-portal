{
  long leftPrimaryKey=1;
  long rightPrimaryKey=2;
  Assert.assertFalse(_tableMapperImpl.containsTableMapping(leftPrimaryKey,rightPrimaryKey));
  PortalCache<Long,long[]> leftToRightPortalCache=_tableMapperImpl.leftToRightPortalCache;
  leftToRightPortalCache.remove(leftPrimaryKey);
  _mappingStore.put(leftPrimaryKey,new long[]{rightPrimaryKey});
  Assert.assertTrue(_tableMapperImpl.containsTableMapping(leftPrimaryKey,rightPrimaryKey));
}
