{
  Assert.assertSame(_dataSource,dataSource);
  Assert.assertEquals("SELECT " + _RIGHT_COLUMN_NAME + " FROM "+ _TABLE_NAME+ " WHERE "+ _LEFT_COLUMN_NAME+ " = ?",sql);
  Assert.assertArrayEquals(new ParamSetter[]{ParamSetter.BIGINT},paramSetters);
  Assert.assertSame(RowMapper.PRIMARY_KEY,rowMapper);
}
