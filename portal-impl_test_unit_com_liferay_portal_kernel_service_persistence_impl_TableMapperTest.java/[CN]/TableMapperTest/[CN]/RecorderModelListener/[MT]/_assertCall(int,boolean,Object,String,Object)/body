{
  if (called) {
    Assert.assertSame(_classPKs[index],classPK);
    Assert.assertEquals(_associationClassNames[index],associationClassName);
    Assert.assertSame(_associationClassPKs[index],associationClassPK);
  }
 else {
    Assert.assertFalse("Called onAfterAddAssociation",_markers[index]);
  }
}
