{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForElementPresent("link=Blogs Permissions Page");
  selenium.clickAt("link=Blogs Permissions Page",RuntimeVariables.replace("Blogs Permissions Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("Blogs Entry Title Temporary"),selenium.getText("//div[@class='entry-title']/h2/a"));
  assertEquals(RuntimeVariables.replace("Blogs Entry Content Temporary"),selenium.getText("//div[@class='entry-body']/p"));
  assertEquals(RuntimeVariables.replace("Showing 1 result."),selenium.getText("//div[@class='search-results']"));
  selenium.type("//input[@id='_33_keywords']",RuntimeVariables.replace("Blogs Entry Title Temporary"));
  selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace("Search"));
  selenium.waitForPageToLoad("30000");
  Thread.sleep(5000);
  assertEquals(RuntimeVariables.replace("Blogs Entry Title Temporary"),selenium.getText("//td[2]/a"));
}
