{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select organizationId from Organization_ where uuid_ is null");
    rs=ps.executeQuery();
    while (rs.next()) {
      long organizationId=rs.getLong("organizationId");
      runSQL("update Organization_ set uuid_ = '" + PortalUUIDUtil.generate() + "' where organizationId = "+ organizationId);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
