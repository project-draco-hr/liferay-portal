{
  ThemeDisplay themeDisplay=(ThemeDisplay)_request.getAttribute(WebKeys.THEME_DISPLAY);
  HttpServletRequest request=PortletContainerUtil.setupOptionalRenderParameters(_request,null,_columnId,_columnPos,_columnCount);
  _restrictPortletServletRequest=(RestrictPortletServletRequest)request;
  try {
    themeDisplay=(ThemeDisplay)themeDisplay.clone();
    request.setAttribute(WebKeys.THEME_DISPLAY,themeDisplay);
    return _render(request,_response);
  }
 catch (  Exception e) {
    Thread currentThread=Thread.currentThread();
    if (!currentThread.isInterrupted()) {
      throw e;
    }
    return null;
  }
}
