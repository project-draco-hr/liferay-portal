{
  _properties.put(_MULTIPLE_KEY,_RESOURCE_MULTIPLE_VALUES);
  when(_mockLocationVariableResolver.isLocationVariable(_RESOURCE_MULTIPLE_VALUES)).thenReturn(true);
  final String expectedValue="resourceValue0,resourceValue1,resourceValue2";
  when(_mockLocationVariableResolver.resolve(_RESOURCE_MULTIPLE_VALUES)).thenReturn(expectedValue);
  Assert.assertArrayEquals(expectedValue.split(","),_propertiesSettings.getValues(_MULTIPLE_KEY,null));
}
