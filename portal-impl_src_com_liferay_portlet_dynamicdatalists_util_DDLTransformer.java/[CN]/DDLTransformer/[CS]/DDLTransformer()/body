{
  _transformerListenerClassNames=Collections.unmodifiableSet(SetUtil.fromArray(PropsUtil.getArray(PropsKeys.DYNAMIC_DATA_LISTS_TRANSFORMER_LISTENER)));
  Set<String> langTypes=TemplateManagerUtil.getSupportedLanguageTypes(PropsKeys.DYNAMIC_DATA_LISTS_ERROR_TEMPLATE);
  for (  String langType : langTypes) {
    String errorTemplateId=PropsUtil.get(PropsKeys.DYNAMIC_DATA_LISTS_ERROR_TEMPLATE,new Filter(langType));
    if (Validator.isNotNull(errorTemplateId)) {
      _errorTemplateIds.put(langType,errorTemplateId);
    }
  }
}
