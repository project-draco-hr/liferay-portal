{
  PowerMockito.mockStatic(PortletLocalServiceUtil.class);
  Mockito.when(PortletLocalServiceUtil.getPortletById(_layout.getCompanyId(),_PORTLET_ID)).thenReturn(getGroupPortlet());
  PowerMockito.mockStatic(LayoutPermissionUtil.class);
  Mockito.when(LayoutPermissionUtil.contains(Mockito.any(PermissionChecker.class),Mockito.eq(_layout),Mockito.eq(ActionKeys.UPDATE))).thenReturn(true);
  PowerMockito.mockStatic(PermissionThreadLocal.class);
  Mockito.when(PermissionThreadLocal.getPermissionChecker()).thenReturn(PowerMockito.mock(PermissionChecker.class));
  long siteGroupId=_layout.getGroupId();
  boolean modeEditGuest=true;
  PortletPreferencesFactoryUtil.getPortletPreferencesIds(siteGroupId,_USER_ID,_layout,_PORTLET_ID,modeEditGuest);
