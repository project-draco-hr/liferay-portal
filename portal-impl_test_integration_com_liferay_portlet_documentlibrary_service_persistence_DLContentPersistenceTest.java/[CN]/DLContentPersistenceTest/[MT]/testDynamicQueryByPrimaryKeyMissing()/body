{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(DLContent.class,DLContent.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("contentId",ServiceTestUtil.nextLong()));
  List<DLContent> result=_persistence.findWithDynamicQuery(dynamicQuery);
  Assert.assertEquals(0,result.size());
}
