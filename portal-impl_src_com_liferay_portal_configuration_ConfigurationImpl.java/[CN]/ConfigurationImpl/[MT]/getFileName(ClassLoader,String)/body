{
  URL url=classLoader.getResource(name + ".properties");
  String protocol=url.getProtocol();
  if (protocol.equals("jar") || protocol.equals("vfszip") || protocol.equals("wsjar")|| protocol.equals("zip")) {
    name=url.toExternalForm();
  }
 else {
    try {
      name=new URI(url.getPath()).getPath();
    }
 catch (    URISyntaxException urise) {
      name=url.getFile();
    }
  }
  int pos=name.lastIndexOf(".properties");
  if (pos != -1) {
    name=name.substring(0,pos);
  }
  return name;
}
