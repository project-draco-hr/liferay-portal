{
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Breadcrumb Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Breadcrumb Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals("Breadcrumb Test Page",selenium.getText("//div[2]/div/div/ul/li[4]/a"));
  assertFalse(selenium.isElementPresent("//div[2]/div/div/ul/li[5]/a"));
  selenium.click(RuntimeVariables.replace("link=Child Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals("Breadcrumb Test Page",selenium.getText("//div[2]/div/div/ul/li[4]/a"));
  assertEquals("Child Test Page",selenium.getText("//div[2]/div/div/ul/li[5]/a"));
}
