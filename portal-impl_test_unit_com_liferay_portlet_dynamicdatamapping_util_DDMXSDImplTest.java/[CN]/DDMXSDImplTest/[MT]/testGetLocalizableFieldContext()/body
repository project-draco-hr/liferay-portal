{
  Element rootElement=_document.getRootElement();
  Element fieldElement=addTextElement(rootElement,"Localizable","Localizable",true);
  Map<String,Object> fieldContext=_ddmXSD.getFieldContext(_mockHttpServletRequest,_mockHttpServletResponse,_PORTLET_NAMESPACE,_NAMESPACE,fieldElement,LocaleUtil.US);
  Assert.assertFalse(fieldContext.containsKey("disabled"));
}
