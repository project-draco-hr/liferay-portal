{
  Element rootElement=_document.getRootElement();
  Element fieldElement=addTextElement(rootElement,"Unlocalizable","Unlocalizable",false);
  Map<String,Object> fieldContext=_ddmXSD.getFieldContext(_mockPageContext,_PORTLET_NAMESPACE,_NAMESPACE,fieldElement,LocaleUtil.US);
  Assert.assertFalse(fieldContext.containsKey("disabled"));
}
