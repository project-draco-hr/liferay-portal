{
  Element root=_document.getRootElement();
  Element field=addTextElement(root,"Unlocalizable","Unloc",false);
  Locale locale=LocaleUtil.BRAZIL;
  Map<String,Object> fieldContext=_ddmXSDImpl.getFieldContext(_mockPageContext,"portlet","namespace",field,locale);
  Assert.assertEquals(Boolean.TRUE.toString(),fieldContext.get("disabled"));
}
