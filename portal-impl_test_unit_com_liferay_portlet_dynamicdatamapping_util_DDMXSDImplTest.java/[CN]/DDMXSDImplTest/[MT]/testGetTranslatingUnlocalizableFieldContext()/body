{
  Element root=_document.getRootElement();
  Element field=addTextElement(root,"Unlocalizable","Unlocalizable",false);
  Map<String,Object> fieldContext=_ddmXSD.getFieldContext(_mockPageContext,_portletNamespace,_namespace,field,LocaleUtil.BRAZIL);
  Assert.assertEquals(Boolean.TRUE.toString(),fieldContext.get("disabled"));
}
