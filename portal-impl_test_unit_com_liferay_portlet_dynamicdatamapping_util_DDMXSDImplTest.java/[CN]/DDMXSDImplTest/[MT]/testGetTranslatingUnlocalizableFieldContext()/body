{
  Element rootElement=_document.getRootElement();
  Element fieldElement=addTextElement(rootElement,"Unlocalizable","Unlocalizable",false);
  Map<String,Object> fieldContext=_ddmXSD.getFieldContext(_mockHttpServletRequest,_mockHttpServletResponse,_PORTLET_NAMESPACE,_NAMESPACE,fieldElement,LocaleUtil.BRAZIL);
  Assert.assertEquals(Boolean.TRUE.toString(),fieldContext.get("disabled"));
}
