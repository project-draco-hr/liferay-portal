{
  if (obj == null) {
    return false;
  }
  DDLRecord ddlRecord=null;
  try {
    ddlRecord=(DDLRecord)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=ddlRecord.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
