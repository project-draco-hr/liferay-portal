{
  try {
    com.liferay.sync.model.SyncDevice returnValue=SyncDeviceServiceUtil.registerSyncDevice(type,buildNumber,featureSet,uuid);
    return com.liferay.sync.model.SyncDeviceSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
