{
  selenium.click("link=Test Poll Question");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Area");
  selenium.waitForPopUp("viewChart","30000");
  selenium.selectWindow("viewChartWindow");
  selenium.close();
  selenium.selectWindow("null");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Horizontal Bar")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click("link=Horizontal Bar");
  selenium.waitForPopUp("viewChart","30000");
  selenium.selectWindow("viewChartWindow");
  selenium.close();
  selenium.selectWindow("null");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Line")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
