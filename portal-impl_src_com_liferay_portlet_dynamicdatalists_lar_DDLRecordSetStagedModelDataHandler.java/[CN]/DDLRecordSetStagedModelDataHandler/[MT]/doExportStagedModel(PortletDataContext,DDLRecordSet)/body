{
  Element recordSetElement=portletDataContext.getExportDataStagedModelElement(recordSet);
  portletDataContext.addClassedModel(recordSetElement,StagedModelPathUtil.getPath(recordSet),recordSet,DDLPortletDataHandler.NAMESPACE);
  DDMStructure ddmStructure=recordSet.getDDMStructure();
  StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,ddmStructure);
  List<DDMTemplate> ddmTemplates=ddmStructure.getTemplates();
  for (  DDMTemplate ddmTemplate : ddmTemplates) {
    StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,ddmTemplate);
  }
}
