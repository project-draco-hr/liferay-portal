{
  DDLRecordSet existingRecordSet=DDLRecordSetLocalServiceUtil.fetchDDLRecordSetByUuidAndGroupId(uuid,portletDataContext.getCompanyGroupId());
  Map<Long,Long> recordSetIds=(Map<Long,Long>)portletDataContext.getNewPrimaryKeysMap(DDLRecordSet.class);
  recordSetIds.put(recordSetId,existingRecordSet.getRecordSetId());
}
