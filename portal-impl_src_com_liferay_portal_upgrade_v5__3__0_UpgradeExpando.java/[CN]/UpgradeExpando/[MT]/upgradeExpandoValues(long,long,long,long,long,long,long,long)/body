{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select * from ExpandoValue where tableId = ? and rowId_ = ?");
    ps.setLong(1,wolTableId);
    ps.setLong(2,wolRowId);
    rs=ps.executeQuery();
    while (rs.next()) {
      long valueId=rs.getLong("valueId");
      long columnId=rs.getLong("columnId");
      long newTableId=0;
      long newRowId=0;
      if (columnId == scColumnId) {
        newRowId=scRowId;
        newTableId=scTableId;
      }
 else       if (columnId == snColumnId) {
        newRowId=snRowId;
        newTableId=snTableId;
      }
      ps=con.prepareStatement("update ExpandoValue set tableId = ?, rowId_ = ? where " + "valueId = ?");
      ps.setLong(1,newTableId);
      ps.setLong(2,newRowId);
      ps.setLong(3,valueId);
      ps.executeUpdate();
      ps.close();
    }
    ps.close();
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
