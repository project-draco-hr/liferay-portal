{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select * from ExpandoColumn where tableId = ?");
    ps.setLong(1,wolTableId);
    rs=ps.executeQuery();
    long scColumnId=0;
    long snColumnId=0;
    while (rs.next()) {
      long wolColumnId=rs.getLong("columnId");
      String name=rs.getString("name");
      long newTableId=0;
      if (name.equals("aboutMe")) {
        newTableId=snTableId;
        snColumnId=wolColumnId;
      }
 else       if (name.equals("jiraUserId")) {
        newTableId=scTableId;
        scColumnId=wolColumnId;
      }
      ps=con.prepareStatement("update ExpandoColumn set tableId = ? where tableId = ? " + "and name = ?");
      ps.setLong(1,newTableId);
      ps.setLong(2,wolTableId);
      ps.setString(3,name);
      ps.executeUpdate();
      ps.close();
    }
    upgradeRows(scColumnId,scTableId,snColumnId,snTableId,wolTableId);
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
