{
  if (key == null) {
    throw new IllegalArgumentException("Key is null");
  }
  String value=doGetValue(key);
  if (Validator.isNull(value) && (parentSettings != null)) {
    value=parentSettings.getValue(key,defaultValue);
  }
  if (Validator.isNull(value)) {
    value=defaultValue;
  }
  return value;
}
