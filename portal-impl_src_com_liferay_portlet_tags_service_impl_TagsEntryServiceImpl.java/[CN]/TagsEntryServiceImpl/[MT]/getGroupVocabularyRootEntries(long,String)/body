{
  List<TagsEntry> tagsEntries=tagsEntryLocalService.getGroupVocabularyRootEntries(groupId,vocabularyName);
  Iterator<TagsEntry> itr=tagsEntries.iterator();
  while (itr.hasNext()) {
    TagsEntry tagEntry=itr.next();
    if (!TagsEntryPermission.contains(getPermissionChecker(),tagEntry,ActionKeys.VIEW)) {
      itr.remove();
    }
  }
  return tagsEntries;
}
