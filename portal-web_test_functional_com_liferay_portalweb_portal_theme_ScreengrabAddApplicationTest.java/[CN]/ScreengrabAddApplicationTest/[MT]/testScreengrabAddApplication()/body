{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.waitForElementPresent("link=Test Page 8");
  selenium.click(RuntimeVariables.replace("link=Test Page 8"));
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Add Application");
  selenium.waitForElementPresent("//div[@id='Collaboration-Blogs']");
  FileUtil.mkdirs(RuntimeVariables.replace("L:\\portal\\build\\portal-web\\test-output\\brochure\\"));
  selenium.captureEntirePageScreenshot(RuntimeVariables.replace("L:\\portal\\build\\portal-web\\test-output\\brochure\\ScreengrabTest10.jpg"),"");
}
