{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  assertEquals(RuntimeVariables.replace("Welcome Joe Bloggs!"),selenium.getText("//h2[@class='user-greeting']/span"));
  selenium.mouseOver("//h2[@class='user-greeting']/span");
  selenium.clickAt("//h2[@class='user-greeting']/span",RuntimeVariables.replace("Welcome Joe Bloggs!"));
  selenium.waitForVisible("link=Sign Out");
  selenium.clickAt("link=Sign Out",RuntimeVariables.replace("Sign Out"));
  selenium.waitForPageToLoad("30000");
  selenium.waitForVisible("//input[@value='Sign In']");
  assertTrue(selenium.isVisible("//input[@value='Sign In']"));
}
