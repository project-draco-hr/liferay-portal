{
  try {
    AddressLocalService addressLocalService=AddressLocalServiceFactory.getService();
    return addressLocalService.updateAddress(addressId,street1,street2,street3,city,zip,regionId,countryId,typeId,mailing,primary);
  }
 catch (  com.liferay.portal.PortalException pe) {
    throw pe;
  }
catch (  com.liferay.portal.SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new com.liferay.portal.SystemException(e);
  }
}
