{
  StringBuilder sb=new StringBuilder();
  sb.append(methodName);
  sb.append(_PARAMS_SEPARATOR);
  for (  String param : params) {
    sb.append(StringPool.PERIOD);
    sb.append(param);
  }
  sb.append(_ARGS_SEPARATOR);
  for (  Object arg : args) {
    sb.append(StringPool.PERIOD);
    sb.append(String.valueOf(arg));
  }
  return sb.toString();
}
