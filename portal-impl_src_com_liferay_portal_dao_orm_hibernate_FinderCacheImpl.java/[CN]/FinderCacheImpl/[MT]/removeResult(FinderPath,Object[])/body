{
  if (!PropsValues.VALUE_OBJECT_FINDER_CACHE_ENABLED || !finderPath.isFinderCacheEnabled() || !CacheRegistry.isActive()) {
    return;
  }
  PortalCache portalCache=_getPortalCache(finderPath.getClassName());
  String key=_encodeKey(finderPath.getMethodName(),finderPath.getParams(),args);
  _multiVMPool.remove(portalCache,key);
}
