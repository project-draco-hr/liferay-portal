{
  InputStream is=null;
  long size=0;
  if (bytes != null) {
    is=new UnsyncByteArrayInputStream(bytes);
    size=bytes.length;
  }
  return updateFileEntry(userId,groupId,folderId,newFolderId,name,sourceFileName,title,description,versionDescription,majorVersion,extraSettings,is,size,serviceContext);
}
