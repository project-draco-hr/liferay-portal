{
  if (isStartWorkflow) {
    WorkflowInstanceLink workflowInstanceLink=workflowInstanceLinkLocalService.getWorkflowInstanceLink(fileVersion.getCompanyId(),fileVersion.getGroupId(),DLFileEntry.class.getName(),fileVersion.getFileVersionId());
    workflowInstanceLink.setClassPK(newFileVersionId);
    workflowInstanceLinkPersistence.update(workflowInstanceLink,false);
  }
}
