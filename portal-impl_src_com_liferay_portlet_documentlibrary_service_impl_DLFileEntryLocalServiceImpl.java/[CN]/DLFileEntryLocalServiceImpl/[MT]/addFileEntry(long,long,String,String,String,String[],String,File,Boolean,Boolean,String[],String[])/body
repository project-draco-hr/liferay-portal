{
  if (!PropsValues.WEBDAV_LITMUS) {
    if ((file == null) || (file.length() == 0)) {
      throw new FileSizeException();
    }
  }
  InputStream is=null;
  try {
    is=new BufferedInputStream(new FileInputStream(file));
    return addFileEntry(null,userId,folderId,name,title,description,tagsEntries,extraSettings,is,file.length(),addCommunityPermissions,addGuestPermissions,communityPermissions,guestPermissions);
  }
 catch (  FileNotFoundException fnfe) {
    throw new FileSizeException();
  }
 finally {
    try {
      if (is != null) {
        is.close();
      }
    }
 catch (    IOException ioe) {
      _log.error(ioe);
    }
  }
}
