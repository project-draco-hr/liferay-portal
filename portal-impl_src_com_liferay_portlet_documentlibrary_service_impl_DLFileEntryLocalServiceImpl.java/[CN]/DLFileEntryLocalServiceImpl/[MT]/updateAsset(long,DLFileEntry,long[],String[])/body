{
  String mimeType=MimeTypesUtil.getContentType(fileEntry.getName());
  assetLocalService.updateAsset(userId,fileEntry.getGroupId(),DLFileEntry.class.getName(),fileEntry.getFileEntryId(),assetCategoryIds,assetTagNames,true,null,null,null,null,mimeType,fileEntry.getTitle(),fileEntry.getDescription(),null,null,0,0,null,false);
}
