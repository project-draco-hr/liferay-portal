{
  InputStream is=null;
  try {
    long size=0;
    if ((file != null) && (file.length() > 0)) {
      is=new BufferedInputStream(new FileInputStream(file));
      size=file.length();
    }
    return updateFileEntry(userId,groupId,folderId,newFolderId,name,sourceFileName,title,description,extraSettings,is,size,serviceContext);
  }
 catch (  FileNotFoundException fnfe) {
    throw new NoSuchFileException();
  }
 finally {
    try {
      if (is != null) {
        is.close();
      }
    }
 catch (    IOException ioe) {
      _log.error(ioe);
    }
  }
}
