{
  if (!PropsValues.WEBDAV_LITMUS) {
    if (bytes == null) {
      throw new FileSizeException();
    }
  }
  InputStream is=new UnsyncByteArrayInputStream(bytes);
  return addFileEntry(uuid,userId,groupId,folderId,name,title,description,versionDescription,extraSettings,is,bytes.length,serviceContext);
}
