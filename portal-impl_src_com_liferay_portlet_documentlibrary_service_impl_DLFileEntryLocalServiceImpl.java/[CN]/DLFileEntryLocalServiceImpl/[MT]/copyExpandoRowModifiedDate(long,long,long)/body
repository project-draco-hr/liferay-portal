{
  ExpandoTable expandoTable=null;
  try {
    expandoTable=expandoTableLocalService.getDefaultTable(companyId,DLFileEntry.class.getName());
  }
 catch (  NoSuchTableException nste) {
    return;
  }
  Date srcModifiedDate=null;
  try {
    ExpandoRow srcExpandoRow=expandoRowLocalService.getRow(expandoTable.getTableId(),srcFileVersionId);
    srcModifiedDate=srcExpandoRow.getModifiedDate();
  }
 catch (  NoSuchRowException nsre) {
    return;
  }
  try {
    ExpandoRow destExpandoRow=expandoRowLocalService.getRow(expandoTable.getTableId(),destFileVersionId);
    destExpandoRow.setModifiedDate(srcModifiedDate);
    expandoRowLocalService.updateExpandoRow(destExpandoRow);
  }
 catch (  NoSuchRowException nsre) {
  }
}
