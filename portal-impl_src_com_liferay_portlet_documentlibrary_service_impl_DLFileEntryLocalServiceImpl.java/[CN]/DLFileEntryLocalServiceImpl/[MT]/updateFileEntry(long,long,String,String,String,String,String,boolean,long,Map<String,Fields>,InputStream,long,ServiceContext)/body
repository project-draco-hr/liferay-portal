{
  DLFileEntry dlFileEntry=dlFileEntryPersistence.findByPrimaryKey(fileEntryId);
  String extension=getExtension(title,sourceFileName);
  String extraSettings=StringPool.BLANK;
  fileEntryTypeId=getFileEntryTypeId(dlFileEntry.getGroupId(),dlFileEntry.getFolderId(),fileEntryTypeId);
  return updateFileEntry(userId,fileEntryId,sourceFileName,extension,mimeType,title,description,changeLog,majorVersion,extraSettings,fileEntryTypeId,fieldsMap,is,size,serviceContext);
}
