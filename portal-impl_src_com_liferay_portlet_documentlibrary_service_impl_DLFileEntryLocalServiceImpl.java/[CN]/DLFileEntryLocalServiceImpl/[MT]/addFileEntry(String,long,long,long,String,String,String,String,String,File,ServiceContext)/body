{
  if (file == null || file.length() == 0) {
    throw new FileSizeException();
  }
  try {
    InputStream is=new UnsyncBufferedInputStream(new FileInputStream(file));
    return addFileEntry(uuid,userId,groupId,folderId,name,title,description,versionDescription,extraSettings,is,file.length(),serviceContext);
  }
 catch (  FileNotFoundException fnfe) {
    throw new FileSizeException();
  }
}
