{
  DLFileEntry dlFileEntry=dlFileEntryPersistence.findByPrimaryKey(fileEntryId);
  String extension=getExtension(title,serviceContext);
  String extraSettings=StringPool.BLANK;
  long fileEntryTypeId=GetterUtil.getLong(serviceContext.getAttribute("fileEntryTypeId"),-1L);
  fileEntryTypeId=getFileEntryTypeId(dlFileEntry.getGroupId(),dlFileEntry.getFolderId(),fileEntryTypeId);
  return updateFileEntry(userId,fileEntryId,sourceFileName,extension,mimeType,title,description,changeLog,majorVersion,extraSettings,fileEntryTypeId,is,size,serviceContext);
}
