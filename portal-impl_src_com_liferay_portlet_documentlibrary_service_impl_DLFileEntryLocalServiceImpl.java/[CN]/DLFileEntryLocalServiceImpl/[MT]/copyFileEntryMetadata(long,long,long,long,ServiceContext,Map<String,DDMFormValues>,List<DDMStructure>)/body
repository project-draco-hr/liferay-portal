{
  for (  DDMStructure ddmStructure : ddmStructures) {
    DLFileEntryMetadata dlFileEntryMetadata=dlFileEntryMetadataLocalService.fetchFileEntryMetadata(ddmStructure.getStructureId(),fromFileVersionId);
    if (dlFileEntryMetadata == null) {
      continue;
    }
    DDMFormValues ddmFormValues=StorageEngineUtil.getDDMFormValues(dlFileEntryMetadata.getDDMStorageId());
    ddmFormValuesMap.put(ddmStructure.getStructureKey(),ddmFormValues);
  }
  if (!ddmFormValuesMap.isEmpty()) {
    dlFileEntryMetadataLocalService.updateFileEntryMetadata(companyId,ddmStructures,fileEntryId,toFileVersionId,ddmFormValuesMap,serviceContext);
  }
}
