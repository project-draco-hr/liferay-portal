{
  if (file == null) {
    throw new FileSizeException();
  }
  try {
    InputStream is=new UnsyncBufferedInputStream(new FileInputStream(file));
    return addFileEntry(userId,groupId,folderId,name,title,description,versionDescription,extraSettings,is,file.length(),serviceContext);
  }
 catch (  FileNotFoundException fnfe) {
    throw new FileSizeException();
  }
}
