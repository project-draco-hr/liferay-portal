{
  String mimeType=MimeTypesUtil.getContentType(fileEntry.getName());
  tagsAssetLocalService.updateAsset(userId,fileEntry.getFolder().getGroupId(),DLFileEntry.class.getName(),fileEntry.getFileEntryId(),tagsCategories,tagsEntries,true,null,null,null,null,mimeType,fileEntry.getTitle(),fileEntry.getDescription(),null,null,0,0,null,false);
}
