{
  if (SearchEngineUtil.isIndexReadOnly()) {
    return;
  }
  DLFileEntry entry=dlFileEntryPersistence.fetchByPrimaryKey(entryId);
  if (entry == null) {
    return;
  }
  DLFolder folder=entry.getFolder();
  long companyId=entry.getCompanyId();
  String portletId=PortletKeys.DOCUMENT_LIBRARY;
  long groupId=folder.getGroupId();
  long folderId=folder.getFolderId();
  String fileName=entry.getName();
  String properties=entry.getLuceneProperties();
  String[] tagsEntries=tagsEntryLocalService.getEntryNames(DLFileEntry.class.getName(),entryId);
  try {
    Indexer.updateFile(companyId,portletId,groupId,folderId,fileName,properties,tagsEntries);
  }
 catch (  SearchException se) {
    _log.error("Reindexing " + entryId,se);
  }
}
