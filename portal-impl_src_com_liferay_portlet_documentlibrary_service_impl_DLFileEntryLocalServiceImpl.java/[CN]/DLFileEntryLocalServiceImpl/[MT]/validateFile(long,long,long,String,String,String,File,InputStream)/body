{
  if (Validator.isNotNull(sourceFileName)) {
    if (file != null) {
      DLStoreUtil.validate(sourceFileName,extension,sourceFileName,true,file);
    }
 else {
      DLStoreUtil.validate(sourceFileName,extension,sourceFileName,true,is);
    }
  }
  validateFileName(title);
  DLStoreUtil.validate(title,false);
  validateFile(groupId,folderId,fileEntryId,extension,title);
}
