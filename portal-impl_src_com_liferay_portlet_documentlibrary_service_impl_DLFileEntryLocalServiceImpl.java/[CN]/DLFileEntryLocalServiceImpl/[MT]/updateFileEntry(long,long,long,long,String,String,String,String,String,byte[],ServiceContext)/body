{
  InputStream is=null;
  long size=0;
  if ((bytes != null) && (bytes.length > 0)) {
    is=new UnsyncByteArrayInputStream(bytes);
    size=bytes.length;
  }
  return updateFileEntry(userId,groupId,folderId,newFolderId,name,sourceFileName,title,description,extraSettings,is,size,serviceContext);
}
