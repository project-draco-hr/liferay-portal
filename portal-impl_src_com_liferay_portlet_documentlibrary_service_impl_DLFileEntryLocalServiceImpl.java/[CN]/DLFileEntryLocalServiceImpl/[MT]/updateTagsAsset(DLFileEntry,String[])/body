{
  String mimeType=MimeTypesUtil.getContentType(fileEntry.getName());
  TagsAssetLocalServiceUtil.updateAsset(fileEntry.getUserId(),DLFileEntry.class.getName(),fileEntry.getFileEntryId(),tagsEntries,null,null,null,null,mimeType,fileEntry.getTitle(),fileEntry.getDescription(),fileEntry.getDescription(),null,0,0);
}
