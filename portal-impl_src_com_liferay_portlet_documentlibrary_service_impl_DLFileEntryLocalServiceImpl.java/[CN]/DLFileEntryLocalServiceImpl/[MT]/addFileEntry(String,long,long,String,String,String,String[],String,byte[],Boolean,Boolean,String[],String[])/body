{
  if ((byteArray == null) || (byteArray.length == 0)) {
    throw new FileSizeException();
  }
  InputStream is=new ByteArrayInputStream(byteArray);
  return addFileEntry(uuid,userId,folderId,name,title,description,tagsEntries,extraSettings,is,byteArray.length,addCommunityPermissions,addGuestPermissions,communityPermissions,guestPermissions);
}
