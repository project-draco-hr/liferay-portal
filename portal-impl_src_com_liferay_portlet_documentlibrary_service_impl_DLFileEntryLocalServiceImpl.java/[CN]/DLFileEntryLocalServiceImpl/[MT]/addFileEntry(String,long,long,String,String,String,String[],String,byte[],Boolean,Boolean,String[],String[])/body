{
  if (!PropsValues.WEBDAV_LITMUS) {
    if ((bytes == null) || (bytes.length == 0)) {
      throw new FileSizeException();
    }
  }
  InputStream is=new ByteArrayInputStream(bytes);
  return addFileEntry(uuid,userId,folderId,name,title,description,tagsEntries,extraSettings,is,bytes.length,addCommunityPermissions,addGuestPermissions,communityPermissions,guestPermissions);
}
