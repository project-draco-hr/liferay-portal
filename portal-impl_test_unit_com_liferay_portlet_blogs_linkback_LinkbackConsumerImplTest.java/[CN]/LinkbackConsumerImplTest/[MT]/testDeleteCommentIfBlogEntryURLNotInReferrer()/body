{
  Mockito.when(_http.URLtoString("__url__")).thenReturn("__URLtoString_not_containing_entryURL__");
  long commentId=RandomTestUtil.randomLong();
  _linkbackConsumerImpl.addNewTrackback(commentId,"__url__","__entryUrl__");
  _linkbackConsumerImpl.verifyNewTrackbacks();
  Mockito.verify(_commentManager).deleteComment(commentId);
  Mockito.verify(_http).URLtoString("__url__");
}
