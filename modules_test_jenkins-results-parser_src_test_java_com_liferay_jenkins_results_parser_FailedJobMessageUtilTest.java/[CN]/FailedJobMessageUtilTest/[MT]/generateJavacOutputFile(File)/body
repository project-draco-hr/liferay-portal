{
  File javacOutputFile=new File(caseDir,_JAVAC_OUTPUT_FILE_NAME);
  int maxLineCount=4000;
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < maxLineCount; i++) {
    sb.append("line: ");
    sb.append(i + 1);
    sb.append(" of ");
    sb.append(maxLineCount);
    sb.append("\n");
  }
  write(javacOutputFile,sb.toString());
}
