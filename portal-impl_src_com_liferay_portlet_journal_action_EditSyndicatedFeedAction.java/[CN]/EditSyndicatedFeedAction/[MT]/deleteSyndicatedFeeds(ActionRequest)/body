{
  long groupId=ParamUtil.getLong(req,"groupId");
  String[] deleteSyndicatedFeedIds=StringUtil.split(ParamUtil.getString(req,"deleteSyndicatedFeedIds"));
  for (int i=0; i < deleteSyndicatedFeedIds.length; i++) {
    JournalSyndicatedFeedServiceUtil.deleteSyndicatedFeed(groupId,deleteSyndicatedFeedIds[i]);
  }
}
