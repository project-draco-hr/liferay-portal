{
  String xsd=null;
  if (Validator.isNotNull(className) && (classPK > 0)) {
    if (className.equals(DDMTemplate.class.getName())) {
      DDMTemplate template=DDMTemplateServiceUtil.getTemplate(classPK);
      xsd=template.getScript();
    }
 else     if (className.equals(DDMStructure.class.getName())) {
      DDMStructure structure=DDMStructureServiceUtil.getStructure(classPK);
      xsd=structure.getCompleteXsd();
    }
  }
  return xsd;
}
