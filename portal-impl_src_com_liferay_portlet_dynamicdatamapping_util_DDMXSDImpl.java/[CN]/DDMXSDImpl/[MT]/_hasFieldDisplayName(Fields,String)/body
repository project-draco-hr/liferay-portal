{
  boolean hasFieldDisplayName=false;
  if (fields != null) {
    Field fieldsDisplayField=fields.get(DDMImpl.FIELDS_DISPLAY_NAME);
    String[] fieldsDisplayValues=DDMUtil.getFieldsDisplayValues(fieldsDisplayField);
    if (fieldsDisplayValues != null) {
      hasFieldDisplayName=ArraysUtil.contains(fieldsDisplayValues,name);
    }
  }
  return hasFieldDisplayName;
}
