{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),CalendarNotificationTemplateService.class.getName());
    if (invokableService instanceof CalendarNotificationTemplateService) {
      _service=(CalendarNotificationTemplateService)invokableService;
    }
 else {
      _service=new CalendarNotificationTemplateServiceClp(invokableService);
    }
    ReferenceRegistry.registerReference(CalendarNotificationTemplateServiceUtil.class,"_service");
  }
  return _service;
}
