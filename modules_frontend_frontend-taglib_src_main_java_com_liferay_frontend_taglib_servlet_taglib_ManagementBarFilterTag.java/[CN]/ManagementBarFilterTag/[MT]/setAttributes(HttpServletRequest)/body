{
  request.setAttribute("liferay-frontend:management-bar-filter:disabled",isDisabled());
  request.setAttribute("liferay-frontend:management-bar-filter:label",_label);
  request.setAttribute("liferay-frontend:management-bar-filter:managementBarFilterItems",_managementBarFilterItems);
  request.setAttribute("liferay-frontend:management-bar-filter:value",_value);
}
