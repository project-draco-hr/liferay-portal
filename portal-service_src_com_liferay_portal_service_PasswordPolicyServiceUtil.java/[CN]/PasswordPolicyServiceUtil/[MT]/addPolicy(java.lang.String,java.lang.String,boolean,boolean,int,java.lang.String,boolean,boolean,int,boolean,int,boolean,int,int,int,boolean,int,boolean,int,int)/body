{
  PasswordPolicyService passwordPolicyService=PasswordPolicyServiceFactory.getService();
  return passwordPolicyService.addPolicy(name,description,changeable,changeRequired,minAge,storageScheme,checkSyntax,allowDictionaryWords,minLength,history,historyCount,expireable,maxAge,warningTime,graceLimit,lockout,maxFailure,requireUnlock,lockoutDuration,resetFailureCount);
}
