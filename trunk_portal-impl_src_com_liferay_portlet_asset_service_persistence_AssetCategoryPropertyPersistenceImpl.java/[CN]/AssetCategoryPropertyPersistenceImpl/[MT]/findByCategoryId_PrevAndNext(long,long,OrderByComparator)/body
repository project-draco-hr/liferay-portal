{
  AssetCategoryProperty assetCategoryProperty=findByPrimaryKey(categoryPropertyId);
  Session session=null;
  try {
    session=openSession();
    AssetCategoryProperty[] array=new AssetCategoryPropertyImpl[3];
    array[0]=getByCategoryId_PrevAndNext(session,assetCategoryProperty,categoryId,orderByComparator,true);
    array[1]=assetCategoryProperty;
    array[2]=getByCategoryId_PrevAndNext(session,assetCategoryProperty,categoryId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
