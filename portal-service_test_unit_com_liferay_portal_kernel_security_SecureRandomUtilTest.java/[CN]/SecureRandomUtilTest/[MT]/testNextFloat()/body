{
  installPredictableRandom();
  for (int i=0; i < 512; i++) {
    byte b=(byte)(i * 4);
    byte[] bytes=new byte[4];
    for (int j=0; j < 4; j++) {
      bytes[j]=(byte)(b + j);
    }
    Assert.assertEquals(BigEndianCodec.getFloat(bytes,0),SecureRandomUtil.nextFloat(),0);
  }
  long firstLong=getFirstLong();
  long gapSeed=getGapSeed();
  long result=firstLong ^ gapSeed;
  Assert.assertEquals(Float.intBitsToFloat((int)result),SecureRandomUtil.nextFloat(),0);
  for (int i=0; i < 512; i++) {
    byte b=(byte)(i * 4);
    byte[] bytes=new byte[4];
    for (int j=0; j < 4; j++) {
      bytes[j]=(byte)(b + j);
    }
    Assert.assertEquals(BigEndianCodec.getFloat(bytes,0),SecureRandomUtil.nextFloat(),0);
  }
}
