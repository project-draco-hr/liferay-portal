{
  _installPredictableRandom();
  for (int i=0; i < 512; i++) {
    byte b=(byte)(i * 4);
    byte[] bytes=new byte[4];
    for (int j=0; j < 4; j++) {
      bytes[j]=(byte)(b + j);
    }
    Assert.assertEquals(BigEndianCodec.getInt(bytes,0),SecureRandomUtil.nextInt(),0);
  }
  long firstLong=_getFirstLong();
  long gapSeed=_getGapSeed();
  long result=firstLong ^ gapSeed;
  Assert.assertEquals((int)result,SecureRandomUtil.nextInt(),0);
  for (int i=0; i < 512; i++) {
    byte b=(byte)(i * 4);
    byte[] bytes=new byte[4];
    for (int j=0; j < 4; j++) {
      bytes[j]=(byte)(b + j);
    }
    Assert.assertEquals(BigEndianCodec.getInt(bytes,0),SecureRandomUtil.nextInt(),0);
  }
}
