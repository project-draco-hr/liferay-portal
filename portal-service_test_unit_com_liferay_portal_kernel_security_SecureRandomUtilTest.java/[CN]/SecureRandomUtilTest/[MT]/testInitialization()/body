{
  System.setProperty(_KEY_BUFFER_SIZE,"10");
  Field bufferSizeField=ReflectionUtil.getDeclaredField(SecureRandomUtil.class,"_BUFFER_SIZE");
  Assert.assertEquals(1024,bufferSizeField.get(null));
  Field bytesField=ReflectionUtil.getDeclaredField(SecureRandomUtil.class,"_bytes");
  byte[] bytes=(byte[])bytesField.get(null);
  Assert.assertEquals(1024,bytes.length);
}
