{
  if ((entryQuery.getAllCategoryIds().length > filteredEntryQuery.getAllCategoryIds().length) || (entryQuery.getAllTagIds().length > filteredEntryQuery.getAllTagIds().length) || ((entryQuery.getAnyCategoryIds().length > 0) && (filteredEntryQuery.getAnyCategoryIds().length == 0))|| ((entryQuery.getAnyTagIds().length > 0) && (filteredEntryQuery.getAnyTagIds().length == 0))) {
    return true;
  }
 else {
    return false;
  }
}
