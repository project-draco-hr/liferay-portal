{
  List<AssetEntryDisplay> entryDisplays=new ArrayList<AssetEntryDisplay>();
  AssetEntryDisplay[] companyEntryDisplays=assetEntryLocalService.getCompanyEntryDisplays(companyId,start,end,languageId);
  for (  AssetEntryDisplay entryDisplay : companyEntryDisplays) {
    try {
      if (AssetEntryPermission.contains(getPermissionChecker(),entryDisplay.getClassName(),entryDisplay.getClassPK(),ActionKeys.VIEW)) {
        entryDisplays.add(entryDisplay);
      }
    }
 catch (    Exception e) {
    }
  }
  return entryDisplays.toArray(new AssetEntryDisplay[entryDisplays.size()]);
}
