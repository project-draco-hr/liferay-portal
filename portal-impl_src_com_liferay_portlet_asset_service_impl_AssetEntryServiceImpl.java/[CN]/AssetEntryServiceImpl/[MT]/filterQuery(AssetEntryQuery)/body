{
  entryQuery.setAllCategoryIds(filterCategoryIds(entryQuery.getAllCategoryIds()));
  entryQuery.setAnyCategoryIds(filterCategoryIds(entryQuery.getAnyCategoryIds()));
  entryQuery.setAllTagIds(filterTagIds(entryQuery.getAllTagIds()));
  entryQuery.setAnyTagIds(filterTagIds(entryQuery.getAnyTagIds()));
}
