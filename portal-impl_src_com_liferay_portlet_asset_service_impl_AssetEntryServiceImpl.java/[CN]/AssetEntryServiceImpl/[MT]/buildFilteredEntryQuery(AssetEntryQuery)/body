{
  AssetEntryQuery filteredEntryQuery=new AssetEntryQuery(entryQuery);
  filteredEntryQuery.setAllCategoryIds(AssetUtil.filterCategoryIds(getPermissionChecker(),entryQuery.getAllCategoryIds()));
  filteredEntryQuery.setAllTagIdsArray(entryQuery.getAllTagIdsArray());
  filteredEntryQuery.setAnyCategoryIds(AssetUtil.filterCategoryIds(getPermissionChecker(),entryQuery.getAnyCategoryIds()));
  filteredEntryQuery.setAnyTagIds(entryQuery.getAnyTagIds());
  return filteredEntryQuery;
}
