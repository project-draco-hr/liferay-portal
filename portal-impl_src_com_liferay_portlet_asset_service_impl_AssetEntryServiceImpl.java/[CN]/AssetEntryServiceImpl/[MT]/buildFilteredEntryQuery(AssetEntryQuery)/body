{
  AssetEntryQuery filteredEntryQuery=new AssetEntryQuery(entryQuery);
  filteredEntryQuery.setAllCategoryIds(AssetUtil.filterCategoryIds(entryQuery.getAllCategoryIds(),getPermissionChecker()));
  filteredEntryQuery.setAllTagIdsArray(AssetUtil.filterTagIdsArray(entryQuery.getAllTagIdsArray(),getPermissionChecker()));
  filteredEntryQuery.setAnyCategoryIds(AssetUtil.filterCategoryIds(entryQuery.getAnyCategoryIds(),getPermissionChecker()));
  filteredEntryQuery.setAnyTagIds(AssetUtil.filterTagIds(entryQuery.getAnyTagIds(),getPermissionChecker()));
  return filteredEntryQuery;
}
