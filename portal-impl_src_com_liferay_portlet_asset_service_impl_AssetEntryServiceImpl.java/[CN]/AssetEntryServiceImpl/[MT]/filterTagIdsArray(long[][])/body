{
  List<long[]> viewableTagIdsArray=new ArrayList<long[]>();
  for (int i=0; i < tagIdsArray.length; i++) {
    long tagIds[]=tagIdsArray[i];
    List<Long> viewableTagIds=new ArrayList<Long>();
    for (    long tagId : tagIds) {
      if (AssetTagPermission.contains(getPermissionChecker(),tagId,ActionKeys.VIEW)) {
        viewableTagIds.add(tagId);
      }
    }
    viewableTagIdsArray.add(ArrayUtil.toArray(viewableTagIds.toArray(new Long[viewableTagIds.size()])));
  }
  return viewableTagIdsArray.toArray(new long[viewableTagIdsArray.size()][]);
}
