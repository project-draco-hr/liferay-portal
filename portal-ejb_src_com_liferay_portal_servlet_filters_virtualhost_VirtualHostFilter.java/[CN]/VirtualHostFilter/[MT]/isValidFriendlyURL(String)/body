{
  if (friendlyURL.equals(_PATH_C) || friendlyURL.equals(_PATH_C_PORTAL_CSS_CACHED) || friendlyURL.equals(_PATH_C_PORTAL_JAVASCRIPT_CACHED)|| friendlyURL.equals(_PATH_C_PORTAL_LAYOUT)|| friendlyURL.equals(_PATH_C_PORTAL_LOGIN)|| friendlyURL.equals(_PATH_C_PORTAL_LOGOUT)|| friendlyURL.equals(_PATH_C_PORTAL_RENDER_PORTLET)|| friendlyURL.startsWith(_PATH_IMAGE)) {
    return false;
  }
  int code=LayoutImpl.validateFriendlyURL(friendlyURL);
  if ((code > -1) && (code != LayoutFriendlyURLException.ENDS_WITH_SLASH)) {
    return false;
  }
  return true;
}
