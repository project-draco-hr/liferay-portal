{
  QueryDefinition<Object> queryDefinition=new QueryDefinition<>(status,start,end,(OrderByComparator<Object>)obc);
  List<Object> dlFoldersAndDLFileEntriesAndDLFileShortcuts=dlFolderLocalService.getFoldersAndFileEntriesAndFileShortcuts(getGroupId(),toFolderId(folderId),null,includeMountFolders,queryDefinition);
  return RepositoryModelUtil.toRepositoryEntries(dlFoldersAndDLFileEntriesAndDLFileShortcuts);
}
