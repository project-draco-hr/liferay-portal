{
  long fileEntryTypeId=ParamUtil.getLong(serviceContext,"fileEntryTypeId",-1L);
  Map<String,DDMFormValues> ddmFormValuesMap=getDDMFormValuesMap(serviceContext,fileEntryTypeId);
  long size=0;
  if (file != null) {
    size=file.length();
  }
  DLFileEntry dlFileEntry=dlFileEntryLocalService.updateFileEntry(userId,fileEntryId,sourceFileName,mimeType,title,description,changeLog,majorVersion,fileEntryTypeId,ddmFormValuesMap,file,null,size,serviceContext);
  return new LiferayFileEntry(dlFileEntry);
}
