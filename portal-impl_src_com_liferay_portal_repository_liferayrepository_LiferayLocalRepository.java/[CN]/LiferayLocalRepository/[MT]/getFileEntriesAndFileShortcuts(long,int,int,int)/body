{
  QueryDefinition<RepositoryEntry> queryDefinition=new QueryDefinition<>(status,start,end,null);
  List<Object> dlFileEntriesAndFileShortcuts=dlFolderLocalService.getFileEntriesAndFileShortcuts(getGroupId(),toFolderId(folderId),queryDefinition);
  return RepositoryModelUtil.toRepositoryEntries(dlFileEntriesAndFileShortcuts);
}
