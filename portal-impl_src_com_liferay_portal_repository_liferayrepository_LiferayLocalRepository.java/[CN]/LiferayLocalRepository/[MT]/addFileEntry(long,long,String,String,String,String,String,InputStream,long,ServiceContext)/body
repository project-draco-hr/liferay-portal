{
  long fileEntryTypeId=ParamUtil.getLong(serviceContext,"fileEntryTypeId",getDefaultFileEntryTypeId(serviceContext,folderId));
  Map<String,DDMFormValues> fieldsMap=getDDMFormValuesMap(serviceContext,fileEntryTypeId);
  DLFileEntry dlFileEntry=dlFileEntryLocalService.addFileEntry(userId,getGroupId(),getRepositoryId(),toFolderId(folderId),sourceFileName,mimeType,title,description,changeLog,fileEntryTypeId,fieldsMap,null,is,size,serviceContext);
  addFileEntryResources(dlFileEntry,serviceContext);
  return new LiferayFileEntry(dlFileEntry);
}
