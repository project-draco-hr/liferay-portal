{
  String fileName=RandomTestUtil.randomString();
  _store.addFile(_companyId,_repositoryId,fileName,_DATA_VERSION_1);
  String versionLabel="1.";
  for (int i=1; i <= newVersionCount; i++) {
    _store.updateFile(_companyId,_repositoryId,fileName,versionLabel + i,_DATA_VERSION_1);
  }
  Assert.assertTrue(_store.hasFile(_companyId,_repositoryId,fileName,Store.VERSION_DEFAULT));
  for (int i=1; i <= newVersionCount; i++) {
    Assert.assertTrue(_store.hasFile(_companyId,_repositoryId,fileName,versionLabel + i));
  }
  return fileName;
}
