{
  String fileName=addFile(1);
  Assert.assertTrue(_store.hasFile(_companyId,_repositoryId,fileName,Store.VERSION_DEFAULT));
  Assert.assertTrue(_store.hasFile(_companyId,_repositoryId,fileName,"1.1"));
  _store.deleteFile(_companyId,_repositoryId,fileName);
  Assert.assertFalse(_store.hasFile(_companyId,_repositoryId,fileName,Store.VERSION_DEFAULT));
  Assert.assertFalse(_store.hasFile(_companyId,_repositoryId,fileName,"1.1"));
}
