{
  Object[] data=addFile(1);
  long companyId=(Long)data[0];
  long repositoryId=(Long)data[1];
  String fileName=(String)data[2];
  Assert.assertTrue(_store.hasFile(companyId,repositoryId,fileName,Store.VERSION_DEFAULT));
  _store.deleteFile(companyId,repositoryId,fileName,Store.VERSION_DEFAULT);
  Assert.assertFalse(_store.hasFile(companyId,repositoryId,fileName,Store.VERSION_DEFAULT));
  Assert.assertTrue(_store.hasFile(companyId,repositoryId,fileName,"1.1"));
  _store.deleteFile(companyId,repositoryId,fileName,"1.1");
  Assert.assertFalse(_store.hasFile(companyId,repositoryId,fileName,"1.1"));
}
