{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
  selenium.waitForVisible("link=Wiki Display Test Page");
  selenium.clickAt("link=Wiki Display Test Page",RuntimeVariables.replace("Wiki Display Test Page"));
  selenium.waitForPageToLoad("30000");
  assertEquals(RuntimeVariables.replace("0 (0 Votes)"),selenium.getText("//div[3]/div/div[2]/div/div/div/div"));
  selenium.clickAt("//div[@class='taglib-ratings thumbs']/div/div/a[1]",RuntimeVariables.replace("+1"));
  selenium.waitForText("//div[3]/div/div[2]/div/div/div/div","+1 (1 Vote)");
  assertEquals(RuntimeVariables.replace("+1 (1 Vote)"),selenium.getText("//div[3]/div/div[2]/div/div/div/div"));
  selenium.clickAt("//div[@class='taglib-ratings thumbs']/div/div/a[1]",RuntimeVariables.replace("+1"));
  selenium.waitForText("//div[3]/div/div[2]/div/div/div/div","0 (0 Votes)");
  assertEquals(RuntimeVariables.replace("0 (0 Votes)"),selenium.getText("//div[3]/div/div[2]/div/div/div/div"));
  selenium.clickAt("//div[@class='taglib-ratings thumbs']/div/div/a[2]",RuntimeVariables.replace("-1"));
  selenium.waitForText("//div[3]/div/div[2]/div/div/div/div","-1 (1 Vote)");
  assertEquals(RuntimeVariables.replace("-1 (1 Vote)"),selenium.getText("//div[3]/div/div[2]/div/div/div/div"));
  selenium.clickAt("//div[@class='taglib-ratings thumbs']/div/div/a[2]",RuntimeVariables.replace("-1"));
  selenium.waitForText("//div[3]/div/div[2]/div/div/div/div","0 (0 Votes)");
  assertEquals(RuntimeVariables.replace("0 (0 Votes)"),selenium.getText("//div[3]/div/div[2]/div/div/div/div"));
}
