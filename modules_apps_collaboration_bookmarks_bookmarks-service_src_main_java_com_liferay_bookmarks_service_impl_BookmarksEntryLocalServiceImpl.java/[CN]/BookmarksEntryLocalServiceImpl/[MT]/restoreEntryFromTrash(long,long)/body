{
  BookmarksEntry entry=bookmarksEntryPersistence.findByPrimaryKey(entryId);
  TrashEntry trashEntry=trashEntryLocalService.getEntry(BookmarksEntry.class.getName(),entryId);
  entry=updateStatus(userId,entry,trashEntry.getStatus());
  trashEntryLocalService.deleteEntry(BookmarksEntry.class.getName(),entry.getEntryId());
  return entry;
}
