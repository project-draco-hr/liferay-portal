{
  Session session=null;
  try {
    session=openSession();
    if (saveOrUpdate) {
      session.saveOrUpdate(dlFileRank);
    }
 else {
      if (dlFileRank.isNew()) {
        session.save(dlFileRank);
      }
    }
    session.flush();
    dlFileRank.setNew(false);
    return dlFileRank;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
