{
  int label=1;
  while (label >= 1) {
switch (label) {
case 1:
      selenium.selectWindow("null");
    selenium.selectFrame("relative=top");
  selenium.open("/web/guest/home/");
selenium.waitForElementPresent("link=Control Panel");
selenium.clickAt("link=Control Panel",RuntimeVariables.replace("Control Panel"));
selenium.waitForPageToLoad("30000");
selenium.clickAt("link=Sites",RuntimeVariables.replace("Sites"));
selenium.waitForPageToLoad("30000");
selenium.type("//input[@id='_134_name']",RuntimeVariables.replace("World Cup"));
selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace("Search"));
selenium.waitForPageToLoad("30000");
boolean community1Present=selenium.isElementPresent("//td[7]/span/ul/li/strong/a/span");
if (!community1Present) {
label=2;
continue;
}
assertEquals(RuntimeVariables.replace("Actions"),selenium.getText("//td[7]/span/ul/li/strong/a/span"));
selenium.clickAt("//td[7]/span/ul/li/strong/a/span",RuntimeVariables.replace("Actions"));
selenium.waitForVisible("//div[@class='lfr-menu-list unstyled']/ul/li[@class='last']/a");
assertEquals(RuntimeVariables.replace("Delete"),selenium.getText("//div[@class='lfr-menu-list unstyled']/ul/li[@class='last']/a"));
selenium.click(RuntimeVariables.replace("//div[@class='lfr-menu-list unstyled']/ul/li[@class='last']/a"));
selenium.waitForPageToLoad("30000");
assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this[\\s\\S]$"));
case 2:
selenium.type("//input[@id='_134_name']",RuntimeVariables.replace("LAR"));
selenium.clickAt("//input[@value='Search']",RuntimeVariables.replace("Search"));
selenium.waitForPageToLoad("30000");
boolean community2Present=selenium.isElementPresent("//td[7]/span/ul/li/strong/a/span");
if (!community2Present) {
label=3;
continue;
}
assertEquals(RuntimeVariables.replace("Actions"),selenium.getText("//td[7]/span/ul/li/strong/a/span"));
selenium.clickAt("//td[7]/span/ul/li/strong/a/span",RuntimeVariables.replace("Actions"));
selenium.waitForVisible("//div[@class='lfr-menu-list unstyled']/ul/li[@class='last']/a");
assertEquals(RuntimeVariables.replace("Delete"),selenium.getText("//div[@class='lfr-menu-list unstyled']/ul/li[@class='last']/a"));
selenium.click(RuntimeVariables.replace("//div[@class='lfr-menu-list unstyled']/ul/li[@class='last']/a"));
selenium.waitForPageToLoad("30000");
assertTrue(selenium.getConfirmation().matches("^Are you sure you want to delete this[\\s\\S]$"));
case 3:
case 100:
label=-1;
}
}
}
