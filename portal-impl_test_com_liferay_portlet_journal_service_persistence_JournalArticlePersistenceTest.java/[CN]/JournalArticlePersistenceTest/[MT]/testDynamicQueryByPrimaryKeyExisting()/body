{
  JournalArticle newJournalArticle=addJournalArticle();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(JournalArticle.class,JournalArticle.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("id",newJournalArticle.getId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  JournalArticle existingJournalArticle=(JournalArticle)result.get(0);
  assertEquals(existingJournalArticle,newJournalArticle);
}
