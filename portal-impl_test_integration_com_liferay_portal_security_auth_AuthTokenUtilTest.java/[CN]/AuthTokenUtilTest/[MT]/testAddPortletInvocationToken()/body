{
  MockHttpServletRequest request=new MockHttpServletRequest();
  LiferayPortletURL liferayPortletURL=new PortletURLImpl(request,PortletKeys.PORTAL,0,PortletRequest.ACTION_PHASE);
  AuthTokenUtil.addPortletInvocationToken(request,liferayPortletURL);
  Assert.assertEquals("TEST_TOKEN_BY_PLID_AND_PORTLET_ID",liferayPortletURL.getParameter("p_p_auth"));
}
