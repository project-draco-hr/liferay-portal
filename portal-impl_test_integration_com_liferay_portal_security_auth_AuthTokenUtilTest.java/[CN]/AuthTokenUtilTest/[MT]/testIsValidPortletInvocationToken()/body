{
  Assert.assertTrue(AuthTokenUtil.isValidPortletInvocationToken(new MockHttpServletRequest(),0L,RandomTestUtil.randomString(),RandomTestUtil.randomString(),"VALID_PORTLET_INVOCATION_TOKEN"));
  Assert.assertFalse(AuthTokenUtil.isValidPortletInvocationToken(new MockHttpServletRequest(),0L,RandomTestUtil.randomString(),RandomTestUtil.randomString(),"INVALID_PORTLET_INVOCATION_TOKEN"));
}
