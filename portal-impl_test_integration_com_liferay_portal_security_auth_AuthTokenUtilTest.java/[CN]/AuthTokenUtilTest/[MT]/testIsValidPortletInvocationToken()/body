{
  Assert.assertTrue(AuthTokenUtil.isValidPortletInvocationToken(new MockHttpServletRequest(),0L,"portletId","struts action","VALID_PORTLET_INVOCATION_TOKEN"));
  Assert.assertFalse(AuthTokenUtil.isValidPortletInvocationToken(new MockHttpServletRequest(),0L,"portletId","struts action","INVALID_PORTLET_INVOCATION_TOKEN"));
}
