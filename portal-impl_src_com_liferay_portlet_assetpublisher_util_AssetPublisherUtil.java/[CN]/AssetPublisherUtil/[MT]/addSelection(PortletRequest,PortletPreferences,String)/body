{
  String assetEntryType=ParamUtil.getString(portletRequest,"assetEntryType");
  long assetEntryId=ParamUtil.getLong(portletRequest,"assetEntryId");
  int assetEntryOrder=ParamUtil.getInteger(portletRequest,"assetEntryOrder");
  addSelection(assetEntryType,assetEntryId,assetEntryOrder,portletPreferences,portletRequest,portletId);
}
