{
  if (assetEntryIds.size() == 0) {
    return;
  }
  String[] assetEntryXmls=preferences.getValues("asset-entry-xml",new String[0]);
  List<String> assetEntryXmlsList=ListUtil.fromArray(assetEntryXmls);
  Iterator<String> itr=assetEntryXmlsList.iterator();
  while (itr.hasNext()) {
    String assetEntryXml=itr.next();
    Document doc=SAXReaderUtil.read(assetEntryXml);
    Element root=doc.getRootElement();
    long assetEntryId=GetterUtil.getLong(root.element("asset-entry-id").getText());
    if (assetEntryIds.contains(assetEntryId)) {
      itr.remove();
    }
  }
  preferences.setValues("asset-entry-xml",assetEntryXmlsList.toArray(new String[assetEntryXmlsList.size()]));
  preferences.store();
}
