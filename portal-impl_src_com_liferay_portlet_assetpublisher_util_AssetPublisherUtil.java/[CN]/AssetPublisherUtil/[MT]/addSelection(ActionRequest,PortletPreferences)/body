{
  String assetEntryType=ParamUtil.getString(actionRequest,"assetEntryType");
  long assetEntryId=ParamUtil.getLong(actionRequest,"assetEntryId");
  int assetEntryOrder=ParamUtil.getInteger(actionRequest,"assetEntryOrder");
  addSelection(assetEntryType,assetEntryId,assetEntryOrder,preferences);
}
