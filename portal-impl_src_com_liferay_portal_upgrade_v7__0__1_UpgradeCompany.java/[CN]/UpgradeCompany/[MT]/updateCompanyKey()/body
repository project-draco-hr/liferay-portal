{
  try (LoggingTimer loggingTimer=new LoggingTimer();PreparedStatement ps=connection.prepareStatement("select companyId, key_ from Company");ResultSet rs=ps.executeQuery()){
    while (rs.next()) {
      String companyId=rs.getString("companyId");
      String keyString=rs.getString("key_");
      Key key=(Key)Base64.stringToObject(keyString);
      keyString=Encryptor.serializeKey(key);
      runSQL("update Company set key_ = '" + keyString + "' where companyId = "+ companyId);
    }
  }
 }
