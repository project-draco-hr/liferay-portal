{
  DynamicQuery resourcePermissionQuery=_resourcePermissionLocalService.dynamicQuery();
  resourcePermissionQuery=resourcePermissionQuery.add(newNameCriterion).add(companyIdProperty.eq(Long.valueOf(companyId))).add(roleIdProperty.eq(Long.valueOf(roleId))).add(scopeProperty.eq(Integer.valueOf(scope)));
  return _resourcePermissionLocalService.dynamicQueryCount(resourcePermissionQuery);
}
