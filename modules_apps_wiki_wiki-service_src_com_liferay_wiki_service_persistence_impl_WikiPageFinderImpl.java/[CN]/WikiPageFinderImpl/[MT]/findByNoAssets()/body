{
  Session session=null;
  try {
    session=openSession();
    String sql=CustomSQLUtil.get(FIND_BY_NO_ASSETS);
    long classNameId=ClassNameLocalServiceUtil.getClassNameId(WikiPage.class);
    sql=StringUtil.replace(sql,"[$CLASS_NAME_ID_COM.LIFERAY.PORTLET.WIKI.MODEL.WIKIPAGE$]",String.valueOf(classNameId));
    SQLQuery q=session.createSynchronizedSQLQuery(sql);
    q.addEntity("WikiPage",WikiPageImpl.class);
    return q.list(true);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
}
