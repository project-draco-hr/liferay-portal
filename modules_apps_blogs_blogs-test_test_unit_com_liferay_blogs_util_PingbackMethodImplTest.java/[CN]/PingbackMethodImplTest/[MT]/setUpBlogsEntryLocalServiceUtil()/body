{
  when(_blogsEntry.getEntryId()).thenReturn(_ENTRY_ID);
  when(_blogsEntry.getGroupId()).thenReturn(_GROUP_ID);
  when(_blogsEntry.getUrlTitle()).thenReturn(_URL_TITLE);
  when(_blogsEntry.isAllowPingbacks()).thenReturn(true);
  when(_blogsEntryLocalService.getEntry(Matchers.anyLong(),Matchers.anyString())).thenReturn(_blogsEntry);
}
