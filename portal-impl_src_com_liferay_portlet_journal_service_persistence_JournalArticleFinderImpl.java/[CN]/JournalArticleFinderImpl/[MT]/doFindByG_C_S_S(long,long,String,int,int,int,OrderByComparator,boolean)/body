{
  Session session=null;
  try {
    session=openSession();
    String sql=CustomSQLUtil.get(FIND_BY_G_C_S_S);
    if (status == WorkflowConstants.STATUS_ANY) {
      sql=StringUtil.replace(sql,"(status = ?)","(status != ?)");
    }
    sql=CustomSQLUtil.replaceOrderBy(sql,orderByComparator);
    if (structureId.equals(String.valueOf(JournalArticleConstants.CLASSNAME_ID_DEFAULT))) {
      sql=StringUtil.replace(sql,"(structureId = ?)","((structureId = ?) OR (structureId = '') OR" + "(structureId = null))");
    }
    if (inlineSQLHelper) {
      sql=InlineSQLHelperUtil.replacePermissionCheck(sql,JournalArticle.class.getName(),"JournalArticle.resourcePrimKey",groupId);
    }
    SQLQuery q=session.createSQLQuery(sql);
    q.addEntity("JournalArticle",JournalArticleImpl.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(groupId);
    qPos.add(classNameId);
    qPos.add(structureId);
    if (status != WorkflowConstants.STATUS_ANY) {
      qPos.add(status);
    }
 else {
      qPos.add(WorkflowConstants.STATUS_IN_TRASH);
    }
    return (List<JournalArticle>)QueryUtil.list(q,getDialect(),start,end);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
}
