{
  Session session=null;
  try {
    session=openSession();
    String sql=null;
    if (status == WorkflowConstants.STATUS_ANY) {
      sql=CustomSQLUtil.get(FIND_BY_G_U_C);
    }
 else {
      sql=CustomSQLUtil.get(FIND_BY_G_U_C_S);
    }
    sql=CustomSQLUtil.replaceOrderBy(sql,orderByComparator);
    if (inlineSQLHelper) {
      sql=InlineSQLHelperUtil.replacePermissionCheck(sql,JournalArticle.class.getName(),"JournalArticle.resourcePrimKey",groupId);
    }
    SQLQuery q=session.createSQLQuery(sql);
    q.addEntity("JournalArticle",JournalArticleImpl.class);
    QueryPos qPos=QueryPos.getInstance(q);
    qPos.add(groupId);
    qPos.add(classNameId);
    qPos.add(userId);
    if (status != WorkflowConstants.STATUS_ANY) {
      qPos.add(status);
    }
    return (List<JournalArticle>)QueryUtil.list(q,getDialect(),start,end);
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
}
