{
  selenium.open("/web/guest/home/");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("link=Image Gallery Permissions Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.clickAt("link=Image Gallery Permissions Test Page",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  selenium.clickAt("//div[2]/ul/li[1]/a",RuntimeVariables.replace("Add Folder"));
  selenium.waitForPageToLoad("30000");
  selenium.type("_31_name",RuntimeVariables.replace("Image Permissions Test Folder"));
  selenium.clickAt("//input[@value='Save']",RuntimeVariables.replace(""));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isTextPresent("Your request completed successfully."));
  assertTrue(selenium.isTextPresent("Image Permissions Test Folder"));
}
