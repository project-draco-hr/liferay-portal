{
  if (((pluginType == null) && (_pluginType != null)) || ((pluginType != null) && (_pluginType == null)) || ((pluginType != null) && (_pluginType != null) && !pluginType.equals(_pluginType))) {
    if (!XSS_ALLOW_PLUGINTYPE) {
      pluginType=XSSUtil.strip(pluginType);
    }
    _pluginType=pluginType;
  }
}
