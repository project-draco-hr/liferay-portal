{
  Object fileVersionModel=fileVersion.getModel();
  if (fileVersionModel instanceof DLFileVersion) {
    DLFileVersion dlFileVersion=(DLFileVersion)fileVersionModel;
    DLFileEntryType dlFileEntryType=_dlFileEntryTypeLocalService.getFileEntryType(dlFileVersion.getFileEntryTypeId());
    List<DDMStructure> ddmStructures=dlFileEntryType.getDDMStructures();
    for (    DDMStructure ddmStructure : ddmStructures) {
      String structureKey=ddmStructure.getStructureKey();
      if (structureKey.equals(Constants.GOOGLE_DOCUMENT_STRUCTURE_KEY)) {
        return true;
      }
    }
  }
  return false;
}
