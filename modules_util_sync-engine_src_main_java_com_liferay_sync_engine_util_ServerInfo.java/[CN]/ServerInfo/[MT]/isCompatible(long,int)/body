{
  Boolean compatible=_compatibilityMap.get(syncAccountId + "#" + minimumVersion);
  if (compatible != null) {
    return compatible;
  }
  SyncAccount syncAccount=SyncAccountService.fetchSyncAccount(syncAccountId);
  String pluginVersion=syncAccount.getPluginVersion();
  if (pluginVersion == null) {
    return false;
  }
  compatible=isCompatible(pluginVersion,minimumVersion);
  _compatibilityMap.put(syncAccountId + "#" + minimumVersion,compatible);
  return compatible;
}
