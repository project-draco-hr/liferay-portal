{
  String contentType=_uploadServletRequest.getContentType(_namespace.concat(name));
  if (contentType == null) {
    contentType=_uploadServletRequest.getContentType(name);
  }
  if (Validator.isNull(contentType) || contentType.equals(ContentTypes.APPLICATION_OCTET_STREAM)) {
    contentType=MimeTypesUtil.getContentType(getFile(name));
  }
  return contentType;
}
