{
  String cmd=req.getParameter(Constants.CMD);
  if (cmd == null) {
    setForward(req,"portlet.mail.add_folder");
  }
 else {
    try {
      _validateFolder(req);
      if (!SessionErrors.isEmpty(req)) {
        setForward(req,"portlet.mail.add_folder");
      }
 else {
        _addFolder(req,res);
      }
    }
 catch (    Exception e) {
      req.setAttribute(PageContext.EXCEPTION,e);
      setForward(req,Constants.COMMON_ERROR);
    }
  }
}
