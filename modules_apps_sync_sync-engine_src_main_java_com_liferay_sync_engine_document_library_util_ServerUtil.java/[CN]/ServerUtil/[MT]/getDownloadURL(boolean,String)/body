{
  if (!supportsModuleFramework) {
    try {
      URL url=new URL(urlString);
      int index=urlString.lastIndexOf(url.getPath());
      return urlString.substring(0,index);
    }
 catch (    Exception e) {
      _logger.error(e.getMessage(),e);
      return urlString;
    }
  }
  return urlString;
}
