{
  this();
  this.producerID=producerID;
  if (markupURL != null) {
    this.markupURL=markupURL;
  }
 else {
    WSRPXHelper.throwX(logger,Logger.ERROR,"init",ErrorCodes.MISSING_MARKUP_PORT);
  }
  if (serviceDescriptionURL != null) {
    this.serviceDescriptionURL=serviceDescriptionURL;
    initServiceDescInterface(serviceDescriptionURL);
  }
 else {
    WSRPXHelper.throwX(logger,Logger.ERROR,"init",ErrorCodes.MISSING_SERVCICE_DESC_PORT);
  }
}
