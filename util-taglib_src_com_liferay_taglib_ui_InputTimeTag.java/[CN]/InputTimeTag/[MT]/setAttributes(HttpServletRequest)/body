  request.setAttribute("liferay-ui:input-time:hourParam",_hourParam);
  request.setAttribute("liferay-ui:input-time:hourValue",String.valueOf(_hourValue));
  request.setAttribute("liferay-ui:input-time:minuteInterval",String.valueOf(_minuteInterval));
  request.setAttribute("liferay-ui:input-time:minuteParam",_minuteParam);
  request.setAttribute("liferay-ui:input-time:minuteValue",String.valueOf(_minuteValue));
  request.setAttribute("liferay-ui:input-time:name",_name);
}
