{
  selenium.selectWindow("null");
  selenium.selectFrame("relative=top");
  selenium.open("/web/site-name/");
  selenium.waitForVisible("link=Message Boards Permissions Page");
  selenium.clickAt("link=Message Boards Permissions Page",RuntimeVariables.replace("Message Boards Permissions Page"));
  selenium.waitForPageToLoad("30000");
  assertTrue(selenium.isPartialText("//a[@id='_145_addApplication']","More"));
  selenium.clickAt("//a[@id='_145_addApplication']",RuntimeVariables.replace("More"));
  selenium.waitForElementPresent("//div[@title='Message Boards']/p/a");
  selenium.clickAt("//div[@title='Message Boards']/p/a",RuntimeVariables.replace("Add"));
  selenium.waitForElementPresent("//section");
  assertTrue(selenium.isElementPresent("//section"));
}
