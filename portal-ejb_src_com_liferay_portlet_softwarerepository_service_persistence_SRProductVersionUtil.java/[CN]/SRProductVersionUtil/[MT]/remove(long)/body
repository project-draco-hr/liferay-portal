{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(productVersionId));
  }
  com.liferay.portlet.softwarerepository.model.SRProductVersion srProductVersion=getPersistence().remove(productVersionId);
  if (listener != null) {
    listener.onAfterRemove(srProductVersion);
  }
  return srProductVersion;
}
