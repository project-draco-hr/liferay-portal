{
  if (Validator.isNull(max) && Validator.isNull(min)) {
    return true;
  }
  if (Validator.isNotNull(max)) {
    float maxFloat=GetterUtil.getFloat(max);
    if (value > maxFloat) {
      return false;
    }
  }
  if (Validator.isNotNull(min)) {
    float minFLoat=GetterUtil.getFloat(min);
    if (value < minFLoat) {
      return false;
    }
  }
  return true;
}
