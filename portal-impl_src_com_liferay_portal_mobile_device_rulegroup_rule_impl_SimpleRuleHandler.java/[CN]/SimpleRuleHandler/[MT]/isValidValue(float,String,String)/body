{
  if (Validator.isNull(max) && Validator.isNull(min)) {
    return true;
  }
  if (Validator.isNotNull(max)) {
    float maxInt=GetterUtil.getFloat(max);
    if (value > maxInt) {
      return false;
    }
  }
  if (Validator.isNotNull(min)) {
    float minInt=GetterUtil.getFloat(min);
    if (value < minInt) {
      return false;
    }
  }
  return true;
}
