{
  SocialActivityCounter socialActivityCounter=findByPrimaryKey(activityCounterId);
  Session session=null;
  try {
    session=openSession();
    SocialActivityCounter[] array=new SocialActivityCounterImpl[3];
    array[0]=getByC_C_PrevAndNext(session,socialActivityCounter,classNameId,classPK,orderByComparator,true);
    array[1]=socialActivityCounter;
    array[2]=getByC_C_PrevAndNext(session,socialActivityCounter,classNameId,classPK,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
