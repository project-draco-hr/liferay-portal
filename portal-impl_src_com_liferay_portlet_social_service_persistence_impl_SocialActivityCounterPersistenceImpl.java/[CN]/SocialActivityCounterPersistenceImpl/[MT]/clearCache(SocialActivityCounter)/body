{
  EntityCacheUtil.removeResult(SocialActivityCounterModelImpl.ENTITY_CACHE_ENABLED,SocialActivityCounterImpl.class,socialActivityCounter.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  clearUniqueFindersCache((SocialActivityCounterModelImpl)socialActivityCounter);
}
