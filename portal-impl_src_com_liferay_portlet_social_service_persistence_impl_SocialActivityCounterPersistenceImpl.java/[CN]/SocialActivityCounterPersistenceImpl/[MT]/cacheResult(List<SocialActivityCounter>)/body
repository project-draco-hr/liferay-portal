{
  for (  SocialActivityCounter socialActivityCounter : socialActivityCounters) {
    if (entityCache.getResult(SocialActivityCounterModelImpl.ENTITY_CACHE_ENABLED,SocialActivityCounterImpl.class,socialActivityCounter.getPrimaryKey()) == null) {
      cacheResult(socialActivityCounter);
    }
 else {
      socialActivityCounter.resetOriginalValues();
    }
  }
}
