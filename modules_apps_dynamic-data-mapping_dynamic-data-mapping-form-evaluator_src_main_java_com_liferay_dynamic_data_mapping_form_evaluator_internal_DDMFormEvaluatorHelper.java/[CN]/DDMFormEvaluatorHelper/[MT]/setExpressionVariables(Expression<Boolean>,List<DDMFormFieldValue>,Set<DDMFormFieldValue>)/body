{
  for (  DDMFormFieldValue ddmFormFieldValue : ddmFormFieldValues) {
    String name=ddmFormFieldValue.getName();
    DDMFormField ddmFormField=_ddmFormFieldsMap.get(name);
    if (ddmFormField.isRepeatable() && !ancestorDDMFormFieldValues.contains(ddmFormFieldValue)) {
      continue;
    }
    Value value=ddmFormFieldValue.getValue();
    if (value != null) {
      setExpressionVariableValue(expression,name,ddmFormField.getDataType(),value.getString(_locale));
    }
    setExpressionVariables(expression,ddmFormFieldValue.getNestedDDMFormFieldValues(),ancestorDDMFormFieldValues);
  }
}
