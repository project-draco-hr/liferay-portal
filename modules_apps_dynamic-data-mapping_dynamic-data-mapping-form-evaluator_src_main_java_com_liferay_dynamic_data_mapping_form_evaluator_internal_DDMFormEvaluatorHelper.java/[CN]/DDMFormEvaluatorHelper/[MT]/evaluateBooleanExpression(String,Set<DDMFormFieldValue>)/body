{
  if (Validator.isNull(expressionString)) {
    return true;
  }
  Expression<Boolean> expression=_expressionFactory.createBooleanExpression(expressionString);
  setExpressionVariables(expression,_rootDDMFormFieldValues,ancestorDDMFormFieldValues);
  try {
    return expression.evaluate();
  }
 catch (  ExpressionEvaluationException eee) {
    if (_log.isWarnEnabled()) {
      _log.warn("Invalid expression or expression that is making " + "reference to a field no longer available: " + expressionString);
    }
  }
  return true;
}
