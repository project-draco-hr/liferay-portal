{
  TasksProposal newTasksProposal=addTasksProposal();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(TasksProposal.class,TasksProposal.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("proposalId",newTasksProposal.getProposalId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  TasksProposal existingTasksProposal=(TasksProposal)result.get(0);
  assertEquals(existingTasksProposal,newTasksProposal);
}
