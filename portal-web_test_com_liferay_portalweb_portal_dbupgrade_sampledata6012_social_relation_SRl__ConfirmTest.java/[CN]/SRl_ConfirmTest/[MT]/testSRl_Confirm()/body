{
  selenium.open("/web/socialrelationsn1/home/");
  selenium.getEval("window.Liferay.fire('initDockbar');");
  for (int second=0; ; second++) {
    if (second >= 90) {
      fail("timeout");
    }
    try {
      if (selenium.isVisible("link=Requests Test Page")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.click(RuntimeVariables.replace("link=Requests Test Page"));
  selenium.waitForPageToLoad("30000");
  selenium.getEval("window.Liferay.fire('initDockbar');");
  assertEquals(RuntimeVariables.replace("Confirm"),selenium.getText("//td[2]/ul/li[1]/a/span"));
  selenium.clickAt("//td[2]/ul/li[1]/a/span",RuntimeVariables.replace("Confirm"));
  selenium.waitForPageToLoad("30000");
  selenium.getEval("window.Liferay.fire('initDockbar');");
  assertFalse(selenium.isTextPresent("Confirm"));
}
