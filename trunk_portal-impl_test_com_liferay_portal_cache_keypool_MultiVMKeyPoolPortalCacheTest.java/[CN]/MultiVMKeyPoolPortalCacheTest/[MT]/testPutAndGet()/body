{
  PortalCache clusterPortalCache=SingleVMPoolUtil.getCache("ClusterPortalCache");
  PortalCache localPortalCache=SingleVMPoolUtil.getCache("LocalPortalCache");
  MultiVMKeyPoolPortalCache multiVMKeyPoolPortalCache=new MultiVMKeyPoolPortalCache(clusterPortalCache,localPortalCache);
  String testKey="testKey";
  String testValue="testValue";
  multiVMKeyPoolPortalCache.put(testKey,testValue);
  assertEquals(testValue,multiVMKeyPoolPortalCache.get(testKey));
}
