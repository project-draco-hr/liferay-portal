{
  ModelListener listener=_getListener();
  if (listener != null) {
    listener.onBeforeRemove(findByPrimaryKey(productScreenshotId));
  }
  com.liferay.portlet.softwarecatalog.model.SCProductScreenshot scProductScreenshot=getPersistence().remove(productScreenshotId);
  if (listener != null) {
    listener.onAfterRemove(scProductScreenshot);
  }
  return scProductScreenshot;
}
