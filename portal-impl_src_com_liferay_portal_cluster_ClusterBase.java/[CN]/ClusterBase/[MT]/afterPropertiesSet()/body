{
  if (!isEnabled()) {
    return;
  }
  if (!_initJGroupsProperties) {
    initSystemProperties();
    try {
      initBindAddress();
    }
 catch (    Exception e) {
      if (_log.isWarnEnabled()) {
        _log.warn("Failed to initialize outgoing IP address",e);
      }
    }
    _initJGroupsProperties=true;
  }
}
