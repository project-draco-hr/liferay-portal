{
  Iterator<Element> itr=parentElement.elements("dynamic-content").iterator();
  while (itr.hasNext()) {
    Element element=itr.next();
    long articleImageId=GetterUtil.getLong(element.attributeValue("id"));
    if (articleImageId <= 0) {
      continue;
    }
    JournalArticleImage articleImage=JournalArticleImageLocalServiceUtil.getArticleImage(articleImageId);
    articleImage.setElInstanceId(instanceId);
    JournalArticleImageLocalServiceUtil.updateJournalArticleImage(articleImage);
  }
}
