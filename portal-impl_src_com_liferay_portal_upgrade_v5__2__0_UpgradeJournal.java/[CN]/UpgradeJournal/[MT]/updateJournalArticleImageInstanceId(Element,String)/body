{
  List<Element> elements=parentElement.elements("dynamic-content");
  for (  Element element : elements) {
    long articleImageId=GetterUtil.getLong(element.attributeValue("id"));
    if (articleImageId <= 0) {
      continue;
    }
    runSQL("update JournalArticleImage set elInstanceId = '" + instanceId + "' where articleImageId = "+ articleImageId);
  }
}
