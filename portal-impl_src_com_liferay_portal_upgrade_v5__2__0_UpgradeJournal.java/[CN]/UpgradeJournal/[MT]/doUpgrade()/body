{
  List<JournalArticle> articles=JournalArticleLocalServiceUtil.getArticles();
  for (  JournalArticle article : articles) {
    String content=GetterUtil.getString(article.getContent());
    if (Validator.isNotNull(article.getStructureId())) {
      String newContent=addDynamicElementInstanceId(content);
      if (!content.equals(newContent)) {
        JournalArticleLocalServiceUtil.updateContent(article.getGroupId(),article.getArticleId(),article.getVersion(),newContent);
      }
    }
  }
  deleteJournalArticleImages();
}
