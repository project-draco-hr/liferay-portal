{
  if ((fileShortcutId > 0) && (fileEntryId == 0)) {
    DLFileShortcut dlFileShortcut=dlFileShortcutLocalService.getDLFileShortcut(fileShortcutId);
    fileEntryId=dlFileShortcut.getToFileEntryId();
  }
  long repositoryId=getRepositoryId(folderId,fileEntryId,fileVersionId);
  return getRepositoryImpl(repositoryId);
}
