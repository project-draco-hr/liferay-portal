{
  Region newRegion=addRegion();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Region.class,Region.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("regionId",newRegion.getRegionId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Region existingRegion=(Region)result.get(0);
  assertEquals(existingRegion,newRegion);
}
