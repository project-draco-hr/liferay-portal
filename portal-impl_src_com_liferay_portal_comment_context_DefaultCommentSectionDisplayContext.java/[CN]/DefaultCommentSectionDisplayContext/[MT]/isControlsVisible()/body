{
  if ((_discussionPermission == null) || _discussionTaglibHelper.isHideControls()) {
    return false;
  }
  return _discussionPermission.hasAddPermission(_discussionRequestHelper.getCompanyId(),_discussionRequestHelper.getScopeGroupId(),_discussionTaglibHelper.getClassName(),_discussionTaglibHelper.getClassPK());
}
