{
  if (((limitCategories == null) && (_limitCategories != null)) || ((limitCategories != null) && (_limitCategories == null)) || ((limitCategories != null) && (_limitCategories != null) && !limitCategories.equals(_limitCategories))) {
    if (!XSS_ALLOW_LIMITCATEGORIES) {
      limitCategories=XSSUtil.strip(limitCategories);
    }
    _limitCategories=limitCategories;
    setModified(true);
  }
