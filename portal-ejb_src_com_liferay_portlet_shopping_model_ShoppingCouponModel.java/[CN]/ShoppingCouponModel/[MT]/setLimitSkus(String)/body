{
  if (((limitSkus == null) && (_limitSkus != null)) || ((limitSkus != null) && (_limitSkus == null)) || ((limitSkus != null) && (_limitSkus != null) && !limitSkus.equals(_limitSkus))) {
    if (!XSS_ALLOW_LIMITSKUS) {
      limitSkus=XSSUtil.strip(limitSkus);
    }
    _limitSkus=limitSkus;
    setModified(true);
  }
}
