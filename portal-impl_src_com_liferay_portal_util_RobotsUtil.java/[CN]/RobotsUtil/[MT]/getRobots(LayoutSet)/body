{
  if (layoutSet != null) {
    String virtualHostname=StringPool.BLANK;
    try {
      virtualHostname=layoutSet.getVirtualHostname();
    }
 catch (    Exception e) {
    }
    return GetterUtil.get(layoutSet.getSettingsProperty(layoutSet.getPrivateLayout() + "-robots.txt"),getDefaultRobots(virtualHostname));
  }
  return getDefaultRobots(null);
}
