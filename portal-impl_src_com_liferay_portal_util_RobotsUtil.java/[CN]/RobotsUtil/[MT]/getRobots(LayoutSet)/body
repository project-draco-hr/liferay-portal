{
  if (layoutSet == null) {
    return getDefaultRobots(null);
  }
  String virtualHostname=StringPool.BLANK;
  try {
    virtualHostname=layoutSet.getVirtualHostname();
  }
 catch (  Exception e) {
  }
  if (Validator.isNull(virtualHostname) && Validator.isNotNull(PropsValues.VIRTUAL_HOSTS_DEFAULT_SITE_NAME)) {
    Group defaultGroup=GroupLocalServiceUtil.getGroup(layoutSet.getCompanyId(),PropsValues.VIRTUAL_HOSTS_DEFAULT_SITE_NAME);
    if (layoutSet.getGroupId() == defaultGroup.getGroupId()) {
      Company company=CompanyLocalServiceUtil.getCompany(layoutSet.getCompanyId());
      virtualHostname=company.getVirtualHostname();
    }
  }
  Group group=layoutSet.getGroup();
  return GetterUtil.get(group.getTypeSettingsProperty(layoutSet.isPrivateLayout() + "-robots.txt"),getDefaultRobots(virtualHostname));
}
