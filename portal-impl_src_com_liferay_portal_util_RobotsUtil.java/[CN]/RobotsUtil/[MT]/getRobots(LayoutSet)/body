{
  if (layoutSet != null) {
    String virtualHost="";
    try {
      virtualHost=layoutSet.getVirtualHost();
    }
 catch (    Exception e) {
    }
    return GetterUtil.get(layoutSet.getSettingsProperty(layoutSet.getPrivateLayout() + "-robots.txt"),getDefaultRobots(virtualHost));
  }
  return getDefaultRobots(null);
}
