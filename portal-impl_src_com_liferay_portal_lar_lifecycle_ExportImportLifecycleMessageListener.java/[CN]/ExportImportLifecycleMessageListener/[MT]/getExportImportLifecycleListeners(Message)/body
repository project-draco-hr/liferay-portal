{
  String destinationName=message.getDestinationName();
  if (destinationName.equals(DestinationNames.EXPORT_IMPORT_LIFECYCLE_EVENT_SYNC)) {
    return ExportImportLifecycleEventListenerRegistryUtil.getSyncExportImportLifecycleListeners();
  }
  return ExportImportLifecycleEventListenerRegistryUtil.getAsyncExportImportLifecycleListeners();
}
