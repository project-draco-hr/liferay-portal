{
  UpgradeTable upgradeTable=UpgradeTableFactoryUtil.getUpgradeTable(PortletTable.TABLE_NAME,PortletTable.TABLE_COLUMNS,new PKUpgradeColumnImpl("id_",false));
  upgradeTable.setCreateSQL(PortletTable.TABLE_SQL_CREATE);
  upgradeTable.setIndexesSQL(PortletTable.TABLE_SQL_ADD_INDEXES);
  upgradeTable.updateTable();
}
