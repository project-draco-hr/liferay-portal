{
  if (key.startsWith("after")) {
    String eventName=StringUtils.uncapitalize(StringUtil.remove(key,"after",StringPool.BLANK));
    afterEventOptionsMap.put(eventName,value);
  }
 else {
    String eventName=StringUtils.uncapitalize(StringUtil.remove(key,"on",StringPool.BLANK));
    eventName=StringUtils.uncapitalize(eventName);
    onEventsOptionsMap.put(eventName,value);
  }
}
