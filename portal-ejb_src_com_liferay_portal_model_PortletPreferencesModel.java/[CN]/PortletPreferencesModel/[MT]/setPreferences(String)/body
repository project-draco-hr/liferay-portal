{
  if (((preferences == null) && (_preferences != null)) || ((preferences != null) && (_preferences == null)) || ((preferences != null) && (_preferences != null) && !preferences.equals(_preferences))) {
    if (!XSS_ALLOW_PREFERENCES) {
      preferences=XSSUtil.strip(preferences);
    }
    _preferences=preferences;
    setModified(true);
  }
}
