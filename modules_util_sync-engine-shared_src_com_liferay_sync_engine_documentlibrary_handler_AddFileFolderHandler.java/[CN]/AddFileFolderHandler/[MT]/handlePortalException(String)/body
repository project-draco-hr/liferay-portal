{
  if (exception.equals("com.liferay.sync.SyncDLObjectChecksumException")) {
    if (_logger.isDebugEnabled()) {
      _logger.debug("Handling exception {}",exception);
    }
    AddFileEntryEvent addFileEntryEvent=new AddFileEntryEvent(getSyncAccountId(),getParameters());
    addFileEntryEvent.run();
    return true;
  }
  return super.handlePortalException(exception);
}
