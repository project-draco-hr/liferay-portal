{
  String structureId=generateId();
  addStructure(getCompanyGroupId(),structureId,getDefultXsd());
  addTemplate(groupId,generateId(),structureId,"First Template","Template Content 1");
  addTemplate(getCompanyGroupId(),generateId(),structureId,"Second Template","Template Content 2");
  long[] groupIds=new long[]{getCompanyGroupId(),groupId};
  List<JournalTemplate> templates=JournalTemplateLocalServiceUtil.search(companyId,groupIds,"Template",null,StringPool.LIKE,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  Assert.assertEquals(2,templates.size());
}
