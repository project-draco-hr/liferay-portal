{
  addTemplate(groupId,generateId(),StringPool.BLANK,"Template Content 1");
  addTemplate(groupId,generateId(),StringPool.BLANK,"Template Content 2");
  int templatesCount=JournalTemplateLocalServiceUtil.getTemplatesCount(groupId);
  Assert.assertEquals(2,templatesCount);
  JournalTemplateLocalServiceUtil.deleteTemplates(groupId);
  templatesCount=JournalTemplateLocalServiceUtil.getTemplatesCount(groupId);
  Assert.assertEquals(0,templatesCount);
}
