{
  String taskName="write" + StringUtil.capitalize(taskCache.getName()) + "CacheFile";
  Zip zip=GradleUtil.addTask(taskCache.getProject(),taskName,Zip.class);
  File cacheFile=taskCache.getCacheFile();
  zip.from(taskCache.getFiles());
  zip.setArchiveName(cacheFile.getName());
  zip.setDestinationDir(cacheFile.getParentFile());
  return zip;
}
