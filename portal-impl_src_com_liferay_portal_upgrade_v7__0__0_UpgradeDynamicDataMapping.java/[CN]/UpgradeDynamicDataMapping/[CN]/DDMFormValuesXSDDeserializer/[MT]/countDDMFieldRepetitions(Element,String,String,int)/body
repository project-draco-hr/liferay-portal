{
  String[] ddmFieldsDisplayValues=getDDMFieldsDisplayValues(rootElement,true);
  if (ddmFieldsDisplayValues.length != 0) {
    return countDDMFieldRepetitions(ddmFieldsDisplayValues,fieldName,parentFieldName,parentOffset);
  }
  Element dynamicElementElement=getDynamicElementElementByName(rootElement,fieldName);
  if (dynamicElementElement != null) {
    return 1;
  }
  return 0;
}
