{
  Element fieldsDisplayElement=getDynamicElementByName(rootElement,"_fieldsDisplay");
  String[] fieldsDisplayValues=null;
  if (fieldsDisplayElement != null) {
    Element fieldsDisplayDynamicContent=fieldsDisplayElement.element("dynamic-content");
    String fieldsDisplayText=fieldsDisplayDynamicContent.getText();
    fieldsDisplayValues=fieldsDisplayText.split(",");
  }
  return fieldsDisplayValues;
}
