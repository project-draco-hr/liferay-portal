{
  TrashVersion trashVersion=findByPrimaryKey(versionId);
  Session session=null;
  try {
    session=openSession();
    TrashVersion[] array=new TrashVersionImpl[3];
    array[0]=getByE_C_PrevAndNext(session,trashVersion,entryId,classNameId,orderByComparator,true);
    array[1]=trashVersion;
    array[2]=getByE_C_PrevAndNext(session,trashVersion,entryId,classNameId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
