{
  Session session=null;
  try {
    session=openSession();
    ShoppingOrderHBM shoppingOrderHBM=(ShoppingOrderHBM)session.get(ShoppingOrderHBM.class,orderId);
    if (shoppingOrderHBM == null) {
      _log.warn("No ShoppingOrder exists with the primary key " + orderId.toString());
      throw new NoSuchOrderException("No ShoppingOrder exists with the primary key " + orderId.toString());
    }
    return ShoppingOrderHBMUtil.model(shoppingOrderHBM);
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
