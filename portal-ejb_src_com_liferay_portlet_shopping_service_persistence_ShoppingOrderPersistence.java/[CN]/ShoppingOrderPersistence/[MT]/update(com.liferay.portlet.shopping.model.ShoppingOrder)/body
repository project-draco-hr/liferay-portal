{
  Session session=null;
  try {
    session=openSession();
    if (shoppingOrder.isNew()) {
      session.save(shoppingOrder);
    }
    session.flush();
    shoppingOrder.setNew(false);
    return shoppingOrder;
  }
 catch (  HibernateException he) {
    throw new SystemException(he);
  }
 finally {
    closeSession(session);
  }
}
