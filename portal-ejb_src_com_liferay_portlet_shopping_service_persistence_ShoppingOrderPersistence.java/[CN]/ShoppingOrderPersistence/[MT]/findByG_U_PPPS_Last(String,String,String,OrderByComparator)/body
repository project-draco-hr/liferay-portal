{
  int count=countByG_U_PPPS(groupId,userId,ppPaymentStatus);
  List list=findByG_U_PPPS(groupId,userId,ppPaymentStatus,count - 1,count,obc);
  if (list.size() == 0) {
    String msg="No ShoppingOrder exists with the key ";
    msg+=StringPool.OPEN_CURLY_BRACE;
    msg+="groupId=";
    msg+=groupId;
    msg+=", ";
    msg+="userId=";
    msg+=userId;
    msg+=", ";
    msg+="ppPaymentStatus=";
    msg+=ppPaymentStatus;
    msg+=StringPool.CLOSE_CURLY_BRACE;
    throw new NoSuchOrderException(msg);
  }
 else {
    return (ShoppingOrder)list.get(0);
  }
}
