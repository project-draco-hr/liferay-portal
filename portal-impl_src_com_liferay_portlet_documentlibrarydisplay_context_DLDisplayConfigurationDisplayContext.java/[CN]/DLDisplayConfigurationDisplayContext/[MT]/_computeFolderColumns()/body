{
  String[] folderColumns=_dlPortletInstanceSettings.getFolderColumns();
  _currentFolderColumns=new ArrayList<KeyValuePair>();
  for (  String folderColumn : folderColumns) {
    _currentFolderColumns.add(new KeyValuePair(folderColumn,LanguageUtil.get(_request,folderColumn)));
  }
  _availableFolderColumns=new ArrayList<KeyValuePair>();
  Arrays.sort(folderColumns);
  Set<String> allFolderColumns=SetUtil.fromArray(_getAllFolderColumns());
  for (  String folderColumn : allFolderColumns) {
    if (Arrays.binarySearch(folderColumns,folderColumn) < 0) {
      _availableFolderColumns.add(new KeyValuePair(folderColumn,LanguageUtil.get(_request,folderColumn)));
    }
  }
  _availableFolderColumns=ListUtil.sort(_availableFolderColumns,new KeyValuePairComparator(false,true));
}
