{
  UnicodeProperties typeSettingsProperties=group.getTypeSettingsProperties();
  typeSettingsProperties.remove("customJspServletContextName");
  groupLocalService.updateGroup(group.getGroupId(),typeSettingsProperties.toString());
  ExpandoValue expandoValue=expandoValueLocalService.getValue(group.getCompanyId(),Group.class.getName(),ExpandoTableConstants.DEFAULT_TABLE_NAME,"socialOfficeEnabled",group.getGroupId());
  expandoValue.setBoolean(false);
  expandoValueLocalService.updateExpandoValue(expandoValue);
}
