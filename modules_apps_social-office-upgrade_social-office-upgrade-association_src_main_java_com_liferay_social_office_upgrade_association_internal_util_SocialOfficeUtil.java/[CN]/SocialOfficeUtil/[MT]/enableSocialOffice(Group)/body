{
  UnicodeProperties typeSettingsProperties=group.getTypeSettingsProperties();
  typeSettingsProperties.setProperty("customJspServletContextName","so-hook");
  groupLocalService.updateGroup(group.getGroupId(),typeSettingsProperties.toString());
  expandoValueLocalService.addValue(group.getCompanyId(),Group.class.getName(),ExpandoTableConstants.DEFAULT_TABLE_NAME,"socialOfficeEnabled",group.getGroupId(),true);
}
