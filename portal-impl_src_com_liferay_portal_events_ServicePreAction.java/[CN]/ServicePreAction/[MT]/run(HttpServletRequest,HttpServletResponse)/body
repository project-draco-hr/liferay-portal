{
  StopWatch stopWatch=null;
  if (_log.isDebugEnabled()) {
    stopWatch=new StopWatch();
    stopWatch.start();
  }
  try {
    servicePre(req,res);
  }
 catch (  Exception e) {
    throw new ActionException(e);
  }
  if (_log.isDebugEnabled()) {
    _log.debug("Running takes " + stopWatch.getTime() + " ms");
  }
}
