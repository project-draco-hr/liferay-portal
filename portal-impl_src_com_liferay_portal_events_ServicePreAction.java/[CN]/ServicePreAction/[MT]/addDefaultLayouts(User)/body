{
  if (user.hasPrivateLayouts()) {
    return;
  }
  Group userGroup=user.getGroup();
  if (privateLARFile != null) {
    importLayoutsByLAR(user.getUserId(),userGroup.getGroupId(),true,privateLARFile);
  }
 else {
    importLayoutsByProperties(user.getUserId(),userGroup.getGroupId());
  }
  if (publicLARFile != null) {
    importLayoutsByLAR(user.getUserId(),userGroup.getGroupId(),false,publicLARFile);
  }
}
