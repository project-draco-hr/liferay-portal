{
  if (!PropsValues.LAYOUT_USER_PRIVATE_LAYOUTS_AUTO_CREATE) {
    return;
  }
  if (!user.hasPrivateLayouts()) {
    Group userGroup=user.getGroup();
    if (privateLARFile != null) {
      addDefaultLayoutsByLAR(user.getUserId(),userGroup.getGroupId(),true,privateLARFile);
    }
 else {
      addDefaultUserPrivateLayoutByProperties(user.getUserId(),userGroup.getGroupId());
    }
  }
}
