{
  JournalContentSearch journalContentSearch=findByPrimaryKey(contentSearchId);
  Session session=null;
  try {
    session=openSession();
    JournalContentSearch[] array=new JournalContentSearchImpl[3];
    array[0]=getByPortletId_PrevAndNext(session,journalContentSearch,portletId,orderByComparator,true);
    array[1]=journalContentSearch;
    array[2]=getByPortletId_PrevAndNext(session,journalContentSearch,portletId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
