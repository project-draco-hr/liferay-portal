{
  ThemeDisplay themeDisplay=(ThemeDisplay)_request.getAttribute(WebKeys.THEME_DISPLAY);
  PermissionChecker permissionChecker=themeDisplay.getPermissionChecker();
  if (!group.isCompany() && GroupPermissionUtil.contains(permissionChecker,group,ActionKeys.DELETE) && !PortalUtil.isSystemGroup(group.getGroupKey())) {
    return true;
  }
  return false;
}
