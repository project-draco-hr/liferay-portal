{
  DDMPermissionHandler ddmPermissionHandler=DDMUtil.getDDMPermissionHandler(classNameId);
  DDMPermission.check(getPermissionChecker(),serviceContext.getScopeGroupId(),ddmPermissionHandler.getResourceName(classNameId),ddmPermissionHandler.getAddStructureActionId());
  return ddmStructureLocalService.addStructure(userId,groupId,parentStructureKey,classNameId,structureKey,nameMap,descriptionMap,ddmForm,ddmFormLayout,storageType,type,serviceContext);
}
