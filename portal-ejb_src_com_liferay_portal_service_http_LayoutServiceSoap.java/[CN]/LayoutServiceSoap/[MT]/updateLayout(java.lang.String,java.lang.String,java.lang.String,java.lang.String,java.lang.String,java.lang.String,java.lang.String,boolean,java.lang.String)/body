{
  try {
    com.liferay.portal.model.Layout returnValue=LayoutServiceUtil.updateLayout(layoutId,ownerId,parentLayoutId,name,title,languageId,type,hidden,friendlyURL);
    return com.liferay.portal.model.LayoutSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
