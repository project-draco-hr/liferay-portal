{
  try {
    com.liferay.portal.model.Layout returnValue=LayoutServiceUtil.updateLayout(layoutId,ownerId,parentLayoutId,name,title,languageId,type,hidden,friendlyURL,iconImage,iconBytes);
    return com.liferay.portal.model.LayoutSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
