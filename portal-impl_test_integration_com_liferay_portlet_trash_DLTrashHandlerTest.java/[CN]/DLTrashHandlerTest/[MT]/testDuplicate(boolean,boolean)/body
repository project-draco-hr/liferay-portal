{
  int initialNotInTrashCount=getNotInTrashCount();
  int initialTrashEntriesCount=getTrashEntriesCount();
  int trashCount=0;
  if (duplicateFileEntry) {
    addAndTrashFileEntry();
    trashCount++;
  }
  if (duplicateFolder) {
    addAndTrashFolder();
    trashCount++;
  }
  Assert.assertEquals(initialNotInTrashCount,getNotInTrashCount());
  Assert.assertEquals(initialTrashEntriesCount + trashCount,getTrashEntriesCount());
  if (duplicateFileEntry) {
    addAndTrashFileEntry();
    trashCount++;
  }
  if (duplicateFolder) {
    addAndTrashFolder();
    trashCount++;
  }
  Assert.assertEquals(initialNotInTrashCount,getNotInTrashCount());
  Assert.assertEquals(initialTrashEntriesCount + trashCount,getTrashEntriesCount());
}
