{
  int initialFoldersAndFileEntriesCount=getFolderAndFileEntriesNotInTrashCount();
  int initialTrashEntriesCount=getTrashEntriesCount();
  int trashCount=0;
  if (duplicateFileEntry) {
    addAndTrashFileEntry();
    trashCount++;
  }
  if (duplicateFolder) {
    addAndTrashFolder();
    trashCount++;
  }
  Assert.assertEquals(initialFoldersAndFileEntriesCount,getFolderAndFileEntriesNotInTrashCount());
  Assert.assertEquals(initialTrashEntriesCount + trashCount,getTrashEntriesCount());
  if (duplicateFileEntry) {
    addAndTrashFileEntry();
    trashCount++;
  }
  if (duplicateFolder) {
    addAndTrashFolder();
    trashCount++;
  }
  Assert.assertEquals(initialFoldersAndFileEntriesCount,getFolderAndFileEntriesNotInTrashCount());
  Assert.assertEquals(initialTrashEntriesCount + trashCount,getTrashEntriesCount());
}
