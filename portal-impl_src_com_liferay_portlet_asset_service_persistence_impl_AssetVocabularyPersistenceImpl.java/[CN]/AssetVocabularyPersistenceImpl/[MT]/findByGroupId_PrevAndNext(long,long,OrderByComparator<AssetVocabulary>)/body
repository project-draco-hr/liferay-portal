{
  AssetVocabulary assetVocabulary=findByPrimaryKey(vocabularyId);
  Session session=null;
  try {
    session=openSession();
    AssetVocabulary[] array=new AssetVocabularyImpl[3];
    array[0]=getByGroupId_PrevAndNext(session,assetVocabulary,groupId,orderByComparator,true);
    array[1]=assetVocabulary;
    array[2]=getByGroupId_PrevAndNext(session,assetVocabulary,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
