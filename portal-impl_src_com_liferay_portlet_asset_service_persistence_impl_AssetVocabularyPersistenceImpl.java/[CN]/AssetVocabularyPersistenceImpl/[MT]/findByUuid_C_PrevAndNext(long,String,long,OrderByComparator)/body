{
  AssetVocabulary assetVocabulary=findByPrimaryKey(vocabularyId);
  Session session=null;
  try {
    session=openSession();
    AssetVocabulary[] array=new AssetVocabularyImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,assetVocabulary,uuid,companyId,orderByComparator,true);
    array[1]=assetVocabulary;
    array[2]=getByUuid_C_PrevAndNext(session,assetVocabulary,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
