{
  MBMessageFlag newMBMessageFlag=addMBMessageFlag();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBMessageFlag.class,MBMessageFlag.class.getClassLoader());
  dynamicQuery.setProjection(ProjectionFactoryUtil.property("messageFlagId"));
  Object newMessageFlagId=newMBMessageFlag.getMessageFlagId();
  dynamicQuery.add(RestrictionsFactoryUtil.in("messageFlagId",new Object[]{newMessageFlagId}));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  Object existingMessageFlagId=result.get(0);
  assertEquals(existingMessageFlagId,newMessageFlagId);
}
