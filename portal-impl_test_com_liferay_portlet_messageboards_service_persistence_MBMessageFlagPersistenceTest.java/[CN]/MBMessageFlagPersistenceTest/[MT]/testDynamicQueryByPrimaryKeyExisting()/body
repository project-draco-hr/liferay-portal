{
  MBMessageFlag newMBMessageFlag=addMBMessageFlag();
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(MBMessageFlag.class,MBMessageFlag.class.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("messageFlagId",newMBMessageFlag.getMessageFlagId()));
  List<Object> result=_persistence.findWithDynamicQuery(dynamicQuery);
  assertEquals(1,result.size());
  MBMessageFlag existingMBMessageFlag=(MBMessageFlag)result.get(0);
  assertEquals(existingMBMessageFlag,newMBMessageFlag);
}
