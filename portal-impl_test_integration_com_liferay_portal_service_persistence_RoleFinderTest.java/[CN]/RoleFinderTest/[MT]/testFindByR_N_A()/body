{
  boolean exists=false;
  List<Role> roles=RoleFinderUtil.findByR_N_A(_resourceBlock.getResourceBlockId(),_resourceBlock.getName(),_modelResourceAction.getActionId());
  for (  Role role : roles) {
    if (role.getRoleId() == _arbitraryRole.getRoleId()) {
      exists=true;
      break;
    }
  }
  Assert.assertTrue("The method findByR_N_A should have returned the role " + _arbitraryRole.getRoleId(),exists);
}
