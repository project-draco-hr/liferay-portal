{
  DocumentRepository documentRepository=capabilityRegistry.getTarget();
  capabilityRegistry.addExportedCapability(RelatedModelCapability.class,new LiferayRelatedModelCapability(new RepositoryEntryConverter(),new RepositoryEntryChecker(documentRepository)));
  capabilityRegistry.addExportedCapability(TrashCapability.class,new LiferayTrashCapability());
  capabilityRegistry.addExportedCapability(WorkflowCapability.class,new MinimalWorkflowCapability());
}
