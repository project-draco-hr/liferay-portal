{
  BufferedReader br=new BufferedReader(new StringReader(data));
  StringMaker sm=new StringMaker();
  String line=null;
  while ((line=br.readLine()) != null) {
    if (line.startsWith(_ALTER_COLUMN_TYPE) || line.startsWith(_ALTER_COLUMN_NAME)) {
      line="-- " + line;
    }
    sm.append(line);
    sm.append("\n");
  }
  br.close();
  return sm.toString();
}
