{
  BufferedReader br=new BufferedReader(new StringReader(data));
  StringMaker sm=new StringMaker();
  String line=null;
  while ((line=br.readLine()) != null) {
    if (!line.startsWith("insert into ") && !line.startsWith("update ")) {
      sm.append(line);
      sm.append("\n");
    }
  }
  br.close();
  return sm.toString();
}
