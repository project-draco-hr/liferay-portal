{
  BufferedImage image=new BufferedImage(5,5,BufferedImage.TYPE_INT_ARGB);
  Graphics g=image.getGraphics();
  int[] rgb=ColorUtil.getRGB(hex1);
  g.setColor(new Color(rgb[0],rgb[1],rgb[2]));
  g.fillRect(0,0,2,1);
  g.fillRect(0,1,4,2);
  g.fillRect(0,3,5,2);
  rgb=ColorUtil.getRGB(hex2);
  g.setColor(new Color(rgb[0],rgb[1],rgb[2]));
  g.fillRect(0,0,2,1);
  g.fillRect(1,1,2,1);
  g.fillRect(3,1,1,3);
  g.fillRect(4,3,1,2);
  ImageUtil.encodeGIF(image,out);
}
