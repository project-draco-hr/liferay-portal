{
  boolean overwrite=GetterUtil.get(System.getProperty("color.scheme.builder.overwrite"),false);
  try {
    ColorSchemeBuilder.build("WEB-INF/liferay-look-and-feel.xml",overwrite);
    ColorSchemeBuilder.build("WEB-INF/liferay-look-and-feel-ext.xml",overwrite);
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
}
