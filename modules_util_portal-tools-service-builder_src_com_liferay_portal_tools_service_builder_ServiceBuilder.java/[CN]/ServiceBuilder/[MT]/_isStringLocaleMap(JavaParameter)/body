{
  Type type=javaParameter.getType();
  Type[] actualArgumentTypes=type.getActualTypeArguments();
  if (actualArgumentTypes.length != 2) {
    return false;
  }
  if (!_isTypeValue(actualArgumentTypes[0],Locale.class.getName()) || !_isTypeValue(actualArgumentTypes[1],String.class.getName())) {
    return false;
  }
  return true;
}
