{
  JavaClass javaClass=_getJavaClass(_outputPath + "/model/impl/" + entity.getName()+ "Impl.java");
  Map<String,Object> context=_getContext();
  context.put("entity",entity);
  context.put("cacheFields",_getCacheFields(javaClass));
  String content=_processTemplate(_tplModelCache,context);
  File modelFile=new File(_outputPath + "/model/impl/" + entity.getName()+ "CacheModel.java");
  writeFile(modelFile,content,_author);
}
