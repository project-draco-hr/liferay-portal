{
  Map<String,Object> context=_getContext();
  context.put("entity",entity);
  context=_putDeprecatedKeys(context,modelImplJavaClass);
  String content=_processTemplate(_tplModel,context);
  File modelFile=new File(_serviceOutputPath + "/model/" + entity.getName()+ "Model.java");
  writeFile(modelFile,content,_author,_modifiedFileNames);
}
