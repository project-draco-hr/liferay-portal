{
  ServerSocketChannel serverSocketChannel=SocketUtil.createServerSocketChannel(InetAddress.getLocalHost(),15238,_serverSocketConfigurator);
  ServerSocket serverSocket=serverSocketChannel.socket();
  SocketChannel clientPeerSocketChannel=SocketChannel.open(new InetSocketAddress(InetAddress.getLocalHost(),serverSocket.getLocalPort()));
  SocketChannel serverPeerSocketChannel=serverSocketChannel.accept();
  serverSocketChannel.close();
  SocketChannel[] socketChannels=new SocketChannel[2];
  socketChannels[0]=serverPeerSocketChannel;
  socketChannels[1]=clientPeerSocketChannel;
  return socketChannels;
}
