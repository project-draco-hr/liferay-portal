{
  User user=getUser();
  JournalTemplatePermission.check(getPermissionChecker(),user.getCompanyId(),groupId,templateId,ActionKeys.UPDATE);
  return JournalTemplateLocalServiceUtil.updateTemplate(user.getCompanyId(),groupId,templateId,structureId,name,description,xsl,formatXsl,langType,smallImage,smallImageURL,smallFile);
}
