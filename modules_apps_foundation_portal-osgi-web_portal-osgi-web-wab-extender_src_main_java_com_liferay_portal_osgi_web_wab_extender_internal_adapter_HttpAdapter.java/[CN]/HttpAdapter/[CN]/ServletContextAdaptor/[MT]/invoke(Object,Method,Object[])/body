{
  if (method.getName().equals("getInitParameter") && (args != null) && (args.length == 1)) {
    if ("osgi.http.endpoint".equals(args[0])) {
      return _servletContext.getInitParameter((String)args[0]);
    }
    return null;
  }
 else   if (method.getName().equals("getInitParameterNames") && (args == null)) {
    return Collections.emptyEnumeration();
  }
 else   if (method.getName().equals("getJspConfigDescriptor") && JspConfigDescriptor.class.isAssignableFrom(method.getReturnType())) {
    return null;
  }
  return method.invoke(_servletContext,args);
}
