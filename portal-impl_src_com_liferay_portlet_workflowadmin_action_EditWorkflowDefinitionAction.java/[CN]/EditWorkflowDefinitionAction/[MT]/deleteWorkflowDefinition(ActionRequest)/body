{
  long userId=PortalUtil.getUserId(actionRequest);
  String name=ParamUtil.getString(actionRequest,"name");
  int version=ParamUtil.getInteger(actionRequest,"version");
  WorkflowDefinition workflowDefinition=new DefaultWorkflowDefinition(null,name,version,null);
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(workflowDefinition,userId,null);
}
