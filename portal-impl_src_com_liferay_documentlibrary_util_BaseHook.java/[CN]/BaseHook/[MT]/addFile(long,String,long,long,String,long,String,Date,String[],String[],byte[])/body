{
  InputStream is=new ByteArrayInputStream(bytes);
  try {
    addFile(companyId,portletId,groupId,repositoryId,fileName,fileEntryId,properties,modifiedDate,tagsCategories,tagsEntries,is);
  }
  finally {
    try {
      is.close();
    }
 catch (    IOException ioe) {
      _log.error(ioe);
    }
  }
}
