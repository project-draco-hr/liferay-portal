{
  MembershipRequest membershipRequest=findByPrimaryKey(membershipRequestId);
  Session session=null;
  try {
    session=openSession();
    MembershipRequest[] array=new MembershipRequestImpl[3];
    array[0]=getByG_S_PrevAndNext(session,membershipRequest,groupId,statusId,orderByComparator,true);
    array[1]=membershipRequest;
    array[2]=getByG_S_PrevAndNext(session,membershipRequest,groupId,statusId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
