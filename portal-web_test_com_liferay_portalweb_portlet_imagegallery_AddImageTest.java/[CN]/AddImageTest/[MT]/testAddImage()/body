{
  selenium.click("//b");
  selenium.waitForPageToLoad("30000");
  selenium.click("//input[@value='Add Image']");
  selenium.waitForPageToLoad("30000");
  selenium.click("link=Use the classic uploader.");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("_31_file")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
  selenium.type("_31_file","L:\\portal\\build\\portal-web\\test\\com\\liferay\\portalweb\\portlet\\imagegallery\\test_image.jpg");
  selenium.typeKeys("_31_description","This is the Liferay logo!");
  selenium.click("//input[@value='Save']");
  selenium.waitForPageToLoad("30000");
  for (int second=0; ; second++) {
    if (second >= 60) {
      fail("timeout");
    }
    try {
      if (selenium.isElementPresent("//img[@title='This is the Liferaylogo']")) {
        break;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(1000);
  }
}
