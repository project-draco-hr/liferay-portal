{
  if (ResourceBlockLocalServiceUtil.isSupported(className)) {
    return ResourceBlockPermissionLocalServiceUtil.getAvailableResourceBlockPermissionActionIds(className,GetterUtil.getLong(primKey),actionIds);
  }
 else {
    return ResourcePermissionLocalServiceUtil.getAvailableResourcePermissionActionIds(companyId,className,ResourceConstants.SCOPE_INDIVIDUAL,primKey,actionIds);
  }
}
