{
  SearchCriteriaInvoker searchCriteriaInvoker=(SearchCriteriaInvoker)searchCriteria;
  searchCriteriaInvoker.invoke();
  List<JournalArticle> articles=(List<JournalArticle>)searchCriteriaInvoker.getReturnValue();
  List<Content> contents=new ArrayList<Content>(articles.size());
  for (  JournalArticle article : articles) {
    contents.add(new MirageJournalArticle(article));
  }
  return contents;
}
