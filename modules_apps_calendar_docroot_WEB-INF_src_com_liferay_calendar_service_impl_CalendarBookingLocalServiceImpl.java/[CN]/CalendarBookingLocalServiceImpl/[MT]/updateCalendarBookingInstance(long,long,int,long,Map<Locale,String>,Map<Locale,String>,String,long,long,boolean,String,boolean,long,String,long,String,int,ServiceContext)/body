{
  long[] childCalendarIds=getChildCalendarIds(calendarBookingId,calendarId);
  return updateCalendarBookingInstance(userId,calendarBookingId,instanceIndex,calendarId,childCalendarIds,titleMap,descriptionMap,location,startTime,endTime,allDay,recurrence,allFollowing,firstReminder,firstReminderType,secondReminder,secondReminderType,status,serviceContext);
}
