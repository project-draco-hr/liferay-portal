{
  DLFolder dlFolder=null;
  try {
    dlFolder=getParentFolder();
  }
 catch (  Exception e) {
    return null;
  }
  while (dlFolder != null) {
    if (dlFolder.isInTrash()) {
      return dlFolder;
    }
    try {
      dlFolder=dlFolder.getParentFolder();
    }
 catch (    Exception e) {
      return null;
    }
  }
  return null;
}
