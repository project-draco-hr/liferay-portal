{
  List<Long> ancestorFolderIds=new ArrayList<Long>();
  DLFolder folder=this;
  while (!folder.isRoot()) {
    try {
      folder=folder.getParentFolder();
      ancestorFolderIds.add(folder.getFolderId());
    }
 catch (    NoSuchFolderException nsfe) {
      if (folder.isInTrash()) {
        break;
      }
      throw nsfe;
    }
  }
  return ancestorFolderIds;
}
