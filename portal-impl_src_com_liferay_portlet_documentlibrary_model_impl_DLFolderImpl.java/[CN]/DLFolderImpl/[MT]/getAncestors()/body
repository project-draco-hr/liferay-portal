{
  List<DLFolder> ancestors=new ArrayList<>();
  DLFolder folder=this;
  while (!folder.isRoot()) {
    try {
      folder=folder.getParentFolder();
      ancestors.add(folder);
    }
 catch (    NoSuchFolderException nsfe) {
      if (folder.isInTrash()) {
        break;
      }
      throw nsfe;
    }
  }
  return ancestors;
}
