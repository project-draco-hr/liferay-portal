{
  Folder parentFolder=null;
  try {
    parentFolder=super.getParentFolder();
    if (parentFolder != null) {
      return parentFolder;
    }
  }
 catch (  Exception e) {
  }
  try {
    parentFolder=CMISRepositoryLocalServiceUtil.toFolder(getRepositoryId(),_document.getParents().get(0));
    setParentFolder(parentFolder);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return parentFolder;
}
